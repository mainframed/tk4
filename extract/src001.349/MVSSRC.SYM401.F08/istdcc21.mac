         TM    CRRSTSAV(CRRPTR),B'00000001'                        0631
         BNO   @RF00631                                            0631
*     CRRDSB=FSBDMGRS;              /* DEVICE MGMT RESPONSE          */
         MVC   CRRDSB(2,CRRPTR),FSBDMGRS(FSBPTR)                   0632
*   ELSE                                                           0633
