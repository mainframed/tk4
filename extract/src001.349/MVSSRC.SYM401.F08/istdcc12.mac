*   ELSE                                                           0475
*     DO;                                                          0475
*       IF LCCWRWOP=LEWT×           /* IF LCCW CONTAINS EITHER       */
*           LCCWRWOP=LEWM THEN      /* ERASE-WRITE OPCODE            */
         TM    LCCWRWOP(LCCWPTR),B'00011101'                       0476
         BNO   @GL00003                                            0476
         TM    LCCWRWOP(LCCWPTR),B'00100010'                       0476
         BZ    @RT00476                                            0476
@GL00003 TM    LCCWRWOP(LCCWPTR),B'00011001'                       0476
         BNO   @RF00476                                            0476
         TM    LCCWRWOP(LCCWPTR),B'00100110'                       0476
