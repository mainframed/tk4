*             DO;                                                  0519
*               IF LDOACHN=ON×      /* IF LDO IS DATA- OR            */
*                   LDOCMHN=ON THEN /* COMMAND-CHAINED               */
         TM    LDOACHN(LDOPTR),B'11000000'                         0520
         BZ    @RF00520                                            0520
*                 DO;               /* SET ERROR CODES:              */
*                   CRRFDB2=CRRCCCPY;/* CC COPY LDO TO 3270          */
         MVI   CRRFDB2(CRRPTR),X'16'                               0522
*                   CRRRTNCD=CRRLOGIC;/* LOGIC ERROR                 */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0523
*                   CRRSW0=ON;      /* INTERNAL ERROR SWITCH         */
         OI    CRRSW0(CRRPTR),B'10000000'                          0524
*                 END;              /* END ERROR CODE SETTING        */
*               ELSE                /* IF LDO IS NOT CC OR DC        */
*                 DO;                                              0526
         B     @RC00520                                            0526
@RF00520 DS    0H                                                  0527
*                   ISTLCCW(2:8)=ISTLDO(2:8);/* MOVE LDO INTO LCCW   */
         MVC   ISTLCCW+1(7,LCCWPTR),ISTLDO+1(LDOPTR)               0527
*                   IF LCPNXLCW->LCCWOP=LDOJUMP THEN/* IF FIRST LCCW
*                                      IS A JUMP,                    */
         L     @15,LCPNXLCW(,LCPBPTR)                              0528
         CLI   LCCWOP(@15),X'08'                                   0528
         BNE   @RF00528                                            0528
*                     LCCWEXT=CRRCLDO;/* PUT ADDR LDO IN LCCW      0529
*                                      EXTENSION AREA                */
         LA    @15,32                                              0529
         AL    @15,CRRCLCCW(,CRRPTR)                               0529
         L     @11,CRRCLDO(,CRRPTR)                                0529
         ST    @11,LCCWEXT(,@15)                                   0529
*                   ELSE                                           0530
*                     DO;           /* IF NOT A JUMP                 */
         B     @RC00528                                            0530
@RF00528 DS    0H                                                  0531
*                       DCLLDO1=CRRCLDO;/* PUT ADDR LDO IN LCPB      */
         L     @15,CRRCLDO(,CRRPTR)                                0531
         ST    @15,DCLLDO1(,LCPBPTR)                               0531
*                       CRRSW3=ON;                                 0532
         OI    CRRSW3(CRRPTR),B'00010000'                          0532
*                     END;                                         0533
*                 END;              /* END NOT-CC-OR-DC DO GROUP     */
*             END;                  /* END 3270 PROCESSING           */
*           ELSE                    /* IF DEVICE IS NOT A 3270       */
*             DO;                   /* SET ERROR CODES:              */
         B     @RC00518                                            0536
@RF00518 DS    0H                                                  0537
*               CRRFDB2=CRRCPYNS;   /* COPY TO NON-3270              */
         MVI   CRRFDB2(CRRPTR),X'26'                               0537
*               CRRRTNCD=CRRLOGIC;  /* LOGIC ERROR                   */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0538
*               CRRSW0=ON;          /* INTERNAL ERROR SWITCH         */
         OI    CRRSW0(CRRPTR),B'10000000'                          0539
*             END;                  /* END ERROR CODE SETTINGS       */
*         END;                      /* END LDO EITHER FORM OF COPY DO*/
*       ELSE                        /* IF LDO IS NOT A COPY MSG      */
*         CALL PROCLDO2;            /* OR TRANS LDO                  */
         B     @RC00516                                            0542
@RF00516 BAL   @14,PROCLDO2                                        0542
*     END;                          /* END NO ERROR(CRRSW0=OFF)DO    */
*   ELSE                                                           0544
*     ;                                                            0544
@RF00514 DS    0H                                                  0545
*   RETADDR=CRRRTN2;                /* RESTORE RETURN REG 14         */
@RC00514 L     RETADDR,CRRRTN2(,CRRPTR)                            0545
*   END PROCLDO1;                   /* RETURN TO PROCLDO             */
@EL00003 DS    0H                                                  0546
@EF00003 DS    0H                                                  0546
@ER00003 BR    @14                                                 0546
*                                                                  0547
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : PROCLDO2 - CONTINUE LDO CHECKING                  */
*/*                                                                  */
*/* FUNCTION : PROCLDO2 DOES THE FOLLOWING :                         */
*/* 1. CHECK TO SEE IF OPCODE IS EAU:                                */
*/*  A.IF IT IS EAU, CHECK TO SEE IF DEVICE IS A 3270:               */
*/*    1. IF IT'S A 3270, BUILD AN LCCW AND GO TO #2.                */
*/*    2. IF IT'S NOT, SET ERROR CODES AND GO TO #2.                 */
*/* B. IF IT'S NOT EAU, CALL PROCLDO3 SUBROUTINE TO CHECK FOR        */
*/*    WRITE NACK OR RVI: CHECK FOR AN INDICATION                    */
*/*    FROM PROCLDO3 THAT LDO IS NACK OR RVI:                        */
*/*    1. IF IT IS, CALL RDLDOCHK TO CHECK FOR A FOLLOWING READ      */
*/*       LDO AND GO TO #2.                                          */
*/*    2. IF IT ISTN'T, BUILD LCCW FROM LDO AND RETURN.              */
*/* 2. CHECK TO SEE IF ANY ERRORS HAVE BEEN DETECTED:                */
*/*   A. IF ERRORS WERE DETECTED, RETURN TO PROCLDO1.                */
*/*   B. IF NO  ERRORS, PUT LDO @ IN LCCW EXTENSION IF FIRST         */
*/*      LCCW IS AN LTIC, OR IN LCPB.                                */
*/*   RETURN TO PROCLDO1.                                            */
*/*                                                                  */
*/* INPUT : SAME AS PROCLDO1                                         */
*/*                                                                  */
*/* OUTPUT : SAME AS PROCLDO1                                        */
*/*          CRRSW3 POSSIBLY SET                                     */
*/*                                                                  */
*/* SUBROUTINES CALLED : PROCLDO3                                    */
*/*                                                                  */
*/* MACROS CALLED : NONE                                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0547
*PROCLDO2:                                                         0547
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0547
PROCLDO2 DS    0H                                                  0548
*   CRRRTN3=RETADDR;                /* SAVE RETURN ADDR (REG 14)     */
         ST    RETADDR,CRRRTN3(,CRRPTR)                            0548
*   IF LCCWRWOP=LEAU THEN                                          0549
         TM    LCCWRWOP(LCCWPTR),B'00010001'                       0549
         BNO   @RF00549                                            0549
         TM    LCCWRWOP(LCCWPTR),B'00101110'                       0549
         BNZ   @RF00549                                            0549
*     DO;                           /* IF OPCODE=ERASE ALL UNPROT    */
*       IF(DEVCHAR2>=DEV3277&DEVCHAR2<=DEV3275) THEN               0551
         LA    @15,36                                              0551
         AL    @15,FMCDNCBA(,FMCBPTR)                              0551
         CLI   DEVCHAR2(@15),X'19'                                 0551
         BL    @RF00551                                            0551
         CLI   DEVCHAR2(@15),X'1C'                                 0551
         BH    @RF00551                                            0551
*         DO;                                                      0552
*           ISTLCCW(2:8)=ISTLDO(2:8);/* COPY LDO INTO LCCW           */
         MVC   ISTLCCW+1(7,LCCWPTR),ISTLDO+1(LDOPTR)               0553
*           CRRSW3=ON;                                             0554
         OI    CRRSW3(CRRPTR),B'00010000'                          0554
*         END;                                                     0555
*       ELSE                        /* IF DEV NOT A 3270             */
*         DO;                       /* SET ERROR CODES               */
         B     @RC00551                                            0556
@RF00551 DS    0H                                                  0557
*           CRRFDB2=CRREWAU3;       /* ERASE ALL TO NON-3270 DEV     */
         MVI   CRRFDB2(CRRPTR),X'13'                               0557
*           CRRRTNCD=CRRLOGIC;      /* INDICATE 'LOGIC ERROR'        */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0558
*           CRRSW0=ON;              /* SET INTERNAL ERROR SWITCH     */
         OI    CRRSW0(CRRPTR),B'10000000'                          0559
*         END;                      /* END ERROR CODE SETTINGS       */
*     END;                                                         0561
*   ELSE                            /* IF OPCODE^= EAU               */
*     DO;                                                          0562
         B     @RC00549                                            0562
@RF00549 DS    0H                                                  0563
*       CALL PROCLDO3;              /* CHECK FOR WRITE NACK/RVI LDO
*                                      OPCODE                        */
         BAL   @14,PROCLDO3                                        0563
*       IF CRRSW3=OFF THEN          /* CRRSW3=OFF WOULD INDICATE A 0564
*                                      WRITE NACK OR RVI             */
         TM    CRRSW3(CRRPTR),B'00010000'                          0564
         BNZ   @RF00564                                            0564
*         CALL RDLDOCHK;            /* CHECK FOR READ LDO            */
         BAL   @14,RDLDOCHK                                        0565
*       ELSE                                                       0566
*         DO;                                                      0566
         B     @RC00564                                            0566
@RF00564 DS    0H                                                  0567
*           ISTLCCW(2:8)=ISTLDO(2:8);/* COPY LDO INTO LCCW           */
         MVC   ISTLCCW+1(7,LCCWPTR),ISTLDO+1(LDOPTR)               0567
*           IF LCCWOP=LWH THEN                                     0568
         CLI   LCCWOP(LCCWPTR),X'0F'                               0568
         BNE   @RF00568                                            0568
*             CRRSW3=OFF;                                          0569
         NI    CRRSW3(CRRPTR),B'11101111'                          0569
*           ELSE                                                   0570
*             ;                                                    0570
@RF00568 DS    0H                                                  0571
*         END;                                                     0571
*     END;                                                         0572
@RC00564 DS    0H                                                  0573
*   IF CRRSW0=ON THEN                                              0573
@RC00549 TM    CRRSW0(CRRPTR),B'10000000'                          0573
         BO    @RT00573                                            0573
*     ;                             /* IF ERRORS FOUND, FALL THRU    */
*   ELSE                                                           0575
*     DO;                                                          0575
*       IF LCPNXLCW->LCCWOP=LDOJUMP THEN/* IF FIRST LDO WAS A JUMP   */
         L     @15,LCPNXLCW(,LCPBPTR)                              0576
         CLI   LCCWOP(@15),X'08'                                   0576
         BNE   @RF00576                                            0576
*         LCCWEXT=CRRCLDO;          /* PUT LDO ADDR IN EXT AREA      */
         LA    @15,32                                              0577
         AL    @15,CRRCLCCW(,CRRPTR)                               0577
         L     @11,CRRCLDO(,CRRPTR)                                0577
         ST    @11,LCCWEXT(,@15)                                   0577
*       ELSE                                                       0578
*         DCLLDO1=CRRCLDO;          /* PUT LDO ADDR IN LCPB          */
         B     @RC00576                                            0578
@RF00576 L     @15,CRRCLDO(,CRRPTR)                                0578
         ST    @15,DCLLDO1(,LCPBPTR)                               0578
*     END;                                                         0579
@RC00576 DS    0H                                                  0580
*   RETADDR=CRRRTN3;                /* RESTORE RET ADDR (REG 14)     */
@RT00573 L     RETADDR,CRRRTN3(,CRRPTR)                            0580
*   END PROCLDO2;                   /* RETURN TO PROCLDO1            */
@EL00004 DS    0H                                                  0581
@EF00004 DS    0H                                                  0581
@ER00004 BR    @14                                                 0581
*                                                                  0582
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : PROCLDO3 - DETERMINE IF COMMAND CHAIN NECESSARY   */
*/*                                                                  */
*/* FUNCTION : PROCLDO3 DOES THE FOLLOWING :                         */
*/* 1. IF LDO IS A WRITE NACK OR RVI, RETURN TO PROCLDO2             */
*/* 2. IF IT'S NOT A WRITE NACK OR RVI, SET CRRSW3 ON AND            */
*/*    RETURN TO PROCLDO2.                                           */
*/*                                                                  */
*/* INPUT : LCCWPTR (REG 9) - PTR TO LCCW FOR THE LDO                */
*/*                                                                  */
*/* OUTPUT : CRRSW3 POSSIBLY SET                                     */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0582
*PROCLDO3:                                                         0582
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0582
PROCLDO3 DS    0H                                                  0583
*   CRRRTN4=RETADDR;                                               0583
         ST    RETADDR,CRRRTN4(,CRRPTR)                            0583
*   IF LCCWOP=LWNA×                 /* WRITE NACK                    */
*       LCCWOP=LWNLG×               /* WRITE NACK LDING GRAPH        */
*       LCCWOP=LWPLG×               /* WRITE ACK WITH LEADING      0584
*                                      GRAPHICS                      */
*       LCCWOP=LWAA×                /* WRITE ALTERNATE ACK           */
*       LCCWOP=LWRVI THEN                                          0584
         CLI   LCCWOP(LCCWPTR),X'2F'                               0584
         BE    @RT00584                                            0584
         CLI   LCCWOP(LCCWPTR),X'6F'                               0584
         BE    @RT00584                                            0584
         CLI   LCCWOP(LCCWPTR),X'5F'                               0584
         BE    @RT00584                                            0584
         CLI   LCCWOP(LCCWPTR),X'3F'                               0584
         BE    @RT00584                                            0584
         CLI   LCCWOP(LCCWPTR),X'1F'                               0584
         BE    @RT00584                                            0584
*     ;                             /* WRITE RVI                     */
*   ELSE                                                           0586
*     CRRSW3=ON;                                                   0586
         OI    CRRSW3(CRRPTR),B'00010000'                          0586
*   END PROCLDO3;                   /* RETURN TO PROCLDO2            */
@EL00005 DS    0H                                                  0587
@EF00005 DS    0H                                                  0587
@ER00005 BR    @14                                                 0587
*                                                                  0588
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : NOCHAIN - COMPLETE LDO PROCESSING                 */
*/*                                                                  */
*/* FUNCTION : NOCHAIN DOES THE FOLLOWING :                          */
*/* ALWAYS UPDATE THE LCCW COUNT (+1). SET LOOP SWITCH: WHEN NO      */
*/* DATA-CHAIN FLAG IS FOUND IN CHAINED LDOS, OR IF MAX (100 LDOS)   */
*/* IS REACHED, THE LOOP SWITCH WILL BE TURNED OFF AND THE LOOP      */
*/* WILL END.                                                        */
*/*                                                                  */
*/* 1. CHECK LDO FOR A DATA-CHAIN FLAG:                              */
*/*   A. IF DC, CHECK FOR MAX LDO COUNT:                             */
*/*      1. IF MAX LDO, SET ERROR CODES, TURN OFF LOOP SWITCH,       */
*/*         AND EXIT.                                                */
*/*      2. IF NOT MAX LDO, ADD 1 TO LDO COUNT AND CALL CHAINING     */
*/*         SUBROUTINE TO BUILD ANOTHER LCCW. LOOP, IF NO ERRORS     */
*/*         ARE FOUND BY CHAINING.                                   */
*/*   B. IF NOT DC, INDICATE 'INITIATE I/O', AND TURN OFF LOOP       */
*/*      SWITCH.  CHECK TO SEE IF 'CONTINUE ANY' IS REQUESTED:       */
*/*      1. IF CONTINUE ANY SW=OFF, INDICATE 'SATISFY SPECIFIC       */
*/*         READ' IN FMCB.                                           */
*/*      2. IF RPLDLGIN=ON, CHECK FOR 'WRITE LAST BLOCK IN           */
*/*         TRANSMISSION':                                           */
*/*         A. IF WLBT, TURN OFF SATISFY SPECIFIC READ SW.           */
*/*         B. IF NOT WLBT, SET SATISFY SPECIFIC READ SW.            */
*/*      IF 'END DIALOG' IS INDICATED, SET END DIALOG IN LCCW.       */
*/*      RETURN TO MAINLINE.                                         */
*/*                                                                  */
*/* INPUT : SAME AS PROCLDO                                          */
*/*         CRRSW3 MAY BE SET                                        */
*/*                                                                  */
*/* OUTPUT : FMCCMPS2 FIELD CHANGED                                  */
*/*           LCCWSD FLAG POSSIBLY SET                               */
*/*           CRRLDOCT FIELD INCREASED TO COUNT OF LDO S             */
*/*                                                                  */
*/* SUBROUTINES CALLED : CHAINING                                    */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0588
*NOCHAIN:                                                          0588
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0588
NOCHAIN  DS    0H                                                  0589
*   RFY                                                            0589
*    (OLDVALUE,                                                    0589
*     NEWVALUE,                                                    0589
*     RPLPTR) RSTD;                 /* RPLPTR WILL UNRSTD IN GETLCCW
*                                      RTN                           */
*   CRRRTN1=RETADDR;                /* SAVE RETURN ADDR (REG 14)     */
         ST    RETADDR,CRRRTN1(,CRRPTR)                            0590
*   RPLPTR=CRRARPL;                 /* GET ADDR OF RPL FROM CRR      */
         L     RPLPTR,CRRARPL(,CRRPTR)                             0591
*   CRRLCNT=CRRLCNT+1;              /* UPDATE LCCW COUNT BY ONE      */
         LA    @15,1                                               0592
         LH    @11,CRRLCNT(,CRRPTR)                                0592
         N     @11,@CF03605                                        0592
         ALR   @15,@11                                             0592
         STH   @15,CRRLCNT(,CRRPTR)                                0592
*   CRRSW1=ON;                      /* INIT DO-WHILE LOOP SWITCH     */
         OI    CRRSW1(CRRPTR),B'01000000'                          0593
*   DO WHILE(CRRSW1=ON);            /* LOOP UNTIL NO DC IS FOUND, OR
*                                      UNTIL MAXLDO (100) IS REACHED,
*                                      OR JUMP-TO-JUMP LDO IS FOUND -
*                                      NOTE: CHAINING PROC WILL TURN
*                                      OFF CRRSW1 IF ERROR IS FOUND  */
         B     @DE00594                                            0594
@DL00594 DS    0H                                                  0595
*     IF LDOACHN=ON THEN            /* IF LDO IS DATA-CHAINED        */
         TM    LDOACHN(LDOPTR),B'10000000'                         0595
         BNO   @RF00595                                            0595
*       IF CRRLDOCT>=MAXLDO THEN    /* IF LDO COUNT IS OVER 100      */
         LH    @15,CRRLDOCT(,CRRPTR)                               0596
         C     @15,@CF03485                                        0596
         BL    @RF00596                                            0596
*         DO;                       /* SET ERROR CODES,LOOP SW=0:    */
*           CRRFDB2=CRRM100;        /* MAX LDO COUNT EXCEEDED        */
         MVI   CRRFDB2(CRRPTR),X'1A'                               0598
*           CRRRTNCD=CRRLOGIC;      /* LOGIC ERROR                   */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0599
*           CRRSW0=ON;              /* INTERNAL ERROR SWITCH         */
*           CRRSW1=OFF;             /* TURN OFF DO-WHILE LOOP SW.    */
         OI    CRRSW0(CRRPTR),B'10000000'                          0601
         NI    CRRSW1(CRRPTR),B'10111111'                          0601
*         END;                      /* END ERROR SETS & LOOP EXIT    */
*       ELSE                        /* IF LDO COUNT = 100 OR LESS    */
*         DO;                       /* CALL'CHAINING'                */
         B     @RC00596                                            0603
@RF00596 DS    0H                                                  0604
*           CRRLDOCT=CRRLDOCT+1;    /* UPDATE LDO COUNT              */
         LA    @15,1                                               0604
         AH    @15,CRRLDOCT(,CRRPTR)                               0604
         STH   @15,CRRLDOCT(,CRRPTR)                               0604
*           CALL CHAINING;          /* CHAINING WILL TURN LOOP SWITCH
*                                      (CRRSW1) OFF IF ANY ERRORS ARE
*                                      FOUND. OTHERWISE, CHAINING  0605
*                                      BUILDS ANOTHER LCCW AND     0605
*                                      UPDATES PTRS AND CTRS         */
         BAL   @14,CHAINING                                        0605
*         END;                      /* END 'CALL CHAINING' DO        */
*     ELSE                          /* IF LDO IS NOT DATA-CHAINED    */
*       DO;                                                        0607
         B     @RC00595                                            0607
@RF00595 DS    0H                                                  0608
*         CRRINIO=ON;               /* SET INITIATE I/O FLAG         */
         OI    CRRINIO(CRRPTR),B'00100000'                         0608
*         CRRSW1=OFF;               /* TURN OFF LOOP SWITCH TO FALL
*                                      THROUGH                       */
*                                                                  0609
         NI    CRRSW1(CRRPTR),B'10111111'                          0609
*         /***********************************************************/
*         /*                                                         */
*         /* THE FIRST OPERATION FLAG IN THE FMCB MUST BE CHECKED TO */
*         /* DETER- IF THIS IS THE FIRST OPERATION GOING OUT TO THE  */
*         /* DEVICE IF THIS IS A GENERAL POLL DEVICE . IF THE FIRST  */
*         /* OPERATION , RESET THE FLAG INDICATING THAT THIS WRITE   */
*         /* HAS GONE OUT.                                           */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0610
*         IF(DEVTCODE>=DEV3277&DEVTCODE<=DEV3275)&FMCFLGB7=OFF THEN
         LA    @15,36                                              0610
         AL    @15,FMCDNCBA(,FMCBPTR)                              0610
         CLI   DEVTCODE(@15),X'19'                                 0610
         BL    @RF00610                                            0610
         CLI   DEVTCODE(@15),X'1C'                                 0610
         BH    @RF00610                                            0610
         TM    FMCFLGB7(FMCBPTR),B'00000001'                       0610
         BNZ   @RF00610                                            0610
*           DO;                                                    0611
*             OLDVALUE=FMCCMPS2;    /* GET FMCCMPS2 VAL.     @XM10597*/
         L     OLDVALUE,FMCCMPS2(,FMCBPTR)                         0612
*CFLAGB7:                                                          0613
*             NEWVALUE=OLDVALUE×FLGB7ON;/* CHANGE B7 FLAG    @XM10597*/
CFLAGB7  LR    NEWVALUE,OLDVALUE                                   0613
         O     NEWVALUE,@CF02400                                   0613
*             CS(OLDVALUE,NEWVALUE,FMCCMPS2);/* CHANGE FMCCMPS2    0614
*                                                            @XM10597*/
         CS    OLDVALUE,@05,FMCCMPS2(FMCBPTR)                      0614
*             BC(7,CFLAGB7);        /* FAILED ,RETRY         @XM10597*/
         BC    7,CFLAGB7                                           0615
*           END;                    /* END FIRST OPER.DO     @XM10597*/
*         ELSE                                                     0617
*           ;                       /* NOT 3270 OR 1ST       @XM10597*/
@RF00610 DS    0H                                                  0618
*         IF RPLDLGIN=OFF&          /* IF CONTINUE ANY IS INDICATED
*                                      IN RPL                        */
*             LCCWRWOP=LWT THEN     /* IF LDO OPCODE IS WRITE-LAST-
*                                      BLOCK-IN-TRANSMISSION         */
         TM    RPLDLGIN(RPLPTR),B'10000000'                        0618
         BNZ   @RF00618                                            0618
         TM    LCCWRWOP(LCCWPTR),B'00001101'                       0618
         BNO   @RF00618                                            0618
         TM    LCCWRWOP(LCCWPTR),B'00110010'                       0618
         BNZ   @RF00618                                            0618
*           DO;                     /* TURN THE 'SATISFY SPECIFIC    */
*             OLDVALUE=FMCCMPS2;    /* READ FLAG OFF IN THE FMCB     */
         L     OLDVALUE,FMCCMPS2(,FMCBPTR)                         0620
*CSWAP2:                                                           0621
*             NEWVALUE=OLDVALUE&FLAG1OFF;                          0621
CSWAP2   LR    NEWVALUE,OLDVALUE                                   0621
         N     NEWVALUE,@CF02507                                   0621
*             CS(OLDVALUE,NEWVALUE,FMCCMPS2);                      0622
         CS    OLDVALUE,@05,FMCCMPS2(FMCBPTR)                      0622
*             BC(7,CSWAP2);                                        0623
         BC    7,CSWAP2                                            0623
*           END;                                                   0624
*         ELSE                                                     0625
*           DO;                     /* TURN 'SATISFY SPECIFIC        */
         B     @RC00618                                            0625
@RF00618 DS    0H                                                  0626
*             OLDVALUE=FMCCMPS2;    /* READ' ON IN THE FMCB          */
         L     OLDVALUE,FMCCMPS2(,FMCBPTR)                         0626
*CSWAP3:                                                           0627
*             NEWVALUE=OLDVALUE×FLAG1ON;                           0627
CSWAP3   LR    NEWVALUE,OLDVALUE                                   0627
         O     NEWVALUE,@CF02374                                   0627
*             CS(OLDVALUE,NEWVALUE,FMCCMPS2);                      0628
         CS    OLDVALUE,@05,FMCCMPS2(FMCBPTR)                      0628
*             BC(7,CSWAP3);                                        0629
         BC    7,CSWAP3                                            0629
*           END;                                                   0630
*       END;                                                       0631
@RC00618 DS    0H                                                  0632
*   END;                                                           0632
@RC00595 DS    0H                                                  0632
@DE00594 TM    CRRSW1(CRRPTR),B'01000000'                          0632
         BO    @DL00594                                            0632
*   RETADDR=CRRRTN1;                /* RESTORE RETURN ADDR (R14)     */
         L     RETADDR,CRRRTN1(,CRRPTR)                            0633
*   END NOCHAIN;                    /* RETURN TO MAINLINE            */
@EL00006 DS    0H                                                  0634
@EF00006 DS    0H                                                  0634
@ER00006 BR    @14                                                 0634
*   RFY                                                            0635
*    (OLDVALUE,                                                    0635
*     NEWVALUE) UNRSTD;                                            0635
*                                                                  0636
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : CHAINING - PROCESS DATA CHAINED LDO S             */
*/*                                                                  */
*/* FUNCTION : CHAINING DOES THE FOLLOWING :                         */
*/* 1. ALWAYS: UPDATE LDO PTR IN CRR                                 */
*/*            UPDATE THE BASE REG FOR LDO                           */
*/*            UPDATE THE LCCW POINTER                               */
*/*            UPDATE THE LCCW BASE REG                              */
*/*            AND CHECK LDO TO SEE IF IT'S A JUMP:                  */
*/*            A. IF IT'S A JUMP, CHECK FOR A JUMP-TO-JUMP:          */
*/*                  1. IF JUMP-TO-JUMP, SET ERROR CODES AND GO TO   */
*/*                     STEP #2.                                     */
*/*                  2. IF NOT JUMP-TO-JUMP, CHECK TO SEE IF LDO     */
*/*                     EXCEEDS MAXIMUM (COUNT > 100):               */
*/*                     A. IF MAX IS EXCEEDED, SET ERROR CODES AND   */
*/*                        GO TO STEP #2.                            */
*/*                     B. IF MAX NOT EXCEEDED, UPDATE LDO POINTER,  */
*/*                        UPDATE THE LDO BASE, AND GO TO STEP #2.   */
*/* 2. CHECK FOR ERRORS PREVIOUSLY SET:                              */
*/*   A. IF ERRORS WERE DETECTED, TURN OFF LOOP SWITCH TO CAUSE      */
*/*      NOCHAIN TO END ITS LOOP, AND RETURN TO NOCHAIN.             */
*/*   B. IF NO ERRORS, CALL GETLCCW TO GET LCCW EXTENSION AREA       */
*/*      IF NEEDED.  THEN BUILD LCCW FROM CURRENT LDO, UPDATE THE    */
*/*      LDO COUNT, AND PUT THE LDO ADDRESS IN THE LCCW EXT. AREA.   */
*/*                                                                  */
*/* INPUT : CRRLDO - POINTER TO CURRENT LDO                          */
*/*        CRRLCCW - POINTER TO CURRENT LCCW                         */
*/*                                                                  */
*/* OUTPUT : CRRLDO POINTER UPDATED BY 8                             */
*/*           CRRLCCW UPDATED BY 8                                   */
*/*           CRRSW0 POSSIBLY SET ( ERROR SWITCH )                   */
*/*                                                                  */
*/* SUBROUTINES CALLED : GETLCCW                                     */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0636
*CHAINING:                                                         0636
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0636
CHAINING DS    0H                                                  0637
*   RFY                                                            0637
*     LASTLCCW RSTD;                                               0637
*   CRRRTN2=RETADDR;                /* SAVE RETURN ADDR - REG 14     */
         ST    RETADDR,CRRRTN2(,CRRPTR)                            0638
*   CRRCLDO=CRRCLDO+8;              /* UPDATE LDO PTR IN CRR         */
         LA    @15,8                                               0639
         L     @00,CRRCLDO(,CRRPTR)                                0639
         ALR   @00,@15                                             0639
         ST    @00,CRRCLDO(,CRRPTR)                                0639
*   LDOPTR=CRRCLDO;                 /* UPDATE LDO BASE REG           */
         LR    LDOPTR,@00                                          0640
*   LASTLCCW=CRRCLCCW;              /* SAVE ADDRESS FOR INSERTING OP
*                                      CODES IN DATA CHAINED LCCWS   */
         L     @05,CRRCLCCW(,CRRPTR)                               0641
         LR    LASTLCCW,@05                                        0641
*   CRRCLCCW=CRRCLCCW+8;            /* UPDATE LCCW PTR IN CRR        */
         ALR   @05,@15                                             0642
         ST    @05,CRRCLCCW(,CRRPTR)                               0642
*   LCCWPTR=CRRCLCCW;               /* UPDATE LCCW BASE REG          */
         LR    LCCWPTR,@05                                         0643
*   IF LDOCMD=LDOJUMP THEN          /* IF CURRENT LDO IS A'JUMP'     */
         CLI   LDOCMD(LDOPTR),X'08'                                0644
         BNE   @RF00644                                            0644
*     IF LDOADDR->LDOCMD=LDOJUMP THEN/* IF LDO JUMPS TO A JUMP LDO   */
         L     @15,LDOADDR(,LDOPTR)                                0645
         CLI   LDOCMD(@15),X'08'                                   0645
         BNE   @RF00645                                            0645
*       DO;                         /* SET ERROR CODES:              */
*         CRRFDB2=CRRJTOJ;          /* JUMP LDO TO JUMP LDO          */
         MVI   CRRFDB2(CRRPTR),X'19'                               0647
*         CRRRTNCD=CRRLOGIC;        /* LOGIC ERROR                   */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0648
*         CRRSW0=ON;                /* INTERNAL ERROR SWITCH         */
         OI    CRRSW0(CRRPTR),B'10000000'                          0649
*       END;                        /* END ERROR CODE SETTINGS       */
*     ELSE                          /* IF LDO JUMPS TO A NON-JUMP    */
*       DO;                                                        0651
         B     @RC00645                                            0651
@RF00645 DS    0H                                                  0652
*         IF CRRLDOCT>=MAXLDO THEN  /* IF LDO COUNT EXCEEDS 100      */
         LH    @15,CRRLDOCT(,CRRPTR)                               0652
         C     @15,@CF03485                                        0652
         BL    @RF00652                                            0652
*           DO;                     /* SET ERROR CODES:              */
*             CRRFDB2=CRRM100;      /* LDO MAX EXCEEDED              */
         MVI   CRRFDB2(CRRPTR),X'1A'                               0654
*             CRRRTNCD=CRRLOGIC;    /* LOGIC ERROR                   */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0655
*             CRRSW0=ON;            /* INTERNAL ERROR SWITCH         */
         OI    CRRSW0(CRRPTR),B'10000000'                          0656
*           END;                    /* END ERROR SETTING             */
*         ELSE                      /* IF LDO COUNT ISN'T EXESIVE    */
*           DO;                                                    0658
         B     @RC00652                                            0658
@RF00652 DS    0H                                                  0659
*             CRRCLDO=LDOADDR;      /* UPDATE LDO POINTER            */
         L     @15,LDOADDR(,LDOPTR)                                0659
         ST    @15,CRRCLDO(,CRRPTR)                                0659
*             LDOPTR=CRRCLDO;       /* UPDATE LDO BASE REG           */
         LR    LDOPTR,@15                                          0660
*           END;                                                   0661
*       END;                        /* END LDOJ-TO-NONLDOJ           */
@RC00652 DS    0H                                                  0663
*   IF CRRSW0=ON THEN               /* IF AN ERROR WAS FOUND         */
@RC00645 DS    0H                                                  0663
@RF00644 TM    CRRSW0(CRRPTR),B'10000000'                          0663
         BNO   @RF00663                                            0663
*     CRRSW1=OFF;                   /* TURN OFF NOCHAIN LOOP SW      */
         NI    CRRSW1(CRRPTR),B'10111111'                          0664
*   ELSE                            /* IF NO ERRORS WERE FOUND       */
*     DO;                                                          0665
         B     @RC00663                                            0665
@RF00663 DS    0H                                                  0666
*       CALL GETLCCW;               /* GET STORAGE FOR LCCW IF NEEDED
*                                      & BUILD LTIC LCCW             */
         BAL   @14,GETLCCW                                         0666
*       ISTLCCW(2:8)=ISTLDO(2:8);   /* COPY LDO INTO LCCW            */
         MVC   ISTLCCW+1(7,LCCWPTR),ISTLDO+1(LDOPTR)               0667
*       LCCWOP=LASTLCCW->LCCWOP;    /* PROPAGATE COMMAND CODE        */
         MVC   LCCWOP(1,LCCWPTR),LCCWOP(LASTLCCW)                  0668
*       CRRLCNT=CRRLCNT+1;          /* ADD 1 TO LCCW COUNT           */
         LA    @15,1                                               0669
         LH    @00,CRRLCNT(,CRRPTR)                                0669
         N     @00,@CF03605                                        0669
         ALR   @15,@00                                             0669
         STH   @15,CRRLCNT(,CRRPTR)                                0669
*       LCCWEXT=CRRCLDO;            /* PUT CURRENT LDO ADDR IN LCCW
*                                      EXTENSION AREA                */
         LA    @15,32                                              0670
         AL    @15,CRRCLCCW(,CRRPTR)                               0670
         L     @00,CRRCLDO(,CRRPTR)                                0670
         ST    @00,LCCWEXT(,@15)                                   0670
*     END;                                                         0671
*   RETADDR=CRRRTN2;                /* RESTORE RETURN ADDR           */
@RC00663 L     RETADDR,CRRRTN2(,CRRPTR)                            0672
*   END CHAINING;                   /* RETURN TO NOCHAIN             */
@EL00007 DS    0H                                                  0673
@EF00007 DS    0H                                                  0673
@ER00007 BR    @14                                                 0673
