*RDLDOCHK:                                                         0900
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0900
RDLDOCHK DS    0H                                                  0901
*   CRRRTN4=RETADDR;                /* SAVE RETURN ADDR (REG 14)     */
         ST    RETADDR,CRRRTN4(,CRRPTR)                            0901
*   IF LDOACHN=ON THEN              /* IF LDO IS DATA-CHAINED        */
         TM    LDOACHN(LDOPTR),B'10000000'                         0902
         BNO   @RF00902                                            0902
*     DO;                                                          0903
*       CRRSW2=ON;                  /* INIT SW FOR DO-WHILE          */
         OI    CRRSW2(CRRPTR),B'00100000'                          0904
*       DO WHILE(CRRSW2=ON);        /* DO UNTIL ERROR OR NON-DC      */
         B     @DE00905                                            0905
@DL00905 DS    0H                                                  0906
*         CRRSW2=OFF;               /* TURN OFF DO-WHILE LOOP SWITCH
*                                      - WILL BE TURNED ON TO LOOP 0906
*                                      THROUGH DO-WHILE UNTIL A    0906
*                                      NON-DATA-CHAINED LDO IS FOUND */
         NI    CRRSW2(CRRPTR),B'11011111'                          0906
*         LDOPTR=LDOPTR+8;          /* ESTABLISH ADDRESSABILITY FOR
*                                      THE NEXT LDO                  */
         AL    LDOPTR,@CF00059                                     0907
*         IF LDOCMD=LDOJUMP THEN    /* IF LDO IS A JUMP LDO          */
         CLI   LDOCMD(LDOPTR),X'08'                                0908
         BNE   @RF00908                                            0908
*           IF LDOADDR->LDOCMD=LDOJUMP THEN/* IF NEXT LDO IS A JUMP
*                                      LDO                           */
         L     @15,LDOADDR(,LDOPTR)                                0909
         CLI   LDOCMD(@15),X'08'                                   0909
         BNE   @RF00909                                            0909
*             DO;                   /* SET ERROR CODES:              */
*               CRRFDB2=CRRJTOJ;    /* JUMP LDO TO JUMP LDO          */
         MVI   CRRFDB2(CRRPTR),X'19'                               0911
*               CRRRTNCD=CRRLOGIC;  /* LOGIC ERROR                   */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0912
*               CRRSW0=ON;          /* INTERNAL ERROR SWITCH         */
         OI    CRRSW0(CRRPTR),B'10000000'                          0913
*             END;                  /* END ERROR SETTINGS            */
*           ELSE                    /* IF NOT JUMP TO JUMP,          */
*             LDOPTR=LDOADDR;       /* ESTAB ADDR FOR JUMPED- TO LDO */
         B     @RC00909                                            0915
@RF00909 L     LDOPTR,LDOADDR(,LDOPTR)                             0915
*         IF CRRSW0=ON THEN                                        0916
@RC00909 DS    0H                                                  0916
@RF00908 TM    CRRSW0(CRRPTR),B'10000000'                          0916
         BO    @RT00916                                            0916
*           ;                       /* IF ERRORS HAVE BEEN FOUND,  0917
*                                      FALL THROUGH THIS DO-WHILE    */
*         ELSE                      /* IF NO ERRORS WERE FOUND       */
*           DO;                                                    0918
*             IF LDOACHN=ON THEN    /* IF THE NEXT LDO IS DATA-      */
         TM    LDOACHN(LDOPTR),B'10000000'                         0919
         BNO   @RF00919                                            0919
*               CRRSW2=ON;          /* CHAINED, KEEP LOOPING UNTIL 0920
*                                      NON-DC LDO IS FOUND           */
         OI    CRRSW2(CRRPTR),B'00100000'                          0920
*             ELSE                                                 0921
*               DO;                 /* CHECK FOR COMMAND CHAINED LDO */
         B     @RC00919                                            0921
@RF00919 DS    0H                                                  0922
*                 IF LDOCMHN=ON THEN                               0922
         TM    LDOCMHN(LDOPTR),B'01000000'                         0922
         BO    @RT00922                                            0922
*                   ;               /* IF NEXT LDO IS COMMAND      0923
*                                      CHAINED, FALL THROUGH         */
*                 ELSE              /* IF LDO ISN'T DC OR COMMAND    */
*                   DO;             /* CHAINED,SET ERRORS:           */
*                     CRRFDB2=CRRWANCR;/* WRITE NOT CHAINED TO READ  */
         MVI   CRRFDB2(CRRPTR),X'08'                               0925
*                     CRRRTNCD=CRRLOGIC;/* LOGIC ERROR               */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0926
*                     CRRSW0=ON;    /* INTERNAL ERROR SWITCH         */
         OI    CRRSW0(CRRPTR),B'10000000'                          0927
*                   END;            /* END ERROR SETTINGS            */
*               END;                /* END CHECK FOR CC DO           */
@RT00922 DS    0H                                                  0930
*           END;                    /* END NO-ERRORS-FOUND DO        */
@RC00919 DS    0H                                                  0931
*       END;                        /* END OF DO-WHILE:LOOP IF CRRSW2
*                                      WAS TURNED ON WHEN ANOTHER DC
*                                      LDO WAS FOUND                 */
@RT00916 DS    0H                                                  0931
@DE00905 TM    CRRSW2(CRRPTR),B'00100000'                          0931
         BO    @DL00905                                            0931
*     END;                          /* END LDO-IS-DATA-CHAINED DO    */
*   ELSE                            /* IF DC WAS NOT FOUND IN LDO, 0933
*                                      CHECK FOR CC - IF CC IS NOT 0933
*                                      FOUND, SET ERROR CODES:       */
*     DO;                                                          0933
         B     @RC00902                                            0933
@RF00902 DS    0H                                                  0934
*       IF LDOCMHN=ON THEN          /* IF LDO IS COMMAND CHAINED,  0934
*                                      ESTABLISH ADDRES- SABILITY FOR
*                                      NEXT LDO AND FALL THROUGH     */
         TM    LDOCMHN(LDOPTR),B'01000000'                         0934
         BNO   @RF00934                                            0934
*         LDOPTR=LDOPTR+8;          /* POINT LDO BASE TO NEXT LDO    */
         AL    LDOPTR,@CF00059                                     0935
*       ELSE                                                       0936
*         DO;                       /* SET ERROR CODES               */
         B     @RC00934                                            0936
@RF00934 DS    0H                                                  0937
*           CRRFDB2=CRRWANCR;       /* WRITE NOT CHAINED TO READ     */
         MVI   CRRFDB2(CRRPTR),X'08'                               0937
*           CRRRTNCD=CRRLOGIC;      /* LOGIC ERROR                   */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0938
*           CRRSW0=ON;              /* INTERNAL ERROR SWITCH         */
         OI    CRRSW0(CRRPTR),B'10000000'                          0939
*         END;                      /* END ERROR SETTINGS            */
*     END;                          /* END CHECK FOR CC DO           */
@RC00934 DS    0H                                                  0942
*   IF CRRSW0=ON THEN                                              0942
@RC00902 TM    CRRSW0(CRRPTR),B'10000000'                          0942
         BO    @RT00942                                            0942
*     ;                             /* IF ERRORS WERE FOUND, FALL  0943
*                                      THRU TO END OF PROCEDURE      */
*   ELSE                            /* IF NO ERRORS,CONTINUE         */
*     DO;                                                          0944
*       IF LDOCMD=LDOREAD THEN                                     0945
         CLI   LDOCMD(LDOPTR),X'02'                                0945
         BNE   @RF00945                                            0945
*         DO;                       /* IF LDO IS A READ              */
*           ISTLCCW(2:8)=CRRCLDO->ISTLDO(2:8);/* MOVE WRITE LDO TO 0947
*                                      LCCW                          */
         L     @15,CRRCLDO(,CRRPTR)                                0947
         MVC   ISTLCCW+1(7,LCCWPTR),ISTLDO+1(@15)                  0947
*/*   CODE ADDED FOR DCR 1939 & ZTR 6471                             */
*                                                                  0948
*           IF PROSETM4^=PROSBLCK THEN                             0948
         CLI   PROSETM4+48(FMCBPTR),X'01'                          0948
         BE    @RF00948                                            0948
*             DO;                   /* THIS IS NOT A READ BLOCK      */
*               CRRRTNCD=CRRLOGIC;  /* SET LOGIC ERROR               */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0950
*               CRRFDB2=CRRWANCR;   /* WRITE NOT CHAINED TO READ     */
         MVI   CRRFDB2(CRRPTR),X'08'                               0951
*               CRRSW0=ON;          /* TURN ON ERROR SWITCH          */
         OI    CRRSW0(CRRPTR),B'10000000'                          0952
*             END;                  /* NOT A READ BLOCK              */
*         END;                      /* END OF CHECK FOR READ         */
*       ELSE                        /* IF LDO IS NOT A READ          */
*         DO;                                                      0955
         B     @RC00945                                            0955
@RF00945 DS    0H                                                  0956
*           IF LDOCMD=LDOJUMP THEN  /* THIS A JUMP LDO ?             */
         CLI   LDOCMD(LDOPTR),X'08'                                0956
         BNE   @RF00956                                            0956
*             IF LDOADDR->LDOCMD=LDOREAD THEN                      0957
         L     @15,LDOADDR(,LDOPTR)                                0957
         CLI   LDOCMD(@15),X'02'                                   0957
         BNE   @RF00957                                            0957
*               DO;                 /* IF JUMP LDO POINTS TO A READ
*                                      LDO MOVE LDO TO LCCW          */
*                 ISTLCCW(2:8)=CRRCLDO->ISTLDO(2:8);               0959
*                                                                  0959
         L     @15,CRRCLDO(,CRRPTR)                                0959
         MVC   ISTLCCW+1(7,LCCWPTR),ISTLDO+1(@15)                  0959
*/*   CODE ADDED FOR DCR 1939 & ZTR 6471                             */
*                                                                  0960
*                 IF PROSETM4^=PROSBLCK THEN                       0960
         CLI   PROSETM4+48(FMCBPTR),X'01'                          0960
         BE    @RF00960                                            0960
*                   DO;             /* THIS IS NOT A READ BLOCK      */
