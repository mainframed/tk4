         MVI   CRRRTNCD(CRRPTR),X'08'                              0962
*                     CRRFDB2=CRRWANCR;/* WRITE NOT CHAINED TO READ  */
         MVI   CRRFDB2(CRRPTR),X'08'                               0963
*                     CRRSW0=ON;    /* TURN ON ERROR SWITCH          */
         OI    CRRSW0(CRRPTR),B'10000000'                          0964
*                   END;            /* NOT A READ BLOCK              */
*               END;                /* END OF CHECK FOR READ         */
*             ELSE                  /* IF JUMP DOES NOT POINT TO     */
*               DO;                 /* A READ, SET ERROR CODES:      */
         B     @RC00957                                            0967
@RF00957 DS    0H                                                  0968
*                 CRRFDB2=CRRWANCR; /* WRITE NOT FOLLOWED BY READ    */
         MVI   CRRFDB2(CRRPTR),X'08'                               0968
*                 CRRRTNCD=CRRLOGIC;/* LOGIC ERROR                   */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0969
*                 CRRSW0=ON;        /* INTERNAL ERROR SWITCH         */
         OI    CRRSW0(CRRPTR),B'10000000'                          0970
*               END;                /* END ERROR SETTINGS            */
*           ELSE                    /* IF NOT A JUMP LDO             */
*             DO;                   /* SET ERROR CODES:              */
         B     @RC00956                                            0972
@RF00956 DS    0H                                                  0973
*               CRRFDB2=CRRWANCR;   /* WRITE NOT CHAINED TO READ     */
         MVI   CRRFDB2(CRRPTR),X'08'                               0973
