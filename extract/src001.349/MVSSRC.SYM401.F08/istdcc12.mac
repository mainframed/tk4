         LA    @15,32                                              0486
         AL    @15,CRRCLCCW(,CRRPTR)                               0486
         L     @11,CRRCLDO(,CRRPTR)                                0486
         ST    @11,LCCWEXT(,@15)                                   0486
*                   ELSE            /* IF LCCW IS NOT JUMP           */
*                     DCLLDO1=CRRCLDO;/* PUT ADDR LDO IN LCP         */
         B     @RC00485                                            0487
@RF00485 L     @15,CRRCLDO(,CRRPTR)                                0487
         ST    @15,DCLLDO1(,LCPBPTR)                               0487
*                   CRRSW3=ON;                                     0488
@RC00485 OI    CRRSW3(CRRPTR),B'00010000'                          0488
*                 END;              /* END DEV IS 3270 DO-GROUP      */
*               ELSE                /* IF DEV IS NOT 3270, SET       */
*                 DO;               /* ERROR CODES IN CRR:           */
         B     @RC00481                                            0490
@RF00481 DS    0H                                                  0491
*                   CRRFDB2=CRREWNS;/* ER/WT TO NON-2265/3270        */
         MVI   CRRFDB2(CRRPTR),X'12'                               0491
*                   CRRRTNCD=CRRLOGIC;/* INDICATE'LOGIC ERROR'       */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0492
