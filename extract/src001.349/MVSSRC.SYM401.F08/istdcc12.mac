*       CRRFDB2=CRRRDNFO;                                          0467
         MVI   CRRFDB2(CRRPTR),X'28'                               0467
*       CRRSW0=ON;                                                 0468
         OI    CRRSW0(CRRPTR),B'10000000'                          0468
*     END;                                                         0469
*   ELSE                                                           0470
*     IF CRRFLDOS=ON THEN                                          0470
         B     @RC00464                                            0470
@RF00464 TM    CRRFLDOS(CRRPTR),B'01000000'                        0470
         BNO   @RF00470                                            0470
*       CALL FIRSTLDO;                                             0471
         BAL   @14,FIRSTLDO                                        0471
*     ELSE                                                         0472
*       ;                                                          0472
@RF00470 DS    0H                                                  0473
*   IF CRRSW0=ON THEN                                              0473
@RC00464 TM    CRRSW0(CRRPTR),B'10000000'                          0473
         BO    @RT00473                                            0473
