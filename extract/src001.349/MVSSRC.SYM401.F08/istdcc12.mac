*             END;                  /* END DEV IS NOT 2265 DO        */
*         END;                      /* END ER/WT OPCODE DO           */
*       ELSE                        /* IF LDO DOES NOT CONTAIN AN  0497
*                                      ERASE-WRITE OPCODE            */
*         DO;                                                      0497
         B     @RC00476                                            0497
@RF00476 DS    0H                                                  0498
*           CALL GETLCCW;           /* GET STORAGE FOR LCCWS IF    0498
*                                      NEEDED & BUILD LCCWS          */
         BAL   @14,GETLCCW                                         0498
