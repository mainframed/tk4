*CDE00A:                            /* RBSTAB1 = '0-'                */
*         CDEPTR=RBCDE1;            /* GET CDE POINTER       @YM03657*/
CDE00A   L     @09,RBCDE1-1(,RBPTR)                                0146
         LA    @09,0(,@09)                                         0146
         LR    CDEPTR,@09                                          0146
