*                                                            @ZA09668*/
         B     @RC00147                                            0149
@RF00147 MVC   PICDENAM(8,AWSAPTR),@CC02967                        0149
*         GO TO ENDCD;                                             0150
         B     ENDCD                                               0150
*CDE40A:                            /* RBSTAB1 = '4-'                */
*         PICDENAM=IRBCDE;          /* CDENAME='**IRB***'            */
CDE40A   MVC   PICDENAM(8,AWSAPTR),@CC02977                        0151
*         GO TO ENDCD;                                             0152
         B     ENDCD                                               0152
*CDE80A:                            /* RBSTAB1 = '8-'                */
*         PICDENAM=RBEXRTNM;        /* CDENAME=ERROR EXIT ROUTINE    */
CDE80A   MVC   PICDENAM(8,AWSAPTR),RBEXRTNM(RBPTR)                 0153
*         IF PINMCHK=0 THEN         /* CHECK FOR 1ST CHAR OF NAME=0  */
         CLI   PINMCHK(AWSAPTR),0                                  0154
         BNE   @RF00154                                            0154
