*         GO TO ENDCD;                                             0173
         B     ENDCD                                               0173
*CDEERRA:                           /* RBSTAB1 NE 0,4,8,C,D          */
*         PICDENAM=ERRIND;          /* CDENAME = '********'          */
CDEERRA  MVC   PICDENAM(8,AWSAPTR),@CC02979                        0174
