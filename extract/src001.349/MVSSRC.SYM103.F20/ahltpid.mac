         STH   @09,PIINT(,AWSAPTR)                                 0128
*       END;                                                       0129
*     IF LCCASRBM=ON THEN           /* CHECK FOR SRB MODE            */
@RC00117 TM    LCCASRBM(LCCAPTR),B'10000000'                       0130
         BNO   @RF00130                                            0130
*       DO;                                                        0131
*         PIINTTCB=NOTAPP;          /* SET INT TCB @ = 'N/A'         */
         MVC   PIINTTCB(4,AWSAPTR),@CC02967                        0132
*         PICDENAM=NOTAPP;          /* SET CDE NAME = 'N/A'          */
         MVC   PICDENAM(8,AWSAPTR),@CC02967                        0133
*       END;                                                       0134
*     ELSE                                                         0135
*       DO;                                                        0135
         B     @RC00130                                            0135
@RF00130 DS    0H                                                  0136
