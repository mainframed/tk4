*         IF RBCDE1=0 THEN          /* IF CDE ADDR = 0               */
CDEC0A   L     @09,RBCDE1-1(,RBPTR)                                0157
         LA    @09,0(,@09)                                         0157
         LTR   @09,@09                                             0157
         BNZ   @RF00157                                            0157
*           PICDENAM=SVCT2;         /* CDENAME = 'SVC-T2 '           */
         MVC   PICDENAM(8,AWSAPTR),@CC02971                        0158
*         ELSE                                                     0159
*           DO;                                                    0159
         B     @RC00157                                            0159
@RF00157 DS    0H                                                  0160
