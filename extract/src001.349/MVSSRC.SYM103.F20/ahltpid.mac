         LA    @09,0(,@09)                                         0165
         LTR   @09,@09                                             0165
         BNZ   @RF00165                                            0165
*           PICDENAM=SVCRES;        /* CDENAME= 'SVC-RES '           */
         MVC   PICDENAM(8,AWSAPTR),@CC02973                        0166
