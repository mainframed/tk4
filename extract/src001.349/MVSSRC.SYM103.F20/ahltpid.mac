*     /*                                                             */
*     /***************************************************************/
*                                                                  0114
*     PIASCB=ASCBPTR;               /* ASCB ADDRESS                  */
@RF00111 ST    ASCBPTR,PIASCB(,AWSAPTR)                            0114
*     PICPUID=PSACPUSA;             /* CPU ID                @YM03489*/
         LH    @09,PSACPUSA                                        0115
         STH   @09,PICPUID(,AWSAPTR)                               0115
*     LCCAPTR=PSALCCAV;             /* GET ADDRESS OF LCCA           */
         L     LCCAPTR,PSALCCAV                                    0116
*     IF MCREID=IEAPINT THEN        /* CHECK EID TYPE                */
         L     @09,MCAMCR(,AWSAPTR)                                0117
         CLC   MCREID(2,@09),@CH01378                              0117
         BNE   @RF00117                                            0117
*       DO;                                                        0118
*         PIREGS=REGSLCCA;          /* REGS 0-15 AT INTERRUPT        */
         MVC   PIREGS(64,AWSAPTR),REGSLCCA(LCCAPTR)                0119
*         PIVPA=LCCAPVAD;           /* GET VIRTUAL PAGE ADDRESS      */
         MVC   PIVPA(4,AWSAPTR),LCCAPVAD(LCCAPTR)                  0120
*         PIPSW=LCCAPPSW;           /* GET PSW                       */
         MVC   PIPSW(8,AWSAPTR),LCCAPPSW(LCCAPTR)                  0121
*         PIINT=INTCODE;            /* OVERLAY PSW WITH INTERRUPT  0122
