*     /***************************************************************/
*     /*                                                             */
*     /* ENSURE VALID ENTRY                                          */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0108
*     IF FLCPICOD=K40X THEN         /* IGNORE ALL BUT BRANCH ENTRY   */
         CLC   FLCPICOD(2),@CH02932                                0108
         BE    @RT00108                                            0108
*       GO TO PIDEXIT;                                             0109
*     ASCBPTR=PSAAOLD;              /* GET POINTER TO ASCB           */
         L     ASCBPTR,PSAAOLD                                     0110
