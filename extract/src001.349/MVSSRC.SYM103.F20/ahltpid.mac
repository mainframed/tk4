         SLR   @09,@09                                             0179
         CR    R8WORK,@09                                          0179
         BNE   @RF00179                                            0179
*       DO;                                                        0180
*         R8WORK=ASCBJBNS;          /* IF SO GET SECOND JOBNAME @    */
         L     R8WORK,ASCBJBNS(,ASCBPTR)                           0181
*         RFY                                                      0182
*          (ASCBPTR) UNRSTD;                                       0182
*         IF R8WORK=0 THEN          /* CHECK FOR ADDRESS OF 0        */
         CR    R8WORK,@09                                          0183
         BE    @RT00183                                            0183
