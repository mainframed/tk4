         MVC   PIINT(2,AWSAPTR),INTCODE(LCCAPTR)                   0122
*       END;                                                       0123
*     ELSE                                                         0124
*       DO;                                                        0124
         B     @RC00117                                            0124
@RF00117 DS    0H                                                  0125
*         PIREGS=TINTREGS;          /* REGS 0-15 AT INTERRUPT        */
         MVC   PIREGS(64,AWSAPTR),TINTREGS(LCCAPTR)                0125
