*                                                                  0441
*WAIT:                                                             0441
*   AFWAIT=ON;                                                     0441
WAIT     OI    AFWAIT(PCTPTR),B'00000100'                          0441
*   IOECBADD=NEXTCHK;               /* BECAUSE THE POSTING OF      0442
*                                      MULTIPLE BSAM WRITES MIGHT NOT
*                                      OCCURR IN THE SAME SEQUENCE AS
*                                      THE WRITES, ONLY ONE IS WAITED
*                                      ON AT A TIME. THIS FORCES THEM
*                                      TO BE NOTICED IN ORDER.       */
         ST    NEXTCHK,IOECBADD                                    0442
*   DO;                             /* WAIT ECBLIST(ECBALIST)        */
*     RESPECIFY                                                    0444
*      (GPR00P,                                                    0444
*       GPR01P) RESTRICTED;                                        0444
*     GPR00P=1;                     /* LOAD WAIT COUNT               */
         LA    GPR00P,1                                            0445
*     GPR01P=-ADDR(ECBALIST);       /* ADDRESS OF LIST OF ECB'S      */
         LA    GPR01P,ECBALIST                                     0446
         LCR   GPR01P,GPR01P                                       0446
*     SVC(1);                       /* WAIT SVC                      */
         SVC   1                                                   0447
*     RESPECIFY                                                    0448
*      (GPR00P,                                                    0448
*       GPR01P) UNRESTRICTED;                                      0448
*   END;                            /* WAIT ECBLIST(ECBALIST)        */
*   AFWAIT=OFF;                                                    0450
