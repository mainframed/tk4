         AR    MSGPTR,I                                            0061
*   DATASW=1;                       /* SET SW TO ISSUE MSG           */
         LA    DATASW,1                                            0062
*   WTOLEN=WTOLEN+I;                /* UPDATE LENGTH OF MSG          */
         L     @11,INITPTR(,PCTPTR)                                0063
         LR    @09,I                                               0063
         AH    @09,WTOLEN(,@11)                                    0063
         STH   @09,WTOLEN(,@11)                                    0063
*   IF WTOLEN>60                    /* TEST FOR LEN > FULL LN        */
*     THEN                          /* LINE FULL                     */
         CH    @09,@CH00390                                        0064
         BH    @RT00064                                            0064
*     GO TO T10316;                 /* ISSUE MSG                     */
*   GO TO T10301;                   /* GET NEXT OPTION               */
         B     T10301                                              0066
*                                                                  0067
*/********************************************************************/
*/*                                                                  */
*/*        PLACE FILTER DEVICES/CODES, IF ANY, IN MESSAGE            */
