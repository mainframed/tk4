         ALR   @09,@11                                             0141
         AL    @09,@CF00421                                        0141
         CLC   OPTBL(3,@09),ENDLST                                 0141
         BNE   @RT00141                                            0141
*     GO TO T10319;                 /* PLACE TBL ENTRY IN MSG        */
