         TITLE '2540/2501/2520 ERROR RECOVERY PROCEDURE              '
IEC2540A CSECT
         USING IEC2540A,BASREG      ESTABLISH ADDRESSABILITY
         USING @DATD,DATDREG
         BC   15,ER2540A
         DC   CL8'IGE0001C'
         DC   CL8'R02000  '
         DC   CL8'05/22/73'
*                                                                  0080
*   /*****************************************************************/
*   /*                                                               */
*   /* 2540/2501/2520 ERROR RECOVERY PROCEDURE                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0080
*ER2540A:                                                          0080
*   CVTPTR=CVTAD;                                                  0080
ER2540A  L     CVTPTR,CVTAD                                        0080
*   EWAPTR=IOSERP;                  /* INIT PTR TO ERP WORK AREA     */
         L     EWAPTR,IOSERP(,IOSBPTR)                             0081
*   DATDREG=ADDR(EWUWORK);          /* DATA ADDR FOR COMPILER TEMPS  */
         LA    DATDREG,EWUWORK(,EWAPTR)                            0082
*   UCBPTR=IOSUCB;                                                 0083
         L     UCBPTR,IOSUCB(,IOSBPTR)                             0083
*   IOSEX=ON;                       /* SET IOSEX AND IOSERR TO     0084
*                                      INDICATE ERP IN CONTROL       */
*   IOSERR=ON;                                                     0085
         OI    IOSEX(IOSBPTR),B'00100100'                          0085
*   IF IOSDCHN=ON&                  /* IF DATA CHAINING AND COMMAND
*                                      CHAINING, XCTL TO WTO TO ISSUE
*                                      'I/O ERROR' MSG               */
*       IOSCCHN=ON THEN                                            0086
         TM    IOSDCHN(IOSBPTR),B'11000000'                        0086
         BO    @RT00086                                            0086
*     GO TO ERRWTO;                                                0087
*   ELSE                                                           0088
*     CCWRST=IOSCSWCA-CCWLEN;       /* CALC CCW ADDR TO RESTART      */
         SR    CCWRST,CCWRST                                       0088
         ICM   CCWRST,7,IOSCSWCA(IOSBPTR)                          0088
         SH    CCWRST,CCWLEN                                       0088
*   IF IOSCCHN=OFF THEN             /* IF CHAN PGM IS NOT CMD      0089
*                                      CHAINED, TEST FOR CHAN CK     */
         TM    IOSCCHN(IOSBPTR),B'01000000'                        0089
         BZ    @RT00089                                            0089
*     GO TO CNTRLCK;                                               0090
*   IF IOENTRY=OFF THEN             /* IF FIRST ENTRY, SET FLAGS     */
         TM    IOENTRY(EWAPTR),B'01000000'                         0091
         BZ    @RT00091                                            0091
*     GO TO SETENTRY;                                              0092
*   RFY                                                            0093
*     IDREG RSTD;                                                  0093
*   IDREG=CCWRST;                                                  0094
         LR    IDREG,CCWRST                                        0094
*   GEN(LRA IDREG,0(IDREG));                                       0095
         LRA IDREG,0(IDREG)
*   IF IDREG=IOSRST THEN            /* IF ERROR ON SAME CCW, DON'T 0096
*                                      RESET ERR COUNTS              */
         C     IDREG,IOSRST(,IOSBPTR)                              0096
         BE    @RT00096                                            0096
*     GO TO CNTRLCK;                                               0097
*   ELSE                                                           0098
*     IOBOERR=OFF;                  /* CLEAR PREVIOUS ERROR CNT      */
         NI    IOBOERR(EWAPTR),B'11011111'                         0098
*   IOOVRN=OFF;                                                    0099
*   CHCNTRL=OFF;                                                   0100
         NI    IOOVRN(EWAPTR),B'11110011'                          0100
*   RETRYCNT=0;                                                    0101
         MVI   RETRYCNT(EWAPTR),X'00'                              0101
*   RFY                                                            0102
*     IDREG UNRSTD;                                                0102
*SETENTRY:                                                         0103
*   IOENTRY=ON;                     /* SET FIRST ENTRY FLAG          */
SETENTRY DS    0H                                                  0104
*   IORSTRT=ON;                     /* SET RESTART FLAG IN EWA       */
         OI    IOENTRY(EWAPTR),B'11000000'                         0104
*CNTRLCK:                                                          0105
*   IF CSWCTK=ON×                   /* IF CHAN CONTROL CHECK OR    0105
*                                      INTERFACE CONTROL CHECK, GO 0105
*                                      TEST ERPIB                    */
*       CSWICK=ON THEN                                             0105
CNTRLCK  TM    CSWCTK(IOSBPTR),B'00000110'                         0105
         BNZ   @RT00105                                            0105
*     GO TO CHANERR;                                               0106
*   IF UCBTBYT4=RD2501×             /* IF 2501 OR 2520, TEST UCK     */
*       UCBTBYT4=RDP2520 THEN                                      0107
         CLI   UCBTBYT4(UCBPTR),X'04'                              0107
         BE    @RT00107                                            0107
         CLI   UCBTBYT4(UCBPTR),X'05'                              0107
         BE    @RT00107                                            0107
*     GO TO TSTUCK2;                                               0108
*   IF CSWCCK=ON THEN               /* IF CHAN CHAIN CHECK, LOGOUT 0109
*                                      AND ISSUE 'I/O ERROR' MSG     */
         TM    CSWCCK(IOSBPTR),B'00000001'                         0109
         BO    @RT00109                                            0109
*     GO TO LOGOUT;                                                0110
*   IF CSWCDK=ON THEN               /* IF CHAN DATA CK, SET INDICATOR
*                                      IN EWASTUP                    */
         TM    CSWCDK(IOSBPTR),B'00001000'                         0111
         BNO   @RF00111                                            0111
*     CDKSTAT=ON;                                                  0112
         OI    CDKSTAT(EWAPTR),B'00000001'                         0112
*TSTUCK:                                                           0113
*   IF CSWUCK=OFF THEN                                             0113
@RF00111 DS    0H                                                  0113
TSTUCK   TM    CSWUCK(IOSBPTR),B'00000010'                         0113
         BZ    @RT00113                                            0113
*     GO TO CHANDCK;                                               0114
*                                                                  0114
*/* 'OR' SENSE BYTE INTO STAT ENTRY AND CLEAR UNWANTED BITS.         */
*                                                                  0115
*   ELSE                                                           0115
*     EWASTUP=EWASTUP×IOSSNS;                                      0115
         OC    EWASTUP(2,EWAPTR),IOSSNS(IOSBPTR)                   0115
*   EWASTUP(1:1)=EWASTUP(1:1)&SNSMSK;                              0116
         NI    EWASTUP(EWAPTR),X'78'                               0116
*   IF EWUOVRN=ON×                  /* IF 'SHOULD NOT OCCUR' SENSE,
*                                      LOGOUT AND ISSUE I/O ERR MSG  */
*       EWUPEK=ON THEN                                             0117
         TM    EWUOVRN(EWAPTR),B'00000101'                         0117
         BNZ   @RT00117                                            0117
*     GO TO LOGOUT;                                                0118
*   IF EWUEQCK=ON THEN              /* IF EQUIP CK SENSE, CHECK FOR
*                                      READ COMMAND                  */
         TM    EWUEQCK(EWAPTR),B'00010000'                         0119
         BO    @RT00119                                            0119
*     GO TO READERR;                                               0120
*   GEN(EJECT);                                                    0121
         EJECT
*TSTSNS:                                                           0122
*   IF CSWUCK=OFF THEN              /* IF NOT UNIT CHECK GO TO TEST
*                                      CSW STATUS BYTES              */
TSTSNS   TM    CSWUCK(IOSBPTR),B'00000010'                         0122
         BZ    @RT00122                                            0122
*     GO TO TSTSTAT;                                               0123
*                                                                  0123
*   /*****************************************************************/
*   /*                                                               */
*   /* LINK TO THE RESIDENT ERROR INTERPRETER ROUTINE TO INTERROGATE */
*   /* THE SENSE BYTE FOR THE 2540.                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0124
*   GEN REFS(CVTPTR,CVTXITP)FLOWS(INTREQ,BUSOUT,DATACK,ERRWTO,UCMDSEQ,
*       LOGOUT)SETS(SAVREG,XCTLREG);                               0124
         LR    SAVREG,BASREG        SAVE REG 15
         L     BASREG,CVTXITP(,CVTPTR) ADDR OF ERR INTERPRETER RTN
SNSBASE  BALR  XCTLREG,BASREG       TO INTERPRET 2540 SENSE BYTE
         DC    X'01'                IF INTERVENTION REQUIRED,
         DC    AL1(ERSNS1-SNSBASE-2)   THEN GO TO INTREQ
         DC    X'02'                IF BUSOUT CHECK,
         DC    AL1(ERSNS2-SNSBASE-4)   THEN GO TO BUSOUT
         DC    X'04'                IF DATA CHECK,
         DC    AL1(ERSNS3-SNSBASE-6)   THEN GO TO DATACK
         DC    X'00'                IF COMMAND REJECT,
         DC    AL1(ERSNS4-SNSBASE-8)   THEN GO TO ERRWTO
         DC    X'06'                IF UNUSUAL COMMAND SEQUENCE,
         DC    AL1(ERSNS5-SNSBASE-10)  THEN GO TO UCMDSEQ
         DC    X'2F'                END OF TEST,
         DC    AL1(ERSNS6-SNSBASE-12)  GO TO LOGOUT
         EJECT
*TSTUCK2:                                                          0125
*   IF CSWUCK=OFF THEN              /* IF NOT UCK, TEST CSW STATUS   */
TSTUCK2  TM    CSWUCK(IOSBPTR),B'00000010'                         0125
         BZ    @RT00125                                            0125
*     GO TO TSTSTAT2;                                              0126
*   ELSE                                                           0127
*     EWASTUP=EWASTUP×IOSSNS;                                      0127
         OC    EWASTUP(2,EWAPTR),IOSSNS(IOSBPTR)                   0127
*   EWASTUP(1:1)=EWASTUP(1:1)&SNSMSK2;                             0128
*                                                                  0128
         NI    EWASTUP(EWAPTR),X'70'                               0128
*   /*****************************************************************/
*   /*                                                               */
*   /* LINK TO THE RESIDENT ERROR INTERPRETER ROUTINE TO INTERROGATE */
*   /* THE SENSE BYTE FOR THE 2501 OR 2520.                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0129
*   GEN REFS(CVTPTR,CVTXITP)FLOWS(INTREQ,BUSOUT,DATACK,ERRWTO,UCMDSEQ,
*       LOGOUT,READERR,OVERRUN)SETS(SAVREG,XCTLREG);               0129
         LR    SAVREG,BASREG        SAVE REG 15
         L     BASREG,CVTXITP(,CVTPTR) ADDR OF ERR INTERPRETER RTN
SNSBAS2  BALR  XCTLREG,BASREG       TO INTERPRET 2501/2520 SENSE BYTE
         DC    X'06'                IF SHOULD NOT OCCUR,
         DC    AL1(ERSNS6-SNSBAS2-2)   THEN GO TO LOGOUT
         DC    X'03'                IF EQUIPMENT CHECK,
         DC    AL1(ERSNS7-SNSBAS2-4)   THEN GO TO READERR
         DC    X'07'                IF SHOULD NOT OCCUR,
         DC    AL1(ERSNS6-SNSBAS2-6)   THEN GO TO LOGOUT
         DC    X'01'                IF INTERVENTION REQUIRED,
         DC    AL1(ERSNS1-SNSBAS2-8)   THEN GO TO INTREQ
         DC    X'02'                IF BUSOUT CHECK,
         DC    AL1(ERSNS2-SNSBAS2-10)  THEN GO TO BUSOUT
         DC    X'04'                IF DATA CHECK,
         DC    AL1(ERSNS3-SNSBAS2-12)  THEN GO TO DATACK
         DC    X'05'                IF OVERRUN,
         DC    AL1(ERSNS8-SNSBAS2-14)  THEN GO TO OVERRUN
         DC    X'00'                IF COMMAND REJECT,
         DC    AL1(ERSNS4-SNSBAS2-16)  THEN GO TO ERRWTO
         DC    X'2F'                END OF TEST,
         DC    AL1(ERSNS6-SNSBAS2-18)  GO TO LOGOUT
         SPACE 2
*     ERROR BRANCH TABLE
         SPACE
ERSNS1   B    INTREQ
ERSNS2   B    BUSOUT
ERSNS3   B    DATACK
ERSNS4   B    ERRWTO
ERSNS5   B    UCMDSEQ
ERSNS6   B    LOGOUT
ERSNS7   B    READERR
ERSNS8   B    OVERRUN
         EJECT
*                                                                  0130
*   /*****************************************************************/
*   /*                                                               */
*   /* INTERVENTION REQUIRED                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0130
*INTREQ:                                                           0130
*   IF EWUUSEQ=OFF THEN             /* IF NOT UNUSUAL CMD SEQ, CHECK
*                                      IF INTERCEPTED ERROR          */
INTREQ   TM    EWUUSEQ(EWAPTR),B'00000010'                         0130
         BZ    @RT00130                                            0130
*     GO TO CKINTCPT;                                              0131
*   ELSE                                                           0132
*     IOSERR=OFF;                   /* CLEAR IOSERR FLAG             */
         NI    IOSERR(IOSBPTR),B'11011111'                         0132
*   GO TO XCTLWTO;                  /* TO ISSUE 'INT REQ' MSG        */
         B     XCTLWTO                                             0133
*CKINTCPT:                                                         0134
*   IF IOSCOD=IOSFINTC THEN                                        0134
CKINTCPT CLI   IOSCOD(IOSBPTR),X'7E'                               0134
         BNE   @RF00134                                            0134
*     DO;                           /* IF INTERCEPTED ERROR,         */
*       IOSCOD=IOSNRMC;             /* CHANGE COMP CODE TO '7F'      */
         MVI   IOSCOD(IOSBPTR),X'7F'                               0136
*       IORSTRT=OFF;                /* CLEAR RESTART FLAG IN EWA     */
         NI    IORSTRT(EWAPTR),B'01111111'                         0137
*     END;                                                         0138
*   GO TO XCTLWTO;                  /* TO ISSUE 'INT REQ' MSG        */
         B     XCTLWTO                                             0139
*   GEN(EJECT);                                                    0140
*                                                                  0140
         EJECT
*   /*****************************************************************/
*   /*                                                               */
*   /* BUSOUT CHECK                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0141
*BUSOUT:                                                           0141
*   IF UCBTBYT4^=RD2501 THEN        /* IF NOT 2501, TEST BUSOUT CNT  */
BUSOUT   CLI   UCBTBYT4(UCBPTR),X'04'                              0141
         BNE   @RT00141                                            0141
*     GO TO BUSCNT;                                                0142
*   IF CSWCHE=ON THEN               /* IF CHAN END OCCURRED, LOGOUT  */
         TM    CSWCHE(IOSBPTR),B'00001000'                         0143
         BO    @RT00143                                            0143
*     GO TO LOGOUT;                                                0144
*BUSCNT:                                                           0145
*   IF IOBOERR=ON THEN              /* IF SECOND OCCURRENCE OF     0145
*                                      BUSOUT, LOGOUT AND ISSUE 'I/O
*                                      ERROR' MSG                    */
BUSCNT   TM    IOBOERR(EWAPTR),B'00100000'                         0145
         BO    @RT00145                                            0145
*     GO TO LOGOUT;                                                0146
*   ELSE                                                           0147
*     IOBOERR=ON;                   /* SET BUSOUT CHECK FLAG         */
         OI    IOBOERR(EWAPTR),B'00100000'                         0147
*   GEN(LRA CCWRST,0(CCWRST));                                     0148
         LRA CCWRST,0(CCWRST)
*   IF IORSTRT=ON THEN              /* IF RESTART FLAG IS ON,        */
         TM    IORSTRT(EWAPTR),B'10000000'                         0149
         BNO   @RF00149                                            0149
*     IOSRST=CCWRST;                /* CHANGE TO RESTART CCW ADDR    */
         ST    CCWRST,IOSRST(,IOSBPTR)                             0150
*   SVC(15);                        /* ISSUE SVC 15 TO RETRY         */
@RF00149 SVC   15                                                  0151
*   SVC(3);                         /* ISSUE SVC 3 TO EXIT           */
         SVC   3                                                   0152
*   GEN(EJECT);                                                    0153
         EJECT
*                                                                  0154
*   /*****************************************************************/
*   /*                                                               */
*   /* DATA CHECK                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0154
*DATACK:                                                           0154
*   IF CCWRST^>0 THEN               /* IF INVALID CCW ADDR, LOGOUT 0154
*                                      AND ISSUE 'I/O ERROR' MSG     */
DATACK   LTR   CCWRST,CCWRST                                       0154
         BNP   @RT00154                                            0154
*     GO TO LOGOUT;                                                0155
*   IF CMDB2=ON THEN                /* IF BINARY MODE, LOGOUT AND  0156
*                                      ISSUE 'I/O ERROR' MSG         */
         TM    CMDB2(CCWRST),B'00100000'                           0156
         BO    @RT00156                                            0156
*     GO TO LOGOUT;                                                0157
*   IF RETRYCNT=FVRTRY THEN         /* IF FIVE RETRIES, LOGOUT AND 0158
*                                      ISSUE 'I/O ERROR' MSG         */
         CLI   RETRYCNT(EWAPTR),X'05'                              0158
         BE    @RT00158                                            0158
*     GO TO LOGOUT;                                                0159
*   ELSE                                                           0160
*     RETRYCNT=RETRYCNT+1;          /* INCREMENT RETRY COUNTER       */
         LA    @14,1                                               0160
         SR    @10,@10                                             0160
         IC    @10,RETRYCNT(,EWAPTR)                               0160
         AR    @14,@10                                             0160
         STC   @14,RETRYCNT(,EWAPTR)                               0160
*   GO TO XCTLWTO;                                                 0161
         B     XCTLWTO                                             0161
*   GEN(EJECT);                                                    0162
*                                                                  0162
         EJECT
*   /*****************************************************************/
*   /*                                                               */
*   /* OVERRUN                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0163
*OVERRUN:                                                          0163
*   IF IOOVRN=ON THEN               /* IF SECOND OCCURRENCE, LOGOUT
*                                      AND ISSUE 'I/O ERROR' MSG     */
OVERRUN  TM    IOOVRN(EWAPTR),B'00000100'                          0163
         BO    @RT00163                                            0163
*     GO TO LOGOUT;                                                0164
*   ELSE                                                           0165
*     IOOVRN=ON;                    /* SET OVERRUN FLAG              */
         OI    IOOVRN(EWAPTR),B'00000100'                          0165
*   GO TO XCTLWTO;                                                 0166
         B     XCTLWTO                                             0166
*   GEN(EJECT);                                                    0167
*                                                                  0167
         EJECT
*   /*****************************************************************/
*   /*                                                               */
*   /* UNUSUAL COMMAND SEQUENCE                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0168
*UCMDSEQ:                                                          0168
*   IF ACPTUCS=OFF THEN             /* IF NOT USEQ AFTER CHAN ERR,   */
UCMDSEQ  TM    ACPTUCS(EWAPTR),B'00010000'                         0168
         BZ    @RT00168                                            0168
*     GO TO ERRWTO;                 /* ISSUE 'I/O ERROR' MSG         */
*   ELSE                                                           0170
*     DO;                                                          0170
*       IORSTRT=OFF;                /* CLEAR RESTART FLAG IN EWA     */
         NI    IORSTRT(EWAPTR),B'01111111'                         0171
*       IOSTATUS=IOSTATUS&&IOSTATUS;/* CLEAR CSW STATUS IN IOSB      */
         XC    IOSTATUS(2,IOSBPTR),IOSTATUS(IOSBPTR)               0172
*     END;                                                         0173
*   GEN(EJECT);                                                    0174
*                                                                  0174
         EJECT
*   /*****************************************************************/
*   /*                                                               */
*   /* ERROR CORRECTED                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0175
*ERCORR:                                                           0175
*   IOSEX=OFF;                      /* CLEAR IOSEX AND IOSERR TO   0175
*                                      INDICATE ERROR CORRECTED      */
ERCORR   DS    0H                                                  0176
*   IOSERR=OFF;                                                    0176
*                                                                  0176
         NI    IOSEX(IOSBPTR),B'11011011'                          0176
*   /*****************************************************************/
*   /*                                                               */
*   /* XCTL TO STATISTICS UPDATE ROUTINE                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0177
*   GEN REFS(CVTPTR,CVTXTLER,STATID)SETS(IDREG,XCTLREG)EXIT NOSEQFLOW;
         LH    IDREG,STATID         ID OF STAT UPDATE ROUTINE
         L     XCTLREG,CVTXTLER(,CVTPTR) ADDR OF XCTL ROUTINE
         BR    XCTLREG              TO XCTL TO STAT UPDATE RTN
*   GEN(EJECT);                                                    0178
         EJECT
*TSTSTAT:                                                          0179
*   IF UCBTBYT4=RD2501×             /* IF 2501 OR 2520, USE SECOND 0179
*                                      STATUS TABLE                  */
*       UCBTBYT4=RDP2520 THEN                                      0179
TSTSTAT  CLI   UCBTBYT4(UCBPTR),X'04'                              0179
         BE    @RT00179                                            0179
         CLI   UCBTBYT4(UCBPTR),X'05'                              0179
         BE    @RT00179                                            0179
*     GO TO TSTSTAT2;                                              0180
*                                                                  0180
*   /*****************************************************************/
*   /*                                                               */
*   /* LINK TO THE RESIDENT ERROR INTERPRETER ROUTINE TO INTERROGATE */
*   /* THE CSW STATUS BYTES FOR THE 2540.                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0181
*   GEN REFS(CVTPTR,CVTXITP)FLOWS(PERMERR,ERCORR)SETS(SAVREG,XCTLREG);
         LR    SAVREG,BASREG        SAVE REG 15
         L     BASREG,CVTXITP(,CVTPTR) ADDR OF ERR INTERPRETER RTN
STATBASE BALR  XCTLREG,BASREG       TO INTERPRET 2540 STATUS BYTESS
         DC    X'1A'                IF PROGRAM CHECK,
         DC    AL1(ERSTAT1-STATBASE-2) THEN GO TO PERMERR
         DC    X'1B'                IF PROTECTION CHECK,
         DC    AL1(ERSTAT1-STATBASE-4) THEN GO TO PERMERR
         DC    X'17'                IF UNIT EXCEPTION,
         DC    AL1(ERSTAT1-STATBASE-6) THEN GO TO PERMERR
         DC    X'19'                IF INCORRECT LENGTH,
         DC    AL1(ERSTAT1-STATBASE-8) THEN GO TO PERMERR
         DC    X'2F'                END OF TEST,
         DC    AL1(ERSTAT2-STATBASE-10) GO TO ERCORR
         EJECT
*TSTSTAT2:                                                         0182
*                                                                  0182
*   /*****************************************************************/
*   /*                                                               */
*   /* LINK TO THE RESIDENT ERROR INTERPRETER ROUTINE TO INTERROGATE */
*   /* THE CSW STATUS BYTES FOR THE 2501 OR 2520.                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0182
*   GEN REFS(CVTPTR,CVTXITP)FLOWS(READERR,PERMERR,ERCORR)SETS(SAVREG,
*       XCTLREG);                                                  0182
TSTSTAT2 DS    0H                                                  0182
         LR    SAVREG,BASREG        SAVE REG 15
         L     BASREG,CVTXITP(,CVTPTR) ADDR OF ERR INTERPRETER RTN
STATBAS2 BALR  XCTLREG,BASREG       TO INTERPRET 2501/2520 STATUS BYTES
         DC    X'1C'                IF CHANNEL DATA CHECK,
         DC    AL1(ERSTAT3-STATBAS2-2) THEN GO TO READERR
         DC    X'1F'                IF CHAINING CHECK,
         DC    AL1(ERSTAT1-STATBAS2-4) THEN GO TO PERMERR
         DC    X'1A'                IF PROGRAM CHECK,
         DC    AL1(ERSTAT1-STATBAS2-6) THEN GO TO PERMERR
         DC    X'1B'                IF PROTECTION CHECK,
         DC    AL1(ERSTAT1-STATBAS2-8) THEN GO TO PERMERR
         DC    X'17'                IF UNIT EXCEPTION,
         DC    AL1(ERSTAT1-STATBAS2-10) THEN GO TO PERMERR
         DC    X'19'                IF INCORRECT LENGTH,
         DC    AL1(ERSTAT1-STATBAS2-12) THEN GO TO PERMERR
         DC    X'2F'                END OF TEST,
         DC    AL1(ERSTAT2-STATBAS2-14) GO TO ERCORR
         SPACE 2
*     ERROR BRANCH TABLE
         SPACE
ERSTAT1  B     PERMERR
ERSTAT2  B     ERCORR
ERSTAT3  B     READERR
         EJECT
*                                                                  0183
*   /*****************************************************************/
*   /*                                                               */
*   /* PERMANENT ERROR                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0183
*PERMERR:                                                          0183
*   IOSERR=OFF;                     /* CLEAR IOSERR - PERM ERROR     */
PERMERR  NI    IOSERR(IOSBPTR),B'11011111'                         0183
*   SVC(15);                        /* TO POST PERMANENT ERROR       */
         SVC   15                                                  0184
*   SVC(3);                         /* ISSUE SVC 3 TO EXIT           */
*                                                                  0185
         SVC   3                                                   0185
*   /*****************************************************************/
*   /*                                                               */
*   /* XCTL TO WRITE-TO-OPERATOR ROUTINE                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0186
*LOGOUT:                                                           0186
*   IOSLOG=ON;                      /* TO INDICATE LOGOUT            */
LOGOUT   OI    IOSLOG(IOSBPTR),B'00000001'                         0186
*ERRWTO:                                                           0187
*   IOSERR=OFF;                                                    0187
ERRWTO   NI    IOSERR(IOSBPTR),B'11011111'                         0187
*   IOSMSG=ON;                      /* TO INDICATE 'I/O ERR' MSG     */
         OI    IOSMSG(IOSBPTR),B'00000100'                         0188
*   IORSTRT=OFF;                                                   0189
         NI    IORSTRT(EWAPTR),B'01111111'                         0189
*XCTLWTO:                                                          0190
*   GEN(LRA CCWRST,0(CCWRST));                                     0190
XCTLWTO  LRA CCWRST,0(CCWRST)
*   IF IORSTRT=ON THEN              /* IF RESTART FLAG SET, CHANGE TO
*                                      CCW RESTART ADDR              */
         TM    IORSTRT(EWAPTR),B'10000000'                         0191
         BNO   @RF00191                                            0191
*     IOSRST=CCWRST;                                               0192
         ST    CCWRST,IOSRST(,IOSBPTR)                             0192
*   GEN REFS(CVTPTR,CVTXTLER,WTOID)SETS(IDREG,XCTLREG)EXIT NOSEQFLOW;
@RF00191 DS    0H                                                  0193
         LH    IDREG,WTOID          ID OF WTO ROUTINE
         L     XCTLREG,CVTXTLER(,CVTPTR) ADDR OF XCTL ROUTINE
         BR    XCTLREG              TO XCTL TO WTO ROUTINE
          EJECT
*                                                                  0194
*   /*****************************************************************/
*   /*                                                               */
*   /* CHANNEL DATA CHECK                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0194
