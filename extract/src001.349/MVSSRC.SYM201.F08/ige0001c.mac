         TM    CMDB3(CCWRST),B'00010000'                           0212
         BO    @RT00212                                            0212
*     GO TO CCHTEST;                                               0213
*   IF CHCNTRL=ON THEN              /* IF SECOND OCCURRENCE, LOGOUT
*                                      AND ISSUE 'I/O ERR' MSG       */
         TM    CHCNTRL(EWAPTR),B'00001000'                         0214
         BO    @RT00214                                            0214
*     GO TO LOGOUT;                                                0215
*   ELSE                                                           0216
*     ACPTUCS=ON;                   /* SET TO ACCEPT USEQ ON RETRY   */
         OI    ACPTUCS(EWAPTR),B'00010000'                         0216
*   GO TO CCHTEST3;                 /* TO SET ERR COUNT AND RETRY    */
         B     CCHTEST3                                            0217
