         BO    @RT00206                                            0206
*     GO TO LOGOUT;                                                0207
*   IF CCWRST^>0 THEN               /* IF INVALID CCW ADDR, LOGOUT 0208
*                                      AND ISSUE 'I/O ERR' MSG       */
         LTR   CCWRST,CCWRST                                       0208
         BNP   @RT00208                                            0208
*     GO TO LOGOUT;                                                0209
*   IF CMDB0=OFF×                   /* IF NOT READ W/O CONTROL, TEST
*                                      IF CSW STORED ON SIO          */
*       CMDB1=OFF THEN                                             0210
         TM    CMDB0(CCWRST),B'11000000'                           0210
