         TITLE '3505/3525 ERROR RECOVERY PROCEDURE                     *
                        '
IEC3505A  CSECT
         USING IEC3505A,BASREG       ESTABLISH ADDRESSABILITY
         USING @DATD,DATDREG
         BC   15,ER3505A
         DC   CL8'IGE0001A'
         DC   CL8' 030 '
         DC   CL8'10/29/74'
*                                                                  0079
*   /*****************************************************************/
*   /*                                                               */
*   /* 3505/3525 ERROR RECOVERY PROCEDURE                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0079
*ER3505A:                                                          0079
*   EWAPTR=IOSERP;                  /* INIT PTR TO ERP WORK AREA     */
ER3505A  L     EWAPTR,IOSERP(,IOSBPTR)                             0079
*   DATDREG=ADDR(EWUWORK);          /* DATA ADDR FOR COMIPLER TEMPS  */
         LA    DATDREG,EWUWORK(,EWAPTR)                            0080
*   CVTPTR=CVTAD;                                                  0081
         L     CVTPTR,CVTAD                                        0081
*   IOSEX=ON;                       /* SET IOSEX AND IOSERR TO     0082
*                                      INDICATE ERP IN CONTROL       */
*   IOSERR=ON;                                                     0083
         OI    IOSEX(IOSBPTR),B'00100100'                          0083
*   IF IOSDCHN=ON&                  /* MIXED CHAINING, PERM ERR      */
*       IOSCCHN=ON THEN                                            0084
         TM    IOSDCHN(IOSBPTR),B'11000000'                        0084
         BO    @RT00084                                            0084
*     GO TO ERRWTO;                                                0085
*   ELSE                                                           0086
*     CCWRST=IOSCSWCA-CCWLEN;       /* CALCULATE CCW RESTART ADDR    */
         L     CCWRST,IOSCSWCA-1(,IOSBPTR)                         0086
         LA    CCWRST,0(,CCWRST)                                   0086
         SH    CCWRST,CCWLEN                                       0086
*   IF IOSCCHN=OFF THEN             /* IF NOT CMD CHAINING, TEST CSW
*                                      STATUS                        */
         TM    IOSCCHN(IOSBPTR),B'01000000'                        0087
         BZ    @RT00087                                            0087
*     GO TO STATUS;                                                0088
*   IF IOENTRY=OFF THEN             /* FIRST ENTRY, SET FLAG         */
         TM    IOENTRY(EWAPTR),B'01000000'                         0089
         BNZ   @RF00089                                            0089
*     DO;                                                          0090
*       IOENTRY=ON;                 /* SET FIRST ENTRY FLAG          */
         OI    IOENTRY(EWAPTR),B'01000000'                         0091
*       GO TO RSTART;               /* TO SET RESTART FLAG           */
         B     RSTART                                              0092
*     END;                                                         0093
*   RFY                                                            0094
*     IDREG RSTD;                                                  0094
@RF00089 DS    0H                                                  0095
*   IDREG=CCWRST;                                                  0095
         LR    IDREG,CCWRST                                        0095
*   GEN(LRA IDREG,0(IDREG));                                       0096
         LRA IDREG,0(IDREG)
*   IF IDREG=IOSRST THEN            /* IF ERROR ON SAME CCW, TEST CSW
*                                      STATUS                        */
         C     IDREG,IOSRST(,IOSBPTR)                              0097
         BE    @RT00097                                            0097
*     GO TO STATUS;                                                0098
*   RFY                                                            0099
*     IDREG UNRSTD;                                                0099
*   IOARTRY=OFF;                    /* CLEAR PREVIOUS ERROR FLAGS    */
*   IOCCC=OFF;                                                     0101
*   IOICC=OFF;                                                     0102
         NI    IOARTRY(EWAPTR),B'11000111'                         0102
*RSTART:                                                           0103
*   IORSTRT=ON;                     /* SET RESTART FLAG IN EWA       */
RSTART   OI    IORSTRT(EWAPTR),B'10000000'                         0103
*   GEN(EJECT);                                                    0104
         EJECT
*STATUS:                                                           0105
*                                                                  0105
*   /*****************************************************************/
*   /*                                                               */
*   /* LINK TO THE RESIDENT ERROR INTERPRETER ROUTINE TO INTERROGATE */
*   /* THE CSW STATUS BYTES.                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0105
*   GEN REFS(CVTXITP,CVTPTR) SETS(SAVREG,XCTLREG) FLOWS(CCCERR,ICCERR,
*       CHANDCK,LOGOUT,SENSE0,ERRWTO,PGMERR,ERRCORR);              0105
STATUS   DS    0H                                                  0105
         LR    SAVREG,BASREG        SAVE REG 15
         L     BASREG,CVTXITP(,CVTPTR) ADDR OF ERR INTERPRETER RTN
STATBAS  BALR  XCTLREG,BASREG        TO INTERPRET STATUS BYTES
         DC    X'1D'                IF CHANNEL CONTROL CHECK,
         DC    AL1(ERSTAT1-STATBAS-2)  THEN GO TO CCCERR
         DC    X'1E'                IF INTERFACE CONTROL CHECK,
         DC    AL1(ERSTAT2-STATBAS-4)  THEN GO TO ICCERR
         DC    X'1C'                IF CHANNEL DATA CHECK,
         DC    AL1(ERSTAT3-STATBAS-6)  THEN GO TO CHANDCK
         DC    X'10'                IF 'SHOULD NOT OCCUR',
         DC    AL1(ERSTAT4-STATBAS-8)  THEN GO TO LOGOUT
         DC    X'11'                IF 'SHOULD NOT OCCUR',
         DC    AL1(ERSTAT4-STATBAS-10) THEN GO TO LOGOUT
         DC    X'12'                IF 'SHOULD NOT OCCUR',
         DC    AL1(ERSTAT4-STATBAS-12) THEN GO TO LOGOUT
         DC    X'16'                IF UNIT CHECK,
         DC    AL1(SENSE0-STATBAS-14)  THEN GO TO SENSE0
         DC    X'1F'                IF CHANNEL CHAINING CHECK,
         DC    AL1(ERSTAT5-STATBAS-16) THEN GO TO ERRWTO
         DC    X'1A'                IF PROGRAM CHECK,
         DC    AL1(ERSTAT6-STATBAS-18) THEN GO TO PGMERR
         DC    X'1B'                IF PROTECTION CHECK,
         DC    AL1(ERSTAT6-STATBAS-20) THEN GO TO PGMERR
         DC    X'17'                IF UNIT EXCEPTION,
         DC    AL1(ERSTAT6-STATBAS-22) THEN GO TO PGMERR
         DC    X'19'                IF INCORRECT LENGTH,
         DC    AL1(ERSTAT6-STATBAS-24) THEN GO TO PGMERR
         DC    X'2F'                END OF TEST,
         DC    AL1(ERSTAT7-STATBAS-26) THEN GO TO ERRCORR
         EJECT
*SENSE0:                                                           0106
*                                                                  0106
*   /*****************************************************************/
*   /*                                                               */
*   /* LINK TO THE RESIDENT ERROR INTERPRETER ROUTINE TO INTERROGATE */
*   /* SENSE BYTE 0.                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0106
*   GEN REFS(CVTXITP,CVTPTR) SETS(SAVREG,XCTLREG) FLOWS(LOGOUT,ERRWTO,
*       EQUIPCK,SENSE1,INTREQ);                                    0106
SENSE0   DS    0H                                                  0106
         LR    SAVREG,BASREG        SAVE REG 15
         L     BASREG,CVTXITP(,CVTPTR) ADDR OF ERR INTERPRETER RTN
SNSBAS   BALR  XCTLREG,BASREG        TO INTERPRET SENSE BYTE 0
         DC    X'05'                IF 'SHOULD NOT OCCUR',
         DC    AL1(ERSTAT4-SNSBAS-2)   THEN GO TO LOGOUT
         DC    X'07'                IF PERMANENT ERROR,
         DC    AL1(ERSTAT8-SNSBAS-4)   THEN GO TO ERRWTO
         DC    X'03'                IF EQUIPMENT CHECK,
         DC    AL1(ERSTAT9-SNSBAS-6)   THEN GO TO EQUIPCK
         DC    X'06'                IF ABNORMAL FORMAT RESET,
         DC    AL1(SENSE1-SNSBAS-8)    THEN GO TO SENSE1
         DC    X'01'                IF INTERVENTION REQUIRED,
         DC    AL1(ERSTATA-SNSBAS-10)  THEN GO TO INTREQ
         DC    X'02'                IF BUSOUT CHECK,
         DC    AL1(SENSE1-SNSBAS-12)   THEN GO TO SENSE1
         DC    X'04'                IF DATA CHECK,
         DC    AL1(SENSE1-SNSBAS-14)   THEN GO TO SENSE1
         DC    X'00'                IF COMMAND REJECT,
         DC    AL1(SENSE1-SNSBAS-16)   THEN GO TO SENSE1
         DC    X'2F'                END OF TEST,
         DC    AL1(ERSTAT4-SNSBAS-18)  GO TO LOGOUT
         EJECT
*SENSE1:                                                           0107
*                                                                  0107
*   /*****************************************************************/
*   /*                                                               */
*   /* LINK TO THE RESIDENT ERROR INTERPRETER ROUTINE TO INTERROGATE */
*   /* SENSE BYTE 1.                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0107
*   GEN REFS(CVTXITP,CVTPTR) SETS(SAVREG,XCTLREG) FLOWS(LOGOUT,ERRWTO,
*       INTRETRY,AUTORTRY);                                        0107
SENSE1   DS    0H                                                  0107
         LR    SAVREG,BASREG        SAVE REG 15
         L     BASREG,CVTXITP(,CVTPTR) ADDR OF ERR INTERPRETER RTN
SNSBAS1  BALR  XCTLREG,BASREG        TO INTERPRET SENSE BYTE 1
         DC    X'0C'                IF 'SHOULD NOT OCCUR',
         DC    AL1(ERSTAT4-SNSBAS1-2)  THEN GO TO LOGOUT
         DC    X'0D'                IF 'SHOULD NOT OCCUR',
         DC    AL1(ERSTAT4-SNSBAS1-4)  THEN GO TO LOGOUT
         DC    X'0E'                IF 'SHOULD NOT OCCUR',
         DC    AL1(ERSTAT4-SNSBAS1-6)  THEN GO TO LOGOUT
         DC    X'0F'                IF 'SHOULD NOT OCCUR',
         DC    AL1(ERSTAT4-SNSBAS1-8)  THEN GO TO LOGOUT
         DC    X'08'                IF PERMANENT ERROR,
         DC    AL1(ERSTAT8-SNSBAS1-10) THEN GO TO ERRWTO
         DC    X'0B'                IF RETRY AFTER INTERVENTION,
         DC    AL1(ERSTATB-SNSBAS1-12) THEN GO TO INTRETRY
         DC    X'09'                IF AUTOMATIC RETRY,
         DC    AL1(ERSTATC-SNSBAS1-14) THEN GO TO AUTORTRY
         DC    X'2F'                END OF TEST,
         DC    AL1(ERSTAT4-SNSBAS1-16) GO TO LOGOUT
         SPACE 2
*   ERROR BRANCH TABLE
         SPACE
ERSTAT1  B     CCCERR               CHANNEL CONTROL CHECK
ERSTAT2  B     ICCERR               INTERFACE CONTROL CHECK
ERSTAT3  B     CHANDCK              CHANNEL DATA CHECK
ERSTAT4  B     LOGOUT               'SHOULD NOT OCCUR' ERRORS
ERSTAT5  B     ERRWTO               CHANNEL CHAINING CHECK
ERSTAT6  B     PGMERR               PROGRAM CHECK, PROTECTION CHECK,
*                                   UNIT EXCEPTION, INCORRECT LENGTH
ERSTAT7  B     ERRCORR              STATUS END OF TEST
ERSTAT8  B     ERRWTO               PERMANENT ERROR
ERSTAT9  B     EQUIPCK              EQUIPMENT CHECK
ERSTATA  B     INTREQ               INTERVENTION REQUIRED
ERSTATB  B     INTRETRY             RETRY AFTER INTERVENTION REQUIRED
ERSTATC  B     AUTORTRY             AUTOMATIC RETRY
         EJECT
*                                                                  0108
*   /*****************************************************************/
*   /*                                                               */
*   /* CHANNEL OR INTERFACE CONTROL CHECK                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0108
*CCCERR:                                                           0108
*   IF IOCCC=ON THEN                /* IF SECOND OCCURRENCE, ISSUE 0108
*                                      'I/O ERR' MSG                 */
CCCERR   TM    IOCCC(EWAPTR),B'00010000'                           0108
         BO    @RT00108                                            0108
*     GO TO ERRWTO;                                                0109
*   ELSE                                                           0110
*     IOCCC=ON;                     /* SET CHAN CONTROL CHECK FLAG   */
         OI    IOCCC(EWAPTR),B'00010000'                           0110
*   GO TO CNTRLCK;                  /* TO INTERROGATE ERPIB          */
         B     CNTRLCK                                             0111
*ICCERR:                                                           0112
*   IF IOICC=ON THEN                /* IF SECOND OCCURRENCE, ISSUE 0112
*                                      'I/O ERR' MSG                 */
ICCERR   TM    IOICC(EWAPTR),B'00001000'                           0112
         BO    @RT00112                                            0112
*     GO TO ERRWTO;                                                0113
*   ELSE                                                           0114
*     IOICC=ON;                     /* SET INTF CONTROL CHECK FLAG   */
         OI    IOICC(EWAPTR),B'00001000'                           0114
*CNTRLCK:                                                          0115
*   IF IOSCOD=IOSFINTC THEN         /* INTERCEPTED, PERM ERR         */
CNTRLCK  CLI   IOSCOD(IOSBPTR),X'7E'                               0115
         BE    @RT00115                                            0115
*     GO TO ERRWTO;                                                0116
*                                                                  0116
*   /*****************************************************************/
*   /*                                                               */
*   /* INTERROGATE ERPIB                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0117
*   IF RTRYCD=ZERO THEN             /* RETRY CODE 0, PERM ERR        */
         TM    RTRYCD(EWAPTR),B'00000111'                          0117
         BZ    @RT00117                                            0117
*     GO TO ERRWTO;                                                0118
*   IF RTRYCD=ONE THEN              /* RETRY CODE 1, CHECK CCW ADDR  */
         TM    RTRYCD(EWAPTR),B'00000001'                          0119
         BNO   @RF00119                                            0119
         TM    RTRYCD(EWAPTR),B'00000110'                          0119
         BZ    @RT00119                                            0119
*     GO TO CNTRLCK2;                                              0120
*   IF RTRYCD=TWO THEN              /* RETRY CODE 2, CHECK FSS CMD   */
@RF00119 TM    RTRYCD(EWAPTR),B'00000010'                          0121
         BNO   @RF00121                                            0121
         TM    RTRYCD(EWAPTR),B'00000101'                          0121
         BZ    @RT00121                                            0121
*     GO TO CNTRLCK1;                                              0122
*   IF RTRYCD=FOUR THEN             /* RETRY CODE 4, CHECK CCW ADDR  */
@RF00121 TM    RTRYCD(EWAPTR),B'00000100'                          0123
         BNO   @RF00123                                            0123
         TM    RTRYCD(EWAPTR),B'00000011'                          0123
         BZ    @RT00123                                            0123
*     GO TO CNTRLCK2;                                              0124
*   IF RTRYCD=SIX THEN              /* RETRY CODE 6, PERM ERR        */
@RF00123 TM    RTRYCD(EWAPTR),B'00000110'                          0125
         BNO   @RF00125                                            0125
         TM    RTRYCD(EWAPTR),B'00000001'                          0125
         BZ    @RT00125                                            0125
*     GO TO ERRWTO;                                                0126
*   IF RTRYCD=SEVEN THEN            /* RETRY CODE 7, CHECK FSS CMD   */
@RF00125 TM    RTRYCD(EWAPTR),B'00000111'                          0127
         BO    @RT00127                                            0127
*     GO TO CNTRLCK1;                                              0128
*                                                                  0128
*   /*****************************************************************/
*   /*                                                               */
*   /* INTERROGATE LAST COMMAND ISSUED                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0129
*   IF CMDB5=ON&                    /* PRINT LINE CMD, PERM ERR      */
*       CMDB7=ON THEN                                              0129
         TM    CMDB5(CCWRST),B'00000101'                           0129
         BO    @RT00129                                            0129
*     GO TO ERRWTO;                                                0130
*                                                                  0130
*/*  IF THE COMMAND IS A READ/FEED/SELECT STACKER OR                 */
*/*  WRITE/FEED/SELECT STACKER, THE ERROR IS PERMANENT.              */
*                                                                  0131
*   IF CMDB0=ON&CMDB1=ON THEN                                      0131
         TM    CMDB0(CCWRST),B'11000000'                           0131
         BO    @RT00131                                            0131
*     GO TO CNTRLCK1;                                              0132
*   IF CMDB3=ON THEN                                               0133
         TM    CMDB3(CCWRST),B'00010000'                           0133
         BO    @RT00133                                            0133
*     GO TO CNTRLCK1;                                              0134
*   IF CMDB6=ON×CMDB7=ON THEN                                      0135
         TM    CMDB6(CCWRST),B'00000011'                           0135
         BNZ   @RT00135                                            0135
*     GO TO ERRWTO;                                                0136
*CNTRLCK1:                                                         0137
*   IF CMDB2=ON&                    /* FEED/SELECT STACKER, PERMERR  */
*       CMDB6=ON&CMDB7=ON THEN                                     0137
CNTRLCK1 TM    CMDB2(CCWRST),B'00100011'                           0137
         BO    @RT00137                                            0137
*     GO TO ERRWTO;                                                0138
*CNTRLCK2:                                                         0139
*   IF CCWRST>0 THEN                /* VALID CCW ADDR, GO RETRY      */
CNTRLCK2 LTR   CCWRST,CCWRST                                       0139
         BP    @RT00139                                            0139
*     GO TO RTRYEXIT;                                              0140
*   ELSE                                                           0141
*     GO TO ERRWTO;                 /* PERMANENT ERROR               */
         B     ERRWTO                                              0141
*   GEN(EJECT);                                                    0142
*                                                                  0142
         EJECT
*   /*****************************************************************/
*   /*                                                               */
*   /* CHANNEL DATA CHECK OR EQUIPMENT CHECK                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0143
*EQUIPCK:                                                          0143
*   IOSLOG=ON;                      /* SET LOGOUT FLAG               */
EQUIPCK  OI    IOSLOG(IOSBPTR),B'00000001'                         0143
*   GO TO EQUIP1;                   /* TEST FOR INTERCEPT            */
         B     EQUIP1                                              0144
*CHANDCK:                                                          0145
*   IF CMDB6=OFF&                   /* WRITE CMD, ERROR CORRECTED    */
*       CMDB7=ON THEN                                              0145
CHANDCK  TM    CMDB7(CCWRST),B'00000001'                           0145
         BNO   @RF00145                                            0145
         TM    CMDB6(CCWRST),B'00000010'                           0145
         BZ    @RT00145                                            0145
*     GO TO ERRCORR;                                               0146
*EQUIP1:                                                           0147
*   IF IOSCOD=IOSFINTC THEN         /* INTERCEPTED, TEST SENSE BYTE1 */
@RF00145 DS    0H                                                  0147
EQUIP1   CLI   IOSCOD(IOSBPTR),X'7E'                               0147
         BE    @RT00147                                            0147
*     GO TO EQUIP2;                                                0148
*   IF CCWRST^>0 THEN               /* INVALID CCW ADDR, PERM ERR    */
         LTR   CCWRST,CCWRST                                       0149
         BNP   @RT00149                                            0149
*     GO TO ERRWTO;                                                0150
*   IF CMDB6=ON&                    /* READ CMD, TEST SENSE BYTE 1   */
*       CMDB7=OFF THEN                                             0151
         TM    CMDB6(CCWRST),B'00000010'                           0151
         BNO   @RF00151                                            0151
         TM    CMDB7(CCWRST),B'00000001'                           0151
         BZ    @RT00151                                            0151
*     GO TO SENSE1;                                                0152
*EQUIP2:                                                           0153
*   IOSCOD=IOSNRMC;                 /* CHANGE COMP CODE TO '7F'      */
@RF00151 DS    0H                                                  0153
EQUIP2   MVI   IOSCOD(IOSBPTR),X'7F'                               0153
*   GO TO SENSE1;                   /* TEST SENSE BYTE 1             */
         B     SENSE1                                              0154
*   GEN(EJECT);                                                    0155
*                                                                  0155
         EJECT
*   /*****************************************************************/
*   /*                                                               */
*   /* INTERVENTION REQUIRED                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0156
*INTREQ:                                                           0156
*   IF EWUMOT=ON THEN               /* MOTION MALFUNCTION, SET LOGOUT
*                                      FLAG                          */
INTREQ   TM    EWUMOT(EWAPTR),B'00100000'                          0156
         BNO   @RF00156                                            0156
*     IOSLOG=ON;                                                   0157
         OI    IOSLOG(IOSBPTR),B'00000001'                         0157
*   GO TO SENSE1;                   /* TEST SENSE BYTE 1             */
*                                                                  0158
         B     SENSE1                                              0158
*   /*****************************************************************/
*   /*                                                               */
*   /* RETRY AFTER INTERVENTION REQUIRED                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0159
*INTRETRY:                                                         0159
*   IF IOSCOD=IOSFINTC THEN         /* INTERCEPTED ERR, ISSUE MSG    */
INTRETRY CLI   IOSCOD(IOSBPTR),X'7E'                               0159
         BE    @RT00159                                            0159
*     GO TO INTRCPT;                                               0160
*   IF CCWRST^>0 THEN               /* INVALID CCW ADDR, PERM ERR    */
         LTR   CCWRST,CCWRST                                       0161
         BNP   @RT00161                                            0161
*     GO TO ERRWTO;                                                0162
*   ELSE                                                           0163
*     GO TO XCTLWTO;                /* ISSUE 'INT REQ' MSG           */
         B     XCTLWTO                                             0163
*INTRCPT:                                                          0164
*   IOSCOD=IOSNRMC;                 /* CHANGE COMP CODE TO '7F'      */
INTRCPT  MVI   IOSCOD(IOSBPTR),X'7F'                               0164
*   IORSTRT=OFF;                    /* CLEAR RESTART FLAG            */
         NI    IORSTRT(EWAPTR),B'01111111'                         0165
*   GO TO XCTLWTO;                  /* ISSUE 'INT REQ' MSG           */
*                                                                  0166
         B     XCTLWTO                                             0166
*   /*****************************************************************/
*   /*                                                               */
*   /* AUTOMATIC RETRY                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0167
*AUTORTRY:                                                         0167
*   IF IOARTRY=ON THEN              /* IF SECOND OCCURRENCE, ISSUE 0167
*                                      'I/O ERR' MSG                 */
AUTORTRY TM    IOARTRY(EWAPTR),B'00100000'                         0167
         BO    @RT00167                                            0167
*     GO TO ERRWTO;                                                0168
*   ELSE                                                           0169
*     IOARTRY=ON;                   /* SET AUTO RETRY FLAG           */
         OI    IOARTRY(EWAPTR),B'00100000'                         0169
*   IF IOSLOG=OFF THEN              /* IF NO LOGOUT, RETRY           */
         TM    IOSLOG(IOSBPTR),B'00000001'                         0170
         BZ    @RT00170                                            0170
*     GO TO RTRYEXIT;                                              0171
*   GEN(EJECT);                                                    0172
*                                                                  0172
         EJECT
*   /*****************************************************************/
*   /*                                                               */
*   /* XCTL TO OUTBOARD RECORDING ROUTINE                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0173
*XCTLOBR:                                                          0173
*   GEN REFS(CVTXTLER,CVTPTR,OBRID) SETS(IDREG,XCTLREG) EXIT NOSEQFLOW;
*                                                                  0173
XCTLOBR  DS    0H                                                  0173
         LH    IDREG,OBRID          ID OF OBR ROUTINE
         L     XCTLREG,CVTXTLER(,CVTPTR) ADDR OF RESIDENT XCTL RTN
         BR    XCTLREG              TO OUTBOARD RECORDING ROUTINE
*   /*****************************************************************/
*   /*                                                               */
*   /* PROGRAMMING ERROR                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0174
*PGMERR:                                                           0174
*   IORSTRT=OFF;                    /* CLEAR RESTART FLAG            */
PGMERR   NI    IORSTRT(EWAPTR),B'01111111'                         0174
*   IOSERR=OFF;                     /* CLEAR IOSERR TO IND PERM ERR  */
         NI    IOSERR(IOSBPTR),B'11011111'                         0175
*   SVC(15);                        /* TO POST PERMANENT ERROR       */
         SVC   15                                                  0176
*   SVC(3);                         /* ISSUE SVC 3 TO EXIT           */
*                                                                  0177
         SVC   3                                                   0177
*   /*****************************************************************/
*   /*                                                               */
*   /* ERROR CORRECTED                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0178
*ERRCORR:                                                          0178
*   IOSTATUS=IOSTATUS&&IOSTATUS;    /* CLEAR STATUS IN IOSB          */
ERRCORR  XC    IOSTATUS(2,IOSBPTR),IOSTATUS(IOSBPTR)               0178
*   IOSEX=OFF;                      /* CLEAR FLAGS TO INDICATE ERROR
*                                      IS CORRECTED                  */
*   IOSERR=OFF;                                                    0180
*                                                                  0180
         NI    IOSEX(IOSBPTR),B'11011011'                          0180
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE SVC 15 TO RETRY.                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0181
*RTRYEXIT:                                                         0181
*   GEN(LRA CCWRST,0(CCWRST));                                     0181
RTRYEXIT LRA CCWRST,0(CCWRST)
*   IF IORSTRT=ON THEN                                             0182
         TM    IORSTRT(EWAPTR),B'10000000'                         0182
         BNO   @RF00182                                            0182
*     IOSRST=CCWRST;                                               0183
         ST    CCWRST,IOSRST(,IOSBPTR)                             0183
*   SVC(15);                                                       0184
@RF00182 SVC   15                                                  0184
*   SVC(3);                                                        0185
         SVC   3                                                   0185
*   GEN(EJECT);                                                    0186
         EJECT
*                                                                  0187
*   /*****************************************************************/
*   /*                                                               */
*   /* XCTL TO WRITE-TO-OPERATOR ROUTINE                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0187
*LOGOUT:                                                           0187
*   IOSLOG=ON;                      /* TO INDICATE LOGOUT REQUIRED   */
LOGOUT   OI    IOSLOG(IOSBPTR),B'00000001'                         0187
*ERRWTO:                                                           0188
*   IOSERR=OFF;                                                    0188
ERRWTO   NI    IOSERR(IOSBPTR),B'11011111'                         0188
*   IOSMSG=ON;                      /* TO INDICATE 'I/O ERR' MSG     */
         OI    IOSMSG(IOSBPTR),B'00000100'                         0189
*   IORSTRT=OFF;                                                   0190
         NI    IORSTRT(EWAPTR),B'01111111'                         0190
*XCTLWTO:                                                          0191
*   GEN(LRA CCWRST,0(CCWRST));                                     0191
XCTLWTO  LRA CCWRST,0(CCWRST)
