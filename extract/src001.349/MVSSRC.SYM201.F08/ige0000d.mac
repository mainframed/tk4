         TITLE '1052/3215 ERROR RECOVERY PROCEDURE                     *
                        '
IEC1052  CSECT
         USING IEC1052,BASREG       ESTABLISH ADDRESSABILITY
         USING @DATD,DATDREG
         BC   15,ER1052A
         DC   CL8'IGE0000D'
         DC   CL8'UZ03507 '
         DC   CL8'01/22/76'
*                                                                  0107
*   /*****************************************************************/
*   /*                                                               */
*   /* 1052/3215 ERROR RECOVERY PROCEDURE                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0107
*ER1052A:                                                          0107
*   CVTREG=CVTPTR;                                                 0107
ER1052A  L     CVTREG,CVTPTR                                       0107
*   EWAPTR=IOSERP;                  /* INIT PTR TO ERP WORK AREA     */
         L     EWAPTR,IOSERP(,IOSBPTR)                             0108
*   DATDREG=ADDR(EWUWORK);          /* ADDRESSABILITY FOR TEMPS      */
         LA    DATDREG,EWUWORK(,EWAPTR)                            0109
*   UCBPTR=IOSUCB;                                                 0110
         L     UCBPTR,IOSUCB(,IOSBPTR)                             0110
*   CCWRST=IOSCSWCA-CCWLEN;         /* CALC CCW RESTART ADDR         */
         SLR   CCWRST,CCWRST                                       0111
         ICM   CCWRST,7,IOSCSWCA(IOSBPTR)                          0111
         SH    CCWRST,CCWLEN                                       0111
*   IF IOENTRY=OFF THEN             /* IF FIRST ENTRY,SKIP ERR CNTS  */
         TM    IOENTRY(EWAPTR),B'00000001'                         0112
         BZ    @RT00112                                            0112
*     GO TO SETENTRY;                                              0113
*   RFY                                                            0114
*     IDREG RSTD;                                                  0114
*   IDREG=CCWRST;                                                  0115
         LR    IDREG,CCWRST                                        0115
*   GEN(LRA IDREG,0(IDREG));                                       0116
*                                                                  0116
         LRA IDREG,0(IDREG)
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ERROR IS ON THE SAME CCW OR THE CSW ADD IS NOT POSITIVE;   */
*   /* DON'T RESET THE COUNTERS                              @ZA12456*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0117
*   IF IDREG=IOSRST×IDREG^>0 THEN   /* ZA12456                       */
         C     IDREG,IOSRST(,IOSBPTR)                              0117
         BE    @RT00117                                            0117
         LTR   IDREG,IDREG                                         0117
         BNP   @RT00117                                            0117
*     GO TO CKCONSL;                                               0118
*   ELSE                                                           0119
*     EWACNTR1=0;                   /* RESET PREVIOUS ERROR COUNTERS */
         MVI   EWACNTR1(EWAPTR),X'00'                              0119
*   EWACNTR2=0;                                                    0120
         MVI   EWACNTR2(EWAPTR),X'00'                              0120
*SETENTRY:                                                         0121
*   IOENTRY=ON;                     /* SET FIRST ENTRY FLAG          */
SETENTRY OI    IOENTRY(EWAPTR),B'00000001'                         0121
*   RFY                                                            0122
*     IDREG UNRSTD;                                                0122
*CKCONSL:                                                          0123
*   IF UCBSYSR=ON THEN              /* IF DEVICE IS CONSOLE, SET FLAG*/
CKCONSL  TM    UCBSYSR(UCBPTR),B'00000010'                         0123
         BNO   @RF00123                                            0123
*     CONSOLE=ON;                                                  0124
         OI    CONSOLE(EWAPTR),B'01000000'                         0124
*   IOSSMDA=OFF;                    /* CLEAR STAT MODIFIER FLAG      */
@RF00123 DS    0H                                                  0126
*   IOSERR=ON;                      /* SET IOSEX AND IOSERR TO     0126
*                                      INDICATE ERP IN CONTROL       */
*   IOSEX=ON;                                                      0127
         OI    IOSERR(IOSBPTR),B'00100100'                         0127
         NI    IOSSMDA(IOSBPTR),B'11101111'                        0127
*   IF CNTRLB1=OFF THEN                                            0128
         TM    CNTRLB1(EWAPTR),B'00001000'                         0128
         BZ    @RT00128                                            0128
*     GO TO ERR500;                                                0129
*                                                                  0129
*   /*****************************************************************/
*   /*                                                               */
*   /* CNTRLB1 IS ON IF THIS IS THE SECOND ENTRY ON AN 'INTERVENTION */
*   /* REQUIRED'. ISSUE IOSGEN TO SET UCBNRY FLAG AND RETRY REQUEST  */
*   /* FROM THE TOP. (NOTE: IF UNSOLICITED DEVICE END HAS BEEN       */
*   /* RECEIVED SINCE THE LAST SIO, THE UCBNRY FLAG WILL NOT BE SET.)*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0130
*   ELSE                                                           0130
*     CNTRLB1=OFF;                  /* CLEAR 'INT REQ' FLAG          */
         NI    CNTRLB1(EWAPTR),B'11110111'                         0130
*   GEN REFS(IOSGENSV,CVTPTR,CVTIXAVL,IOCGENA,UCBBGN,UCBFLA,UCBFLB,
*       UCBFLC,UCBCCWOF,UCB3TAPE) SETS(IDREG);                     0131
         LA   IDREG,IOSGENSV
         IOSGEN UCBFLG,UCB=(UCBPTR),VAR=ON,TABLE=UCBNRY
*RETRYINT:                                                         0132
*   IORSTRT=OFF;                    /* CLEAR RESTART FLAG IN EWA     */
RETRYINT NI    IORSTRT(EWAPTR),B'01111111'                         0132
*   SVC(15);                        /* TO RETRY REQUEST FROM TOP     */
         SVC   15                                                  0133
*   SVC(3);                         /* ISSUE SVC 3 TO EXIT           */
         SVC   3                                                   0134
*   GEN(EJECT);                                                    0135
         EJECT
*ERR500:                                                           0136
*   IF CNTRLB0=OFF THEN                                            0136
ERR500   TM    CNTRLB0(EWAPTR),B'00000100'                         0136
         BZ    @RT00136                                            0136
*     GO TO ERR500B;                                               0137
*   ELSE                                                           0138
*     IOSCSW=EWUCSW;                /* RESTORE ORIGINAL CSW          */
         MVC   IOSCSW(7,IOSBPTR),EWUCSW                            0138
*   GO TO PERMERR;                  /* TO XCTL TO STAT UPDATE        */
         B     PERMERR                                             0139
*ERR500B:                                                          0140
*   IF IOSCCHN=OFF THEN             /* IF NO CMD CHAINING, SKIP    0140
*                                      SETTING RESTART FLAG          */
ERR500B  TM    IOSCCHN(IOSBPTR),B'01000000'                        0140
         BZ    @RT00140                                            0140
*     GO TO ERR500A;                                               0141
*   ELSE                                                           0142
*     IORSTRT=ON;                   /* SET RESTART FLAG IN EWA       */
         OI    IORSTRT(EWAPTR),B'10000000'                         0142
*ERR500A:                                                          0143
*   IF CSWCTK=ON×                   /* CHAN OR INTERFACE CONTROL   0143
*                                      CHECK, INTERROGATE ERPIB      */
*       CSWICK=ON THEN                                             0143
ERR500A  TM    CSWCTK(IOSBPTR),B'00000110'                         0143
         BNZ   @RT00143                                            0143
*     GO TO ERPIBINT;                                              0144
*   IF CSWCCK=ON THEN               /* CHAN CHAINING CHECK, TEST IF
*                                      CONSOLE                       */
         TM    CSWCCK(IOSBPTR),B'00000001'                         0145
         BO    @RT00145                                            0145
*     GO TO ERR524A;                                               0146
*   IF CSWUCK=OFF THEN              /* IF NOT UCK, TEST STATUS       */
         TM    CSWUCK(IOSBPTR),B'00000010'                         0147
         BZ    @RT00147                                            0147
*     GO TO STATUS;                                                0148
*   ELSE                                                           0149
*                                                                  0149
*/*  'OR' SENSE BYTE INTO STATISTICS ENTRY AND CLEAR UNWANTED BITS   */
*                                                                  0149
*     EWASTUP=EWASTUP×IOSSNS;                                      0149
         OC    EWASTUP(2,EWAPTR),IOSSNS(IOSBPTR)                   0149
*   EWASTUP(1:1)=EWASTUP(1:1)&SNSMSK;                              0150
         NI    EWASTUP(EWAPTR),X'70'                               0150
*   GEN(EJECT);                                                    0151
*                                                                  0151
         EJECT
*   /*****************************************************************/
*   /*                                                               */
*   /* LINK TO THE RESIDENT ERROR INTERPRETER ROUTINE TO INTERPRET   */
*   /* THE SENSE BYTE.                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0152
*SENSE:                                                            0152
*   GEN REFS(CVTREG,CVTXITP) SETS(SAVREG,XCTLREG) FLOWS(HDWERR,EQUIPCK,
*       INTREQ,BUSOUT,PGMERR);                                     0152
SENSE    DS    0H                                                  0152
         LR    SAVREG,BASREG        SAVE REG 15
         L     BASREG,CVTXITP(,CVTREG) ADDR OF ERR INTERPRETER RTN
*                                                            YM06225
SNSBAS   BALR  XCTLREG,BASREG       TO INTERPRET SENSE BYTE
         DC    X'07'                SHOULD NOT OCCUR
         DC    AL1(ERSNS1-SNSBAS-2)
         DC    X'06'                SHOULD NOT OCCUR
         DC    AL1(ERSNS1-SNSBAS-4)
         DC    X'05'                SHOULD NOT OCCUR
         DC    AL1(ERSNS1-SNSBAS-6)
         DC    X'04'                SHOULD NOT OCCUR
         DC    AL1(ERSNS1-SNSBAS-8)
         DC    X'03'                EQUIPMENT CHECK
         DC    AL1(ERSNS2-SNSBAS-10)
         DC    X'01'                INTERVENTION REQUIRED
         DC    AL1(ERSNS3-SNSBAS-12)
         DC    X'02'                BUSOUT CHECK
         DC    AL1(ERSNS4-SNSBAS-14)
         DC    X'00'                COMMAND REJECT
         DC    AL1(ERSNS5-SNSBAS-16)
         DC    X'2F'                END OF TEST
         DC    AL1(ERSNS1-SNSBAS-18)
ERSNS1   B     HDWERR
ERSNS2   B     EQUIPCK
ERSNS3   B     INTREQ
ERSNS4   B     BUSOUT
ERSNS5   B     PGMERR
         EJECT
*                                                                  0153
*   /*****************************************************************/
*   /*                                                               */
*   /* PERMANENT ERROR                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0153
*HDWERR:                                                           0153
*   IF CONSOLE=ON THEN              /* IF CONSOLE, GO TO RETRY       */
HDWERR   TM    CONSOLE(EWAPTR),B'01000000'                         0153
         BO    @RT00153                                            0153
*     GO TO CONRETRY;                                              0154
*PERMERR:                                                          0155
*   IOSLOG=ON;                      /* SET LOGOUT REQUIRED FLAG      */
PERMERR  OI    IOSLOG(IOSBPTR),B'00000001'                         0155
*   IOSERR=OFF;                     /* CLEAR ERP IN CONTROL-PERM ERR */
         NI    IOSERR(IOSBPTR),B'11011111'                         0156
*   IORSTRT=OFF;                                                   0157
         NI    IORSTRT(EWAPTR),B'01111111'                         0157
*   GO TO XCTLSTAT;                 /* TO XCTL TO STAT UPDATE        */
*                                                                  0158
         B     XCTLSTAT                                            0158
*   /*****************************************************************/
*   /*                                                               */
*   /* EQUIPMENT CHECK                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0159
*EQUIPCK:                                                          0159
*   IF CONSOLE=ON THEN              /* IF CONSOLE, GO TO RETRY       */
EQUIPCK  TM    CONSOLE(EWAPTR),B'01000000'                         0159
         BO    @RT00159                                            0159
*     GO TO CONRETRY;                                              0160
*ERR506A:                                                          0161
*   IF CCWRST^>0 THEN               /* IF INVALID CCW ADDR, POST   0161
*                                      PERMANENT ERROR               */
ERR506A  LTR   CCWRST,CCWRST                                       0161
         BNP   @RT00161                                            0161
*     GO TO PGMERR;                                                0162
*   IF CMDCODE^=WRITECM THEN        /* IF READ CMD, LOGOUT AND ISSUE
*                                      'I/O ERR' MSG                 */
         CLI   CMDCODE(CCWRST),X'01'                               0163
         BNE   @RT00163                                            0163
*     GO TO ERRWTO;                                                0164
*   IF WRITECT=ON THEN              /* IF SECOND OCCURRENCE, RING  0165
*                                      BELL                          */
         TM    WRITECT(EWAPTR),B'00100000'                         0165
         BO    @RT00165                                            0165
*     GO TO ERR511;                                                0166
*   ELSE                                                           0167
*     WRITECT=ON;                   /* SET WRITE ERROR FLAG          */
         OI    WRITECT(EWAPTR),B'00100000'                         0167
*   GO TO RETRY;                                                   0168
         B     RETRY                                               0168
*   GEN(EJECT);                                                    0169
         EJECT
*                                                                  0170
*   /*****************************************************************/
*   /*                                                               */
*   /* INTERVENTION REQUIRED                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0170
*INTREQ:                                                           0170
*   CNTRLB1=ON;                     /* SET 'INT REQ' FLAG            */
INTREQ   OI    CNTRLB1(EWAPTR),B'00001000'                         0170
*   IF CONSOLE=ON THEN              /* IF CONSOLE, BROADCAST WTO     */
         TM    CONSOLE(EWAPTR),B'01000000'                         0171
         BNO   @RF00171                                            0171
*     DO;                                                          0172
*       IOSBDCST=ON;                /* SET BROADCAST FLAG            */
         OI    IOSBDCST(IOSBPTR),B'00000010'                       0173
*       GO TO XCTLWTO;              /* TO ISSUE BROADCAST 'INT REQ'  */
         B     XCTLWTO                                             0174
*     END;                                                         0175
*   ELSE                                                           0176
*     GO TO ALARM;                  /* TO RING BELL                  */
*   GEN(EJECT);                                                    0177
         EJECT
*                                                                  0178
*   /*****************************************************************/
*   /*                                                               */
*   /* BUSOUT CHECK                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0178
*BUSOUT:                                                           0178
*   IF CONSOLE=ON THEN              /* IF CONSOLE, GO TO RETRY       */
BUSOUT   TM    CONSOLE(EWAPTR),B'01000000'                         0178
         BO    @RT00178                                            0178
*     GO TO CONRETRY;                                              0179
*   IF IOBOERR=OFF THEN             /* IF FIRST OCCURRENCE, RETRY    */
         TM    IOBOERR(EWAPTR),B'00010000'                         0180
         BZ    @RT00180                                            0180
*     GO TO ERR514;                                                0181
*   ELSE                                                           0182
*ERR511:                                                           0182
*     CNTRLB0=ON;                   /* SET BUSOUT CHECK FLAG         */
ERR511   OI    CNTRLB0(EWAPTR),B'00000100'                         0182
*   EWUCSW=IOSCSW;                  /* SAVE ORIGINAL CSW             */
         MVC   EWUCSW(7),IOSCSW(IOSBPTR)                           0183
*ALARM:                                                            0184
*   IOSMDB=CNTLALRM;                /* IMMEDIATE OP TO RING BELL     */
ALARM    MVI   IOSMDB(IOSBPTR),X'0B'                               0184
*   IOSSMDA=ON;                     /* SET STAT MODIFIER FLAG        */
         OI    IOSSMDA(IOSBPTR),B'00010000'                        0185
*   SVC(15);                        /* TO RING THE BELL              */
         SVC   15                                                  0186
*   SVC(3);                         /* ISSUE SVC 3 TO EXIT           */
         SVC   3                                                   0187
*ERR514:                                                           0188
*   IOBOERR=ON;                     /* SET BUSOUT CHECK COUNT        */
ERR514   OI    IOBOERR(EWAPTR),B'00010000'                         0188
*   IF CSWCHE=OFF THEN              /* IF NO CHAN END, RETRY FROM TOP*/
         TM    CSWCHE(IOSBPTR),B'00001000'                         0189
         BNZ   @RF00189                                            0189
*     IORSTRT=OFF;                                                 0190
         NI    IORSTRT(EWAPTR),B'01111111'                         0190
*   GO TO RETRY;                                                   0191
         B     RETRY                                               0191
*   GEN(EJECT);                                                    0192
         EJECT
*                                                                  0193
*   /*****************************************************************/
*   /*                                                               */
*   /* LINK TO THE RESIDENT ERROR INTERPRETER ROUTINE TO INTERPRET   */
*   /* THE CSW STATUS BYTES.                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0193
*STATUS:                                                           0193
*   GEN REFS(CVTREG,CVTXITP) SETS(SAVREG,XCTLREG) FLOWS(CHANDCK,PGMERR,
*       UNITEX,ERRCORR);                                           0193
STATUS   DS    0H                                                  0193
         LR    SAVREG,BASREG        SAVE REG 15
         L     BASREG,CVTXITP(,CVTREG) ADDR OF ERR INTERPRETER RTN
STATBAS  BALR  XCTLREG,BASREG       TO INTERPRET STATUS BYTESS
         DC    X'1C'                CHANNEL DATA CHECK
         DC    AL1(ERSTAT1-STATBAS-2)
         DC    X'1A'                PROGRAM CHECK
         DC    AL1(ERSTAT2-STATBAS-4)
         DC    X'1B'                PROTECTION CHECK
         DC    AL1(ERSTAT2-STATBAS-6)
         DC    X'17'                UNIT EXCEPTION
         DC    AL1(ERSTAT3-STATBAS-8)
         DC    X'19'                INCORRECT LENGTH
         DC    AL1(ERSTAT2-STATBAS-10)
         DC    X'2F'                END OF TEST
         DC    AL1(ERSTAT4-STATBAS-12)
ERSTAT1  B     CHANDCK
ERSTAT2  B     PGMERR
ERSTAT3  B     UNITEX
ERSTAT4  B     ERRCORR
         EJECT
*                                                                  0194
*   /*****************************************************************/
*   /*                                                               */
*   /* UNIT EXCEPTION                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0194
*UNITEX:                                                           0194
*   IF CONSOLE=OFF THEN             /* IF NOT CONSOLE, PERM ERR      */
UNITEX   TM    CONSOLE(EWAPTR),B'01000000'                         0194
         BZ    @RT00194                                            0194
*     GO TO PGMERR;                                                0195
*   IF DATACMD^=RDINQ THEN          /* IF NOT READ,SKIP BLANKING   0196
*                                                            @ZA07601*/
         L     @14,IOSVST(,IOSBPTR)                                0196
         CLI   DATACMD(@14),X'0A'                                  0196
         BNE   @RT00196                                            0196
*     GO TO RETRY;                  /*                       @ZA07601*/
*   RFY                                                            0198
*    (IDREG,                                                       0198
*     SAVREG,                                                      0198
*     REG11) RSTD;                                                 0198
*                                                                  0198
*   /*****************************************************************/
*   /*                                                               */
*   /* BLANK OUT BUFFER AND GO TO RETRY REQUEST                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0199
*   SAVREG=DATACNT;                                                0199
         LH    SAVREG,DATACNT(,@14)                                0199
*   IDREG=DATADDR;                                                 0200
         L     IDREG,DATADDR(,@14)                                 0200
*   REG11=BASREG;                                                  0201
         LR    REG11,BASREG                                        0201
*   GEN SETS(IDREG,SAVREG,REG0,REG11,XCTLREG) REFS(BLANKBUF,IOSGENSV,
*       CVTREG,CVTPTRV) FLOWS(PGMERR);                             0202
         STM  REG0,IOSBPTR,IOSGENSV  SAVE REGS 0,1
         LR   IOSBPTR,IDREG     ADDR TO CONVERT
         LA    IOSBPTR,0(IOSBPTR)
         BCTR IOSBPTR,0   DECREMENT TO INSURE PAGE BNDRY
         L    BASREG,CVTPTRV(,CVTREG)   ADDR OF CONVERT RTN
         BALR XCTLREG,BASREG   TO CONVERT REAL TO VIRTUAL
         LR   IDREG,IOSBPTR   TRANSLATED BUFFER ADDRESS
         LM   REG0,IOSBPTR,IOSGENSV  RESTORE REGS 0,1
         LTR  BASREG,BASREG   TRANSLATION SUCCESSFUL
         LR   BASREG,REG11    RESTORE BASE REGISTER
         BNZ  PGMERR          NO-PERMANENT ERROR
         LA   IDREG,1(IDREG)  INCREMENT ADDRESS
         MVI  0(IDREG),X'40'
         BCTR SAVREG,0
         BCTR SAVREG,0
         EX   SAVREG,BLANKBUF
*   GO TO RETRY;                                                   0203
         B     RETRY                                               0203
*   RFY                                                            0204
*    (IDREG,                                                       0204
*     SAVREG,                                                      0204
*     REG11) UNRSTD;                                               0204
*BLANKBUF:                                                         0205
*   GEN(MVC 1(0,IDREG),0(IDREG));                                  0205
BLANKBUF MVC 1(0,IDREG),0(IDREG)
*   GEN(EJECT);                                                    0206
         EJECT
*                                                                  0207
*   /*****************************************************************/
*   /*                                                               */
*   /* PROGRAMMING ERROR                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0207
*PGMERR:                                                           0207
*   IOSERR=OFF;                     /* CLEAR ERP IN CONTROL-PERM ERR */
PGMERR   NI    IOSERR(IOSBPTR),B'11011111'                         0207
*   IORSTRT=OFF;                                                   0208
         NI    IORSTRT(EWAPTR),B'01111111'                         0208
*   SVC(15);                        /* TO POST PERMANENT ERROR       */
         SVC   15                                                  0209
*   SVC(3);                         /* ISSUE SVC 3 TO EXIT           */
         SVC   3                                                   0210
*   GEN(EJECT);                                                    0211
         EJECT
*                                                                  0212
*   /*****************************************************************/
*   /*                                                               */
*   /* ERROR CORRECTED                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0212
*ERRCORR:                                                          0212
*   IF IOSCOD=IOSNRMC THEN                                         0212
ERRCORR  CLI   IOSCOD(IOSBPTR),X'7F'                               0212
         BE    @RT00212                                            0212
*     GO TO ERR521;                                                0213
*   ELSE                                                           0214
*     IOSCOD=IOSNRMC;               /* CHANGE COMP CODE TO '7F'      */
         MVI   IOSCOD(IOSBPTR),X'7F'                               0214
*ERR521:                                                           0215
*   IOSERR=OFF;                     /* CLEAR FLAGS TO INDICATE ERROR
*                                      CORRECTED                     */
ERR521   DS    0H                                                  0216
*   IOSEX=OFF;                                                     0216
         NI    IOSERR(IOSBPTR),B'11011011'                         0216
*   IORSTRT=OFF;                                                   0217
         NI    IORSTRT(EWAPTR),B'01111111'                         0217
*   CSWUCK=OFF;                                                    0218
*                                                                  0218
         NI    CSWUCK(IOSBPTR),B'11111101'                         0218
*   /*****************************************************************/
*   /*                                                               */
*   /* XCTL TO STATISTICS UPDATE ROUTINE                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0219
*XCTLSTAT:                                                         0219
*   GEN REFS(CVTREG,CVTXTLER,STATID) SETS(IDREG,XCTLREG) EXIT NOSEQFLOW
*       ;                                                          0219
*                                                                  0219
XCTLSTAT DS    0H                                                  0219
         LH    IDREG,STATID         ID OF STAT UPDATE RTN
         L     XCTLREG,CVTXTLER(,CVTREG) ADDR OF RES XCTL RTN
         BR    XCTLREG              TO XCTL TO STAT UPDATE RTN
*   /*****************************************************************/
*   /*                                                               */
*   /* XCTL TO WRITE-TO-OPERATOR ROUTINE                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0220
*ERR524A:                                                          0220
*   IF CONSOLE=ON THEN              /* IF CONSOLE, GO TO RETRY       */
ERR524A  TM    CONSOLE(EWAPTR),B'01000000'                         0220
         BO    @RT00220                                            0220
*     GO TO CONRETRY;                                              0221
*ERRWTO:                                                           0222
*   IOSERR=OFF;                                                    0222
ERRWTO   NI    IOSERR(IOSBPTR),B'11011111'                         0222
*   IOSLOG=ON;                      /* TO INDICATE LOGOUT REQUIRED   */
*   IOSMSG=ON;                      /* TO INDICATE 'I/O ERR' MSG     */
         OI    IOSLOG(IOSBPTR),B'00000101'                         0224
*   IORSTRT=OFF;                                                   0225
         NI    IORSTRT(EWAPTR),B'01111111'                         0225
*XCTLWTO:                                                          0226
*   GEN REFS(CVTREG,CVTXTLER,WTOID) SETS(IDREG,XCTLREG) EXIT NOSEQFLOW;
XCTLWTO  DS    0H                                                  0226
         LH    IDREG,WTOID          ID OF WTO RTN
         L     XCTLREG,CVTXTLER(,CVTREG) ADDR OF RES XCTL RTN
         BR    XCTLREG              TO XCTL TO WTO RTN
               EJECT
*                                                                  0227
*   /*****************************************************************/
*   /*                                                               */
*   /* CHANNEL DATA CHECK                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0227
*CHANDCK:                                                          0227
*   IF CDKSTAT=OFF THEN                                            0227
CHANDCK  TM    CDKSTAT(EWAPTR),B'00000001'                         0227
         BNZ   @RF00227                                            0227
*     CDKSTAT=ON;                   /* SET DATA CHECK FLAG           */
         OI    CDKSTAT(EWAPTR),B'00000001'                         0228
*   IF CONSOLE=ON THEN              /* IF CONSOLE, GO TO RETRY       */
@RF00227 TM    CONSOLE(EWAPTR),B'01000000'                         0229
         BO    @RT00229                                            0229
*     GO TO CONRETRY;                                              0230
*   ELSE                                                           0231
*     GO TO ERR506A;                /* HANDLE LIKE EQUIP CHECK       */
         B     ERR506A                                             0231
*   GEN(EJECT);                                                    0232
         EJECT
*                                                                  0233
*   /*****************************************************************/
*   /*                                                               */
*   /* RETRY REQUEST                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0233
*CONRETRY:                                                         0233
*   IF EWANORTY=ON×                 /* IF ERPIB INDICATES NO RETRY 0233
*                                                            @ZA05633*/
