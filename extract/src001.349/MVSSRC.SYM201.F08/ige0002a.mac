*                                  DEVICE DEPENDENT INFORMATION    0070
*WAIERP  DS    XL128               AREA FOR INDIVIDUAL ERP USE     0070
*        ORG   EWAIERP                                             0070
*        SPACE 1                                                   0070
*  IOS USAGE OF ERP DEPENDENT AREA FOR READING SENSE DATA  AND     0070
*  FOR A TEMPORARY STORAGE BEFORE THE ERP IS INITIALLY ENTERED     0070
*WASNS   EQU   *                   SENSE DATA START                0070
*        DS    XL64                SENSE INFORMATION               0070
*WASCSW  DS    CL7                 SLOT TO SAVE CSW ON INTERCEPT   0070
*WAHL    EQU   32                  HEADER LENGTH                   0070
*        MEND                                                      0070
* */                                                               0070
*                                                                  0070
* /************************************************************/   0070
* /*                                                          */   0070
* /* THIS STRUCTURE DESCRIBES THE ERROR PROCEDURE  WORK AREA  */   0070
* /* COMMON SECTION WHICH IS INVOKED BY IECDERWA AS AN INNER  */   0070
* /* MACRO.  IT IS GOTTEN BY THE I/O  INTERRUPT  HANDLER  OR  */   0070
* /* POST STATUS. THE WORKAREA CONSISTS OF TWO SECTIONS: THE  */   0070
* /* COMMON SECTION WHICH CONTAINS INDICATORS, COUNTERS, AND  */   0070
* /* SENSE INFORMATION - AND  ERP DEPENDENT  SECTIONS  WHICH  */   0070
* /* ARE AVAILABLE FOR ERP USE AS NEEDED. THE DEPENDENT SEC-  */   0070
* /* TIONS ARE INCLUDED BY IECDERWA VIA OPTIONS THROUGH  THE  */   0070
* /* MACRO CALL.                                              */   0070
* /*                                                          */   0070
* /************************************************************/   0070
*                                                                  0070
*   DCL 1 EWA  BDY(DWORD) BASED(EWAPTR),                           0070
*       2  EWAHDR   PTR(31),       /* EWA HEADER              */   0070
*         3 EWAEXT  PTR(31),       /* ADDRESS OF WA EXTENTION */   0070
*       2  EWAFLAGS CHAR(4),       /* FOUR BYTES OF FLAGS     */   0070
*         3 EWAFLG1 BIT(8),        /* FLAG BYTE 1             */   0070
*          4 EWASLIS BIT(1),       /* WORK AREA INVOLVED IN   */   0070
*                                  /* RECOVERY SENSE          */   0070
*          4 *        BIT(3),      /* RESERVED                */   0070
*          4 EWASCCD  BIT(2),      /* SIO CONDITION CODE      */   0070
*           5 EWASCC3 BIT(2),      /* CONDITION CODE 3        */   0070
*            6 EWASCC2 BIT(1),     /* CONDITION CODE 2        */   0070
*            6 EWASCC1 BIT(1),     /* CONDITION CODE 1             0070
*                                     SEE END OF EWA FOR CC 0 */   0070
*          4 EWADDMSG BIT(1),      /* ERP DEPENDENT DATA TO BE     0070
*                                     INCLUDED IN I/O ERROR        0070
*                                     MESSAGE                 */   0070
*          4 EWABDSNS BIT(1),      /* SENSE UNSUCCESSFUL      */   0070
*         3 EWAFLG2 BIT(8),        /* FLAG BYTE 2             */   0070
*          4 EWAMDR BIT(1),        /* IF ON, MDR REQUEST           0070
*                                     IF OFF, OBR REQUEST     */   0070
*          4 EWAWTEMP BIT(1),      /* ON - TEMPORARY WRITE ERR     0070
*                                     COUNTER TO BE UPDATE IF      0070
*                                     DATA CHECK CONDITION         0070
*                                     OFF - TEMPORARY READ ERR     0070
*                                     COUNTER TO BE UPDATED   */   0070
*          4 EWACOVF BIT(1),       /* COUNTER OVERFLOW FOR         0070
*                                     STATISTICS UPDATE       */   0070
*          4 EWADIR  BIT(1),       /* DIR IN PROGRESS         */   0070
*          4 *     BIT(4),         /* RESERVED                */   0070
*         3 EWAFLG3 BIT(8),        /* FOR DEVICE DEPENDENT ERP     0070
*                                     USAGE                   */   0070
*          4 EWAJAM BIT(1),        /* 3800 PAPER JAM         @Y40MPLG*/
*         3 EWASNSCT BIT(8),       /* LOOP COUNT FOR SENSE         0070
*                                     FAILURE                 */   0070
*         4 *        BIT(3),       /* RESERVED                */   0070
*         4 EWASCTMX BIT(1),       /* MAX. # OF SENSES TRIED  */   0070
*       2  EWASSTAT BIT(16),       /* CSW STATUS ON SENSE OP-      0070
*                                     ERATION IF THIS IS A         0070
*                                     UNIT CHECK.             */   0070
*       2  EWACNTRS CHAR(4),       /* COUNTERS FOR ERP USE    */   0070
*         3 EWACNTR1 PTR(8),       /* COUNTER 1               */   0070
*         3 EWACNTR2 PTR(8),       /* COUNTER 2               */   0070
*         3 EWACNTR3 PTR(8),       /* COUNTER 3               */   0070
*         3 EWACNTR4 PTR(8),       /* COUNTER 4               */   0070
*       2  EWASTUP  CHAR(2),       /* STATISTICS INFORMATION       0070
*                                     FOR UPDATING STATISTICS */   0070
*       2  EWAERPIB CHAR(8),       /* ERPIB BUILT BY CCH FOR       0070
*                                     CHANNEL ERRORS TO INDI-      0070
*                                     CATE TO ATTEMPT A RETRY */   0070
*         3 *        CHAR(1),      /* RESERVED                */   0070
*         3 EWAUCB   PTR(24)   BDY(BYTE),                          0070
*                                  /* UCB ADDRESS             */   0070
*         3 EWARGFG1 BIT(8),       /* FLAG BYTE               */   0070
*          4 EWACSIO BIT(1),       /* CSW STORED AFTER SIO    */   0070
*          4 EWACINT BIT(1),       /* CSW STORED AFTER I/O INT*/   0070
*          4 EWACTIO BIT(1),       /* CSW STORED AFTR TEST I/O*/   0070
*          4 EWACHIO BIT(1),       /* CSW STORED AFTR HALT I/O*/   0070
*          4 *       BIT(1),       /* RESERVED                */   0070
*          4 EWACSNS BIT(1),       /* SENSE DATA WAS STORED   */   0070
*          4 EWACCNT BIT(1),       /* CSW COUNT IS VALID      */   0070
*          4 EWANORTY BIT(1),      /* NO RETRY INDICATOR      */   0070
*         3 EWARGFG2 BIT(8),       /* PROBABLE SOURCE INDICTRS*/   0070
*          4 EWACCPU BIT(1),       /* CPU ERROR               */   0070
*          4 EWACCHA BIT(1),       /* CHANNEL ERROR           */   0070
*          4 EWACSCU BIT(1),       /* STORAGE CONTROL UNIT ERR*/   0070
*          4 EWACSTG BIT(1),       /* STORAGE ERROR           */   0070
*          4 EWACCUE BIT(1),       /* CONTROL UNIT ERROR      */   0070
*          4 *       BIT(3),       /* RESERVED                */   0070
*         3 EWAXCSW1 BIT(8),       /* VALIDITY INDICATORS     */   0070
*          4 EWACITF BIT(1),       /* INTERFACE ADDR IS VALID */   0070
*          4 *       BIT(2),       /* RESERVED                */   0070
*          4 EWACSQV BIT(1),       /* SEQUENCE CODE IS VALID  */   0070
*          4 EWACUNS BIT(1),       /* UNIT STATUS IS VALID    */   0070
*          4 EWACCMD BIT(1),       /* COMMAND ADDRESS IS VALID*/   0070
*          4 EWACCHV BIT(1),       /* CHANNEL ADDR IS VALID   */   0070
*          4 EWACDAV BIT(1),       /* DEVICE ADDRESS IS VALID */   0070
*         3 EWAXCSW2 BIT(8),       /* TERMINATN & SEQNCE CODES*/   0070
*          4 EWACTEC BIT(2),       /* TERMINATION CODE             0070
*                                     SEE BELOW FOR VALUES    */   0070
*          4 *       BIT(2),       /* RESERVED                */   0070
*          4 EWACDIN BIT(1),       /* I/O ERROR ALERT         */   0070
*          4 EWACSEQ BIT(3),       /* SEQUENCE CODES - SEE         0070
*                                     BELOW                   */   0070
*       2  EWACHA   CHAR(2),       /* UNIT ADDRESS ON WHICH        0070
*                                     LAST I/O WAS STARTED    */   0070
*       2  *        CHAR(1),       /* RESERVED                */   0070
*       2  EWACPU   CHAR(1),       /* CPUID OF I/O ERROR      */   0070
*       2  EWADCNT  CHAR(1),       /* # OF BYTES OF OBR INFO  */   0070
*       2  EWADDISP PTR(24),       /* ADDRESS OF OBR DEVICE        0070
*                                     DEPENDENT INFORMATION   */   0070
*       2  EWAIERP  CHAR(128)  BDY(DWORD);                         0070
*                                  /* AREA FOR INDIVIDUL ERPS */   0071
*                                                                  0071
*  DCL 1 EWADDIOS CHAR(71) DEF(EWAIERP) BDY(DWORD),                0071
*                                                                  0071
* /*  IOS USAGE OF ERP DEPENDENT AREA FOR READING SENSE DATA  */   0071
* /*  FOR TEMPORARY STORAGE BEFORE ERP IS INITIALLY ENTERED.  */   0071
*                                                                  0071
*       2  EWASNS  CHAR(64),       /* SENSE DATA START        */   0071
*       2  EWASCSW CHAR(7),        /* SLOT TO SAVE CSW ON          0071
*                                     INTERCEPT               */   0071
*       2  EWAEND  CHAR(0);        /* END OF EWA              */   0071
*   DCL EWASCC0 BIT(8) CONSTANT('00'X);  /*                        0072
*                                     CONDITION CODE 0        */   0072
*   DCL EWATER0 BIT(2) CONSTANT('00'B);  /*                        0073
*                                  INTERFACE DISCONNECT       */   0073
*   DCL EWATER1 BIT(2) CONSTANT('01'B);  /*                        0074
*                                  STOP, STACK, NORMAL TERMNTN*/   0074
*   DCL EWATER2 BIT(2) CONSTANT('10'B);  /*                        0075
*                                  SELECTIVE RESET            */   0075
*   DCL EWATER3 BIT(2) CONSTANT('11'B);  /*                        0076
*                                  SYSTEM RESET               */   0076
*   DCL EWACSEQ0 BIT(3) CONSTANT('000'B);                          0077
*   DCL EWACSEQ1 BIT(3) CONSTANT('001'B);                          0078
*   DCL EWACSEQ2 BIT(3) CONSTANT('010'B);                          0079
*   DCL EWACSEQ3 BIT(3) CONSTANT('011'B);                          0080
*   DCL EWACSEQ4 BIT(3) CONSTANT('100'B);                          0081
*   DCL EWACSEQ5 BIT(3) CONSTANT('101'B);                          0082
*   DCL EWACSEQ6 BIT(3) CONSTANT('110'B);                          0083
*   DCL EWACSEQ7 BIT(3) CONSTANT('111'B);                          0084
*/*                                                                0085
*        MACRO                                                     0085
*        EWUMAP                                                    0085
**/                                                                0085
*/********************************************************************/
*/* EWUMAP MAPS THE DEVICE DEPENDENT SECTION OF THE ERP WORK AREA    */
*/*        FOR UNIT RECORD DEVICES. IT IS CALLED AS AN INNER MACRO   */
*/*        FROM IECDERWA.                                            */
*/*        THE FOLLOWING CODES ARE USED TO INDICATE WHICH DEVICES    */
*/*        THE FIELDS APPLY TO:                                      */
*/*        A - 1403,1443                                             */
*/*        B - 3211                                                  */
*/*        C - 1442,2501,2520,2596                                   */
*/*        D - 1402,2540                                             */
*/*        E - 3505,3525                                             */
*/*        F - 2671                                                  */
*/********************************************************************/
*/*                                                                0085
*        ORG   EWAIERP                                             0085
*WUSN0   DS    X                  1ST SENSE BYTE                   0085
*WUCREJ  EQU   X'80'              COMMAND REJECT      (ALL)        0085
*WUINT   EQU   X'40'              INTERVENTION REQD.  (ALL)        0085
*WUBUSO  EQU   X'20'              BUS OUT             (ALL)        0085
*WUEQCK  EQU   X'10'              EQUIPMENT CHECK     (ALL)        0085
*WUDTCK  EQU   X'08'              DATA CHECK          (ALL)        0085
*WUBUFP  EQU   X'04'              BUFFER PARITY       (A,B)        0085
*WUOVRN  EQU   X'04'              OVERRUN             (C)          0085
*WULDCK  EQU   X'02'              LOAD CHECK          (B)          0085
*WUUSEQ  EQU   X'02'              UNUSUAL SEQUENCE    (D)          0085
*WUABFR  EQU   X'02'              ABNORMAL FMT RESET  (E)          0085
*WUCH9   EQU   X'01'              CHANNEL 9           (A,B)        0085
*WUPEK   EQU   X'01'              PERMANENT ERROR KEY (E)          0085
*WUSN1   DS    X                  2ND SENSE BYTE                   0085
*WUCRTRY EQU   X'80'              COMMAND RETRY       (B)          0085
*WUPERM  EQU   X'80'              PERMANENT ERROR     (E)          0085
*WUPCK   EQU   X'40'              PRINT CHECK         (B)          0085
*WUARTRY EQU   X'40'              AUTOMATIC RETRY     (E)          0085
*WUPQ    EQU   X'20'              PRINT QUALITY       (B)          0085
*WUMOT   EQU   X'20'              MOTION ERROR        (E)          0085
*WULNPOS EQU   X'10'              LINE POSITION       (B)          0085
*WUIRTRY EQU   X'10'              RETRY AFTER INT.REQ.(E)          0085
*WUFORM  EQU   X'08'              FORMS CHECK         (B)          0085
*WUSUPR  EQU   X'04'              COMMAND SUPPRESS    (B)          0085
*WUMECMO EQU   X'02'              MECHANICAL MOTION   (B)          0085
*WUSN2   DS    X                  3RD SENSE BYTE                   0085
*WUUCSBP EQU   X'80'              UCS PARITY ERROR    (B)          0085
*WUPLBP  EQU   X'40'              PLB PARITY ERROR    (B)          0085
*WUFCBP  EQU   X'20'              FCB PARITY ERROR    (B)          0085
*WUSN3   DS    X                  4TH SENSE BYTE                   0085
*WUSN4   DS    X                  5TH SENSE BYTE                   0085
*WUSN5   DS    X                  6TH SENSE BYTE                   0085
*        DS    XL2                RESERVED                         0085
*WUWORK  DS    0D,120X            RESERVED FOR COMPILER ASSIGNED TEMPS
*        MEND                                                      0085
**/                                                                0085
* DCL                                                              0085
*  1 EWUMAP CHAR(128)BDY(DWORD)DEF(EWAIERP),                       0085
*   2 EWUSN0 BIT(8),            /*1ST SENSE BYTE                     */
*    3 EWUCREJ BIT(1),          /*COMMAND REJECT      (ALL)          */
*    3 EWUINT  BIT(1),          /*INTERVENTION REQD.  (ALL)          */
*    3 EWUBUSO BIT(1),          /*BUS OUT             (ALL)          */
*    3 EWUEQCK BIT(1),          /*EQUIPMENT CHECK     (ALL)          */
*    3 EWUDTCK BIT(1),          /*DATA CHECK          (ALL)          */
*    3 EWUBUFP BIT(1),          /*BUFFER PARITY       (A,B)          */
*     4 EWUOVRN BIT(1),         /*OVERRUN             (C)            */
*    3 EWULDCK BIT(1),          /*LOAD CHECK          (B)            */
*     4 EWUUSEQ BIT(1),         /*UNUSUAL SEQUENCE    (D)            */
*      5 EWUABFR BIT(1),        /*ABNORMAL FMT RESET  (E)            */
*    3 EWUCH9  BIT(1),          /*CHANNEL 9           (A,B)          */
*     4 EWUPEK  BIT(1),         /*PERMANENT ERROR KEY (E)            */
*   2 EWUSN1 BIT(8),            /*2ND SENSE BYTE                     */
*    3 EWUCRTRY BIT(1),         /*COMMAND RETRY       (B)            */
*     4 EWUPERM BIT(1),         /*PERMANENT ERROR     (E)            */
*    3 EWUPCK BIT(1),           /*PRINT CHECK         (B)            */
*     4 EWUARTRY BIT(1),        /*AUTOMATIC RETRY     (E)            */
*    3 EWUPQ BIT(1),            /*PRINT QUALITY       (B)            */
*     4 EWUMOT BIT(1),          /*MOTION ERROR        (E)            */
*    3 EWULNPOS BIT(1),         /*LINE POSITION       (B)            */
*     4 EWUIRTRY BIT(1),        /*RETRY AFTER INT.REQ.(E)            */
*    3 EWUFORM BIT(1),          /*FORMS CHECK         (B)            */
*    3 EWUSUPR BIT(1),          /*COMMAND SUPPRESS    (B)            */
*    3 EWUMECMO BIT(1),         /*MECHANICAL MOTION   (B)            */
*    3 * BIT(1),                /*RESERVED                           */
*   2 EWUSN2 BIT(8),            /*3RD SENSE BYTE                     */
*    3 EWUUCSBP BIT(1),         /*UCS PARITY ERROR    (B)            */
*    3 EWUPLBP BIT(1),          /*PLB PARITY ERROR    (B)            */
*    3 EWUFCBP BIT(1),          /*FCB PARITY ERROR    (B)            */
*   2 EWUSN3 BIT(8),            /*4TH SENSE BYTE                     */
*   2 EWUSN4 BIT(8),            /*5TH SENSE BYTE                     */
*   2 EWUSN5 BIT(8),            /*6TH SENSE BYTE                     */
*   2 * CHAR(2),                /*RESERVED                           */
*   2 EWUWORK CHAR(120)BDY(DWORD);/*RESERVED FOR COMPILER TEMPS      */
*START:                                                            0086
*   RFY                                                            0086
*     RE RSTD;                                                     0086
START    DS    0H                                                  0087
*   IOSBPTR=R1;                     /* PICKUP POINTER TO IOSB FROM 0087
*                                      LINKAGE REGISTER              */
         LR    IOSBPTR,R1                                          0087
*   EWAPTR=IOSERP;                  /* INITIALIZE POINTER TO ERP WORK
*                                      AREA                          */
         L     EWAPTR,IOSERP(,IOSBPTR)                             0088
*   DATABASE=ADDR(EWUWORK);         /* BASE REG FOR @DATD DSECT      */
         LA    DATABASE,EWUWORK(,EWAPTR)                           0089
*   RFY                                                            0090
*     R1 UNRSTD;                                                   0090
*   RFY                                                            0091
*     RE UNRSTD;                                                   0091
*   CVTPTR=CVTPTRXX;                                               0092
         MVC   CVTPTR(4),CVTPTRXX                                  0092
*   IOSERR=ON;                      /* INDICATE ERP IN CONTROL       */
*   IOSEX=ON;                                                      0094
         OI    IOSERR(IOSBPTR),B'00100100'                         0094
*   IF IOSCCHN=ON&IOSDCHN=ON THEN                                  0095
         TM    IOSCCHN(IOSBPTR),B'11000000'                        0095
         BO    @RT00095                                            0095
*     GOTO MSG;                     /* NO RECOVERY ATTEMPTED FOR   0096
*                                      MIXED CHANNEL PROGS           */
*   IF IOSCCHN=ON THEN                                             0097
         TM    IOSCCHN(IOSBPTR),B'01000000'                        0097
         BNO   @RF00097                                            0097
*     DO;                                                          0098
*       CCWPTR=IOSCSWCA-8;          /* POINT TO FAILING CCW FOR CC   */
         L     @12,IOSCSWCA-1(,IOSBPTR)                            0099
         LA    @12,0(,@12)                                         0099
         SH    @12,@CH00088                                        0099
         ST    @12,CCWPTR                                          0099
*       RFY                                                        0100
*         R3 RSTD;                                                 0100
*       R3=CCWPTR;                                                 0101
         LR    R3,@12                                              0101
*       GEN REFS(R3) SETS(R3)(LRA   R3,0(R3));                     0102
         LRA   R3,0(R3)
*       IF ENTFLAG=ON THEN          /* TEST FOR ERROR ON SAME CCW -
*                                      RESET EQUIP CK CTR IF NOT SAME
*                                                            @ZA11096*/
         TM    ENTFLAG(EWAPTR),B'01000000'                         0103
         BNO   @RF00103                                            0103
*         IF R3^=IOSRST THEN                                       0104
         C     R3,IOSRST(,IOSBPTR)                                 0104
         BE    @RF00104                                            0104
*           EQCTR=0;                                               0105
         MVI   EQCTR(EWAPTR),X'00'                                 0105
*         ELSE                                                     0106
*           ;                                                      0106
@RF00104 DS    0H                                                  0107
*       ELSE                                                       0107
*         ;                                                        0107
@RF00103 DS    0H                                                  0108
*       IOSRST=R3;                                                 0108
         ST    R3,IOSRST(,IOSBPTR)                                 0108
*       RFY                                                        0109
*         R3 UNRSTD;                                               0109
*     END;                                                         0110
*   ELSE                                                           0111
*     CCWPTR=IOSVST;                /* POINT TO FIRST CCW FOR CD     */
         B     @RC00097                                            0111
@RF00097 MVC   CCWPTR(4),IOSVST(IOSBPTR)                           0111
*STATUS:                                                           0112
*   RFY                                                            0112
*    (R0,                                                          0112
*     R1,                                                          0112
*     RE,                                                          0112
*     RB,                                                          0112
*     RC,                                                          0112
*     RF) RSTD;                                                    0112
@RC00097 DS    0H                                                  0112
STATUS   DS    0H                                                  0113
*   R1=IOSBPTR;                                                    0113
         LR    R1,IOSBPTR                                          0113
*   RF=CVTXITP;                     /* ADDRESS OF ERROR INTERPERTER  */
         L     @04,CVTPTR                                          0114
         L     RF,CVTXITP(,@04)                                    0114
*   GEN REFS(RE,RF) FLOWS(LOG$MSG,NOLOGMSG,UEXIL,OK,SENSE);        0115
G1       BALR  RE,RF  CALL INTERPERTER WITH LIST ADDR IN REG 12
         DC    X'1D',AL1(G2-G1-2)   CHANNEL CONTROL CHECK
         DC    X'1E',AL1(G2-G1-4)   INTERFACE CONTROL CHECK
         DC    X'1F',AL1(G2-G1-6)   CHAINING CHECK
         DC    X'16',AL1(SENSE-G1-8) UNIT CHECK
         DC    X'1C',AL1(G2-G1-10)  CHANNEL DATA CHECK
         DC    X'1A',AL1(G4-G1-12)  PROGRAM CHECK
         DC    X'1B',AL1(G4-G1-14)  PROTECTION CHECK
         DC    X'17',AL1(G5-G1-16)  UNIT EXCEPTION
         DC    X'19',AL1(G5-G1-18)  INCORRECT LENGTH
         DC    X'2F',AL1(G6-G1-20)  GOOD STATUS
*SENSE:                                                            0116
*   EWASTUP(1:1)=EWASTUP(1:1)IOSSNS(1:8)&STATMASK;                0116
SENSE    MVC   @TS00001(1),IOSSNS(IOSBPTR)                         0116
         NI    @TS00001,X'78'                                      0116
         OC    EWASTUP(1,EWAPTR),@TS00001                          0116
*   R1=IOSBPTR;                                                    0117
         LR    R1,IOSBPTR                                          0117
*   RF=CVTXITP;                                                    0118
         L     @04,CVTPTR                                          0118
         L     RF,CVTXITP(,@04)                                    0118
*   GEN REFS(RE,RF) FLOWS(LOG$MSG,INTREQ,RETRY,MSG);               0119
G3       BALR  RE,RF
         DC    X'05',AL1(G2-G3-2)   UNDEFINED
         DC    X'06',AL1(G2-G3-4)   UNDEFINED
         DC    X'07',AL1(G2-G3-6)   UNDEFINED
         DC    X'03',AL1(GA-G3-8)   EQUIPMENT CHECK        /*@ZA11096*/
         DC    X'01',AL1(G7-G3-10)  INTERVENTION REQUIRED
         DC    X'02',AL1(G8-G3-12)  BUSOUT CHECK
         DC    X'04',AL1(G2-G3-14)  DATA CHECK
         DC    X'00',AL1(G9-G3-16)  COMMAND REJECT
         DC    X'2F',AL1(G2-G3-18)  UNIT CHECK & NO SENSE BITS
G2       B     LOG$MSG
G4       B     NOLOGMSG
G5       B     UEXIL
G6       B     OK
G7       B     INTREQ
G8       B     RETRY
G9       B     MSG
GA       B     EQCHECK                                     /*@ZA11096*/
*   RFY                                                            0120
*    (R0,                                                          0120
*     R1,                                                          0120
*     RE,                                                          0120
*     RB,                                                          0120
*     RC,                                                          0120
*     RF) UNRSTD;                                                  0120
*                                                                  0120
*   /*****************************************************************/
*   /*                                                               */
*   /* EQUIPMENT CHECK RETRY CCW 3 TIMES.PRINT INTERVENTION REQ FOR  */
*   /* EACH TRY. IF NOT SUCCESSFUL, GO TO PERMANENT ERROR    @ZA11096*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0121
*EQCHECK:                           /*                       @ZA11096*/
*   IF EQCTR<'03'X THEN             /*                       @ZA11096*/
EQCHECK  CLI   EQCTR(EWAPTR),X'03'                                 0121
         BNL   @RF00121                                            0121
*     DO;                                                          0122
*       EQCTR=EQCTR+1;              /*                       @ZA11096*/
         LA    @12,1                                               0123
         MVC   @ZT00003+3(1),EQCTR(EWAPTR)                         0123
         AL    @12,@ZT00003                                        0123
         STC   @12,EQCTR(,EWAPTR)                                  0123
*       ENTFLAG=ON;                 /*                       @ZA18279*/
         OI    ENTFLAG(EWAPTR),B'01000000'                         0124
