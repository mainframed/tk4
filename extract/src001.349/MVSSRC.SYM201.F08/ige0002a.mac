         LA    RD,253                                              0133
*   RE=CVTXTLER;                                                   0134
         L     @12,CVTPTR                                          0134
         L     RE,CVTXTLER(,@12)                                   0134
*   R1=IOSBPTR;                                                    0135
         LR    R1,IOSBPTR                                          0135
*   GEN REFS(RE)(BR    RE);                                        0136
