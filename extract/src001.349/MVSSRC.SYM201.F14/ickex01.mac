         TITLE '******************  ICKEX01:  DMS EXECUTIVE  ***********
               *********'
        MACRO
        GETMAIN &R,&LV=
        L     REG15,0(REG1)           GDTTBL ADDRESS
        L     REG15,GDTPRL(REG15)     PROL ROUTINE ADDRESS
        CNOP  2,4                     BOUNDARY ALIGNMENT
        LA    REG14,*+14              RETURN ADDRESS
        BALR  REG1,REG15              LINKAGE TO PROL ROUTINE
        DC    A(MODID)                MODULE ID ADDRESS
        DC    A(@SIZDATD)             AUTOMATIC STORAGE SIZE ADDRESS
        MEND
ICKEX01  CSECT ,                                                   0002
@MAINENT DS    0H                                                  0002
         USING *,@15                                               0002
         B     @PROLOG                                             0002
         DC    AL1(16)                                             0002
         DC    C'ICKEX01   78.192'                                 0002
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0002
         BALR  @10,0                                               0002
@PSTART  LA    @12,4095(,@10)                                      0002
         USING @PSTART,@10                                         0002
         USING @PSTART+4095,@12                                    0002
         L     @00,@SIZDATD                                        0002
         GETMAIN  R,LV=(0)
         LR    @08,@01                                             0002
         LA    @09,4095(,@08)                                      0002
         USING @DATD,@08                                           0002
         USING @DATD+4095,@09                                      0002
         ST    @13,@SA00001+4                                      0002
         LM    @00,@01,20(@13)                                     0002
         ST    @08,8(,@13)                                         0002
         LR    @13,@08                                             0002
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
         MVC   @PC00001(4),0(@01)                                  0002
*                                      /*PATCH AREA FOR MAINTENANCE  */
*        /************************************************************/
*        /*                                                          */
*        /*  CALL ICKDMS INITIALIZATION ROUTINE - SCANS INVOKER'S    */
*        /*  PARAMETER LIST AND PERFORMS SYSTEM INITIALIZATION       */
*        /*  FOR THE TEXT PROCESSOR AND THE INPUT/OUTPUT ADAPTER.    */
*        /*  (SYSTEM ADAPTER INITIALIZATION HAS BEEN COMPLETED       */
*        /*  BEFORE THE EXECUTIVE RECEIVES CONTROL).                 */
*        /*                                                          */
*        /************************************************************/
*                                                                  0193
*        CALL ICKSACL0(GDTTBL,INITNAME);                           0193
         L     @11,@PC00001                                        0193
         ST    @11,@AL00001                                        0193
         LA    @03,@CC00483                                        0193
         ST    @03,@AL00001+4                                      0193
         MVI   @AL00001+4,X'80'                                    0193
         L     @15,GDTCAL(,@11)                                    0193
         LA    @01,@AL00001                                        0193
         BALR  @14,@15                                             0193
*        RESPECIFY RTNREG RSTD;                                    0194
*        TESTRC = RTNREG;                                          0195
         LR    TESTRC,RTNREG                                       0195
*        RESPECIFY RTNREG UNRSTD;                                  0196
*                                                                  0196
*        /************************************************************/
*        /*                                                          */
*        /*  SET THE ICKDMS SYSTEM CONDITION CODES -                 */
*        /*    MAXIMUM COND CODE FOR ALL ICKDMS FUNCTION PROCESSING  */
*        /*    LAST FUNCTION COND CODE FOR THE CURRENT FUNCTION      */
*        /*                                                          */
*        /************************************************************/
*                                                                  0197
*        MAXCOND  = TESTRC;                                        0197
         STH   TESTRC,MAXCOND                                      0197
*        LASTCOND = TESTRC;                                        0198
         STH   TESTRC,LASTCOND                                     0198
*        /************************************************************/
*        /*                                                          */
*        /*  ROUTE CONTROL BETWEEN THE READER/INTERPRETER (PROCESS   */
*        /*  THE ICKDMS COMMAND INPUT AND BUILD THE FDT) AND THE     */
*        /*  SPECIFIED FUNCTION SUPPORT ROUTINE (THE FSR NAME IS     */
*        /*  RETURNED FROM THE READER/INTERPRETER) UNTIL SYSIN       */
*        /*  END-OF-FILE HAS BEEN ENCOUNTERED.                       */
*        /*                                                          */
*        /************************************************************/
*                                                                  0199
*        CALL MAIN;                                                0199
*                                                                  0199
         BAL   @14,MAIN                                            0199
*        IF GDTDBG = NULLPTR                                       0200
*          THEN;                                                   0200
         L     @11,@PC00001                                        0200
         L     @11,GDTDBG(,@11)                                    0200
         LTR   @11,@11                                             0200
         BZ    @RT00200                                            0200
*          ELSE                                                    0202
*            CALL ICKDB010(GDTTBL,'EXMN');                         0202
*                                                                  0202
         L     @11,@PC00001                                        0202
         ST    @11,@AL00001                                        0202
         LA    @15,@CC00495                                        0202
         ST    @15,@AL00001+4                                      0202
         MVI   @AL00001+4,X'80'                                    0202
         L     @15,GDTDBG(,@11)                                    0202
         LA    @01,@AL00001                                        0202
         BALR  @14,@15                                             0202
*        /************************************************************/
*        /*                                                          */
*        /*  CALL ICKDMS TERMINATION ROUTINE - PERFORMS TERMINATION  */
*        /*  FOR THE TEXT PROCESSOR AND THE INPUT/OUTPUT ADAPTER,    */
*        /*  AND UPDATES THE INVOKER'S PARAMETER LIST.               */
*        /*                                                          */
*        /************************************************************/
*                                                                  0203
*        CALL ICKSACL0(GDTTBL,TERMNAME,MAXCOND);                   0203
@RT00200 L     @11,@PC00001                                        0203
         ST    @11,@AL00001                                        0203
         LA    @03,@CC00485                                        0203
         ST    @03,@AL00001+4                                      0203
         LA    @03,MAXCOND                                         0203
         ST    @03,@AL00001+8                                      0203
         MVI   @AL00001+8,X'80'                                    0203
         L     @15,GDTCAL(,@11)                                    0203
         LA    @01,@AL00001                                        0203
         BALR  @14,@15                                             0203
*        RESPECIFY RTNREG RSTD;                                    0204
*        TESTRC = RTNREG;                                          0205
         LR    TESTRC,RTNREG                                       0205
*        RESPECIFY RTNREG UNRSTD;                                  0206
*                                                                  0206
*        /************************************************************/
*        /*                                                          */
*        /*  RETURN TO THE SYSTEM ADAPTER TO COMPLETE PROCESSING     */
*        /*                                                          */
*        /************************************************************/
*                                                                  0207
*        CALL ICKSAEP0(GDTTBL,MODID,MAXCOND);                      0207
         L     @11,@PC00001                                        0207
         ST    @11,@AL00001                                        0207
         LA    @15,MODID                                           0207
         ST    @15,@AL00001+4                                      0207
         LA    @15,MAXCOND                                         0207
         ST    @15,@AL00001+8                                      0207
         MVI   @AL00001+8,X'80'                                    0207
         L     @15,GDTEPL(,@11)                                    0207
         LA    @01,@AL00001                                        0207
         BALR  @14,@15                                             0207
*/********************************************************************/
*/*                                                                  */
*/*  PROCEDURE NAME - MAIN                                           */
*/*                                                                  */
*/*  FUNCTION - ROUTE CONTROL BETWEEN THE READER/INTERPRETER (TO     */
*/*             PROCESS THE ICKDMS INPUT COMMANDS AND BUILD THE      */
*/*             FDT) AND THE SPECIFIED FUNCTION SUPPORT ROUTINE (THE */
*/*             FSR NAME IS RETURNED FROM THE READER/INTERPRETER)    */
*/*                                                                  */
*/*             ROUTING IS CONTROLLED BY THE 'SWITCH' SWITCH WHICH   */
*/*             IS INITIALIZED TO 'SWITCHRI' (R/I NEXT TO RECEIVE    */
*/*             CONTROL) AND FOLLOWING THE CALLS TO THE READER/      */
*/*             INTERPRETER AND FUNCTION ROUTINES IS UPDATED TO      */
*/*             'SWITCHFS' AND 'SWITCHRI', RESPECITIVELY             */
*/*                                                                  */
*/*             THE SYSTEM CONDITION CODES ARE UPDATED BY THE        */
*/*             READER/INTERPRETER AND FOLLOWING EACH CALL TO THE    */
*/*             FUNCTION SUPPORT ROUTINE                             */
*/*                                                                  */
*/*  INPUT - MAXIMUM SYSTEM CONDITION CODE - INITIALIZED TO ZERO     */
*/*          (NO EXCEPTIONAL CONDITIONS)                             */
*/*                                                                  */
*/*  OUTPUT - MAXIMUM SYSTEM CONDITION CODE - UPDATED AS A RESULT OF */
*/*           READER/INTERPRETER AND FUNCTION ROUTINE PROCESSING     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0208
*MAIN:   PROCEDURE;                                                0208
@EL00001 L     @13,4(,@13)                                         0208
@EF00001 L     @00,@SIZDATD                                        0208
         LR    @01,@08                                             0208
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0208
         BR    @14                                                 0208
MAIN     STM   @14,@12,@SA00002                                    0208
*        OLDERID2 = NEWERID2;                                      0209
         L     @11,@PC00001                                        0209
         L     @11,GDTTR2(,@11)                                    0209
         MVC   @TS00001(95),NEWERID2(@11)                          0209
         MVC   OLDERID2(95,@11),@TS00001                           0209
*        NEWID2 = 'EX1M';                                          0210
*                                                                  0210
         MVC   NEWID2(4,@11),@CC00499                              0210
*        SWITCH = SWITCHRI;                                        0211
         NI    SWITCH,B'01111111'                                  0211
*        EOFCOND = NOEOF;                                          0212
*                                                                  0212
         SLR   EOFCOND,EOFCOND                                     0212
*        /************************************************************/
*        /*                                                          */
*        /*  LOOP THROUGH READER/INTERPRETER AND FUNCTION SUPPORT    */
*        /*  ROUTINES UNTIL SYSTEM CONDITION CODES = 16 (TERMINATING */
*        /*  CONDITION), OR UNTIL SYSIN END-OF-FILE HAS BEEN FOUND   */
*        /*  BY THE READER/INTERPRETER (END-OF-FILE RETURN CODE)     */
*        /*                                                          */
*        /************************************************************/
*                                                                  0213
*        DO WHILE (MAXCOND < CONDTERM & EOFCOND = NOEOF);          0213
*                                                                  0213
         B     @DE00213                                            0213
@DL00213 DS    0H                                                  0214
*          /**********************************************************/
*          /*                                                        */
*          /*  TEST SWITCH TO DETERMINE WHETHER READER/INTERPRETER   */
*          /*  OR FUNCTION SUPPORT ROUTINE RECEIVES CONTROL          */
*          /*                                                        */
*          /**********************************************************/
*                                                                  0214
*          IF SWITCH = SWITCHRI                                    0214
*            THEN                                                  0214
         TM    SWITCH,B'10000000'                                  0214
         BNZ   @RF00214                                            0214
*              DO;                                                 0215
*              /******************************************************/
*              /*                                                    */
*              /*  READER/INTERPRETER PROCESSING NEXT                */
*              /*                                                    */
*              /******************************************************/
*                                                                  0216
*              CALL CALLRI;                                        0216
*                                                                  0216
         BAL   @14,CALLRI                                          0216
*              SWITCH = SWITCHFS;                                  0217
         OI    SWITCH,B'10000000'                                  0217
*              END;                                                0218
*                                                                  0218
*            ELSE                                                  0219
*              DO;                                                 0219
         B     @RC00214                                            0219
@RF00214 DS    0H                                                  0220
*              /******************************************************/
*              /*                                                    */
*              /*  FUNCTION SUPPORT ROUTINE (FSR) PROCESSING NEXT    */
*              /*                                                    */
*              /******************************************************/
*                                                                  0220
*              CALL CALLFSR;                                       0220
*                                                                  0220
         BAL   @14,CALLFSR                                         0220
*              SWITCH = SWITCHRI;                                  0221
         NI    SWITCH,B'01111111'                                  0221
*              END;                                                0222
*                                                                  0222
*        END;                                                      0223
*                                                                  0223
@RC00214 DS    0H                                                  0223
@DE00213 LH    @11,MAXCOND                                         0223
         CH    @11,@CH00263                                        0223
         BNL   @DC00213                                            0223
         LTR   EOFCOND,EOFCOND                                     0223
         BZ    @DL00213                                            0223
@DC00213 DS    0H                                                  0224
*        RETURN;                                                   0224
@EL00002 DS    0H                                                  0224
@EF00002 DS    0H                                                  0224
@ER00002 LM    @14,@12,@SA00002                                    0224
         BR    @14                                                 0224
*/********************************************************************/
*/*                                                                  */
*/*  PROCEDURE NAME - CALLRI                                         */
*/*                                                                  */
*/*  FUNCTION - INVOKE THE READER/INTERPRETER TO PROCESS THE NEXT    */
*/*             INPUT ICKDMS COMMAND.  THE POOL AREA NAME IN WHICH   */
*/*             THE FDT WILL BE BUILT IS PASSED TO THE READER/       */
*/*             INTERPRETER.  (THIS POOL AREA WILL BE RELEASED BY    */
*/*             THE EXECUTIVE FOLLOWING FUNCTION PROCESSING)         */
*/*                                                                  */
*/*  INPUT - MAXIMUM AND LAST FUNCTION CONDITION CODES - TO CONTROL  */
*/*          READER/INTERPRETER PROCESSING                           */
*/*                                                                  */
*/*  OUTPUT - FUNCTION SUPPORT ROUTINE NAME (RETURNED BY THE         */
*/*           READER/INTERPRETER)                                    */
*/*           FUNCTION DATA TABLE (BUILT BY THE READER/INTERPRETER)  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0225
*CALLRI: PROCEDURE;                                                0225
*                                                                  0225
CALLRI   STM   @14,@02,@SA00003                                    0225
         STM   @04,@12,@SA00003+20                                 0225
*        OLDERID2 = NEWERID2;                                      0226
         L     @11,@PC00001                                        0226
         L     @15,GDTTR2(,@11)                                    0226
         MVC   @TS00001(95),NEWERID2(@15)                          0226
         MVC   OLDERID2(95,@15),@TS00001                           0226
*        NEWID2 = 'EX1R';                                          0227
*                                                                  0227
         MVC   NEWID2(4,@15),@CC00505                              0227
*        IF GDTDBG = NULLPTR                                       0228
*          THEN;                                                   0228
         L     @11,GDTDBG(,@11)                                    0228
         LTR   @11,@11                                             0228
         BZ    @RT00228                                            0228
*          ELSE                                                    0230
*            CALL ICKDB010(GDTTBL,'EXRI');                         0230
*                                                                  0230
         L     @11,@PC00001                                        0230
         ST    @11,@AL00001                                        0230
         LA    @15,@CC00506                                        0230
         ST    @15,@AL00001+4                                      0230
         MVI   @AL00001+4,X'80'                                    0230
         L     @15,GDTDBG(,@11)                                    0230
         LA    @01,@AL00001                                        0230
         BALR  @14,@15                                             0230
*        /************************************************************/
*        /*                                                          */
*        /*  CALL THE READER/INTERPRETER                             */
*        /*                                                          */
*        /************************************************************/
*                                                                  0231
*        CALL ICKSACL0(GDTTBL,RINAME,FTABPTR,AREANAME,MAXCOND,LASTCOND,
*SRNAME);                                                          0231
*                                                                  0231
@RT00228 L     @11,@PC00001                                        0231
         ST    @11,@AL00001                                        0231
         LA    @15,@CC00487                                        0231
         ST    @15,@AL00001+4                                      0231
         LA    @15,FTABPTR                                         0231
         ST    @15,@AL00001+8                                      0231
         LA    @15,@CC00479                                        0231
         ST    @15,@AL00001+12                                     0231
         LA    @15,MAXCOND                                         0231
         ST    @15,@AL00001+16                                     0231
         LA    @15,LASTCOND                                        0231
         ST    @15,@AL00001+20                                     0231
         LA    @15,FSRNAME                                         0231
         ST    @15,@AL00001+24                                     0231
         MVI   @AL00001+24,X'80'                                   0231
         L     @15,GDTCAL(,@11)                                    0231
         LA    @01,@AL00001                                        0231
         BALR  @14,@15                                             0231
*        /************************************************************/
*        /*                                                          */
*        /*  OBTAIN THE READER/INTERPRETER (END-OF-FILE) RETURN CODE */
*        /*                                                          */
*        /************************************************************/
*                                                                  0232
*        RESPECIFY RTNREG RSTD;                                    0232
*        TESTRC = RTNREG;                                          0233
         LR    TESTRC,RTNREG                                       0233
*        RESPECIFY RTNREG UNRSTD;                                  0234
*                                                                  0234
*        EOFCOND = TESTRC;                                         0235
*                                                                  0235
         LR    EOFCOND,TESTRC                                      0235
*        RETURN;                                                   0236
*                                                                  0236
@EL00003 DS    0H                                                  0236
@EF00003 DS    0H                                                  0236
@ER00003 LM    @14,@02,@SA00003                                    0236
         LM    @04,@12,@SA00003+20                                 0236
         BR    @14                                                 0236
*        END CALLRI;                                               0237
*/********************************************************************/
*/*                                                                  */
*/*  PROCEDURE NAME - CALLFSR                                        */
*/*                                                                  */
*/*  FUNCTION - INVOKE THE FUNCTION SUPPORT ROUTINE (FSR) TO PROCESS */
*/*             THE UTILITY FUNCTION DESIRED.  FOLLOWING FUNCTION    */
*/*             PROCESSING, FREE THE AREA HELD BY THE FUNCTION DATA  */
*/*             TABLE (UFPOOL MACRO), AND RESET THE TEXT PROCESSING  */
*/*             OPTIONS TO THEIR DEFAULT VALUES (URESET MACRO)       */
*/*                                                                  */
*/*             THE LAST FUNCTION CONDITION CODE IS SET BY THE       */
*/*             FUNCTION SUPPORT ROUTINE.  THE MAXIMUM CONDITION     */
*/*             CODE IS UPDATED IF THE LAST FUNCTION CONDITION CODE  */
*/*             IS GREATER THAN THE MAXIMUM CONDITION CODE           */
*/*                                                                  */
*/*  INPUT - FSR NAME (RETURNED FROM READER/INTERPRETER)             */
*/*          FUNCTION DATA TABLE (FDT) (BUILT BY READER/INTERPRETER) */
*/*                                                                  */
*/*  OUTPUT - FUNCTION SUPPORT ROUTINE CONDITION CODE                */
*/*           UPDATED MAXIMUM SYSTEM CONDITION CODE                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0238
*CALLFSR: PROCEDURE;                                               0238
*                                                                  0238
CALLFSR  STM   @14,@12,@SA00004                                    0238
*        OLDERID2 = NEWERID2;                                      0239
         L     @11,@PC00001                                        0239
         L     @15,GDTTR2(,@11)                                    0239
         MVC   @TS00001(95),NEWERID2(@15)                          0239
         MVC   OLDERID2(95,@15),@TS00001                           0239
*        NEWID2 = 'EX1F';                                          0240
*                                                                  0240
         MVC   NEWID2(4,@15),@CC00510                              0240
*        IF GDTDBG = NULLPTR                                       0241
*          THEN;                                                   0241
         L     @11,GDTDBG(,@11)                                    0241
         LTR   @11,@11                                             0241
         BZ    @RT00241                                            0241
*          ELSE                                                    0243
*            CALL ICKDB010(GDTTBL,'EXFS');                         0243
*                                                                  0243
         L     @11,@PC00001                                        0243
         ST    @11,@AL00001                                        0243
         LA    @15,@CC00511                                        0243
         ST    @15,@AL00001+4                                      0243
         MVI   @AL00001+4,X'80'                                    0243
         L     @15,GDTDBG(,@11)                                    0243
         LA    @01,@AL00001                                        0243
         BALR  @14,@15                                             0243
*        /************************************************************/
*        /*                                                          */
*        /*  RESET THE LAST FUNCTION CONDITION CODE FOR CURRENT FSR  */
*        /*                                                          */
*        /************************************************************/
*                                                                  0244
*        LASTCOND = CONDNONE;                                      0244
*                                                                  0244
@RT00241 SLR   @11,@11                                             0244
         STH   @11,LASTCOND                                        0244
*        /************************************************************/
*        /*                                                          */
*        /*  CALL THE FUNCTION SUPPORT ROUTINE (FSR)                 */
*        /*                                                          */
*        /************************************************************/
*                                                                  0245
*        CALL ICKSACL0(GDTTBL,FSRNAME,FTABLE,LASTCOND);            0245
*                                                                  0245
         L     @11,@PC00001                                        0245
         ST    @11,@AL00001                                        0245
         LA    @15,FSRNAME                                         0245
         ST    @15,@AL00001+4                                      0245
         L     @01,FTABPTR                                         0245
         ST    @01,@AL00001+8                                      0245
         LA    @15,LASTCOND                                        0245
         ST    @15,@AL00001+12                                     0245
         MVI   @AL00001+12,X'80'                                   0245
         L     @15,GDTCAL(,@11)                                    0245
         LA    @01,@AL00001                                        0245
         BALR  @14,@15                                             0245
*        /************************************************************/
*        /*                                                          */
*        /*  UPDATE THE MAXIMUM SYSTEM CONDITION CODE                */
*        /*                                                          */
*        /************************************************************/
*                                                                  0246
*        MAXCOND = MAX(MAXCOND,LASTCOND);                          0246
*                                                                  0246
         LH    @11,MAXCOND                                         0246
         LH    @15,LASTCOND                                        0246
         CR    @11,@15                                             0246
         BNL   *+6
         LR    @11,@15                                             0246
         STH   @11,MAXCOND                                         0246
*        /************************************************************/
*        /*                                                          */
*        /*  FREE THE POOL AREA CONTAINING THE FUNCTION DATA TABLE   */
*        /*  BUILT FOR THE JUST-COMPLETED FUNCTION SUPPORT ROUTINE   */
*        /*                                                          */
*        /************************************************************/
*                                                                  0247
*        CALL ICKSAFP0(GDTTBL,AREANAME,ALL);                       0247
*                                                                  0247
         MVC   @AL00001+4(8),@AL00247                              0247
         L     @11,@PC00001                                        0247
         ST    @11,@AL00001                                        0247
         L     @15,GDTFPL(,@11)                                    0247
         LA    @01,@AL00001                                        0247
         BALR  @14,@15                                             0247
*        /************************************************************/
*        /*                                                          */
*        /*  RESET THE TEXT PROCESSING AREA BACK TO INITIAL DEFAULTS */
*        /*  (DEFAULT PARAMETERS MAY HAVE BEEN OVERRIDDEN BY THE FSR)*/
*        /*                                                          */
*        /************************************************************/
*                                                                  0248
*        CALL ICKTPRE0(GDTTBL,PRTFILE);                            0248
*                                                                  0248
*                                                                  0248
         L     @11,@PC00001                                        0248
         ST    @11,@AL00001                                        0248
         LA    @15,@CF00089                                        0248
         ST    @15,@AL00001+4                                      0248
         MVI   @AL00001+4,X'80'                                    0248
         L     @15,GDTRES(,@11)                                    0248
         LA    @01,@AL00001                                        0248
         BALR  @14,@15                                             0248
*        RETURN;                                                   0249
*                                                                  0249
@EL00004 DS    0H                                                  0249
@EF00004 DS    0H                                                  0249
@ER00004 LM    @14,@12,@SA00004                                    0249
         BR    @14                                                 0249
*        END CALLFSR;                                              0250
*                                                                  0250
*                                                                  0250
*      END MAIN;                                                   0251
*                                                                  0251
*                                                                  0251
*    END ICKEX01                                                   0252
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.      *
*/*%INCLUDE SYSLIB  (DMSDF01 )                                        *
*/*%INCLUDE SYSLIB  (DMSDF00 )                                        *
