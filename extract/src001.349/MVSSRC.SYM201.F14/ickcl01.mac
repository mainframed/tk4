*    VTOCREC = 1;                                                  0189
*                                                                  0189
         MVI   VTOCREC(@15),X'01'                                  0189
*/*  IF USER VOLUME LABELS REQUESTED                                 */
*    IF LABELS > 0                                                 0190
*                                                                  0190
*/*    THEN SET ACCESSIBILITY BYTE                                   */
*      THEN VOL1ACC = 'F0'X × LABELS;                              0190
*                                                                  0190
         L     @14,@PC00001+20                                     0190
         L     @14,LABELS(,@14)                                    0190
         CR    @14,@10                                             0190
         BNH   @RF00190                                            0190
         O     @14,@CF00513                                        0191
         STC   @14,VOL1ACC(,@15)                                   0191
*/*  WRITE_VOLUME_LABEL                                              */
*    CALL ICKWL01(GDTTBL                                           0192
*                ,VOLIB                                            0192
*                ,VOL1                                             0192
*                ,LABELS                                           0192
*                ,RETCODE);                                        0192
*                                                                  0192
