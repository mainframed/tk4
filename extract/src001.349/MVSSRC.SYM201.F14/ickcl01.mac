*        CALL ICKDB010(GDTTBL,'CL99');                             0199
         L     @10,@PC00001                                        0199
         ST    @10,@AL00001                                        0199
         LA    @15,@CC00515                                        0199
         ST    @15,@AL00001+4                                      0199
         MVI   @AL00001+4,X'80'                                    0199
         L     @15,GDTDBG(,@10)                                    0199
         LA    @01,@AL00001                                        0199
         BALR  @14,@15                                             0199
*    CALL ICKSAEP0(GDTTBL,MODID);                                  0200
*                                                                  0200
@RT00197 L     @10,@PC00001                                        0200
         ST    @10,@AL00001                                        0200
         LA    @15,MODID                                           0200
         ST    @15,@AL00001+4                                      0200
         MVI   @AL00001+4,X'80'                                    0200
         L     @15,GDTEPL(,@10)                                    0200
         LA    @01,@AL00001                                        0200
         BALR  @14,@15                                             0200
*    END ICKCL01                                                   0201
