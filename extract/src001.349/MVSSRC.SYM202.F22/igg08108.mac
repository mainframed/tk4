*/********************************************************************/
*/*                                                                  */
*/*  FUNCTION-THE 3886 SECTION OF SETDEV SEARCHES THE FORMAT         */
*/*           RECORD TABLE FOR A DUPLICATE ENTRY OR AN AVAILABLE     */
*/*           ENTRY. THE ID IS APPENDED TO 'FRLG' TO LOAD THE FORMAT */
*/*           RECORD. THE FORMAT RECORD IS LOADED FROM SYS1.IMAGELIB */
*/*           AND PASSED TO THE DEVICE. CONTROL IS THEN RETURNED TO  */
*/*           THE USER.                                              */
*/*                                                                  */
*/*  SEGMENTS-DEVT3886,T3886PMS,FRTABMG,LOADFRID                     */
*/*                                                                  */
*/*  INPUT-SETDEV PARAMETER LIST,POINTER TO UCB                      */
*/*                                                                  */
*/*  OUTPUT-3886 CONTROL UNIT LOADED WITH FORMAT RECORD              */
*/*         UPDATED FORMAT RECORD TABLE                              */
*/*                                                                  */
*/*  EXTERNAL REFERENCES-LOAD -INNER PROCEDURE TO THIS MODULE,LOADS  */
*/*           THE FORMAT RECORD TO MAIN STORAGE FROM SYS1.IMAGELIB.  */
*/*                      IORTN -INNER PROCEDURE TO THIS MODULE,LOADS */
*/*           THE FORMAT RECORD TO THE 3886                          */
*/*                      CLEANUP -INNER BACK TO CALLER               */
*/*                                                                  */
*/*  EXITS,NORMAL -RETURN TO CALLER WITH RETURN CODE X'00' IN REG 15 */
*/*               -RETURN TO EOF ADDRESS IN CALLERS DCB FOR UNIT EXC */
*/*                                                                  */
*/*  EXITS,ERROR  - RETURN TO CALLER WITH RETURN CODE IN REG 15      */
*/*                 X'04' MEMBER NOT FOUND IN SYS1.IMAGELIB          */
*/*                 X'08' UNABLE TO LOAD MEMBER FROM SYS1.IMAGELIB   */
*/*                 X'0C' PERMANENT I/O ERROR LOADING DEVICE         */
*/*                 X'10' INVALID PARAMETER LIST                     */
*/*                                                                  */
*/*  TABLES/WORKAREAS-SETDEV PARAMETER LIST                          */
*/*                   FORMAT RECORD TABLE                            */
*/*                   WORK AREA FOR THIS MODULE                      */
*/*                                                                  */
*/*  CONTROL BLOCKS-DCB,IOB,ECB,TIOT,DEB,TCB,RB,UCB                  */
*/*                                                                  */
*/*  NOTES-FOR FURTHER INFORMATION SEE PLM SY21-0028                 */
*/*  NOTES- FOR FURTHER INFORMATION SEE PLM SY21-5162                */
*/*  NOTES- FOR FURTHER INFORMATION SEE PLM SY21-5162                */
*/*                                                                  */
*/********************************************************************/
*                                                                  0164
*           /*********************************************************/
*           /*                                                       */
*           /* ******** TEST FOR VALID 3886 PARAMETER LIST AND DCB   */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0164
*           RFRT=DCBFRTBA;          /* INIT PTR TO FR TAB    @YM30699*/
         L     RFRT,DCBFRTBA+4(,DCBPTR)                            0164
*           IF FRID=NOTSPCFD THEN   /* TEST IF FRID KEEP IT          */
         CLC   FRID(4,RPAR),@CB00189                               0165
         BNE   @RF00165                                            0165
*             DO;                                                  0166
*               RTNCODE=RCIPLT;     /* SET RETURN CODE               */
         LA    RTNCODE,16                                          0167
*               NOMSGFLG=ON;        /* NOT SPECIFIED FOR OPEN        */
         MVI   NOMSGFLG(RCORE),X'01'                               0168
*             END;                                                 0169
*           ELSE                    /* ELSE STATEMENT                */
*             IF FRID=BLANKS THEN   /* FRID NOT ON L OR E FORM       */
         B     @RC00165                                            0170
@RF00165 CLC   FRID(4,RPAR),@CC00195                               0170
         BNE   @RF00170                                            0170
*               RTNCODE=RCIPLT;     /* FRID NOT SPECIFIED            */
         LA    RTNCODE,16                                          0171
*             ELSE                  /* FRID IS SPECIFIED             */
*               IF FRIDTAB^=CONST000 THEN/* TEST IF OPEN IS NOT    0172
*                                      CALLER                        */
         B     @RC00170                                            0172
@RF00170 L     @14,FRIDTAB(,RFRT)                                  0172
         LTR   @14,@14                                             0172
         BZ    @RF00172                                            0172
*                 IF DCBOFOPN^='1'B×/* TEST IF DCB OPENED            */
*                     DCBIFLGS=PERMERR THEN/* OR DCB HAS PERM ERR IND*/
         TM    DCBOFOPN+40(DCBPTR),B'00010000'                     0173
         BNO   @RT00173                                            0173
         CLI   DCBIFLGS+40(DCBPTR),B'11000000'                     0173
         BNE   @RF00173                                            0173
@RT00173 DS    0H                                                  0174
*                   RTNCODE=RCIPLT; /* INVALID RC SET                */
         LA    RTNCODE,16                                          0174
*                 ELSE                                             0175
*                   ;                                              0175
@RF00173 DS    0H                                                  0176
*               ELSE                /* OPEN IS THE CALLER            */
*                 NOMSGFLG=ON;      /* IF OPEN CALLER SET NO MSG F   */
         B     @RC00172                                            0176
@RF00172 MVI   NOMSGFLG(RCORE),X'01'                               0176
*           IF RTNCODE=NOERROR THEN /* TEST IF PARM LIST WAS VALID   */
@RC00172 DS    0H                                                  0177
@RC00170 DS    0H                                                  0177
@RC00165 LTR   RTNCODE,RTNCODE                                     0177
         BNZ   @RF00177                                            0177
*             DO;                                                  0178
*                                                                  0178
*               /*****************************************************/
*               /*                                                   */
*               /* ******** FORMAT RECORD TABLE MANAGEMENT ROUTINE   */
*               /* (1) ADDRESS'S ARE FILLED IN THE TABLE BY OPEN. (2)*/
*               /* FRID ARE FILLED IN BY THIS ROUTINE (3) THERE CAN  */
*               /* BE A MAXIMUM OF 8 ENTRIES IN THE TABLE (4) IF THE */
*               /* FRID SPECIFIED IN THE SETDEV IS ALREADY IN THE    */
*               /* TABLE THE FR IS NOT LOAD FROM SYS1.IMAGELIB. (5)  */
*               /* IF THE FRID IS NOT IN THE TABLE THEN THE FR IS    */
*               /* LOADED FROM SYS1.IMAGELIB. (6) IF THE FR TABLE IS */
*               /* FULL THE TOP FR ENTRY WILL BE REPLACED            */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0179
*FRTABMG:                           /* LABEL TO IDENTIFY TAB MG      */
*               RFRL=CONST000;      /* INIT FMT RCD PTR TO ZERO      */
FRTABMG  SLR   RFRL,RFRL                                           0179
*               DO X=ONE TO EIGHT WHILE RFRL=CONST000;/* LOOP THRU 0180
*                                      FMT RCD TABLE                 */
*                                                                  0180
         LA    @14,1                                               0180
         ST    @14,X(,RCORE)                                       0180
@DL00180 LTR   RFRL,RFRL                                           0180
         BNZ   @DC00180                                            0180
*                 /***************************************************/
*                 /*                                                 */
*                 /* FIND THE NXT AVAILABLE ENTRY OR ENTRY TO BE USED*/
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0181
*                 IF FRIDTAB=CONST000 THEN/* TST FOR NXT AVAILABLE 0181
*                                      ENTRY                         */
         L     @14,FRIDTAB(,RFRT)                                  0181
         LTR   @14,@14                                             0181
         BNZ   @RF00181                                            0181
*                   RFRL=FRADDR;    /* YES SET PTR TO USE THIS ENTRY */
         L     RFRL,FRADDR(,RFRT)                                  0182
*                 ELSE                                             0183
*                   IF FRID=FRIDTAB THEN/* TST IF THIS ENTRY TO BE 0183
*                                      USED                          */
         B     @RC00181                                            0183
@RF00181 CLC   FRID(4,RPAR),FRIDTAB(RFRT)                          0183
         BNE   @RF00183                                            0183
*                     RFRL=FRADDR;  /* SET PTR TO ENTRY TO BE USED   */
         L     RFRL,FRADDR(,RFRT)                                  0184
*                   ELSE            /* ENTRY IN TABLE ALREADY TAKEN  */
*                     IF X^=EIGHT THEN/* TST IF END OF TABLE REACHED */
         B     @RC00183                                            0185
@RF00183 CLC   X(4,RCORE),@CF00042                                 0185
         BE    @RF00185                                            0185
*                       RFRT=RFRT+EIGHT;/* NO,INCR PTR TO NEXT TABLE
*                                      ENTRY                         */
         LA    RFRT,8(,RFRT)                                       0186
*                     ELSE          /* YES,END OF TABLE REACHED      */
*                       DO;                                        0187
         B     @RC00185                                            0187
@RF00185 DS    0H                                                  0188
*                         RWK2=DCBFRTBA->FRADDR;/* SAVE ADDR FOR NXT
*                                      FORMAT RCD LOADED@YM30699     */
         L     @14,DCBFRTBA+4(,DCBPTR)                             0188
         L     RWK2,FRADDR(,@14)                                   0188
*                         FRTABLE(1:56)=FRTABSB;/* MOVE LAST 7     0189
*                                                            @ZA04490*/
         L     @14,DCBFRTBA+4(,DCBPTR)                             0189
         L     @12,DCBFRTBA+4(,DCBPTR)                             0189
         MVC   FRTABLE(56,@14),FRTABSB+8(@12)                      0189
*                         FRTABLE(57:64)='';/* ENTRIES IN TBLE     0190
*                                                            @ZA04490
*                                      UP TO 1ST 7 POSITIONS @ZA04490*/
         L     @14,DCBFRTBA+4(,DCBPTR)                             0190
         MVI   FRTABLE+56(@14),C' '                                0190
         MVC   FRTABLE+57(7,@14),FRTABLE+56(@14)                   0190
*                         FRADDR=RWK2;/* RESTORE ADDR FOR FMT RCD    */
         ST    RWK2,FRADDR(,RFRT)                                  0191
*                         FRIDTAB=CONST000;/* ZERO OUT FRID FIELD    */
         SLR   @14,@14                                             0192
         ST    @14,FRIDTAB(,RFRT)                                  0192
*                         RFRL=FRADDR;/* SET PTR TO USE THIS ENTRY   */
         L     RFRL,FRADDR(,RFRT)                                  0193
*                       END;                                       0194
*               END;                                               0195
@RC00185 DS    0H                                                  0195
@RC00183 DS    0H                                                  0195
@RC00181 LA    @14,1                                               0195
         AL    @14,X(,RCORE)                                       0195
         ST    @14,X(,RCORE)                                       0195
         C     @14,@CF00042                                        0195
         BNH   @DL00180                                            0195
@DC00180 DS    0H                                                  0196
*               IF FRIDTAB=CONST000 THEN/* TST IF NEED TO LOAD FR    */
         L     @14,FRIDTAB(,RFRT)                                  0196
         LTR   @14,@14                                             0196
         BNZ   @RF00196                                            0196
*                 DO;                                              0197
*                   PREFIX=FRLG;    /* SET PREFIX FOR FR NAME        */
         MVC   PREFIX(4,RCORE),@CC00197                            0198
*                   IDFR=FRID;      /* COMPLETE FR NAME              */
         MVC   IDFR(4,RCORE),FRID(RPAR)                            0199
*                   NAMEPTR=ADDR(MODNAME);/* SET PTR TO MEMBER TO LD */
         LA    NAMEPTR,MODNAME(,RCORE)                             0200
*                   CALL LOAD;      /* LOAD FR INTO CORE             */
         BAL   @14,LOAD                                            0201
*                   IF RTNCODE=RCSUCC THEN/* IF NO ERRORS CONTINUE   */
         LTR   RTNCODE,RTNCODE                                     0202
         BNZ   @RF00202                                            0202
*                     DO;                                          0203
*                       FRIDTAB=FRID;/* MOVE FRID INTO TABLE         */
         MVC   FRIDTAB(4,RFRT),FRID(RPAR)                          0204
*                       RFY                                        0205
*                         UCBPTR RSTD;/*                     @ZA24702*/
*                       UCBPTR=DEBSUCBA;/* GET UCB PTR       @ZA24702*/
         L     @14,DEBPTR(,RCORE)                                  0206
         L     UCBPTR,DEBSUCBA(,@14)                               0206
*                       DO;         /* MODESET EXTKEY(SUPR)          */
*                         SPKA(0);  /* SET PSW KEY                   */
         SLR   @14,@14                                             0208
         SPKA  0(@14)                                              0208
*                       END;        /* MODESET EXTKEY(SUPR) GET    0209
*                                      SUPERVISOR PROTECT KEY      0209
*                                                            @YM02506*/
*                       UCBXTADR=FRID;/* PUT FRID IN UCB FOR Y02870  */
         L     @14,FRID(,RPAR)                                     0210
         ST    @14,UCBXTADR(,UCBPTR)                               0210
*                       DO;         /* MODESET KEYADDR(CALLKEY)    0211
*                                      WORKREG(REG1)                 */
*                         REG1=CALLKEY;/* GET KEY FORM SAVE LOCATION */
         SLR   REG1,REG1                                           0212
         IC    REG1,CALLKEY(,RWTG)                                 0212
*                         SPKA(REG1);/* SET PSW KEY                  */
         SPKA  0(REG1)                                             0213
*                       END;        /* MODESET KEYADDR(CALLKEY)    0214
*                                      WORKREG(REG1)         @ZA24698*/
*                       RFY                                        0215
*                         UCBPTR UNRSTD;/*                   @ZA24702*/
*                       BYTECNT=MAXRFLTH;/* MAX NUMB BYTES TO MV IN  */
         LA    BYTECNT,224                                         0216
*                       RESPECIFY                                  0217
*                         NEWFRLTH RSTD;                           0217
*                       NEWFRLTH=LNTHFR+FRPRLNTH;/* SET LENGTH OF FR
*                                      LOADED                        */
         LA    NEWFRLTH,2                                          0218
         AH    NEWFRLTH,LNTHFR(,LDMODPTR)                          0218
*                       GEN(MVCL   8,10      MOVE FR LOADED INTO TABLE)
*                           ;                                      0219
*                                                                  0219
         MVCL   8,10      MOVE FR LOADED INTO TABLE
*                       /*********************************************/
*                       /*                                           */
*                       /* DELETE FORMAT RECORD JUST LOADED          */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0220
*                       REG1=ADDR(BLDNAME);/* GET ADDR OF MOD NAME   */
         LA    REG1,BLDNAME(,RCORE)                                0220
*                       GEN(DELETE   EPLOC=(1)    DELETE FR LOADED);
         DELETE   EPLOC=(1)    DELETE FR LOADED
*                       RESPECIFY                                  0222
*                         NEWFRLTH UNRSTD;                         0222
*                       RFRL=FRADDR;/* REINIT PTR TO FR AFTER MOVE   */
         L     RFRL,FRADDR(,RFRT)                                  0223
*                     END;                                         0224
*                 END;                                             0225
@RF00202 DS    0H                                                  0226
*                                                                  0226
*               /*****************************************************/
*               /*                                                   */
*               /* ******** SEQUENCE TO LOAD THE FORMAT RECORD INTO  */
*               /* THE 3886 (1) BUILD IOB IN WORKAREA (2) BUILD LOAD */
*               /* FORMAT CCW IN WORKAREA (3) ISSUE COMMAND          */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0226
*LOADFRID:                          /* LABEL FOR LOADFRID     SEGMENT*/
*               IF RTNCODE=RCSUCC THEN/* TEST IF OK TO CCNTINUE      */
@RF00196 DS    0H                                                  0226
LOADFRID LTR   RTNCODE,RTNCODE                                     0226
         BNZ   @RF00226                                            0226
*                 DO;                                              0227
*                   IOBSTPTR=ADDR(IOBBODY);                        0228
         LA    IOBSTPTR,IOBBODY(,RCORE)                            0228
*                   IOBBODY=IOBBODY&&IOBBODY;/* ZERO OUT IOB BODY    */
         XC    IOBBODY(32,RCORE),IOBBODY(RCORE)                    0229
*                   IOBFLAG1=UNRELTD;/* SET IOB UNRELATED FLAG       */
         MVI   IOBFLAG1(IOBSTPTR),X'02'                            0230
*                   IOBECBPT=IOBSTPTR-ECBLNGTH;/* SET PTR TO ECB     */
         LR    @14,IOBSTPTR                                        0231
         SL    @14,@CF00038                                        0231
         ST    @14,IOBECBPT(,IOBSTPTR)                             0231
*                   IOBSTART=IOBSTPTR+IOBLENGT;/* SET PTR TO LOAD CCW*/
         LA    @14,32                                              0232
         ALR   @14,IOBSTPTR                                        0232
         ST    @14,IOBSTART(,IOBSTPTR)                             0232
*                   IOBDCBPT=DEBDCBB;/* SET PTR TO USER'S DCB      0233
*                                                            @ZA04496*/
         L     @14,DEBPTR(,RCORE)                                  0233
         L     @14,DEBDCBB-1(,@14)                                 0233
         LA    @14,0(,@14)                                         0233
         ST    @14,IOBDCBPT(,IOBSTPTR)                             0233
*                   CCWDADR=ADDR(FRIMAG);/* SET DATA ADDR IN CCW     */
         LA    @14,FRIMAG(,RFRL)                                   0234
         STCM  @14,7,CCWDADR(RCORE)                                0234
*                   CCWOPCD=LDFMATR;/* SET CCW OP CODE TO LOAD FR    */
         MVI   CCWOPCD(RCORE),X'53'                                0235
*                   CCWFLGS=SILICCON;/* SET SILI AND CC ON           */
         MVI   CCWFLGS(RCORE),B'01100000'                          0236
*                   CCWBCNT=FRLNTH; /* SET LENTH OF FR IN CCW        */
         LH    @14,FRLNTH(,RFRL)                                   0237
         STH   @14,CCWBCNT(,RCORE)                                 0237
*                   CCW2=NOOPCCW;   /* MOVE NOOP CCW INTO CHAIN      */
         MVC   CCW2(8,RCORE),NOOPCCW                               0238
*                   CALL IORTN;     /* CALL I/O RTN TO LOAD FR       */
         BAL   @14,IORTN                                           0239
*                   IF RTNCODE=RCPIOE THEN/* TEST FOR I/O ERROR      */
         C     RTNCODE,@CF00158                                    0240
         BNE   @RF00240                                            0240
*                     DO;                                          0241
*                       WRKPTR=ADDR(IOBCSW);/* INIT PTR TO CSW IN IOB*/
         LA    @14,IOBCSW(,IOBSTPTR)                               0242
         ST    @14,WRKPTR(,RCORE)                                  0242
*                       IF STBYT1B7=ONBIT THEN/* UNIT EXCEPTION      */
         L     @14,WRKPTR(,RCORE)                                  0243
         TM    STBYT1B7(@14),B'00000001'                           0243
         BNO   @RF00243                                            0243
*                         IF DCBEODAD^=NOEODAD&/* DCB EODAD SPEC     */
*                             NOMSGFLG^=ON THEN/* RTN AND OPEN NOT 0244
*                                      CALLER                        */
         CLC   DCBEODAD+28(4,DCBPTR),@CF00040                      0244
         BE    @RF00244                                            0244
         CLI   NOMSGFLG(RCORE),X'01'                               0244
         BE    @RF00244                                            0244
*                           DO;                                    0245
*                             RFY                                  0246
*                               RBSECPTR RSTD;/*             @ZA24702*/
*                             DEBPTR=DCBDEBA;/* INIT PTR TO DEB FROM
*                                      DCB                           */
         L     @14,DCBDEBA+39(,DCBPTR)                             0247
         LA    @14,0(,@14)                                         0247
         ST    @14,DEBPTR(,RCORE)                                  0247
*                             TCBPTR=DEBTCBAD;/* INIT PTR TO TCB FROM
*                                      DEB                           */
         L     @14,DEBPTR(,RCORE)                                  0248
         L     TCBPTR,DEBTCBAD(,@14)                               0248
