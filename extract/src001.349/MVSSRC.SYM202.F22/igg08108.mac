*                                                                  0127
*   DO;                             /* MODESET KEYADDR(CALLKEY)    0127
*                                      WORKREG(RWK2)                 */
*     RWK2=CALLKEY;                 /* GET KEY FORM SAVE LOCATION    */
         SLR   RWK2,RWK2                                           0128
         IC    RWK2,CALLKEY(,RWTG)                                 0128
*     SPKA(RWK2);                   /* SET PSW KEY                   */
         SPKA  0(RWK2)                                             0129
*   END;                            /* MODESET KEYADDR(CALLKEY)    0130
*                                      WORKREG(RWK2) GET CALLER KEY
*                                                            @ZA24698*/
*   REG0=WKARFACT*WKARCNST;         /* COMPUTE SIZE OF WORK AREA     */
         SLR   REG0,REG0                                           0131
         IC    REG0,WKARFACT(,RPAR)                                0131
         SLA   REG0,7                                              0131
*   REG1=REG0;                      /* GET LENGTH OF CORE NEEDED   0132
*                                                            @YM02505*/
         LR    REG1,REG0                                           0132
*   DCBPTR=DCBADDR;                 /* POINT TO DCB Y02870           */
         L     DCBPTR,DCBADDR(,RPAR)                               0133
