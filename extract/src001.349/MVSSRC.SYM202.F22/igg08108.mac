         LR    NAMEPTR,REG0                                        0134
*/*THIS ROUTINE WILL GET CORE ACCORDINGLY                    @YM02505*/
*/*      SUBPOOL 250 IF SETDEV WAS INVOKED BY THE USER       @YM02505*/
*/*      SUBPOOL 230 IF SETDEV WAS INVOKED BY THE OPEN       @YM02505*/
*                                                                  0135
*   IF DCBDEVT=D3886 THEN                                          0135
         CLI   DCBDEVT+16(DCBPTR),X'57'                            0135
         BNE   @RF00135                                            0135
*     RFRT=DCBFRTBA;                /* 3886 DEVICE           @YM02505
*                                                            @YM30699*/
         L     RFRT,DCBFRTBA+4(,DCBPTR)                            0136
*   IF DCBDEVT=D3886&FRIDTAB=CONST000 THEN                         0137
@RF00135 CLI   DCBDEVT+16(DCBPTR),X'57'                            0137
         BNE   @RF00137                                            0137
         L     @14,FRIDTAB(,RFRT)                                  0137
         LTR   @14,@14                                             0137
         BNZ   @RF00137                                            0137
*     DO;                           /*                       @YM02505*/
*       REG0=REG1+SUBPL230;         /* SET UP GETMAIN CONSTANT     0139
*                                                            @YM02505*/
         LR    REG0,REG1                                           0139
         AL    REG0,@CF00191                                       0139
*       GEN(GETMAIN R,LV=(0));      /* GET CORE FROM SUBPOOL 230   0140
*                                                            @YM02505*/
         GETMAIN R,LV=(0)
*       RCORE=REG1;                 /* PUT GETMAIN ADDR IN WORKAREA
*                                      REGISTER              @YM02505*/
         LR    RCORE,REG1                                          0141
*       SUBPOLL=SUBPOLE6;           /* SET 230 FREEMAIN FLAG @YM02505*/
         MVI   SUBPOLL(RCORE),X'E6'                                0142
*     END;                          /*                       @YM02505*/
*   ELSE                                                           0144
*     DO;                           /* IF 3886 NOT OPEN GET CORE FROM
*                                      SUBPOOL 250           @YM02505*/
         B     @RC00137                                            0144
@RF00137 DS    0H                                                  0145
*       REG0=REG1+SUBPL250;         /* SET UP GETMAIN CONSTANT     0145
*                                                            @YM02505*/
         LR    REG0,REG1                                           0145
         AL    REG0,@CF00193                                       0145
*       GEN(GETMAIN R,LV=(0));      /* GET CORE FROM SUBPOOL 230   0146
*                                                            @YM02505*/
         GETMAIN R,LV=(0)
*       RCORE=REG1;                 /* PUT GETMAIN ADDR IN WORKAREA
*                                      REGISTER              @YM02505*/
         LR    RCORE,REG1                                          0147
*       SUBPOLL=SUBPOLFA;           /* SET 250 FREEMAIN FLAG @YM02505*/
         MVI   SUBPOLL(RCORE),X'FA'                                0148
*     END;                          /*                       @YM02505*/
*   CORESIZE=NAMEPTR;               /* PUT SIZE IN FREEMAIN CONSTANT
*                                                            @YM02505*/
@RC00137 STCM  NAMEPTR,7,CORESIZE(RCORE)                           0150
*   RESPECIFY                                                      0151
*    (RTNPTR) RSTD;                 /*                       @YM02505*/
*   RTNPTR=CVTTCBP;                 /* GET TCB POINTER       @YM02505*/
         L     @12,CVTPTR                                          0152
         L     RTNPTR,CVTTCBP(,@12)                                0152
*   TCBPTR=CURTCB;                  /* GET CURRENT TCB ADDRESS     0153
*                                                            @YM02505*/
         L     TCBPTR,CURTCB+4(,RTNPTR)                            0153
*   RESPECIFY                                                      0154
*    (RTNPTR) UNRSTD;               /*                       @YM02505*/
*   DEBPTR=DCBDEBA;                 /* POINT TO DEB Y02870           */
         L     @14,DCBDEBA+39(,DCBPTR)                             0155
         LA    @14,0(,@14)                                         0155
         ST    @14,DEBPTR(,RCORE)                                  0155
*   TCBPTR=DEBTCBAD;                /* GET USER TCB ADDRESS Y02870   */
         L     @14,DEBPTR(,RCORE)                                  0156
         L     TCBPTR,DEBTCBAD(,@14)                               0156
*   RTNCODE=NOERROR;                /* INIT RETURN CODE TO 0         */
         SLR   RTNCODE,RTNCODE                                     0157
*   NOMSGFLG=BOF;                   /* INIT FLAG TO OFF              */
         MVI   NOMSGFLG(RCORE),X'00'                               0158
*   SAVERC=CONST000;                /* INIT RETURN CODE SAVEAREA     */
         SLR   @14,@14                                             0159
         ST    @14,SAVERC(,RCORE)                                  0159
*   IF DCBPTR^=CONST000 THEN        /* TEST FOR VALID DCB            */
         LTR   DCBPTR,DCBPTR                                       0160
         BZ    @RF00160                                            0160
*     DO;                                                          0161
*       IF DCBDEVT=D3886 THEN       /* TEST FOR 3886 - LOAD FMT RD   */
         CLI   DCBDEVT+16(DCBPTR),X'57'                            0162
         BNE   @RF00162                                            0162
