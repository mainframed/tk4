SVTM     TITLE ' COMMAND FAC-- ISTCFCPC -- POI CLOSE PROCESSOR         *
                        '
ISTCFCPC CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'ISTCFCPC  75.325'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         EJECT
*                                                               01S0119
*   /*****************************************************************/
*   /*                                                               */
*   /* MAINLINE CODE OF ISTCFCPC -- POI CLOSE PROCESS                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0119
*   DO;                                                         01S0119
*                                                               01S0119
*/***  CRRMAINT  PROCNM(ISTOCPOI)COMP(OC)FUNC(SUB)RPHADD((CPCRPHPT))  *
*                                                                    */
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)                        */
*/* END OF ATBUILD                                                   */
*/* ** SET ENTERED BIT FOR THIS PROCEDURE **                         */
*                                                               01S0120
*     CPCRPHPT->RPHCRR->CRACRR->NOCPOI='1'B;                    01S0120
*                                                               01S0120
         L     @15,RPHCRR(,CPCRPHPT)                            01S0120
         L     @15,CRACRR(,@15)                                 01S0120
         OI    NOCPOI(@15),B'00000001'                          01S0120
*/* ** CLEAR AND SET CURRENT PROCEDURE BIT **                        */
*                                                               01S0121
*     CPCRPHPT->RPHCRR->CRACRR->OCCCRT='0'B;                    01S0121
         XC    OCCCRT+1(2,@15),OCCCRT+1(@15)                    01S0121
         MVI   OCCCRT(@15),B'00000000'                          01S0121
*     CPCRPHPT->RPHCRR->CRACRR->COCPOI='1'B;                    01S0122
         OI    COCPOI(@15),B'00000001'                          01S0122
*   END;                                                        01S0123
*   IF ATCPOIA^=ZERO                /* POIA AND POCB EXIST           */
*     THEN                                                      01S0124
         L     @15,RPHWEA(,CPCRPHPT)                            01S0124
         L     @15,OCWACVT(,@15)                                01S0124
         L     @00,ATCPOIA-1(,@15)                              01S0124
         ICM   @00,8,@CB01117                                   01S0124
         LTR   @00,@00                                          01S0124
         BZ    @RF00124                                         01S0124
*     DO;                                                       01S0125
*       CALL CPCBFIND;              /* FIND CORRECT POCB             */
         BAL   @14,CPCBFIND                                     01S0126
*       RFY                                                     01S0127
*         ISTPOCB BASED(CPCBASE);                               01S0127
*       IF CPCFND=ON                /* POCB FOUND                    */
*         THEN                                                  01S0128
         LA    @15,176                                          01S0128
         AL    @15,RPHWEA(,CPCRPHPT)                            01S0128
         TM    CPCFND(@15),B'10000000'                          01S0128
         BNO   @RF00128                                         01S0128
*         DO;                                                   01S0129
*           CPCFND=OFF;             /* CLEAR BYTE FOR NEXT USE       */
         NI    CPCFND(@15),B'01111111'                          01S0130
*           IF POCINACT=ON          /* IF THE POCB FOUND IS INACTIVE */
*             THEN                                              01S0131
         TM    POCINACT(CPCBASE),B'01000000'                    01S0131
         BNO   @RF00131                                         01S0131
*             DO;                                               01S0132
*                                                               01S0132
*               /*****************************************************/
*               /*                                                   */
*               /* FOLLOWING DO GROUP WILL DETERMINE IF THE POCB HAS */
*               /* ALREADY BEEN TPQUED TO CLOSE THE POI.THE CHECK IS */
*               /* NECESSARY TO PREVENT RECURSIVE ABEND FROM CAUSING */
*               /* POCB TO BE QUEUED MORE THAN ONCE.                 */
*               /*                                                   */
*               /*****************************************************/
*                                                               01S0133
*               CALL CPCCHK;        /* CHECKS DYPAB QUEUE FOR POCB   */
         BAL   @14,CPCCHK                                       01S0133
*               IF CPCFND=ON        /* IF POCB WAS FOUND ON DYPAB
*                                      QUEUE                         */
*                 THEN                                          01S0134
         LA    @15,176                                          01S0134
         AL    @15,RPHWEA(,CPCRPHPT)                            01S0134
         TM    CPCFND(@15),B'10000000'                          01S0134
         BNO   @RF00134                                         01S0134
*                 DO;                                           01S0135
*                   CPCPTH=CPCRPL;  /* CHECK FOR WAITING RPL         */
         NI    CPCPTH(@15),B'10111111'                          01S0136
         OI    CPCPTH(@15),B'10000000'                          01S0136
*                   CPCFND=OFF;     /* RESET SWITCH                  */
         NI    CPCFND(@15),B'01111111'                          01S0137
*                 END;                                          01S0138
*               ELSE                                            01S0139
*                 CPCPTH=CPCLORPL;  /* POCB NOT FOUND,WE WILL   01S0139
*                                      THEREFORE ISSUE A FULL CLOSE  */
         B     @RC00134                                         01S0139
@RF00134 L     @15,RPHWEA(,CPCRPHPT)                            01S0139
         OI    CPCPTH+176(@15),B'11000000'                      01S0139
*             END;                                              01S0140
*           ELSE                                                01S0141
*             DO;                                               01S0141
*                                                               01S0141
         B     @RC00131                                         01S0141
@RF00131 DS    0H                                               01S0142
*               /*****************************************************/
*               /*                                                   */
*               /* FOLLOWING DO GROUP PROCESSES ACTIVE AND QUIESCED  */
*               /* POCBS IT DETERMINES WHETHER OR NOT WE WILL CLOSE  */
*               /* SUCCESSFULLY                                      */
*               /*                                                   */
*               /*****************************************************/
*                                                               01S0142
*               IF CPCBASE=POIPRIMQ /* POCB PRIMARY?                 */
*                 THEN                                          01S0142
         L     @15,RPHWEA(,CPCRPHPT)                            01S0142
         L     @15,OCWACVT(,@15)                                01S0142
         L     @14,ATCPOIA-1(,@15)                              01S0142
         LA    @14,0(,@14)                                      01S0142
         L     @00,POIPRIMQ-1(,@14)                             01S0142
         ICM   @00,8,@CB01117                                   01S0142
         CR    CPCBASE,@00                                      01S0142
         BNE   @RF00142                                         01S0142
*                 DO;                                           01S0143
*                   ATCPOUNS=OFF;   /* UNSOLICITED MSG FLAG          */
         NI    ATCPOUNS(@15),B'01111111'                        01S0144
*                   POIPRIMQ=ZERO;  /* POI PTR TO PRIM IS ZEROED     */
         SLR   @00,@00                                          01S0145
         STCM  @00,7,POIPRIMQ(@14)                              01S0145
*                 END;                                          01S0146
*               ELSE                                            01S0147
*                 ;                                             01S0147
@RF00142 DS    0H                                               01S0148
*               IF OCACLDEB=ON      /* ARE WE PROCESSING AN ABEND    */
*                 THEN                                          01S0148
         TM    OCACLDEB(CPCOCAPT),B'01000000'                   01S0148
         BNO   @RF00148                                         01S0148
*                 CPCPTH=CPCLORPL;  /* WE WANT A FULL CLOSE          */
         L     @15,RPHWEA(,CPCRPHPT)                            01S0149
         OI    CPCPTH+176(@15),B'11000000'                      01S0149
*               ELSE                                            01S0150
*                 IF POCQUI=ON      /* POCB IS QUIESCED              */
*                   THEN                                        01S0150
         B     @RC00148                                         01S0150
@RF00148 TM    POCQUI(CPCBASE),B'00100000'                      01S0150
         BNO   @RF00150                                         01S0150
*                   CPCPTH=CPCLORPL;/* WANT A FULL CLOSE             */
         L     @15,RPHWEA(,CPCRPHPT)                            01S0151
         OI    CPCPTH+176(@15),B'11000000'                      01S0151
*                 ELSE              /* POCB IS ACTIVE                */
*                   IF POCMSGQ=ZERO /* NO OUTSTANDING MSG            */
*                     THEN                                      01S0152
         B     @RC00150                                         01S0152
@RF00150 L     @00,POCMSGQ-1(,CPCBASE)                          01S0152
         ICM   @00,8,@CB01117                                   01S0152
         LTR   @00,@00                                          01S0152
         BNZ   @RF00152                                         01S0152
*                     IF POCAWTRQ=ZERO/* NO AWAITING REPLIES         */
*                       THEN                                    01S0153
         L     @00,POCAWTRQ-1(,CPCBASE)                         01S0153
         ICM   @00,8,@CB01117                                   01S0153
         LTR   @00,@00                                          01S0153
         BNZ   @RF00153                                         01S0153
*                       CPCPTH=CPCLORPL;/* FULL CLOSE                */
         L     @15,RPHWEA(,CPCRPHPT)                            01S0154
         OI    CPCPTH+176(@15),B'11000000'                      01S0154
*                     ELSE                                      01S0155
*                       DO;                                     01S0155
*                                                               01S0155
         B     @RC00153                                         01S0155
@RF00153 DS    0H                                               01S0156
*                         /*******************************************/
*                         /*                                         */
*                         /* FOLLOWING DO LOOP SEARCHES FOR PORCBS ON*/
*                         /* DYPAB QUEUE. IF PORCB IS ON THIS QUEUE  */
*                         /* THEN IT HAS ALREADY BEEN SATISFIED AND  */
*                         /* CANNOT BE CONSIDERED AWAITING REPLY.    */
*                         /*                                         */
*                         /*******************************************/
*                                                               01S0156
*                         CPCPOCSV=CPCBASE;                     01S0156
         LA    @15,176                                          01S0156
         AL    @15,RPHWEA(,CPCRPHPT)                            01S0156
         ST    CPCBASE,CPCPOCSV(,@15)                           01S0156
*                         RFY                                   01S0157
*                           ISTPOCB BASED(CPCPOCSV);            01S0157
*                         CPCBASE=POCAWTRQ;/* POINT TO AWAIT. REP. Q */
         L     @14,RPHWEA(,CPCRPHPT)                            01S0158
         L     @14,CPCPOCSV+176(,@14)                           01S0158
         L     CPCBASE,POCAWTRQ-1(,@14)                         01S0158
         LA    CPCBASE,0(,CPCBASE)                              01S0158
*                         RFY                                   01S0159
*                           CPCPTR RSTD;                        01S0159
*                         CPCPTR=CPCBASE;                       01S0160
         LR    CPCPTR,CPCBASE                                   01S0160
*                         RFY                                   01S0161
*                           ISTPORCB BASED(CPCPTR);             01S0161
*                         CPCPTH=CPCRPL;/* SETS THE PATH TO A VALUE
*                                      OTHER THAN 0'S                */
*                                                               01S0162
         NI    CPCPTH(@15),B'10111111'                          01S0162
         OI    CPCPTH(@15),B'10000000'                          01S0162
*                         /*******************************************/
*                         /*                                         */
*                         /* WE NOW HAVE ALL THE POINTERS TO SEARCH  */
*                         /* THROUGH THE PORCB QUEUE                 */
*                         /*                                         */
*                         /*******************************************/
*                                                               01S0163
*                         DO UNTIL(PORFWD=ZERO×CPCPTH=CPCQUI);  01S0163
@DL00163 DS    0H                                               01S0164
*                           CPCPTR=CPCBASE;                     01S0164
         LR    CPCPTR,CPCBASE                                   01S0164
*                           CALL CPCCHK;/* SEARCHES THROUGH PAB QUEUE
*                                      FOR PORCB                     */
         BAL   @14,CPCCHK                                       01S0165
*                           IF CPCFND=ON/* PORCB FOUND               */
*                             THEN                              01S0166
         L     @11,RPHWEA(,CPCRPHPT)                            01S0166
         TM    CPCFND+176(@11),B'10000000'                      01S0166
         BNO   @RF00166                                         01S0166
*                             CPCBASE=PORFWD;/* UPDATE PTR           */
         L     CPCBASE,PORFWD-1(,CPCPTR)                        01S0167
         LA    CPCBASE,0(,CPCBASE)                              01S0167
*                           ELSE                                01S0168
*                             CPCPTH=CPCQUI;/* REPLY HAS NOT BEEN
*                                      PROCESSED                     */
         B     @RC00166                                         01S0168
@RF00166 L     @11,RPHWEA(,CPCRPHPT)                            01S0168
         NI    CPCPTH+176(@11),B'00111111'                      01S0168
*                         END;                                  01S0169
@RC00166 DS    0H                                               01S0169
@DE00163 L     @11,PORFWD-1(,CPCPTR)                            01S0169
         LA    @11,0(,@11)                                      01S0169
         LTR   @11,@11                                          01S0169
         BZ    @DC00163                                         01S0169
         L     @11,RPHWEA(,CPCRPHPT)                            01S0169
         TM    CPCPTH+176(@11),B'11000000'                      01S0169
         BNZ   @DL00163                                         01S0169
@DC00163 DS    0H                                               01S0170
*RESTORE:                                                       01S0170
*                         CPCBASE=CPCPOCSV;/* RESET POCB PTR         */
RESTORE  LA    @11,176                                          01S0170
         AL    @11,RPHWEA(,CPCRPHPT)                            01S0170
         L     CPCBASE,CPCPOCSV(,@11)                           01S0170
*                         RFY                                   01S0171
*                           ISTPOCB BASED(CPCBASE);             01S0171
*                         IF CPCPTH^=CPCQUI THEN                01S0172
         TM    CPCPTH(@11),B'11000000'                          01S0172
         BZ    @RF00172                                         01S0172
*                           CPCPTH=CPCLORPL;/* NO REPLIES OUT   01S0173
*                                      STANDING THEREFORE A FULL
*                                      CLOSE                         */
         OI    CPCPTH(@11),B'11000000'                          01S0173
*                         ELSE                                  01S0174
*                           ;                                   01S0174
@RF00172 DS    0H                                               01S0175
*                       END;                                    01S0175
*                   ELSE                                        01S0176
*                     CPCPTH=CPCQUI;/* WANT TO FAIL CLOSE            */
         B     @RC00152                                         01S0176
@RF00152 L     @11,RPHWEA(,CPCRPHPT)                            01S0176
         NI    CPCPTH+176(@11),B'00111111'                      01S0176
*             END;                                              01S0177
@RC00152 DS    0H                                               01S0177
@RC00150 DS    0H                                               01S0177
@RC00148 DS    0H                                               01S0178
*           IF CPCPTH2=ON THEN                                  01S0178
@RC00131 L     @11,RPHWEA(,CPCRPHPT)                            01S0178
         TM    CPCPTH2+176(@11),B'01000000'                     01S0178
         BNO   @RF00178                                         01S0178
*             DO;                                               01S0179
*                                                               01S0179
*               /*****************************************************/
*               /*                                                   */
*               /* THE DO GROUP MAKES THE POCB INACTIVE AND TPQUES IT*/
*               /* AS A WORK ELEMENT TO THE POI DYPAB TO CLEAN UP    */
*               /* QUEUES FOR A SUCCESSFULL CLOSE.                   */
*               /*                                                   */
*               /*****************************************************/
*                                                               01S0180
*               POCINACT=ON;        /* POCB INACTIVE                 */
*               POCQUI=OFF;         /* POCB NO LONGER QUIESCED       */
         OI    POCINACT(CPCBASE),B'01000000'                    01S0181
         NI    POCQUI(CPCBASE),B'11011111'                      01S0181
*/*  TPQUE (CPCBASE,DYPPAB) NORM                                     */
*                                                               01S0182
*               DO;                                             01S0182
*                 RFY                                           01S0183
*                  (GPR00P,                                     01S0183
*                   GPR01P,                                     01S0183
*                   GPR04P,                                     01S0183
*                   GPR05P,                                     01S0183
*                   GPR15P) RSTD;                               01S0183
*                 GEN REFS(GPR01P) SETS(RPHWORK)                01S0184
*                     (STM GPR02P,GPR08P,RPHWORK(GPR01P));      01S0184
         STM GPR02P,GPR08P,RPHWORK(GPR01P)
*                 GPR00P=0;                                     01S0185
         SLR   GPR00P,GPR00P                                    01S0185
*                 GPR15P=ADDR(DYPPAB);/* HOLD PAB PTR                */
         L     @11,RPHWEA(,CPCRPHPT)                            01S0186
         L     GPR15P,OCWACVT(,@11)                             01S0186
         LA    GPR15P,DYPPAB+1552(,GPR15P)                      01S0186
*                 GPR04P=CPCBASE;   /* WORK ELE PTR                  */
         LR    GPR04P,CPCBASE                                   01S0187
*                 GPR05P=GPR15P;    /* PASS PAB PTR                  */
         LR    GPR05P,GPR15P                                    01S0188
*                 CALL ATCLCPTR->ATCAP37->ISTAPC37;             01S0189
         L     @11,ATCLCPTR                                     01S0189
         L     @15,ATCAP37(,@11)                                01S0189
         BALR  @14,@15                                          01S0189
*                 GEN(NOPR  0)                                  01S0190
*                 NOSETS NOREFS;                                01S0190
         NOPR  0
*                 RFY                                           01S0191
*                  (GPR00P,                                     01S0191
*                   GPR01P,                                     01S0191
*                   GPR04P,                                     01S0191
*                   GPR05P,                                     01S0191
*                   GPR15P) UNRSTD;                             01S0191
*               END;                /* *** END OF TPQUE GENERATION   */
*               OCARC=HEXZERO;                                  01S0193
         MVI   OCARC(CPCOCAPT),X'00'                            01S0193
*             END;                                              01S0194
*           ELSE                                                01S0195
*             IF CPCPTH=CPCQUI      /* FAIL THE CLOSE                */
*               THEN                                            01S0195
         B     @RC00178                                         01S0195
@RF00178 L     @11,RPHWEA(,CPCRPHPT)                            01S0195
         TM    CPCPTH+176(@11),B'11000000'                      01S0195
         BNZ   @RF00195                                         01S0195
*               DO;                                             01S0196
*                 POCQUI=ON;        /* POCB QUIESCED                 */
*                 POCACT=OFF;       /* POCB NO LONGER ACTIVE         */
         OI    POCQUI(CPCBASE),B'00100000'                      01S0198
         NI    POCACT(CPCBASE),B'01111111'                      01S0198
*                 OCARC=CPCABORT;   /* FAIL APPL. CLOSE              */
         MVI   OCARC(CPCOCAPT),X'4C'                            01S0199
*                 OCAABORT=ON;      /* FLAGS WHICH WILL CAUSE CLOSE
*                                      TO FAIL                       */
         OI    OCAABORT(CPCOCAPT),B'00100000'                   01S0200
*               END;                                            01S0201
*                                                               01S0201
*           /*********************************************************/
*           /*                                                       */
*           /* WILL EXECUTE ONCE ONLY IF WE WANT RPL CHECK           */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0202
*           DO CPCINDEX=1 WHILE(CPCPTH1=ON);                    01S0202
@RF00195 DS    0H                                               01S0202
@RC00178 LA    @11,1                                            01S0202
         L     @15,RPHWEA(,CPCRPHPT)                            01S0202
         ST    @11,CPCINDEX+176(,@15)                           01S0202
         L     @11,RPHWEA(,CPCRPHPT)                            01S0202
         TM    CPCPTH1+176(@11),B'10000000'                     01S0202
         BNO   @DC00202                                         01S0202
*             IF POCRPL^=ZERO       /* RPL WAITING FOR RCVCMD        */
*               THEN                                            01S0203
         L     @11,POCRPL(,CPCBASE)                             01S0203
         LTR   @11,@11                                          01S0203
         BZ    @RF00203                                         01S0203
*               IF RPLSTYP='FF'X    /* IF IT IS A COPIED RPL         */
*                 THEN                                          01S0204
*                                                               01S0204
         CLI   RPLSTYP(@11),255                                 01S0204
         BNE   @RF00204                                         01S0204
*                 /***************************************************/
*                 /*                                                 */
*                 /* RELSTORE BUFADDR(POCRPL)                        */
*                 /*                                                 */
*                 /***************************************************/
*                                                               01S0205
*                 DO;                                           01S0205
*                                                               01S0205
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */
*/* END OF ATBUILD                                                   */
*                                                               01S0206
*                   RFY                                         01S0206
*                    (GPR02P,                                   01S0206
*                     GPR03P,                                   01S0206
*                     GPR04P,                                   01S0206
*                     GPR15P) RSTD;                             01S0206
*                   RFY                                         01S0207
*                    (GPR01P,                                   01S0207
*                     GPR13P) RSTD;                             01S0207
*                   GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);
          STM GPR02P,GPR13P,RPHWORK(GPR01P)
*                   RFY                                         01S0209
*                     GPR13P UNRSTD;                            01S0209
*                   GPR04P=POCRPL;                              01S0210
         L     GPR04P,POCRPL(,CPCBASE)                          01S0210
*                   DCL                                         01S0211
*                     1 FMZA0018 FIXED(31) STATIC LOCAL,        01S0211
*                      2 * FIXED(8) INIT(64),                   01S0211
*                      2 * PTR(24) INIT(0);                     01S0211
*                   GPR03F=FMZA0018;                            01S0212
         L     GPR03F,FMZA0018                                  01S0212
*                   DO;             /* LISATCVT                 01S0213
*                                      CVTBASE(PAGEABLE,(GPR15P))    */
*                     GPR15P=ATCLCPTR;/* SET UP ATCVT POINTER        */
         L     GPR15P,ATCLCPTR                                  01S0214
*                   END;            /* LISATCVT                      */
*                   GPR02P=GPR15P->ATCBPDA;                     01S0216
         L     GPR02P,ATCBPDA(,GPR15P)                          01S0216
*                   GPR15P=GPR15P->ATCSMRS;                     01S0217
         L     GPR15P,ATCSMRS(,GPR15P)                          01S0217
*                   DCL                                         01S0218
*                     FMZL0018 BASED(GPR15P) LABEL VALUERANGE(*);
*                   CALL FMZL0018;                              01S0219
         BALR  @14,@15                                          01S0219
*                   RFY                                         01S0220
*                     GPR13P RSTD;                              01S0220
*                   GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);
          LM GPR02P,GPR13P,RPHWORK(GPR01P)
*                   RFY                                         01S0222
*                    (GPR01P,                                   01S0222
*                     GPR13P) UNRSTD;                           01S0222
*                   RFY                                         01S0223
*                    (GPR02P,                                   01S0223
*                     GPR03P,                                   01S0223
*                     GPR04P,                                   01S0223
*                     GPR15P) UNRSTD;                           01S0223
*                 END;              /* *********END OF RELSTORE 01S0224
*                                      EXPANSION*********            */
*           END;                                                01S0225
@RF00204 DS    0H                                               01S0225
@RF00203 DS    0H                                               01S0225
@DC00202 DS    0H                                               01S0226
*         END;                                                  01S0226
*     END;                                                      01S0227
         EJECT
*CPCBFIND:                                                      01S0228
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            01S0228
         B     @PB00002                                         01S0228
CPCBFIND DS    0H                                               01S0229
*   RFY                                                         01S0229
*     CPCRETPT RSTD;                                            01S0229
*   CPCR14SV=CPCRETPT;              /* SAVE VALUE OF RETURN ADDRESS  */
         L     @15,RPHWEA(,CPCRPHPT)                            01S0230
         LA    @11,176                                          01S0230
         ALR   @11,@15                                          01S0230
         ST    CPCRETPT,CPCR14SV(,@11)                          01S0230
*   RFY                                                         01S0231
*     CPCRETPT UNRSTD;                                          01S0231
*   CPCBASE=POIPOCBQ;               /* SET PTR TO POCB QUEUE         */
         L     @15,OCWACVT(,@15)                                01S0232
         L     @15,ATCPOIA-1(,@15)                              01S0232
         LA    @15,0(,@15)                                      01S0232
         L     CPCBASE,POIPOCBQ-1(,@15)                         01S0232
         LA    CPCBASE,0(,CPCBASE)                              01S0232
*   RFY                                                         01S0233
*     ISTPOCB BASED(CPCBASE);                                   01S0233
*   CPCFND=OFF;                     /* INIT FOUND SWITCH             */
*                                                               01S0234
         NI    CPCFND(@11),B'01111111'                          01S0234
