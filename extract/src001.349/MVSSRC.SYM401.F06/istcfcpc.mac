         L     @15,OCWACVT(,@11)                                01S0254
         L     CPCREG8,PABWEQP+1567(,@15)                       01S0254
         LA    CPCREG8,0(,CPCREG8)                              01S0254
*       CPCFND=OFF;                 /* INIT FOUND SWITCH             */
         NI    CPCFND+176(@11),B'01111111'                      01S0255
