*/*                                                          #Y30OE72*/
*/* ROUTINE NAME = INSPECT                                   #Y30OE72*/
*/*                                                          #Y30OE72*/
*/* FUNCTION = CHECK PRESENCE AND VALIDITY OF JCL DD         #Y30OE72*/
*/*            STATEMENTS.                                   #Y30OE72*/
*/*                                                          @Y30OE72*/
*/* INPUT = TIOT (SEE INPUT SECTION OF MODULE SPECS)         @Y30OE72*/
*/*                                                          @Y30OE72*/
*/* OUTPUT= SYSPRINT DD OPENED.                              @Y30OE72*/
*/*         WORKSYS1 & WORKSYS2 POINTING TO DD ENTRIES       @Y30OE72*/
*/*         FOR SYSUT1 & SYSUT2.                             @Y30OE72*/
*/*         WORKSISW SET ON IF SYSIN DD WAS FOUND            @Y30OE72*/
*/*         FAILSW ON IF ERROR WAS FOUND                     @Y30OE72*/
*/*                                                          @Y30OE72*/
*/* ROUTINES CALLED = MSGWRTR - MESSAGE WRITING ROUTINE      @Y30OE72*/
*/*                                                          @Y30OE72*/
*/* MACROS = IEFTIOT1,EXTRACT,OPEN,WTO                       @Y30OE72*/
*/*                                                          @Y30OE72*/
*/********************************************************************/
*                                                                  0347
*INSPECT:                           /*                       @Y30OE72*/
*   PROCEDURE OPTIONS(SAVE(14));    /* PROCEDURE START       @Y30OE72*/
INSPECT  ST    @14,@SA00006                                        0347
*   GEN(EXTRACT TIOTADDR,,FIELDS=(TIOT));/* GET TIOT ADDR    @Y30OE72*/
         EXTRACT TIOTADDR,,FIELDS=(TIOT)
*   WORKSW=ON;                      /* SET DOWHILE CONTROL   @Y30OE72*/
         MVC   WORKSW(1),ON                                        0349
*   DO WHILE WORKSW=ON;             /* SCAN TIOT ENTRIES     @Y30OE72*/
         B     @DE00350                                            0350
@DL00350 DS    0H                                                  0351
*     IF TIOEDDNM=SYSPRTDD THEN     /* SYSPRINT DD ?         @Y30OE72*/
         L     @12,TIOTADDR                                        0351
         CLC   TIOEDDNM(8,@12),@CC02087                            0351
         BNE   @RF00351                                            0351
*       WORKSPSW=ON;                /* INDICATE SYSPRINT     @Y30OE72
*                                      DD PROVIDED           @Y30OE72*/
         MVC   WORKSPSW(1),ON                                      0352
*     IF TIOEDDNM=SYSINDD THEN      /* SYSIN DD ?            @Y30OE72*/
@RF00351 L     @12,TIOTADDR                                        0353
         CLC   TIOEDDNM(8,@12),@CC02089                            0353
         BNE   @RF00353                                            0353
*       WORKSISW=ON;                /* INDICATE SYSIN        @Y30OE72
*                                      DD PROVIDED           @Y30OE72*/
         MVC   WORKSISW(1),ON                                      0354
*     IF TIOEDDNM=SYSUT1DD THEN     /* SYSUT1 DD ?           @Y30OE72*/
@RF00353 L     @12,TIOTADDR                                        0355
         CLC   TIOEDDNM(8,@12),@CC02091                            0355
         BNE   @RF00355                                            0355
*       DO;                         /* YES. INDICATE         @Y30OE72*/
*         WORKU1SW=ON;              /* SYSUT1 PROVIDED AND   @Y30OE72*/
         MVC   WORKU1SW(1),ON                                      0357
*         WORKSYS1=TIOTADDR;        /* SAVE DD ENTRY ADDR    @Y30OE72*/
         STCM  @12,7,WORKSYS1                                      0358
*       END;                        /*                       @Y30OE72*/
*     IF TIOEDDNM=SYSUT2DD THEN     /* SYSUT2 DD ?           @Y30OE72*/
@RF00355 L     @12,TIOTADDR                                        0360
         CLC   TIOEDDNM(8,@12),@CC02093                            0360
         BNE   @RF00360                                            0360
*       DO;                         /* YES. INDICATE         @Y30OE72*/
*         WORKU2SW=ON;              /* SYSUT2 PROVIDED AND   @Y30OE72*/
         MVC   WORKU2SW(1),ON                                      0362
*         WORKSYS2=TIOTADDR;        /* SAVE DD ENTRY ADDR    @Y30OE72*/
         STCM  @12,7,WORKSYS2                                      0363
*       END;                        /*                       @Y30OE72*/
*     IF TIOELNGH=ZERO THEN         /* END OF TIOT ?         @Y30OE72*/
@RF00360 L     @12,TIOTADDR                                        0365
         CLI   TIOELNGH(@12),0                                     0365
         BNE   @RF00365                                            0365
*       WORKSW=OFF;                 /* YES. GET OUT OF LOOP  @Y30OE72*/
         MVC   WORKSW(1),OFF                                       0366
*     ELSE                                                         0367
*       TIOTADDR=TIOTADDR+TIOELNGH; /* POINT TO NEXT DD ENT  @Y30OE72*/
         B     @RC00365                                            0367
@RF00365 L     @12,TIOTADDR                                        0367
         SLR   @11,@11                                             0367
         IC    @11,TIOELNGH(,@12)                                  0367
         ALR   @11,@12                                             0367
         ST    @11,TIOTADDR                                        0367
*   END;                            /* END DOWHILE           @Y30OE72*/
@RC00365 DS    0H                                                  0368
@DE00350 CLC   WORKSW(1),ON                                        0368
         BE    @DL00350                                            0368
*   IF WORKSPSW=ON THEN             /* SYSPRINT DD FOUND ?   @Z30OE72*/
         CLC   WORKSPSW(1),ON                                      0369
         BNE   @RF00369                                            0369
*     DO;                           /* YES.                  @Z30OE72*/
*       GEN(OPEN (SYSPRINT,(OUTPUT)));/* OPEN SYSPRINT FILE  @Y30OE72*/
         OPEN (SYSPRINT,(OUTPUT))
*       REG1=ADDR(SYSPRINT);        /* POINT TO DCB          @Y30OE72*/
         LA    REG1,SYSPRINT                                       0372
*       IF DCBOFOPN^=SETON THEN     /* OPEN SUCCESSFUL?      @Y30OE72*/
         TM    DCBOFOPN+40(REG1),B'00010000'                       0373
         BO    @RF00373                                            0373
