*/*                                                          @Y30OE72*/
*/* ROUTINE NAME = INTERPR                                   @Y30OE72*/
*/*                                                          @Y30OE72*/
*/* FUNCTION = READ    UTILITY CONTROL STATEMENTS AND        @Y30OE72*/
*/*            CHECK THEM FOR PROPER SEQUENCE, FORMAT AND    @Y30OE72*/
*/*            VALIDITY.                                     @Y30OE72*/
*/*                                                          @Y30OE72*/
*/* INPUT = CONTROL STATEMENTS ARE READ FROM THE SYSIN       @Y30OE72*/
*/*         DATA SET. SET INPUT SECTION OF MODULE            @Y30OE72*/
*/*         SPECIFICATIONS FOR FORMAT DESCRIPTION.           @Y30OE72*/
*/*                                                          @Y30OE72*/
*/* OUTPUT = SET VOLSW ON & SAVE VOL PARAMETER IN            @Y30OE72*/
*/*          REPVOL IF VOL STMT SUPPLIED.                    @Y30OE72*/
*/*          STORE DS STMT INFO IN REPTAB (IF PRESENT AND    @Y30OE72*/
*/*          CORRECT). STORE PERTAINING SEQUENCE NUMBERS     @Y30OE72*/
*/*          IN NUMTAB (REPLACEMENTS) OR DELTAB              @Y30OE72*/
*/*          (DELETIONS).                                    @Y30OE72*/
*/*          FAILSW = ON IF AN ERROR IS DETECTED.            @Y30OE72*/
*/*                                                          @Y30OE72*/
*/* ROUTINES CALLED = MSGWRTR - MESSAGE WRITING ROUTINE      @Y30OE72*/
*/*                   VOLCHECK -TO CHECK VOL STMT VOLID      @Y30OE72*/
*/* MACROS = OPEN,GET                                        @Y30OE72*/
*/*                                                          @Y30OE72*/
*/********************************************************************/
*                                                                  0441
*INTERPR:                           /*                       @Y30OE72*/
*   PROCEDURE OPTIONS(SAVE(14));    /*                       @Y30OE72*/
INTERPR  ST    @14,@SA00007                                        0441
*   GEN(OPEN (SYSIN));              /* OPEN CONTROL DATA SET @Y30OE72*/
         OPEN (SYSIN)
*   REG1=ADDR(SYSIN);               /* POINT TO DCB          @Y30OE72*/
         LA    REG1,SYSIN                                          0443
*   IF DCBOFOPN^=SETON THEN         /* OPEN SUCCESSFUL?      @Y30OE72*/
         TM    DCBOFOPN+40(REG1),B'00010000'                       0444
         BO    @RF00444                                            0444
*     DO;                           /* NO.                   @Y30OE72*/
*       I511DDNM=SYSINDD;           /* DDNAME                @Y30OE72*/
         MVC   I511DDNM(8),@CC02089                                0446
*       REG1=ADDR(IEB511I);         /* MSG ADDR              @Y30OE72*/
         LA    REG1,IEB511I                                        0447
*       MSGLEN=LENGTH(IEB511I);     /* ISSUE MSG             @Y30OE72*/
         MVI   MSGLEN,X'28'                                        0448
*       CALL MSGWRTR;               /* IEB511I               @Y30OE72*/
         BAL   @14,MSGWRTR                                         0449
*       FAILSW=ON;                  /* TERMINATE RUN         @Y30OE72*/
         MVC   FAILSW(1),ON                                        0450
*     END;                          /*                       @Y30OE72*/
*   D=1;                            /* INIT DELTAB INDEX     @Y30OE72*/
@RF00444 MVI   D,X'01'                                             0452
*   N=1;                            /* INIT REPTAB INDEX     @Y30OE72*/
         MVI   N,X'01'                                             0453
*   IF FAILSW=OFF THEN              /* FILE OPENED OK?       @Y30OE72*/
         CLC   FAILSW(1),OFF                                       0454
         BNE   @RF00454                                            0454
*     WORKSW=ON;                    /* YES. SET CONTROL SW   @Y30OE72*/
         MVC   WORKSW(1),ON                                        0455
*INTDO1:                            /*                       @Y30OE72*/
*   DO WHILE WORKSW=ON;             /* PROCESS CONTROL STMTS @Y30OE72*/
@RF00454 DS    0H                                                  0456
INTDO1   B     @DE00456                                            0456
@DL00456 DS    0H                                                  0457
*     GEN(GET SYSIN,CARDBUF)        /* READ A                @Y30OE72*/
*     REFS(CARDBUF);                /* CONTROL STATEMENT     @Y30OE72*/
         GET SYSIN,CARDBUF
*     MSGLEN=LENGTH(CARD);          /* PRINT                 @Y30OE72*/
         MVI   MSGLEN,X'50'                                        0458
*     REG1=ADDR(CARD);              /* CONTROL               @Y30OE72*/
         LA    REG1,CARD                                           0459
*     CALL MSGWRTR;                 /* STATEMENT             @Y30OE72*/
         BAL   @14,MSGWRTR                                         0460
*     IF PRINTERR=SETON THEN        /* SYSPRINT ERROR?       @Y30OE72*/
         TM    PRINTERR,B'00001000'                                0461
         BNO   @RF00461                                            0461
*       WORKSW=OFF;                 /* YES. QUIT READING     @Y30OE72*/
         MVC   WORKSW(1),OFF                                       0462
*     SWDS=ZERO;                    /* CLEAR SWITCH BYTE     @Y30OE72*/
@RF00461 MVI   SWDS,X'00'                                          0463
*     IF CARDBUF(1:5)=CHARVOL THEN  /* VOL CONTROL STMT?     @Y30OE72*/
         CLC   CARDBUF(5),@CC02043                                 0464
         BNE   @RF00464                                            0464
*       CASESELT=VOLSTMT;           /* YES. TAKE VOL PATH    @Y30OE72*/
         LA    CASESELT,1                                          0465
*     ELSE                          /* NO.                   @Y30OE72*/
*       IF CARDBUF(1:3)=BLDS THEN   /* DS CONTROL STMT?      @Y30OE72*/
         B     @RC00464                                            0466
@RF00464 CLC   CARDBUF(3),@CC02041                                 0466
         BNE   @RF00466                                            0466
*         CASESELT=DSSTMT;          /* YES. TAKE DS PATH     @Y30OE72*/
         LA    CASESELT,2                                          0467
*       ELSE                        /* NO.                   @Y30OE72*/
