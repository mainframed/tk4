         CLI   DSNR,19                                             0517
         BH    @RT00517                                            0517
         CLI   DSNR,1                                              0517
         BNL   @RF00517                                            0517
@RT00517 DS    0H                                                  0518
*             SW515I=ERRON;         /* IF NOT CANCEL         @Y30OE72*/
         OI    SW515I,B'00001000'                                  0518
