*/* ROUTINE NAME = WRITEIO                                   @Y30OE72*/
*/*                                                          @Y30OE72*/
*/* FUNCTION = ISSUE I/O TO WRITE DATA ON DISKETTE (DATA IS  @Y30OE72*/
*/*            WRITTEN A TRACK AT A TIME).                   @Y30OE72*/
*/*                                                          @Y30OE72*/
*/* INPUT = OT1IOB,OT2IOB AND THEIR ASSOCIATED ECBS,CCWS,    @Y30OE72*/
*/*         AND BUFFERS.                                     @Y30OE72*/
*/*         MODE - SWITCH INDICATING WHETHER OR NOT DATA SET @Y30OE72*/
*/*                IS COMPLETED (I.E. FLUSH BUFFERS).        @Y30OE72*/
*/*                                                          @Y30OE72*/
*/* OUTPUT = DATA WRITTEN TO OUTPUT DISKETTE                 @Y30OE72*/
*/*                                                          @Y30OE72*/
*/* ROUTINES CALLED = NONE                                   @Y30OE72*/
*/*                                                          @Y30OE72*/
*/* MACROS = EXCP,WAIT                                       @Y30OE72*/
*/*                                                          @Y30OE72*/
*/********************************************************************/
*                                                                  1933
*WRITEIO:                           /* SEE GENERAL COMMENTS  @Y30OE72*/
*   PROC OPTIONS(SAVE(14));         /* ON PREVIOUS PAGE      @Y30OE72*/
WRITEIO  ST    @14,@SA00027                                        1933
*   OTIOBECB=ZEROREC(1:4);          /* CLEAR ECB             @Y30OE72*/
*                                                                  1934
         L     @12,OTIOBPTR                                        1934
         AL    @12,@CF03567                                        1934
         MVC   OTIOBECB-4(4,@12),ZEROREC                           1934
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP CCW CHAIN TO WRITE SHORT TRACKS, IF NECASSARY  @Y30OE72*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1935
*   IF STARTREC^=SECTOR1 THEN       /* NON-TRK BDY START?    @Y30OE72*/
         CLC   STARTREC(1),SECTOR1                                 1935
         BE    @RF01935                                            1935
*     DO;                           /* YES.                  @Y30OE72*/
*       TICADDR=ADDR(OT1WTCCW)+     /* SET TIC ADDR TO       @Y30OE72*/
*           ((STARTREC-1)*CCWLEN);  /* PROPER WRITE CCW      @Y30OE72*/
         LA    @12,OT1WTCCW                                        1937
         SLR   @11,@11                                             1937
         IC    @11,STARTREC                                        1937
         BCTR  @11,0                                               1937
         SLA   @11,3                                               1937
         ALR   @12,@11                                             1937
         STCM  @12,7,TICADDR                                       1937
*       OT1WTCCW=OT1WTCCW&&TICCCW;  /* REPLACE FIRST         @Y30OE72*/
         XC    OT1WTCCW(8),TICCCW                                  1938
*       TICCCW=TICCCW&&OT1WTCCW;    /* WRITE CCW             @Y30OE72*/
         XC    TICCCW(8),OT1WTCCW                                  1939
*       OT1WTCCW=OT1WTCCW&&TICCCW;  /* WITH TIC CCW          @Y30OE72*/
         XC    OT1WTCCW(8),TICCCW                                  1940
*       STARTREC=SECTOR1;           /* RESET START ADDR      @Y30OE72*/
         MVC   STARTREC(1),SECTOR1                                 1941
*     END;                          /*                       @Y30OE72*/
*   IF CCWPTR^=ADDR(OTNOOP) THEN    /* FULL BUFFER?          @Y30OE72*/
@RF01935 L     @12,CCWPTR                                          1943
         L     @11,OTIOBPTR                                        1943
         LA    @02,OTNOOP(,@11)                                    1943
         CR    @12,@02                                             1943
         BE    @RF01943                                            1943
*     DO;                           /* NO.                   @Y30OE72*/
*       CURCCW=CURCCW&&OTNOOP;      /* PLACE LAST NOOP       @Y30OE72*/
         XC    CURCCW(8,@12),OTNOOP(@11)                           1945
*       OTNOOP=OTNOOP&&CURCCW;      /* AT CURRENT            @Y30OE72*/
         XC    OTNOOP(8,@11),CURCCW(@12)                           1946
*       CURCCW=CURCCW&&OTNOOP;      /* END OF CHAIN          @Y30OE72*/
         XC    CURCCW(8,@12),OTNOOP(@11)                           1947
*       NOOP@=CCWPTR;               /* SAVE PTR TO NOOP CCW  @Y30OE72*/
         ST    @12,NOOP@                                           1948
*     END;                          /*                       @Y30OE72*/
*                                                                  1949
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE EXCP, WAIT , AND CHECK FOR ERRORS               @Y30OE72*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1950
*   REG1=OTIOBPTR;                  /* SET UP FOR I/O        @Y30OE72*/
@RF01943 L     REG1,OTIOBPTR                                       1950
*   GEN(EXCP (REG1));               /* ISSUE EXCP            @Y30OE72*/
         EXCP (REG1)
*   IF MODE=FLIP THEN               /* SWITCH MODE?          @Y30OE72*/
         CLI   MODE,C'F'                                           1952
         BNE   @RF01952                                            1952
*     DO;                           /* YES.                  @Y30OE72*/
*       OTIOBPTR=OTIOBNXT;          /* PT TO OTHER IOB       @Y30OE72*/
         L     @12,OTIOBPTR                                        1954
         L     @12,OTIOBNXT(,@12)                                  1954
         ST    @12,OTIOBPTR                                        1954
*       OUTSKTT=OUTSKTT+2;          /* UPDATE TRK NUM        @Y30OE72*/
         LA    @11,2                                               1955
         SLR   @02,@02                                             1955
         IC    @02,OUTSKTT(,@12)                                   1955
         ALR   @11,@02                                             1955
         STC   @11,OUTSKTT(,@12)                                   1955
*       CCWPTR=ADDR(OUTWTCCW);      /* PT TO 1ST WRITE CCW   @Y30OE72*/
         LA    @12,OUTWTCCW(,@12)                                  1956
         ST    @12,CCWPTR                                          1956
*     END;                          /*                       @Y30OE72*/
*   IF OUTECBC^=SETON THEN          /* ECB COMPLETE?         @Y30OE72*/
@RF01952 L     @12,OTIOBPTR                                        1958
         SH    @12,@CH00034                                        1958
         TM    OUTECBC(@12),B'01000000'                            1958
         BO    @RF01958                                            1958
*     DO;                           /* NO.                   @Y30OE72*/
*       REG1=ADDR(OTIOBECB);        /* SET UP FOR WAIT       @Y30OE72*/
         LA    REG1,OTIOBECB(,@12)                                 1960
*       GEN(WAIT ECB=(REG1));       /* ISSUE WAIT            @Y30OE72*/
         WAIT ECB=(REG1)
*     END;                          /*                       @Y30OE72*/
*   IF OUTECBCC^=GOODIO THEN        /* I/O ERROR?            @Y30OE72*/
@RF01958 L     @12,OTIOBPTR                                        1963
         AL    @12,@CF03567                                        1963
         CLI   OUTECBCC-4(@12),X'7F'                               1963
         BE    @RF01963                                            1963
*     SW504I=ERRON;                 /* YES. SET IND FOR CALR @Y30OE72*/
*                                                                  1964
         OI    SW504I,B'00001000'                                  1964
*   /*****************************************************************/
*   /*                                                               */
*   /* RESTORE CCW CHAIN TO WRITE FULL TRACKS, IF SHORT      @Y30OE72*/
*   /* TRACK WAS WRITTEN                                     @Y30OE72*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1965
*   IF CURCCWOP=TIC THEN            /* 1ST CCW A TIC?        @Y30OE72*/
@RF01963 L     @12,CCWPTR                                          1965
         CLI   CURCCWOP(@12),8                                     1965
         BNE   @RF01965                                            1965
*     DO;                           /* YES.                  @Y30OE72*/
*       OUTSKSS=SECTOR1;            /* RESET SECTOR NUM      @Y30OE72*/
         L     @12,OTIOBPTR                                        1967
         MVC   OUTSKSS(1,@12),SECTOR1                              1967
*       TICCCW=TICCCW&&OUTWTCCW;    /* RESTORE               @Y30OE72*/
         XC    TICCCW(8),OUTWTCCW(@12)                             1968
*       OUTWTCCW=OUTWTCCW&&TICCCW;  /* FIRST                 @Y30OE72*/
         XC    OUTWTCCW(8,@12),TICCCW                              1969
*       TICCCW=TICCCW&&OUTWTCCW;    /* WRITE CCW             @Y30OE72*/
         XC    TICCCW(8),OUTWTCCW(@12)                             1970
*     END;                          /*                       @Y30OE72*/
*   IF OTNOOPOP=WRITE THEN          /* 27TH CCW A WRITE?     @Y30OE72*/
@RF01965 L     @12,OTIOBPTR                                        1972
         CLI   OTNOOPOP(@12),5                                     1972
         BNE   @RF01972                                            1972
*     DO;                           /* YES.                  @Y30OE72*/
*       OTNOOP=OTNOOP&&NOOPCCW;     /* RESTORE NOOP CCW      @Y30OE72*/
         L     @11,NOOP@                                           1974
         XC    OTNOOP(8,@12),NOOPCCW(@11)                          1974
*       NOOPCCW=NOOPCCW&&OTNOOP;    /* TO END                @Y30OE72*/
         XC    NOOPCCW(8,@11),OTNOOP(@12)                          1975
*       OTNOOP=OTNOOP&&NOOPCCW;     /* OF CHAIN              @Y30OE72*/
         XC    OTNOOP(8,@12),NOOPCCW(@11)                          1976
*     END;                          /*                       @Y30OE72*/
*   DFOPSCNT=0;                     /* RESET DEFINE OPS CNT  @Y30OE72*/
@RF01972 L     @12,OTIOBPTR                                        1978
         MVI   DFOPSCNT(@12),X'00'                                 1978
*   END WRITEIO;                    /*                       @Y30OE72*/
