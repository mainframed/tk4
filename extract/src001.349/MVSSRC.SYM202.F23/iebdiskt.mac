         B     @RC00478                                            0485
@RF00478 DS    0H                                                  0486
*         LETPTR=ADDR(VOLAREA);     /* PT TO 1ST VOLID CHAR  @Y30OE72*/
         LA    LETPTR,VOLAREA                                      0486
