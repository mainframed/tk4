*   DSEODSEC=WORK3(4);              /* DS TABLE              @Y30OE72*/
         MVC   DSEODSEC(1),WORK3+3                                 1916
*   WORK1=WORK1&&WORK1;             /* INITIALIZE WORKFIELD  @Y30OE72*/
         XC    WORK1(4),WORK1                                      1917
