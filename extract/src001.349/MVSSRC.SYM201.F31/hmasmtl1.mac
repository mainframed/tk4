         TITLE 'HMASMTL1 - HMASMP HMASMICT LMOD SET UP MODULE'
HMASMTL1 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMTL1  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*                                                                  0067
*   /*****************************************************************/
*   /*                                                               */
*   /* HMASMTL1 << MAINLINE                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0067
*   TL1RTNCD=ZERO;                  /* PRESET GOOD RTN CODE          */
         SLR   TL1RTNCD,TL1RTNCD                                   0067
*   TL1FLGS=ZERO;                   /* INIT FLAGS                    */
*                                                                  0068
         MVI   TL1FLGS,X'00'                                       0068
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP THRU MOD SECTION. FOR EACH MOD FROM A FAILING SYSMOD,    */
*   /* MARK THE MOD NOGO                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0069
*   CALL NOGOCHK;                   /*                       @Z67SP00*/
*                                                                  0069
         BAL   @14,NOGOCHK                                         0069
*   /*****************************************************************/
*   /*                                                               */
*   /* RESET LMM GTP SO THAT MOD&SYSMOD NAMES ARE THE KEY            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0070
*   RFY                                                            0070
*     HMASMGTP BASED(TBLXLMGT);     /*                       @Z67SP00*/
*   RFY                                                            0071
*     LMMRECRD BASED(GTPBUFAD);     /*                       @Z67SP00*/
*   SAVEKEYL=GTPKEYLN;              /* SAVE ORIGINNAL KEY LENGTH   0072
*                                                            @Z67SP00*/
         L     @10,@PC00001                                        0072
         L     @10,TBLXLMGT(,@10)                                  0072
         LH    SAVEKEYL,GTPKEYLN(,@10)                             0072
*   GTPKEYLN=LENGTH(LMMMNAME)+LENGTH(LMMSMD);/* LENGTH OF KEY FOR  0073
*                                      SEARCH                @Z67SP00*/
*                                                                  0073
         MVC   GTPKEYLN(2,@10),@CH00157                            0073
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP THRU THE MOD SECTION AND FOR EACH SELECTED SYSMOD, LOCATE*/
*   /* IT ON THE GTA FILE. IF FOUND, ONE OR MORE LMOD ENTRIES ARE TO */
*   /* BE PROCESSED FOR THIS MOD ENTRY                       @Z67SP00*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0074
*   LMMRECRD=''B;                   /* CLEAR RECORD BUFFER   @Z67SP00*/
         L     @10,GTPBUFAD(,@10)                                  0074
         XC    LMMRECRD(23,@10),LMMRECRD(@10)                      0074
*   DO MCTR=1 BY 1                  /* PROCESS ALL MOD ENTRIES     0075
*                                                            @Z67SP00*/
*         WHILE(ICTMEND(MCTR)^=ICTENDNT);/*                  @Z67SP00*/
*                                                                  0075
         LA    MCTR,1                                              0075
         B     @DE00075                                            0075
@DL00075 DS    0H                                                  0076
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS MOD ONLY UNDER CERTAIN CONDITIONS                   */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0076
*     CALL MODCHK(ICTMOD(MCTR));    /*                       @Z67SP00*/
         LR    @10,MCTR                                            0076
         MH    @10,@CH00939                                        0076
         L     @05,CCAICMOD(,CCAPTR)                               0076
         AL    @05,@CF00947                                        0076
         LA    @10,ICTMOD(@10,@05)                                 0076
         ST    @10,@AL00001                                        0076
         LA    @01,@AL00001                                        0076
         BAL   @14,MODCHK                                          0076
*     RFY                                                          0077
*       RTNCODE RSTD;                                              0077
*     IF RTNCODE=ZERO THEN          /* MOD TO BE PROCESSED?          */
         LTR   RTNCODE,RTNCODE                                     0078
         BNZ   @RF00078                                            0078
*       DO;                         /* YES,                          */
*         RFY                                                      0080
*           RTNCODE UNRSTD;                                        0080
*                                                                  0080
*         /***********************************************************/
*         /*                                                         */
*         /* FOR EACH LMOD, SEARCH TO SEE IF IT IS IN THE ICT LMOD   */
*         /* SECTION IF NOT, BUILD ONE. REGARDLESS, ADD LMOD PTR TO  */
*         /* MOD TO LMOD INDEX.                              @Z67SP00*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0081
*         LMMSMD=ICTPTFS(1);        /* SYSMOD NAME           @Z67SP00*/
         L     @10,@PC00001                                        0081
         L     @10,TBLXLMGT(,@10)                                  0081
         L     @05,GTPBUFAD(,@10)                                  0081
         LR    @15,MCTR                                            0081
         MH    @15,@CH00939                                        0081
         L     @14,CCAICMOD(,CCAPTR)                               0081
         LA    @01,0(@15,@14)                                      0081
         AL    @01,@CF00948                                        0081
         L     @01,ICTPPTR-96(,@01)                                0081
         MVC   LMMSMD(7,@05),ICTPTFS(@01)                          0081
*         LMMMNAME=ICTMNAME(MCTR);  /* MOD NAME              @Z67SP00*/
         ALR   @14,@15                                             0082
         AL    @14,@CF00947                                        0082
         MVC   LMMMNAME(8,@05),ICTMNAME(@14)                       0082
*         LMMLNAME=''B;             /* CLEAR LMOD NAME       @Z67SP00*/
         XC    LMMLNAME(8,@05),LMMLNAME(@05)                       0083
*         GTPFUNCT=GTPLOC;          /* FUNCTION IS LOCATE    @Z67SP00*/
         MVI   GTPFUNCT(@10),X'03'                                 0084
*         CALL HMASMGTA(HMASMGTP);  /*                       @Z67SP00*/
         ST    @10,@AL00001                                        0085
         L     @15,@CV00880                                        0085
         LA    @01,@AL00001                                        0085
         BALR  @14,@15                                             0085
*         IF GTPRETRN=GTPGOOD THEN  /* RECORD FOUND ?        @Z67SP00*/
         L     @10,@PC00001                                        0086
         L     @10,TBLXLMGT(,@10)                                  0086
         CLI   GTPRETRN(@10),0                                     0086
         BNE   @RF00086                                            0086
*           DO;                     /*                       @Z67SP00*/
*             GTPFUNCT=GTPSTRTN;    /* FUNCTION IS START NEXT      0088
*                                                            @Z67SP00*/
         MVI   GTPFUNCT(@10),X'06'                                 0088
*             CALL HMASMGTA(HMASMGTP);/*                     @Z67SP00*/
         ST    @10,@AL00001                                        0089
         L     @15,@CV00880                                        0089
         LA    @01,@AL00001                                        0089
         BALR  @14,@15                                             0089
*             GTPFUNCT=GTPGETN;     /* FUNCTION IS GET NEXT  @Z67SP00*/
         L     @10,@PC00001                                        0090
         L     @10,TBLXLMGT(,@10)                                  0090
         MVI   GTPFUNCT(@10),X'08'                                 0090
*             ENDMOD=OFF;           /* END OF LMODS FOR MOD NOT    0091
*                                      REACHED               @Z67SP00*/
         NI    ENDMOD,B'01111111'                                  0091
*             DO WHILE(ENDMOD=OFF   /* PROCESS ALL LMODS FOR MOD   0092
*                                                            @Z67SP00*/
*                   &TL1RTNCD<STOPFUNC);/*                   @Z67SP00*/
         B     @DE00092                                            0092
@DL00092 DS    0H                                                  0093
*               CALL HMASMGTA(HMASMGTP);/*                   @Z67SP00*/
         L     @10,@PC00001                                        0093
         L     @10,TBLXLMGT(,@10)                                  0093
         ST    @10,@AL00001                                        0093
         L     @15,@CV00880                                        0093
         LA    @01,@AL00001                                        0093
         BALR  @14,@15                                             0093
*               IF GTPRETRN=GTPEOF THEN/* END OF FILE?       @Z67SP00*/
         L     @10,@PC00001                                        0094
         L     @10,TBLXLMGT(,@10)                                  0094
         CLI   GTPRETRN(@10),4                                     0094
         BNE   @RF00094                                            0094
*                 DO;               /* YES,                  @Z67SP00*/
*                   ENDMOD=ON;      /* END OF THIS MOD       @Z67SP00*/
         OI    ENDMOD,B'10000000'                                  0096
*                 END;              /*                       @Z67SP00*/
*               ELSE                                               0098
*                 DO;               /*                       @Z67SP00*/
         B     @RC00094                                            0098
@RF00094 DS    0H                                                  0099
*                   IF LMMMNAME^=ICTMNAME(MCTR)/* NOT FOR CURRENT  0099
*                                      MOD?                          */
*                       ×LMMSMD^=ICTPTFS(1) THEN                   0099
         L     @10,@PC00001                                        0099
         L     @10,TBLXLMGT(,@10)                                  0099
         L     @10,GTPBUFAD(,@10)                                  0099
         LR    @05,MCTR                                            0099
         MH    @05,@CH00939                                        0099
         L     @15,CCAICMOD(,CCAPTR)                               0099
         ST    @05,@TF00001                                        0099
         ALR   @05,@15                                             0099
         AL    @05,@CF00947                                        0099
         CLC   LMMMNAME(8,@10),ICTMNAME(@05)                       0099
         BNE   @RT00099                                            0099
         L     @05,@TF00001                                        0099
         AL    @15,@CF00948                                        0099
         L     @05,ICTPPTR-96(@05,@15)                             0099
         CLC   LMMSMD(7,@10),ICTPTFS(@05)                          0099
         BE    @RF00099                                            0099
@RT00099 DS    0H                                                  0100
*                     DO;           /*                       @Z67SP00*/
*                       ENDMOD=ON;  /* END OF RECORDS FOR CURRENT MOD*/
         OI    ENDMOD,B'10000000'                                  0101
*                     END;          /*                       @Z67SP00*/
*                   ELSE                                           0103
*                     ;             /* LMM RECORD IS FOR CURRENT MOD
*                                                            @Z67SP00*/
@RF00099 DS    0H                                                  0104
*                 END;              /*                       @Z67SP00*/
*               IF ENDMOD=OFF THEN  /* LOAD MOD FOR THIS MOD?      0105
*                                                            @Z67SP00*/
@RC00094 TM    ENDMOD,B'10000000'                                  0105
         BNZ   @RF00105                                            0105
*                 DO;               /* YES,                  @Z67SP00*/
*                   CALL ADDLMOD(ICTMOD(MCTR),LMMLNAME);/* PROCESS 0107
*                                      THE LMOD                      */
         LR    @10,MCTR                                            0107
         MH    @10,@CH00939                                        0107
         L     @05,CCAICMOD(,CCAPTR)                               0107
         AL    @05,@CF00947                                        0107
         LA    @10,ICTMOD(@10,@05)                                 0107
         ST    @10,@AL00001                                        0107
         L     @10,@PC00001                                        0107
         L     @10,TBLXLMGT(,@10)                                  0107
         L     @10,GTPBUFAD(,@10)                                  0107
         LA    @10,LMMLNAME(,@10)                                  0107
         ST    @10,@AL00001+4                                      0107
         LA    @01,@AL00001                                        0107
         BAL   @14,ADDLMOD                                         0107
*                   RFY                                            0108
*                     RTNCODE RSTD; /*                       @Z67SP00*/
*                   TL1RTNCD=MAX(TL1RTNCD,RTNCODE);/* SAVE RC      0109
*                                                            @Z67SP00*/
         CR    TL1RTNCD,RTNCODE                                    0109
         BNL   *+6
         LR    TL1RTNCD,RTNCODE                                    0109
*                   RFY                                            0110
*                     RTNCODE UNRSTD;/*                      @Z67SP00*/
*                 END;              /*                       @Z67SP00*/
*               ELSE                                               0112
*                 ;                 /* NOT FOR THIS MOD      @Z67SP00*/
@RF00105 DS    0H                                                  0113
*             END;                  /* END OF PROCESSING ONE MOD   0113
*                                                            @Z67SP00*/
@DE00092 TM    ENDMOD,B'10000000'                                  0113
         BNZ   @DC00092                                            0113
         CH    TL1RTNCD,@CH00265                                   0113
         BL    @DL00092                                            0113
@DC00092 DS    0H                                                  0114
*           END;                    /*                       @Z67SP00*/
*         ELSE                                                     0115
*           ;                       /* NOT FOUND, PROCESS NEXT MOD 0115
*                                                            @Z67SP00*/
@RF00086 DS    0H                                                  0116
*       END;                                                       0116
*     ELSE                                                         0117
*       ;                           /* SKIP CURRENT MOD ENTRY        */
@RF00078 DS    0H                                                  0118
*   END;                            /* END OF PROCESS ALL MOD ENTRIES
*                                                            @Z67SP00*/
*                                                                  0118
         AH    MCTR,@CH00061                                       0118
@DE00075 LR    @10,MCTR                                            0118
         MH    @10,@CH00939                                        0118
         L     @05,CCAICMOD(,CCAPTR)                               0118
         AL    @05,@CF00947                                        0118
         L     @10,ICTMEND(@10,@05)                                0118
         CL    @10,ICTENDNT                                        0118
         BNE   @DL00075                                            0118
*   /*****************************************************************/
*   /*                                                               */
*   /* RESTORE LMM GTP TO ORIGINAL KEY LENGTH                @Z67SP00*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0119
*   GTPKEYLN=SAVEKEYL;              /* RESTORE THE KEY LENGTH      0119
*                                                            @Z67SP00*/
         L     @10,@PC00001                                        0119
         L     @10,TBLXLMGT(,@10)                                  0119
         STH   SAVEKEYL,GTPKEYLN(,@10)                             0119
*   RETURN CODE(TL1RTNCD);                                         0120
         LR    @15,TL1RTNCD                                        0120
         L     @13,4(,@13)                                         0120
         L     @14,12(,@13)                                        0120
         LM    @00,@12,20(@13)                                     0120
         BR    @14                                                 0120
         EJECT
*                                                                  0121
*   /*****************************************************************/
*   /*                                                               */
*   /* NOGOCHK - MARK MOD NOGO IF ITS SYSMOD IS NOGO         @Z67SP00*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0121
*NOGOCHK:                                                          0121
*   PROC;                           /*                       @Z67SP00*/
NOGOCHK  STM   @14,@12,12(@13)                                     0121
*   DCL                                                            0122
*     NOGRC FIXED(15);              /*                       @Z67SP00*/
*   DCL                                                            0123
*     NOGNDX FIXED(15);             /*                       @Z67SP00*/
*   RFY                                                            0124
*     ICTMOD BASED(CCAICMOD);       /*                       @Z67SP00*/
*   RFY                                                            0125
*     ICTPTF BASED(ICTPPTR(NOGNDX));/*                       @Z67SP00*/
*   NOGRC=ZERO;                     /* CLEAR RETURN CODE     @Z67SP00*/
         SLR   NOGRC,NOGRC                                         0126
*   DO NOGNDX=1 BY 1                /* PROCESS ALL MOD ENTRIES     0127
*                                                            @Z67SP00*/
*         WHILE(ICTMEND(NOGNDX)^=ICTENDNT);/*                @Z67SP00*/
         LA    NOGNDX,1                                            0127
         B     @DE00127                                            0127
@DL00127 DS    0H                                                  0128
*     IF ICTPNOGO(1)=ON THEN        /* SYSMOD IS NOGO?       @Z67SP00*/
         LR    @10,NOGNDX                                          0128
         MH    @10,@CH00939                                        0128
         L     @15,CCAICMOD(,CCAPTR)                               0128
         LA    @01,0(@10,@15)                                      0128
         AL    @01,@CF00948                                        0128
         L     @01,ICTPPTR-96(,@01)                                0128
         TM    ICTPNOGO(@01),B'01000000'                           0128
         BNO   @RF00128                                            0128
*       DO;                         /* YES,                  @Z67SP00*/
*         ICTMNOGO(NOGNDX)=ON;      /* MARK THE MOD 'NOGO'   @Z67SP00*/
*         ICTMMSG(NOGNDX)=ON;       /* ... MESSAGE ISSUED    @Z67SP00*/
*         ICTMCPL(NOGNDX)=ON;       /* ... COMPLETE          @Z67SP00*/
         ALR   @15,@10                                             0132
         AL    @15,@CF00951                                        0132
         OI    ICTMNOGO-8(@15),B'01110000'                         0132
*       END;                        /*                       @Z67SP00*/
*     ELSE                                                         0134
*       ;                           /* SYSMOD NOT NOGO       @Z67SP00*/
@RF00128 DS    0H                                                  0135
*   END;                            /*                       @Z67SP00*/
         AH    NOGNDX,@CH00061                                     0135
@DE00127 LR    @10,NOGNDX                                          0135
         MH    @10,@CH00939                                        0135
         L     @01,CCAICMOD(,CCAPTR)                               0135
         AL    @01,@CF00947                                        0135
         L     @10,ICTMEND(@10,@01)                                0135
         CL    @10,ICTENDNT                                        0135
         BNE   @DL00127                                            0135
*   RETURN CODE(NOGRC);             /*                       @Z67SP00*/
         LR    @15,NOGRC                                           0136
         L     @14,12(,@13)                                        0136
         LM    @00,@12,20(@13)                                     0136
         BR    @14                                                 0136
*   RFY                                                            0137
*     ICTMOD BASED;                 /*                       @Z67SP00*/
*   RFY                                                            0138
*     ICTPTF BASED;                 /*                       @Z67SP00*/
*   END NOGOCHK;                    /*                       @Z67SP00*/
         EJECT
*                                                                  0140
*   /*****************************************************************/
*   /*                                                               */
*   /* MODCHK - MARK MOD NOGO IF ITS SYSMOD IS NOGO          @Z67SP00*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0140
*MODCHK:                                                           0140
*   PROC(MODCENT);                  /*                       @Z67SP00*/
MODCHK   STM   @14,@12,12(@13)                                     0140
         MVC   @PC00003(4),0(@01)                                  0140
*   DCL                                                            0141
*     MODCENT CHAR(*);                                             0141
*   DCL                                                            0142
*     MODCRC FIXED(15);             /*                       @Z67SP00*/
*   DCL                                                            0143
*     MODCNDX FIXED(15);            /*                       @Z67SP00*/
*   DCL                                                            0144
*     MODCSKIP CONSTANT(4);         /* SKIP MOD RETURN CODE  @Z67SP00*/
*   RFY                                                            0145
*     ICTMOD BASED(ADDR(MODCENT));  /*                       @Z67SP00*/
*   RFY                                                            0146
*     ICTPTF BASED(ICTPPTR(MODCNDX));/*                      @Z67SP00*/
*   MODCRC=ZERO;                    /* ASSUME LMOD TO BE CREATED     */
         SLR   MODCRC,MODCRC                                       0147
*                                                                  0148
*   /*****************************************************************/
*   /*                                                               */
*   /* SKIP MOD IF NOT SELECTED OR NOGO                      @Z67SP00*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0148
*   IF ICTMNOGO(1)=ON×              /* NOGO ...              @Z67SP00*/
*       ICTMISEL(1)=OFF THEN        /* OR NOT SELECTED?      @Z67SP00*/
         L     @10,@PC00003                                        0148
         TM    ICTMNOGO(@10),B'01000000'                           0148
         BO    @RT00148                                            0148
         TM    ICTMISEL(@10),B'10000000'                           0148
         BNZ   @RF00148                                            0148
@RT00148 DS    0H                                                  0149
*     DO;                           /*                       @Z67SP00*/
*       MODCRC=MODCSKIP;            /* SKIP THIS MOD         @Z67SP00*/
         LA    MODCRC,4                                            0150
*     END;                          /*                       @Z67SP00*/
*                                                                  0151
*   /*****************************************************************/
*   /*                                                               */
*   /* SKIP LMOD PROCESSING IF ELEMENT DOES NOT CREATE A LOAD MODULE */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0152
*   IF ICTMMACR(1)=ON               /* IF MAC REP            @Z67SP00*/
*       ×ICTMMACU(1)=ON             /* OR MAC UPDATE         @Z67SP00*/
*       ×ICTMSRCR(1)=ON             /* OR SRC REP            @Z67SP00*/
*       ×ICTMSRCU(1)=ON             /* OR SRC UPDATE         @Z67SP00*/
*     THEN                          /*                       @ZD00009*/
@RF00148 L     @10,@PC00003                                        0152
         TM    ICTMMACR(@10),B'00001111'                           0152
         BZ    @RF00152                                            0152
*     DO;                           /*                       @Z67SP00*/
*       MODCRC=MODCSKIP;            /* SKIP THIS MOD         @Z67SP00*/
         LA    MODCRC,4                                            0154
*     END;                          /*                       @Z67SP00*/
*                                                                  0155
*   /*****************************************************************/
*   /*                                                               */
*   /* SKIP IF THE MOD IS COMPLETE                           @Z67SP00*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0156
*   IF(ICTMPROC(1)=ON&ICTMCPL(1)=ON)/* OR INPROC AND CPL     @Z67SP00*/
*     THEN                          /*                       @Z67SP00*/
@RF00152 L     @10,@PC00003                                        0156
         TM    ICTMPROC(@10),B'10100000'                           0156
         BNO   @RF00156                                            0156
*     DO;                           /*                       @Z67SP00*/
*       MODCRC=MODCSKIP;            /* SKIP THIS MOD         @Z67SP00*/
         LA    MODCRC,4                                            0158
*     END;                          /*                       @Z67SP00*/
*   RETURN CODE(MODCRC);            /*                       @Z67SP00*/
@RF00156 LR    @15,MODCRC                                          0160
         L     @14,12(,@13)                                        0160
         LM    @00,@12,20(@13)                                     0160
         BR    @14                                                 0160
*   RFY                                                            0161
*     ICTMOD BASED;                 /*                       @Z67SP00*/
*   RFY                                                            0162
*     ICTPTF BASED;                 /*                       @Z67SP00*/
*   END MODCHK;                     /*                       @Z67SP00*/
         EJECT
*                                                                  0164
*   /*****************************************************************/
*   /*                                                               */
*   /* ADDLMOD - ADD/FIND LMOD FOR THE CURRENT MOD ENTRY     @Z67SP00*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0164
*ADDLMOD:                                                          0164
*   PROC(ADDMODEN,ADDMLMOD);                                       0164
ADDLMOD  STM   @14,@12,@SA00004                                    0164
         MVC   @PC00004(8),0(@01)                                  0164
*   DCL                                                            0165
*     ADDMODEN CHAR(LENGTH(ICTMOD));/* ICT MOD ENTRY         @Z67SP00*/
*   DCL                                                            0166
*     ADDMLMOD CHAR(LENGTH(ICTLMNAM));/* LMOD NAME           @Z67SP00*/
*   DCL                                                            0167
*     ADDMRC FIXED(15);             /*                       @Z67SP00*/
*   DCL                                                            0168
*     ADDMLMDX FIXED(15);           /*                       @Z67SP00*/
*   DCL                                                            0169
*     ADDMLMD@ PTR(31);             /* ADDRESS OF ICT LMOD ENTRY   0169
*                                                            @Z67SP00*/
*   DCL                                                            0170
*     1 ADDMFLGS,                   /* LOCAL CONTROL FLAGS           */
*      2 ADDMFND BIT(1),            /* FOUND IN ICT                  */
*      2 * BIT(7);                  /* UNUSED                        */
*   RFY                                                            0171
*     ICTMOD BASED(ADDR(ADDMODEN)); /*                       @Z67SP00*/
*   RFY                                                            0172
*     ICTLMOD BASED(CCAICLMD);      /*                       @Z67SP00*/
*   ADDMRC=ZERO;                    /* CLEAR RETURN CODE     @Z67SP00*/
         SLR   ADDMRC,ADDMRC                                       0173
*   ADDMFLGS=''B;                   /* CLEAR CONTROL FLAGS   @Z67SP00*/
         MVI   ADDMFLGS,X'00'                                      0174
*   DO ADDMLMDX=1 BY 1              /* PROCESS ALL MOD ENTRIES     0175
*                                                            @Z67SP00*/
*         WHILE(ICTLEND(ADDMLMDX)^=ICTENDNT/*                @Z67SP00*/
*         &ADDMFND=OFF);            /*                       @Z67SP00*/
         LA    ADDMLMDX,1                                          0175
         B     @DE00175                                            0175
@DL00175 DS    0H                                                  0176
*     IF ICTLMNAM(ADDMLMDX)=ADDMLMOD THEN                          0176
         LR    @10,ADDMLMDX                                        0176
         MH    @10,@CH00940                                        0176
         L     @15,CCAICLMD(,CCAPTR)                               0176
         L     @14,@PC00004+4                                      0176
         ST    @10,@TF00001                                        0176
         ALR   @10,@15                                             0176
         AL    @10,@CF00952                                        0176
         CLC   ICTLMNAM(8,@10),ADDMLMOD(@14)                       0176
         BNE   @RF00176                                            0176
*       DO;                         /*                       @Z67SP00*/
*         ADDMLMD@=ADDR(ICTLMOD(ADDMLMDX));/*                @Z67SP00*/
         L     @10,@TF00001                                        0178
         AL    @15,@CF00952                                        0178
