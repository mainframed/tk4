         TITLE 'HMASMTMW   WKX DATA SET LOAD PROCEDURE'
HMASMTMW CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMTMW  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(16),0(@01)                                 0001
*   HIGHTMW=0;                      /* CLEAR RETURN CODE COLLECTOR   */
         SLR   HIGHTMW,HIGHTMW                                     0183
*   TMWFLGS=''B;                    /* CLEAR LOCAL FLAGS             */
         MVI   TMWFLGS,X'00'                                       0184
*   WRKCARD=''B;                    /* CLEAR WRK DATASET INPUT BUFFER*/
         XC    WRKCARD(80),WRKCARD                                 0185
*   PTSCARD=''B;                    /* CLEAR PTS DATASET INPUT BUFFER*/
         XC    PTSCARD(80),PTSCARD                                 0186
*   IOPPTR=ADDR(LIOP);              /* POINT TO LOCAL IOP            */
         LA    IOPPTR,LIOP                                         0187
*   HMASMIOP=''B;                   /* CLEAR THE IOP                 */
         XC    HMASMIOP(68,IOPPTR),HMASMIOP(IOPPTR)                0188
*   ELEMENT=ICTMNAME(1);            /* SET UP ELEMENT NAME           */
*                                                                  0189
         L     @14,@PC00001+4                                      0189
         L     @14,MODPTR(,@14)                                    0189
         MVC   ELEMENT(8),ICTMNAME(@14)                            0189
*   /*****************************************************************/
*   /*                                                               */
*   /* ESTABLISH TYPE CODE                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0190
*   TYPE=ZERO;                      /* CLEAR TYPE VARIABLE           */
         MVI   TYPE,X'00'                                          0190
*   IF ICTMMACU(1)=ON THEN          /* IF MACRO UPDATE               */
         TM    ICTMMACU(@14),B'00000100'                           0191
         BNO   @RF00191                                            0191
*     TYPE=IOPMCU;                  /* SET UP IOP TYPE               */
         MVI   TYPE,X'04'                                          0192
*   ELSE                                                           0193
*     IF ICTMSRCU(1)=ON THEN        /* IF SOURCE UPDATE              */
         B     @RC00191                                            0193
@RF00191 L     @14,@PC00001+4                                      0193
         L     @14,MODPTR(,@14)                                    0193
         TM    ICTMSRCU(@14),B'00000001'                           0193
         BNO   @RF00193                                            0193
*       TYPE=IOPSCU;                /* SET UP IOP TYPE               */
         MVI   TYPE,X'09'                                          0194
*     ELSE                                                         0195
*       IF ICTMMOD(1)=ON THEN       /* IF MODULE REPLACEMENT         */
         B     @RC00193                                            0195
@RF00193 L     @14,@PC00001+4                                      0195
         L     @14,MODPTR(,@14)                                    0195
         TM    ICTMMOD(@14),B'10000000'                            0195
         BNO   @RF00195                                            0195
*         TYPE=IOPMOD;              /* SET UP IOP TYPE               */
         MVI   TYPE,X'05'                                          0196
*       ELSE                                                       0197
*         IF ICTMMACR(1)=ON THEN    /* IF MACRO REPLACEMENT          */
         B     @RC00195                                            0197
@RF00195 L     @14,@PC00001+4                                      0197
         L     @14,MODPTR(,@14)                                    0197
         TM    ICTMMACR(@14),B'00001000'                           0197
         BNO   @RF00197                                            0197
*           TYPE=IOPMCR;            /* SET UP IOP TYPE               */
         MVI   TYPE,X'03'                                          0198
*         ELSE                                                     0199
*           IF ICTMSRCR(1)=ON THEN  /* IF SOURCE REPLACEMENT         */
         B     @RC00197                                            0199
@RF00197 L     @14,@PC00001+4                                      0199
         L     @14,MODPTR(,@14)                                    0199
         TM    ICTMSRCR(@14),B'00000010'                           0199
         BNO   @RF00199                                            0199
*             TYPE=IOPSCR;          /* SET UP IOP TYPE               */
         MVI   TYPE,X'08'                                          0200
*           ELSE                                                   0201
*             IF ICTMXPND(1)=ON THEN/* IF EXPAND ZAP                 */
         B     @RC00199                                            0201
@RF00199 L     @14,@PC00001+4                                      0201
         L     @14,MODPTR(,@14)                                    0201
         TM    ICTMXPND(@14),B'00010000'                           0201
         BNO   @RF00201                                            0201
*               TYPE=IOPXZP;        /* SET UP IOP TYPE               */
         MVI   TYPE,X'0D'                                          0202
*             ELSE                                                 0203
*               IF ICTMZAP(1)=ON THEN/* IF ZAP                       */
         B     @RC00201                                            0203
@RF00201 L     @14,@PC00001+4                                      0203
         L     @14,MODPTR(,@14)                                    0203
         TM    ICTMZAP(@14),B'00100000'                            0203
         BNO   @RF00203                                            0203
*                 TYPE=IOPZAP;      /* SET UP IOP TYPE               */
         MVI   TYPE,X'0C'                                          0204
*               ELSE                                               0205
*                 CALL SKIPTEXT;    /* UN-KNOWN TYPE ... SKIP IT     */
         B     @RC00203                                            0205
@RF00203 BAL   @14,SKIPTEXT                                        0205
*                                                                  0206
*   /*****************************************************************/
*   /*                                                               */
*   /* MOVE ELEMENT''S IN-LINE TEXT TO A WRK DATASET MEMBER          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0206
*   IF TYPE^=ZERO                   /* VALID ICT ELEMENT TYPE        */
*     THEN                                                         0206
@RC00203 DS    0H                                                  0206
@RC00201 DS    0H                                                  0206
@RC00199 DS    0H                                                  0206
@RC00197 DS    0H                                                  0206
@RC00195 DS    0H                                                  0206
@RC00193 DS    0H                                                  0206
@RC00191 CLI   TYPE,0                                              0206
         BE    @RF00206                                            0206
*     DO;                                                          0207
*       IF TMWOPT=NOWRITE           /* CALLER SPECIFED NO WRITES TO
*                                      WRK DATASET                   */
*         THEN                                                     0208
         L     @14,@PC00001                                        0208
         CLI   TMWOPT(@14),C'N'                                    0208
         BNE   @RF00208                                            0208
*         CALL SKIPTEXT;            /* SKIP TO NEXT SMP            0209
*                                      MOD.CNTL.STMT. (OR          0209
*                                      END-OF-FILE)                  */
         BAL   @14,SKIPTEXT                                        0209
*       ELSE                                                       0210
*         DO;                                                      0210
         B     @RC00208                                            0210
@RF00208 DS    0H                                                  0211
*           IF TMWMODE=CREATE       /* CALLER SPECIFIED "CREATE" NEW
*                                      WRK DATASET MEMBER            */
*             THEN                                                 0211
         L     @14,@PC00001+12                                     0211
         CLI   TMWMODE(@14),C'C'                                   0211
         BNE   @RF00211                                            0211
*             CALL REPLACE;         /* CREATE NEW WRK DATASET MEMBER */
         BAL   @14,REPLACE                                         0212
*           ELSE                                                   0213
*             CALL MERGEWRK;        /* MERGE INPUT TEXT WITH EXISTING
*                                      WRK DATASET MEMBER            */
         B     @RC00211                                            0213
@RF00211 BAL   @14,MERGEWRK                                        0213
*         END;                                                     0214
@RC00211 DS    0H                                                  0215
*     END;                                                         0215
@RC00208 DS    0H                                                  0216
*   IF PTSEOF=ON                    /* END-OF-FILE ON PTS DATASET    */
*     THEN                                                         0216
@RF00206 TM    PTSEOF,B'10000000'                                  0216
         BNO   @RF00216                                            0216
*     HIGHTMW=MAX(HIGHTMW,CRPEOF);  /* PASS END-OF-FILE TO CALLER    */
         LA    @14,4                                               0217
         CR    HIGHTMW,@14                                         0217
         BNL   *+6
         LR    HIGHTMW,@14                                         0217
*   RETURN CODE(HIGHTMW);           /* RETURN TO CALLER              */
@RF00216 LR    @15,HIGHTMW                                         0218
         L     @13,4(,@13)                                         0218
         L     @14,12(,@13)                                        0218
         LM    @00,@12,20(@13)                                     0218
         BR    @14                                                 0218
*                                                                  0219
*/****************************************************************** */
*/*                                                                  */
*/*   MERGEWRK  - MERGE INPUT TEXT FOR MACRO AND SOURCE UPDATES      */
*/*               WITH TEXT EXISTING IN A WRK DATASET MEMBER         */
*/*                                                                  */
*/****************************************************************** */
*                                                                  0219
*MERGEWRK:                                                         0219
*   PROC;                                                          0219
*                                                                  0219
@EL00001 L     @13,4(,@13)                                         0219
@EF00001 DS    0H                                                  0219
@ER00001 LM    @14,@12,12(@13)                                     0219
         BR    @14                                                 0219
MERGEWRK STM   @14,@03,@SA00002                                    0219
         STM   @05,@12,@SA00002+24                                 0219
*   /*****************************************************************/
*   /*                                                               */
*   /* CLOSE SYSUT1 DATASET FOR MERGE                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0220
*   IOPFUNCT=IOPCLOSE;                                             0220
         MVI   IOPFUNCT(IOPPTR),X'50'                              0220
*   IOPDSID=IOPSCR1;                /* SYSUT1 - LRECL OF 80          */
         MVI   IOPDSID(IOPPTR),X'03'                               0221
*   CALL HMASMIO(HMASMIOP);         /* CLOSE SYSUT1                  */
         ST    IOPPTR,@AL00001                                     0222
         L     @15,@CV01645                                        0222
         LA    @01,@AL00001                                        0222
         BALR  @14,@15                                             0222
*                                                                  0223
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCATE WRK DATASET MEMBER                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0223
*   WRKSEQ=MAXSEQ;                  /* IMPLIES NO WRK MEMBER FOUND   */
         MVC   WRKSEQ(8),MAXSEQ                                    0223
*   WRKEOF=ON;                      /* END-OF-FILE FOR WRK MEMBER    */
*   WRKEOD=ON;                      /* END-OF-DATA FOR WRK MEMBER    */
         OI    WRKEOF,B'01010000'                                  0225
*   IOPFUNCT=IOPBLDL;                                              0226
         MVI   IOPFUNCT(IOPPTR),X'04'                              0226
*   IOPDSID=IOPWKX;                 /* WRK DATASET                   */
         MVI   IOPDSID(IOPPTR),X'28'                               0227
*   IOPTYPE=TYPE;                   /* MEMBER TYPE                   */
         MVC   IOPTYPE(1,IOPPTR),TYPE                              0228
*   IOPNAME=ELEMENT;                /* MEMBER NAME                   */
         MVC   IOPNAME(8,IOPPTR),ELEMENT                           0229
*   CALL HMASMIO(HMASMIOP);                                        0230
         ST    IOPPTR,@AL00001                                     0230
         L     @15,@CV01645                                        0230
         LA    @01,@AL00001                                        0230
         BALR  @14,@15                                             0230
*   IF IOPRETRN>IOPNOTFD            /* TERMINATING ERROR             */
*     THEN                                                         0231
         CLI   IOPRETRN(IOPPTR),4                                  0231
         BNH   @RF00231                                            0231
*     HIGHTMW=MAX(HIGHTMW,IOPRETRN);/* MAINTAIN RETURN CODE          */
         SLR   @14,@14                                             0232
         IC    @14,IOPRETRN(,IOPPTR)                               0232
         CR    HIGHTMW,@14                                         0232
         BNL   *+6
         LR    HIGHTMW,@14                                         0232
*   IF IOPRETRN=IOPGOOD             /* SUCCESSFUL LOCATE             */
*     THEN                                                         0233
@RF00231 CLI   IOPRETRN(IOPPTR),0                                  0233
         BNE   @RF00233                                            0233
*     DO;                                                          0234
*                                                                  0234
*       /*************************************************************/
*       /*                                                           */
*       /* POSITION TO WRK MEMBER                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0235
*       IOPFUNCT=IOPPOSI;                                          0235
         MVI   IOPFUNCT(IOPPTR),X'02'                              0235
*       CALL HMASMIO(HMASMIOP);                                    0236
         ST    IOPPTR,@AL00001                                     0236
         L     @15,@CV01645                                        0236
         LA    @01,@AL00001                                        0236
         BALR  @14,@15                                             0236
*       HIGHTMW=MAX(HIGHTMW,IOPRETRN);/* MAINTAIN RETURN CODE        */
         SLR   @14,@14                                             0237
         IC    @14,IOPRETRN(,IOPPTR)                               0237
         CR    HIGHTMW,@14                                         0237
         BNL   *+6
         LR    HIGHTMW,@14                                         0237
*       IF HIGHTMW<STOPPTF          /* NOT TERMINATING               */
*         THEN                                                     0238
         CH    HIGHTMW,@CH00116                                    0238
         BNL   @RF00238                                            0238
*         DO;                                                      0239
*                                                                  0239
*           /*********************************************************/
*           /*                                                       */
*           /* SUCCESSFULLY POSITIONED TO WRK DATASET MEMBER         */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0240
*           WRKEOF=OFF;             /* CLEAR END-OF-FILE FLAG        */
*           WRKEOD=OFF;             /* CLEAR END-OF-DATA FLAG        */
         NI    WRKEOF,B'10101111'                                  0241
*           WRKSEQ=''B;             /* CLEAR SEQUENCE # FIELD        */
         XC    WRKSEQ(8),WRKSEQ                                    0242
*         END;                                                     0243
*     END;                                                         0244
@RF00238 DS    0H                                                  0245
*   IF HIGHTMW<STOPPTF THEN                                        0245
@RF00233 CH    HIGHTMW,@CH00116                                    0245
         BNL   @RF00245                                            0245
*     DO;                                                          0246
*                                                                  0246
*       /*************************************************************/
*       /*                                                           */
*       /* PUT THE "./ CHANGE" STATEMENT FROM THE PTS FILE TO SYSUT1 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0247
*       CALL GETPTS;                /* FIRST PTS_RECORD ---> PTSCARD */
         BAL   @14,GETPTS                                          0247
*       PTSSEQ=' ';                 /* BLANK SEQUENCE FIELD          */
         MVI   PTSSEQ+1,C' '                                       0248
         MVC   PTSSEQ+2(6),PTSSEQ+1                                0248
         MVI   PTSSEQ,C' '                                         0248
*       IF HIGHTMW<STOPPTF          /* SUCCESSFUL READ               */
*           &PTSEOD=OFF             /* NOT AT END-OF-DATA            */
*         THEN                                                     0249
         CH    HIGHTMW,@CH00116                                    0249
         BNL   @RF00249                                            0249
         TM    PTSEOD,B'00100000'                                  0249
         BNZ   @RF00249                                            0249
*         CALL PUTUT1(PTSCARD);     /* PTSCARD ---> SYSUT1           */
         LA    @01,@AL00250                                        0250
         BAL   @14,PUTUT1                                          0250
*       DO WHILE HIGHTMW<STOPPTF    /* SUCCESSFUL READ               */
*             &PTSEOD=OFF           /* NOT AT END-OF-DATA            */
*             &PTSCRD72^=BLANK;     /* CONTINUED IEBUPDTE STATEMENT  */
@RF00249 B     @DE00251                                            0251
@DL00251 DS    0H                                                  0252
*         CALL GETPTS;              /* NEXT PTS_RECORD ---> PTSCARD  */
         BAL   @14,GETPTS                                          0252
*         PTSSEQ=' ';               /* BLANK SEQUENCE FIELD          */
         MVI   PTSSEQ+1,C' '                                       0253
         MVC   PTSSEQ+2(6),PTSSEQ+1                                0253
         MVI   PTSSEQ,C' '                                         0253
*         CALL PUTUT1(PTSCARD);     /* PTSCARD ---> SYSUT1           */
         LA    @01,@AL00254                                        0254
         BAL   @14,PUTUT1                                          0254
*       END;                                                       0255
@DE00251 CH    HIGHTMW,@CH00116                                    0255
         BNL   @DC00251                                            0255
         TM    PTSEOD,B'00100000'                                  0255
         BNZ   @DC00251                                            0255
         CLI   PTSCRD72,C' '                                       0255
         BNE   @DL00251                                            0255
@DC00251 DS    0H                                                  0256
*       CALL GETPTS;                /* GET 1ST DATA RCD ---> PTSCARD */
*                                                                  0256
         BAL   @14,GETPTS                                          0256
*       /*************************************************************/
*       /*                                                           */
*       /* MOVE OVER THE "./ CHANGE" STMT. IN THE WRK DATASET MEMBR. */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0257
*       CALL GETWRK;                /* FIRST WRK_RECORD ---> WRKCARD */
         BAL   @14,GETWRK                                          0257
*       DO WHILE HIGHTMW<STOPPTF    /* NO TERMINATING ERROR          */
*             &WRKEOD=OFF           /* NOT AT END-OF-DATA ON WRK DS  */
*             &WRKCRD72^=BLANK;     /* CONTINUED IEBUPDTE STATEMENT  */
         B     @DE00258                                            0258
@DL00258 DS    0H                                                  0259
*         CALL GETWRK;              /* NEXT WRK_RECORD ---> WRKCARD  */
         BAL   @14,GETWRK                                          0259
*       END;                                                       0260
@DE00258 CH    HIGHTMW,@CH00116                                    0260
         BNL   @DC00258                                            0260
         TM    WRKEOD,B'00010000'                                  0260
         BNZ   @DC00258                                            0260
         CLI   WRKCRD72,C' '                                       0260
         BNE   @DL00258                                            0260
@DC00258 DS    0H                                                  0261
*       CALL GETWRK;                /* 1ST WRK DATA RCD ---> WRKCARD */
         BAL   @14,GETWRK                                          0261
*     END;                                                         0262
*   IF HIGHTMW<STOPPTF THEN                                        0263
@RF00245 CH    HIGHTMW,@CH00116                                    0263
         BNL   @RF00263                                            0263
*     DO;                                                          0264
*                                                                  0264
*       /*************************************************************/
*       /*                                                           */
*       /* MERGE PTS DATA WITH WRK DATA TO SYSUT1                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0265
*       DO WHILE(PTSSEQ<MAXSEQ      /* PTS INPUT PRESENT             */
*             ×WRKSEQ<MAXSEQ)       /* WRK DATASET INPUT PRESENT     */
*             &HIGHTMW<STOPPTF;     /* NO TERMINATING ERRORS         */
*                                                                  0265
         B     @DE00265                                            0265
@DL00265 DS    0H                                                  0266
*         /***********************************************************/
*         /*                                                         */
*         /* IF PTS CARD IS EQUAL TO WRK CARD THEN PUT OUT PTS CARD  */
*         /* AND GET BOTH ANOTHER PTS AND ANOTHER WRK CARD.          */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0266
*         IF PTSSEQ=WRKSEQ THEN     /* PTS SEQ # = WRK SEQ #         */
         CLC   PTSSEQ(8),WRKSEQ                                    0266
         BNE   @RF00266                                            0266
*           DO;                                                    0267
*             CALL PUTUT1(PTSCARD); /* PTSCARD ---> SYSUT1           */
         LA    @01,@AL00268                                        0268
         BAL   @14,PUTUT1                                          0268
*             CALL GETPTS;          /* NEXT PTS_RECORD ---> PTSCARD  */
         BAL   @14,GETPTS                                          0269
*             CALL GETWRK;          /* NEXT WRK_RECORD ---> WRKCARD  */
         BAL   @14,GETWRK                                          0270
*           END;                                                   0271
*                                                                  0271
*         /***********************************************************/
*         /*                                                         */
*         /* IF PTS CARD IS LESS THAN WRK CARD THEN PUT OUT PTS CARD */
*         /* AND GET ANOTHER PTS.                                    */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0272
*         IF PTSSEQ<WRKSEQ THEN     /* PTS SEQ # < WRK SEQ #         */
@RF00266 CLC   PTSSEQ(8),WRKSEQ                                    0272
         BNL   @RF00272                                            0272
*           DO;                                                    0273
*             CALL PUTUT1(PTSCARD); /* PTSCARD ---> SYSUT1           */
         LA    @01,@AL00274                                        0274
         BAL   @14,PUTUT1                                          0274
*             CALL GETPTS;          /* NEXT PTS_RECORD ---> PTSCARD  */
         BAL   @14,GETPTS                                          0275
*           END;                                                   0276
*                                                                  0276
*         /***********************************************************/
*         /*                                                         */
*         /* IF PTS CARD IS GREATER THAN WRK CARD THEN PUT OUT WRK   */
*         /* CARD AND GET ANOTHER WRK.                               */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0277
*         IF PTSSEQ>WRKSEQ THEN     /* PTS SEQ # > WRK SEQ #         */
@RF00272 CLC   PTSSEQ(8),WRKSEQ                                    0277
         BNH   @RF00277                                            0277
*           DO;                                                    0278
*             CALL PUTUT1(WRKCARD); /* WRKCARD ---> SYSUT1           */
         LA    @01,@AL00279                                        0279
         BAL   @14,PUTUT1                                          0279
*             CALL GETWRK;          /* NEXT WRK_RECORD ---> WRKCARD  */
         BAL   @14,GETWRK                                          0280
*           END;                                                   0281
*       END;                                                       0282
@RF00277 DS    0H                                                  0282
@DE00265 CLC   PTSSEQ(8),MAXSEQ                                    0282
         BL    @GL00006                                            0282
         CLC   WRKSEQ(8),MAXSEQ                                    0282
         BNL   @DC00265                                            0282
@GL00006 CH    HIGHTMW,@CH00116                                    0282
         BL    @DL00265                                            0282
@DC00265 DS    0H                                                  0283
*                                                                  0283
*       /*************************************************************/
*       /*                                                           */
*       /* IF WRKCARD CONTAINS A "./" IT MUST BE AN ALIAS ... WRITE  */
*       /* IT AND ANY OTHER ALIAS STATEMENTS TO SYSUT1               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0283
*       DO WHILE WRKCARD(1:2)=DOTSLASH/* AN ALIAS STATEMENT          */
*             &HIGHTMW<STOPPTF;     /* NOT TERMINATING               */
         B     @DE00283                                            0283
@DL00283 DS    0H                                                  0284
*         WRKSEQ=' ';               /* BLANK SEQUENCE FIELD          */
         MVI   WRKSEQ+1,C' '                                       0284
         MVC   WRKSEQ+2(6),WRKSEQ+1                                0284
         MVI   WRKSEQ,C' '                                         0284
*         CALL PUTUT1(WRKCARD);     /* WRKCARD ---> SYSUT1           */
         LA    @01,@AL00285                                        0285
         BAL   @14,PUTUT1                                          0285
*         CALL GETWRK;              /* NEXT WRK_RECORD ---> WRKCARD  */
         BAL   @14,GETWRK                                          0286
*       END;                                                       0287
*                                                                  0287
@DE00283 CLC   WRKCARD(2),@CC01665                                 0287
         BNE   @DC00283                                            0287
         CH    HIGHTMW,@CH00116                                    0287
         BL    @DL00283                                            0287
@DC00283 DS    0H                                                  0288
*       /*************************************************************/
*       /*                                                           */
*       /* IF PTSCARD CONTAINS A "./" IT MUST BE AN ENDUPD ... MOVE  */
*       /* OVER IT                                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0288
*       DO WHILE PTSCARD(1:2)=DOTSLASH/* AN ENDUPD STATEMENT         */
*             &HIGHTMW<STOPPTF;     /* NOT TERMINATING               */
         B     @DE00288                                            0288
@DL00288 DS    0H                                                  0289
*         CALL GETPTS;              /* NEXT PTS_RECORD ---> PTSCARD  */
         BAL   @14,GETPTS                                          0289
*       END;                                                       0290
*                                                                  0290
@DE00288 CLC   PTSCARD(2),@CC01665                                 0290
         BNE   @DC00288                                            0290
         CH    HIGHTMW,@CH00116                                    0290
         BL    @DL00288                                            0290
@DC00288 DS    0H                                                  0291
*       /*************************************************************/
*       /*                                                           */
*       /* PUT NEW "./ ALIAS" STATEMENTS INTO SYSUT1 DATASET         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0291
*       DO KK=1 BY 1 WHILE ICTTALND(KK)^=ICTENDIX/* NOT AT END       */
*             &HIGHTMW<STOPPTF;     /* NOT TERMINATING               */
         LA    KK,1                                                0291
         B     @DE00291                                            0291
@DL00291 DS    0H                                                  0292
*         ALIASNM=ICTTALNM(KK);     /* PUT ALIAS NAME IN CARD IMAGE  */
         LR    @14,KK                                              0292
         SLA   @14,3                                               0292
         L     @10,@PC00001+4                                      0292
         L     @10,MODPTR(,@10)                                    0292
         L     @10,ICTMTCHN(,@10)                                  0292
         ALR   @10,@14                                             0292
         AL    @10,@CF01756                                        0292
         MVC   ALIASNM(8),ICTTALNM(@10)                            0292
*         CALL PUTUT1(ALIASCRD);    /* WRITE CARD IMAGE TO SYSUT1    */
         LA    @01,@AL00293                                        0293
