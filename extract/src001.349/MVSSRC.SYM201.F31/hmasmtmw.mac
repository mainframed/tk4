*   L=LENGTH(PTSCARD);              /* GET THE LENGTH                */
         LA    L,80                                                0402
*   DO WHILE L>LENGTH(EXPAND)&      /* SCAN UNTIL END OF DATA AREA   */
*         CARDDATA(1:1)=BLANK;      /* OR NON BLANK CHARACTER FOUND  */
         B     @DE00403                                            0403
@DL00403 DS    0H                                                  0404
*     P=P+1;                        /* POINT TO THE NEXT COLUMN      */
         AH    P,@CH00040                                          0404
*     L=L-1;                        /* SUBTRACT FROM LENGTH          */
         BCTR  L,0                                                 0405
*   END;                                                           0406
