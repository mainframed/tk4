         TITLE 'HMASMRJD REJECT DELETE ROUTINE'
HMASMRJD CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMRJD  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(12),0(@01)                                 0001
*                                      DELETED                       */
         EJECT
*                                                                  0149
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZATION AND SETUP FOR MAINLINE CODE                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0149
*   RJDRTNCD=ZERO;                  /* INITIALIZE RETURN CODE        */
         SLR   RJDRTNCD,RJDRTNCD                                   0149
*   IOPPTR=ADDR(RJDIOP);            /* INITIALIZE IOPPTR TO LOCAL IOP*/
         LA    IOPPTR,RJDIOP                                       0150
*   HMASMIOP=''B;                   /* CLEAR THE IOP                 */
         XC    HMASMIOP(68,IOPPTR),HMASMIOP(IOPPTR)                0151
*   DSPREFIX=RLFDSPRE;              /* SAVE DATASET NAME PREFIX      */
         L     @10,@PC00001+4                                      0152
         MVC   DSPREFIX(26),RLFDSPRE(@10)                          0152
*   RELFILE#=NORLFS;                /* SAVE # OF RELFILES TO DELETE  */
*                                                                  0153
         L     @10,@PC00001+8                                      0153
         MVC   RELFILE#(2),NORLFS(@10)                             0153
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL RJDRLFD TO SCRATCH ANY LOADED RELFILE DATASETS           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0154
*   CALL PTSLOC;                    /* GET PTS SYSMOD ENTRY          */
         BAL   @14,PTSLOC                                          0154
*   IF RELFILE#^=ZERO               /* RELFILES WERE LOADED AT REC   */
*     THEN                                                         0155
         ICM   @10,3,RELFILE#                                      0155
         BZ    @RF00155                                            0155
*     DO;                                                          0156
*       CALL RJDRLFD;               /* PROCEDURE TO CREATE AND DELETE
*                                      ANY LOADED RELFILES           */
         BAL   @14,RJDRLFD                                         0157
*       RFY                                                        0158
*         RTNCODE RSTD;             /* CHECK THE RETURN CODE FROM    */
*       RJDRTNCD=MAX(RJDRTNCD,RTNCODE);/* RJDRLFD AND SET IT IN      */
         CR    RJDRTNCD,RTNCODE                                    0159
         BNL   *+6
         LR    RJDRTNCD,RTNCODE                                    0159
*       RFY                                                        0160
*         RTNCODE UNRSTD;           /* RJDRTNCD                      */
*     END;                                                         0161
*                                                                  0161
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL PROCEDURE TO DELETE ANY PTS CONTROL ENTRIES              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0162
*   CALL RJDPTSD;                   /* PROCEDURE TO DELETE THE PTS 0162
*                                      CONTROL ENTRY                 */
@RF00155 BAL   @14,RJDPTSD                                         0162
*   RFY                                                            0163
*     RTNCODE RSTD;                 /* CHECK THE RETURN CODE FROM    */
*   RJDRTNCD=MAX(RJDRTNCD,RTNCODE); /* RJDPTSD AND SET IT IN         */
         CR    RJDRTNCD,RTNCODE                                    0164
         BNL   *+6
         LR    RJDRTNCD,RTNCODE                                    0164
*   RFY                                                            0165
*     RTNCODE UNRSTD;               /* RJDRTNCD                      */
*                                                                  0165
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO CALLER WITH THE HIGHEST RETURN CODE ENCOUNTERED     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0166
*   RETURN CODE(RJDRTNCD);          /* REGISTER 15 RETURN CODE       */
         LR    @15,RJDRTNCD                                        0166
         L     @13,4(,@13)                                         0166
         L     @14,12(,@13)                                        0166
         LM    @00,@12,20(@13)                                     0166
         BR    @14                                                 0166
         EJECT
*                                                                  0167
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS IS THE RELFILE DELETE ROUTINE. IT WILL ENTERED ONLY IF   */
*   /* THE NUMBER OF RELFILES PASSED TO THIS MODULE IS NOT ZERO. IT  */
*   /* WILL LOOP FROM 1 TO THE NUMBER OF FILES PASSED AND FOR EACH   */
*   /* POSSIBLE FILE PASSED IT WILL DO: (A) CALL HMASMSUB TO BUILD   */
*   /* THE RELFILE DATASET NAME (B) CALL HMASMIO TO SCRATCH THE      */
*   /* DATASET (C) EXIT IF THE RETURN CODE IS BAD (D) CONTINUE ON    */
*   /* WHEN ALL MEMBERS HAS BEEN PROCESSED                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0167
*RJDRLFD:                                                          0167
*   PROCEDURE;                                                     0167
RJDRLFD  STM   @14,@12,@SA00002                                    0167
*   DECLARE                         /*                               */
*     I FIXED(31);                  /* RELFILE NUMBER INDEX          */
*   DECLARE                         /*                               */
*     RLFDELRC FIXED(8);            /* RETURN CODE AREA FOR PROCEDURE*/
*   RLFDELRC=ZERO;                  /* CLEAR OUT THE RETURN CODE     */
         SLR   RLFDELRC,RLFDELRC                                   0170
*   HMASMSPL=''B;                   /* CLEAR THE SUP PARM            */
*                                                                  0171
         XC    HMASMSPL(5),HMASMSPL                                0171
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP THRU PROCESSING ALL RELFILES                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0172
*   DO I=1 BY 1 TO RELFILE# WHILE(  /* LOOP TILL SERIOUS ERROR       */
*         RLFDELRC<STOPFUNC);       /*                               */
*                                                                  0172
         LA    I,1                                                 0172
         B     @DE00172                                            0172
@DL00172 CH    RLFDELRC,@CH00181                                   0172
         BNL   @DC00172                                            0172
*     /***************************************************************/
*     /*                                                             */
*     /* CALL HMASMSUB TO BUILD THE RELFILE DATASET NAME FROM THE    */
*     /* PASSED PARAMETERS (THE RELFILE DATASET NAME PREFIX, THE     */
*     /* SYSMOD NUMBER,AND THE FILE NUMBER.                          */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0173
*     SPLPMAD=ADDR(BLDRLFDS);       /* ADDR OF AREA MAPPED BY SPLPDSN*/
         LA    @10,BLDRLFDS                                        0173
         ST    @10,SPLPMAD                                         0173
*     SPLIPFX=DSPREFIX;             /* LRF DSN PREFIX                */
         MVC   SPLIPFX(26,@10),DSPREFIX                            0174
*     SPLISMD=SYSMODNO;             /* SYSMOD NUMBER                 */
         L     @05,@PC00001                                        0175
         MVC   SPLISMD(7,@10),SYSMODNO(@05)                        0175
*     SPLIFIL=I;                    /* THE FILE NUMBER THAT THE LOOP
*                                      COUNTER IS POINTING TO        */
         STH   I,SPLIFIL(,@10)                                     0176
*     SPLFUNCT=SPLBDDSN;            /* BUILD RELFILE DATASET NAME    */
         MVI   SPLFUNCT,X'05'                                      0177
*     CALL HMASMSUB(HMASMSPL);      /*                               */
         L     @15,@CV01266                                        0178
         LA    @01,@AL00178                                        0178
         BALR  @14,@15                                             0178
*     RFY                                                          0179
*       RTNCODE RSTD;               /*                               */
*     RLFDELRC=RTNCODE;             /* SAVE RETURN CODE FROM RELFILE
*                                      BUILD                         */
         LR    RLFDELRC,RTNCODE                                    0180
*     RFY                                                          0181
*       RTNCODE UNRSTD;             /*                               */
*     IF RLFDELRC=ZERO THEN         /*                               */
         CH    RLFDELRC,@CH00256                                   0182
         BNE   @RF00182                                            0182
*       DO;                         /* IF THE RETURN CODE FROM     0183
*                                      RELFILE BUILD IS ZERO         */
*                                                                  0183
*         /***********************************************************/
*         /*                                                         */
*         /* CALL HMASMIO TO SCRATCH RELFILE                         */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0184
*         IOPDSID=IOPLRF;           /* DATASET ID IS SET TO IOPLRF   */
         MVI   IOPDSID(IOPPTR),X'20'                               0184
*         IOPDSN=SPLODSN;           /* THE DATASET NAME TO BE DELETED
*                                      IS THE ONE MAPPED IN SPLPDSN  */
         L     @10,SPLPMAD                                         0185
         MVC   IOPDSN(44,IOPPTR),SPLODSN(@10)                      0185
*         IOPFUNCT=IOPDEL;          /* DO AN IOPDEL FUNCTION         */
         MVI   IOPFUNCT(IOPPTR),X'0F'                              0186
*         CALL HMASMIO(HMASMIOP);   /*                               */
         ST    IOPPTR,@AL00001                                     0187
         L     @15,@CV01265                                        0187
         LA    @01,@AL00001                                        0187
         BALR  @14,@15                                             0187
*         RLFDELRC=MAX(RLFDELRC,IOPRETRN);/* SAVE RCN CODE           */
         SLR   @10,@10                                             0188
         IC    @10,IOPRETRN(,IOPPTR)                               0188
         CR    RLFDELRC,@10                                        0188
         BNL   *+6
         LR    RLFDELRC,@10                                        0188
*       END;                        /* END IO PROCESSING             */
*   END;                            /* END LOOP PROCESSING           */
@RF00182 AH    I,@CH00091                                          0190
@DE00172 CH    I,RELFILE#                                          0190
         BNH   @DL00172                                            0190
@DC00172 DS    0H                                                  0191
*   RETURN CODE(RLFDELRC);          /* RETURN WITH HIGHEST RETURN  0191
*                                      ENCOUNTERED DURING RJDRLFD  0191
*                                      PROCESSING                    */
         LR    @15,RLFDELRC                                        0191
         L     @14,@SA00002                                        0191
         LM    @00,@12,@SA00002+8                                  0191
         BR    @14                                                 0191
*   END RJDRLFD;                    /* END OF PROCEDURE              */
         EJECT
*                                                                  0193
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE WILL BE CALLED TO DELETE THE PTS MCS AND SYSMOD  */
*   /* ENTRIES                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0193
*RJDPTSD:                           /*                               */
*   PROCEDURE;                      /*                               */
RJDPTSD  STM   @14,@12,@SA00003                                    0193
*   DECLARE                         /*                               */
*     PTSDELRC FIXED(8);            /* RETURN CODE FOR RJDPTSD       */
*   PTSDELRC=ZERO;                  /* CLEAR OUT THE RETURN CODE     */
*                                                                  0195
         SLR   PTSDELRC,PTSDELRC                                   0195
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL HMASMIO TO DELETE PTS MCS ENTRY                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0196
*   IOPDSID=IOPPTS;                 /* THE DATASET ID IS THE PTS DSN */
         MVI   IOPDSID(IOPPTR),X'06'                               0196
*   IOPNAME=SYSMODNO;               /* THE SYSMOD N0. TO BE DELETED  */
         MVI   IOPNAME+7(IOPPTR),C' '                              0197
         L     @10,@PC00001                                        0197
         MVC   IOPNAME(7,IOPPTR),SYSMODNO(@10)                     0197
*   IOPTYPE=IOPSMD;                 /* THE PTS CONTROL ENTRY         */
         MVI   IOPTYPE(IOPPTR),X'0B'                               0198
*   IOPFUNCT=IOPSTOWD;              /* DO STOW DELETE OF THE ENTRY   */
         MVI   IOPFUNCT(IOPPTR),X'53'                              0199
*   CALL HMASMIO(HMASMIOP);         /*                               */
         ST    IOPPTR,@AL00001                                     0200
         L     @15,@CV01265                                        0200
         LA    @01,@AL00001                                        0200
         BALR  @14,@15                                             0200
*   PTSDELRC=IOPRETRN;              /* SAVE THE RETURN CODE FROM IO  */
*                                                                  0201
         SLR   PTSDELRC,PTSDELRC                                   0201
         IC    PTSDELRC,IOPRETRN(,IOPPTR)                          0201
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL HMASMIO TO DELETE PTS SYSMOD ENTRY                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0202
*   IOPTYPE=IOPMCS;                 /* THE PTS SYSMOD NUMBER         */
         MVI   IOPTYPE(IOPPTR),X'10'                               0202
*   CALL HMASMIO(HMASMIOP);         /*                               */
         ST    IOPPTR,@AL00001                                     0203
         L     @15,@CV01265                                        0203
         LA    @01,@AL00001                                        0203
         BALR  @14,@15                                             0203
*   PTSDELRC=MAX(PTSDELRC,IOPRETRN);/* SAVE THE RETURN CODE FROM IO  */
*                                                                  0204
         SLR   @10,@10                                             0204
         IC    @10,IOPRETRN(,IOPPTR)                               0204
         CR    PTSDELRC,@10                                        0204
         BNL   *+6
         LR    PTSDELRC,@10                                        0204
*   /*****************************************************************/
*   /*                                                               */
*   /* SETUP FOR DELETION OF FUNCTION ID FROM THE SMPPTS VIA CALL TO */
*   /* HMASMVLU, IF A FUNCTION AND RESTORE IS BEING RUN              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0205
*   IF PTSDELRC=IOPGOOD             /* NO ERRORS,                    */
*       &CCARES=ON THEN             /* AND RESTORE FUNCTION          */
         CH    PTSDELRC,@CH00256                                   0205
         BNE   @RF00205                                            0205
         TM    CCARES(CCAPTR),B'00001000'                          0205
         BNO   @RF00205                                            0205
*     DO;                           /*                               */
*       IOPPTR=CCAIOPTR;            /* USE FULL IOP                  */
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0207
*       IOPDSID=IOPPTS;             /* PTS DATASET                   */
         MVI   IOPDSID(IOPPTR),X'06'                               0208
*       IOPTYPE=IOPSYS;             /* SYSTEM ENTRY                  */
         MVI   IOPTYPE(IOPPTR),X'07'                               0209
*       IOPFUNCT=IOPLOC;            /* LOCATE FUNCTION               */
         MVI   IOPFUNCT(IOPPTR),X'03'                              0210
*       CALL HMASMIO(HMASMIOP);     /* DO LOCATE OF PTS SYSTEM ENTRY */
         ST    IOPPTR,@AL00001                                     0211
         L     @15,@CV01265                                        0211
         LA    @01,@AL00001                                        0211
         BALR  @14,@15                                             0211
*       PTSDELRC=IOPRETRN;          /* PICK UP RETURN CODE           */
         SLR   PTSDELRC,PTSDELRC                                   0212
         IC    PTSDELRC,IOPRETRN(,IOPPTR)                          0212
*       IF PTSDELRC=IOPGOOD         /* SUCCESSFULL LOCATE OF PTS SYS */
*           &IOPPTSNJ=OFF           /* AND REJECT AT RESTORE         */
*         THEN                      /*                               */
         CH    PTSDELRC,@CH00256                                   0213
         BNE   @RF00213                                            0213
         TM    IOPPTSNJ(IOPPTR),B'01000000'                        0213
         BNZ   @RF00213                                            0213
*         DO;                       /*                               */
*           HMASMVLP=''B;           /*                               */
         XC    HMASMVLP(52),HMASMVLP                               0215
*           VLPLPTR=ADDR(IOPPTNTR(1));/* IOP LIST TO BE DELETED      */
         LA    @10,IOPPTNTR(,IOPPTR)                               0216
         ST    @10,VLPLPTR                                         0216
*           VLPFMSG=OFF;            /* NO MESSAGE                    */
         NI    VLPFMSG,B'01111111'                                 0217
*           VLPDATA(1:LENGTH(IOPPTFMD))=SYSMODNO;/* FMID SYSMOD      */
         L     @10,@PC00001                                        0218
         MVC   VLPDATA(7),SYSMODNO(@10)                            0218
*           VLPTYPE=IOPPTFM;        /* FMID TYPE TO BE DELETED       */
         MVI   VLPTYPE,X'01'                                       0219
*           VLPDLEN=LENGTH(IOPPTFMD);/* LEN OF KEY IN VLPDATA        */
         MVI   VLPDLEN,X'07'                                       0220
*           VLPFUNCT=VLPDEL;        /* DELETE ELEMENT FROM LIST      */
         MVI   VLPFUNCT,X'02'                                      0221
*           VLPMTYPE=CCADSIDA(IOPPTS);/* SMPPTS                      */
         L     @10,CCADSAD(,CCAPTR)                                0222
         MVC   VLPMTYPE(8),CCADSIDA+40(@10)                        0222
*           VLPMNAME=CCATYPEA(IOPSYS);/* SYSTEM                      */
         L     @10,CCATPAD(,CCAPTR)                                0223
         MVC   VLPMNAME(8),CCATYPEA+48(@10)                        0223
*           VLPSTYPE=CCATYPEA(IOPFMD);/*                             */
         MVI   VLPSTYPE+8,C' '                                     0224
         MVC   VLPSTYPE+9(7),VLPSTYPE+8                            0224
         MVC   VLPSTYPE(8),CCATYPEA+104(@10)                       0224
*           CALL HMASMVLU(HMASMVLP);/*                               */
         L     @15,@CV01267                                        0225
         LA    @01,@AL00225                                        0225
         BALR  @14,@15                                             0225
*           IF VLPRETRN=VLPGOOD     /* IF SYSMOD WAS FMID AND DELETED*/
*             THEN                  /*                               */
         CLI   VLPRETRN,0                                          0226
         BNE   @RF00226                                            0226
*             DO;                   /*                               */
*               IOPFUNCT=IOPSTOWR;  /* REPLACE ENTRY                 */
         MVI   IOPFUNCT(IOPPTR),X'54'                              0228
*               CALL HMASMIO(HMASMIOP);/*                            */
         ST    IOPPTR,@AL00001                                     0229
         L     @15,@CV01265                                        0229
         LA    @01,@AL00001                                        0229
         BALR  @14,@15                                             0229
*               PTSDELRC=MAX(PTSDELRC,IOPRETRN);/*                   */
         SLR   @10,@10                                             0230
         IC    @10,IOPRETRN(,IOPPTR)                               0230
         CR    PTSDELRC,@10                                        0230
         BNL   *+6
         LR    PTSDELRC,@10                                        0230
*             END;                  /*                               */
*         END;                      /*                               */
@RF00226 DS    0H                                                  0233
*     END;                          /*                               */
@RF00213 DS    0H                                                  0234
*   IOPPTR=ADDR(RJDIOP);            /* RESTORE TO LOCAL IOP          */
@RF00205 LA    IOPPTR,RJDIOP                                       0234
*   RETURN CODE(PTSDELRC);          /* RETURN TO CALL WITH MAX RETURN
*                                      CODE FOUND                    */
         LR    @15,PTSDELRC                                        0235
         L     @14,@SA00003                                        0235
         LM    @00,@12,@SA00003+8                                  0235
         BR    @14                                                 0235
*   END RJDPTSD;                    /* END RJDMCSD PROCESSING        */
*                                                                  0237
*   /*****************************************************************/
*   /*                                                               */
*   /* PTSLOC - LOCATE PTS SYSMOD ENTRY FOR THE SYSMOD BEING         */
*   /* PROCESSED AND SET RELFILE# AND DSPREFIX ... APPLY, ACCEPT AND */
*   /* RESTORE PROCESSING ONLY                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0237
*PTSLOC:                                                           0237
*   PROC;                                                          0237
PTSLOC   STM   @14,@01,@SA00004                                    0237
         STM   @03,@12,@SA00004+16                                 0237
*   IOPPTR=CCAIOPTR;                /* POINT TO FULL LENGTH IOP      */
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0238
*   IF CCAAPPLY=ON                  /* APPLY PROCESSING              */
*       ×CCAACCPT=ON                /* ACCEPT PROCESSING             */
*       ×CCARES=ON                  /* RESTORE PROCESSING            */
*     THEN                                                         0239
         TM    CCAAPPLY(CCAPTR),B'01101000'                        0239
         BZ    @RF00239                                            0239
*     DO;                                                          0240
*                                                                  0240
*       /*************************************************************/
*       /*                                                           */
*       /* CLEAR RELFILE# (NUMBER OF RELFILES) AND DSPREFIX (DATASET */
*       /* NAME PREFIX) FIELDS                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0241
*       RELFILE#=0;                 /* CLEAR NO. OF RELFILES         */
         SLR   @10,@10                                             0241
         STH   @10,RELFILE#                                        0241
*       DSPREFIX=' ';               /* BLANK DATASET PREFIX          */
         MVI   DSPREFIX+1,C' '                                     0242
         MVC   DSPREFIX+2(24),DSPREFIX+1                           0242
         MVI   DSPREFIX,C' '                                       0242
*       IOPFUNCT=IOPLOC;            /* LOCATE                        */
         MVI   IOPFUNCT(IOPPTR),X'03'                              0243
*       IOPTYPE=IOPSMD;             /* ... SYSMOD ENTRY              */
         MVI   IOPTYPE(IOPPTR),X'0B'                               0244
*       IOPNAM17=SYSMODNO;          /* ... FOR SYSMOD PASSED         */
         L     @10,@PC00001                                        0245
         MVC   IOPNAM17(7,IOPPTR),SYSMODNO(@10)                    0245
*       IOPDSID=IOPPTS;             /* ... ON SMPPTS                 */
         MVI   IOPDSID(IOPPTR),X'06'                               0246
*       CALL HMASMIO(HMASMIOP);                                    0247
         ST    IOPPTR,@AL00001                                     0247
         L     @15,@CV01265                                        0247
         LA    @01,@AL00001                                        0247
         BALR  @14,@15                                             0247
*       IF IOPRETRN=IOPGOOD         /* LOCATE SUCCESSFUL             */
*         THEN                                                     0248
         CLI   IOPRETRN(IOPPTR),0                                  0248
         BNE   @RF00248                                            0248
*         DO;                                                      0249
*                                                                  0249
*           /*********************************************************/
*           /*                                                       */
*           /* SET RELFILE# AND DSPREFIX FROM PTS SYSMOD ENTRY       */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0250
*           RELFILE#=IOPPRELN;      /* NUMBER OF RELFILES            */
         MVC   RELFILE#(2),IOPPRELN(IOPPTR)                        0250
*           DSPREFIX=IOPPTPFX;      /* DATA SET NAME PREFIX          */
         MVC   DSPREFIX(26),IOPPTPFX(IOPPTR)                       0251
*         END;                                                     0252
*     END;                                                         0253
@RF00248 DS    0H                                                  0254
*   IOPPTR=ADDR(RJDIOP);            /* RESTORE PTR TO LOCAL IOP      */
@RF00239 LA    IOPPTR,RJDIOP                                       0254
*   RETURN;                                                        0255
@EL00004 DS    0H                                                  0255
@EF00004 DS    0H                                                  0255
@ER00004 LM    @14,@01,@SA00004                                    0255
         LM    @03,@12,@SA00004+16                                 0255
         BR    @14                                                 0255
*   END PTSLOC;                                                    0256
*   END HMASMRJD                    /* END RJD PROCESSING            */
*                                                                  0257
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */
*/*%INCLUDE SYSLIB  (HMASMCCA)                                       */
*/*%INCLUDE SYSLIB  (HMASMIOP)                                       */
*/*%INCLUDE SYSLIB  (HMASMSPL)                                       */
*/*%INCLUDE SYSLIB  (HMASMVLP)                                       */
*                                                                  0257
*       ;                                                          0257
@DATA    DS    0H
@CH00256 DC    H'0'
@CH00091 DC    H'1'
@CH00181 DC    H'12'
         DS    0F
@AL00178 DC    A(HMASMSPL)             LIST WITH   1 ARGUMENT(S)
@AL00225 DC    A(HMASMVLP)             LIST WITH   1 ARGUMENT(S)
         DS    0F
@SA00001 DS    18F
@PC00001 DS    3F
@SA00003 DS    0F
@SA00002 DS    0F
@SA00004 DS    0F
         DS    15F
@AL00001 DS    1A
         DS    0F
@CV01265 DC    V(HMASMIO)
@CV01266 DC    V(HMASMSUB)
@CV01267 DC    V(HMASMVLU)
         DS    0D
RELFILE# DS    FL2
         DS    CL2
HMASMSPL DS    CL5
         ORG   HMASMSPL
SPLPMAD  DS    AL4
SPLFUNCT DS    FL1
         ORG   HMASMSPL+5
         DS    CL3
HMASMVLP DS    CL52
         ORG   HMASMVLP
VLPFUNCT DS    FL1
VLPRETRN DS    FL1
VLPFLAG  DS    BL1
         ORG   VLPFLAG
VLPFMSG  DS    BL1
VLPFRSV  EQU   VLPFLAG+0
         ORG   HMASMVLP+3
VLPDLEN  DS    FL1
VLPSNTRY DS    CL9
         ORG   VLPSNTRY
VLPDATA  DS    CL8
