*/****************************************************************** */
*/*                                                                  */
*/* ACCSREQS  - ACCESS THE (A)CDS SYSMOD ENTRIES FOR ALL REQUISITES  */
*/*             FOUND IN THE TEMP GTA FILE.                          */
*/*                                                                  */
*/*           - IF THE REQUISITE IS FOUND ON THE (A)CDS AND IT       */
*/*             IS                                                   */
*/*                                                                  */
*/*               (1) NOT "IN ERROR" ON (A)CDS,                      */
*/*               (2) NOT DELETED ON (A)CDS,                         */
*/*               (3) NOT FOUND IN THE ICT AS DELETED,               */
*/*                                                                  */
*/*             SET ICTEREQT IN THE REQ LIST ENTRIES WHICH REFER     */
*/*             TO THE "FOUND" REQUISITE.                            */
*/*                                                                  */
*/****************************************************************** */
*                                                                  0416
*ACCSREQS:                                                         0416
*   PROC;                                                          0416
ACCSREQS STM   @14,@02,@SA00006                                    0416
         STM   @04,@12,@SA00006+20                                 0416
*                                                                  0417
*   /*****************************************************************/
*   /*                                                               */
*   /* RESTART TEMP GTA FILE CONTAINING REQUISITE SYSMODS            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0417
*   GTPFUNCT=GTPOPENN;              /* RE-START                      */
         MVI   GTPFUNCT,X'01'                                      0417
*   CALL HMASMGTA(HMASMGTP);                                       0418
         L     @15,@CV01755                                        0418
         LA    @01,@AL00418                                        0418
         BALR  @14,@15                                             0418
*   HIGHTCR=MAX(HIGHTCR,GTPRETRN);  /* MAINTAIN RETURN CODE          */
*                                                                  0419
         SLR   @10,@10                                             0419
         IC    @10,GTPRETRN                                        0419
         CR    HIGHTCR,@10                                         0419
         BNL   *+6
         LR    HIGHTCR,@10                                         0419
*   /*****************************************************************/
*   /*                                                               */
*   /* READ THE FIRST RECORD FROM GTA FILE OF REQUISITE SYSMODS      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0420
*   IF HIGHTCR<STOPFUNC THEN                                       0420
         CH    HIGHTCR,@CH00267                                    0420
         BNL   @RF00420                                            0420
*     DO;                                                          0421
*       GTPFUNCT=GTPGETN;           /* GET NEXT (FIRST) RECORD       */
         MVI   GTPFUNCT,X'08'                                      0422
*       CALL HMASMGTA(HMASMGTP);    /* GET FIRST RECORD              */
         L     @15,@CV01755                                        0423
         LA    @01,@AL00423                                        0423
         BALR  @14,@15                                             0423
*       IF GTPRETRN>GTPEOF THEN                                    0424
         CLI   GTPRETRN,4                                          0424
         BNH   @RF00424                                            0424
*         HIGHTCR=MAX(HIGHTCR,GTPRETRN);/* MAINTAIN HIGHTCR          */
         SLR   @10,@10                                             0425
         IC    @10,GTPRETRN                                        0425
         CR    HIGHTCR,@10                                         0425
         BNL   *+6
         LR    HIGHTCR,@10                                         0425
*     END;                                                         0426
@RF00424 DS    0H                                                  0427
*                                                                  0427
*   /*****************************************************************/
*   /*                                                               */
*   /* GO THRU THE ENTIRE GTA FILE OF REQUISITE SYSMODS              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0427
*   DO WHILE GTPRETRN<GTPEOF&HIGHTCR<STOPFUNC;                     0427
@RF00420 B     @DE00427                                            0427
@DL00427 DS    0H                                                  0428
*     SYSMD#=GTASYSMD;              /* SAVE THE CURRENT SYSMOD NAME  */
         MVC   SYSMD#(7),GTASYSMD                                  0428
*     CALL CHKICT(SYSMD#,CHKICTRC); /* LOOK IN ICT FOR SYSMOD        */
         LA    @01,@AL00429                                        0429
         BAL   @14,CHKICT                                          0429
*     IF CHKICTRC=NOTFND THEN                                      0430
*                                                                  0430
         CLI   CHKICTRC,C'N'                                       0430
         BNE   @RF00430                                            0430
*       /*************************************************************/
*       /*                                                           */
*       /* NOT FOUND IN ICT ... ACCESS (A)CDS FOR CURRENT SYSMOD     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0431
*       CALL CHKCDS(SYSMD#,CHKCDSRC);/* LOOK IN (A)CDS FOR SYSMOD    */
*                                                                  0431
         LA    @01,@AL00431                                        0431
         BAL   @14,CHKCDS                                          0431
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS ALL RECORDS IN THE GTA FILE FOR CURRENT SYSMOD      */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0432
*     DO WHILE SYSMD#=GTASYSMD      /* SYSMOD IN GTA IS STILL SAME   */
*           &GTPRETRN<GTPEOF        /* NOT AT END OF FILE OR ERROR   */
*           &HIGHTCR<STOPFUNC;      /* NO ERRORS ENCOUNTERED         */
@RF00430 B     @DE00432                                            0432
@DL00432 DS    0H                                                  0433
*       IF CHKICTRC=NOTFND          /* NOT FOUND IN ICT              */
*           &CHKCDSRC=FOUND         /* IF FOUND IN (A)CDS            */
*         THEN                                                     0433
         CLI   CHKICTRC,C'N'                                       0433
         BNE   @RF00433                                            0433
         CLI   CHKCDSRC,C'F'                                       0433
         BNE   @RF00433                                            0433
*         GTALSTPT->ICTEREQT(1)=ON; /* REQ ON TARGET SYSTEM FLG      */
         MVC   @TF00001(4),GTALSTPT                                0434
         L     @10,@TF00001                                        0434
         OI    ICTEREQT(@10),B'00000010'                           0434
*       CALL HMASMGTA(HMASMGTP);    /* ACCESS NEXT GTA RECORD        */
@RF00433 L     @15,@CV01755                                        0435
         LA    @01,@AL00435                                        0435
         BALR  @14,@15                                             0435
*       IF GTPRETRN>GTPEOF THEN                                    0436
         CLI   GTPRETRN,4                                          0436
         BNH   @RF00436                                            0436
*         HIGHTCR=MAX(HIGHTCR,GTPRETRN);/* MAINTAIN HIGHTCR          */
         SLR   @10,@10                                             0437
         IC    @10,GTPRETRN                                        0437
         CR    HIGHTCR,@10                                         0437
         BNL   *+6
         LR    HIGHTCR,@10                                         0437
*     END;                                                         0438
@RF00436 DS    0H                                                  0438
@DE00432 CLC   SYSMD#(7),GTASYSMD                                  0438
         BNE   @DC00432                                            0438
         CLI   GTPRETRN,4                                          0438
         BNL   @DC00432                                            0438
         CH    HIGHTCR,@CH00267                                    0438
         BL    @DL00432                                            0438
@DC00432 DS    0H                                                  0439
*   END;                            /* END OF LOOP THRU ENTIRE GTA 0439
*                                      FILE                          */
@DE00427 CLI   GTPRETRN,4                                          0439
         BNL   @DC00427                                            0439
         CH    HIGHTCR,@CH00267                                    0439
         BL    @DL00427                                            0439
@DC00427 DS    0H                                                  0440
*   RETURN;                                                        0440
@EL00006 DS    0H                                                  0440
@EF00006 DS    0H                                                  0440
@ER00006 LM    @14,@02,@SA00006                                    0440
         LM    @04,@12,@SA00006+20                                 0440
         BR    @14                                                 0440
*   END ACCSREQS;                                                  0441
*                                                                  0442
*/****************************************************************** */
*/*                                                                  */
*/* CHKICT  - CHECK THE ICT PTF SECTION TO DETERMINE WHETHER THE     */
*/*           SYSMOD PASSED IS IN THE ICT.                           */
*/*                                                                  */
*/*           RETURNS:                                               */
*/*                                                                  */
*/*             'N'  - NOT FOUND IN ICT                              */
*/*             'F'  - FOUND IN ICT ... NOT DELETED                  */
*/*             'D'  - FOUND IN ICT ... DELETED                      */
*/*                                                                  */
*/****************************************************************** */
*                                                                  0442
*CHKICT:                                                           0442
*   PROC(SMD#ICT,CHKIRTN);                                         0442
CHKICT   STM   @14,@12,12(@13)                                     0442
         MVC   @PC00007(8),0(@01)                                  0442
*   DCL                                                            0443
*     SMD#ICT CHAR(7);              /* SYSMOD NAME                   */
*   DCL                                                            0444
*     CHKIRTN CHAR(1);              /* RETURNS: ....................
*                                      'N' SYSMOD NOT FOUND IN ICT .
*                                      'D' SYSMOD FOUND IN ICT .....
*                                      ---> DELETED ................
*                                      'F' SYSMOD FOUND IN ICT .....
*                                      ---> NOT DELETED ............ */
*   DCL                                                            0445
*     KK FIXED(15);                 /* LOCAL INDEX - PTF SECT ENTRIES*/
*   RESPECIFY                                                      0446
*     ICTPTF BASED(CCAICPTF);       /* ADDRESS ENTIRE PTF SECTION    */
*   CHKIRTN=NOTFND;                 /* INITIALIZE RETURN PARM        */
         L     @10,@PC00007+4                                      0447
         MVI   CHKIRTN(@10),C'N'                                   0447
*   DO KK=1 BY 1 WHILE ICTPEND(KK)^=ICTENDNT&CHKIRTN=NOTFND;       0448
         LA    KK,1                                                0448
         B     @DE00448                                            0448
@DL00448 DS    0H                                                  0449
*     IF ICTPTFS(KK)=SMD#ICT        /* MATCH CALLER''S SYSMOD ?      */
*       THEN                                                       0449
         LR    @10,KK                                              0449
         MH    @10,@CH01886                                        0449
         L     @03,CCAICPTF(,CCAPTR)                               0449
         L     @15,@PC00007                                        0449
         ST    @10,@TF00001                                        0449
         ALR   @10,@03                                             0449
         AL    @10,@CF01902                                        0449
         CLC   ICTPTFS(7,@10),SMD#ICT(@15)                         0449
         BNE   @RF00449                                            0449
*       DO;                                                        0450
*                                                                  0450
*         /***********************************************************/
*         /*                                                         */
*         /* SYSMOD FOUND IN ICT PTF SECTION                         */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0451
*         CHKIRTN=FOUND;            /* SYSMOD AT LEAST FOUND         */
         L     @10,@PC00007+4                                      0451
         MVI   CHKIRTN(@10),C'F'                                   0451
*         IF ICTPDEXP(KK)=ON        /* EXPLICITLY DELETED ?          */
*             ×ICTPDIMP(KK)=ON      /* IMPLICITLY DELETED ?          */
*           THEN                                                   0452
         AL    @03,@TF00001                                        0452
         AL    @03,@CF01900                                        0452
         TM    ICTPDEXP-13(@03),B'00110000'                        0452
         BZ    @RF00452                                            0452
*           CHKIRTN=DELTD;          /* RETURN "DELETED"              */
         MVI   CHKIRTN(@10),C'D'                                   0453
*       END;                                                       0454
@RF00452 DS    0H                                                  0455
*   END;                                                           0455
@RF00449 AH    KK,@CH00064                                         0455
@DE00448 LR    @10,KK                                              0455
         MH    @10,@CH01886                                        0455
         L     @03,CCAICPTF(,CCAPTR)                               0455
         ALR   @03,@10                                             0455
         AL    @03,@CF01902                                        0455
         MVC   @TF00001(4),ICTPEND(@03)                            0455
         L     @10,@TF00001                                        0455
         CL    @10,ICTENDNT                                        0455
         BE    @DC00448                                            0455
         L     @10,@PC00007+4                                      0455
         CLI   CHKIRTN(@10),C'N'                                   0455
         BE    @DL00448                                            0455
@DC00448 DS    0H                                                  0456
*   RETURN;                                                        0456
@EL00007 DS    0H                                                  0456
@EF00007 DS    0H                                                  0456
@ER00007 LM    @14,@12,12(@13)                                     0456
         BR    @14                                                 0456
*   END CHKICT;                                                    0457
*                                                                  0458
*/****************************************************************** */
*/*                                                                  */
*/* CHKCDS   - ACCESS THE (A)CDS USING THE LOCAL IOP (POINTED TO     */
*/*            BY IOPPTR ON ENTRY TO THIS PROCEDURE).                */
*/*                                                                  */
*/*            RETURNS:                                              */
*/*                                                                  */
*/*              'F' - "FOUND"                                       */
*/*                                                                  */
*/*                    1. THE (A)CDS SYSMOD ENTRY FOUND AND          */
*/*                       NONE OF THE "NOT FOUND" CONDITIONS         */
*/*                       LISTED BELOW ARE TRUE.                     */
*/*                                                                  */
*/*              'N' - "NOT FOUND"                                   */
*/*                                                                  */
*/*                    1. THE (A)CDS SYSMOD ENTRY NOT FOUND.         */
*/*                                                                  */
*/*                    2. THE (A)CDS SYSMOD ENTRY FOUND WITH         */
*/*                       IOPERROR SET.                              */
*/*                                                                  */
*/*                    3. THE (A)CDS SYSMOD ENTRY FOUND AS A         */
*/*                       DELETED SYSMOD ENTRY.                      */
*/*                                                                  */
*/*                    4. THE (A)CDS SYSMOD ENTRY FOUND AS A         */
*/*                       "DUMMY" SUP AND THE SUPERSEDING SYSMOD     */
*/*                       IS (1) "IN ERROR", (2) DELETED OR (3)      */
*/*                       NOT FOUND.                                 */
*/*                                                                  */
*/*          - NOTE: IF I/O ERRORS ARE ENCOUNTERED, HIGHTCR WILL     */
*/*                  BE SET TO THE RETURN CODE FROM HMASMIO.         */
*/*                                                                  */
*/****************************************************************** */
*                                                                  0458
*CHKCDS:                                                           0458
*   PROC(SMD#CDS,CHKCANS);                                         0458
CHKCDS   STM   @14,@02,@SA00008                                    0458
         STM   @04,@12,@SA00008+20                                 0458
         MVC   @PC00008(8),0(@01)                                  0458
*   DCL                                                            0459
*     SMD#CDS CHAR(7);              /* SYSMOD TO BE FOUND            */
*   DCL                                                            0460
*     CHKCANS CHAR(1);              /* RETURNS "FOUND" OR "NOTFND"   */
*   RESPECIFY                                                      0461
*     HMASMIOP BASED(IOPPTR);       /* ADDRESS IOP                   */
*   CHKCANS=NOTFND;                 /* INITIALIZE RETURN PARM        */
*                                                                  0462
         L     @10,@PC00008+4                                      0462
         MVI   CHKCANS(@10),C'N'                                   0462
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE IOP TO ACCESS (A)CDS SYSMOD ENTRY                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0463
*   IOPPTR=ADDR(LOCALIOP);          /* POINT TO LOCAL "SMALL" IOP    */
         LA    IOPPTR,LOCALIOP                                     0463
*   IOPDSID=TBLXCDSI;               /* APPLY => CDS, ACCEPT => ACDS  */
         L     @10,@PC00001                                        0464
         MVC   IOPDSID(1,IOPPTR),TBLXCDSI(@10)                     0464
*   IOPFUNCT=IOPBLDL;               /* BLDL FUNCTION                 */
         MVI   IOPFUNCT(IOPPTR),X'04'                              0465
*   IOPTYPE=IOPSMD;                 /* SYSMOD ENTIRES                */
         MVI   IOPTYPE(IOPPTR),X'0B'                               0466
*   IOPNAME=' ';                    /* CLEAR ENTRY NAME FIELD        */
         MVI   IOPNAME+1(IOPPTR),C' '                              0467
         MVC   IOPNAME+2(6,IOPPTR),IOPNAME+1(IOPPTR)               0467
         MVI   IOPNAME(IOPPTR),C' '                                0467
*   IOPNAM17=SMD#CDS;               /* FILL IN SYSMOD-ID PASSED      */
*                                                                  0468
         L     @10,@PC00008                                        0468
         MVC   IOPNAM17(7,IOPPTR),SMD#CDS(@10)                     0468
*   /*****************************************************************/
*   /*                                                               */
*   /* ACCESS (A)CDS                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0469
*   CALL HMASMIO(HMASMIOP);         /* ACCESS (A)CDS                 */
         ST    IOPPTR,@AL00001                                     0469
         L     @15,@CV01753                                        0469
         LA    @01,@AL00001                                        0469
         BALR  @14,@15                                             0469
*   IF IOPRETRN=IOPNOTFD            /* ENTRY NOT FOUND ?             */
*     THEN                                                         0470
         CLI   IOPRETRN(IOPPTR),4                                  0470
         BNE   @RF00470                                            0470
*     DO;                                                          0471
*       CHKCANS=NOTFND;             /* RETURN NOT FOUND TO CALLER    */
         L     @10,@PC00008+4                                      0472
         MVI   CHKCANS(@10),C'N'                                   0472
*     END;                                                         0473
*   IF IOPRETRN>IOPNOTFD            /* ACCESS ERROR ?                */
*     THEN                                                         0474
@RF00470 CLI   IOPRETRN(IOPPTR),4                                  0474
         BNH   @RF00474                                            0474
*     DO;                                                          0475
*       CHKCANS=NOTFND;             /* RETURN NOT FOUND TO CALLER    */
         L     @10,@PC00008+4                                      0476
         MVI   CHKCANS(@10),C'N'                                   0476
*       HIGHTCR=MAX(HIGHTCR,IOPRETRN);/* MAINTAIN HIGHTCR            */
         SLR   @10,@10                                             0477
         IC    @10,IOPRETRN(,IOPPTR)                               0477
         CR    HIGHTCR,@10                                         0477
         BNL   *+6
         LR    HIGHTCR,@10                                         0477
*     END;                                                         0478
*   IF IOPRETRN=IOPGOOD             /* SUCCESSFUL ACCESS ?           */
*     THEN                                                         0479
@RF00474 CLI   IOPRETRN(IOPPTR),0                                  0479
         BNE   @RF00479                                            0479
*     DO;                                                          0480
