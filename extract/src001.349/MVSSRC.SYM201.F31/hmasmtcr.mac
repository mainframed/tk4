         BNZ   @RF00226                                            0226
         LR    @10,@15                                             0226
         AL    @10,@TF00001                                        0226
         AL    @10,@CF01900                                        0226
         TM    ICTPDEXP-13(@10),B'00111000'                        0226
         BNZ   @RF00226                                            0226
