         MVC   @AL00001+4(8),@AL00238                              0238
         L     @10,@PC00001                                        0238
         ST    @10,@AL00001                                        0238
         L     @10,@TF00001                                        0238
         AL    @15,@CF01902                                        0238
         LA    @10,ICTPTF(@10,@15)                                 0238
         ST    @10,@TF00001                                        0238
         L     @15,@CV01756                                        0238
         LA    @01,@AL00001                                        0238
         BALR  @14,@15                                             0238
*         ICTPRQCK(I)=ON;           /* SET REQS CHECKED INDICATOR    */
*         ICTPRQOK(I)=ON;           /* CONSIDER REQUISITES SATISFIED */
         LR    @10,I                                               0240
         MH    @10,@CH01886                                        0240
         L     @15,CCAICPTF(,CCAPTR)                               0240
         ST    @10,@TF00001                                        0240
         ALR   @10,@15                                             0240
         AL    @10,@CF01901                                        0240
         OI    ICTPRQCK-15(@10),B'00001100'                        0240
*         IF TR1RETRN=BYPASSED      /* REQUISITES WERE BYPASSED      */
