*           /* TO THIS INDEX ENTRY                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0397
*           GTASYSMD=ICTPRENM(LL);  /* REQUISITE SYSMOD              */
         LR    @10,@05                                             0397
         AL    @10,@TF00001                                        0397
         AL    @10,@CF01909                                        0397
         MVC   GTASYSMD(7),ICTPRENM(@10)                           0397
*           GTALSTPT=ADDR(ICTPRELS(LL));/* PTR TO THIS INDEX ENTRY   */
         L     @10,@TF00001                                        0398
         AL    @05,@CF01909                                        0398
         LA    @10,ICTPRELS(@10,@05)                               0398
         ST    @10,@TF00001                                        0398
         MVC   GTALSTPT(4),@TF00001                                0398
*           GTPFUNCT=GTPADD;        /* SET GTA FUNCTION              */
         MVI   GTPFUNCT,X'04'                                      0399
*           CALL HMASMGTA(HMASMGTP);/* ADD RECORD TO FILE            */
         L     @15,@CV01755                                        0400
         LA    @01,@AL00400                                        0400
         BALR  @14,@15                                             0400
*           HIGHTCR=MAX(HIGHTCR,GTPRETRN);/* MAINTAIN RETURN CODE    */
         SLR   @10,@10                                             0401
         IC    @10,GTPRETRN                                        0401
         CR    HIGHTCR,@10                                         0401
         BNL   *+6
         LR    HIGHTCR,@10                                         0401
*         END;                                                     0402
*                                                                  0402
         AH    LL,@CH00064                                         0402
@DE00394 LR    @10,LL                                              0402
         SLA   @10,3                                               0402
         LR    @05,PP                                              0402
         MH    @05,@CH01886                                        0402
         L     @01,CCAICPTF(,CCAPTR)                               0402
         ALR   @01,@05                                             0402
         AL    @01,@CF01913                                        0402
         MVC   @TF00001(4),ICTPPCHN-76(@01)                        0402
         L     @05,@TF00001                                        0402
         AL    @05,@CF01909                                        0402
         L     @10,ICTPREND(@10,@05)                               0402
         CL    @10,ICTENDIX                                        0402
         BE    @DC00394                                            0402
         CH    HIGHTCR,@CH00267                                    0402
         BL    @DL00394                                            0402
@DC00394 DS    0H                                                  0403
*         /***********************************************************/
*         /*                                                         */
*         /* PUT THE IFREQS FOR THIS SYSMOD IN GTA FILE              */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0403
*         DO LL=1 BY 1 WHILE ICTIRQND(LL)^=ICTENDIX&HIGHTCR<STOPFUNC;
*                                                                  0403
         LA    LL,1                                                0403
         B     @DE00403                                            0403
@DL00403 DS    0H                                                  0404
*           /*********************************************************/
*           /*                                                       */
*           /* CLEAR SOME STATUS BITS IN THE IFREQ LIST              */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0404
*           ICTIREQS(LL)=OFF;       /* REQ SATISFIED INDICATOR       */
*           ICTIREQT(LL)=OFF;       /* REQ ON TARGET SYSTEM INDICATOR*/
*                                                                  0405
         LR    @10,LL                                              0405
         SLA   @10,4                                               0405
         LR    @05,PP                                              0405
         MH    @05,@CH01886                                        0405
         L     @01,CCAICPTF(,CCAPTR)                               0405
         ALR   @01,@05                                             0405
         AL    @01,@CF01914                                        0405
         MVC   @TF00001(4),ICTPICHN-80(@01)                        0405
         L     @05,@TF00001                                        0405
         ST    @10,@TF00001                                        0405
         ALR   @10,@05                                             0405
         AL    @10,@CF01910                                        0405
         NI    ICTIREQS-7(@10),B'11101101'                         0405
*           /*********************************************************/
*           /*                                                       */
*           /* PUT REQUISITE SYSMOD NAME IN GTA FILE WITH A POINTER  */
*           /* TO THIS INDEX ENTRY                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0406
*           GTASYSMD=ICTIRQNM(LL);  /* REQUISITE SYSMOD              */
         LR    @10,@05                                             0406
         AL    @10,@TF00001                                        0406
         AL    @10,@CF01911                                        0406
         MVC   GTASYSMD(7),ICTIRQNM(@10)                           0406
*           GTALSTPT=ADDR(ICTIRQLS(LL));/* PTR TO THIS INDEX ENTRY   */
         L     @10,@TF00001                                        0407
         AL    @05,@CF01911                                        0407
         LA    @10,ICTIRQLS(@10,@05)                               0407
         ST    @10,@TF00001                                        0407
         MVC   GTALSTPT(4),@TF00001                                0407
*           GTPFUNCT=GTPADD;        /* SET GTA FUNCTION              */
         MVI   GTPFUNCT,X'04'                                      0408
*           CALL HMASMGTA(HMASMGTP);/* ADD RECORD TO FILE            */
         L     @15,@CV01755                                        0409
         LA    @01,@AL00409                                        0409
         BALR  @14,@15                                             0409
*           HIGHTCR=MAX(HIGHTCR,GTPRETRN);/* MAINTAIN RETURN CODE    */
         SLR   @10,@10                                             0410
         IC    @10,GTPRETRN                                        0410
         CR    HIGHTCR,@10                                         0410
         BNL   *+6
         LR    HIGHTCR,@10                                         0410
*         END;                                                     0411
         AH    LL,@CH00064                                         0411
@DE00403 LR    @10,LL                                              0411
         SLA   @10,4                                               0411
         LR    @05,PP                                              0411
         MH    @05,@CH01886                                        0411
         L     @01,CCAICPTF(,CCAPTR)                               0411
         ALR   @01,@05                                             0411
         AL    @01,@CF01914                                        0411
         MVC   @TF00001(4),ICTPICHN-80(@01)                        0411
         L     @05,@TF00001                                        0411
         AL    @05,@CF01911                                        0411
         L     @10,ICTIRQND(@10,@05)                               0411
         CL    @10,ICTENDIX                                        0411
         BE    @DC00403                                            0411
         CH    HIGHTCR,@CH00267                                    0411
         BL    @DL00403                                            0411
@DC00403 DS    0H                                                  0412
*       END;                        /* END OF PROCESSING NON-NOGO, 0412
*                                      NON-DELETED SYSMOD IN PTF   0412
*                                      SECTION                       */
*   END;                            /* END OF LOOP THRU ENTIRE PTF 0413
*                                      SECTION                       */
@RF00372 AH    PP,@CH00064                                         0413
@DE00371 LR    @10,PP                                              0413
         MH    @10,@CH01886                                        0413
         L     @01,CCAICPTF(,CCAPTR)                               0413
         ALR   @01,@10                                             0413
         AL    @01,@CF01902                                        0413
         MVC   @TF00001(4),ICTPEND(@01)                            0413
         L     @10,@TF00001                                        0413
         CL    @10,ICTENDNT                                        0413
         BE    @DC00371                                            0413
         CH    HIGHTCR,@CH00267                                    0413
         BL    @DL00371                                            0413
@DC00371 DS    0H                                                  0414
*   RETURN;                                                        0414
@EL00005 DS    0H                                                  0414
@EF00005 DS    0H                                                  0414
@ER00005 LM    @14,@02,@SA00005                                    0414
         LM    @04,@12,@SA00005+20                                 0414
         BR    @14                                                 0414
*   END BUILDGTA;                                                  0415
