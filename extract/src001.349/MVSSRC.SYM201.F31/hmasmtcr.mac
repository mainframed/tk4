*             ICTPCPL(I)=ON;        /* MARK SYSMOD AS PROCESSED      */
         LR    @10,I                                               0249
         MH    @10,@CH01886                                        0249
         L     @15,CCAICPTF(,CCAPTR)                               0249
         ST    @10,@TF00001                                        0249
         ALR   @10,@15                                             0249
         AL    @10,@CF01899                                        0249
         OI    ICTPNOGO-8(@10),B'01100000'                         0249
*             ICTPRQOK(I)=OFF;      /* REQUISITES ARE NOT SATISFIED  */
         AL    @15,@TF00001                                        0250
         AL    @15,@CF01901                                        0250
         NI    ICTPRQOK-15(@15),B'11111011'                        0250
*             CALL REQFAIL;         /* ISSUE ERROR MESSAGES          */
         BAL   @14,REQFAIL                                         0251
*             IF ICTPTYPE(I)=IOPPFUNC/* FUNCTION-TYPE SYSMOD         */
