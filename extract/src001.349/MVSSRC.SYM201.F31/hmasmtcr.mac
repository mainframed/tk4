         MH    @10,@CH01886                                        0236
         L     @15,CCAICPTF(,CCAPTR)                               0236
         ST    @10,@TF00001                                        0236
         ALR   @10,@15                                             0236
         AL    @10,@CF01899                                        0236
         TM    ICTPNOGO-8(@10),B'01000000'                         0236
         BNZ   @RF00236                                            0236
         LR    @10,@15                                             0236
         AL    @10,@TF00001                                        0236
         AL    @10,@CF01900                                        0236
         TM    ICTPDEXP-13(@10),B'00111000'                        0236
         BNZ   @RF00236                                            0236
         LR    @10,@15                                             0236
         AL    @10,@TF00001                                        0236
         AL    @10,@CF01903                                        0236
         CLC   ICTPSUPN-20(2,@10),@CH00082                         0236
