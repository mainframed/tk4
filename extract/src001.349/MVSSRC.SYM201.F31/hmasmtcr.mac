         LR    @10,I                                               0252
         MH    @10,@CH01886                                        0252
         L     @01,CCAICPTF(,CCAPTR)                               0252
         AL    @01,@CF01907                                        0252
         SLR   @00,@00                                             0252
         IC    @00,ICTPTYPE-17(@10,@01)                            0252
         CH    @00,@CH00038                                        0252
         BNE   @RF00252                                            0252
*               CALL FUNCFAIL;      /* ISSUE MSG HMA370 FOR FUNCTION */
         BAL   @14,FUNCFAIL                                        0253
*           END;                                                   0254
@RF00252 DS    0H                                                  0255
*       END;                        /* END OF PROCESSING FOR       0255
*                                      NON-DELETED, NON-NOGO SYSMODS */
@RF00246 DS    0H                                                  0256
*   END;                            /* END OF REQUISITE CHECK FOR ALL
*                                      SYSMODS IN ICT                */
@RF00236 AH    I,@CH00064                                          0256
@DE00235 LR    @10,I                                               0256
         MH    @10,@CH01886                                        0256
         L     @01,CCAICPTF(,CCAPTR)                               0256
