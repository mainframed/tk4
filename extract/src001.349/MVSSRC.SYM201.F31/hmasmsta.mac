*                                                                  0033
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP PTRS SWITCHES ETC. - DETERMINE OPERATING SYSTEM TYPE   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0034
*   CALL INITPROC;                  /* POINTERS SWITCHES ETC         */
*                                                                  0034
         BAL   @14,INITPROC                                        0034
*   /*****************************************************************/
*   /*                                                               */
*   /* CONTINUE ONLY IF MVS OR VS1.                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0035
*   IF SYSMVS=ON×SYSVS1=ON THEN     /*                               */
         TM    SYSMVS,B'11000000'                                  0035
         BZ    @RF00035                                            0035
*     DO;                           /*                               */
*                                                                  0036
*       /*************************************************************/
*       /*                                                           */
*       /* IF INITALIZE STAE CALL THEN SETUP FOR STAE SVC.           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0037
*       IF CCARVEST=ON THEN         /*                               */
         TM    CCARVEST(CCAPTR),B'00001000'                        0037
         BNO   @RF00037                                            0037
*         DO;                       /*                               */
*                                                                  0038
*           /*********************************************************/
*           /*                                                       */
*           /* GET THE RVA.                                          */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0039
*           CALL RVAGET;            /*                               */
         BAL   @14,RVAGET                                          0039
*           IF STAHIRC=ZERO THEN    /*                               */
         LTR   STAHIRC,STAHIRC                                     0040
         BNZ   @RF00040                                            0040
*             DO;                   /*                               */
*                                                                  0041
*               /*****************************************************/
*               /*                                                   */
*               /* SET UP THE RVA FOR THE STAE SVC CALL.             */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0042
*               CALL RVAINIT;       /*                               */
*                                                                  0042
         BAL   @14,RVAINIT                                         0042
*               /*****************************************************/
*               /*                                                   */
*               /* ISSUE THE STAE SVC .                              */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0043
*               CALL STAESVC;       /*                               */
         BAL   @14,STAESVC                                         0043
*             END;                  /*                               */
*         END;                      /*                               */
*                                                                  0045
@RF00040 DS    0H                                                  0046
*       /*************************************************************/
*       /*                                                           */
*       /* IF CANCELL CALL THEN ISSSUE SVC FOR CANCEL AND FREE RVA   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0046
*       IF CCARVCAN=ON              /*                               */
*           &CCARVAD^=ZERO THEN     /* ONLY IF RVA PRESENT           */
@RF00037 TM    CCARVCAN(CCAPTR),B'00000100'                        0046
         BNO   @RF00046                                            0046
         ICM   @10,15,CCARVAD(CCAPTR)                              0046
         BZ    @RF00046                                            0046
*         DO;                       /*                               */
*           CALL STAESVC;           /*                               */
*                                                                  0048
         BAL   @14,STAESVC                                         0048
*           /*********************************************************/
*           /*                                                       */
*           /* FREE THE RVA.                                         */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0049
*           CALL RVAFREE;           /*                               */
         BAL   @14,RVAFREE                                         0049
*         END;                      /*                               */
*     END;                          /*                               */
*                                                                  0051
@RF00046 DS    0H                                                  0052
*   /*****************************************************************/
*   /*                                                               */
*   /* RETRUN TO CALLER WITH RETRUN CODE.                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0052
*   RETURN CODE(STAHIRC);           /*                               */
@RF00035 LR    @15,STAHIRC                                         0052
         L     @13,4(,@13)                                         0052
         L     @14,12(,@13)                                        0052
         LM    @00,@12,20(@13)                                     0052
         BR    @14                                                 0052
*                                                                  0053
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE PTRS, SWITHCES ETC.                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0053
*INITPROC:                                                         0053
*   PROC;                           /*                               */
@EL00001 L     @13,4(,@13)                                         0053
@EF00001 DS    0H                                                  0053
@ER00001 LM    @14,@12,12(@13)                                     0053
         BR    @14                                                 0053
INITPROC STM   @14,@04,12(@13)                                     0053
         STM   @06,@12,44(@13)                                     0053
*   STAFLG1=''B;                    /*                               */
         MVI   STAFLG1,X'00'                                       0054
*   STAHIRC=ZERO;                   /*                               */
*                                                                  0055
         SLR   STAHIRC,STAHIRC                                     0055
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE OPERATING SYSTEM TYPE                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0056
*   LOCTEN='10'X;                   /* LOCATION OF CVT ADDR          */
         LA    LOCTEN,16                                           0056
*   RFY                                                            0057
*     CVT@ BASED(LOCTEN);           /* SETUP TO GET CVT ADDR         */
*   CVTPTR=CVT@;                    /*                               */
         L     CVTPTR,CVT@(,LOCTEN)                                0058
*   IF CVT4MS1=ON                   /*                               */
*       &CVT6DAT=ON                 /*                               */
*       &CVTMVS2=ON THEN            /*                               */
         TM    CVT4MS1(CVTPTR),B'00010011'                         0059
         BNO   @RF00059                                            0059
*     SYSMVS=ON;                    /* MVS SYSTEM                    */
         OI    SYSMVS,B'10000000'                                  0060
*   IF CVT2SPS=ON                   /*                               */
*       &CVT6DAT=ON THEN            /*                               */
@RF00059 TM    CVT2SPS(CVTPTR),B'00100010'                         0061
         BNO   @RF00061                                            0061
*     SYSVS1=ON;                    /* VS1 SYSTEM                    */
         OI    SYSVS1,B'01000000'                                  0062
*   END INITPROC;                   /*                               */
@EL00002 DS    0H                                                  0063
@EF00002 DS    0H                                                  0063
@ER00002 LM    @14,@04,12(@13)                                     0063
         LM    @06,@12,44(@13)                                     0063
         BR    @14                                                 0063
*RVAGET:                                                           0064
*   PROC;                                                          0064
*                                                                  0064
RVAGET   STM   @14,@04,@SA00003                                    0064
         STM   @06,@12,@SA00003+28                                 0064
*   /*****************************************************************/
*   /*                                                               */
*   /* GETMAIN A RECOVERY WORK AREA                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0065
*   RFY                                                            0065
*     R3 RSTD;                      /* FOR ADDRESS OF AREA           */
*   R3=LENGTH(RVAPARM);             /* SIZE OF GETMAIN AREA          */
*                                                                  0066
         LA    R3,45                                               0066
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE THE GETMAIN FOR THE RVA.                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0067
*   GEN(GETMAIN EC,LV=(R3),A=GMADDR)/*                               */
*   REFS(R3,GMADDR);                /*                               */
         GETMAIN EC,LV=(R3),A=GMADDR
*   RFY                                                            0068
*     RTNCODE RSTD;                                                0068
*                                                                  0068
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ERROR SET CODE                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0069
*   STAHIRC=RTNCODE;                /* GETMAIN ERROR RETURN CODE     */
         LR    STAHIRC,RTNCODE                                     0069
*   RFY                                                            0070
*     R3 UNRSTD;                    /*                               */
*                                                                  0070
*   /*****************************************************************/
*   /*                                                               */
*   /* PUT ADDRES OF RVA INTO CCA                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0071
*   IF STAHIRC=ZERO THEN            /*                               */
         LTR   STAHIRC,STAHIRC                                     0071
         BNZ   @RF00071                                            0071
*     CCARVAD=GMADDR;               /*                               */
         MVC   CCARVAD(4,CCAPTR),GMADDR                            0072
*   RFY                                                            0073
*     RTNCODE UNRSTD;               /*                               */
*   END RVAGET;                     /*                               */
@EL00003 DS    0H                                                  0074
@EF00003 DS    0H                                                  0074
@ER00003 LM    @14,@04,@SA00003                                    0074
         LM    @06,@12,@SA00003+28                                 0074
         BR    @14                                                 0074
*RVAINIT:                                                          0075
*   PROC;                           /*                               */
*                                                                  0075
RVAINIT  STM   @14,@12,12(@13)                                     0075
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE FIELDS IN THE RVA.                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0076
*   RVAPARM=''B;                    /* ZERO OUT THE RVA              */
         L     @10,CCARVAD(,CCAPTR)                                0076
         XC    RVAPARM(45,@10),RVAPARM(@10)                        0076
*   RVACCAP=CCAPTR;                 /* ADDRESS FOR CCA               */
         ST    CCAPTR,RVACCAP(,@10)                                0077
*   RVAHDR=RVATXT;                  /* SET EYE CATCHER IN THE RVA    */
         MVC   RVAHDR(4,@10),@CC01051                              0078
*   RVAEPRNM='HMASMP  ';            /* SMP NAME INTO RVA             */
         MVC   RVAEPRNM(8,@10),@CC01083                            0079
*   IF SYSMVS=ON THEN               /*                               */
         TM    SYSMVS,B'10000000'                                  0080
         BNO   @RF00080                                            0080
*     RVAMVS=ON;                    /* SYSTME IS MVS                 */
         OI    RVAMVS(@10),B'10000000'                             0081
*   IF SYSVS1=ON THEN               /*                               */
@RF00080 TM    SYSVS1,B'01000000'                                  0082
         BNO   @RF00082                                            0082
*     RVAVS1=ON;                    /* SYSTEM IS VS1                 */
         L     @10,CCARVAD(,CCAPTR)                                0083
         OI    RVAVS1(@10),B'01000000'                             0083
*   END RVAINIT;                    /*                               */
@EL00004 DS    0H                                                  0084
@EF00004 DS    0H                                                  0084
@ER00004 LM    @14,@12,12(@13)                                     0084
         BR    @14                                                 0084
*                                                                  0085
