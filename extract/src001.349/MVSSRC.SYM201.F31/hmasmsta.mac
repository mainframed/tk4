*   /* ISSUE STAE                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0085
*STAESVC:                                                          0085
*   PROC;                                                          0085
STAESVC  STM   @14,@04,@SA00005                                    0085
         STM   @06,@12,@SA00005+28                                 0085
*   RFY                                                            0086
*     R2 RSTD;                                                     0086
*                                                                  0086
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR INITIALIZE STAE OR CANCEL.                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0087
*   IF CCARVEST=ON THEN             /* INITIALIZE                    */
         TM    CCARVEST(CCAPTR),B'00001000'                        0087
         BNO   @RF00087                                            0087
*     R2=ADDR(HMASMSER);            /* ADDRESS OF STAE EXIT ROUTINE  */
         L     R2,@CV01045                                         0088
*   IF CCARVCAN=ON THEN             /* IS THIS A CANCEL REQUEST      */
@RF00087 TM    CCARVCAN(CCAPTR),B'00000100'                        0089
         BNO   @RF00089                                            0089
*     R2=ZERO;                                                     0090
*                                                                  0090
         SLR   R2,R2                                               0090
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE THE SVC FOR INITIALIZE OR CANCEL.                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0091
*   RFY                                                            0091
*     R3 RSTD;                      /*                               */
@RF00089 DS    0H                                                  0092
*   R3=CCARVAD;                     /*                               */
*                                                                  0092
         L     @10,CCARVAD(,CCAPTR)                                0092
         LR    R3,@10                                              0092
*   /*****************************************************************/
*   /*                                                               */
*   /* ESTAE IN MVS.                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0093
*   IF RVAMVS=ON THEN               /*                               */
         TM    RVAMVS(@10),B'10000000'                             0093
         BNO   @RF00093                                            0093
*     DO;                           /*                               */
*       IF CCARVEST=ON THEN         /* INITIALIZE                    */
         TM    CCARVEST(CCAPTR),B'00001000'                        0095
         BNO   @RF00095                                            0095
*         DO;                       /*                               */
*           GEN CODE REFS(HMASMSER,CCARVAD,R2,R3);                 0097
             ESTAE (R2),                                             XXX
               CT,                                                   XXX
               PARAM=(R3),                                           XXX
               XCTL=NO,                                              XXX
               PURGE=NONE,                                           XXX
               ASYNCH=NO,                                            XXX
               TERM=YES
*         END;                      /*                               */
*       ELSE                        /* CANCEL                        */
*         DO;                       /*                               */
         B     @RC00095                                            0099
@RF00095 DS    0H                                                  0100
*           GEN CODE;                                              0100
             ESTAE 0
*         END;                      /*                               */
*       RFY                                                        0102
*         RTNCODE RSTD;             /*                               */
@RC00095 DS    0H                                                  0103
*       IF RTNCODE^=ZERO THEN       /*                               */
         LTR   RTNCODE,RTNCODE                                     0103
         BZ    @RF00103                                            0103
*         STAHIRC=STOPSMP;          /*                               */
         LA    STAHIRC,16                                          0104
*       RFY                                                        0105
*         RTNCODE UNRSTD;           /*                               */
@RF00103 DS    0H                                                  0106
*     END;                          /*                               */
*                                                                  0106
*   /*****************************************************************/
*   /*                                                               */
*   /* STAE IN VS1.                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0107
*   IF RVAVS1=ON THEN               /*                               */
@RF00093 L     @10,CCARVAD(,CCAPTR)                                0107
         TM    RVAVS1(@10),B'01000000'                             0107
         BNO   @RF00107                                            0107
*     DO;                           /*                               */
*       IF CCARVEST=ON THEN         /* INITIALIZE                    */
         TM    CCARVEST(CCAPTR),B'00001000'                        0109
         BNO   @RF00109                                            0109
*         DO;                       /*                               */
*           GEN CODE REFS(HMASMSER,CCARVAD,R2,R3);                 0111
            STAE (R2),                                               XXX
               CT,                                                   XXX
               PARAM=(R3),                                           XXX
               XCTL=NO,                                              XXX
               PURGE=NONE,                                           XXX
               ASYNCH=NO,                                            XXX
               TERM=YES
*           RFY                                                    0112
*             RTNCODE RSTD;         /*                               */
*         END;                      /*                               */
*       ELSE                        /* CANCEL                        */
*         DO;                       /*                               */
         B     @RC00109                                            0114
@RF00109 DS    0H                                                  0115
*           GEN CODE;                                              0115
             ESTAE 0
*         END;                      /*                               */
*       RFY                                                        0117
*         RTNCODE RSTD;             /*                               */
@RC00109 DS    0H                                                  0118
*       IF RTNCODE^=ZERO THEN       /*                               */
         LTR   RTNCODE,RTNCODE                                     0118
         BZ    @RF00118                                            0118
*         STAHIRC=STOPSMP;          /*                               */
         LA    STAHIRC,16                                          0119
*       RFY                                                        0120
*         RTNCODE UNRSTD;           /*                               */
@RF00118 DS    0H                                                  0121
*     END;                          /*                               */
*   RFY                                                            0122
*     R2 UNRSTD;                    /*                               */
*   RFY                                                            0123
*     R3 UNRSTD;                    /*                               */
*   END STAESVC;                                                   0124
@EL00005 DS    0H                                                  0124
@EF00005 DS    0H                                                  0124
@ER00005 LM    @14,@04,@SA00005                                    0124
         LM    @06,@12,@SA00005+28                                 0124
         BR    @14                                                 0124
*RVAFREE:                                                          0125
*   PROC;                                                          0125
