*   /*****************************************************************/
*                                                                  0126
*   RFY                                                            0126
*     R3 RSTD;                      /*                               */
*   R3=LENGTH(RVAPARM);             /* ITS SIZE                      */
         LA    R3,45                                               0127
*   GMADDR=CCARVAD;                 /* GET ADDRESS FROM CCA          */
         MVC   GMADDR(4),CCARVAD(CCAPTR)                           0128
*   GEN(FREEMAIN E,LV=(R3),A=GMADDR)/*                               */
*   REFS(R3,GMADDR);                /*                               */
         FREEMAIN E,LV=(R3),A=GMADDR
*   RFY                                                            0130
*     R3 UNRSTD;                    /*                               */
*   END RVAFREE;                    /*                               */
@EL00006 DS    0H                                                  0131
@EF00006 DS    0H                                                  0131
@ER00006 LM    @14,@12,@SA00006                                    0131
         BR    @14                                                 0131
