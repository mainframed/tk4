         TITLE 'HMASMSUB - HMASMP SUBROUTINES'
HMASMSUB CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMSUB  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*                                                                  0160
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE RETURN CODE AND CALL PROPER SUBROUTINE FOR FUNCTION*/
*   /* REQUESTED IN SPLFUNCT THEN RETURN                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0160
*   SUBRTNCD=SPLGOOD;               /* SET GOOD RETURN CODE          */
         SLR   SUBRTNCD,SUBRTNCD                                   0160
*   CCAPTR=ADDR(HMASMCCA);          /*                       @Z67SP00*/
         L     CCAPTR,@CA00071                                     0161
*   CALL SUBRTN;                    /* CALL REQUESTED SUBROUTINE     */
         L     @10,@PC00001                                        0162
         SLR   @05,@05                                             0162
         IC    @05,SPLFUNCT(,@10)                                  0162
         SLA   @05,2                                               0162
         L     @15,SUBRTNS-4(@05)                                  0162
         BALR  @14,@15                                             0162
*   RFY                                                            0163
*     RTNCODE RSTD;                 /*                       @Z67SP00*/
*   SUBRTNCD=MAX(SUBRTNCD,RTNCODE); /*                       @Z67SP00*/
         CR    SUBRTNCD,RTNCODE                                    0164
         BNL   *+6
         LR    SUBRTNCD,RTNCODE                                    0164
*   RFY                                                            0165
*     RTNCODE UNRSTD;               /*                       @Z67SP00*/
*   RETURN CODE(SUBRTNCD);          /* RETURN WITH CODE IN PARM AREA */
         LR    @15,SUBRTNCD                                        0166
         L     @13,4(,@13)                                         0166
         L     @14,12(,@13)                                        0166
         LM    @00,@12,20(@13)                                     0166
         BR    @14                                                 0166
         EJECT
*                                                                  0167
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY POINT TO PACK 8 CHARACTER DLIB OR SOURCE NAMES INTO 6   */
*   /* CHARACTERS                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0167
*PACKSRTN:                          /* SMP 8 TO 6 CHAR DLIB PACK     */
*   PROCEDURE;                      /* ROUTINE                       */
PACKSRTN STM   @14,@12,12(@13)                                     0167
*   CNVDLIB8=CNVPRM8;               /* MOVE 8 CHAR NAME              */
         L     @10,@PC00001                                        0168
         L     @10,SPLPMAD(,@10)                                   0168
         MVC   CNVDLIB8(8),CNVPRM8(@10)                            0168
*   TR(CNVDLIB8,TRTABLE1);          /* CONVERT TO INT. FMT.  @Z67SP00*/
*                                                                  0169
         TR    CNVDLIB8(8),TRTABLE1                                0169
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP THRU THE 8 CHARACTER NAME IN GROUPS OF 4 CHARACTERS      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0170
*   DO CNVCNT1=1 TO DIM(DLIB8WD);   /* PACK ONE WORD AT A TIME       */
         LA    CNVCNT1,1                                           0170
@DL00170 DS    0H                                                  0171
*     CNVCHAR4=DLIB8WD(CNVCNT1);    /* PICK UP 4 CHARACTERS          */
*                                                                  0171
         LR    @10,CNVCNT1                                         0171
         SLA   @10,2                                               0171
         L     @10,DLIB8WD-4(@10)                                  0171
         ST    @10,CNVCHAR4                                        0171
*     /***************************************************************/
*     /*                                                             */
*     /* LOOP THRU THE 4 CHARACTERS. THE HIGH ORDER BITS ARE SHIFTED */
*     /* OUT. THEN THE REMAINING 6 BITS ARE SHIFTED LEFT INTO THE    */
*     /* ADJOINING REGISTER. AT THE END OF THIS LOOP THE LOWER THREE */
*     /* BYTES OF THE ADJOINING REGISTER WILL CONTAIN THE PACKED     */
*     /* REPRSENTATION OF THE FOUR BYTES PROCESSED.                  */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0172
*     RFY                                                          0172
*      (CNVREG2,                                                   0172
*       CNVREG3) RSTD;              /*                       @PK00006*/
*     CNVREG3=CNVCHAR4;             /* PICK UP 4 CHAR TO BE PACKED   */
         LR    CNVREG3,@10                                         0173
*     CNVREG2=0;                    /* ZERO OUT REG TO HOLD PACKED 0174
*                                      CHARACTER                     */
         SLR   CNVREG2,CNVREG2                                     0174
*     DO CNVCNT2=1 TO LENGTH(DLIB8CH);/* LOOP THRU 4 CHAR PER WRD    */
         LA    CNVCNT2,1                                           0175
@DL00175 DS    0H                                                  0176
*       SLL(CNVREG3,2);             /* SHIFT OUT HIGH 2 BITS OF THE
*                                      LEFTMOST CHAR IN CNVREG3      */
         SLL   CNVREG3,2                                           0176
*       SLDL(CNVREG2,6);            /* SHIFT THE REMAINING LOWER 6 0177
*                                      BITS INTO THE OTHER CNV REG   */
         SLDL  CNVREG2,6                                           0177
*     END;                          /* END 4 CHAR SHIFT GROUP        */
         AH    CNVCNT2,@CH00039                                    0178
         CH    CNVCNT2,@CH00045                                    0178
         BNH   @DL00175                                            0178
*     DLIB6WD(CNVCNT1)=CNVREG2;     /* STORE THE CONVERTED 4 CHAR IN
*                                      THE 6 CHAR CONVERSION AREA    */
         LR    @10,CNVCNT1                                         0179
         SLA   @10,2                                               0179
         ST    CNVREG2,DLIB6WD-4(@10)                              0179
*     RFY                                                          0180
*      (CNVREG2,                                                   0180
*       CNVREG3) UNRSTD;            /*                       @PK00006*/
*   END;                            /* END 2 WORD CONVERSION GROUP   */
*                                                                  0181
         AH    CNVCNT1,@CH00039                                    0181
         CH    CNVCNT1,@CH00041                                    0181
         BNH   @DL00170                                            0181
*   /*****************************************************************/
*   /*                                                               */
*   /* SHIFT THE CONVERTED CHARACTERS INTO THE CORRECT SPOT IN THE   */
*   /* RETURN AREA, SET THE SPECIAL CHARACTER INDICATING THAT THE    */
*   /* NAME HAS BEEN PACKED, AND RETURN. THE NAME WILL BE IN THE     */
*   /* SECOND THRU THE EIGTH CHAR OF THE WORK AREA                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0182
*   DLB6AREA(3:5)=DLB6AREA(2:4);    /* SHIFT THE FIRST CONVERTED WORD
*                                      OVER 8 BITS SO THAT IT WILL BE
*                                      POSITIONED NEXT TO THE OTHER
*                                      CHAR                          */
         MVC   @TS00001(3),DLB6AREA+1                              0182
         MVC   DLB6AREA+2(3),@TS00001                              0182
*   CNVPRM6=DLB6AREA(3:8);          /* SET RETURN NAME               */
         L     @10,@PC00001                                        0183
         L     @10,SPLPMAD(,@10)                                   0183
         MVC   CNVPRM6(6,@10),DLB6AREA+2                           0183
*   RETURN CODE(SPLGOOD);           /*                       @Z67SP00*/
         SLR   @15,@15                                             0184
         L     @14,12(,@13)                                        0184
         LM    @00,@12,20(@13)                                     0184
         BR    @14                                                 0184
*   END PACKSRTN;                   /* RETURN TO CALLING POINT       */
         EJECT
*                                                                  0186
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY POINT TO UNPACK 6 CHARACTER DLIB OR SOURCE NAMES INTO 8 */
*   /* CHARACTER RETURN AREA                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0186
*UNPKRTN:                           /* SMP 7 TO 8 CHAR DLIB UNPACK   */
*   PROCEDURE;                      /* ROUTINE                       */
UNPKRTN  STM   @14,@12,12(@13)                                     0186
*   DLB6AREA(3:8)=CNVPRM6;          /* MOVE 7 CHAR DLIB NAME         */
*                                                                  0187
         L     @10,@PC00001                                        0187
         L     @10,SPLPMAD(,@10)                                   0187
         MVC   DLB6AREA+2(6),CNVPRM6(@10)                          0187
*   /*****************************************************************/
*   /*                                                               */
*   /* THE NAME IS IN PACKED FORMAT AND MUST THEREFORE BE UNPACKED.  */
*   /* PROCESS THREE BYTES AT A TIME, REPRESENTING FOUR CHARACTERS.  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0188
*   DLB6AREA(2:4)=DLB6AREA(3:5);    /* SHIFT TO ALIGN FOR LOAD       */
         MVC   @TS00001(3),DLB6AREA+2                              0188
         MVC   DLB6AREA+1(3),@TS00001                              0188
*   DO CNVCNT1=1 TO DIM(DLIB6WD);   /* DO 2 WORD OF CHAR             */
         LA    CNVCNT1,1                                           0189
@DL00189 DS    0H                                                  0190
*     RFY                                                          0190
*      (CNVREG2,                                                   0190
*       CNVREG3) RSTD;              /*                       @PK00006*/
*     CNVREG3=DLIB6WD(CNVCNT1);     /* PICK UP WORD OF PACKED BITS   */
         LR    @10,CNVCNT1                                         0191
         SLA   @10,2                                               0191
         L     CNVREG3,DLIB6WD-4(@10)                              0191
*     SLL(CNVREG3,8);               /* SHIFT OUT HIGH ORDER BYTE     */
*                                                                  0192
         SLL   CNVREG3,8                                           0192
*     /***************************************************************/
*     /*                                                             */
*     /* LOOP THRU SHIFTING 6 BITS AT A TIME INTO THE ADJECENT REG   */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0193
*     DO CNVCNT2=1 TO LENGTH(DLIB8CH);/* DO 4 CHAR PER WORD          */
         LA    CNVCNT2,1                                           0193
@DL00193 DS    0H                                                  0194
*       CNVREG2=0;                  /* INITIALIZE REG TO HOLD CHAR   */
         SLR   CNVREG2,CNVREG2                                     0194
*       SLDL(CNVREG2,6);            /* SHIFT 6 BITS TO CNV REG THE 0195
*                                      REGISTER                      */
         SLDL  CNVREG2,6                                           0195
*       DLIB8CH(CNVCNT1,CNVCNT2)=CNVREG2;/* STORE CHAR IN CONVERTED
*                                      AREA                          */
         LR    @10,CNVCNT1                                         0196
         SLA   @10,2                                               0196
         ALR   @10,CNVCNT2                                         0196
         STC   CNVREG2,DLIB8CH-5(@10)                              0196
*     END;                          /* END 4 CHAR GROUP              */
         AH    CNVCNT2,@CH00039                                    0197
         CH    CNVCNT2,@CH00045                                    0197
         BNH   @DL00193                                            0197
*     RFY                                                          0198
*      (CNVREG2,                                                   0198
*       CNVREG3) UNRSTD;            /*                       @PK00006*/
*   END;                            /* END 2 WORD GROUP              */
         AH    CNVCNT1,@CH00039                                    0199
         CH    CNVCNT1,@CH00041                                    0199
         BNH   @DL00189                                            0199
*   TR(CNVDLIB8,TRTABLE2);          /* CONVERT TO APLH-NUMER @Z67SP00*/
         TR    CNVDLIB8(8),TRTABLE2                                0200
*   CNVPRM8=CNVDLIB8;               /* RETURN CONVERTED NAME         */
         L     @10,@PC00001                                        0201
         L     @10,SPLPMAD(,@10)                                   0201
         MVC   CNVPRM8(8,@10),CNVDLIB8                             0201
*   RFY                                                            0202
*    (CNVREG2,                                                     0202
*     CNVREG3) UNRSTD;                                             0202
*   RETURN CODE(SPLGOOD);           /*                       @Z67SP00*/
         SLR   @15,@15                                             0203
         L     @14,12(,@13)                                        0203
         LM    @00,@12,20(@13)                                     0203
         BR    @14                                                 0203
*   END UNPKRTN;                    /* RETURN TO CALLER              */
         EJECT
*                                                                  0205
*   /*****************************************************************/
*   /*                                                               */
*   /* GETMAIN IOP SUBROTINE                                 @Z67SP00*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0205
*GMIOPRTN:                          /*                       @Z67SP00*/
*   PROCEDURE;                      /*                       @Z67SP00*/
GMIOPRTN STM   @14,@12,@SA00004                                    0205
*   DECLARE                         /*                       @Z67SP00*/
*     GMRTNCD FIXED(8);             /*                       @Z67SP00*/
*   GMRTNCD=SPLGOOD;                /*                       @Z67SP00*/
         SLR   GMRTNCD,GMRTNCD                                     0207
*   RFY                                                            0208
*     SIZE RSTD;                    /*                       @Z67SP00*/
*   SIZE=LENGTH(IOPPTSYS)           /* LEN OF FIXED SIZE OF LARGEST
*                                      ENTRY IN ANY DATASET  @Z67SP00*/
*       +LENGTH(IOPPNTRY)*(CCAPEMAX+1)/* VARIABLE SIZE OF    @Z67SP00*/
*       +1;                         /* FOR EOLST             @Z67SP00*/
         L     @10,@CA00071                                        0209
         LA    SIZE,1                                              0209
         AH    SIZE,CCAPEMAX-HMASMCCA(,@10)                        0209
         MH    SIZE,@CH00455                                       0209
         AH    SIZE,@CH01376                                       0209
*   REMAINDR=(SIZE-LENGTH(IOPDIRCT))//LRECL;/* ROUND TO 80   @Z67SP00*/
         LA    @10,80                                              0210
         LR    @00,SIZE                                            0210
         SH    @00,@CH00191                                        0210
         SRDA  @00,32                                              0210
         DR    @00,@10                                             0210
         LR    REMAINDR,@00                                        0210
*   IF REMAINDR^=ZERO               /* 80 IF NOT ALREADY     @Z67SP00*/
*     THEN                          /*                       @Z67SP00*/
         LTR   REMAINDR,REMAINDR                                   0211
         BZ    @RF00211                                            0211
*     REMAINDR=LRECL-REMAINDR;      /*                       @Z67SP00*/
         SLR   @10,REMAINDR                                        0212
         LR    REMAINDR,@10                                        0212
*   SIZE=SIZE+REMAINDR;             /*                       @Z67SP00*/
@RF00211 ALR   SIZE,REMAINDR                                       0213
*   SIZE=SIZE+LENGTH(HMASMIOP);     /*                       @Z67SP00*/
         AH    SIZE,@CH01052                                       0214
*   GEN(GETMAIN  EC,LV=(SIZE),A=GMADDR)                            0215
*   REFS(SIZE,GMADDR);              /*                       @Z67SP00*/
         GETMAIN  EC,LV=(SIZE),A=GMADDR
*   RFY                                                            0216
*     RTNCODE RSTD;                 /*                       @Z67SP00*/
*   IF RTNCODE=ZERO                 /*                       @Z67SP00*/
*     THEN                          /*                       @Z67SP00*/
         LTR   RTNCODE,RTNCODE                                     0217
         BNZ   @RF00217                                            0217
*     DO;                           /*                       @Z67SP00*/
*       CCAPESIZ=SIZE-LENGTH(HMASMIOP);/*                    @Z67SP00*/
         L     @14,@CA00071                                        0219
         LR    @10,SIZE                                            0219
         SH    @10,@CH01052                                        0219
         ST    @10,CCAPESIZ-HMASMCCA(,@14)                         0219
*       SPLPMAD=GMADDR;             /* RETURN ADDR           @Z67SP00*/
         L     @14,@PC00001                                        0220
         MVC   SPLPMAD(4,@14),GMADDR                               0220
*     END;                          /*                       @Z67SP00*/
*   ELSE                            /*                       @Z67SP00*/
*     DO;                           /*                       @Z67SP00*/
         B     @RC00217                                            0222
@RF00217 DS    0H                                                  0223
*       HMASMMGP=''B;               /* ZERO MSG PARM         @Z67SP00*/
         XC    HMASMMGP(16),HMASMMGP                               0223
*       MGPPRINT=ON;                /*                       @Z67SP00*/
*       MGPHLDS=ON;                 /*                       @Z67SP00*/
         OI    MGPPRINT,B'10010000'                                0225
*       MGPMGNO1=HMA202I;           /* UNABL TO OBTAIN STORAGE FOR 0226
*                                      WORK AREAS            @Z67SP00*/
         MVC   MGPMGNO1(2),@CH00043                                0226
*       IF CCAUPDU=ON               /* IF IN UCLIN PROCESS   @Z67SP00*/
*           ×CCAFLAG1=ZERO          /* OR IN INITIALIZATION  @Z67SP00*/
*         THEN                      /*                       @Z67SP00*/
         L     @14,@CA00071                                        0227
         TM    CCAUPDU-HMASMCCA(@14),B'00000010'                   0227
         BO    @RT00227                                            0227
         ICM   @10,7,CCAFLAG1-HMASMCCA(@14)                        0227
         BNZ   @RF00227                                            0227
@RT00227 DS    0H                                                  0228
*         DO;                       /*                       @Z67SP00*/
*           GMRTNCD=SPLTSMP;        /* TERMINATE SMP         @Z67SP00*/
         LA    GMRTNCD,16                                          0229
*           MGPTYPE=MGPTERM;        /*                       @Z67SP00*/
         MVI   MGPTYPE,X'05'                                       0230
*         END;                      /*                       @Z67SP00*/
*       ELSE                        /*                       @Z67SP00*/
*         DO;                       /*                       @Z67SP00*/
         B     @RC00227                                            0232
@RF00227 DS    0H                                                  0233
*           GMRTNCD=SPLSPACE;       /* TERMINATE FUNCTION    @Z67SP00*/
         LA    GMRTNCD,12                                          0233
*           MGPTYPE=MGPSEV;         /*                       @Z67SP00*/
         MVI   MGPTYPE,X'04'                                       0234
*         END;                      /*                       @Z67SP00*/
*       CALL HMASMMSG(HMASMMGP);    /*                       @Z67SP00*/
@RC00227 L     @15,@CV01263                                        0236
         LA    @01,@AL00236                                        0236
         BALR  @14,@15                                             0236
*     END;                          /*                       @Z67SP00*/
*   RFY                                                            0238
*     SIZE UNRSTD;                  /*                       @Z67SP00*/
@RC00217 DS    0H                                                  0239
*   RETURN CODE(GMRTNCD);           /*                       @Z67SP00*/
         LR    @15,GMRTNCD                                         0239
         L     @14,@SA00004                                        0239
         LM    @00,@12,@SA00004+8                                  0239
         BR    @14                                                 0239
*   END GMIOPRTN;                   /*                       @Z67SP00*/
         EJECT
*                                                                  0241
*   /*****************************************************************/
*   /*                                                               */
*   /* FREEMAIN IOP ROUTINE                                  @Z67SP00*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0241
*FMIOPRTN:                          /*                       @Z67SP00*/
*   PROCEDURE;                      /*                       @Z67SP00*/
FMIOPRTN STM   @14,@12,@SA00005                                    0241
*   GMADDR=SPLPMAD;                 /* ADDR OF IOP TO FREE   @Z67SP00*/
         L     @14,@PC00001                                        0242
         MVC   GMADDR(4),SPLPMAD(@14)                              0242
*   RFY                                                            0243
*     SIZE RSTD;                    /*                       @Z67SP00*/
*   SIZE=CCAPESIZ+LENGTH(HMASMIOP); /* IOP SIZE TO FREE      @Z67SP00*/
         L     @14,@CA00071                                        0244
         LA    SIZE,68                                             0244
         AL    SIZE,CCAPESIZ-HMASMCCA(,@14)                        0244
*   GEN(FREEMAIN E,LV=(SIZE),A=GMADDR)                             0245
*   REFS(SIZE,GMADDR);              /*                       @Z67SP00*/
         FREEMAIN E,LV=(SIZE),A=GMADDR
*   RFY                                                            0246
*     SIZE UNRSTD;                  /*                       @Z67SP00*/
*   RETURN CODE(SPLGOOD);           /*                       @Z67SP00*/
         SLR   @15,@15                                             0247
         L     @14,@SA00005                                        0247
         LM    @00,@12,@SA00005+8                                  0247
         BR    @14                                                 0247
*   END FMIOPRTN;                   /*                       @Z67SP00*/
         EJECT
*                                                                  0249
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINE TO CONSTRUCT THE RELFILE DSN FROM THE THREE PARTS THAT*/
*   /* DETERMINE NAME                                        @Z67SP00*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0249
*BDDSNRTN:                          /*                       @Z67SP00*/
*   PROCEDURE;                      /*                       @Z67SP00*/
BDDSNRTN STM   @14,@12,12(@13)                                     0249
*   DECLARE                         /*                       @Z67SP00*/
*     SAVPFX CHAR(26),              /* SAVE PREFIX AREA      @Z67SP00*/
*     SAVSMD CHAR(7),               /* SAVE SYSMOD AREA      @Z67SP00*/
*     SAVFIL FIXED(15);             /* SAVE FILE AREA        @Z67SP00*/
*   DECLARE                         /*                       @Z67SP00*/
*     DSNX FIXED(31);               /* LOOP CONTROL          @Z67SP00*/
*   DECLARE                         /*                       @Z67SP00*/
*     DBLWORD CHAR(8) BDY(DWORD),   /* FOR CVD               @Z67SP00*/
*     FTEMP CHAR(4) BDY(WORD);      /* FOR UNPACK            @Z67SP00*/
*   DECLARE                         /*                       @Z67SP00*/
*     THREE0 CHAR(3) CONSTANT('000'),/* FOR COMPARE          @Z67SP00*/
*     TWO0 CHAR(2) CONSTANT('00'),  /*                       @Z67SP00*/
*     ONE0 CHAR(1) CONSTANT('0');   /*                       @Z67SP00*/
*   SAVPFX=SPLIPFX;                 /* SAV DSN PREFIX        @Z67SP00*/
         L     @14,@PC00001                                        0254
         L     @14,SPLPMAD(,@14)                                   0254
         MVC   SAVPFX(26),SPLIPFX(@14)                             0254
*   SAVSMD=SPLISMD;                 /* SAVE SYSMOD NUMBER    @Z67SP00*/
         MVC   SAVSMD(7),SPLISMD(@14)                              0255
*   SAVFIL=SPLIFIL;                 /* AND FILE              @Z67SP00*/
         LH    SAVFIL,SPLIFIL(,@14)                                0256
*   SPLODSN=SPLIPFX;                /* MOVE BACK PREFIX AND BLANK OUT
*                                      REST OF NAME          @Z67SP00*/
         MVC   @TS00001(26),SPLIPFX(@14)                           0257
         MVI   SPLODSN+26(@14),C' '                                0257
         MVC   SPLODSN+27(17,@14),SPLODSN+26(@14)                  0257
         MVC   SPLODSN(26,@14),@TS00001                            0257
*   DO DSNX=1 TO LENGTH(SPLIPFX) WHILE/* FIND LENGTH OF      @Z67SP00*/
*         SPLIPFX(DSNX)^=BLANK;     /* PREFIX                @Z67SP00*/
         LA    DSNX,1                                              0258
@DL00258 L     @14,@PC00001                                        0258
         L     @14,SPLPMAD(,@14)                                   0258
         ALR   @14,DSNX                                            0258
         BCTR  @14,0                                               0258
         CLI   SPLIPFX(@14),C' '                                   0258
         BE    @DC00258                                            0258
*   END;                            /*                       @Z67SP00*/
         AH    DSNX,@CH00039                                       0259
         CH    DSNX,@CH00066                                       0259
         BNH   @DL00258                                            0259
@DC00258 DS    0H                                                  0260
*   IF DSNX^=1                      /* IF PREFIX SPECIFIED   @Z67SP00*/
*     THEN                          /* THEN                  @Z67SP00*/
         LA    @14,1                                               0260
         CR    DSNX,@14                                            0260
         BE    @RF00260                                            0260
*     DO;                           /*                       @Z67SP00*/
*       SPLODSN(DSNX)='.';          /* SEPERATE IT           @Z67SP00*/
         L     @10,@PC00001                                        0262
         L     @10,SPLPMAD(,@10)                                   0262
         ALR   @10,DSNX                                            0262
         BCTR  @10,0                                               0262
         MVI   SPLODSN(@10),C'.'                                   0262
*       DSNX=DSNX+1;                /*                       @Z67SP00*/
         ALR   DSNX,@14                                            0263
*     END;                          /*                       @Z67SP00*/
*   SPLODSN(DSNX:DSNX+6)=SAVSMD;    /* FROM SYSMOD           @Z67SP00*/
@RF00260 L     @14,@PC00001                                        0265
         L     @14,SPLPMAD(,@14)                                   0265
         LR    @10,@14                                             0265
         ALR   @10,DSNX                                            0265
         BCTR  @10,0                                               0265
         MVC   SPLODSN(7,@10),SAVSMD                               0265
*   SPLODSN(DSNX+7:DSNX+8)='.F';    /* AND SEPERATE IT FROM  @Z67SP00*/
         LR    @10,@14                                             0266
         ALR   @10,DSNX                                            0266
         MVC   SPLODSN+6(2,@10),@CC01371                           0266
*   CVD(SAVFIL,DBLWORD);            /* CNV FILE TO DECIMAL   @Z67SP00*/
         CVD   SAVFIL,DBLWORD                                      0267
