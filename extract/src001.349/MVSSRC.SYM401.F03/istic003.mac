         ST    @14,ITATCFR                                         0197
*                   ITATCTO=ADDR($DEVSESP(X8));/* SET THE TO ADDRESS */
         L     @14,X8                                              0198
         MH    @14,@CH00373                                        0198
         LA    @14,$DEVSESP-68(@14,WAPTR)                          0198
         ST    @14,ITATCTO                                         0198
*                   ITATCTP='0'B;   /* SET THE TYPE OF CONVERT BIT   */
         NI    ITATCTP,B'01111111'                                 0199
*                   ITATCNT=12;     /* GIVE THE CONVERT BYTE COUNT   */
         MVI   ITATCNT,X'0C'                                       0200
