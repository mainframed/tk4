*     GO TO DEV007;                 /* YES ,TAKE BRANCH              */
*   IF INPUTBUF(1:5)=NDR(1:5) THEN  /* CHECK FOR NDR SPECIFIED       */
         CLC   INPUTBUF(5,R5),NDR                                  0067
         BNE   @RF00067                                            0067
*     DO;                           /* YES                           */
*       $NDR='1'B;                  /* YES                           */
         OI    $NDR(WAPTR),B'00100000'                             0069
*       $DEVCNTS=3;                 /* SET DEVICE COUNT              */
         MVC   $DEVCNTS(2,WAPTR),@CH00081                          0070
