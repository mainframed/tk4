         L     @14,X8                                              0205
         MH    @14,@CH00373                                        0205
         ALR   @14,WAPTR                                           0205
         OI    $DVSPRM-68(@14),B'00010000'                         0205
*                   R5=R5+1;        /* SKIP OVER QUOTE       @ZD10882*/
         AL    R5,@CF00072                                         0206
*                   GOTO DEV086;    /*                       @D32JPFR*/
         B     DEV086                                              0207
*                 END;              /* 2D32JPFR                      */
*               X7=R5+1;            /* PT TO FIRST CHAR OF NAME    0209
