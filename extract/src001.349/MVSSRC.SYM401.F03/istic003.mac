@RF00133 CLC   X10(4),@CF00068                                     0138
         BNE   @RF00138                                            0138
         L     @14,X7                                              0138
         CLI   INPUT(@14),C'0'                                     0138
         BNE   @RT00138                                            0138
*     GO TO DEV800;                 /* IS NOT ZERO,ERROR             */
*   IF INPUTBUF(1)^='-' THEN        /* IF EXTENT NOT SPECIFIED       */
@RF00138 CLI   INPUTBUF(R5),C'-'                                   0140
         BNE   @RT00140                                            0140
*     GO TO DEV070;                 /* GO CONVERT UA TO HEX          */
*   R5=R5+1;                        /* DASH FOUND,PT TO NEXT         */
         AL    R5,@CF00072                                         0142
*   IF DASH='1'B THEN               /* IF DASH PREVIOUSLY FND        */
         TM    DASH,B'00100000'                                    0143
         BO    @RT00143                                            0143
*     GO TO DEV800;                 /* GO TO ERROR HNDLING           */
*   DASH='1'B;                      /* SET DASH FLAG ON              */
         OI    DASH,B'00100000'                                    0145
*   GO TO DEV008;                   /* GO PROCESS HIGH EXTENT        */
         B     DEV008                                              0146
*DEV040:                            /* SYMBOLIC NAME PROCESS         */
*   IF INPUTBUF(1)='-' THEN         /* INCLUSIVE RANGE NOT           */
DEV040   CLI   INPUTBUF(R5),C'-'                                   0147
         BE    @RT00147                                            0147
*     GO TO DEV800;                 /* ALLOWED W/SYMBOLIC NAME       */
*   CDSSYM=' ';                     /* INIT SYMBOLIC NAME AREA       */
         MVI   CDSSYM+1,C' '                                       0149
         MVC   CDSSYM+2(6),CDSSYM+1                                0149
         MVI   CDSSYM,C' '                                         0149
