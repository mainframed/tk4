         MVI   ITATCNT,X'10'                                       0161
*   R1=ADDR(ITATCPRM);              /* POINT TO CONVERT PARM LIST    */
         LA    R1,ITATCPRM                                         0162
*   CALL ISTSC021;                  /* CALL CONVERT DIRECTLY         */
         L     @15,@CV00737                                        0163
         BALR  @14,@15                                             0163
*   IF R15^=0 THEN                  /* ERROR CONVERTING ?            */
         LTR   R15,R15                                             0164
         BNZ   @RT00164                                            0164
*     GO TO DEV072;                 /* YES                           */
*   IF DASH='1'B THEN               /* INCLUSIVE UA RANGE            */
         TM    DASH,B'00100000'                                    0166
         BNO   @RF00166                                            0166
*     DO;                           /* YES,CONTINUE                  */
