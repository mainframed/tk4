*   IF INPUTBUF(1:2)=CTST(1:2) THEN /* CHK FOR ASTERISK ENT          */
@RF00067 CLC   INPUTBUF(2,R5),CTST                                 0075
         BNE   @RF00075                                            0075
*     DO;                           /* YES ,CT TEST                  */
*                                                                  0076
*       /*************************************************************/
*       /*                                                           */
*       /* CANNOT ALLOW SYSTEM CONSOL TO ENTER ,CHECK IT             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0077
*       IF $SYSMN(1)=SYSCON THEN                                   0077
         CLC   $SYSMN(8,WAPTR),SYSCON                              0077
         BE    @RT00077                                            0077
*         GO TO DEV007;             /* BAD ENTRY,ERROR               */
*       INTAREB=$SYSMN(1);          /* MOVE TO WORK AREA             */
         LA    @14,INTAREA                                         0079
         MVI   INTAREB+8(@14),C' '                                 0079
         MVC   INTAREB(8,@14),$SYSMN(WAPTR)                        0079
*       DO I=1 TO 9;                /* NO. OF CHARS TO SCAN          */
         LA    @14,1                                               0080
         STH   @14,I                                               0080
@DL00080 DS    0H                                                  0081
*         IF INTAREA(I)=' ' THEN    /* SCAN FOR BLANK                */
         LA    @08,INTAREA-1(@14)                                  0081
         CLI   0(@08),C' '                                         0081
         BNE   @RF00081                                            0081
*           DO;                     /* BLANK FOUND                   */
*             INTAREA(I)='/';       /* REPLACE WITH SLASH            */
         LA    @08,INTAREA-1(@14)                                  0083
         MVI   0(@08),C'/'                                         0083
*             GO TO DEV006;         /* DO NORM DECODE                */
         B     DEV006                                              0084
*           END;                                                   0085
*       END;                                                       0086
@RF00081 LA    @14,1                                               0086
         AH    @14,I                                               0086
         STH   @14,I                                               0086
         C     @14,@CF00658                                        0086
         BNH   @DL00080                                            0086
*DEV006:                            /* FOR CT TEST                   */
*       R5=ADDR(INTAREA);           /* INPUT=WORKAREA                */
DEV006   LA    R5,INTAREA                                          0087
*     END;                                                         0088
*DEV007:                                                           0089
