         LA    @14,3                                               0061
         ST    @14,X8                                              0061
*   DO I=3 TO 18 BY 1;              /* CLEAR DEVTABLE                */
         STH   @14,I                                               0062
@DL00062 DS    0H                                                  0063
*     $DEVTBLE(I)=$DEVTBLE(I)&&$DEVTBLE(I);                        0063
         LR    @08,@14                                             0063
         MH    @08,@CH00373                                        0063
         ALR   @08,WAPTR                                           0063
         XC    $DEVTBLE-68(68,@08),$DEVTBLE-68(@08)                0063
*   END;                                                           0064
         AL    @14,@CF00072                                        0064
         STH   @14,I                                               0064
         C     @14,@CF00085                                        0064
         BNH   @DL00062                                            0064
*   IF $DEVERR='1'B THEN            /* PREVIOUS DECODE ERROR         */
         TM    $DEVERR(WAPTR),B'10000000'                          0065
