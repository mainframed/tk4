*           R5=R5-1;                /* PT TO ERROR                   */
DEV072   BCTR  R5,0                                                0170
*           GO TO DEV800;           /* ISSUE 108 MSG                 */
         B     DEV800                                              0171
*         END;                                                     0172
*     END;                                                         0173
@RF00168 DS    0H                                                  0174
*DEV075:                                                           0174
*   CDSINFO(3:4)=WORKC(3:4);        /* MOVE UNIT ADDR TO CONFG       */
@RF00166 DS    0H                                                  0174
DEV075   MVC   CDSINFO+2(2),WORKC+2                                0174
*   CDSINFO(10)='00'X;              /* SET REMOTE FILE FLAG          */
         MVI   CDSINFO+9,X'00'                                     0175
