@RF00075 DS    0H                                                  0089
DEV007   XC    INTFLAGS(1),INTFLAGS                                0089
*DEV008:                                                           0090
*   X7=R5;                          /* SET BEGIN ENTRY PTR           */
DEV008   ST    R5,X7                                               0090
*DEV009:                            /* CHECK FOR TYPE OF ENTRY,UA OR
*                                      SYM NAME                      */
*   IF INPUTBUF(1)>'Z' THEN         /* NUMERIC ENTRY                 */
DEV009   CLI   INPUTBUF(R5),C'Z'                                   0091
         BNH   @RF00091                                            0091
*     DO;                           /* YES,UNIT ADDRESS              */
*       IF SYM='1'B THEN            /* PREVIOUS SYM NME ENTRY        */
         TM    SYM,B'10000000'                                     0093
         BO    @RT00093                                            0093
*         GO TO DEV600;             /* MIXED MODE ERROR              */
*       UNITAD='1'B;                /* SET FLG FOR UNIT ADDR         */
*       DEV='1'B;                   /* SET 2ND FLG FOR UA            */
         OI    UNITAD,B'01010000'                                  0096
