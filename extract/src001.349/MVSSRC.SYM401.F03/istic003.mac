         ST    R13,BACKSAVE+8(,R4)                                 0051
*   SVEIC003(2)=R4;                 /* INTO MY SAVE                  */
         ST    R4,SVEIC003+4(,WAPTR)                               0052
*   R5=ADDR($DTORESP)+1;            /* ADDRESS OF INPUT              */
         LA    R5,$DTORESP(,WAPTR)                                 0053
         AL    R5,@CF00072                                         0053
*   $VALDDEV='0'B;                  /* NO VALID DEVICES YET          */
*   $PARTIAL='0'B;                  /* INIT PARTIAL ERROR FLAG       */
*   $NDR='0'B;                      /* NO DEVICE REQ FLAG            */
         NI    $VALDDEV(WAPTR),B'11010110'                         0056
