@RF00191 LA    @14,1                                               0209
         ALR   @14,R5                                              0209
         ST    @14,X7                                              0209
*DEV082:                            /* PROCESS NIB NAME      @D32JPFR*/
*               R5=R5+1;            /* PT TO FIRST CHAR OF NAME    0210
*                                                            @D32JPFR*/
DEV082   AL    R5,@CF00072                                         0210
*               IF INPUTBUF(1)='$'× /* CHK FOR VALID SYMBOLIC NAME 0211
*                                                            @D32JPFR*/
*                   INPUTBUF(1)='@'×/*                       @D32JPFR*/
*                   INPUTBUF(1)='#'×/*                       @D32JPFR*/
*                   INPUTBUF(1)>='A'&/*                      @D32JPFR*/
