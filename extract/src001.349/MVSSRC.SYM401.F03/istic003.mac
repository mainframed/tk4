         B     DEV012                                              0097
*     END;                                                         0098
*                                                                  0098
*   /*****************************************************************/
*   /*                                                               */
*   /* SYMBOLIC NAME ENTERED                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0099
*   IF UNITAD='1'B THEN             /* PREVIOUS UA ENTERED           */
@RF00091 TM    UNITAD,B'00010000'                                  0099
         BO    @RT00099                                            0099
*     GO TO DEV600;                 /* YES,MIXED MODE ERROR          */
*   SYM='1'B;                       /* SET FLG FOR SYM NM ENT        */
         OI    SYM,B'10000000'                                     0101
*   IF DASH='1'B×                   /* INCLUSIVE SYM NAMES,ERROR     */
*       DEV='1'B THEN               /* CHAR IN SYM NAME              */
         TM    DASH,B'01100000'                                    0102
         BNZ   @RT00102                                            0102
*     GO TO DEV800;                 /* INCL RANGE NOT ALLOWED        */
*DEV010:                                                           0104
*   IF INPUTBUF(1)='$'×             /* CHK FOR VALID                 */
