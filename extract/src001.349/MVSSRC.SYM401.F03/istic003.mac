DEV010   CLI   INPUTBUF(R5),C'$'                                   0104
         BE    @RT00104                                            0104
         CLI   INPUTBUF(R5),C'@'                                   0104
         BE    @RT00104                                            0104
         CLI   INPUTBUF(R5),C'#'                                   0104
         BE    @RT00104                                            0104
         CLI   INPUTBUF(R5),C'A'                                   0104
         BL    @RF00104                                            0104
@RT00104 DS    0H                                                  0105
*     DO;                                                          0105
*       GO TO DEV012;               /* CONTINUE DECODING             */
         B     DEV012                                              0106
*     END;                                                         0107
*   ELSE                                                           0108
*     DO;                                                          0108
@RF00104 DS    0H                                                  0109
*       GO TO DEV800;               /* INVALID CHAR                  */
         B     DEV800                                              0109
*     END;                                                         0110
*DEV012:                                                           0111
*   R5=R5+1;                        /* INCR INPUT PTR                */
DEV012   AL    R5,@CF00072                                         0111
*   IF INPUTBUF(1)='(' THEN         /* SESSPARM SPECIFIED    @D32JPFR*/
         CLI   INPUTBUF(R5),C'('                                   0112
         BE    @RT00112                                            0112
*     GOTO DEV030;                  /* YES,CONTINUE DECODING @D32JPFR*/
*   IF INPUTBUF(1)<'-' THEN         /* CHECK FOR DASH                */
         CLI   INPUTBUF(R5),C'-'                                   0114
         BL    @RT00114                                            0114
*     GO TO DEV020;                 /* NO                            */
*   IF INPUTBUF(1)<=',' THEN        /* CHECK FOR COMMA               */
         CLI   INPUTBUF(R5),C','                                   0116
         BNH   @RT00116                                            0116
*     GO TO DEV030;                 /* BRANCH IF DELIMITER           */
*DEV020:                                                           0118
*   IF R5=X7+8 THEN                 /* CHK FOR ENTRY> 8 CHARS        */
DEV020   LA    @14,8                                               0118
         AL    @14,X7                                              0118
         CR    R5,@14                                              0118
         BE    @RT00118                                            0118
*     GO TO DEV800;                 /* YES,ERROR                     */
*   GO TO DEV010;                   /* CONTINUE DECODING             */
         B     DEV010                                              0120
*DEV030:                            /* CHECK FOR VALID SIZE OF ENTRY */
*   DEV='0'B;                       /* RESET 2ND UA FLAG             */
DEV030   NI    DEV,B'10111111'                                     0121
*   X10=R5-X7;                      /* COMPUTE ENTRY SIZE            */
         LR    @14,R5                                              0122
         SL    @14,X7                                              0122
         ST    @14,X10                                             0122
*   IF SYM='1'B THEN                /* SYMBOLIC NAME ?               */
         TM    SYM,B'10000000'                                     0123
         BO    @RT00123                                            0123
*     GO TO DEV040;                 /* GO SET UP FOR CONFIG          */
*   IF X10>4×                       /* VALID SIZE FOR UNIT           */
*       X10<3 THEN                  /* ADDRESS                       */
         C     @14,@CF00068                                        0125
         BH    @RT00125                                            0125
         C     @14,@CF00081                                        0125
         BL    @RT00125                                            0125
*     GO TO DEV072;                 /* NO,ERROR                      */
*   WORKB=EBCF0;                    /* INIT WORK AREA                */
         MVC   WORKB(8),EBCF0                                      0127
*   N=8;                            /* MAX UNIT ADDR SIZE            */
         MVC   N(2),@CH00054                                       0128
*   DO I=X10 TO 1 BY-1;             /* MOVE UNIT ADDRESS             */
         STH   @14,I                                               0129
         B     @DE00129                                            0129
@DL00129 DS    0H                                                  0130
*     WORKB(N)=INPUT(I);            /* INTO WORKAREA                 */
         LH    @08,N                                               0130
         LA    @11,WORKB-1(@08)                                    0130
         L     @10,X7                                              0130
         ALR   @10,@14                                             0130
         BCTR  @10,0                                               0130
         MVC   0(1,@11),INPUT(@10)                                 0130
*     N=N-1;                        /* DECR INDEX                    */
         BCTR  @08,0                                               0131
         STH   @08,N                                               0131
*   END;                                                           0132
         BCTR  @14,0                                               0132
         STH   @14,I                                               0132
@DE00129 LTR   @14,@14                                             0132
         BP    @DL00129                                            0132
*   IF DASH='0'B THEN               /* IF DASH HAS NOT BEEN          */
         TM    DASH,B'00100000'                                    0133
         BNZ   @RF00133                                            0133
*     DO;                           /* ENCOUNTERED MOVE UNIT         */
*       WORKA=WORKB;                /* ADDR INTO BEGIN OF            */
         MVC   WORKA(8),WORKB                                      0135
