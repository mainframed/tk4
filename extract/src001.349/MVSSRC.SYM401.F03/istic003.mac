*   CALL ISTSC030;                  /* ACQUISITION MODULE            */
         L     @15,@CV00743                                        0178
         BALR  @14,@15                                             0178
*   IF R15^=0 THEN                  /* ERROR ?                       */
         LTR   R15,R15                                             0179
         BZ    @RF00179                                            0179
*     DO;                           /* YES                           */
*       SKIPENT='1'B;               /* BAD ENTRY                     */
         OI    SKIPENT,B'00001000'                                 0181
*       $PARTIAL='1'B;              /* SET PARTIAL ERROR FLAG        */
         OI    $PARTIAL(WAPTR),B'00000001'                         0182
*       GO TO DEV170;               /* SKIP THIS ENTRY               */
         B     DEV170                                              0183
*     END;                                                         0184
*   IF NOCDS='0'B THEN                                             0185
@RF00179 TM    NOCDS,B'00000100'                                   0185
         BNZ   @RF00185                                            0185
*     DO;                           /* CDS WAS FOUND         @D32JPFR*/
*       IF INPUTBUF(1)='(' THEN     /* SESSPARM SPECIFIED    @D32JPFR*/
         CLI   INPUTBUF(R5),C'('                                   0187
         BNE   @RF00187                                            0187
*         DO;                       /* YES                   @D32JPFR*/
*           IF DEVLU='1'B THEN      /* IS DEV AN LU          @D32JPFR*/
         TM    DEVLU+2376(WAPTR),B'00001000'                       0189
         BNO   @RF00189                                            0189
*             DO;                   /* YES                   @D32JPFR*/
*               IF INPUTBUF(2:3)='X''' THEN/* BIND PARMS SPECIFIED 0191
*                                                            @ZD10882*/
         CLC   INPUTBUF+1(2,R5),@CC00745                           0191
         BNE   @RF00191                                            0191
*                 DO;               /* YES                   @D32JPFR*/
*                   R5=R5+15;       /* PT TO MATCHING QUOTE  @D32JPFR*/
         AL    R5,@CF00058                                         0193
*                   IF INPUTBUF(1)^='''' THEN/* IS QUOTE THERE     0194
*                                                            @D32JPFR*/
         CLI   INPUTBUF(R5),C''''                                  0194
         BNE   @RT00194                                            0194
*                     GOTO DEV800;  /* NO,MUST CONTAIN 12 CHARS    0195
*                                                            @D32JPFR*/
*                   X7=R5-12;       /* PT TO FIRST CHAR OF INPUT   0196
*                                                            @D32JPFR*/
*                                                                  0196
         LR    @14,R5                                              0196
         SL    @14,@CF00134                                        0196
         ST    @14,X7                                              0196
*                   /*************************************************/
*                   /*                                               */
*                   /* ITACONV                                       */
*                   /* FROM(INPUT)TO($DEVSESP(X8))TYPE(EH)COUNT(12)  */
*                   /* INVOKED                                       */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0197
