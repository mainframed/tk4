@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*   R4=R13;                         /* CALLERS SAVE AREA             */
         LR    R4,R13                                              0049
*   R13=ADDR(SVEIC003);             /* GET ADDR OF MY SAVE           */
         LA    R13,SVEIC003(,WAPTR)                                0050
