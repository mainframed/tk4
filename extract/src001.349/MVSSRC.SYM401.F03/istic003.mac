         TITLE 'ISTIC003-DEVICE DECODE                                 *
                        '
ISTIC003 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ISTIC003  75.184'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
