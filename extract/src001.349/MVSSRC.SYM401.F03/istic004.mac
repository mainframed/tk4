         MVI   $INBUFR+1(WAPTR),C' '                               0066
         MVC   $INBUFR+2(6,WAPTR),$INBUFR+1(WAPTR)                 0066
         MVI   $INBUFR(WAPTR),C' '                                 0066
*   DO I=8 TO 1 BY-1;               /* SCAN INPUT                    */
         LA    I,8                                                 0067
@DL00067 DS    0H                                                  0068
*     $INBUFR(I)=INPUTBUF(1);       /* MOVE ERROR IN MSG             */
         LR    @15,WAPTR                                           0068
         ALR   @15,I                                               0068
         MVC   $INBUFR-1(1,@15),INPUTBUF(R5)                       0068
*     IF R9=R5 THEN                 /* END OF FIELD ?                */
         CR    R9,R5                                               0069
         BE    @RT00069                                            0069
*       GO TO CECTST05;             /* GO WRITE MSG                  */
*     R5=R5-1;                      /* DECR INPUT PTR                */
         BCTR  R5,0                                                0071
