         LA    R8,$SECLST(,WAPTR)                                  0099
*   $FAMILY(1)='T';                 /* ENTER T AS DEFAULT            */
         MVI   $FAMILY(WAPTR),C'T'                                 0100
*   IF INPUTBUF(1)='T'×             /* IF                            */
*       INPUTBUF(1)='P'×            /* TEST PREFIX                   */
*       INPUTBUF(1)='N'×            /* IS VALID                      */
*       INPUTBUF(1)='R' THEN        /* MOVE IT                       */
         CLI   INPUTBUF(R5),C'T'                                   0101
         BE    @RT00101                                            0101
         CLI   INPUTBUF(R5),C'P'                                   0101
         BE    @RT00101                                            0101
         CLI   INPUTBUF(R5),C'N'                                   0101
         BE    @RT00101                                            0101
         CLI   INPUTBUF(R5),C'R'                                   0101
         BNE   @RF00101                                            0101
@RT00101 DS    0H                                                  0102
