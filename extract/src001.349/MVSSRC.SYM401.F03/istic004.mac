@RF00101 DS    0H                                                  0106
CECTST11 LA    @15,1                                               0106
         STH   @15,J                                               0106
@DL00106 DS    0H                                                  0107
*     R6=ADDR(NUMBERS);             /* ID NUMERIC. NO, SET           */
         LA    R6,NUMBERS                                          0107
