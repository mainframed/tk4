         MVC   @TS00001(4),$FAMILY(WAPTR)                          0089
         MVC   $FAMILY+1(4,WAPTR),@TS00001                         0089
*   $VALDTST='0'B;                  /* INIT VALID TEST BIT           */
         NI    $VALDTST(WAPTR),B'11111011'                         0090
