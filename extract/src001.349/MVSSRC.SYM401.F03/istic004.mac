         XR    @00,@15                                             0086
         STH   @00,$TSTCNT(,WAPTR)                                 0086
*   $RTNSLCT='0'B;                  /* INIT RTN SELECT BIT           */
         NI    $RTNSLCT(WAPTR),B'11011111'                         0087
