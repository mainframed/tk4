         BCTR  I,0                                                 0112
         LTR   I,I                                                 0112
         BP    @DL00108                                            0112
*     R5=R5+J-1;                    /* PT TO ERROR                   */
         LR    @15,R5                                              0113
         AH    @15,J                                               0113
