         LA    I,10                                                0108
@DL00108 DS    0H                                                  0109
*       IF INPUTBUF(J)=NUMBER THEN  /* AND PRINT ERROR               */
         LH    @15,J                                               0109
         ALR   @15,R5                                              0109
