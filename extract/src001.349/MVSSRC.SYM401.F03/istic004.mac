         LA    I,1                                                 0095
@DL00095 DS    0H                                                  0096
*     $SECLST(I)=SECINIT;                                          0096
         LR    @15,I                                               0096
         SLA   @15,2                                               0096
         ALR   @15,WAPTR                                           0096
         MVC   $SECLST-4(4,@15),SECINIT                            0096
*   END;                                                           0097
         AL    I,@CF00072                                          0097
         C     I,@CF00490                                          0097
         BNH   @DL00095                                            0097
*   SECCNTR=0;                                                     0098
         SLR   @15,@15                                             0098
         ST    @15,SECCNTR                                         0098
