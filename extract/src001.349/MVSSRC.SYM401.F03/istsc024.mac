*     GOTO S24C3012;                /* GO SEARCH SENSE ENTRIES       */
*   IF TECBEVOC=0                   /* IF NO EVENTS HAVE BEEN        */
*     THEN                          /* POSTED,                       */
         CLI   TECBEVOC(TECBPTR),0                                 0515
         BNE   @RF00515                                            0515
*     DO;                           /*                               */
*S24C3003:                                                         0517
