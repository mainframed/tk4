@RC00592 TM    MASK,B'00100000'                                    0598
         BNO   @RF00598                                            0598
*     DO;                                                          0599
*       CSWSTAT=WORKCSW(1:LENGTH(CSWSTAT));                        0600
         MVC   CSWSTAT(4,BFRPTR),WORKCSW(WORKPTR)                  0600
*       WORKPTR=WORKPTR+LENGTH(CSWSTAT);                           0601
         AL    WORKPTR,@CF00041                                    0601
*     END;                                                         0602
*   ELSE                                                           0603
*     CSWSTAT=                      /* IF NOT USE X'S                */
