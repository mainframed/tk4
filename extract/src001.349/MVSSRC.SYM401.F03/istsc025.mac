         TITLE 'ISTSC025 - INITIAL SEND/RECEIVE INTERFACE              *
                        '
*  IBM DISK OPERATING SYSTEM
*  TOLTEP
          ISEQ 73,80
ISTSC025 CSECT ,                                                   0002
@MAINENT DS    0H                                                  0002
         USING *,@15                                               0002
         B     @PROLOG                                             0002
         DC    AL2(@EP00002-@MAINENT)
         DC    AL1(16)                                             0002
         DC    C'ISTSC025  76.244'                                 0002
S25C2000 DS    0H                                                  0002
         USING *,@15                                               0002
         B     @PROLOG                                             0002
         DC    AL2(@EP00291-S25C2000)
         ENTRY S25C2000
         DROP  @15
@PROLOG  ST    @14,12(,@13)                                        0002
         STM   @00,@12,20(@13)                                     0002
         BALR  @12,0                                               0002
@PSTART  DS    0H                                                  0002
         USING @PSTART,@12                                         0002
         AH    @15,4(,@15)                                         0002
         BR    @15                                                 0002
@EP00002 DS    0H                                                  0003
*                                                                  0117
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY FROM RCM                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0118
*   R14=R13;                        /* CALLER SAVEAREA PTR           */
         LR    R14,R13                                             0118
*   R13=ADDR(SVESC025);             /* PT TO MY SAVEAREA             */
         LA    R13,SVESC025(,WAPTR)                                0119
*   BACKSAVE(3)=R13;                /* PUT MY SAVE PTR IN CALLERS    */
         STCM  R13,15,BACKSAVE+8(R14)                              0120
*   SVESC025(2)=R14;                /* SAVE CALLER SAVE PTR          */
         ST    R14,SVESC025+4(,WAPTR)                              0121
*   SENDPTR=R1;                     /* BASE FOR SEND PARM LIST       */
         LR    SENDPTR,R1                                          0122
*   RFY                                                            0123
*    (R1,                                                          0123
*     R14) UNRSTD;                                                 0123
*   RFY                                                            0124
*     SENDMAP BASED(SENDPTR);       /* SET BASE                      */
*   RFY                                                            0125
*     IFGRPL BASED(ADDR($TLTRPL3)); /* SEND-RECEIVE RPL              */
*   RFY                                                            0126
*     CDSMAP BASED(CDSPTR);         /* BASE CDS MAP                  */
*   IF $SENDCDS=0 THEN              /* IF CDS PTR NOT GIVEN          */
         L     @14,$SENDCDS(,SENDPTR)                              0127
         LTR   @14,@14                                             0127
         BNZ   @RF00127                                            0127
*     CDSPTR=ADDR($PDEVADR);        /* USE PRIMARY DEVICE            */
         LA    CDSPTR,$PDEVADR(,WAPTR)                             0128
*   ELSE                                                           0129
*     CDSPTR=$SENDCDS;              /* ELSE USE DEVICE GIVEN         */
*                                                                  0129
         B     @RC00127                                            0129
@RF00127 L     CDSPTR,$SENDCDS(,SENDPTR)                           0129
*   /*****************************************************************/
*   /*                                                               */
*   /* SEARCH DEVICE TABLE TO INSURE DEVICE IS PRIMARY OR GRABBED,   */
*   /* AND NOT LETGO, AND IS ACQUIRED                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0130
*   DO I=3 TO $DEVCNTS;             /* SEARCH DEVICE TABLE           */
@RC00127 LA    I,3                                                 0130
         B     @DE00130                                            0130
@DL00130 DS    0H                                                  0131
*     IF CDSSYMB=ON THEN            /* IF DEV HAS SYM NAME IN CDS    */
         TM    CDSSYMB(CDSPTR),B'00001000'                         0131
         BNO   @RF00131                                            0131
*       DO;                                                        0132
*         IF CDSSYMBN=$$SYMNME(I)   /* MATCH                         */
*           THEN                                                   0133
         LR    @14,I                                               0133
         MH    @14,@CH00373                                        0133
         ALR   @14,WAPTR                                           0133
         CLC   CDSSYMBN(8,CDSPTR),$$SYMNME-68(@14)                 0133
         BE    @RT00133                                            0133
*           GOTO S25C0100;          /* CHK FOR VALID DEV             */
*         ELSE                                                     0135
*           GOTO S25C0200;          /* CHK NEXT DEV                  */
         B     S25C0200                                            0135
*       END;                                                       0136
*     ELSE                                                         0137
*       DO;                                                        0137
@RF00131 DS    0H                                                  0138
*         IF CDSUA^=$DEVUNAD(I)     /* DO UNIT ADDRS MATCH           */
*           THEN                                                   0138
         LR    @14,I                                               0138
         MH    @14,@CH00373                                        0138
         L     @11,CDSUA(,CDSPTR)                                  0138
         CL    @11,$DEVUNAD-68(@14,WAPTR)                          0138
         BNE   @RT00138                                            0138
*           GOTO S25C0200;          /* NO,CHK NEXT DEVICE            */
*       END;                                                       0140
*S25C0100:                                                         0141
*     IF $DEVLU(I)=OFF THEN         /* IF DEVICE IS NOT AN LU        */
S25C0100 LR    @14,I                                               0141
         MH    @14,@CH00373                                        0141
         ALR   @14,WAPTR                                           0141
         TM    $DEVLU-68(@14),B'00001000'                          0141
         BNZ   @RF00141                                            0141
*       DO;                                                        0142
*         R15=4;                    /* SET ERROR RET CODE            */
         LA    R15,4                                               0143
*         GOTO S25C9999;            /* RETURN                        */
         B     S25C9999                                            0144
*       END;                                                       0145
*     IF($PRIMARY(I)=ON×            /* DEV MUST BE PRIMARY OR        */
*         ($GRABBED(I)=ON&          /* GRABBED AND                   */
*         $LETGO(I)=OFF))&          /* NOT LETGO                     */
*         $ACQRED(I)=ON THEN        /* AND ACQUIRED                  */
@RF00141 LR    @14,I                                               0146
         MH    @14,@CH00373                                        0146
         ST    @14,@TF00001                                        0146
         ALR   @14,WAPTR                                           0146
         TM    $PRIMARY-68(@14),B'00000001'                        0146
         BO    @GL00001                                            0146
         LR    @14,WAPTR                                           0146
         AL    @14,@TF00001                                        0146
         TM    $GRABBED-68(@14),B'00100000'                        0146
         BNO   @RF00146                                            0146
         TM    $LETGO-68(@14),B'10000000'                          0146
         BNZ   @RF00146                                            0146
@GL00001 LR    @14,I                                               0146
         MH    @14,@CH00373                                        0146
         ALR   @14,WAPTR                                           0146
         TM    $ACQRED-68(@14),B'01000000'                         0146
         BO    @RT00146                                            0146
*       GOTO S25C0300;              /* O.K. TO CONTINUE              */
*     ELSE                                                         0148
*       GOTO S25C0250;              /* DEV DOES NOT MEET REQS        */
         B     S25C0250                                            0148
*S25C0200:                                                         0149
*   END;                                                           0149
S25C0200 AL    I,@CF00072                                          0149
@DE00130 CH    I,$DEVCNTS(,WAPTR)                                  0149
         BNH   @DL00130                                            0149
*S25C0250:                                                         0150
*   R15=8;                          /* RETURN CODE=8                 */
S25C0250 LA    R15,8                                               0150
*   GOTO S25C9999;                                                 0151
*                                                                  0151
         B     S25C9999                                            0151
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR CONTROL TERMINAL = TEST TERMINAL                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0152
*S25C0300:                                                         0152
*   $SNDRFLG=ON;                    /* TURN RCM RECALL FLAG ON       */
S25C0300 OI    $SNDRFLG(WAPTR),B'00000100'                         0152
*   IF $CID(I)=$CID(1) THEN         /* IF CT=TT THEN                 */
         LR    @14,I                                               0153
         MH    @14,@CH00373                                        0153
         ST    @14,@TF00001                                        0153
         ALR   @14,WAPTR                                           0153
         CLC   $CID-68(4,@14),$CID(WAPTR)                          0153
         BNE   @RF00153                                            0153
*     DO;                                                          0154
*       IF $MSGOTSD(1)^=0×          /* API MSGS OUTSTANDING          */
*           $CLRSNT(I)=ON THEN      /* OR CLEAR HAS BEEN SENT        */
         L     @14,$MSGOTSD(,WAPTR)                                0155
         LTR   @14,@14                                             0155
         BNZ   @RT00155                                            0155
         LR    @14,WAPTR                                           0155
         AL    @14,@TF00001                                        0155
         TM    $CLRSNT-68(@14),B'00000010'                         0155
         BO    @RT00155                                            0155
*         GOTO S25C9999;            /* RETURN TO RCM                 */
*       $CTLINE=0;                  /* TELL ISTCC016 SCREEN        0157
*                                      UNPREDICTABLE                 */
         MVI   $CTLINE(WAPTR),X'00'                                0157
*     END;                                                         0158
*   IF $SRTEST(I)=OFF THEN          /* HAS A CLEAR SDT BEEN SENT     */
@RF00153 LR    @14,I                                               0159
         MH    @14,@CH00373                                        0159
         ALR   @14,WAPTR                                           0159
         TM    $SRTEST-68(@14),B'00001000'                         0159
         BNZ   @RF00159                                            0159
*     DO;                                                          0160
*       RFY                                                        0161
*         R1 RSTD;                                                 0161
*       R1=ADDR($TLTRPL3);          /* NO,GET ADDRESS OF S-R RPL     */
         LA    R1,$TLTRPL3(,WAPTR)                                 0162
*       CALL S25C1000;              /* ISSUE CLEAR SDT               */
         BAL   @14,S25C1000                                        0163
*       RFY                                                        0164
*         R1 UNRSTD;                                               0164
*       GOTO S25C9999;              /* RETURN TO RCM                 */
         B     S25C9999                                            0165
*     END;                                                         0166
*                                                                  0166
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR PERMANENT ERROR ON CLEAR COMMAND                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0167
*   IF $CATASTR(I)=ON THEN          /* PERMANENT ERROR ?             */
@RF00159 LR    @14,I                                               0167
         MH    @14,@CH00373                                        0167
         ST    @14,@TF00001                                        0167
         ALR   @14,WAPTR                                           0167
         TM    $CATASTR-68(@14),B'01000000'                        0167
         BNO   @RF00167                                            0167
*     DO;                           /* YES                           */
*       $CATASTR(I)=OFF;            /* RESET ERROR FLAG              */
         LR    @14,WAPTR                                           0169
         AL    @14,@TF00001                                        0169
         NI    $CATASTR-68(@14),B'10111111'                        0169
*       $SNDRFLG=OFF;               /* DONT RECALL           @ZD10888*/
         NI    $SNDRFLG(WAPTR),B'11111011'                         0170
*       R15=8;                      /* PERMANENT ERROR RET CODE      */
         LA    R15,8                                               0171
*       GOTO S25C9999;              /* RETURN                        */
         B     S25C9999                                            0172
*     END;                                                         0173
*   $SNDRFLG=OFF;                   /* SET RCM RECALL FLAG OFF       */
*                                                                  0174
@RF00167 NI    $SNDRFLG(WAPTR),B'11111011'                         0174
*   /*****************************************************************/
*   /*                                                               */
*   /* HAS API SEND A MSG DURING TEST MODE                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0175
*   IF $OLTERR(I)=ON THEN           /* HAS API SENT A MSG            */
         LR    @14,I                                               0175
         MH    @14,@CH00373                                        0175
         ST    @14,@TF00001                                        0175
         ALR   @14,WAPTR                                           0175
         TM    $OLTERR-68(@14),B'00000100'                         0175
         BNO   @RF00175                                            0175
*     DO;                           /* YES,BRACKETING MAY BE UPSET   */
*       $OLTERR(I)=OFF;             /* RESET FLAG                    */
         LR    @14,WAPTR                                           0177
         AL    @14,@TF00001                                        0177
         NI    $OLTERR-68(@14),B'11111011'                         0177
*       R15=20;                     /* RET CODE FOR OLT TO REISSUE   */
         LA    R15,20                                              0178
*       GOTO S25C9999;                                             0179
         B     S25C9999                                            0179
*     END;                                                         0180
*                                                                  0180
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE IF THIS IS SEND OR RECEIVE REQUEST                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0181
*   IF $SENDMID='33' THEN           /* RECEIVE REQUEST               */
@RF00175 CLC   $SENDMID(2,SENDPTR),@CC01234                        0181
         BNE   @RF00181                                            0181
*     DO;                           /* YES                           */
*       RFY                                                        0183
*        (R1,                                                      0183
*         R3) RSTD;                                                0183
*       R3=I;                       /* DEVICE TABLE INDEX            */
         LR    R3,I                                                0184
*       R1=SENDPTR;                 /* PARM LIST PTR                 */
         LR    R1,SENDPTR                                          0185
*       CALL ISTSC033;              /* CALL RECEIVE                  */
         L     @15,@CV01235                                        0186
         BALR  @14,@15                                             0186
*       RFY                                                        0187
*        (R1,                                                      0187
*         R3) UNRSTD;                                              0187
*       GOTO S25C9999;              /* RETURN TO RCM                 */
         B     S25C9999                                            0188
*     END;                                                         0189
*                                                                  0189
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS SEND REQUEST                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0190
*   TCBSNCNT=9;                     /* NUMBER OF SENSE FIELD         */
@RF00181 LA    TCBSNCNT,9                                          0190
*   $INBUFR(1:TECBDLEN)=TECBDFLT(1:TECBDLEN);/* MOVE DEFLT TECB      */
         L     @14,TECBDLEN                                        0191
         BCTR  @14,0                                               0191
         EX    @14,@SM01269                                        0191
*   IF $SENDTCB^=0 THEN             /* OLT SPECIFY TECB              */
         L     @14,$SENDTCB(,SENDPTR)                              0192
         LTR   @14,@14                                             0192
         BZ    @RF00192                                            0192
*     DO;                                                          0193
*       RFY                                                        0194
*         TECBMAPS BASED($SENDTCB); /* POINT TO OLT'S TECB           */
*       IF TECBSNCT^=0&             /* IF SENSE FIELD COUNT          */
*           TECBSNOC^=0 THEN        /* AND SENSES OCCURRED^=0        */
         CLI   TECBSNCT(@14),0                                     0195
         BE    @RF00195                                            0195
         CLI   TECBSNOC(@14),0                                     0195
         BE    @RF00195                                            0195
*         DO;                                                      0196
*           $INBUFR(1:15)=TCBSENSE(1:25);/* THEN MOVE IN OLT SNS     */
         LA    @11,TECBEVT1(,@14)                                  0197
         SLR   @01,@01                                             0197
         IC    @01,TECBFDCT(,@14)                                  0197
         SLR   @08,@08                                             0197
         IC    @08,TECBFDLN(,@14)                                  0197
         MR    @00,@08                                             0197
         ALR   @11,@01                                             0197
         SLR   @08,@08                                             0197
         IC    @08,TECBSNOC(,@14)                                  0197
         MH    @08,TECBSNLN(,@14)                                  0197
         ALR   @11,@08                                             0197
         MVC   $INBUFR(15,WAPTR),TCBSENSE(@11)                     0197
*           TCBSNCNT=TECBSNCT-3;    /* AND SENSE FIELD COUNT         */
         SLR   TCBSNCNT,TCBSNCNT                                   0198
         IC    TCBSNCNT,TECBSNCT(,@14)                             0198
         SL    TCBSNCNT,@CF00081                                   0198
*         END;                                                     0199
*     END;                                                         0200
*                                                                  0200
@RF00195 DS    0H                                                  0201
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD RPL FROM TECB AND PARMLIST                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0201
*   RFY                                                            0201
*     TECBSEND BASED(ADDR($INBUFR));/* MAP INBUFR                    */
@RF00192 DS    0H                                                  0202
*   TECBSEND(1:25)=TECBSEND(1:25)&  /* MASK OUT UNUSED BITS          */
*       TECBMASK(1:25);             /* IN TECB                       */
         NC    TECBSEND+2104(25,WAPTR),TECBMASK                    0202
*   RPLVTFL2=RPLVTFL2&MSKRESP;      /* MASK OUT RESPOND FLD          */
         LA    @14,1872                                            0203
         ALR   @14,WAPTR                                           0203
         NI    RPLVTFL2(@14),X'F8'                                 0203
*   RPLRLEN=0;                      /* MASK OUT RECLEN               */
         SLR   @11,@11                                             0204
         ST    @11,RPLRLEN+1856(,WAPTR)                            0204
*   RPLSRTYP=RPLSRTYP&MSKRTYP;      /* MASK OUT RTYPE                */
         NI    RPLSRTYP(@14),X'71'                                 0205
*   RPLRH3=RPLRH3&MSKBRCHG;         /* MSK BRACKT,CHDIR,CDSEL        */
         NI    RPLRH3(@14),X'07'                                   0206
*   IF $SENDBB=ON THEN              /* BEGIN BRACKET                 */
         TM    $SENDBB(SENDPTR),B'10000000'                        0207
         BNO   @RF00207                                            0207
*     RPLBB=ON;                     /* YES SET ON IN RPL             */
         OI    RPLBB(@14),B'10000000'                              0208
*   IF $SENDEB=ON THEN              /* END BRACKET                   */
@RF00207 TM    $SENDEB(SENDPTR),B'01000000'                        0209
         BNO   @RF00209                                            0209
*     RPLEB=ON;                     /* YES SET ON IN RPL             */
         OI    RPLEB+1872(WAPTR),B'01000000'                       0210
*   IF $SENDFME=OFF THEN            /* FINAL DESTINATION RESPONSE    */
@RF00209 TM    $SENDFME(SENDPTR),B'00100000'                       0211
         BNZ   @RF00211                                            0211
*     RPLNFME=ON;                   /* NO, SET IN RPL                */
         OI    RPLNFME+1872(WAPTR),B'00000010'                     0212
*   IF $SENDEX=ON THEN              /* EXCEPTION RESPONSE            */
@RF00211 TM    $SENDEX(SENDPTR),B'00010000'                        0213
         BNO   @RF00213                                            0213
*     RPLEX=ON;                     /* YES, SET IN RPL               */
         OI    RPLEX+1872(WAPTR),B'00000100'                       0214
*   IF $SENDRRN=ON THEN             /* RECOVERY EXCEPTED             */
@RF00213 TM    $SENDRRN(SENDPTR),B'00001000'                       0215
         BNO   @RF00215                                            0215
*     RPLRRN=ON;                    /* YES, SET IN RPL               */
         OI    RPLRRN+1872(WAPTR),B'00000001'                      0216
*   IF $SENDCSL=ON THEN             /* CODE SELECT=ALT               */
@RF00215 TM    $SENDCSL(SENDPTR),B'00000100'                       0217
         BNO   @RF00217                                            0217
*     RPLCSI=ON;                    /* YES, SET IN RPL               */
         OI    RPLCSI+1872(WAPTR),B'00001000'                      0218
*   IF $SENDSTP=ON THEN             /* STYPE = RESP                  */
@RF00217 TM    $SENDSTP(SENDPTR),B'00000010'                       0219
         BNO   @RF00219                                            0219
*     RPLSRESP=ON;                  /* YES, SET IN RPL               */
         OI    RPLSRESP+1872(WAPTR),B'10000000'                    0220
*   IF $SENDFMH=OFF THEN            /* OPTCD=FMHDR           @AX11982*/
@RF00219 TM    $SENDFMH(SENDPTR),B'00000001'                       0221
         BNZ   @RF00221                                            0221
*     RPLFMHDR=ON;                  /* YES, SET IN RPL               */
         OI    RPLFMHDR+1856(WAPTR),B'00000001'                    0222
*   IF $SENDCMD=OFF THEN            /* CHNG DIRECTION COMMAND      0223
*                                                            @DA04694*/
@RF00221 TM    $SENDCMD(SENDPTR),B'10000000'                       0223
         BNZ   @RF00223                                            0223
*     RPLCMD=ON;                    /* YES, SET IN RPL               */
         OI    RPLCMD+1872(WAPTR),B'00100000'                      0224
*   IF $SENDREQ=OFF THEN            /* REQUEST CHANGE DIRECTION    0225
*                                                            @DA04694*/
@RF00223 TM    $SENDREQ(SENDPTR),B'01000000'                       0225
         BNZ   @RF00225                                            0225
*     RPLCHREQ=ON;                  /* YES, SET IN RPL               */
*                                                                  0226
         OI    RPLCHREQ+1872(WAPTR),B'00010000'                    0226
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE BY TECB FIELD COUNT WHAT TO UPDATE IN RPL           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0227
*   IF TCBSNCNT>=1 THEN             /* 1 OR MORE FIELDS?             */
@RF00225 C     TCBSNCNT,@CF00072                                   0227
         BL    @RF00227                                            0227
*     RPLSSEO=TCBSSEN;              /* YES, SYSTEM SENSE TO RPL      */
         MVC   RPLSSEO+1856(1,WAPTR),TCBSSEN+2104(WAPTR)           0228
*   IF TCBSNCNT>=2 THEN             /* 2 OR MORE FIELDS?             */
@RF00227 C     TCBSNCNT,@CF00137                                   0229
         BL    @RF00229                                            0229
*     RPLSSMO=TCBSSENM;             /* YES, SYS SENSE MOD TO RPL     */
         IC    @14,TCBSSENM+2104(,WAPTR)                           0230
         STC   @14,RPLSSMO+1856(,WAPTR)                            0230
*   IF TCBSNCNT>=3 THEN             /* 3 OR MORE FIELDS?             */
@RF00229 C     TCBSNCNT,@CF00081                                   0231
         BL    @RF00231                                            0231
*     RPLUSNSO=TCBUSEN;             /* YES, USER SENSE TO RPL        */
         MVC   RPLUSNSO+1856(2,WAPTR),TCBUSEN+2104(WAPTR)          0232
*   IF TCBSNCNT>=8 THEN             /* 8 OR MORE FIELDS?             */
@RF00231 C     TCBSNCNT,@CF00055                                   0233
         BL    @RF00233                                            0233
*     DO;                           /* YES                           */
*       RPLCHN=RPLCHN&MSKCHAIN;     /* N USED FLAGS IN RPL           */
         LA    @14,1872                                            0235
         ALR   @14,WAPTR                                           0235
         NI    RPLCHN(@14),X'0F'                                   0235
*       RPLCHN=RPLCHN×TCBCHAIN;     /* OR CHAIN FLG IN RPL           */
         OC    RPLCHN(1,@14),TCBCHAIN+2104(WAPTR)                  0236
*     END;                                                         0237
*   IF TCBSNCNT>=9 THEN             /* 9 OR MORE FIELDS?             */
@RF00233 C     TCBSNCNT,@CF00815                                   0238
         BL    @RF00238                                            0238
*     DO;                           /* YES                           */
*       RPLCNTRL=RPLCNTRL&MSKCNTRL; /* N USED FLAGS IN RPL           */
         LA    @14,1872                                            0240
         ALR   @14,WAPTR                                           0240
         NC    RPLCNTRL(3,@14),@CB01215                            0240
*       RPLCNTRL=RPLCNTRL×TCBCNTRL; /* OR CNTRL FLGS IN RPL          */
         OC    RPLCNTRL(3,@14),TCBCNTRL+2104(WAPTR)                0241
*     END;                                                         0242
*   IF TCBSNCNT>=10 THEN            /* 10 OR MORE FIELDS             */
@RF00238 C     TCBSNCNT,@CF01173                                   0243
         BL    @RF00243                                            0243
*     RPLSEQNO=TCBSEQNO;            /* YES, MOVE SEQ NO. TO RPL      */
         MVC   RPLSEQNO+1916(2,WAPTR),TCBSEQNO+2104(WAPTR)         0244
*   IF TCBSNCNT>=11 THEN            /* 11 OR MORE FIELDS             */
@RF00243 C     TCBSNCNT,@CF00771                                   0245
         BL    @RF00245                                            0245
*     RPLSIGDA=TCBSIGDA;            /* YES,MOVE SIGNAL DAT TO RPL    */
         MVC   RPLSIGDA+1856(4,WAPTR),TCBSIGDA+2104(WAPTR)         0246
*   IF TCBSNCNT>=12 THEN            /* 12 OR MORE FIELDS             */
@RF00245 C     TCBSNCNT,@CF00134                                   0247
         BL    @RF00247                                            0247
*     DO;                           /* YES                           */
*       RPLIBSQ=RPLIBSQ&MSKIBSQ;    /* N IBSQ FIELD IN RPL           */
         LA    @14,1916                                            0249
         ALR   @14,WAPTR                                           0249
         NI    RPLIBSQ(@14),X'01'                                  0249
*       RPLIBSQ=RPLIBSQ×TCBIBSQ;    /* OR ON IBSQ FLAGS IN RPL       */
         OC    RPLIBSQ(1,@14),TCBIBSQ+2104(WAPTR)                  0250
