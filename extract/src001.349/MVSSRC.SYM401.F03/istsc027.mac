         TITLE 'ISTSC027,EXIO - SETS UP THE I/O INTERFACE              *
                        '
ISTSC027 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'ISTSC027  76.259'                                 0001
S27C1000 DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00643-S27C1000)
         ENTRY S27C1000
         DROP  @15
@PROLOG  ST    @14,12(,@13)                                        0001
         STM   @00,@12,20(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*   R14=R13;                        /* HOLD CALLER'S SVAREA PTR      */
         LR    R14,R13                                             0405
*   R13=ADDR(SVESC027);             /* PT TO MY SAVEAREA             */
         LA    R13,SVESC027(,WAPTR)                                0406
*   BACKSAVE(3)=R13;                /* PUT MY SVAPTR IN CALLR        */
         STCM  R13,15,BACKSAVE+8(R14)                              0407
*   SVESC027(2)=R14;                /* PUT CALLER'S SVAREA PTR IN MY
*                                      SAVEAREA                      */
         ST    R14,SVESC027+4(,WAPTR)                              0408
*/*                                                                  */
*                                                                  0409
*   EXIPTR=R1;                      /* BASE FOR EXIO PARM LIST       */
         ST    R1,EXIPTR                                           0409
*   RFY                                                            0410
*     CDSMAP BASED(CDSPTR);         /* BASE CDS MAP                  */
*   IF $XIOTCBA=0                   /* IF TECB ADDR = 0              */
*     THEN                          /* THEN                          */
         SLR   @15,@15                                             0411
         L     @03,EXIPTR                                          0411
         C     @15,$XIOTCBA(,@03)                                  0411
         BE    @RT00411                                            0411
*     GOTO S27C0350;                /* RC=08 AND RET TO OLT          */
*   IF $XIOCDSA=0                   /* IF CDSADDR NOT GIVEN          */
*     THEN                          /* THEN                          */
         C     @15,$XIOCDSA(,@03)                                  0413
         BNE   @RF00413                                            0413
*     CDSPTR=ADDR($PDEVADR);        /* USE PRIMARY DEVICE            */
         LA    CDSPTR,$PDEVADR(,WAPTR)                             0414
*   ELSE                                                           0415
*     CDSPTR=$XIOCDSA;              /* ELSE USE DEVICE GIVEN         */
*                                                                  0415
         B     @RC00413                                            0415
@RF00413 L     @15,EXIPTR                                          0415
         L     CDSPTR,$XIOCDSA(,@15)                               0415
*   /*****************************************************************/
*   /*                                                               */
*   /* SEARCH DEVICE TABLE TO INSURE DEVICE IS PRIMARY, OR GRABBED   */
*   /* AND NOT LETGO, AND IS ACQUIRED                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0416
*   DO I=3 TO $DEVCNTS;             /* SEARCH DEVICE TABLE           */
@RC00413 LA    I,3                                                 0416
         B     @DE00416                                            0416
@DL00416 DS    0H                                                  0417
*     IF CDSSYMB=ON                 /* IF DEV HAS SYMB NM IN CDS     */
*       THEN                        /* THEN                          */
         TM    CDSSYMB(CDSPTR),B'00001000'                         0417
         BNO   @RF00417                                            0417
*       DO;                                                        0418
*         IF CDSSYMBN=$$SYMNME(I)   /* CHK IF THEY MATCH             */
*           THEN                                                   0419
         LR    @15,I                                               0419
         MH    @15,@CH00373                                        0419
         ALR   @15,WAPTR                                           0419
         CLC   CDSSYMBN(8,CDSPTR),$$SYMNME-68(@15)                 0419
         BE    @RT00419                                            0419
*           GOTO S27C0100;          /* IF YES, CHK FOR VALID DEV     */
*         ELSE                                                     0421
*           GOTO S27C0200;          /* ELSE GO TO NEXT DEV           */
         B     S27C0200                                            0421
*       END;                                                       0422
*     ELSE                                                         0423
*       DO;                                                        0423
@RF00417 DS    0H                                                  0424
*         IF CDSUA^=$DEVUNAD(I)     /* DO UNIT ADDRESSES MATCH?      */
*           THEN                                                   0424
         LR    @15,I                                               0424
         MH    @15,@CH00373                                        0424
         L     @05,CDSUA(,CDSPTR)                                  0424
         CL    @05,$DEVUNAD-68(@15,WAPTR)                          0424
         BNE   @RT00424                                            0424
*           GOTO S27C0200;          /* NO, CHK FOR NEXT DEV          */
*       END;                                                       0426
*S27C0100:                                                         0427
*     IF($PRIMARY(I)=ON×            /* DEV MUST BE PRIMARY           */
*         ($GRABBED(I)=ON           /* GRABBED                       */
*         &$LETGO(I)=OFF))          /* AND NOT LETGO                 */
*         &$ACQRED(I)=ON            /* AND MUST BE ACQUIRED          */
*         &$DEVLU(I)=OFF            /* AND NOT AN LU         @D32JPFR*/
*       THEN                        /* THEN                          */
S27C0100 LR    @15,I                                               0427
         MH    @15,@CH00373                                        0427
         ST    @15,@TF00001                                        0427
         ALR   @15,WAPTR                                           0427
         TM    $PRIMARY-68(@15),B'00000001'                        0427
         BO    @GL00002                                            0427
         LR    @15,WAPTR                                           0427
         AL    @15,@TF00001                                        0427
         TM    $GRABBED-68(@15),B'00100000'                        0427
         BNO   @RF00427                                            0427
         TM    $LETGO-68(@15),B'10000000'                          0427
         BNZ   @RF00427                                            0427
@GL00002 LR    @15,I                                               0427
         MH    @15,@CH00373                                        0427
         ST    @15,@TF00001                                        0427
         ALR   @15,WAPTR                                           0427
         TM    $ACQRED-68(@15),B'01000000'                         0427
         BNO   @RF00427                                            0427
         LR    @15,WAPTR                                           0427
         AL    @15,@TF00001                                        0427
         TM    $DEVLU-68(@15),B'00001000'                          0427
         BZ    @RT00427                                            0427
*       GOTO S27C0300;              /* IT IS O.K.                    */
*     ELSE                          /* ELSE                          */
*       GOTO S27C0350;              /* RC=08                         */
         B     S27C0350                                            0429
*S27C0200:                                                         0430
*   END;                                                           0430
S27C0200 AL    I,@CF00072                                          0430
@DE00416 CH    I,$DEVCNTS(,WAPTR)                                  0430
         BNH   @DL00416                                            0430
*   GOTO S27C0350;                  /* DEV NOT IN DEV TBL            */
*                                                                  0431
         B     S27C0350                                            0431
*   /*****************************************************************/
*   /*                                                               */
*   /* IF DEVICE IS ACTIVE AND WAITIO HAS BEEN ISSUED AND DEVICE NOT */
*   /* REMOTE, PURGE I/O, AND START OVER. IF WAITIO NOT ISSUED, RC=08*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0432
*S27C0300:                                                         0432
*   IF $SYSMN(I)=$SYSMN(1) THEN     /* CHECK IF TESTING CT           */
S27C0300 LR    @15,I                                               0432
         MH    @15,@CH00373                                        0432
         ALR   @15,WAPTR                                           0432
         CLC   $SYSMN-68(8,@15),$SYSMN(WAPTR)                      0432
         BNE   @RF00432                                            0432
*     DO;                           /* IF YES, SPECIAL CHECKS ARE  0433
*                                      REQUIRED                      */
*       $EXIOWAT=ON;                /* WAIT AROUND TILL CHECKS ARE OK*/
         OI    $EXIOWAT(WAPTR),B'00100000'                         0434
*       IF $CTERROR=OFF&$MSGOTSD(1)^=0 THEN/* MSGS FOR CT?   @ZA12778*/
         TM    $CTERROR(WAPTR),B'01000000'                         0435
         BNZ   @RF00435                                            0435
         L     @15,$MSGOTSD(,WAPTR)                                0435
         LTR   @15,@15                                             0435
         BNZ   @RT00435                                            0435
*         GO TO S27C0999;           /* YES,WAIT TILL ALL ARE GONE    */
*       IF $REMOTE(I)=ON THEN       /* IF REMOTE, DO A DISCONTACT    */
@RF00435 LR    @15,I                                               0437
         MH    @15,@CH00373                                        0437
         ALR   @15,WAPTR                                           0437
         TM    $REMOTE-68(@15),B'01000000'                         0437
         BNO   @RF00437                                            0437
*         DO;                       /* ISSUE DO WITH DISCONTACT LDO
*                                      IF NECESSARY                  */
*           IF $LDOIS=ON THEN       /* HAS A DISCONTACT BEEN ISSUED? */
         TM    $LDOIS(WAPTR),B'00001000'                           0439
         BO    @RT00439                                            0439
*             GO TO S27C0999;       /* YES,KEEP WAITING              */
*           IF $DCONTCT=OFF THEN    /* SHOULD A DISCONTACT BE ISSUED?*/
         TM    $DCONTCT(WAPTR),B'00010000'                         0441
         BNZ   @RF00441                                            0441
*             DO;                   /* NONE ISSUED, DO FOR DISCONTACT
*                                      AND WAIT                      */
*               $LDOIS=ON;          /* PROCESSING A DISCONTACT       */
         OI    $LDOIS(WAPTR),B'00001000'                           0443
*               R1=ADDR($TLTRPL1);  /* POINT TO THE CT RPL IN      0444
*                                      WORKAREA                      */
         LA    R1,$TLTRPL1(,WAPTR)                                 0444
*               RESPECIFY                                          0445
*                 IFGRPL BASED(R1); /* PUT STRUCTURE OVER RPL        */
*               RPLARG=$CID(1);     /* PUT IS CT CID IN THE RPL      */
         MVC   RPLARG(4,R1),$CID(WAPTR)                            0446
*               GEN REFS(S27CEXIT)                                 0447
*                   (DO RPL=(1),OPTCD=ASY,EXIT=S27CEXIT,AREA=S27CLDO);
         DO RPL=(1),OPTCD=ASY,EXIT=S27CEXIT,AREA=S27CLDO
*               GO TO S27C0999;     /* LEAVE AND WAIT UNTIL DONE     */
         B     S27C0999                                            0448
*             END;                  /* END OF DISCONTACT LOGIC       */
*         END;                      /* END OF REMOTE LOGIC           */
@RF00441 DS    0H                                                  0451
*       $EXIOWAT=OFF;               /* STOP WAITING AND WORK         */
@RF00437 NI    $EXIOWAT(WAPTR),B'11011111'                         0451
*     END;                          /* END OF CT=TT LOGIC            */
*   IF $REMOTE(I)=ON                /* IF A REMOTE DEVICE            */
*     THEN                          /* THEN                          */
@RF00432 LR    @15,I                                               0453
         MH    @15,@CH00373                                        0453
         ST    @15,@TF00001                                        0453
         ALR   @15,WAPTR                                           0453
         TM    $REMOTE-68(@15),B'01000000'                         0453
         BO    @RT00453                                            0453
*     GOTO S27C0355;                /* DON'T CHECK FOR ALREADY ACTIVE*/
*   IF $DEVACT(I)=ON                /* IF DEV IS ACTIVE ALREADY      */
*     THEN                                                         0455
         LR    @15,WAPTR                                           0455
         AL    @15,@TF00001                                        0455
         TM    $DEVACT-68(@15),B'00000100'                         0455
         BNO   @RF00455                                            0455
*     DO;                                                          0456
*       IF $WTISUED(I)=OFF          /* AND WAITIO NOT ISSUD          */
*         THEN                      /* THEN                          */
         LR    @15,WAPTR                                           0457
         AL    @15,@TF00001                                        0457
         TM    $WTISUED-68(@15),B'00000001'                        0457
         BNZ   @RF00457                                            0457
*         DO;                                                      0458
*S27C0350:                                                         0459
*           R15=8;                  /* SET RC=08                     */
S27C0350 LA    R15,8                                               0459
*           GOTO S27C0999;          /* RETURN TO OLT                 */
         B     S27C0999                                            0460
*         END;                                                     0461
*       ELSE                                                       0462
*                                                                  0462
*         /***********************************************************/
*         /*                                                         */
*         /* KILL ANY ACTIVE I/O BEFORE STARTING OVER                */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0462
*         DO;                                                      0462
@RF00457 DS    0H                                                  0463
*           $IGLXIFB(I)=ON;         /* IGNORE HDV FEEDBACK           */
         LR    @15,I                                               0463
         MH    @15,@CH00373                                        0463
         ST    @15,@TF00001                                        0463
         ALR   @15,WAPTR                                           0463
         OI    $IGLXIFB-68(@15),B'01000000'                        0463
*           DIOCDS=ADDR($DEVTBLE(I));/* ADDR OF CDS                  */
         L     @15,@TF00001                                        0464
         LA    @15,$DEVTBLE-68(@15,WAPTR)                          0464
         ST    @15,DIOCDS                                          0464
*           DIOTECB=$XIOTCBA;       /* ADDR OF TECB                  */
         L     @15,EXIPTR                                          0465
         L     @15,$XIOTCBA(,@15)                                  0465
         ST    @15,DIOTECB                                         0465
*           R1=ADDR(DIOPARM);       /* PT TO PARM LIST               */
         LA    R1,DIOPARM                                          0466
*           CALL ISTSC032;          /* CALL DIO                      */
         L     @15,@CV03740                                        0467
         BALR  @14,@15                                             0467
*         END;                                                     0468
*     END;                                                         0469
*                                                                  0469
*   /*****************************************************************/
*   /*                                                               */
*   /* DECIDE WHICH SET OF CONTROL BLOCKS TO USE FOR THIS DEVICE     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0470
*S27C0355:                                                         0470
*   IF $1STUSED(I)=ON               /* IF 1ST BEING USED             */
*     THEN                                                         0470
@RF00455 DS    0H                                                  0470
S27C0355 LR    @15,I                                               0470
         MH    @15,@CH00373                                        0470
         ST    @15,@TF00001                                        0470
         ALR   @15,WAPTR                                           0470
         TM    $1STUSED-68(@15),B'00100000'                        0470
         BO    @RT00470                                            0470
*     GOTO S27C0360;                /* USE SAME ONES                 */
*   IF $2NDUSED(I)=ON               /* IF 2ND SET BEING USED         */
*     THEN                                                         0472
         LR    @15,WAPTR                                           0472
         AL    @15,@TF00001                                        0472
         TM    $2NDUSED-68(@15),B'00010000'                        0472
         BO    @RT00472                                            0472
*     GOTO S27C0370;                /* USE SAME ONES                 */
*   IF $1STDATA=ON                  /* 1ST CNTRLBLKS IN USE?         */
*       &$2NDDATA=ON                /* AND 2ND ALSO                  */
*     THEN                          /* THEN                          */
         TM    $1STDATA(WAPTR),B'11000000'                         0474
         BNO   @RF00474                                            0474
*     DO;                           /* 2 DEV ALREADY ACTIVE          */
*       R15=4;                      /* SET RC=04                     */
         LA    R15,4                                               0476
*       GOTO S27C0999;              /* AND RETURN                    */
         B     S27C0999                                            0477
*     END;                                                         0478
*   IF $1STDATA=OFF                 /* IF 1ST AREA NOT USED          */
*     THEN                                                         0479
@RF00474 TM    $1STDATA(WAPTR),B'10000000'                         0479
         BNZ   @RF00479                                            0479
*     DO;                                                          0480
*S27C0360:                                                         0481
*       $1STDATA=ON;                /* IND 1ST AREA IN USE           */
S27C0360 OI    $1STDATA(WAPTR),B'10000000'                         0481
*       $1STUSED(I)=ON;             /* 1ST LCPB FOR THIS DEV         */
         LR    @15,I                                               0482
         MH    @15,@CH00373                                        0482
         ST    @15,@TF00001                                        0482
         ALR   @15,WAPTR                                           0482
         OI    $1STUSED-68(@15),B'00100000'                        0482
*       IF $SYSMN(I)=$SYSMN(1)      /* IF THIS DEV CT ALSO           */
*         THEN                      /* THEN                          */
         LR    @15,WAPTR                                           0483
         AL    @15,@TF00001                                        0483
         CLC   $SYSMN-68(8,@15),$SYSMN(WAPTR)                      0483
         BNE   @RF00483                                            0483
*         DO;                                                      0484
*           $1STUSED(1)=ON;         /* SET 1ST USED FOR CT ALSO      */
         OI    $1STUSED(WAPTR),B'00100000'                         0485
*           $SSCPDAF(1)=$SSCPDAF(I);/* SET UP CT DAF                 */
         LR    @15,WAPTR                                           0486
         AL    @15,@TF00001                                        0486
         MVC   @TS00001(2),$SSCPDAF-68(@15)                        0486
         MVC   $SSCPDAF(2,WAPTR),@TS00001                          0486
*           $CTLINE=0;              /* CLEAR SCREEN ON THE NEXT    0487
*                                      MESSAGE TO CT                 */
         MVI   $CTLINE(WAPTR),X'00'                                0487
*         END;                                                     0488
*     END;                                                         0489
*   ELSE                                                           0490
*     DO;                                                          0490
         B     @RC00479                                            0490
@RF00479 DS    0H                                                  0491
*S27C0370:                                                         0491
*       $2NDDATA=ON;                /* IND 2ND AREA IN USE           */
S27C0370 OI    $2NDDATA(WAPTR),B'01000000'                         0491
*       $2NDUSED(I)=ON;             /* 2ND LCPB FOR THIS DEV         */
         LR    @15,I                                               0492
         MH    @15,@CH00373                                        0492
         ST    @15,@TF00001                                        0492
         ALR   @15,WAPTR                                           0492
         OI    $2NDUSED-68(@15),B'00010000'                        0492
*       IF $SYSMN(I)=$SYSMN(1)      /* IF THIS DEV CT ALSO           */
*         THEN                      /* THEN                          */
         LR    @15,WAPTR                                           0493
         AL    @15,@TF00001                                        0493
         CLC   $SYSMN-68(8,@15),$SYSMN(WAPTR)                      0493
         BNE   @RF00493                                            0493
*         DO;                                                      0494
*           $2NDUSED(1)=ON;         /* 2ND USED FOR CT ALSO          */
         OI    $2NDUSED(WAPTR),B'00010000'                         0495
*           $SSCPDAF(1)=$SSCPDAF(I);/* SET UP CT DAF                 */
         LR    @15,WAPTR                                           0496
         AL    @15,@TF00001                                        0496
         MVC   @TS00001(2),$SSCPDAF-68(@15)                        0496
         MVC   $SSCPDAF(2,WAPTR),@TS00001                          0496
*           $CTLINE=0;              /* CLEAR SCREEN ON THE NEXT    0497
*                                      MESSAGE TO CT                 */
         MVI   $CTLINE(WAPTR),X'00'                                0497
*         END;                                                     0498
*     END;                                                         0499
@RF00493 DS    0H                                                  0500
*   $DEVTECB(I)=$XIOTCBA;           /* GET TECB ADDR                 */
@RC00479 LR    @15,I                                               0500
         MH    @15,@CH00373                                        0500
         L     @11,EXIPTR                                          0500
         L     @00,$XIOTCBA(,@11)                                  0500
         ST    @15,@TF00001                                        0500
         ALR   @15,WAPTR                                           0500
         STCM  @00,15,$DEVTECB-68(@15)                             0500
*   IF $XIOATTN=ON                  /* IF OLT WANTS ATTNS            */
*     THEN                          /* THEN                          */
         TM    $XIOATTN(@11),B'10000000'                           0501
         BNO   @RF00501                                            0501
*     $ATTNEXP(I)=ON;               /* INDICATE SO IN DEVTABL        */
         LR    @15,WAPTR                                           0502
         AL    @15,@TF00001                                        0502
         OI    $ATTNEXP-68(@15),B'00001000'                        0502
*   IF $REMOTE(I)=ON                /* IF A REMOTE DEVICE            */
*     THEN                          /* THEN                          */
@RF00501 LR    @15,I                                               0503
         MH    @15,@CH00373                                        0503
         ALR   @15,WAPTR                                           0503
         TM    $REMOTE-68(@15),B'01000000'                         0503
         BO    @RT00503                                            0503
*     GOTO S27C0400;                /* THEN SEARCH OLTTCP CMNDS      */
*                                                                  0504
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCAL                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0505
*S27C0380:                          /* LABEL FOR HURSLEY             */
*   LOCPIU=LOCPIU&&LOCPIU;          /* ZERO OUT PIU                  */
S27C0380 LA    @15,8                                               0505
         LA    @10,$INBUFR+4(,WAPTR)                               0505
         LR    @09,@10                                             0505
         ALR   @09,@15                                             0505
         XC    LOCPIU(21,@09),LOCPIU(@09)                          0505
*   PIUPTR=ADDR(LOCPIU);            /* SET UP                        */
         ST    @09,PIUPTR(,@10)                                    0506
*   PIULEN=LENGTH(LOCPIU);          /* LOCAL CCWS                    */
         LA    @09,21                                              0507
         STH   @09,PIULEN(,@10)                                    0507
*   LCCWDATA=$XIOCCWA;              /* ADDR OF OLT CCW'S INTO LCCW   */
         ALR   @10,@09                                             0508
         L     @09,EXIPTR                                          0508
         L     @00,$XIOCCWA(,@09)                                  0508
         ST    @00,LCCWDATA(,@10)                                  0508
*   LCCWOP=LTEST;                   /* INDICATE TEST OP CD IN LCCW   */
         MVI   LCCWOP(@10),X'FE'                                   0509
*   LCCWCNT=$XIOCCWC*8;             /* NO. BYTES OF DATA INTO LCCW   */
         SLR   @00,@00                                             0510
         IC    @00,$XIOCCWC(,@09)                                  0510
         SLA   @00,3                                               0510
         STH   @00,LCCWCNT(,@10)                                   0510
*   J=1;                            /* INDEX TO NCSPL LENGTHS        */
         MVC   J(4),@CF00072                                       0511
*   COUNTAR(1)=LENGTH(ISTLCCW);     /* SIZE OF LCCWS                 */
         STH   @15,COUNTAR(,WAPTR)                                 0512
*   CCWPTR=ADDR(LOCCCW);            /* ADDR OF LOCAL CCW             */
         LA    CCWPTR,$INBUFR+4(,WAPTR)                            0513
*   GO TO S27C0650;                 /* BUILD NCSPL                   */
*                                                                  0514
         B     S27C0650                                            0514
*   /*****************************************************************/
*   /*                                                               */
*   /* REMOTE                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0515
*S27C0400:                                                         0515
*   CCWPTR=$XIOCCWA;                /* PT TO OLT CCW'S               */
S27C0400 L     @15,EXIPTR                                          0515
         L     CCWPTR,$XIOCCWA(,@15)                               0515
*   DO WHILE(CCWCODE='93'X×CCWCODE='52'X×CCWCODE='51'X×CCWCODE='32'X×
*         CCWCODE='31'X);                                          0516
         B     @DE00516                                            0516
@DL00516 DS    0H                                                  0517
*     CCWPTR=CCWPTR+8;                                             0517
         AL    CCWPTR,@CF00055                                     0517
*   END;                                                           0518
@DE00516 CLI   CCWCODE(CCWPTR),X'93'                               0518
         BE    @DL00516                                            0518
         CLI   CCWCODE(CCWPTR),X'52'                               0518
         BE    @DL00516                                            0518
         CLI   CCWCODE(CCWPTR),X'51'                               0518
         BE    @DL00516                                            0518
         CLI   CCWCODE(CCWPTR),X'32'                               0518
         BE    @DL00516                                            0518
         CLI   CCWCODE(CCWPTR),X'31'                               0518
         BE    @DL00516                                            0518
*   IF CCWCODE^=2                   /* IF CCW IS NOT A READ          */
*       &CCWCODE^=1                 /* OR NOT A WRITE                */
*       &CCWCODE^=9                 /* OR NOT A WRITE BREAK          */
*     THEN                          /* THEN                          */
         CLI   CCWCODE(CCWPTR),2                                   0519
         BE    @RF00519                                            0519
         CLI   CCWCODE(CCWPTR),1                                   0519
         BE    @RF00519                                            0519
         CLI   CCWCODE(CCWPTR),9                                   0519
         BE    @RF00519                                            0519
*     DO;                                                          0520
*       RFY                                                        0521
*         R15 RSTD;                 /*                       @ZD10900*/
*S27C0420:                                                         0522
*       R15=8;                      /* SET RC=08                     */
S27C0420 LA    R15,8                                               0522
*       IF $1STUSED(I)=ON           /* IF 1ST BEING USED             */
*         THEN                      /* THEN                          */
         LR    @11,I                                               0523
         MH    @11,@CH00373                                        0523
         ST    @11,@TF00001                                        0523
         ALR   @11,WAPTR                                           0523
