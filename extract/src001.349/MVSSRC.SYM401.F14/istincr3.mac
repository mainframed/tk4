SVTM     TITLE 'SSCP - ISTINCR3 - CONFIGURATION NCP RESET              *
                        '
ISTINCR3 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ISTINCR3  75.301'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
* DO;                                                              0406
*/***  CRRMAINT  FUNC(SUB)COMP(VY)PROCNM(ISTINCR3)RPHADD((NCSPLRPH))  *
**/                                                                0407
*                                                                  0407
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0407
* RFY ISTRPH  ;                                                    0407
* RFY ISTLCCW ;                                                    0408
* RFY ISTLCPB ;                                                    0409
* RFY ISTZSAVE;                                                    0410
* RFY ISTPFCRR;                                                    0411
* RFY ISTCFCRR;                                                    0412
* RFY ISTZCRR ;                                                    0413
* RFY ISTASCRR;                                                    0414
* RFY ISTAPCRR;                                                    0415
* RFY ISTCRA  ;                                                    0416
* RFY ISTCNSTA;                                                    0417
* RFY ISTVYCRR;                                                    0418
*/* END OF ATBUILD */                                              0419
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0419
*NCSPLRPH->RPHCRR->CRACRR->NINCR3 = '1'B;                          0419
         L     @11,NCSPLRPH(,NCSPLPTR)                             0419
         L     @11,RPHCRR(,@11)                                    0419
         L     @11,CRACRR(,@11)                                    0419
         OI    NINCR3(@11),B'00000001'                             0419
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0420
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0420
         XC    VYCCRT+1(11,@11),VYCCRT+1(@11)                      0420
         MVI   VYCCRT(@11),B'00000000'                             0420
*NCSPLRPH->RPHCRR->CRACRR->CINCR3 = '1'B;                          0421
         OI    CINCR3(@11),B'00000001'                             0421
* END;                                                             0422
*   NCSPLSAV=NCSPLPTR;                /*SAVE PTR TO NCSPL FROM R1.  */
         LR    NCSPLSAV,NCSPLPTR                                   0423
*   RFY (CSAVEREG,CR3R14,CR3R15) RSTD;                             0424
*   RFY (CR3RDTE) RSTD;                                            0425
*   RFY (ISTNCSPL) BASED(NCSPLSAV);                                0426
*   RFY (CR3WKA) RSTD;                                             0427
*   GEN REFS(CR3WKA);                                              0428
           USING @DATD,CR3WKA
*    RFY ISTRUB BASED (NCSPLRUO);                                  0429
*/*                                                                 */
*/* IF THE WORK AREA OVERLAPS SAVE AREA, SET FAILURE RETURN CODE    */
*/*  AND RETURN TO CALLER.                                          */
*/*                                                                 */
*   IF ((CR3WKNXT/8*8+8)+@SIZDATD+8) > CSAVEREG-72                 0430
*    THEN DO;                    /* DO ON OVERLAP                    */
         LA    @11,8                                               0430
         L     @07,NCSPLWKA(,NCSPLSAV)                             0430
         L     @00,CR3WKNXT(,@07)                                  0430
         SRL   @00,3                                               0430
         SLA   @00,3                                               0430
         ALR   @00,@11                                             0430
         AL    @00,@SIZDATD                                        0430
         ALR   @00,@11                                             0430
         LR    @11,CSAVEREG                                        0430
         SL    @11,@CF01013                                        0430
         CR    @00,@11                                             0430
         BNH   @RF00430                                            0430
*     NCSPLPTR=NCSPLSAV;         /* RESTORE NCSPL POINTER            */
         LR    NCSPLPTR,NCSPLSAV                                   0432
*     NCSIORC=NCSNOSTG;                                            0433
         MVI   NCSIORC(NCSPLSAV),X'20'                             0433
*    END;                 /*END DO IF OVERLAP                        */
*   ELSE DO;              /* DO IF NO OVERLAP                       */
         B     @RC00430                                            0435
@RF00430 DS    0H                                                  0436
*/*                                                                 */
*/*  SAVE AREA - DO STANDARD LINKAGE USING NEXT AVAILABLE SAVE AREA */
*/*  AND UPDATE NEXT AVAILABLE S.A. ACCORDING TO NCSPL W.A. ALLOCA- */
*/*  TION RULES.                                                    */
*/*                                                                 */
*  RFY CR3R2 RSTD;                                                 0436
*  CR3R2=CSAVEREG;                                                 0437
         LR    CR3R2,CSAVEREG                                      0437
*  CSAVEREG=CSAVEREG-LENGTH(CR3SVARE);                             0438
         SL    CSAVEREG,@CF01013                                   0438
*  CR3R2->FWDCHN=CSAVEREG;                                         0439
         ST    CSAVEREG,FWDCHN(,CR3R2)                             0439
*  CSAVEREG->BACKCHN=CR3R2;                                        0440
         ST    CR3R2,BACKCHN(,CSAVEREG)                            0440
*  RFY CR3R2 UNRSTD;                                               0441
*  CSAVEREG->FWDCHN=CR3ZERO;                                       0442
         SLR   @11,@11                                             0442
         ST    @11,FWDCHN(,CSAVEREG)                               0442
*/*                                                                 */
*/*                                                                 */
*/*WORK SPACE - SET BASE FOR WORK SPACE USING NCSPL NEXT AVAILABLE  */
*/* POINTER. CLEAR THE WORK SPACE AND UPDATE THE NEXT AVAILABLE PTR.*/
*/* TO POINT TO THE BYTE FOLLOWING CR3'S WORK SPACE.                */
*/*                                                                 */
*   CR3WKA=((NCSPLWKA->CR3NXTAV+7) & (-8));  /*W.A. ON DWORD BND*/ 0443
         L     @11,NCSPLWKA(,NCSPLSAV)                             0443
         LA    CR3WKA,7                                            0443
         AL    CR3WKA,CR3NXTAV(,@11)                               0443
         N     CR3WKA,@CF01864                                     0443
*   CR3NXTAV=CR3WKA + @SIZDATD;         /*UPDATE NEXT AVAIL. PTR.   */
         LR    @00,CR3WKA                                          0444
         AL    @00,@SIZDATD                                        0444
         ST    @00,CR3NXTAV(,@11)                                  0444
*   SAVNCCMD=NCSPLCMD;                  /*PRESERVE NCSPL FIELD TO  */
         MVC   SAVNCCMD(2),NCSPLCMD(NCSPLSAV)                      0445
*                                      /* BE ALTERED BY CR3 IN W.A.*/
         EJECT
*   CR3RDTE=NCSPLRPT;                   /*INITIALIZE RDTE PTR TO RN */
         L     CR3RDTE,NCSPLRPT(,NCSPLSAV)                         0446
*   NCSIORC=NCSIOSUC;                   /* INITIALIZE GOOD R.C.*/  0447
         MVI   NCSIORC(NCSPLSAV),X'00'                             0447
*   RFY (ISTRPRE,ISTRLN,ISTRPT,ISTRIN,ISTRRN,ISTRTR) BASED(CR3RDTE);
*  /*INITIALIZE RDTE POINTER TO LAST ENTRY IN SEGMENT*/            0449
*     CR3RDTE=RRNLASTE;                                            0449
         L     CR3RDTE,RRNLASTE(,CR3RDTE)                          0449
*/*                                                                 */
*/*CALL THE APPROPRIATE ROUTINE TO PROCESS THE RDT SEGMENT FOR THE  */
*/*     TYPE OF ACTPU RESPONSE RECEIVED                              */
*IF NCSFMCLD=CR3ON    /*CHECK FOR FME(COLD) RESP. TO ACTPU           */
*  THEN                                                            0450
         TM    NCSFMCLD(NCSPLSAV),B'00100000'                      0450
         BNO   @RF00450                                            0450
*    CALL CR3FMCLD;                                                0451
         BAL   @14,CR3FMCLD                                        0451
*  ELSE               /*ASSUME FME(ERP) RESPONSE                     */
*    CALL CR3FMERP;                                                0452
         B     @RC00450                                            0452
@RF00450 BAL   @14,CR3FMERP                                        0452
*/*                                                                  */
*/*  RESTORE NCSPL FIELD ALTERED IN CR3 AND RESET SAVE AREA          */
*/*  AND WORK AREA POINTERS. SET NCSIORC FOR RETURN TO CALLER.       */
*/*                                                                  */
*    NCSPLCMD=SAVNCCMD;                                            0453
@RC00450 MVC   NCSPLCMD(2,NCSPLSAV),SAVNCCMD                       0453
*    CSAVEREG=BACKCHN;                                             0454
         L     CSAVEREG,BACKCHN(,CSAVEREG)                         0454
*    NCSPLWKA -> CR3NXTAV=CR3WKA;                                  0455
         L     @11,NCSPLWKA(,NCSPLSAV)                             0455
         ST    CR3WKA,CR3NXTAV(,@11)                               0455
*/*                                                                 */
*/* RESET AN I/O ERROR RETURN CODE TO A SUCCESSFUL NCSIORC.         */
*/* OTHERWISE PASS NCSIORC BACK TO CALLER AS IS.                    */
*   IF NCSIORC=NCSIOERR                                            0456
*    THEN                                                          0456
         CLI   NCSIORC(NCSPLSAV),X'04'                             0456
         BNE   @RF00456                                            0456
*     NCSIORC=NCSIOSUC;                                            0457
         MVI   NCSIORC(NCSPLSAV),X'00'                             0457
*    ELSE;                                                         0458
@RF00456 DS    0H                                                  0459
*  END;           /*END DO IF NO OVERLAP    */                     0459
*/*                                                                 */
         EJECT
*CR3FMCLD:PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0460
         B     @PB00002                                            0460
CR3FMCLD DS    0H                                                  0461
*/* THIS ROUTINE WILL PROCESS AN RN RDT SEGMENT WHEN AN FME(COLD)   */
*/*  RESPONSE IS RECEIVED TO THE ACTPU.  ENTRY WILL BE FOR VARY ACT */
*/*  (WARM) PROCESSING AND ISTINCR2 WILL BE CALLED FOR ANY LINE, TR,*/
*/*  UTERM OR COMPONENT WHOSE RDTE INDICATES A CHANGED LINE SCHED*/
*/*  PARM(S)CR3RDTE POINTS TO THE RN RDTE UPON ENTRY.               */
*/*  CR3 IS CALLED ONLY IF A CHECKPOINT DATA SET HAS BEEN APPLIED   */
*/*  THE SEGMENT.                                                   */
*CR3RSCLD=CR3R14;          /*SAVE RETURN ADDRESS ACROSS CALL         */
         ST    CR3R14,CR3RSCLD                                     0461
* DO WHILE (CR3RDTE^=NCSPLRPT)   /*LOOP FROM BOTTOM TO TOP OF RDT   */
*    & NCSIORC < NCSIOPRG;  /* SEGMENT UNTIL BACK AT RN OR I/O IS   */
         B     @DE00462                                            0462
@DL00462 DS    0H                                                  0463
*                           /* PURGED                               */
*   NCSIORC=NCSIOSUC;     /*INITIALIZE SUCCESSFUL RETURN CODE FOR CR2*/
         MVI   NCSIORC(NCSPLSAV),X'00'                             0463
*   IF RPRENTRY=RPRENTLN  /*PREPARE TO ISSUE L.S.P.'S FOR A          */
*     THEN DO;            /*LINE IF ANY FLAGS ARE SET.       @AS07973*/
         CLI   RPRENTRY(CR3RDTE),X'50'                             0464
         BNE   @RF00464                                            0464
*      IF RLNRACAT=CR3OFF                                   /*@X50BHF9*
*      & RLNRACAN=CR3OFF  /*LINE MUST BE NON-SWITCHED,BASIC  @X50BHF9*/
*      & RLNRAPOL=CR3ON   /*AND POLLED TO HAVE L.S.P.'S      @XM18659*/
*      & RLNRATRN=CR3OFF                                   /*@X50BHF9*/
*      & RLNLSCMD^=CR3ALOFF/*IF ANY LSP FLAGS ARE SET              */
*       THEN                                                       0466
         TM    RLNRACAT(CR3RDTE),B'11000000'                       0466
         BNZ   @RF00466                                            0466
         TM    RLNRAPOL(CR3RDTE),B'00000100'                       0466
         BNO   @RF00466                                            0466
         TM    RLNRATRN(CR3RDTE),B'00000010'                       0466
         BNZ   @RF00466                                            0466
         TM    RLNLSCMD(CR3RDTE),B'00111000'                       0466
         BZ    @RF00466                                            0466
*        DO;                                                       0467
*          NCSPLRCR=CR3RDTE;  /*SET RDTE @ FOR CR2                   */
         ST    CR3RDTE,NCSPLRCR(,NCSPLSAV)                         0468
*          CALL ISTINCR2;     /*REISSUE LINE SCHED. PARMS            */
*                                                                  0469
         L     @15,@CV01807                                        0469
         BALR  @14,@15                                             0469
*           DO;                                                    0470
*/***  CRRMAINT  FUNC(RET)COMP(VY)PROCNM(ISTINCR3)RPHADD((NCSPLRPH))  *
**/                                                                0471
*           /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/          0471
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0471
         L     @11,NCSPLRPH(,NCSPLSAV)                             0471
         L     @11,RPHCRR(,@11)                                    0471
         L     @11,CRACRR(,@11)                                    0471
         XC    VYCCRT+1(11,@11),VYCCRT+1(@11)                      0471
         MVI   VYCCRT(@11),B'00000000'                             0471
*NCSPLRPH->RPHCRR->CRACRR->CINCR3 = '1'B;                          0472
         OI    CINCR3(@11),B'00000001'                             0472
*           END;                                                   0473
*        END;                                                      0474
*       ELSE;                                                      0475
@RF00466 DS    0H                                                  0476
*      IF RLNRBISA=CR3OFF &  /* IF ISTATUS OF OFF AND        @AS07973*/
*         RLNRBUSE=CR3ON     /* NCP = USE                    @AS07973*/
*         THEN DO;           /* SWITCH THE LINE TO EP        @AS07973*/
         TM    RLNRBISA(CR3RDTE),B'01000000'                       0476
         BNZ   @RF00476                                            0476
         TM    RLNRBUSE(CR3RDTE),B'10000000'                       0476
         BNO   @RF00476                                            0476
*           CR3EP=CR3ON;     /* INDICATING SWITCH TO EP      @AS07973*/
         OI    CR3EP,B'10000000'                                   0478
*           CALL CR3DEACT;                                 /*@AS07973*/
         BAL   @14,CR3DEACT                                        0479
*         END;                                             /*@AS07973*/
*         ELSE;                                            /*@AS07973*/
@RF00476 DS    0H                                                  0482
*     END;                   /* END IF IT IS A LINE */             0482
*     ELSE                                                         0483
*      IF(RPRENTRY=RPRENTTR×RPRENTRY=RPRENTCM)                     0483
*          & (RTRDTRLM=CR3ON)                                      0483
*       THEN                   /*PREPARE TO REISSUE L.S.P.           */
         B     @RC00464                                            0483
@RF00464 CLI   RPRENTRY(CR3RDTE),X'80'                             0483
         BE    @GL00004                                            0483
         CLI   RPRENTRY(CR3RDTE),X'90'                             0483
         BNE   @RF00483                                            0483
@GL00004 TM    RTRDTRLM(CR3RDTE),B'00100000'                       0483
         BNO   @RF00483                                            0483
*        DO;                   /*FOR A TR OR CM IF DEV TR LIM IS SET */
*         NCSPLRCR=CR3RDTE;    /*SET RDTE @ FOR CR2                  */
         ST    CR3RDTE,NCSPLRCR(,NCSPLSAV)                         0485
*         CALL ISTINCR2;       /*REISSUE DEV. TRANS. LIM.            */
*                                                                  0486
         L     @15,@CV01807                                        0486
         BALR  @14,@15                                             0486
*         DO;                                                      0487
*/***  CRRMAINT  FUNC(RET)COMP(VY)PROCNM(ISTINCR3)RPHADD((NCSPLRPH))  *
**/                                                                0488
*         /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/            0488
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0488
         L     @11,NCSPLRPH(,NCSPLSAV)                             0488
         L     @11,RPHCRR(,@11)                                    0488
         L     @11,CRACRR(,@11)                                    0488
         XC    VYCCRT+1(11,@11),VYCCRT+1(@11)                      0488
         MVI   VYCCRT(@11),B'00000000'                             0488
*NCSPLRPH->RPHCRR->CRACRR->CINCR3 = '1'B;                          0489
         OI    CINCR3(@11),B'00000001'                             0489
*         END;                                                     0490
*        END;                                                      0491
*       ELSE;                                                      0492
@RF00483 DS    0H                                                  0493
*   CR3RDTE=CR3RDTE-RPRPELEN;   /*DECREMENT POINTER TO PROCESS       */
@RC00464 SH    CR3RDTE,RPRPELEN(,CR3RDTE)                          0493
*                               /*NEXT RDTE                          */
* END;                                                             0494
@DE00462 C     CR3RDTE,NCSPLRPT(,NCSPLSAV)                         0494
         BE    @DC00462                                            0494
         CLI   NCSIORC(NCSPLSAV),X'08'                             0494
         BL    @DL00462                                            0494
@DC00462 DS    0H                                                  0495
* CR3R14=CR3RSCLD;              /*RESTORE RETURN ADDRESS             */
         L     CR3R14,CR3RSCLD                                     0495
*END CR3FMCLD;                                                     0496
@EL00002 DS    0H                                                  0496
@EF00002 DS    0H                                                  0496
@ER00002 BR    @14                                                 0496
         EJECT
*CR3FMERP:PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0497
CR3FMERP DS    0H                                                  0498
*/* THIS ROUTINE WILL PROCESS AN RN RDT SEGMENT WHEN AN*/          0498
*/*   FME(ERP) RESPONSE IS RECEIVED TO THE ACTPU.  IF THE CALL TO */
*/*   CR3 IS FOR ACTIVATE PROCESSING, THIS SECTION WILL ISSUE DEACT.*/
*/*   LINK RU'S TO ANY LINE WHOSE ISTATUS FLAG IS NOT SET.  IF THE  */
*/*   COMMAND IS COLD, LINE SCHEDULING PARAMETERS ARE REISSUED AS   */
*/*   NEEDED.  IF THE CALL WAS FOR ERP PROCESSING, DEACTIVATE LINK  */
*/*   RU'S ARE ISSUED TO ALL LINES WHOSE ONLINE STATUS IS           */
*/*   'DEACTIVATED'                                                 */
*/*   NOTE THAT ALL LINE SCHEDULING PARAMETERS APPLY ONLY TO NON-   */
*/*   SWITCHED, POLLED, BASIC LINES OR THE TERMINALS OR COMPONENTS  */
*/*   ON SUCH LINES.                                                */
*/*                                                                 */
*                                                                  0498
* CR3RSERP=CR3R14;        /*SAVE RETURN ADDRESS ACROSS CALL          */
         ST    CR3R14,CR3RSERP                                     0498
* IF NCSPLVCD=NCSPLVC1                                             0499
*  THEN                   /*PREPARE TO PROCESS FOR ACT CMD.          */
         CLI   NCSPLVCD(NCSPLSAV),X'01'                            0499
         BNE   @RF00499                                            0499
*   DO WHILE (CR3RDTE^=NCSPLRPT) & NCSIORC < NCSIOPRG;             0500
         B     @DE00500                                            0500
@DL00500 DS    0H                                                  0501
*   NCSIORC=NCSIOSUC;     /*INITIALIZE R.C. FOR EXT. CALLS*/       0501
         MVI   NCSIORC(NCSPLSAV),X'00'                             0501
*   IF (RPRENTRY=RPRENTLN)                                         0502
*    THEN                                                          0502
         CLI   RPRENTRY(CR3RDTE),X'50'                             0502
         BNE   @RF00502                                            0502
*     DO;                                                          0503
*      IF (RLNRBISA^=CR3ON)                                        0504
*       THEN                                                       0504
         TM    RLNRBISA(CR3RDTE),B'01000000'                       0504
         BO    @RF00504                                            0504
*        DO;     /*DEACT THE LINE SINCE ISTATUS IS OFF               */
*          CALL CR3DEACT;                                          0506
         BAL   @14,CR3DEACT                                        0506
*        END;                                                      0507
*       ELSE;                                                      0508
*                                                                  0508
*                                                                  0508
@RF00504 DS    0H                                                  0509
*      IF NCSPLWRM^=CR3ON & NCSIORC < NCSIOPRG                     0509
*                    /*IF THE COMMAND IS ACT(COLD) AND NO ERROR*/  0509
*      & RLNRACAT=CR3OFF                                   /*@X50BHF9*/
*      & RLNRACAN=CR3OFF  /*LINE MUST BE NON-SWITCHED,BASIC  @X50BHF9*/
*      & RLNRAPOL=CR3ON   /*AND POLLED TO HAVE L.S.P.'S      @XM18659*/
*      & RLNRATRN=CR3OFF                                   /*@X50BHF9*/
*         THEN DO;                                                 0509
         TM    NCSPLWRM(NCSPLSAV),B'10000000'                      0509
         BO    @RF00509                                            0509
         CLI   NCSIORC(NCSPLSAV),X'08'                             0509
         BNL   @RF00509                                            0509
         TM    RLNRACAT(CR3RDTE),B'11000000'                       0509
         BNZ   @RF00509                                            0509
         TM    RLNRAPOL(CR3RDTE),B'00000100'                       0509
         BNO   @RF00509                                            0509
         TM    RLNRATRN(CR3RDTE),B'00000010'                       0509
         BNZ   @RF00509                                            0509
*          NCSIORC=NCSIOSUC;   /*INSURE ZERO RC FOR CALL TO CR2*/  0511
         MVI   NCSIORC(NCSPLSAV),X'00'                             0511
*          CALL CR3LSP;    /*REISSUE L.S.P. AS NECESSARY             */
         BAL   @14,CR3LSP                                          0512
*          END;                                                    0513
*         ELSE;                                                    0514
@RF00509 DS    0H                                                  0515
*      END;               /* LINE IS PROCESSED.                      */
*                                                                  0515
*    ELSE                                                          0516
*    IF (RPRENTRY=RPRENTTR×RPRENTRY=RPRENTCM)                      0516
*      &  NCSPLWRM^=CR3ON                                          0516
*      THEN           /*IF THE ENTRY IS A TR, CM ON POLLED BASIC LINE*/
         B     @RC00502                                            0516
@RF00502 CLI   RPRENTRY(CR3RDTE),X'80'                             0516
         BE    @GL00010                                            0516
         CLI   RPRENTRY(CR3RDTE),X'90'                             0516
         BNE   @RF00516                                            0516
@GL00010 TM    NCSPLWRM(NCSPLSAV),B'10000000'                      0516
         BO    @RF00516                                            0516
*       DO;           /* REISSUE DEV. TRAN. LIM. AS NEEDED.          */
*        RFY (CR3NSNEA) RSTD;                              /*@XM18658*/
*        CR3NSNEA=CR3RDTE;                                 /*@XM18658*/
         LR    CR3NSNEA,CR3RDTE                                    0519
*        DO WHILE (CR3NSNEA->RPRENTRY^=RPRENTLN &          /*@XM18658*/
*                  CR3NSNEA ^= NCSPLRPT);                  /*@AS08973*/
         B     @DE00520                                            0520
@DL00520 DS    0H                                                  0521
*         CR3NSNEA=CR3NSNEA->RPRNSNEA;                             0521
         L     CR3NSNEA,RPRNSNEA(,CR3NSNEA)                        0521
*        END;         /*LOOP TO FIND LINE ASSOC. W/ TR OR CM         */
@DE00520 CLI   RPRENTRY(CR3NSNEA),X'50'                            0522
         BE    @DC00520                                            0522
         C     CR3NSNEA,NCSPLRPT(,NCSPLSAV)                        0522
         BNE   @DL00520                                            0522
@DC00520 DS    0H                                                  0523
*                                                          /*@XM18658*/
*        IF (CR3NSNEA->RLNRACAN=CR3OFF  /*CHECK FOR NON-SWITCHED,    */
*          & CR3NSNEA->RLNRACAT=CR3OFF                             0523
*          & CR3NSNEA->RLNRAPOL=CR3ON   /*POLLED AND                 */
*          & CR3NSNEA->RLNRATRN=CR3OFF) /*NON-SDLC LINK FOR THIS TR  */
*          & CR3NSNEA ^= NCSPLRPT       /* OR COMPONENT      @AS08973*/
*           THEN                                                   0523
         TM    RLNRACAN(CR3NSNEA),B'11000000'                      0523
         BNZ   @RF00523                                            0523
         TM    RLNRAPOL(CR3NSNEA),B'00000100'                      0523
         BNO   @RF00523                                            0523
         TM    RLNRATRN(CR3NSNEA),B'00000010'                      0523
         BNZ   @RF00523                                            0523
         C     CR3NSNEA,NCSPLRPT(,NCSPLSAV)                        0523
         BE    @RF00523                                            0523
*             DO;                                                  0524
*              RFY (CR3NSNEA) UNRSTD;                              0525
*              CALL CR3LSP;                                        0526
         BAL   @14,CR3LSP                                          0526
*             END;                                                 0527
*            ELSE;                                                 0528
@RF00523 DS    0H                                                  0529
*        END;                                                      0529
*       ELSE;                                                      0530
@RF00516 DS    0H                                                  0531
*     CR3RDTE=CR3RDTE-RPRPELEN; /*STEP UP TO NEXT RDTE               */
@RC00502 SH    CR3RDTE,RPRPELEN(,CR3RDTE)                          0531
*    END;                       /*LOOP TO PROCESS RDT SEG.           */
@DE00500 C     CR3RDTE,NCSPLRPT(,NCSPLSAV)                         0532
         BE    @DC00500                                            0532
         CLI   NCSIORC(NCSPLSAV),X'08'                             0532
         BL    @DL00500                                            0532
@DC00500 DS    0H                                                  0533
