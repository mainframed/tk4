*             /* CHECK FOR ACCEPT ANY REQUEST FOR ZD1 3702           */
*             /*******************************************************/
*             IF CSPIDX=CSPIACPT &   /*IF ACCEPT             @DM13702*/
*                CSPRSC=BITON        /*ANY REQUEST           @DM13702*/
*               THEN                                       /*@DM13702*/
         CLC   CSPIDX(2,CSPPTR),@CH00069                        03S0483
         BNE   @RF00483                                         03S0483
         TM    CSPRSC(CSPPTR),B'10000000'                       03S0483
         BNO   @RF00483                                         03S0483
*                RRET=GOOD;          /*SET GOOD RC           @DM13702*/
         SLR   RRET,RRET                                        03S0484
*             ELSE                                         /*@DM13702*/
*             DO;                                          /*@DM13702*/
         B     @RC00483                                         03S0485
@RF00483 DS    0H                                               03S0486
*             WORK=CSPRSCE;          /*POINT AT RSCE LIST            */
         L     WORK,CSPRSCE(,CSPPTR)                            03S0486
*             RFY                                               03S0487
*               (RALLOC1                                        03S0487
*               ,LISTCT ) RSTD;                                 03S0487
*             DO LISTCT=1 TO CSPNO WHILE(RRET=GOOD);/*FOR       03S0488
*                                      ALL LIST ENTRIES              */
         LA    LISTCT,1                                         03S0488
         B     @DE00488                                         03S0488
@DL00488 CH    RRET,@CH00619                                    03S0488
         BNE   @DC00488                                         03S0488
*               RALLOC1=-(WORK->CSLRDTE);                       03S0489
         L     RALLOC1,CSLRDTE(,WORK)                           03S0489
         LCR   RALLOC1,RALLOC1                                  03S0489
*                                    /*GET TRML ITEM TO CHK  @D32CKMP*/
*               CALL CHECRDTE;       /*CHECK TRML ITEM               */
         BAL   @14,CHECRDTE                                     03S0490
*               WORK=WORK+LENGTH(ISTCSL);                       03S0491
         AH    WORK,@CH00440                                    03S0491
*                                    /*GET NEXT LIST ENTRY   @D32CKMP*/
*             END;                   /*LIST DONE OR ERROR            */
         AH    LISTCT,@CH00038                                  03S0492
@DE00488 CH    LISTCT,CSPNO(,CSPPTR)                            03S0492
         BNH   @DL00488                                         03S0492
@DC00488 DS    0H                                               03S0493
*             RFY                                               03S0493
*               LISTCT UNRSTD;                                  03S0493
*             END;                                         /*@DM13702*/
*                                                               03S0494
*             /*******************************************************/
*             /* LEAVE IF THE PREVIOUS PARAMETERS WERE NOT           */
*             /* VALID OR DESTINATION VALIDATION IS NOT              */
*             /* REQUIRED.                                           */
*             /*******************************************************/
*                                                               03S0495
*/*           LEAVE(BLOCK) ON(RRET^=GOOD×CPFDEST=BITON) */      03S0495
*             IF (RRET^=GOOD×CPFDEST=BITON) THEN                03S0495
@RC00483 CH    RRET,@CH00619                                    03S0495
         BNE   @RT00495                                         03S0495
         TM    CPFDEST+124(SAVEPTR),B'00100000'                 03S0495
         BO    @RT00495                                         03S0495
*               GOTO #BC00017;                                  03S0496
*                                                               03S0496
*             RALLOC1=CSPDEST;       /*GET APPL DEST ITEM            */
         L     RALLOC1,CSPDEST(,CSPPTR)                         03S0497
*             CALL CHECRDTE;         /*CHECK DEST ITEM               */
*                                                               03S0498
         BAL   @14,CHECRDTE                                     03S0498
*             /*******************************************************/
*             /* LEAVE IF THE PREVIOUS PARAMETERS WERE NOT           */
*             /* VALID OR SOURCE VALIDATION IS NOT REQUIRED.         */
*             /*******************************************************/
*                                                               03S0499
*/*           LEAVE(BLOCK) ON(RRET^=GOOD×CPFSRC=BITON×CSPNOREL=BITON) *
*                                                               03S0499
*             IF (RRET^=GOOD×CPFSRC=BITON×CSPNOREL=BITON) THEN  03S0499
         CH    RRET,@CH00619                                    03S0499
         BNE   @RT00499                                         03S0499
         TM    CPFSRC+124(SAVEPTR),B'00000001'                  03S0499
         BO    @RT00499                                         03S0499
         TM    CSPNOREL(CSPPTR),B'00100000'                     03S0499
         BO    @RT00499                                         03S0499
*               GOTO #BC00017;                                  03S0500
*                                                               03S0500
*             RALLOC1=CSPSRC;        /*GET APPL SOURCE ITEM          */
         L     RALLOC1,CSPSRC(,CSPPTR)                          03S0501
*             CALL CHECRDTE;         /*CHECK SOURCE ITEM             */
*                                                               03S0502
         BAL   @14,CHECRDTE                                     03S0502
*           END; /* END(BLOCK)  */                              03S0503
*#BC00017:;             /*END OF LOGIC BLOCK SG         */      03S0504
*                                                               03S0504
#BC00017 DS    0H                                               03S0505
*         END CHECPARM;   /*END OF CHECPARM LOGIC SEGMENT            */
*                                                               02S0506
*/*         LEAVE(MAIN) ON(RRET^=0)ANDDO */                     02S0506
*           IF (RRET^=0) THEN                                   02S0506
         CH    RRET,@CH00619                                    02S0506
         BE    @RF00506                                         02S0506
*             DO;/*FOR NVLD PARMS          */                   02S0507
*             CSPCC=INVALID;             /*SET COND CODE AND         */
*                                                               02S0508
         MVC   CSPCC(2,CSPPTR),@CH00075                         02S0508
*               GOTO #BCM0016; /* END(LEAVE)  */                02S0509
         B     #BCM0016                                         02S0509
*             END;                 /*EXIT MAIN LOGIC           */
*           IF ATCAUTHA^=0               /*IF AUTHX SUPPLIED         */
*             THEN                                              02S0511
@RF00506 ICM   @14,15,ATCAUTHA(ATCVTPTR)                        02S0511
         BZ    @RF00511                                         02S0511
*         /***********************************************************/
*         /* THE FUNCTION OF THIS LOGIC SEGMENT IS TO INVOKE         */
*         /* THE INSTALLATION SUPPLIED AUTHORIZATION EXIT            */
*         /* ROUTINE, IF PRESENT, TO DETERMINE WHETHER THE           */
*         /* REQUESTOR IS AUTHORIZED TO ISSUE THE REQUEST OR         */
*         /* NOT.                                                    */
*         /***********************************************************/
*CHECAUTH:DO;             /*BGN OF CHECAUTH LOGIC SEGMENT            */
*                                                               03S0512
CHECAUTH DS    0H                                               03S0513
*         /***********************************************************/
*         /* THE FUNCTION OF THIS LOGIC SEGMENT IS TO INVOKE         */
*         /* THE SETCRR MACRO TO FLAG THE CONNECTION SERVICE         */
*         /* RECOVERY RECORD TO INDICATE THAT THE AUTHORIZA-         */
*         /* TION EXIT FUNCTION OF ISTCSCMR HAS BEEN ENTERED.        */
*         /***********************************************************/
*FLGAUIN :DO;             /*BGN OF FLGAUIN  LOGIC SEGMENT            */
*                                                               04S0513
*                                                               04S0513
FLGAUIN  DS    0H                                               04S0514
*           DO; /* SETCRR FNID(CNSAUTH)TYPE(SENTRY) */          04S0514
*             CRACRR->CNSAUTHA='1'B; /*SET HISTORY FLAG*/       04S0515
         L     @14,RPHCRR(,RPHPTR)                              04S0515
         L     @14,CRACRR(,@14)                                 04S0515
         OI    CNSAUTHA(@14),B'01000000'                        04S0515
*             CRACRR->CNSAUTHB='1'B; /*SET CURRENT FLAG*/       04S0516
         OI    CNSAUTHB(@14),B'01000000'                        04S0516
*           END; /*SETCRR*/                                     04S0517
*                                                               04S0517
*         END FLGAUIN;    /*END OF FLGAUIN  LOGIC SEGMENT            */
