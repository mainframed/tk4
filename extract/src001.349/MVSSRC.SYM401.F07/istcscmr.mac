*CHECPARM:DO;             /*BGN OF CHECPARM LOGIC SEGMENT            */
*                                                               03S0476
*                                                               03S0476
CHECPARM DS    0H                                               03S0477
*           DO; /* BLOCK  */                   /*BGN OF LOGIC BLOCK SG
*       */                                                      03S0477
*             RRET    =GOOD;         /*SETUP GOOD RET CODE           */
         SLR   RRET,RRET                                        03S0478
*             CHECPFLG=MASKFLAG(CSPIDX)&&CSPOPT; /* GET PARM         */
         LA    @14,124                                          03S0479
         ALR   @14,SAVEPTR                                      03S0479
         LH    @05,CSPIDX(,CSPPTR)                              03S0479
         ALR   @05,@05                                          03S0479
         LA    @04,MASKFLAG-2(@05)                              03S0479
         MVC   CHECPFLG(2,@14),0(@04)                           03S0479
         XC    CHECPFLG(2,@14),CSPOPT(CSPPTR)                   03S0479
*             IF CPFSRSC=BITOF       /*IF SINGLE RSCE RQST           */
*               THEN                                            03S0480
         TM    CPFSRSC(@14),B'10000000'                         03S0480
         BNZ   @RF00480                                         03S0480
*                 CSPNO=1;           /*SETUP CSP LIST COUNT          */
         MVC   CSPNO(2,CSPPTR),@CH00038                         03S0481
*               ELSE;                /*MULT RQST - ALRDY SET         */
@RF00480 DS    0H                                               03S0483
