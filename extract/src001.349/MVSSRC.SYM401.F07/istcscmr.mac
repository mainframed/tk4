@RC00522 L     ENTRYREG,ATCAUTHA(,ATCVTPTR)                     03S0525
*           SAVEPTR=SAVEPTR+LNGTHSAV;/*GET NEW SAVE AREA PTR         */
         AH    SAVEPTR,@CH03523                                 03S0526
*           CALL MNTRN;              /*CHECK AUTHORIZATION           */
         BALR  @14,@15                                          03S0527
*           SAVEPTR=SAVEPTR-LNGTHSAV;/*GET OLD SAVE AREA PTR         */
         SH    SAVEPTR,@CH03523                                 03S0528
*           RPHPTR=WK2;              /*RESTORE RPH POINTER           */
         L     RPHPTR,WK2+124(,SAVEPTR)                         03S0529
*           CSPRSCE=RPHCSPA;         /* RESTORE CSPRSCE      @ZA10947*/
         MVC   CSPRSCE(4,CSPPTR),RPHCSPA(RPHPTR)                03S0530
*           RPHCSPA=CSPPTR;          /* RESTORE RPH CSP ADDR @ZA10947*/
         ST    CSPPTR,RPHCSPA(,RPHPTR)                          03S0531
*           IF RRET ^= 0             /* AUTH OR NOT?         @ZA18910*/
*             THEN CSPCC = UNAUTH;   /* NOT AUTH.            @ZA18910*/
         CH    RRET,@CH00619                                    03S0532
         BE    @RF00532                                         03S0532
         MVC   CSPCC(2,CSPPTR),@CH00040                         03S0533
*           ELSE;                    /*                      @ZA18910*/
@RF00532 DS    0H                                               03S0535
*         /***********************************************************/
*         /* THE FUNCTION OF THIS LOGIC SEGMENT IS TO INVOKE         */
*         /* THE SETCRR MACRO TO FLAG THE CONNECTION SERVICE         */
