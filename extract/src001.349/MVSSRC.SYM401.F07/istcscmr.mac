         ST    RPHPTR,WK2+124(,SAVEPTR)                         03S0519
*           RPHCSPA=CSPRSCE;         /* SAVE ORIGINAL CSPRSCE DURING
*                                       CALL TO AUTH RTN     @ZA10947*/
         L     @14,CSPRSCE(,CSPPTR)                             03S0520
         ST    @14,RPHCSPA(,RPHPTR)                             03S0520
*           PARMPTR=CSPPTR;          /* REG 1 EQ PARM LIST   @ZA10947*/
         LR    PARMPTR,CSPPTR                                   03S0521
*           IF CSPRSC='1'B           /* GROUP LIST           @ZA10947*/
*             THEN                   /* YES                  @ZA10947*/
         TM    CSPRSC(CSPPTR),B'10000000'                       03S0522
         BNO   @RF00522                                         03S0522
*           CSPRSCE=CSPRSCE+(CSPNO*LENGTH(ISTCSL));             03S0523
         LH    @02,CSPNO(,CSPPTR)                               03S0523
         MH    @02,@CH00440                                     03S0523
         ALR   @14,@02                                          03S0523
         ST    @14,CSPRSCE(,CSPPTR)                             03S0523
*                                    /*GET CONTIG AUTH LIST  @D32CKMP*/
*             ELSE                   /* SINGLE LIST (1 CSL)  @ZA10947*/
*           CSPRSCE=CSPRSCE->CSLRDTE; /* THIS WILL POINT DIRECTLY TO
*                                       THE RDTE NAME        @ZA10947*/
         B     @RC00522                                         03S0524
@RF00522 L     @14,CSPRSCE(,CSPPTR)                             03S0524
         L     @14,CSLRDTE(,@14)                                03S0524
         ST    @14,CSPRSCE(,CSPPTR)                             03S0524
