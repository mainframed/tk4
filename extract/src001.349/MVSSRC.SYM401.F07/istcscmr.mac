SVTM     TITLE 'ISTCSCMR - CONNECTION SERVICES MASTER ROUTER           *
                        '
         ISEQ  73,80
         PRINT OFF
         PRINT ON
*         /***********************************************************/
*         /* THE FUNCTION OF THIS LOGIC SEGMENT IS TO SETUP          */
*         /* THE ENTRY ENVIRONMENT NECESSARY WHEN THE MASTER         */
*         /* ROUTER RECEIVES CONTROL FROM A FUNCTION THAT IS         */
*         /* EXTERNAL TO THE CONNECTION SERVICES                     */
*         /* SUB-FUNCTION OF OPEN/CLOSE.  THE MASTER ROUTER          */
*         /* RECEIVES CONTROL TO ROUTE AN ACCEPT, ACQUIRE,           */
*         /* SIMLOGON, CANCEL, LOGON, PASS OR RELEASE                */
*         /* REQUEST TO THE PROPER FUNCTION PROCESSOR.               */
*         /***********************************************************/
*MSTREENV:DO;             /*BGN OF MSTREENV LOGIC SEGMENT            */
*                                                               03S0451
MSTREENV DS    0H                                               03S0452
*           GEN  REFS(ENTRYREG,ISTCSCMR,                        03S0452
*                     WAIT,POST,QUEUE,SEARCHQ,                  03S0452
*                     TCONNECT,ACONNECT,GETSTORE,LOGON0,TPQ,    03S0452
*                     FREESTOR,DOACCT)                          03S0452
*               FLOWS(EXTCALL);                            /*@XM07935*/
*                                                               03S0452
ISTCSCMR CSECT ,           OS/VS2 ENTRY POINT NAME           @XM07935
@MAINENT DS    0H                                            @XM8798
         ENTRY WAIT,POST,QUEUE,SEARCHQ,TCONNECT,ACONNECT
         ENTRY GETSTORE,LOGON0,TPQ,FREESTOR,DOACCT
         USING ISTCSCMR,ENTRYREG   PROVIDE ADDRESSABILITY
         B     EXTCALL             BYPASS ID & VECTOR TABLE
* GEN(DC AL1(16),CL16'ISTCSCMR  77.089');                       03S0453
         DC AL1(16),CL16'ISTCSCMR  77.089'
* GEN(DC AL3(00));                                                   /*
*X04BHYY*/                                                      03S0454
         DC AL3(00)
*          GEN REFS(BASEREG,ENTRYREG,ISTCSCAC,ISTCSCAQ,         03S0455
*                   ISTSCCCL,ISTCSCLG,ISTCSCMR,ISTCSCPS,ISTCSCRL);
CSVECTOR DS    0H                  START OF VECTOR TABLE     @D32CKMP
CVSACQ   DC    V(ISTCSCAQ)         ACQUIRE/SIMLOGON PROCESSOR
CSVREL   DC    V(ISTCSCRL)         RELEASE PROCESSOR
CSVACC   DC    V(ISTCSCAC)         ACCEPT PROCESSOR
CSVLOG   DC    V(ISTCSCLG)         LOGON PROCESSOR
CSVPASS  DC    V(ISTCSCPS)         PASS PROCESSOR
         DC    A(0)
CSVCANC  DC    V(ISTCSCCL)         CANCEL PROCESSOR
CSVWAITA DC    A(WAIT)             WAIT SUBROUTINE
CSVPOSTA DC    A(POST)             POST SUBROUTINE
CSVQUEU  DC    A(QUEUE)            QUEUE SUBROUTINE
CSVSEAR  DC    A(SEARCHQ)          SEARCH QUEUE SUBROUTINE
CSVTCON  DC    A(TCONNECT)         CONNECT TRML SUBROUTINE
CSVACON  DC    A(ACONNECT)         CONNECT APPL SUBROUTINE
         DC    A(0)
         DC    A(0)
CSVGETS  DC    A(GETSTORE)         GET CE/UECB SUBROUTINE
CSVLOG0  DC    A(LOGON0)           COMPLETE LOGON SUBROUTINE
CSVTPQA  DC    A(TPQ)              TPQ UECB ON OWN PAB SUB
CSVFREES DC    A(FREESTOR)         FREE CE/UECB SUBROUTINE
CSVDOACC DC    A(DOACCT)           CALL ACCT RTN SUBROUTINE
         USING ISTCSCMR,BASEREG    ESTABLISH NEW BASE
         DROP  ENTRYREG            DROP PREVIOUS BASE
*EXTCALL:;                                                      03S0456
EXTCALL  DS    0H                                               03S0457
*           GEN  SETS(SAVE114)                                  03S0457
*                REFS(RETR,ATCVTPTR,SAVEPTR);                   03S0457
*                                                               03S0457
         STM   RETR,BASEREG,SAVE114(SAVEPTR) SAVE CALL REGS  @XM01122
*           BASEREG=ENTRYREG;        /*ESTABLISH BASE REG            */
*                                                               03S0458
         LR    BASEREG,ENTRYREG                                 03S0458
*         /***********************************************************/
*         /* THE FUNCTION OF THIS LOGIC SEGMENT IS TO INVOKE         */
*         /* THE LISATCVT MACRO TO LOCATE THE PAGEABLE               */
*         /* PORTION OF THE VTAM COMMUNICATION VECTOR TABLE          */
*         /* AND RETURN ITS ADDRESS IN ATCVTPTR FOR THE              */
*         /* MAINLINE FUNCTION.                                      */
*         /***********************************************************/
*FINDCVT :DO;             /*BGN OF FINDCVT  LOGIC SEGMENT            */
*                                                               04S0459
*                                                               04S0459
FINDCVT  DS    0H                                               04S0460
*           DO; /* LISATCVT CVTBASE(PAGEABLE,(ATCVTPTR)) */     04S0460
*             ATCVTPTR = ATCLCPTR; /*SET UP ATCVT POINTER*/     04S0461
         L     ATCVTPTR,ATCLCPTR                                04S0461
*           END; /*LISATCVT*/                                   04S0462
*                                                               04S0462
*         END FINDCVT;    /*END OF FINDCVT  LOGIC SEGMENT            */
*           CNSTFLDA=CNSTFLDA&&CNSTFLDA;/*CLEAR CRR HIST FLG         */
         L     @14,RPHCRR(,RPHPTR)                              03S0464
         L     @14,CRACRR(,@14)                                 03S0464
         XC    CNSTFLDA(3,@14),CNSTFLDA(@14)                    03S0464
*         /***********************************************************/
*         /* THE FUNCTION OF THIS LOGIC SEGMENT IS TO INVOKE         */
*         /* THE SETCRR MACRO TO FLAG THE CONNECTION SERVICE         */
*         /* RECOVERY RECORD TO INDICATE THAT THE MAIN LOGIC         */
*         /* OF ISTCSCMR HAS BEEN ENTERED.                           */
*         /***********************************************************/
*FLGMRIN :DO;             /*BGN OF FLGMRIN  LOGIC SEGMENT            */
*                                                               04S0465
*                                                               04S0465
FLGMRIN  DS    0H                                               04S0466
*           DO; /* SETCRR FNID(CNSMRIN)TYPE(ENTRY) */           04S0466
*             CRACRR->CNSTFLDB = ''B; /*CLEAR CURRENT TRACE FIELD*/
         L     @14,RPHCRR(,RPHPTR)                              04S0467
         L     @14,CRACRR(,@14)                                 04S0467
         XC    CNSTFLDB(3,@14),CNSTFLDB(@14)                    04S0467
*             CRACRR->CNSMRINA='1'B; /*SET HISTORY FLAG*/       04S0468
         OI    CNSMRINA(@14),B'10000000'                        04S0468
*             CRACRR->CNSMRINB='1'B; /*SET CURRENT FLAG*/       04S0469
         OI    CNSMRINB(@14),B'10000000'                        04S0469
*           END; /*SETCRR*/                                     04S0470
*                                                               04S0470
*         END FLGMRIN;    /*END OF FLGMRIN  LOGIC SEGMENT            */
*           CSPPTR  =RPHCSPA;        /*ACCESS INPUT PARMS            */
         L     CSPPTR,RPHCSPA(,RPHPTR)                          03S0472
*           CSPCC   =0;              /*CLEAR RETURN CODE     @D32CKMP*/
*                                                               03S0473
         SLR   @14,@14                                          03S0473
         STH   @14,CSPCC(,CSPPTR)                               03S0473
*         END MSTREENV;   /*END OF MSTREENV LOGIC SEGMENT            */
*                                                               02S0475
*                                                               02S0475
*         DO; /* BLOCK MAIN */                    /*START MAIN LOGIC
*      */                                                       02S0475
*         /***********************************************************/
*         /* THE FUNCTION OF THIS LOGIC SEGMENT IS TO CHECK,         */
*         /* ACCORDING TO THE FUNCTION REQUESTED, THE                */
*         /* APPROPRIATE RDT ENTRY POINTERS PASSED.  EACH            */
