         BAL   @14,WTORMSG                                         0300
*       END;                        /*                       @Z40GKD1*/
*     ELSE                                                         0302
*       DO;                         /* NOT SRB OR WTOR       @Z40GKD1*/
         B     @RC00298                                            0302
@RF00298 DS    0H                                                  0303
*         CALL WTOMSG;              /* CALL RTN TO SCHED WTO OR PO 0303
*                                      MSG                   @Z40GKD1*/
         BAL   @14,WTOMSG                                          0303
*       END;                        /*                       @Z40GKD1*/
*   IF MTMBUFPT=0                   /* ANY BUFFER TO FREE?   @Z40GKD1*/
*     THEN                                                         0305
@RC00298 DS    0H                                                  0305
@RC00294 LTR   MTMBUFPT,MTMBUFPT                                   0305
         BZ    @RT00305                                            0305
*     ;                             /* NO                    @Z40GKD1*/
*   ELSE                                                           0307
