         NOPR 9
*         RFY                                                      0380
*          (GPR00P,                                                0380
*           GPR01P,                                                0380
*           GPR05P) UNRSTD;                                        0380
*       END;                        /* **** END TPSCHED              */
*       MTMERR=MTMERR×HIBITON;      /* INDICATE ERROR        @Z40GKD1*/
         O     MTMERR,@CF01227                                     0382
*     END;                          /* END NO BUFFER AVAILABLE     0383
*                                                            @Z40GKD1*/
*   ELSE                                                           0384
*     DO;                           /* GOT A BUFFER, NOW POINT     0384
*                                      MTMWPLPT TO IT, MOVE MSG TO IT
*                                                            @Z40GKD1*/
         B     @RC00370                                            0384
@RF00370 DS    0H                                                  0385
*       MTMWPLPT=ADDR(POWWPL)-8;    /* SETWTO PLIST BASE TO PT TO  0385
*                                      AREA IN POWE          @Z40GKD9*/
         LA    MTMWPLPT,POWWPL(,MTMBUFPT)                          0385
         SL    MTMWPLPT,@CF00064                                   0385
*       CALL MOVEBUF;               /* ON RETURN, MSG IS FORMATTED 0386
*                                                            @Z40GKD1*/
         BAL   @14,MOVEBUF                                         0386
*       DO;                         /* LISATCVT                    0387
*                                      CVTBASE(PAGEABLE,(MTMTPTR))   */
*         MTMTPTR=ATCLCPTR;         /* SET UP ATCVT POINTER          */
         L     MTMTPTR,ATCLCPTR                                    0388
*       END;                        /* LISATCVT RESTORE PTR  @Z40GKD9*/
*       IF(ATCMSGSP&MTMMSGSP)^=0    /*                       @Z40GKD1*/
*         THEN                      /* MESSAGE IS TO BE SUPPRESSED 0390
*                                                            @Z40GKD1*/
         SLR   @10,@10                                             0390
         IC    @10,ATCMSGSP(,MTMTPTR)                              0390
         SLR   @09,@09                                             0390
         IC    @09,MTMMSGSP(,MTMMSGPT)                             0390
         NR    @10,@09                                             0390
         LTR   @10,@10                                             0390
         BZ    @RF00390                                            0390
*         WPLMCSFG=ON;              /* SO SET HARD-COPY ONLY FLAG  0391
*                                                            @Z40GKD1*/
         OI    WPLMCSFG+8(MTMWPLPT),B'00000010'                    0391
*       ELSE                                                       0392
*         DO;                       /* IF NOT TO BE SUPPRESSED     0392
*                                                            @Z40GKD1*/
         B     @RC00390                                            0392
@RF00390 DS    0H                                                  0393
*           MTMR15=MTMSAVEF;        /* RESTORE OPTION FLD ADDR     0393
*                                                            @Z40GKD1*/
*                                                                  0393
         L     MTMR15,MTMSAVEF(,MTMR13)                            0393
*           /*********************************************************/
*           /*                                                       */
*           /* UNSOLICITED PO MSG                                    */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0394
*           IF((ATCPOUNS=ON)&(MTMR15=0))×/* IF PO ACTIVE & NO OPTNS  */
*               ((ATCPOUNS=ON)&(MTMR15^=0)&/* OR PO ACTIVE, OPTIONS  */
*               (NCSPLNID='00'X))×  /* AND NO CONSOLE ID     @ZM41323*/
*                                                                  0394
*           /*********************************************************/
*           /*                                                       */
*           /* SOLICITED PO MSG                                      */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0394
