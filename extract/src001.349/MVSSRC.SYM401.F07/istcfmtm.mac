*/*------------------------------------------------------------------*/
*                                                                  0269
*   DO WHILE((MTMEND-MTMSTART)>8&MTMARGID(1:3)^=MTMMSGPT->MTMMSID);
         B     @DE00269                                            0269
@DL00269 DS    0H                                                  0270
*     MTMMSGPT=(MTMSTART+MTMEND)/2&MTMMASK;                        0270
         LR    @08,MTMSTART                                        0270
         ALR   @08,MTMEND                                          0270
