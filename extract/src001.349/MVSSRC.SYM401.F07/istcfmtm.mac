         B     @PB00002                                            0350
SRBMSG   DS    0H                                                  0351
*/*  ******************************************************************
*  * THIS SUBROUTINE PROCESSES MESSAGES REQUESTED BY A CALLING        *
*  * ROUTINE RUNNING UNDER AN SRB.  A BUFFER MUST BE OBTAINED. IF     *
*  * STORAGE IS NOT AVAILABLE, THE TPMSG PAB IS QUEUED TO ISSUE       *
*  * AN APPROPRIATE ERROR MESSAGE.  IF STORAGE IS AVAILABLE AND THE   *
*  * MESSAGE WAS TO BE SENT TO THE PROGRAM OPERATOR, THE BUFFER WITH  *
*  * THE MESSAGE APPROPRIATELY FORMATTED IS QUEUED TO THE PGM OPERATOR*
*  * PAB.  IF THE MESSAGE WAS NOT FOR THE PGM OPER, THE BUFFER IS     *
*  * QUEUED TO THE TPMSG PAB.  WHEN THE APPROPRIATE PAB HAS BEEN      *
*  * SCHEDULED, THE BUFFER ADDRESS IS SET = 0 TO INDICATE IT IS       *
*  * NOT TO BE RELEASED, AND RETURN IS MADE TO THE MAINLINE CODE.     *
*  *                                                                  *
*  * INPUT: BASE REG = REG 11                                         *
*  *        @ OF MSG IN MSG CSECT = REG 6                             *
*  *        @ OF PARAM LIST = REG 7                                   *
*  *        @ OF ATCVT = REG 5                                        *
*  *        @ OF CALLER'S SAVE AREA = REG 13                          *
*  *        RETURN ADDR FOR THIS ROUTINE = REG 14                     *
*  *******************************************************************/
*                                                                  0351
*   MTMSAV14=MTMR14;                /* SAVE RETURN ADDR OVER CALL  0351
*                                                            @Z40GKD1*/
         LR    MTMSAV14,MTMR14                                     0351
*   GPR01P=MTMMSGPT;                /* SAVE REG USED BY GETSTOR    0352
*                                                            @ZM40278*/
         LR    GPR01P,MTMMSGPT                                     0352
*   ADDR(ATCTPMPB)->DYPTSKID=ADDR(ATCZPAB2)->DYPTSKID;/* MOVE PST  0353
*                                      ADDR INTO MSG DYPAB   @ZM41311*/
*                                                                  0353
         L     @10,DYPTSKID+1136(,MTMTPTR)                         0353
         ST    @10,DYPTSKID+1512(,MTMTPTR)                         0353
*   /*****************************************************************/
*   /*                                                               */
*   /* GETSTOR SIZE(LENGTH(ISTPOWE))CLEAR(YES)SAVE(NO)FETCHP(NO)     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0354
*   DO;                                                            0354
*                                                                  0354
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)                        */
*/* END OF ATBUILD                                                   */
*                                                                  0355
*     RFY                                                          0355
*      (GPR00P,                                                    0355
*       GPR01P,                                                    0355
*       GPR02P,                                                    0355
*       GPR03P) RSTD;                                              0355
*     GPR00F=LENGTH(ISTPOWE);       /* SIZE IN REG0                  */
         LA    GPR00F,283                                          0356
*     DCL                                                          0357
*       1 CMZA0037 FIXED(31) STATIC LOCAL,                         0357
*        2 * FIXED(8) INIT(32),     /* OPTION FLAGS                  */
*        2 * FIXED(8) INIT(96),     /* STORAGE KEY                   */
*        2 * FIXED(8) INIT(241),    /* SUBPOOL NUMBER                */
*        2 * FIXED(8) INIT('00'X);  /* REQUEST TYPE                  */
*     GPR03F=CMZA0037;                                             0358
         L     GPR03F,CMZA0037                                     0358
*     DO;                           /* LISATCVT                    0359
*                                      CVTBASE(PAGEABLE,(GPR02P))    */
*       GPR02P=ATCLCPTR;            /* SET UP ATCVT POINTER          */
         L     GPR02P,ATCLCPTR                                     0360
*     END;                          /* LISATCVT                      */
*     CALL GPR02P->ATCGTSTR;        /* CALL ISTORFSR                 */
         L     @15,ATCGTSTR(,GPR02P)                               0362
         BALR  @14,@15                                             0362
*     RFY                                                          0363
*      (GPR00P,                                                    0363
*       GPR01P,                                                    0363
*       GPR02P,                                                    0363
*       GPR03P) UNRSTD;                                            0363
*   END;                            /* END OF GETSTOR                */
*                                                                  0364
*   /*****************************************************************/
*   /*                                                               */
*   /* BUFFER RETURNED IN REG 3                              @Z40GKD1*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0365
*   MTMMSGPT=GPR01P;                /* RESTORE REG 6         @ZM40278*/
         LR    MTMMSGPT,GPR01P                                     0365
*   MTMPARPT=MTMSAVE1;              /* PARM PTR                      */
         L     MTMPARPT,MTMSAVE1(,MTMR13)                          0366
*   DO;                             /* LISATCVT                    0367
*                                      CVTBASE(PAGEABLE,(MTMTPTR))   */
*     MTMTPTR=ATCLCPTR;             /* SET UP ATCVT POINTER          */
         L     MTMTPTR,ATCLCPTR                                    0368
*   END;                            /* LISATCVT RESTORE PTR  @Z40GKD1*/
*   IF GPR15F^=0 THEN               /* IF GETSTORE FAILED    @Z40GKD1*/
         SLR   @10,@10                                             0370
         CR    GPR15F,@10                                          0370
         BE    @RF00370                                            0370
*     DO;                                                          0371
*       MTMBUFPT=0;                 /* SET REG3 TO ZERO      @Z40GKD1*/
         LR    MTMBUFPT,@10                                        0372
*       NOCORE=ON;                  /* SET NOCORE FLAG ON IN PSS DVT
*                                      USING GENERATED CS LOGIC    0373
*                                                            @Z40GKD1*/
         L     @10,NOCORE(,MTMTPTR)                                0373
@GL00003 LR    @09,@10                                             0373
         O     @09,@CF02456                                        0373
         CS    @10,@09,NOCORE(MTMTPTR)                             0373
         BNE   @GL00003                                            0373
*/* TPSCHED (ADDR(ADDR(ATCTPMPB)->DYPPAB)) NSTR ENA UNCOND           */
*/*                          CMZVER = OS/VS2                         */
*                                                                  0374
