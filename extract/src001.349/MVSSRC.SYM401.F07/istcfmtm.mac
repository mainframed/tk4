*         GPR00P=0;                                                0376
         SLR   GPR00P,GPR00P                                       0376
*         GPR05P=ADDR(ADDR(ATCTPMPB)->DYPPAB);                     0377
         LA    @10,DYPPAB+1512(,MTMTPTR)                           0377
         LR    GPR05P,@10                                          0377
*         CALL ATCLCPTR->ATCAP38->ISTAPC38;                        0378
         L     @10,ATCLCPTR                                        0378
         L     @15,ATCAP38(,@10)                                   0378
         BALR  @14,@15                                             0378
*         GEN(NOPR 9)                                              0379
