LOGON0L1 DS    0H                                               06S0614
*                               RPREPTR=CSLRDTE;/* POINT AT TRML RDTE
*                                      TO WHICH THE CONNECTION IS TO
*                                      BE MADE               @D32CKMP*/
         LR    @15,RSCLINDX                                     06S0614
         BCTR  @15,0                                            06S0614
         MH    @15,@CH00447                                     06S0614
         LR    @14,RSCLPTR                                      06S0614
         ALR   @14,@15                                          06S0614
         L     RPREPTR,CSLRDTE(,@14)                            06S0614
*                               ICEPTR=CES;/* TAKE ONE CE OFF CHAIN  */
         L     ICEPTR,CES(,RSAVE)                               06S0615
*                               RFY                             06S0616
*                                 CHAINFLD BASED(ICEPTR);       06S0616
*                               CES=CHAINFLD;/* LEAVING REST OF CHAIN*/
         MVC   CES(4,RSAVE),CHAINFLD(ICEPTR)                    06S0617
*                               CHAINFLD=0;/* RESET SMS CHAIN FIELD  */
         SLR   @14,@14                                          06S0618
         ST    @14,CHAINFLD(,ICEPTR)                            06S0618
*                               ICEDATA=CSLDATA;/* POINT ICE AT LOGON
*                                      MESSAGE               @D32CKMP*/
         ALR   @15,RSCLPTR                                      06S0619
         MVC   ICEDATA(4,ICEPTR),CSLDATA(@15)                   06S0619
*                               IF UECBS^=0 THEN                06S0620
         L     @15,UECBS(,RSAVE)                                06S0620
         CR    @15,@14                                          06S0620
         BE    @RF00620                                         06S0620
*                                 DO;/* IF UECBS WERE OBTAINED       */
*                                   UECBPTR=UECBS;/* POINT AT FIRST
*                                      UECB ON CHAIN                 */
         LR    UECBPTR,@15                                      06S0622
*                                   RFY                         06S0623
*                                     CHAINFLD BASED(UECBPTR);  06S0623
*                                   UECBS=CHAINFLD;/* UNHOOK UECB
*                                      FROM REST OF CHAIN            */
         MVC   UECBS(4,RSAVE),CHAINFLD(UECBPTR)                 06S0624
