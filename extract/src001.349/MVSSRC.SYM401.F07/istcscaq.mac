         BALR  @14,@15                                          06S0631
*                               RFY                             06S0632
*                                 ENTRYR UNRSTD;                06S0632
*                             END;  /* END OF QLOG1 LOGIC SEGMENT
*                                                            @D32CKMP*/
*                             ICXREQCT=ICXREQCT-1;/* DECREMENT CONNTS
