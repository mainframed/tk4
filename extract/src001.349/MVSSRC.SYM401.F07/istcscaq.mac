@RF00691 DS    0H                                               05S0697
*STAPTST:                                                       05S0697
*                                                               06S0697
*/********************************************************************/
*/* THE FUNCTION OF THIS LOGIC SEGMENT IS TO QUEUE ANY               */
*/* UECBS ON THE USER EXIT PAB OF THE OWNER IF SHOULDER              */
*/* TAPPING IS REQUIRED, OTHERWISE, TO FREE THE UECB                 */
*/* STORAGE AREA OBTAINED.                                           */
*/********************************************************************/
*                                                               06S0697
*                             DO;   /* BGN OF STAPTST LOGIC SEGMENT
*                                                            @D32CKMP*/
@RF00687 DS    0H                                               06S0697
STAPTST  DS    0H                                               06S0698
*                               RESPECIFY                       06S0698
*                                 ISTRCPRE BASED(RPREPTR);      06S0698
*                               RFY                             06S0699
*                                 STAPER RSTD;                  06S0699
*                               RFY                             06S0700
*                                 RAPPTR UNRSTD;                06S0700
*                               IF RPRTIED='1'B THEN/* IF TERMINAL IS
*                                      TIED                          */
         TM    RPRTIED(RPREPTR),B'00100000'                     06S0701
         BNO   @RF00701                                         06S0701
*                                 IF CSPSTAP='1'B THEN          06S0702
         TM    CSPSTAP(CSPPTR),B'10000000'                      06S0702
         BNO   @RF00702                                         06S0702
*                                   DO;/* IF SHOULDER TAPPING IS
*                                      SPECIFIED                     */
*                                    RFY                        06S0704
*                                      CHAINFLD BASED(UECBPTR); 06S0704
*                                    UECBPTR=UECBS;/* GET A UECB     */
         L     UECBPTR,UECBS(,RSAVE)                            06S0705
*                                    UECBS=CHAINFLD;/* FROM CHAIN OF
*                                      UECBS                         */
         MVC   UECBS(4,RSAVE),CHAINFLD(UECBPTR)                 06S0706
*                                    CHAINFLD=0;/* RESET SMS CHAIN
*                                      FIELD                         */
         SLR   @09,@09                                          06S0707
         ST    @09,CHAINFLD(,UECBPTR)                           06S0707
*                                    IF RCPACETR=ADDR(RCPACETR) THEN
         LA    @09,RCPACETR(,RPREPTR)                           06S0708
         C     @09,RCPACETR(,RPREPTR)                           06S0708
         BNE   @RF00708                                         06S0708
*                                     DO;/* FOR NO TRUE OWNER        */
*RELSTOR1:                          /* FREE THE UECB                 */
*                                      FREEOFF=-1;/* FREEOFF -VE TO
*                                      INDICATE SINGLE RELEASE       */
RELSTOR1 SLR   FREEOFF,FREEOFF                                  06S0710
         BCTR  FREEOFF,0                                        06S0710
*                                      RFY                      06S0711
*                                        SERTNPTR BASED(ATCCSMA+4*(
*                                            CSPIFSTR+4));/* @D32CKMP*/
*                                      RFY                      06S0712
*                                        ENTRYR RSTD;           06S0712
*                                      ENTRYR=SERTNPTR;/* POINT AT
*                                      FREESTOR ROUTINE              */
         L     @09,ATCCSMA(,ATCVTPTR)                           06S0713
         L     ENTRYR,SERTNPTR+96(,@09)                         06S0713
*                                      CALL MXTRN;              06S0714
         BALR  @14,@15                                          06S0714
*                                      RFY                      06S0715
*                                        ENTRYR UNRSTD;         06S0715
*                                     END;                      06S0716
*                                    ELSE                       06S0717
*                                     DO;/* FIND TRUE OWNER          */
         B     @RC00708                                         06S0717
@RF00708 DS    0H                                               06S0718
*                                      STAPER=RCPACETR;/* STAPER
*                                      GIVES FIRST ICE               */
         L     STAPER,RCPACETR(,RPREPTR)                        06S0718
*                                      DO WHILE(STAPER->ACEOBJQ^=ADDR(
*                                           RCPACETR));/* SCAN FOR
*                                      END OF CHAIN                  */
         B     @DE00719                                         06S0719
@DL00719 DS    0H                                               06S0720
*                                       STAPER=STAPER->ACEOBJQ; 06S0720
         L     STAPER,ACEOBJQ(,STAPER)                          06S0720
*                                      END;                     06S0721
@DE00719 LA    @09,RCPACETR(,RPREPTR)                           06S0721
         C     @09,ACEOBJQ(,STAPER)                             06S0721
         BNE   @DL00719                                         06S0721
*                                      STAPER=STAPER->ACESUBJ;/* TRUE
*                                      OWNER                         */
         L     STAPER,ACESUBJ(,STAPER)                          06S0722
*                                      UECTYPE=UECRELRQ;/* EXIT IS
*                                      SHOULDERTAP                   */
         MVI   UECTYPE(UECBPTR),X'0A'                           06S0723
*                                      UECACB=STAPER->RAPACBA;/*
*                                      OWNERS ACB                    */
         MVC   UECACB(4,UECBPTR),RAPACBA(STAPER)                06S0724
*                                      UECNDID=ADDR(UECSYMNM);/*
*                                      POINT TO SYMBOLIC NAME        */
         LA    @09,UECSYMNM(,UECBPTR)                           06S0725
         ST    @09,UECNDID(,UECBPTR)                            06S0725
*                                      UECSYMNM=RPRNAME;/* TERMINAL
