         ST    ICEPTR,FICE(,RSAVE)                              05S0675
*                               END;/*                       @DL31HBS*/
*                             ELSE                              05S0677
*                               DO; /*                       @DL31HBS*/
         B     @RC00672                                         05S0677
@RF00672 DS    0H                                               05S0678
*                                 LICE->ICESIDE=ICEPTR;/* UPDATE
*                                      ICESIDE PTR           @DL31HBS*/
         L     @15,LICE(,RSAVE)                                 05S0678
         ST    ICEPTR,ICESIDE(,@15)                             05S0678
*                                 LICE=ICEPTR;/* UPDATE LAST ICE
*                                      QUEUED PTR            @DL31HBS*/
         ST    ICEPTR,LICE(,RSAVE)                              05S0679
*                               END;/*                       @DL31HBS*/
*                             ICERPHA=RPHPTR;/* SET PTR TO RPLH OF
*                                      QUEUED REQUEST                */
@RC00672 ST    RPHPTR,ICERPHA(,ICEPTR)                          05S0681
*                             IF CSPLOGI='1'B THEN/* IF LOGONI  05S0682
