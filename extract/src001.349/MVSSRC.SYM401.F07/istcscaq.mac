         LA    @15,1                                            05S0655
         AH    @15,RPRSTPCT(,RPREPTR)                           05S0655
         STH   @15,RPRSTPCT(,RPREPTR)                           05S0655
*                               END;/* SHOULDER TAPPING IS DESIRED   */
*SEARCHQ:                                                       05S0657
*                             RPHSAVE1=ICEPTR;/* MAKE ICE THE CE TO
*                                      BE QUEUED                     */
@RF00652 DS    0H                                               05S0657
SEARCHQ  ST    ICEPTR,RPHSAVE1(,RPHPTR)                         05S0657
*                             RPHSAVE2=ADDR(RAPICEAQ);/* HEAD OF
*                                      CHAIN IS APLN ACQBYME FIELD   */
         LA    @15,RAPICEAQ(,RAPPTR)                            05S0658
         ST    @15,RPHSAVE2(,RPHPTR)                            05S0658
