         TM    CSPLOGI(CSPPTR),B'00100000'                      05S0691
         BNO   @RF00691                                         05S0691
         LTR   UECBPTR,UECBPTR                                  05S0691
         BZ    @RF00691                                         05S0691
*                                   DO;                         05S0692
*                                    UECFORW=ICXUECBA;/* QUEUE UECB
