         LA    @15,RCPICEAQ(,RPREPTR)                           05S0665
         ST    @15,RPHSAVE2(,RPHPTR)                            05S0665
*                             RPHSAVE3=ADDR(ICEOBJQ)-ADDR(ISTICE);/*
*                                      LINK FIELD OFFSET             */
         LA    @15,ICEOBJQ(,ICEPTR)                             05S0666
         SLR   @15,ICEPTR                                       05S0666
         ST    @15,RPHSAVE3(,RPHPTR)                            05S0666
*                             RFY                               05S0667
*                               SERTNPTR BASED(ATCCSMA+4*(CSPIQ+4));
*                                   /*                       @D32CKMP*/
*                             RFY                               05S0668
*                               ENTRYR RSTD;                    05S0668
*                             ENTRYR=SERTNPTR;/* POINT AT QUEUE 05S0669
*                                      SERVICE ROUTINE               */
         L     @14,ATCCSMA(,ATCVTPTR)                           05S0669
         L     ENTRYR,SERTNPTR+60(,@14)                         05S0669
*QUEUE2:                                                        05S0670
*                             CALL MXTRN;                       05S0670
QUEUE2   BALR  @14,@15                                          05S0670
*                             RFY                               05S0671
*                               ENTRYR UNRSTD;                  05S0671
*                             IF LICE=0/* IF LAST ICE QUEUED PTR 0
*                                                            @DL31HBS*/
*                               THEN                            05S0672
         ICM   @15,15,LICE(RSAVE)                               05S0672
         BNZ   @RF00672                                         05S0672
*                               DO; /*                       @DL31HBS*/
*                                 LICE=ICEPTR;/* UPDATE LAST ICE
*                                      QUEUED PTR            @DL31HBS*/
         ST    ICEPTR,LICE(,RSAVE)                              05S0674
*                                 FICE=ICEPTR;/* UPDATE FIRST ICE
