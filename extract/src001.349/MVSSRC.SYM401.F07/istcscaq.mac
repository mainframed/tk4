SVTM     TITLE 'ISTCSCAQ - CONNECTION SERVICES ACQUIRE ROUTINE         *
                        '
         ISEQ  73,80
ISTCSCAQ CSECT ,                                                01S0002
@MAINENT DS    0H                                               01S0002
         USING *,@15                                            01S0002
         B     @PROLOG                                          01S0002
         DC    AL1(16)                                          01S0002
         DC    C'ISTCSCAQ  78.076'                              01S0002
         DROP  @15
@PROLOG  BALR  @12,0                                            01S0002
@PSTART  DS    0H                                               01S0002
         USING @PSTART,@12                                      01S0002
         PRINT OFF
*   ;                                                           01S0352
*   DCL                                                         01S0353
*     ICEPTR PTR(31) REG(10);       /* PTR TO ICE                    */
*   ;                                                           01S0354
*   DCL                                                         01S0355
*     CSPPTR PTR(31) REG(11) RSTD;  /* PTR TO CSP                    */
*   DCL                                                         01S0356
*     ATCVTPTR PTR(31) REG(6) RSTD; /* PTR TO ATCVT                  */
*   DCL                                                         01S0357
*     RSAVE PTR(31) REG(13) RSTD;   /* SAVE AREA PTR                 */
*   DCL                                                         01S0358
*     RETR PTR(31) REG(14);         /* RETURN PT. REG.               */
*   DCL                                                         01S0359
*     ENTRYR PTR(31) REG(15);       /* ENTRY PT. REG.                */
*   DCL                                                         01S0360
*     RC FIXED(31) REG(15);         /* RET. CODE REG.                */
*                                                               01S0361
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FOLLOWING ARE ROUTINE ADDRESSES IN MASTER                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0361
*   DCL                                                         01S0361
*     MXTRN LABEL BASED(ENTRYR) VALUERANGE(*);                  01S0361
*   DCL                                                         01S0362
*     SERTNPTR PTR(31) BASED;       /* PTR TO SERVICE ROUTINES       */
*                                                               01S0362
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FOLLOWING ARE WORK VARIABLES BASED ON THE WORK AREA WITHIN*/
*   /* THE SECOND SAVE AREA                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0363
*   DCL                                                         01S0363
*     1 AQSAVEA BDY(WORD) BASED(RSAVE) POS(125),/* ACQUIRE SAVE AREA */
*      2 RETLOC PTR(31),            /* RET. PTR SAVE AREA            */
*      2 FIRSTICE PTR(31),          /* PTR TO PREVIOUS ICE GROUP
*                                      CHAIN SLOT                    */
*      2 ICXPTR PTR(31),            /* PTR TO ICEX                   */
*      2 CES PTR(31),               /* PTR TO CHAIN OF CES ALLOCATED
*                                      BY GETSTORE                   */
*      2 UECBS PTR(31),             /* PTR TO CHAIN OF UECBS DITTO   */
*      2 LICE PTR(31),              /* PTR TO LAST ICE QUEUED   01S0363
*                                                            @DL31HBS*/
*      2 FICE PTR(31);              /* PTR TO FIRST ICE QUEUED  01S0363
*                                                            @DL31HBS*/
*                                                               01S0363
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FOLLOWING ARE GENERAL DECLARATIONS                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0364
*   DCL                                                         01S0364
*     CHAINFLD PTR(31) BASED;       /* PTR TO NEXT ELEMENT IN CHAIN  */
*   DCL                                                         01S0365
*     BITON BIT(01) CONSTANT('1'B); /* USED TO TEST & SET A BIT FOR
*                                      ON                    @D32CKMP*/
*   DCL                                                         01S0366
*     BITOF BIT(01) CONSTANT('0'B); /* USED TO TEST & SET A BIT FOR
*                                      OFF                   @D32CKMP*/
*                                                               01S0366
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FOLLOWING ARE CONSTANTS REQUIRED TO DEFINE THE FOLLOWING  */
*   /* STATES FOR FEEDBACK IN THE CSP TO THE CALLER: SUCCESSFUL EXIT,*/
*   /* STORAGE UNAVAILABLE, LOGON REJECTED AND RESOURCE UNAVAILABLE  */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0367
*   DCL                                                         01S0367
*     SUCCESS FIXED(15) CONSTANT(0),                            01S0367
*     NOCORE FIXED(15) CONSTANT(12),                            01S0367
*     LGNRJCTD FIXED(15) CONSTANT(20),                          01S0367
*     CANCLD FIXED(15) CONSTANT(24),/*                       @ZA17866*/
*     BUSY FIXED(15) CONSTANT(28);                              01S0367
*   GEN(PRINT ON);                  /*                       @D32CKMP*/
         PRINT ON
*   RFY                                                         01S0369
*     RAPPTR RSTD;                                              01S0369
*   RFY                                                         01S0370
*     RETR RSTD;                                                01S0370
*   RETLOC=RETR;                    /* SAVE CALLER'S RETURN POINT    */
         ST    RETR,RETLOC(,RSAVE)                              01S0371
*   RFY                                                         01S0372
*     RETR UNRSTD;                                              01S0372
*   DO;                             /* SETCRR                   01S0373
*                                      FNID(CNSAQIN)TYPE(ENTRY)      */
*     CRACRR->CNSTFLDB=''B;         /* CLEAR CURRENT TRACE FIELD     */
         L     @10,RPHCRR(,RPHPTR)                              01S0374
         L     @10,CRACRR(,@10)                                 01S0374
         XC    CNSTFLDB(3,@10),CNSTFLDB(@10)                    01S0374
*     CRACRR->CNSAQINA='1'B;        /* SET HISTORY FLAG              */
         OI    CNSAQINA(@10),B'00100000'                        01S0375
*     CRACRR->CNSAQINB='1'B;        /* SET CURRENT FLAG              */
         OI    CNSAQINB(@10),B'00100000'                        01S0376
*   END;                            /* SETCRR                        */
*   CSPPTR=RPHCSPA;                 /* INITIALIZE PTR TO CSP         */
         L     CSPPTR,RPHCSPA(,RPHPTR)                          01S0378
*   RAPPTR=CSPSRC;                  /* INITIALIZE PTR TO REQUESTOR   */
         L     RAPPTR,CSPSRC(,CSPPTR)                           01S0379
*   IF CSPLOGI='1'B&RAPABLGN='1'B   /* IS IT A LOGONI REQUEST FOR AN
*                                      APLN. THAT NEVER ACCEPTS 01S0380
*                                      LOGONS?                       */
*     THEN                                                      01S0380
         TM    CSPLOGI(CSPPTR),B'00100000'                      01S0380
         BNO   @RF00380                                         01S0380
         TM    RAPABLGN(RAPPTR),B'10000000'                     01S0380
         BNO   @RF00380                                         01S0380
*     CSPCC=LGNRJCTD;               /* LOGON REJECTED        @ZA19898*/
         MVC   CSPCC(2,CSPPTR),@CH00504                         01S0381
*   ELSE                                                        01S0382
*     IF RAPAQUI='1'B               /* QUIESCE IN PROG       @ZA19898*/
*         &CSPLOGI='1'B             /* AND SIMLOGON          @ZA29056*/
*       THEN                                                    01S0382
         B     @RC00380                                         01S0382
@RF00380 TM    RAPAQUI(RAPPTR),B'01000000'                      01S0382
         BNO   @RF00382                                         01S0382
         TM    CSPLOGI(CSPPTR),B'00100000'                      01S0382
         BNO   @RF00382                                         01S0382
*       CSPCC='1C'X;                /* WRONG CODE            @ZA29056*/
         LA    @10,28                                           01S0383
         STH   @10,CSPCC(,CSPPTR)                               01S0383
*     ELSE                                                      01S0384
*SETUPREQ:                                                      01S0384
*                                                               02S0384
*/********************************************************************/
*/* THE FUNCTION OF THIS LOGIC SEGMENT IS TO SET UP THE              */
*/* INTERNAL VARIABLES REQUIRED BY THE ACQUIRE/SIMLOGON              */
*/* REQUEST PROCESSOR (ISTCSCAQ) BEFORE PROCESSING THE               */
*/* CQLLERS REQUEST.                                                 */
*/********************************************************************/
*                                                               02S0384
*       DO;                         /* BGN OF SETUPREQ LOGIC SEGMENT
*                                                            @D32CKMP*/
         B     @RC00382                                         02S0384
@RF00382 DS    0H                                               02S0384
SETUPREQ DS    0H                                               02S0385
*         RFY                                                   02S0385
*           RSCLPTR RSTD;                                       02S0385
*         RSCLPTR=CSPRSCE;          /* POINT AT RESOURCE LIST        */
         L     RSCLPTR,CSPRSCE(,CSPPTR)                         02S0386
*         RFY                                                   02S0387
*           NORSCRQD RSTD;                                      02S0387
*         IF CSPRSC='1'B            /* IS IT A GROUP REQUEST?        */
*           THEN                                                02S0388
         TM    CSPRSC(CSPPTR),B'10000000'                       02S0388
         BNO   @RF00388                                         02S0388
*           DO;                                                 02S0389
*             IF CSPCOREQ='1'B      /* A COREQUESITE REQUEST?        */
*               THEN                                            02S0390
         TM    CSPCOREQ(CSPPTR),B'01000000'                     02S0390
         BNO   @RF00390                                         02S0390
*               NORSCRQD=CSPNO;     /* NO. REQUIRED IS NO. IN LIST   */
         LH    NORSCRQD,CSPNO(,CSPPTR)                          02S0391
*             ELSE                                              02S0392
*               NORSCRQD=1;         /* NO. REQUIRED IS 1             */
         B     @RC00390                                         02S0392
@RF00390 LA    NORSCRQD,1                                       02S0392
*           END;                                                02S0393
*         ELSE                                                  02S0394
*           DO;                     /* SINGLE TERMINAL REDUIRED      */
         B     @RC00388                                         02S0394
@RF00388 DS    0H                                               02S0395
*             NORSCRQD=1;           /* NO. REQUIRED IS 1             */
         LA    @10,1                                            02S0395
         LR    NORSCRQD,@10                                     02S0395
*             CSPNO=1;              /* CREATE A LIST OF LENGTH 1
*                                                            @D32CKMP*/
         STH   @10,CSPNO(,CSPPTR)                               02S0396
*           END;                                                02S0397
*INTRPREQ:                                                      02S0398
*                                                               03S0398
*/********************************************************************/
*/* THE FUNCTION OF THIS LOGIC SEGMENT IS TO SEE IF THE              */
*/* REQUIRED NUMBER OF RESOURCES (TERMINALS) ARE FREE                */
*/* TO SATISFY THE REQUEST, AND IF SO, TO TRY AND                    */
*/* SATISFY THE REQUEST.  IF THE REQUEST CANNOT BE                   */
*/* SATISFIED, BUT QUEUING HAS BEEN REQUESTED, TO QUEUE              */
*/* THE REQUEST ON THE DESTINATION (APPLICATION) AND                 */
*/* RESOURCE (TERMINAL) RDT ENTRIES.                                 */
*/********************************************************************/
*                                                               03S0398
*         DO;                       /* BGN OF INTRPREQ LOGIC SEGMENT
*                                                            @D32CKMP*/
@RC00388 DS    0H                                               03S0398
INTRPREQ DS    0H                                               03S0399
*           RFY                                                 03S0399
*             FREECT RSTD;                                      03S0399
*           FREECT=0;               /* INITIALISE FREE TRML COUNT    */
         SLR   FREECT,FREECT                                    03S0400
*           RFY                                                 03S0401
*             FFTI RSTD;                                        03S0401
*           RFY                                                 03S0402
*            (RPREPTR,                                          03S0402
*             RCPREPTR,                                         03S0402
*             RSCLINDX) RSTD;                                   03S0402
*           DO RSCLINDX=1 TO CSPNO WHILE(FREECT<NORSCRQD);      03S0403
         LA    RSCLINDX,1                                       03S0403
         B     @DE00403                                         03S0403
@DL00403 CR    FREECT,NORSCRQD                                  03S0403
         BNL   @DC00403                                         03S0403
*             RPREPTR=CSLRDTE;      /* POINT AT TRML RDTE WHOSE 03S0404
*                                      STATUS IS TO BE EXAMINED 03S0404
*                                                            @D32CKMP*/
         LR    @10,RSCLINDX                                     03S0404
         BCTR  @10,0                                            03S0404
         MH    @10,@CH00447                                     03S0404
         ALR   @10,RSCLPTR                                      03S0404
         L     RPREPTR,CSLRDTE(,@10)                            03S0404
*             IF RPRLIVE='1'B×      /* FOR A LIVE TERMINAL           */
*                 (RPRENTRY=RPRENTLU&/* DEVICE IS LU         @ZA19893*/
*                 RPRNSNEA->RCCRESET='1'B) THEN/* RESET IN PROG 03S0405
*                                                            @ZA19893*/
         TM    RPRLIVE(RPREPTR),B'00000001'                     03S0405
         BO    @RT00405                                         03S0405
         CLI   RPRENTRY(RPREPTR),X'81'                          03S0405
         BNE   @RF00405                                         03S0405
         L     @10,RPRNSNEA(,RPREPTR)                           03S0405
         TM    RCCRESET(@10),B'00000100'                        03S0405
         BNO   @RF00405                                         03S0405
@RT00405 DS    0H                                               03S0406
*               IF RPRTIED='0'B     /* IS THE TRML FREE?             */
*                 THEN                                          03S0406
         TM    RPRTIED(RPREPTR),B'00100000'                     03S0406
         BNZ   @RF00406                                         03S0406
*                 DO;                                           03S0407
*                   FREECT=FREECT+1;/* INCREMENT FREE COUNT          */
         AH    FREECT,@CH00039                                  03S0408
*                   IF CSPLOGI='0'B /* IF COREQ ACQUIRE      @DL31HBS*/
*                     THEN                                      03S0409
         TM    CSPLOGI(CSPPTR),B'00100000'                      03S0409
         BNZ   @RF00409                                         03S0409
*                     CSLRDTEF=BITON;/* INDICATE TRML RDTE IS BEING
*                                      ACQUIRED              @D32CKMP*/
         LR    @10,RSCLINDX                                     03S0410
         BCTR  @10,0                                            03S0410
         MH    @10,@CH00447                                     03S0410
         ALR   @10,RSCLPTR                                      03S0410
         OI    CSLRDTEF(@10),B'10000000'                        03S0410
*                   ELSE                                        03S0411
*                     ;                                         03S0411
@RF00409 DS    0H                                               03S0412
*                   IF FREECT=1     /* IS IT THE FIRST FREE TRML?    */
*                     THEN                                      03S0412
         CH    FREECT,@CH00039                                  03S0412
         BNE   @RF00412                                         03S0412
*                     FFTI=RSCLINDX;/* SAVE POSN. OF ENTRY           */
         LR    FFTI,RSCLINDX                                    03S0413
*                 END;                                          03S0414
@RF00412 DS    0H                                               03S0415
*           END;                                                03S0415
@RF00406 DS    0H                                               03S0415
@RF00405 AH    RSCLINDX,@CH00039                                03S0415
@DE00403 CH    RSCLINDX,CSPNO(,CSPPTR)                          03S0415
         BNH   @DL00403                                         03S0415
@DC00403 DS    0H                                               03S0416
*           IF FREECT=NORSCRQD×(CSPLOGI='0'B&CSPCOREQ='1'B&FREECT^=0)
*                                                               03S0416
*           /*********************************************************/
*           /*                                                       */
*           /* SHOULD EXECUTE REQUEST?                       @DL31HBS*/
*           /*                                                       */
*           /*********************************************************/
*                                                               03S0416
*             THEN                                              03S0416
         CR    FREECT,NORSCRQD                                  03S0416
         BE    @RT00416                                         03S0416
         TM    CSPLOGI(CSPPTR),B'00100000'                      03S0416
         BNZ   @RF00416                                         03S0416
         TM    CSPCOREQ(CSPPTR),B'01000000'                     03S0416
         BNO   @RF00416                                         03S0416
         LTR   FREECT,FREECT                                    03S0416
         BZ    @RF00416                                         03S0416
@RT00416 DS    0H                                               03S0417
*XREQ:                                                          03S0417
*                                                               04S0417
*/********************************************************************/
*/* THE FUNCTION OF THIS LOGIC SEGMENT IS TO TRY AND                 */
*/* OBTAIN THE REQUIRED NUMBER OF UECBS AND CES TO                   */
*/* QUEUE THE LOGON REQUEST ONTO THE DESTINATION AND                 */
*/* RESOURCE RDT ENTRIES, OR TO OBTAIN THE REQUIRED                  */
*/* NUMBER OF ACES TO MAKE THE REQUESTED CONNECTIONS                 */
*/* FOR A SIMLOGON/ACQUIRE REQUEST RESPECTIVELY.                     */
*/********************************************************************/
*                                                               04S0417
*             DO;                   /* BGN OF XREQ LOGIC SEGMENT
*                                                            @D32CKMP*/
XREQ     DS    0H                                               04S0418
*               RFY                                             04S0418
*                 LFTI RSTD;                                    04S0418
*               DO;                 /* SETCRR                   04S0419
*                                      FNID(CNSXREQ)TYPE(SENTRY)     */
*                 CRACRR->CNSXREQA='1'B;/* SET HISTORY FLAG          */
         L     @10,RPHCRR(,RPHPTR)                              04S0420
         L     @10,CRACRR(,@10)                                 04S0420
         OI    CNSXREQA(@10),B'00010000'                        04S0420
*                 CRACRR->CNSXREQB='1'B;/* SET CURRENT FLAG          */
         OI    CNSXREQB(@10),B'00010000'                        04S0421
*               END;                /* SETCRR                        */
*               LFTI=RSCLINDX-1;    /* SAVE POSN. OF LAST TRML  04S0423
*                                      EXAMINED                      */
         LR    LFTI,RSCLINDX                                    04S0423
         BCTR  LFTI,0                                           04S0423
*               RFY                                             04S0424
*                 CECT RSTD;                                    04S0424
*               CECT=FREECT;        /* NO. CE'S REQUIRED             */
         LR    CECT,FREECT                                      04S0425
*               RSCLINDX=FFTI;      /* CONSIDER FIRST FREE TRML ON
*                                      RSCE LIST                     */
         LR    RSCLINDX,FFTI                                    04S0426
*               IF CSPLOGI='1'B     /* IS IT A SIMLOGON REQUEST?     */
*                 THEN                                          04S0427
         TM    CSPLOGI(CSPPTR),B'00100000'                      04S0427
         BNO   @RF00427                                         04S0427
*XREQSIM:                                                       04S0428
*                                                               05S0428
*/********************************************************************/
*/* THE FUNCTION OF THIS LOGIC SEGMENT IS TO COMPLETE                */
*/* PROCESSING A SIMLOGON REQUEST WHEN EVERYTHING HAS                */
*/* BECOME AVAILABLE.                                                */
*/********************************************************************/
*                                                               05S0428
*                 DO;               /* BGN OF XREQSIM LOGIC SEGMENT
*                                                            @D32CKMP*/
XREQSIM  DS    0H                                               05S0429
*                   RFY                                         05S0429
*                     UECBCT RSTD;                              05S0429
*                   IF CSPSRC->RAPASLGI=BITOF/* IF LGNS BLCKD   05S0430
*                                                            @D32CKMP*/
*                     THEN                                      05S0430
         L     @10,CSPSRC(,CSPPTR)                              05S0430
         TM    RAPASLGI(@10),B'00100000'                        05S0430
         BNZ   @RF00430                                         05S0430
*                     UECBCT=0;     /* NO UECBS REQUIRED     @D32CKMP*/
         SLR   UECBCT,UECBCT                                    05S0431
*                   ELSE                                        05S0432
*                     UECBCT=FREECT;/* NEED UECBS TO SCHEDULE LOGON
*                                      EXITS                 @D32CKMP*/
         B     @RC00430                                         05S0432
@RF00430 LR    UECBCT,FREECT                                    05S0432
*                   RFY                                         05S0433
*                     RC RSTD;                                  05S0433
*                                                               05S0433
@RC00430 DS    0H                                               05S0434
*                   /*************************************************/
*                   /*                                               */
*                   /* GET STORAGE FOR CES AND UECBS                 */
*                   /*                                               */
*                   /*************************************************/
*                                                               05S0434
*GETSTOR1:                                                      05S0434
*                                                               06S0434
*/********************************************************************/
*/* THE FUNCTION OF THIS LOGIC SEGMENT IS TO CALL THE                */
*/* CONNECTION SERVICES MASTER ROUTER GET STORAGE                    */
*/* FUNCTION TO OBTAIN STORAGE FOR THE NUMBER OF CES                 */
*/* AND/OR UECBS REQUESTED BY THE ACQUIRE/SIMLOGON                   */
*/* REQUEST PROCESSOR (ISTCSCAQ) AND RETURN POINTERS                 */
*/* TO THE STORAGE OBTAINED.                                         */
*/********************************************************************/
*                                                               06S0434
*                   DO;             /* BGN OF GETSTORE LOGIC SEGMENT
*                                                            @D32CKMP*/
GETSTOR1 DS    0H                                               06S0435
*                     RFY                                       06S0435
*                       SERTNPTR BASED(ATCCSMA+4*(CSPIGSTR+4));/*
*                                                            @D32CKMP*/
*                     RFY                                       06S0436
*                       ENTRYR RSTD;                            06S0436
*                     ENTRYR=SERTNPTR;/* POINT AT GETSTORE SERVICE
*                                      FUNCTION IN MASTER ROUTER     */
         L     @10,ATCCSMA(,ATCVTPTR)                           06S0437
         L     ENTRYR,SERTNPTR+84(,@10)                         06S0437
*                     CALL MXTRN;   /* GET REQUIRED STORAGE          */
         BALR  @14,@15                                          06S0438
*                     RFY                                       06S0439
*                       ENTRYR UNRSTD;                          06S0439
*                     CES=CECT;     /* SAVE PTR TO CHAIN OF CES      */
         ST    CECT,CES(,RSAVE)                                 06S0440
*                     UECBS=UECBCT; /* SAVE PTR TO CHAIN OF UECBS    */
         ST    UECBCT,UECBS(,RSAVE)                             06S0441
*                   END;            /* END OF GETSTORE LOGIC SEGMENT
*                                                            @D32CKMP*/
*                   IF RC^=0        /* IS THERE INSUFFICIENT CORE
*                                      AVAILABLE?                    */
*                     THEN                                      05S0443
         LTR   RC,RC                                            05S0443
         BZ    @RF00443                                         05S0443
*                     CSPCC=NOCORE; /* SAY SO                        */
         MVC   CSPCC(2,CSPPTR),@CH00447                         05S0444
*                   ELSE                                        05S0445
*                     DO;                                       05S0445
         B     @RC00443                                         05S0445
@RF00443 DS    0H                                               05S0446
*                       RFY                                     05S0446
*                        (ICEPTR,                               05S0446
*                         UECBPTR) RSTD;                        05S0446
*                       RFY                                     05S0447
*                         RC UNRSTD;                            05S0447
*                       RAPPTR=CSPSRC;/* POINT AT APLN RDTE          */
         L     RAPPTR,CSPSRC(,CSPPTR)                           05S0448
*QLOG:                                                          05S0449
*                                                               06S0449
*/********************************************************************/
*/* THE FUNCTION OF THIS LOGIC SEGMENT IS TO QUEUE                   */
*/* EACH LOGON ICE ASSOCIATED WITH THE CONNECTION                    */
*/* SERVICES RESOURCE LIST TO THE RESPECTIVE                         */
*/* RESOURCE AND DESTINATION RDT ENTRY.                              */
*/********************************************************************/
*                                                               06S0449
*                       DO;         /* BGN OF QLOG LOGIC SEGMENT
*                                                            @D32CKMP*/
QLOG     DS    0H                                               06S0450
*                         DO RSCLINDX=FFTI TO LFTI;             06S0450
         LR    RSCLINDX,FFTI                                    06S0450
         B     @DE00450                                         06S0450
@DL00450 DS    0H                                               06S0451
*LOGON0L2:                                                      06S0451
*                                                               07S0451
*/********************************************************************/
*/* THE FUNCTION OF THIS LOGIC SEGMENT IS TO CALL THE                */
*/* CONNECTION SERVICES MASTER ROUTER LOGON0 FUNCTION                */
*/* TO COMPLETE PROCESSING OF THE LOGON ICE.                         */
*/********************************************************************/
*                                                               07S0451
*                           DO;     /* BGN OF QLOG1 LOGIC SEGMENT
*                                                            @D32CKMP*/
LOGON0L2 DS    0H                                               07S0452
*                             RPREPTR=CSLRDTE;/* POINT AT TRML RDTE
*                                      TO WHICH THE CONNECTION IS TO
*                                      BE MADE               @D32CKMP*/
         LR    @15,RSCLINDX                                     07S0452
         BCTR  @15,0                                            07S0452
         MH    @15,@CH00447                                     07S0452
         LR    @14,RSCLPTR                                      07S0452
         ALR   @14,@15                                          07S0452
         L     RPREPTR,CSLRDTE(,@14)                            07S0452
*                             ICEPTR=CES;/* TAKE ONE CE OFF CHAIN    */
         L     ICEPTR,CES(,RSAVE)                               07S0453
*                             RFY                               07S0454
*                               CHAINFLD BASED(ICEPTR);         07S0454
*                             CES=CHAINFLD;/* LEAVING REST OF CHAIN  */
         MVC   CES(4,RSAVE),CHAINFLD(ICEPTR)                    07S0455
*                             CHAINFLD=0;/* RESET SMS CHAIN FIELD    */
         SLR   @14,@14                                          07S0456
         ST    @14,CHAINFLD(,ICEPTR)                            07S0456
*                             ICEDATA=CSLDATA;/* POINT ICE AT LOGON
*                                      MESSAGE               @D32CKMP*/
         ALR   @15,RSCLPTR                                      07S0457
         MVC   ICEDATA(4,ICEPTR),CSLDATA(@15)                   07S0457
*                             IF UECBS^=0 THEN                  07S0458
         L     @15,UECBS(,RSAVE)                                07S0458
         CR    @15,@14                                          07S0458
         BE    @RF00458                                         07S0458
*                               DO; /* IF UECBS WERE OBTAINED        */
*                                 UECBPTR=UECBS;/* POINT AT FIRST
*                                      UECB ON CHAIN                 */
         LR    UECBPTR,@15                                      07S0460
*                                 RFY                           07S0461
*                                   CHAINFLD BASED(UECBPTR);    07S0461
