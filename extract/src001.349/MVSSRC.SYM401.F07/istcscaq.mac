*/********************************************************************/
*                                                               05S0605
*                     DO;           /* BGN OF QICE LOGIC SEGMENT
*                                                            @D32CKMP*/
QICE2    DS    0H                                               05S0606
*                       RFY                                     05S0606
*                         FREEOFF RSTD;                         05S0606
*                       LICE=0;     /* ZERO LAST ICE QUEUED PTR 05S0607
*                                                            @DL31HBS*/
         SLR   @15,@15                                          05S0607
         ST    @15,LICE(,RSAVE)                                 05S0607
*                       FICE=0;     /* ZERO FIRST ICE QUEUED PTR
*                                                            @DL31HBS*/
         ST    @15,FICE(,RSAVE)                                 05S0608
*                       DO RSCLINDX=1 TO CSPNO;                 05S0609
         LA    RSCLINDX,1                                       05S0609
         B     @DE00609                                         05S0609
@DL00609 DS    0H                                               05S0610
