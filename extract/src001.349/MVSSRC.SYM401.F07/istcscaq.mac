*                                      LINK FIELD OFFSET             */
         LA    @15,ICESUBJQ(,ICEPTR)                            05S0659
         SLR   @15,ICEPTR                                       05S0659
         ST    @15,RPHSAVE3(,RPHPTR)                            05S0659
*                             RFY                               05S0660
*                               SERTNPTR BASED(ATCCSMA+4*(CSPIQ+4));
*                             RFY                               05S0661
*                               ENTRYR RSTD;                    05S0661
