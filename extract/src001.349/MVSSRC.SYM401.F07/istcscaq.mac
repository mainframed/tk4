         LH    @14,ICXAVLCT(,@15)                               05S0635
         BCTR  @14,0                                            05S0635
         STH   @14,ICXAVLCT(,@15)                               05S0635
*                           END;                                05S0636
*                         ELSE                                  05S0637
*                           DO;                                 05S0637
         B     @RC00611                                         05S0637
@RF00611 DS    0H                                               05S0638
*                             ICEPTR=CES;/* POINT AT CE AT HEAD OF
*                                      CHAIN                         */
         L     ICEPTR,CES(,RSAVE)                               05S0638
*                             IF UECBS^=0 THEN                  05S0639
         SLR   @15,@15                                          05S0639
         L     @14,UECBS(,RSAVE)                                05S0639
         CR    @14,@15                                          05S0639
         BE    @RF00639                                         05S0639
*                               DO; /* IF UECBS WERE OBTAINED   05S0640
*                                                            @DL31HBS*/
*                                 UECBPTR=UECBS;/* POINT AT FIRST
*                                      UECB ON CHAIN@DL31HBS         */
         LR    UECBPTR,@14                                      05S0641
*                                 RFY                           05S0642
*                                   CHAINFLD BASED(UECBPTR);/*  05S0642
*                                                            @DL31HBS*/
*                                 UECBS=CHAINFLD;/* UNHOOK UECB FROM
*                                      CHAIN                 @DL31HBS*/
         MVC   UECBS(4,RSAVE),CHAINFLD(UECBPTR)                 05S0643
*                                 CHAINFLD=0;/* RESET SMS CHAIN FIELD
*                                                            @DL31HBS*/
         ST    @15,CHAINFLD(,UECBPTR)                           05S0644
*                                 ICEUECBX=UECBPTR;/* DITTO  @DL31HBS*/
         ST    UECBPTR,ICEUECBX(,ICEPTR)                        05S0645
*                               END;/*                       @DL31HBS*/
*                             RFY                               05S0647
*                               CHAINFLD BASED(ICEPTR);         05S0647
@RF00639 DS    0H                                               05S0648
*                             CES=CHAINFLD;/* DETACH CE FROM CHAIN   */
         MVC   CES(4,RSAVE),CHAINFLD(ICEPTR)                    05S0648
*                             CHAINFLD=0;/* RESET SMS CHAIN FIELD    */
         SLR   @15,@15                                          05S0649
         ST    @15,CHAINFLD(,ICEPTR)                            05S0649
*                             ICESUBJ=RAPPTR;/* POINT ICE TO APPLIC.
*                                      RDTE                          */
         ST    RAPPTR,ICESUBJ(,ICEPTR)                          05S0650
*                             ICEOBJ=RPREPTR;/* POINT ICE TO TERMINAL
*                                      RDTE                          */
         ST    RPREPTR,ICEOBJ(,ICEPTR)                          05S0651
*                             IF CSPSTAP='1'B/* IF CSP SHOULDER TAP
*                                      FLAG IS SET                   */
*                               THEN/* SHOULDER TAPPING IS DESIRED,
*                                      SO                            */
         TM    CSPSTAP(CSPPTR),B'10000000'                      05S0652
         BNO   @RF00652                                         05S0652
*                               DO;                             05S0653
*                                 ICESTAPR='1'B;/* MARK ICE FOR 05S0654
*                                      SHOULDER TAPPING              */
         OI    ICESTAPR(ICEPTR),B'00001000'                     05S0654
