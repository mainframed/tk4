         LR    @15,RSCLINDX                                     05S0685
         BCTR  @15,0                                            05S0685
         MH    @15,@CH00447                                     05S0685
         ALR   @15,RSCLPTR                                      05S0685
         MVC   ICEDATA(4,ICEPTR),CSLDATA(@15)                   05S0685
*                                 /***********************************/
*                                 /*                                 */
*                                 /* COPY DATA PTR FROM RESOURCE LIST*/
*                                 /* CURRENT ENTRY           @D32CKMP*/
*                                 /*                                 */
*                                 /***********************************/
*                                                               05S0686
*                               END;                            05S0686
*                             IF CSPRSC='1'B THEN/* IF GROUP REQUEST */
@RF00682 TM    CSPRSC(CSPPTR),B'10000000'                       05S0687
         BNO   @RF00687                                         05S0687
*                               DO;                             05S0688
*                                 ICEUECBX=ICXPTR;/* POINT ICE TO
*                                      ICEX OF GROUP                 */
         L     @15,ICXPTR(,RSAVE)                               05S0689
         ST    @15,ICEUECBX(,ICEPTR)                            05S0689
