*                                      WHICH AN ICE IS TO BE QUEUED
*                                                            @D32CKMP*/
         LR    @15,RSCLINDX                                     05S0610
         BCTR  @15,0                                            05S0610
         MH    @15,@CH00447                                     05S0610
         ALR   @15,RSCLPTR                                      05S0610
         L     RPREPTR,CSLRDTE(,@15)                            05S0610
*                         IF((RPRLIVE=BITON/* IF THE TRML RDTE IS
*                                      LIVE AND              @ZA19893*/
*                             ×(RPRENTRY=RPRENTLU&/* DEVICE IS LU
*                                                            @ZA19893*/
*                             RPRNSNEA->RCCRESET=BITON))/* RESET IN
*                                      PROGRESS              @ZA19893*/
*                             &RPRTIED=BITOF)/* NOT TIED TO SOMEONE
*                                      ALREADY               @DL31HBS*/
*                           THEN                                05S0611
         TM    RPRLIVE(RPREPTR),B'00000001'                     05S0611
         BO    @GL00009                                         05S0611
         CLI   RPRENTRY(RPREPTR),X'81'                          05S0611
         BNE   @RF00611                                         05S0611
         L     @15,RPRNSNEA(,RPREPTR)                           05S0611
         TM    RCCRESET(@15),B'00000100'                        05S0611
         BNO   @RF00611                                         05S0611
@GL00009 TM    RPRTIED(RPREPTR),B'00100000'                     05S0611
         BNZ   @RF00611                                         05S0611
*                           DO;     /*                       @DL31HBS*/
*LOGON0L1:                                                      05S0613
*                                                               06S0613
*/********************************************************************/
*/* THE FUNCTION OF THIS LOGIC SEGMENT IS TO CALL THE                */
*/* CONNECTION SERVICES MASTER ROUTER LOGON0 FUNCTION                */
