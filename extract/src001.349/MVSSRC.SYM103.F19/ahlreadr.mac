WS06     TITLE 'AHLREADR, DATA INTERFACE TO THE TRACE WRITER           *
                        '
*                                                                  0028
*   /*****************************************************************/
*   /*                                                               */
*   /* STRUCTURE DECLARES                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0028
*   ;                                                              0028
*                                                                  0028
*/*                                                                  */
*/*  FRR PARM SAVE AREA - ENTRY POINTS AHLREAD1 AND AHLRDFR ONLY     */
*/*                                                                  */
*                                                                  0029
*   DECLARE                                                        0029
*     1 FRRPARM BASED(R9PARM),      /* FRR PARM SAVE AREA            */
*       3 ECBSAVE PTR(31),          /* SAVE AREA FOR LOCAL ECB TO BE
*                                      POSTED                        */
*       3 SAVESRB PTR(31),          /* SRB@ FOR AHLREADR. IF 0 THE 0029
*                                      AREA HAS BEEN FREED OR SHOULD
*                                      NOT BE FREED ( THE PLIST HAS
*                                      BEEN QUEUED IN THE GTFPCT)    */
*       3 MCCESAVE PTR(31),         /* MCCE @                        */
*       3 GMANSAVE PTR(31),         /* SAVE AREA FOR @ OF 800 BYTE 0029
*                                      GETMAINED AREA. IF 0 THE AREA
*                                      HAS BEEN FREED                */
*       3 FRRTRAIL PTR(32),         /* TRAIL USED BY FRR ROUTINE   0029
*                                      (AHLRDFR) TO DETERMINE WHAT 0029
*                                      SHOULD BE CLEANED UP. THE   0029
*                                      TRAIL BITS ARE SET BY THE MAIN
*                                      ENTRY POINT (AHLREAD1). SEE 0029
*                                      ALSO SAVESRB AND GMANSAVE FOR
*                                      TRAIL INFORMATION USED BY THE
*                                      FRR ROUTINE                   */
*         5 MCENQD BIT(1),          /* THE MC CONTROL BLOCKS ARE   0029
*                                      ENQ'D ON                      */
*         5 LOCHELD BIT(1),         /* THE LOCAL LOCK IS HELD        */
*         5 SLOCHELD BIT(1),        /* THE SALLOC LOCK IS HELD       */
*         5 POSTDONE BIT(1),        /* LOCAL ECB HAS BEEN POSTED OR
*                                      DOES NOT NEED TO BE POSTED    */
*       3 RTNSAVE PTR(31);          /* R14 SAVE AREA               0029
*                                                                    */
*                                                                  0030
*/*                                                                  */
*/*  INPUT PARAMETER LIST - ENTRY POINTS AHLREAD1, AHLREAD2, AHLRDCU2,
*    AHLRDCU3                                                        */
*/*                                                                  */
*                                                                  0030
*   DECLARE                                                        0030
*     1 AHLRDPL BASED(PLISTREG),    /* INPUT PARAMETER LIST          */
*       3 AHLCHAIN PTR(31),         /* THIS IS THE AHLRDPLQ QUEUE  0030
*                                      CHAIN FIELD AND WILL POINT TO
*                                      THE NEXT AHLRDPL OR BE 0 -  0030
*                                      AHLREAD1 ONLY                 */
*       3 AHLRDID CHAR(8),          /* THIS ID IS MATCHED WITH THE 0030
*                                      MCQENAME FIELD TO DETERMINE 0030
*                                      WHICH APPLICATION THE DATA IS
*                                      TO COME FROM - AHLREAD1 ONLY  */
*       3 * PTR(16),                                               0030
*       3 AHLRDDID CHAR(6) BDY(WORD,3),/* DUMP ID                    */
*        4 AHLRDDASI PTR(16),       /* ASID OF ADDR SPACE BEING    0030
*                                      DUMPED                        */
*        4 AHLRDTCB PTR(31),        /* TCB ADDR FOR DUMP OR 0        */
*       3 AHLRDECB PTR(31),         /* ECB@ TO BE POSTED IN ERROR  0030
*                                      RECOVERY OR IF GTF NOT FOUND
*                                      (AHLREAD1) OR WHEN THE GTF  0030
*                                      WRITER COMPLETES THE DUMP   0030
*                                      REQUEST (AHLREAD2)            */
*       3 AHLRDATA PTR(31),         /* ADDR OF DATA AREA             */
*       3 AHLRDASC PTR(31),         /* ASCB ADDR WHERE ECB IS LOCATED*/
*       3 AHLPCOD FIXED(31),        /* COMPLETION CODE TO BE USED IN
*                                      POSTING THE LOCAL ECB -     0030
*                                      AHLREAD2 AND AHLRDCU3 ONLY    */
*       3 AHLRDSRB PTR(31);         /* ADDR OF STORAGE OBTAINED FOR
*                                      AHLREADR'S SRBS. THE SAME SRB
*                                      IS USED BOTH FOR AHLREAD1 AND
*                                      AHLREAD2. THE SRB IS        0030
*                                      IMMEDIATELY FOLLOWED BY THIS
*                                      PARMLIST WHICH MUST BE FREED
*                                      WHEN THE SRB IS FREED         */
*/* STORAGE LIST FOR SDUMP                                           */
*                                                                  0031
*   DECLARE                                                        0031
*     1 READRMOD STATIC LOCAL,                                     0031
*       3 * PTR(31) INIT(ADDR(AHLREADR)),/* ADDR OF START OF MODULE  */
*       3 * PTR(32),                                               0031
*         5 * BIT(8) INIT('80'X),   /* END OF LIST INDICATOR         */
*         5 * PTR(24) INIT(ADDR(AHLREADN));/* ADDR OF END OF MODULE
*                                                                    */
*                                                                  0032
*   /*****************************************************************/
*   /*                                                               */
*   /* CONSTANT DECLARES                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0032
*   DECLARE                                                        0032
*     ON BIT(1) CONSTANT('1'B),     /* USED TO TURN BITS ON          */
*     OFF BIT(1) CONSTANT('0'B),    /* USED TO TURN BITS OFF         */
*     ECBSIGN BIT(32) CONSTANT('80000000'X),/* USED TO TURN ON HIGH
*                                      ORDER BIT IN REGISTER       0032
*                                      CONTAINING ECB@ FOR     XMPOST*/
*     ERRCODE FIXED(8) CONSTANT(4), /* ERROR RETURN CODE             */
*     XPOSTCD FIXED(8) CONSTANT(0); /* RETURN CODE FOR CROSS-MEMORY
*                                      POST                          */
*   GEN;                                                           0033
AHLRDSP EQU   245       SUBPOOL FOR GETMAINS AND FREEMAINS
*                                                                  0034
*   /*****************************************************************/
*   /*                                                               */
*   /* MISCELLANEOUS DECLARES                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0034
*   DECLARE                                                        0034
*     READBASE PTR(31) INIT(ADDR(AHLREADR)) STATIC LOCAL;/* BASE ADDR*/
*   DECLARE                                                        0035
*     CVTPTR PTR(31) DEFINED(FLCCVT);/* BASE FOR CVT                 */
*   DECLARE                                                        0036
*     FRRSPTR PTR(31) BASED,        /* INCLUDED TO ELIMINATE ERRORS  */
*     FRREPTR PTR(31) BASED,        /* DURING COMPILATION            */
*     AWSAPTR PTR(31) BASED,        /* DITTO                         */
*     ASCBPTR PTR(31) BASED;        /* DITTO                         */
*   DECLARE                                                        0037
*     ACRADDR LABEL BASED(SDWANXT1),/* MAPS ACR RETRY ADDRESS        */
*     SRBADDR LABEL BASED(RTNSAVE); /* MAPS ALL OTHER RETRY ADDRESSES*/
*   DECLARE                                                        0038
*     RECID CHAR(24) INIT('AHLREADRAHLREADRAHLRDFR ') STATIC LOCAL;
*   DECLARE                                                        0039
*     CLRPARM BIT(192) BASED(R1) BDY(WORD);/* USED TO CLEAR SVCDUMP'S
*                                      PARMLIST                      */
*   DECLARE                                                        0040
*     I256C CHAR(256) BASED;                                       0040
*   DECLARE                                                        0041
*     AHLREAD2 LABEL LOCAL EXTERNAL,/* DECLARE LABEL FOR ENTRY       */
*     AHLRDCU3 LABEL LOCAL EXTERNAL,/* DECLARE LABEL FOR ENTRY       */
*     AHLRDXP1 LABEL LOCAL EXTERNAL,/* DECLARE LABEL FOR ENTRY       */
*     AHLRDFR LABEL LOCAL EXTERNAL; /* DECLARE LABEL FOR ENTRY     0041
*                                                                    */
*   RFY                                                            0042
*     MCCE BASED(R0);                                              0042
*   RFY                                                            0043
*     MCQE BASED(MCQEPNT);                                         0043
*   GEN REFS(AHLREAD1,AHLRDCU2) NOEXIT NOFLOWS SEQFLOW;            0044
AHLREADR CSECT
         DC    A(AHLREAD1)         ADCON FOR MAIN ENTRY POINT
         DC    A(AHLRDCU2)         ADCON FOR ITS RMTR ENTRY POINT
*AHLREAD1:                                                         0045
*   GEN SETS(BASEREG) REFS(R15ENTRY,READBASE) NOEXIT NOFLOWS SEQFLOW
*       NODEFS;                                                    0045
*                                                                  0045
AHLREAD1 DS    0H                                                  0045
         USING AHLREAD1,R15ENTRY   SETUP TEMP BASE
         L     BASEREG,READBASE    GET BASE ADDRESS UPON ENTRY
         USING AHLREADR,BASEREG    TELL ASSEMBLER ABOUT OUR BASE
         DROP  R15ENTRY            TELL ASSEMBLER TO QUIT USING R15
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ENTRY POINT PERFORMS MAIN LINE FUNCTIONS - QUEUING THE   */
*   /* PLIST ONTO THE CHAIN IN THE GTFPCT AND NOTIFYING THE GTF      */
*   /* WRITER VIA A CROSS-MEMORY POST TO DUMP THE GTF BUFFERS        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0046
*   DO;                                                            0046
*                                                                  0046
*     /***************************************************************/
*     /*                                                             */
*     /* HOUSEKEEPING - DEFINE THE ERROR ENVIRONMENT VIA SETFRR AND  */
*     /* SAVE REG14, REQUESTOR'S ECB@ AND AHLREAD1'S SRB@ IN THE FRR */
*     /* PARM AREA. THE ECB MUST BE POSTED DURING ERROR RECOVERY     */
*     /* PROCESSING OR IF GTF IS NOT RUNNING. THE SRB MUST BE        */
*     /* FREEMAINED BOTH DURING NORMAL PROCESSING AND ERROR RECOVERY */
*     /* IF THE PARMLIST IS NOT QUEUED                               */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0047
*     RFY                                                          0047
*      (R14,                                                       0047
*       FRRADDR,                                                   0047
*       R12WORK,                                                   0047
*       PLISTREG,                                                  0047
*       R1) RSTD;                                                  0047
*     PLISTREG=R1;                  /* SAVE PARMLIST ADDRESS         */
         LR    PLISTREG,R1                                         0048
*     RFY                                                          0049
*       R1 UNRSTD;                                                 0049
*     FRRADDR=ADDR(AHLRDFR);        /* GET @ OF THE RECOVERY ROUTINE */
         LA    FRRADDR,AHLRDFR                                     0050
*     GEN SETS(R9PARM,R12WORK) REFS(FRRADDR,PSACSTK,FRRSCURR,FRRSLAST,
*         FRRSENTR,FRRS,PSA,PSALSFCC,FRRSELEN,FRRSFRRA,FRRSPARM) NOEXIT
*         NOFLOWS SEQFLOW;                                         0051
         SETFRR A,FRRAD=(FRRADDR),PARMAD=(R9PARM),                     *
               WRKREGS=(R9PARM,R12WORK)  ADD FRR ROUTINE TO STACK
*     RTNSAVE=R14;                  /* SAVE SCHEDULER RETURN @       */
         ST    R14,RTNSAVE(,R9PARM)                                0052
*     RFY                                                          0053
*      (R14,                                                       0053
*       FRRADDR,                                                   0053
*       R12WORK) UNRSTD;                                           0053
*     ECBSAVE=AHLRDECB;             /* SAVE ECB @                    */
         MVC   ECBSAVE(4,R9PARM),AHLRDECB(PLISTREG)                0054
*     SAVESRB=AHLRDSRB;             /* SAVE SRB @                  0055
*                                                                    */
         MVC   SAVESRB(4,R9PARM),AHLRDSRB(PLISTREG)                0055
*                                                                  0056
*     /***************************************************************/
*     /*                                                             */
*     /* ENQ ON THE MC CONTROL BLOCKS TO PREVENT THEIR CHANGING WHILE*/
*     /* I AM SCANNING AND USING THEM. THE @ OF THE MCCE WILL BE     */
*     /* RETURNED IN R0. THE MCCE @ MUST BE SAVED FOR USE IN DEQUING */
*     /* THE CONTROL BLOCKS LATER                                    */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0056
*     DO;                                                          0056
*                                                                  0056
*       /*************************************************************/
*       /*                                                           */
*       /* THE ENQING MACRO EXPECTS REG3 TO CONTAIN THE @ OF THE     */
*       /* MCAWSA. A BRANCH ENTRY GETMAIN IS REQUIRED TO FULFILL THIS*/
*       /* REQUIREMENT                                               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0057
*       DO;                                                        0057
*                                                                  0057
*         /***********************************************************/
*         /*                                                         */
*         /* THE SALLOC LOCK MUST BE HELD BEFORE PERFORMING A BRANCH */
*         /* ENTRY GETMAIN FOR SUBPOOL AHLRDSP                       */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0058
*         GEN REFS(PSALITA,FLC) EXIT NOFLOWS SEQFLOW;              0058
GETSAL3  SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                       *
               RELATED=(GLOBAL,AHLREADR(FREESAL3,FREESAL4))
*         SLOCHELD=ON;              /* INDICATE SALLOC LOCK HELD   0059
*                                                                    */
         OI    SLOCHELD(R9PARM),B'00100000'                        0059
*                                                                  0060
*         /***********************************************************/
*         /*                                                         */
*         /* GET THE DUMMY MCAWSA                                    */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0060
*         RFY                                                      0060
*          (R0,                                                    0060
*           R1,                                                    0060
*           R15,                                                   0060
*           CODERG) RSTD;                                          0060
*         R0=LENGTH(MCAWSA);        /* LENGTH OF AREA TO BE ACQUIRED */
         LA    R0,800                                              0061
*         GEN SETS(R14,R15,R1,R3,R4) REFS(R0,CVTSPSA,WSAGGMFM,WSAG,
*             CVTGLMN,CVTMAP) EXIT SEQFLOW NOFLOWS;                0062
         GETMAIN RC,LV=(0),SP=AHLRDSP,BRANCH=(YES,GLOBAL)
*         CODERG=R15;               /* SAVE RETURN CODE              */
         LR    CODERG,R15                                          0063
*         RFY                                                      0064
*           R3 RSTD;                                               0064
*         R3=R1;                    /* LOAD ADDRESS OF 'MCAWSA' INTO
*                                      R3 FOR SETEVENT SERVICE (ENQ) */
         LR    R3,R1                                               0065
*         RFY                                                      0066
*          (R1,                                                    0066
*           R15) UNRSTD;                                           0066
*                                                                  0066
*         /***********************************************************/
*         /*                                                         */
*         /* RELEASE SALLOC LOCK                                     */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0067
*         GEN REFS(PSALITA,FLC) EXIT NOFLOWS SEQFLOW;              0067
FREESAL3 SETLOCK RELEASE,TYPE=SALLOC,                                  *
               RELATED=(GLOBAL,AHLREADR(GETSAL3))
*         SLOCHELD=OFF;             /* INDICATE SALLOC LOCK NOT HELD */
         NI    SLOCHELD(R9PARM),B'11011111'                        0068
*       END;                                                       0069
*                                                                  0069
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK FOR SUCCESSFUL GETMAIN                              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0070
*       IF CODERG^=0 THEN           /* CHECK FOR SUCCESSFUL GETMAIN  */
         SLR   @12,@12                                             0070
         CR    CODERG,@12                                          0070
         BNE   @RT00070                                            0070
*         GO TO CLEANUP;            /* UNSUCCESSFUL - POST REQUESTOR */
*       RFY                                                        0072
*         CODERG UNRSTD;                                           0072
*       GMANSAVE=R3;                /* SAVE ADDRESS OF GETMAINED AREA
*                                                                    */
         ST    R3,GMANSAVE(,R9PARM)                                0073
*       MCENQD=ON;                  /* INDICATE THAT DEQ OF MC     0074
*                                      CONTROL BLOCKS WILL BE      0074
*                                      REQUIRED                      */
*                                                                  0074
         OI    MCENQD(R9PARM),B'10000000'                          0074
*       /*************************************************************/
*       /*                                                           */
*       /* ENQ ON THE MC CONTROL BLOCKS                              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0075
*       DO;                         /* SETEVENT ACTION(ENQ)          */
*                                                                  0075
*         /***********************************************************/
*         /*                                                         */
*         /* THE FOLLOWING SECTION OF CODE IS GENERATED BY THE       */
*         /* SETEVENT MACRO. THIS CODE WILL BE EITHER AN INITIALIZED */
*         /* PARAMETER LIST, OR IF IT IS THE RESULT OF STANDARD OR   */
*         /* EXECUTE FORM IT WILL FILL in the list, GENERATE ALL     */
*         /* LINKAGES AND CALL THE PROPER sERVICE, EITHER AHLSETEV OR*/
*         /* AHLSETD                                                 */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0076
*         DCL                                                      0076
*           AHLPPTR REG(1) PTR(31) RSTD;/* PARAMETER LIST ADDRESS    */
*         DCL                                                      0077
*           AHLSETEV ENTRY OPTIONS(EXIT,SEQFLOW) BASED(MCHSETE) VALRG(0
*               );                  /* ENTRY FOR ADD ACTIVAT,FREE    */
*         DCL                                                      0078
*           AHLSETD ENTRY OPTIONS(EXIT,SEQFLOW) BASED(MCHSETD) VALRG(0)
*             ;                     /* ENTRY FOR DISABLE AND CHANGE  */
*         DCL                                                      0079
*           1 SEPL BASED(AHLPPTR) BDY(WORD),/* DSECT PARAMETER LIST  */
*            2 SEFG PTR(8),         /* ACTION GLAGS                  */
*            2 * PTR(8),                                           0079
*            2 SEEN PTR(8),         /* EID COUNT                     */
*            2 SECN PTR(8),         /* CLASS COUNT                   */
*            2 SENM CHAR(8),        /* APPLICATION NMAE              */
*            2 SEEL PTR(31),        /* ADDRESS OF LIST OF EIDS       */
*            2 SECL PTR(31),        /* ADDRESS OF LIST OF CLASSES    */
*            2 SEMC PTR(31);        /* MCQE ADDRESS                  */
*         AHLPPTR=0;                /* SET TO INDCIATE ENQ           */
         LR    AHLPPTR,@12                                         0080
*         CALL AHLSETD;                                            0081
         L     @12,CVTPTR                                          0081
         L     @12,CVTGTF(,@12)                                    0081
         L     @15,MCHSETD(,@12)                                   0081
         BALR  @14,@15                                             0081
*       END;                        /* LOCK UP THE MC CONTROL BLOCKS */
*       ;                                                          0083
*       MCCESAVE=R0;                /* SAVE THE MCCE @ IN THE FRR  0084
*                                      PARM AREA                     */
         ST    R0,MCCESAVE(,R9PARM)                                0084
*       RFY                                                        0085
*         R3 UNRSTD;                                               0085
*     END;                                                         0086
*                                                                  0087
*     /***************************************************************/
*     /*                                                             */
*     /* SCAN THE MCQE CHAIN LOOKING FOR THE GTF MCQE. IF FOUND      */
*     /* NORMAL PROCESSING WILL CONTINUE, IF NOT CLEANUP FUNCTIONS   */
*     /* MUST BE PERFORMED                                           */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0087
*     IF R0=0 THEN                  /* WAS THE MCCE @ RETURNED ?     */
         LTR   R0,R0                                               0087
         BZ    @RT00087                                            0087
*       GO TO CLEANUP;              /* IF NOT THE CHAIN CANNOT BE  0088
*                                      SCANNED                       */
*     RFY                                                          0089
*       MCQEPNT RSTD;                                              0089
*     MCQEPNT=MCCEQEA;              /* GET @ OF START OF MCQE QUEUE
*                                      FROM THE MCCE                 */
         LR    @12,R0                                              0090
         L     MCQEPNT,MCCEQEA(,@12)                               0090
*     RFY                                                          0091
*       R0 UNRSTD;                                                 0091
*MCQESCAN:                                                         0092
*     DO;                                                          0092
MCQESCAN DS    0H                                                  0093
*       IF MCQEPNT=0 THEN           /* END OF QUEUE?                 */
         LTR   MCQEPNT,MCQEPNT                                     0093
         BZ    @RT00093                                            0093
*         GO TO CLEANUP;            /* YES - GTF IS NOT ON MCQE CHAIN*/
*/*                    OTHERWISE CONTINUE SCANNING QUEUE             */
*                                                                  0095
*       IF MCQENAME=AHLRDID THEN    /* IS IT THE GTF MCQE ?          */
         CLC   MCQENAME(8,MCQEPNT),AHLRDID(PLISTREG)               0095
         BE    @RT00095                                            0095
*         GO TO GOODMCQE;           /* YES - GET OUT OF SCAN LOOP    */
*       ELSE                                                       0097
*         DO;                                                      0097
*           MCQEPNT=MCQEMCQE;       /* STEP TO NEXT MCQE ON QUEUE    */
         L     MCQEPNT,MCQEMCQE(,MCQEPNT)                          0098
*           GO TO MCQESCAN;         /* CONTINUE LOOKING FOR END OF 0099
*                                      QUEUE OR GTF MCQE             */
         B     MCQESCAN                                            0099
*         END;                                                     0100
*     END;                                                         0101
*GOODMCQE:                                                         0102
*                                                                  0102
*     /***************************************************************/
*     /*                                                             */
*     /* IF THIS IS GTF'S ADDR SPACE BEING DUMPED, DO NOT ATTEMPT TO */
*     /* GET TRACE DATA FROM THE TRACE WRITER BECAUSE IT IS MARKED   */
*     /* NONDISPATCHABLE.                                    @YM03681*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0102
*     IF MCQEASCB=AHLRDASC THEN                                    0102
GOODMCQE CLC   MCQEASCB(4,MCQEPNT),AHLRDASC(PLISTREG)              0102
         BE    @RT00102                                            0102
*       GO TO CLEANUP;                                             0103
*                                                                  0103
*     /***************************************************************/
*     /*                                                             */
*     /* ENSURE THAT THE GTFPCT POINTER IS GOOD AND ADD THE INPUT    */
*     /* PLIST TO THE QUEUE IN THE PCT. THE GTF WRITER WILL LATER    */
*     /* DEQUE THE PLIST. ONCE ON THE QUEUE THE PLIST MAY NOT BE     */
*     /* REFERENCED AGAIN BY THIS PROGRAM. A COMPARE & SWAP IS USED  */
*     /* TO OBTAIN A STABLE COPY OF THE QUEUE BEFORE ADDING THE PLIST*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0104
*     RFY                                                          0104
*       PCTPTR RSTD;                                               0104
*     PCTPTR=MCQEAT;                /* GET @ OF GTFPCT               */
         L     PCTPTR,MCQEAT(,MCQEPNT)                             0105
*     RFY                                                          0106
*       MCQEPNT UNRSTD;                                            0106
*     IF PCTPTR=0 THEN              /* IS THERE A GTFPCT ?           */
         LTR   PCTPTR,PCTPTR                                       0107
         BZ    @RT00107                                            0107
*       GO TO CLEANUP;              /* NO - THE PLIST CANNOT BE    0108
*                                      QUEUED                        */
*     DO;                                                          0109
*                                                                  0109
*       /*************************************************************/
*       /*                                                           */
*       /* UPDATE THE SRB FOR AHLREAD1 FOR USE WITH AHLREAD2. THE GTF*/
*       /* WRITER WILL USE THIS SRB IN SCHEDULING REENTRY TO AHLREADR*/
*       /* TO POST THE REQUEST ORIGINATOR                            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0110
*       RFY                                                        0110
*         SRBPTR RSTD;                                             0110
*       SRBPTR=AHLRDSRB;            /* GET POINTER TO SRB            */
         L     SRBPTR,AHLRDSRB(,PLISTREG)                          0111
*       SRBEP=ADDR(AHLREAD2);       /* FILL IN ADDRESS OF SRB ROUTINE*/
         LA    @12,AHLREAD2                                        0112
         ST    @12,SRBEP(,SRBPTR)                                  0112
*       SRBRMTR=ADDR(AHLRDCU3);     /* AND THE ADDRESS OF ITS      0113
*                                      RESOURCE MANAGER TERMINATION
*                                      ROUTINE                       */
         LA    @12,AHLRDCU3                                        0113
         ST    @12,SRBRMTR(,SRBPTR)                                0113
*       RFY                                                        0114
*         SRBPTR UNRSTD;                                           0114
*                                                                  0115
*       /*************************************************************/
*       /*                                                           */
*       /* COMPLETE THE PARMLIST (AHLRDPLQ CHAINING) AND ADD IT TO   */
*       /* THE QUEUE                                                 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0115
*       RFY                                                        0115
*         R12WORK RSTD;                                            0115
*       R12WORK=AHLRDPLQ;           /* GET @ OF LAST ITEM PLACED ON
*                                      QUEUE                         */
         L     R12WORK,AHLRDPLQ(,PCTPTR)                           0116
*ADDPLIST:                                                         0117
*       AHLCHAIN=R12WORK;           /* PUSH IT DOWN IN THE QUEUE   0117
*                                      (LIFO)                        */
ADDPLIST ST    R12WORK,AHLCHAIN(,PLISTREG)                         0117
*       CS(R12WORK,PLISTREG,AHLRDPLQ);/* C&S TO PLACE THIS PLIST AT
*                                      TOP OF QUEUE IN PCT           */
         CS    R12WORK,PLISTREG,AHLRDPLQ(PCTPTR)                   0118
*       BC(4,ADDPLIST);             /* QUEUE WAS UPDATED SINCE WE  0119
*                                      LAST LOOKED AT IT - CONTINUE
*                                      ATTEMPT TO GET A STABLE COPY
*                                      OF THE PLIST. R12WORK HAS NEW
*                                      LAST ITEM PLACED ON THE QUEUE */
         BC    4,ADDPLIST                                          0119
*       RFY                                                        0120
*        (R12WORK,                                                 0120
*         PLISTREG) UNRSTD;                                        0120
*                                                                  0120
*/*                      THE COMPARE AND SWAP WAS SUCCESSFUL - THE   */
*/*                      PLIST HAS BEEN ADDED TO THE AHLRDPLQ IN     */
*/*                      THE GTFPCT                                  */
*                                                                  0121
*       SAVESRB=0;                  /* INDICATE THAT THE SRB SHOULD
*                                      NOT BE FREEMAINED DURING    0121
*                                      NORMAL CLEANUP OR FRR       0121
*                                      PROCESSING                    */
         SLR   @12,@12                                             0121
         ST    @12,SAVESRB(,R9PARM)                                0121
