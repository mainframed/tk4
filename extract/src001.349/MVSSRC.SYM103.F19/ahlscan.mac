         TITLE 'AHLSCAN-GTF INITIALIZATION SUBROUTINE TO SCAN START COM*
               MAND    '
AHLSCAN  CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'AHLSCAN   73.240'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
*                                                                  0083
*/********************************************************************/
*/* THIS MARKS THE BEGINING OF EXECUTABLE CODE IN AHLSCAN            */
*/********************************************************************/
*/********************************************************************/
*/*     INITIALIZE GTFPCT GTF STATUS FIELDS WITH GTF DEFAULT   Y02014*/
*/*                       PARAMETER VALUES                           */
*/********************************************************************/
*                                                                  0084
*   GTFMODE='1'B;                   /* SET GTF MODE EXT        Y02014*/
*   GTFDEBUG='0'B;                  /* SET DEBUG OFF           Y02014*/
         OI    GTFMODE(PCTPTR),B'10000000'                         0085
         NI    GTFDEBUG(PCTPTR),B'10111111'                        0085
*   OPTTIME='0'B;                   /* SET NO TIMESTAMP        Y02014*/
         NI    OPTTIME(PCTPTR),B'11111110'                         0086
*   HQLENGTH=10;                    /* SET DEFAULT BUF NUMBER  Y02014*/
         MVC   HQLENGTH(4,PCTPTR),@CF00294                         0087
*                                                                  0088
*/********************************************************************/
*/* CHECK CIBDATLN FIELD TO DETERMINE IF PARAMETERS WERE SPECIFIED   */
*/* WITH THE START GTF COMMAND. IF NONE, THEN CHECK EXECUTE STATMENT */
*/* PARAMETER FIELD FOR USER DEFAULT VALUES. THEN BUILD ECBLIST.     */
*/********************************************************************/
*                                                                  0088
*   IF CIBDATLN=ZEROS               /* IS CIB PARM FIELD BLANK       */
*     THEN                          /* NO PARAMETERS FOUND IN CIB    */
         L     @14,INITPTR(,PCTPTR)                                0088
         L     @14,CIBPTR(,@14)                                    0088
         LH    @03,CIBDATLN(,@14)                                  0088
         C     @03,ZEROS                                           0088
         BE    @RT00088                                            0088
*     GOTO PARMCHK;                 /* GO CHECK EXEC STMT            */
*   PARMCTR=CIBDATLN;               /* INITIALIZE PARMCTR WITH LENGTH
*                                      OF PARM FIELD                 */
         ST    @03,PARMCTR                                         0090
*   SCANPTR=ADDR(CIBDATA);          /* INITIALIZE SCANPTR WITH ADDR
*                                      OF CIB PARAMETER FIELD        */
         LA    SCANPTR,CIBDATA(,@14)                               0091
*   GOTO LSTBLD;                    /* BRANCH TO BUILD ECBLIST       */
         B     LSTBLD                                              0092
*                                                                  0093
*/********************************************************************/
*/*  SCAN EXECUTE STATEMENT PARAMETER FIELD FOR VALID USER DEFAULT   */
*/*             VALUES FOR GTF EXECUTION OPTIONS                     */
*/********************************************************************/
*                                                                  0093
*PARMCHK:                                                          0093
*   SCANPTR=DEFLPARM;               /* INITIALIZE SCANPTR WITH ADDR
*                                      OF PARM FIELD                 */
PARMCHK  L     @14,INITPTR(,PCTPTR)                                0093
         L     @14,EXECPTR(,@14)                                   0093
         L     SCANPTR,DEFLPARM(,@14)                              0093
*   IF PARMLNG^=ZEROS               /* ANY USER DEFULT GTF PARM    0094
*                                      VALUES                        */
*     THEN                          /* YES                           */
         LH    @14,PARMLNG(,SCANPTR)                               0094
         C     @14,ZEROS                                           0094
         BE    @RF00094                                            0094
*     DO;                           /* UPDATE POINTERS               */
*       PARMCTR=PARMLNG;            /* INITIALIZE PARMCTR WITH LENGTH
*                                      OF PARM FIELD                 */
         ST    @14,PARMCTR                                         0096
*       SCANPTR=SCANPTR+TWO;        /* INITIALIZE SCANPTR WITH     0097
*                                      ADDRESS OF EXECUTE STATMENT 0097
*                                      PARAMETER FIELD               */
         AH    SCANPTR,TWO                                         0097
*       GOTO LSTBLD;                /* BRANCH TO BUILD ECBLIST       */
         B     LSTBLD                                              0098
*     END;                                                         0099
*   ELSE                            /* NO DEFAULT PARMS FOUND        */
*     DO;                           /* INITIALIZE SEITCH             */
@RF00094 DS    0H                                                  0101
*       DEFULTSW='10'X;             /* SET DEFAULT SWITCH ON       0101
*                                      INDICATING NO SCAN TO BE    0101
*                                      PREFORMED                     */
         MVI   DEFULTSW,X'10'                                      0101
*     END;                                                         0102
*LSTBLD:                                                           0103
*   WTORECBL=ADDR(WTORECB);         /* ADDR OF WTOR ECB        Y02014*/
LSTBLD   L     @14,INITPTR(,PCTPTR)                                0103
         LA    @14,WTORECB(,@14)                                   0103
         ST    @14,WTORECBL                                        0103
*   ECBSTOP=STOPECBA;               /* PUT ADDR OF STOP ECB IN     0104
*                                      ECBLIST                 Y02014*/
         MVC   ECBSTOP(3),STOPECBA+1(PCTPTR)                       0104
*                                                                  0105
*/********************************************************************/
*/*    DETERMINE IF DEFAULT OPTIONS WERE SELECTED, IF SO CALL        */
*/*    AHLTCTL1 TO OBTAIN TRACE OPTIONS.  IF DEFAULT OPTIONS WERE    */
*/*                NOT SELECTED GO SCAN PARAMETER FIELD              */
*/********************************************************************/
*                                                                  0105
*   IF DEFULTSW^='10'X              /* IS DEFULTSW SET TO INDICATE 0105
*                                      DEFAULT OPTIONS               */
*     THEN                          /* NO                            */
         CLI   DEFULTSW,16                                         0105
         BNE   @RT00105                                            0105
*     GOTO STARTSCN;                /* BRANCH TO BEGIN SCAN OF     0106
*                                      PARAMETER FIELD               */
*   ELSE                            /* YES                           */
*     GO TO ENDRETRN;               /* CALL AHLTCTL1 TO OBTAIN TRACE
*                                      OPTIONS AND EXIT.       Y02014*/
*                                                                  0107
         B     ENDRETRN                                            0107
*/********************************************************************/
*/* FOLLOWING CODE IS THE SCANNING ROUTINE WHICH WILL DETERMINE WHICH*/
*/* OPTIONS HAVE BEEN REQUESTED BY THE USER.  THE GTFPCT  WILL       */
*/* BE INITIALIZED WITH THE OPTION SELECTIONS                        */
*/********************************************************************/
*                                                                  0108
*STARTSCN:                                                         0108
*   KEYWDPTR=ADDR(KYWRDTAB);        /* INITIALIZE KEYWDPTR WITH ADDR
*                                      OF KEYWORD TABLE              */
*                                                                  0108
STARTSCN LA    KEYWDPTR,KYWRDTAB                                   0108
*/********************************************************************/
*/* THIS CODE IS USED TO LOOP THROUGH THE KEYWORD TABLE AND COMPARE  */
*/* EACH KEYWORD ENTRY IN THE KEYWORD TABLE AGAINST KEYWORD BEING    */
*/* SCANNED IN THE PARAMETER FIELD. WHEN A MATCH IS FOUND A BRANCH IS*/
*/* TAKEN TO THE CORRESPONDING SUBROUTINE TO DETERMINE IF VALID.     */
*/********************************************************************/
*                                                                  0109
*KYWDCOMP:                                                         0109
*   DO I=ONE TO FOUR;               /* LOOP THROUGH KEYWORD TABLE    */
*                                                                  0109
KYWDCOMP LH    I,ONE                                               0109
         B     @DE00109                                            0109
@DL00109 DS    0H                                                  0110
*/********************************************************************/
*/* COMPARE KEYWORDS IN TABLE AND PARMFIELD FOR MATCH                */
*/********************************************************************/
*                                                                  0110
*     /***************************************************************/
*     /*                                                             */
*     /* DOES KEYWORD MATCH                                          */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0110
*     IF KYWORDS(ONE:KEYWDLN)=COMPAREA(ONE:KEYWDLN)THEN            0110
         LH    @14,ONE                                             0110
         LH    @07,KEYWDLN(,KEYWDPTR)                              0110
         SR    @07,@14                                             0110
         ST    @14,@TF00001                                        0110
         ALR   @14,KEYWDPTR                                        0110
         LR    @03,SCANPTR                                         0110
         AL    @03,@TF00001                                        0110
         AL    @03,@CF01201                                        0110
         EX    @07,@SC01202                                        0110
         BE    @RT00110                                            0110
*       GOTO OPSCAN;                /* BRANCH TO BEGIN OPTION VALUE
*                                      SCAN                          */
*     KEYWDPTR=KEYWDPTR+TWELVE;     /* INCREMENT PTR TO NEXT KEYWORD
*                                      ENTRY IN TABLE                */
         AH    KEYWDPTR,TWELVE                                     0112
*   END;                            /* BRANCH TO COMPARE NEXT KEY  0113
*                                      WORD IN TABLE                 */
         AH    I,@CH00112                                          0113
@DE00109 CH    I,FOUR                                              0113
         BNH   @DL00109                                            0113
*   MSGPTR=ADDR(AHL001);            /* INITIALIZE MSGPTR WITH ADDR OF
*                                      WTOR AHL001A            Y02014*/
         L     MSGPTR,@CV01076                                     0114
*   GOTO SCANERR;                   /* BRANCH TO PROCESS ERROR     0115
*                                      CONDITION                     */
         B     SCANERR                                             0115
*                                                                  0116
*/********************************************************************/
*/*  THIS CODE CHECKS FOR THE EQUAL SIGN DELIMITER IN THE PARM FIELD */
*/********************************************************************/
*                                                                  0116
*OPSCAN:                                                           0116
*   SCANPTR=SCANPTR+KEYWDLN;        /* INCREMENT SCANPTR BY LNG OF 0116
*                                      KEYWORD MATCHED               */
OPSCAN   LH    @14,KEYWDLN(,KEYWDPTR)                              0116
         AR    SCANPTR,@14                                         0116
*   PARMCTR=PARMCTR-KEYWDLN;        /* DECREMENT PARMCTR BY LNG OF 0117
*                                      KEYWORD                       */
         LCR   @14,@14                                             0117
         A     @14,PARMCTR                                         0117
         ST    @14,PARMCTR                                         0117
*   OPTABPTR=OPADDR;                /* ADDR OF OPTION TABLE          */
         SR    OPTABPTR,OPTABPTR                                   0118
         ICM   OPTABPTR,7,OPADDR(KEYWDPTR)                         0118
*COMPEQL:                                                          0119
*   IF DELIMEQL^=COMPAREA(ONE)      /* IS DELIMATOR EQUAL SIGN       */
*     THEN                          /* NO                            */
COMPEQL  LH    @14,ONE                                             0119
         ALR   @14,SCANPTR                                         0119
         AL    @14,@CF01201                                        0119
         CLC   DELIMEQL(1),COMPAREA(@14)                           0119
         BE    @RF00119                                            0119
*     DO;                           /* SET UP ERROR MESSAGE          */
*       MSGPTR=ADDR(AHL002);        /* INITIALIZE MSGPTR WITH ADDR OF
*                                      WTOR AHL002A            Y02014*/
*                                                                  0121
         L     MSGPTR,@CV01077                                     0121
*/********************************************************************/
*/* BRANCH TO SCANERR ROUTINE ISSUE WTOR AHL002A INDICATING INVALID  */
*/* DELIMATOR FOUND IN PARAMETER FIELD                               */
*/********************************************************************/
*                                                                  0122
*       GOTO SCANERR;               /* BRANCH TO ISSUE WTOR          */
         B     SCANERR                                             0122
*     END;                                                         0123
*                                                                  0124
*/********************************************************************/
*/* DETERMINE WHICH KEYWORD WAS MATCHED, CHECK TO SEE IF THIS KEYWORD*/
*/* HAS BEEN REPEATED. IF REPEATED ISSUE ERROR MESSAGE. IF KEYWORD   */
*/* WAS NOT REPEATED BRANCH TO PROCESS OPTIONS ASSOCIATED WITH       */
*/* KEYWORD                                                          */
*/********************************************************************/
*                                                                  0124
*   SCANPTR=SCANPTR+ONE;            /* INCREMENT SCANPTR BY LNG OF 0124
*                                      DELIMATOR                     */
@RF00119 LH    @14,ONE                                             0124
         AR    SCANPTR,@14                                         0124
*   PARMCTR=PARMCTR-ONE;            /* DECREMENT PARMCTR BY LNG OF 0125
*                                      DELIMETER                     */
         L     @07,PARMCTR                                         0125
         SR    @07,@14                                             0125
         ST    @07,PARMCTR                                         0125
*   IF I^=ONE                       /* WAS KEYWORD MODE MATCHED      */
*     THEN                          /* NO                            */
         CR    I,@14                                               0126
         BNE   @RT00126                                            0126
*     GOTO DEBUGCHK;                /* GO CHECK FOR DEBUG KEYWORD  0127
*                                      MATCH                         */
*   IF MODEBIT='0'B                 /* WAS KEYWORD REPEATED          */
*     THEN                          /* NO                            */
         TM    MODEBIT,B'10000000'                                 0128
         BZ    @RT00128                                            0128
*     GOTO MODEOPS;                 /* GO PROCESS MODE OPTIONS       */
*   ELSE                            /* YES                           */
*     GOTO REPERROR;                /* GOTO RTN TO INITIALIZE PTR  0130
*                                      BEFORE BRANCHING TO SCANERR TO
*                                      ISSUE WTOR                    */
         B     REPERROR                                            0130
*DEBUGCHK:                                                         0131
*   IF I^=TWO                       /* WAS KEYWORD DEBUG MATCHED     */
*     THEN                          /* NO                            */
DEBUGCHK CH    I,TWO                                               0131
         BNE   @RT00131                                            0131
*     GOTO TIMECHK;                 /* GO CHECK FOR TIME KEYOWRD   0132
*                                      MATCH                         */
*   IF DEBUGBIT='0'B                /* WAS KEYWORD REPEATED          */
*     THEN                          /* NO                            */
         TM    DEBUGBIT,B'01000000'                                0133
         BZ    @RT00133                                            0133
*     GOTO DEBUGOPS;                /* GO PROCESS DEBUG OPTION       */
*   ELSE                            /* YES                           */
*     GOTO REPERROR;                /* GOTO SUBRTN TO INITIALIZE PTRS
*                                      BEFORE BRANCHING TO SCANERR TO
*                                      ISSUE WTOR                    */
         B     REPERROR                                            0135
*TIMECHK:                                                          0136
*   IF I^=THREE                     /* WAS TIME KEY WORD MATCHED     */
*     THEN                          /* YES                           */
TIMECHK  CH    I,THREE                                             0136
         BNE   @RT00136                                            0136
*     GOTO BUFCHK;                  /* GO CHECK FOR BUF KEYWORD      */
*   IF TIMEBIT='0'B                 /* WAS KEYWORD REPEATED          */
*     THEN                          /* NO                            */
         TM    TIMEBIT,B'00100000'                                 0138
         BZ    @RT00138                                            0138
*     GOTO TIMEOPS;                 /* GO PROCESS TIME OPTIONS OF  0139
*                                      WTOR REPLY AREA               */
*   ELSE                            /* GO ISSUE ERROR MSG      Y02014*/
*     GOTO REPERROR;                /* FOR REPEATING KWD       Y02014*/
         B     REPERROR                                            0140
*BUFCHK:                                                           0141
*   IF BUFBIT='0'B                  /* WAS BUF KEYWORD REPEATED      */
*     THEN                          /* NO                            */
BUFCHK   TM    BUFBIT,B'00010000'                                  0141
         BZ    @RT00141                                            0141
*     GOTO BUFSCAN;                 /* GOTO TO SCAN BUF VALUE        */
*REPERROR:                                                         0143
*   MSGPTR=ADDR(AHL004);            /* INITIALIZE MSGPTR WITH ADDR OF
*                                      WTOR AHL004A                  */
REPERROR L     MSGPTR,@CV01079                                     0143
*   GOTO SCANERR;                   /* BRANCH TO PROCESS ERROR       */
         B     SCANERR                                             0144
*                                                                  0145
*/********************************************************************/
*/* THIS SECTION OF CODE SCANS THE PARAMETER FIELD FOR VALID MODE    */
*/* OPTIONS                                                          */
*/********************************************************************/
*                                                                  0145
*MODEOPS:                                                          0145
*                                                                  0145
*/********************************************************************/
*/* COMPARE FOR 'EXT' OPTION VALUE                                   */
*/********************************************************************/
*                                                                  0145
*   /*****************************************************************/
*   /*                                                               */
*   /* IS OPTION VALUE - EXT                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0145
*   IF OPTVALUE(ONE:OPLENGTH)=COMPAREA(ONE:OPLENGTH)THEN/* YES       */
MODEOPS  LH    @14,ONE                                             0145
         LH    @05,OPLENGTH(,OPTABPTR)                             0145
         SR    @05,@14                                             0145
         ST    @14,@TF00001                                        0145
         ALR   @14,OPTABPTR                                        0145
         LR    @01,SCANPTR                                         0145
         AL    @01,@TF00001                                        0145
         AL    @01,@CF01201                                        0145
         EX    @05,@SC01204                                        0145
         BE    @RT00145                                            0145
*     GOTO ENDMODE;                 /* GOTO INITIALIZE FIRTIMSW      */
*   OPTABPTR=OPTABPTR+TEN;          /* INCREMENT OPTABPTR TO NEXT  0147
*                                      ENTRY IN OPTION TABLE         */
*                                                                  0147
         AH    OPTABPTR,TEN                                        0147
*/********************************************************************/
*/* COMPARE FOR 'INT' OPTION VALUE                                   */
*/********************************************************************/
*                                                                  0148
*   /*****************************************************************/
*   /*                                                               */
*   /* IS OPTION VALUE - INT                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0148
*   IF OPTVALUE(ONE:OPLENGTH)=COMPAREA(ONE:OPLENGTH)THEN/* YES       */
         LH    @14,OPLENGTH(,OPTABPTR)                             0148
         S     @14,@TF00001                                        0148
         LR    @05,OPTABPTR                                        0148
         AL    @05,@TF00001                                        0148
         LR    @01,SCANPTR                                         0148
         AL    @01,@TF00001                                        0148
         AL    @01,@CF01201                                        0148
         EX    @14,@SC01206                                        0148
         BNE   @RF00148                                            0148
*     DO;                                                          0149
*       GTFMODE='0'B;               /* SET MODE INT IN PCT     Y02014*/
         NI    GTFMODE(PCTPTR),B'01111111'                         0150
*       GOTO ENDMODE;               /* GOTO INITIALIZE FIRTIMSW      */
         B     ENDMODE                                             0151
*     END;                                                         0152
*   ELSE                                                           0153
*     GO TO OPTERR;                 /* PROCESS ERROR           Y02014*/
*ENDMODE:                                                          0154
*   MODEBIT='1'B;                   /* SET SWITCH INDICATING MODE  0154
*                                      KEYWORD HAS BEEN PROCESSED    */
ENDMODE  OI    MODEBIT,B'10000000'                                 0154
*   GOTO OPSEND;                    /* GOTO INITIALIZE PTRS          */
         B     OPSEND                                              0155
*                                                                  0156
*/********************************************************************/
*/* THIS SECTION SCANS THE PARAMETER FIELD FOR VALID DEBUG OPTION(S) */
*/********************************************************************/
*                                                                  0156
*DEBUGOPS:                                                         0156
*                                                                  0156
*/********************************************************************/
*/* COMPARE FOR 'NO' OPTION VALUE                                    */
*/********************************************************************/
*                                                                  0156
*   /*****************************************************************/
*   /*                                                               */
*   /* IS OPTION VALUE - NO                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0156
*   IF OPTVALUE(ONE:OPLENGTH)=COMPAREA(ONE:OPLENGTH)THEN/* YES       */
DEBUGOPS LH    @14,ONE                                             0156
         LH    @05,OPLENGTH(,OPTABPTR)                             0156
         SR    @05,@14                                             0156
         ST    @14,@TF00001                                        0156
         ALR   @14,OPTABPTR                                        0156
         LR    @01,SCANPTR                                         0156
         AL    @01,@TF00001                                        0156
         AL    @01,@CF01201                                        0156
         EX    @05,@SC01204                                        0156
         BE    @RT00156                                            0156
*     GOTO ENDDEBUG;                /* GO INITIALIZE FIRTIMSW        */
*   OPTABPTR=OPTABPTR+TEN;          /* INCREMENT PTR TO NEXT ENTRY IN
*                                      THE TABLE                     */
*                                                                  0158
         AH    OPTABPTR,TEN                                        0158
*/********************************************************************/
*/* COMPARE FOR 'YES' OPTION VALUE                                   */
*/********************************************************************/
*                                                                  0159
*   /*****************************************************************/
*   /*                                                               */
*   /* IS OPTION VALUE - YES                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0159
*   IF OPTVALUE(ONE:OPLENGTH)=COMPAREA(ONE:OPLENGTH)THEN/* YES       */
         LH    @14,OPLENGTH(,OPTABPTR)                             0159
         S     @14,@TF00001                                        0159
         LR    @05,OPTABPTR                                        0159
         AL    @05,@TF00001                                        0159
         LR    @01,SCANPTR                                         0159
         AL    @01,@TF00001                                        0159
         AL    @01,@CF01201                                        0159
         EX    @14,@SC01206                                        0159
         BNE   @RF00159                                            0159
*     DO;                           /* INITIALIZE HCT                */
*       GTFDEBUG='1'B;              /* INITIAL GTFPCT TO INDICATE  0161
*                                      DEBUG OPTION = YES      Y02014*/
         OI    GTFDEBUG(PCTPTR),B'01000000'                        0161
*     END;                                                         0162
*   ELSE                            /* NO VALID DEBUG OPTIONS- ERROR */
*     GOTO OPTERR;                  /* GO TO ERROR RTN               */
*ENDDEBUG:                                                         0164
*   DEBUGBIT='1'B;                  /* SET SWITCH INDICATING DEBUG 0164
*                                      KEYWORD HAS BEEN PROCESSED    */
ENDDEBUG OI    DEBUGBIT,B'01000000'                                0164
*   GOTO OPSEND;                    /* GOTO INITIALIZE PTRS          */
         B     OPSEND                                              0165
*                                                                  0166
*/********************************************************************/
*/* THIS SECTION SCANS THE PARAMETER FIELD FOR VALID TIME OPTION(S)  */
*/********************************************************************/
*                                                                  0166
*TIMEOPS:                                                          0166
*                                                                  0166
*/********************************************************************/
*/* COMPARE FOR 'NO' OPTION VALUE                                    */
*/********************************************************************/
*                                                                  0166
*   /*****************************************************************/
*   /*                                                               */
*   /* IS OPTION VALUE - NO                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0166
*   IF OPTVALUE(ONE:OPLENGTH)=COMPAREA(ONE:OPLENGTH)THEN/* YES       */
TIMEOPS  LH    @14,ONE                                             0166
         LH    @05,OPLENGTH(,OPTABPTR)                             0166
         SR    @05,@14                                             0166
         ST    @14,@TF00001                                        0166
         ALR   @14,OPTABPTR                                        0166
         LR    @01,SCANPTR                                         0166
         AL    @01,@TF00001                                        0166
         AL    @01,@CF01201                                        0166
         EX    @05,@SC01204                                        0166
         BE    @RT00166                                            0166
*     GOTO ENDTIME;                 /* GO TO INITIALIZE FIRTIMSW     */
*   OPTABPTR=OPTABPTR+TEN;          /* INCREMENT PTR TO NEXT ENTRY IN
*                                      THE OPTION TABLE              */
*                                                                  0168
         AH    OPTABPTR,TEN                                        0168
*/********************************************************************/
*/* COMPARE FOR 'YES' OPTION VALUE                                   */
*/********************************************************************/
*                                                                  0169
*   /*****************************************************************/
*   /*                                                               */
*   /* IS OPTION VALUE - YES                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0169
*   IF OPTVALUE(ONE:OPLENGTH)^=COMPAREA(ONE:OPLENGTH)THEN/* NOT FOUND
*                                      - ERROR                       */
         LH    @14,OPLENGTH(,OPTABPTR)                             0169
         S     @14,@TF00001                                        0169
         LR    @05,OPTABPTR                                        0169
         AL    @05,@TF00001                                        0169
         LR    @01,SCANPTR                                         0169
         AL    @01,@TF00001                                        0169
         AL    @01,@CF01201                                        0169
         EX    @14,@SC01206                                        0169
         BNE   @RT00169                                            0169
*     GOTO OPTERR;                  /* GO PROCESS ERROR              */
*   ELSE                            /* MATCH FOUND                   */
*     DO;                           /* SET BITS IN HCT AND FIRSTMSW  */
*       OPTTIME='1'B;               /* INDICATE TIME =YES IN GTFPCT
*                                                              Y02014*/
         OI    OPTTIME(PCTPTR),B'00000001'                         0172
*ENDTIME:                                                          0173
*       TIMEBIT='1'B;               /* INDICATE FIRST TIME FOR TIME
*                                      KEYWORD                       */
ENDTIME  OI    TIMEBIT,B'00100000'                                 0173
*       GOTO OPSEND;                /* EXIT TO ADJUST PTRS           */
         B     OPSEND                                              0174
*     END;                                                         0175
*BUFSCAN:                                                          0176
*                                                                  0176
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN THE BUF= PARAMETER TO DETERMINE LENGTH OF VALUE AND IF   */
*   /* VALUE FALLS WITHIN REQUIRED RANGE                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0176
*   SCANSAVE=SCANPTR;               /* SAVE ADDR OF PARM FOLLOWING 0176
*                                      BUF=                          */
BUFSCAN  LR    SCANSAVE,SCANPTR                                    0176
*   BUFLEN=ONE;                     /* INITIALIAE CTR TO INDICATE  0177
*                                      WHICH SCAN                    */
         MVC   BUFLEN(1),ONE+1                                     0177
*BUMP:                                                             0178
*   SCANPTR=SCANPTR+ONE;            /* BUMP CTR TO NEXT CHARACTER    */
BUMP     LH    @14,ONE                                             0178
         AR    SCANPTR,@14                                         0178
*   PARMCTR=PARMCTR-ONE;            /* ADJUST FOR NUMBER OF PARMS  0179
*                                      LEFT                          */
*                                                                  0179
         L     @05,PARMCTR                                         0179
         SR    @05,@14                                             0179
         ST    @05,PARMCTR                                         0179
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE IF SCANNING A WTOR REPLY SO THAT PROPER DELIMITERS  */
*   /* ARE CHECKED INDICATING END OF THE PARM FIELD                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0180
*   IF WTORSCAN^='10'X              /* SCAN OF WTOR?                 */
*     THEN                          /* NO                            */
         CLI   WTORSCAN,16                                         0180
         BNE   @RT00180                                            0180
*     GOTO LENCHK;                  /* GOTO TEST FOR END OF PARMS    */
*   IF COMPAREA(ONE)=' '            /* END OF WTOR REPLY?            */
*     THEN                          /* YES                           */
         ALR   @14,SCANPTR                                         0182
