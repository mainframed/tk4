         LH    @05,USEFIELD(,AWSAPTR)                              0079
         BCTR  @05,0                                               0079
         STH   @05,USEFIELD(,AWSAPTR)                              0079
*       CDS(DWCOMP,DWSWAP,CURRENT->BLOKSWAP);/* ATTEMPT TO UPDATE THE
*                                      GTFBLOK                       */
         LM    @10,@11,DWCOMP(AWSAPTR)                             0080
         LM    @14,@15,DWSWAP(AWSAPTR)                             0080
         CDS   @10,@14,BLOKSWAP(CURRENT)                           0080
         BZ    @BC00080                                            0080
         STM   @10,@11,DWCOMP(AWSAPTR)                             0080
