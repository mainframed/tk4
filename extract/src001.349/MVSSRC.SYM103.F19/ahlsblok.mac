*       IF RCREG=0                  /* DID AHLSFEOB SUCCEED          */
*         THEN                                                     0091
         LTR   RCREG,RCREG                                         0091
         BZ    @RT00091                                            0091
*         GO TO RESTART;            /* YES, SO GO BACK AND TRY TO FIT
*                                      THE RECORD INTO THE NEW     0092
*                                      GTFBLOK                       */
*       RFY                                                        0093
*        (COMPREG,                                                 0093
*         SWAPREG) RSTD;                                           0093
*       COMPREG=GTFLOSTR;           /* GTFLOSTR MUST BE UPDATED VIA
*                                      CS LOGIC BECAUSE IT IS ALSO 0094
*                                      ALTERED BY AHLSBUF            */
         L     COMPREG,GTFLOSTR(,PCTPTR)                           0094
*INCRGLR:                                                          0095
*       SWAPREG=COMPREG+1;          /* PREPARE TO INCREMENT THE COUNT
*                                      BY 1 FOR THE RECORD WHICH   0095
*                                      CANNOT BE BLOCKED             */
