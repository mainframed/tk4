*       BC(4,INCRGLR);              /* IF AT FIRST YOU DON'T SUCCEED
*                                      TRY TRY AGAIN                 */
         BC    4,INCRGLR                                           0097
*       RFY                                                        0098
*        (COMPREG,                                                 0098
*         SWAPREG) UNRSTD;                                         0098
