*                                      BEFORE THE CDS WAS DONE       */
         LA    MVCLTPTR,BLOKAREA(,CURRENT)                         0072
         AH    MVCLTPTR,OLDLEN(,AWSAPTR)                           0072
*       MVCLTLEN=TRECLEN;                                          0073
         LH    MVCLTLEN,TRECLEN(,RECPTR)                           0073
*       MVCLFLEN=MVCLTLEN;          /* THE LENGTH OF BOTH FIELDS IS
*                                      THE LENGTH OF THE RECORD      */
         LR    MVCLFLEN,MVCLTLEN                                   0074
*       MVCLFPTR=RECPTR;            /* THE TRACE RECORD WILL BE    0075
