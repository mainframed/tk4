*      THEN DO;                        /* THEN CK SPEC CASE  @DM14066*/
         TM    CPWEOR(CUBCPWA),B'00100000'                      04S0352
         BO    @RT00352                                         04S0352
         CLI   NEXTCHAR(CUBIPT),X'40'                           04S0352
         BE    @RT00352                                         04S0352
         CLI   NEXTCHAR(CUBIPT),X'05'                           04S0352
         BNE   @RF00352                                         04S0352
@RT00352 DS    0H                                               04S0353
*        CPWEOR = CPWONE;              /* SET END OF RECORD  @DM14066*/
         OI    CPWEOR(CUBCPWA),B'00100000'                      04S0354
