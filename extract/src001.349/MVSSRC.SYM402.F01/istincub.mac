         OI    CPWEOR(CUBCPWA),B'00100000'                      04S0333
*    CPWRTC = CPWEOV;                  /* CODE = 'END-OF'VALUE'      */
         MVI   CPWRTC(CUBCPWA),X'0C'                            04S0334
*   END;                                                        04S0335
*  ELSE CPWRTC = CPWERR;               /* ELSE CODE ='SYNTAX ERROR'  */
         B     @RC00331                                         04S0336
@RF00331 MVI   CPWRTC(CUBCPWA),X'08'                            04S0336
*                                      /* BECAUSE DELIMITER FOUND    */
*                                      /* BEFORE CLOSING PARENTHESES */
* END;                                                          04S0337
*ELSE                                                           04S0338
*IF NEXTCHAR = LPAREN                  /* IF LEFT PARENTHESES FOUND  */
* THEN CPWPNC = CPWPNC+1;              /* COUNT PARENTHESES          */
         B     @RC00329                                         04S0338
@RF00329 CLI   NEXTCHAR(CUBIPT),C'('                            04S0338
         BNE   @RF00338                                         04S0338
         LA    @08,1                                            04S0339
         SLR   @04,@04                                          04S0339
         IC    @04,CPWPNC(,CUBCPWA)                             04S0339
         ALR   @08,@04                                          04S0339
         STC   @08,CPWPNC(,CUBCPWA)                             04S0339
*ELSE                                                           04S0340
*IF NEXTCHAR = RPAREN                  /* IF RIGHT PARENTHESES FOUND */
* THEN DO;                                                      04S0340
         B     @RC00338                                         04S0340
@RF00338 CLI   NEXTCHAR(CUBIPT),C')'                            04S0340
         BNE   @RF00340                                         04S0340
