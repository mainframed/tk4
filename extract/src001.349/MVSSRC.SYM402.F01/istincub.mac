*  THEN DO;                                                     04S0305
         CLI   NEXTCHAR(CUBIPT),X'50'                           04S0305
         BNE   @RF00305                                         04S0305
*   CPWSSR = CPWONE;                   /* INDICATE 'START OF SECURE  */
*                                      /* STRING FOUND               */
*   CPWSEL = CPWZRO;                   /* RESET 'ESCAPE FOUND'       */
         OI    CPWSSR(CUBCPWA),B'01000000'                      04S0308
         NI    CPWSEL(CUBCPWA),B'01111111'                      04S0308
*  END;                                                         04S0309
* ELSE CPWRTC = CPWERR;                /* ELSE CODE ='SYNTAX ERROR'  */
         B     @RC00305                                         04S0310
@RF00305 MVI   CPWRTC(CUBCPWA),X'08'                            04S0310
*ELSE                                                           04S0311
*IF NEXTCHAR = ESCCHAR                 /* IF ESCAPE CHAR. FOUND      */
* THEN CPWSEL =CPWONE;                 /* SET 'ESCAPE FOUND' @D32CKKG*/
         B     @RC00304                                         04S0311
@RF00304 CLI   NEXTCHAR(CUBIPT),X'04'                           04S0311
         BNE   @RF00311                                         04S0311
         OI    CPWSEL(CUBCPWA),B'10000000'                      04S0312
*ELSE                                                           04S0313
*IF CPWSSR = CPWONE                    /* IF SCAN WITHIN MCR DATA    */
* THEN                                                          04S0313
         B     @RC00311                                         04S0313
@RF00311 TM    CPWSSR(CUBCPWA),B'01000000'                      04S0313
         BNO   @RF00313                                         04S0313
* IF NEXTCHAR = IRSCHAR                /* CHECK FOR END OF MCR DATA  */
*  THEN CPWSSR = CPWZRO;               /* INDICATE END OF MCR DATA   */
         CLI   NEXTCHAR(CUBIPT),X'1E'                           04S0314
         BNE   @RF00314                                         04S0314
         NI    CPWSSR(CUBCPWA),B'10111111'                      04S0315
* ELSE;                                                         04S0316
@RF00314 DS    0H                                               04S0317
*ELSE                                                           04S0317
*IF NEXTCHAR = QUOTE                   /* IF QUOTE FOUND             */
* THEN                                 /* THEN FLIP THE QUOTE SWITCH */
         B     @RC00313                                         04S0317
@RF00313 CLI   NEXTCHAR(CUBIPT),X'7D'                           04S0317
         BNE   @RF00317                                         04S0317
*  IF CPWQUO = CPWONE                                           04S0318
*   THEN CPWQUO = CPWZRO;                                       04S0318
         TM    CPWQUO(CUBCPWA),B'00100000'                      04S0318
         BNO   @RF00318                                         04S0318
         NI    CPWQUO(CUBCPWA),B'11011111'                      04S0319
*  ELSE CPWQUO = CPWONE;                                        04S0320
         B     @RC00318                                         04S0320
@RF00318 OI    CPWQUO(CUBCPWA),B'00100000'                      04S0320
*ELSE                                                           04S0321
*IF CPWQUO = CPWONE                    /* IF QUOTE SWITCH ON         */
* THEN;                                /* THEN CONTINUE SCAN (WITHIN */
         B     @RC00317                                         04S0321
@RF00317 TM    CPWQUO(CUBCPWA),B'00100000'                      04S0321
         BO    @RT00321                                         04S0321
*                                      /* QUOTED STRING)             */
*ELSE CPWRTC = CPWUCH;                 /* CODE ='UNRECOGNIZED CHAR.' */
         MVI   CPWRTC(CUBCPWA),X'28'                            04S0323
*                                      /* SO SCAN WILL CHECK FOR     */
*                                      /* DELIMITERS                 */
*                                      /* BOTH BAL AND PL/1 SYNTAX   */
*IF CPWRTC = CPWUCH                    /* IF 'UNRECOGNIZED CHARACTER'*/
* THEN DO;                                                      03S0324
@RT00321 DS    0H                                               03S0324
@RC00317 DS    0H                                               03S0324
@RC00313 DS    0H                                               03S0324
@RC00311 DS    0H                                               03S0324
@RC00304 CLI   CPWRTC(CUBCPWA),40                               03S0324
         BNE   @RF00324                                         03S0324
*  CPWRTC = CPWCON;                    /* CODE ='CONTINUE SCAN'      */
         MVI   CPWRTC(CUBCPWA),X'2C'                            03S0326
*  IF CPWFMT = CPWBAL                  /* IF COMMAND FORMAT IS BAL   */
*   THEN DO;                                                    03S0327
         TM    CPWFMT(CUBCPWA),B'10000000'                      03S0327
         BNO   @RF00327                                         03S0327
*/********************************************************************/
*/*                                                                  */
*/* CHECK FOR BAL PARAMETER VALUE DELIMITERS.                        */
*/*                                                                  */
*/********************************************************************/
*IF NEXTCHAR = BLANK                   /* IF BLANK OR HORIZONTAL TAB */
*  ×NEXTCHAR = HT                      /* FOUND                      */
* THEN DO;                                                      04S0329
         CLI   NEXTCHAR(CUBIPT),X'40'                           04S0329
         BE    @RT00329                                         04S0329
         CLI   NEXTCHAR(CUBIPT),X'05'                           04S0329
