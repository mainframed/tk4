         DC    AL1(16)                                          01S0001
         DC    C'ISTINCUB  76.257'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                                      /* OF INPUT STRING            */
*/********************************************************************/
*/*                                                                  */
*/* ISTINCUB MAINLINE PROCESSING -                                   */
*/*                                                                  */
*/********************************************************************/
*CUBNCSPL = CUBINPUT;                  /* SET NCSPL BASE FROM INPUT  */
         LR    CUBNCSPL,CUBINPUT                                02S0232
*RFY ISTNCSPL BASED(CUBNCSPL);                                  02S0233
*/********************************************************************/
*/*                                                                  */
*/* UPDATE COMPONENT RECOVERY RECORD AUDIT TRAIL TO INDICATE THAT    */
*/* SUBROUTINE ISTINCUB HAS BEEN ENTERED.                            */
*/*                                                                  */
*/********************************************************************/
*  DO;                                                          03S0234
*/***  CRRMAINT  PROCNM(ISTINCUB)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  *
**/                                                             03S0235
*                                                               03S0235
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */               03S0235
*  RFY ISTLCCW ;                                                03S0235
*  RFY ISTLCPB ;                                                03S0236
*  RFY ISTZSAVE;                                                03S0237
*  RFY ISTPFCRR;                                                03S0238
*  RFY ISTCFCRR;                                                03S0239
*  RFY ISTZCRR ;                                                03S0240
*  RFY ISTASCRR;                                                03S0241
*  RFY ISTAPCRR;                                                03S0242
*  RFY ISTCRA  ;                                                03S0243
*  RFY ISTCNSTA;                                                03S0244
*  RFY ISTVYCRR;                                                03S0245
*/* END OF ATBUILD */                                           03S0246
*  /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                 03S0246
*NCSPLRPH->RPHCRR->CRACRR->NINCUB = '1'B;                       03S0246
         L     @08,NCSPLRPH(,CUBNCSPL)                          03S0246
         L     @08,RPHCRR(,@08)                                 03S0246
         L     @08,CRACRR(,@08)                                 03S0246
         OI    NINCUB(@08),B'10000000'                          03S0246
*  /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                03S0247
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                       03S0247
         XC    VYCCRT+1(11,@08),VYCCRT+1(@08)                   03S0247
         MVI   VYCCRT(@08),B'00000000'                          03S0247
*NCSPLRPH->RPHCRR->CRACRR->CINCUB = '1'B;                       03S0248
         OI    CINCUB(@08),B'10000000'                          03S0248
*  END;                                                         03S0249
*/********************************************************************/
*/*                                                                  */
*/* PERFORM STANDARD SSCP ROUTINE ENTRY LINKAGE. ESTABLISH CURRENT   */
*/* ROUTINE'S SAVE AREA IN THE NCSPL WORK AREA BY DECREMENTING THE   */
*/* ADDRESS OF THE CALLER'S SAVE AREA BY THE LENGTH OF THE SAVE AREA.*/
*/* DO FORWARD AND BACKWARD CHAINING OF SAVE AREAS.                  */
*/*                                                                  */
*/********************************************************************/
*DCL 1 SSCPSAVE BASED,                 /* SAVE AREA OVERLAY          */
*      2 * FIXED(31),                                           03S0250
*      2 SSCPBPTR PTR(31),             /* BACK POINTER               */
*      2 SSCPFPTR PTR(31),             /* FORWARD POINTER            */
*      2 *(15) FIXED(31);              /* SAVE AREA REGS 14-12       */
*DCL SSCPNXAV PTR(31) BASED(NCSPLWKA); /* NEXT AVAILABLE POINTER     */
*                                      /* IN NCSPL WORK AREA         */
*DCL SSCPSAVR PTR(31) REG(13) RSTD;    /* SAVE AREA POINTER          */
*RFY SSCPWORK RSTD;                                             03S0253
*SSCPWORK = SSCPSAVR;                  /* PRESERVE SAVE AREA PTR     */
         LR    SSCPWORK,SSCPSAVR                                03S0254
*SSCPSAVR = SSCPSAVR-LENGTH(SSCPSAVE); /* SAVE AREA BASE             */
         SL    SSCPSAVR,@CF00737                                03S0255
*SSCPWORK->SSCPFPTR=SSCPSAVR;          /* SET CALLER'S FORWARD PTR   */
         ST    SSCPSAVR,SSCPFPTR(,SSCPWORK)                     03S0256
*SSCPSAVR->SSCPBPTR=SSCPWORK;          /* SET BACK POINTER           */
         ST    SSCPWORK,SSCPBPTR(,SSCPSAVR)                     03S0257
*RFY SSCPWORK UNRSTD;                                           03S0258
*SSCPSAVR->SSCPFPTR=0;                 /* ZERO FORWARD POINTER       */
         SLR   @08,@08                                          03S0259
         ST    @08,SSCPFPTR(,SSCPSAVR)                          03S0259
*RFY CUBWKA RSTD;                                               02S0260
*CUBWKA = SSCPNXAV;                    /* NEXT AVAILABLE WORK AREA   */
         L     @08,NCSPLWKA(,CUBNCSPL)                          02S0261
         L     CUBWKA,SSCPNXAV(,@08)                            02S0261
*GENERATE NOSETS REFS(CUBWKA);         /* BASE FOR DYNAMIC AREA      */
         USING @DATD,CUBWKA
*SSCPNXAV = SSCPNXAV + @SIZDATD;       /* ADJUST NEXT AVAILABLE      */
         L     @10,NCSPLWKA(,CUBNCSPL)                          02S0263
         L     @08,SSCPNXAV(,@10)                               02S0263
         AL    @08,@SIZDATD                                     02S0263
         ST    @08,SSCPNXAV(,@10)                               02S0263
*RFY CUBCPWA RSTD;                                              02S0264
*CUBCPWA = NCSCPWPT;                   /* BASE FOR CPWA FROM NCSPL   */
         L     CUBCPWA,NCSCPWPT(,CUBNCSPL)                      02S0265
*RFY ISTCPWA BASED(CUBCPWA);                                    02S0266
*CPWLCL(1:8) = '00'X;                  /*RESET LOCAL SWITCHES IN CPWA
*                                                            @ZA08920*/
         MVI   CPWLCL(CUBCPWA),X'00'                            02S0267
*CPWVLN = 0;                           /* RESET VALUE LENGTH IN CPWA */
         MVI   CPWVLN(CUBCPWA),X'00'                            02S0268
*CPWRTC = CPWCON;                      /* RETURN CODE='CONTINUE'     */
         MVI   CPWRTC(CUBCPWA),X'2C'                            02S0269
*CPWSIG = CPWZRO;                      /* SIGNIFICANCE = OFF @DM14066*/
         NI    CPWSIG(CUBCPWA),B'11111101'                      02S0270
*RFY CUBIPT RSTD;                                               02S0271
*CUBIPT = CPWIPT;                      /* INPUT SCAN PTR             */
         L     CUBIPT,CPWIPT(,CUBCPWA)                          02S0272
*OUTPTR = ADDR(CPWVCH);                /* 1ST OUTPUT CHAR. POSITION  */
         LA    OUTPTR,CPWVCH(,CUBCPWA)                          02S0273
*OUTLNGTH = 0;                         /* RESET LENGTH ACCUMULATOR   */
         SLR   OUTLNGTH,OUTLNGTH                                02S0274
*IF CPWFMT=CPWPL1 THEN DO;             /* SUPPRESS BLANKS    @DM14066*/
         TM    CPWFMT(CUBCPWA),B'10000000'                      02S0275
         BNZ   @RF00275                                         02S0275
*/********************************************************************/
*/*                                                                  */
*/* UPDATE SCAN POINTER TO SKIP BLANKS IN PL/1 PARAMETER VALUES.     */
*/*                                                                  */
*/********************************************************************/
*IF NEXTCHAR = BLANK                   /* IF BLANK OR HORIZONTAL TAB */
*  ×NEXTCHAR = HT                      /* FOUND                      */
* THEN DO;                                                      03S0277
         CLI   NEXTCHAR(CUBIPT),X'40'                           03S0277
         BE    @RT00277                                         03S0277
         CLI   NEXTCHAR(CUBIPT),X'05'                           03S0277
         BNE   @RF00277                                         03S0277
@RT00277 DS    0H                                               03S0278
*  IF CPWSIG = CPWONE                  /* IF 'SIGNIFICANCE STARTED'  */
*   THEN DO;                                                    03S0279
         TM    CPWSIG(CUBCPWA),B'00000010'                      03S0279
         BNO   @RF00279                                         03S0279
*    CPWSIG = CPWZRO;                  /* RESET INDICATOR            */
*    CPWBLS = CPWONE;                  /*INDICATE 'BLANKS SUPPRESSED'*/
         OI    CPWBLS(CUBCPWA),B'00000001'                      03S0282
         NI    CPWSIG(CUBCPWA),B'11111101'                      03S0282
*   END;                                                        03S0283
*  ELSE;                                                        03S0284
*                                                               03S0284
@RF00279 DS    0H                                               03S0285
*/*  DOUNTIL(NEXTCHAR^=BLANK&NEXTCHAR^=HT)  */                  03S0285
*    DO  UNTIL(NEXTCHAR^=BLANK&NEXTCHAR^=HT);                   03S0285
@DL00285 DS    0H                                               03S0286
*     CUBIPT = CUBIPT+1;               /* UPDATE INPUT SCAN PTR.     */
         AL    CUBIPT,@CF00040                                  03S0286
*     CPWILN = CPWILN-1;               /* DECREMENT REMAINING LENGTH */
*                                                               03S0287
         LH    @08,CPWILN(,CUBCPWA)                             03S0287
         BCTR  @08,0                                            03S0287
         STH   @08,CPWILN(,CUBCPWA)                             03S0287
*/*     LEAVE(DO) ON(CPWILN=0)ANDDO */                          03S0288
*       IF (CPWILN=0) THEN                                      03S0288
         LTR   @08,@08                                          03S0288
         BNZ   @RF00288                                         03S0288
*         DO;                                                   03S0289
*                                      /* IF REMAINING LENGTH IS 0   */
*        CPWEOR = CPWONE;              /* INDICATE 'END OF RECORD'   */
*                                                               03S0290
         OI    CPWEOR(CUBCPWA),B'00100000'                      03S0290
*           GOTO #DC00009; /* END  */                           03S0291
         B     #DC00009                                         03S0291
*         END;                                                  03S0292
*                                                               03S0292
*     END; /* END(DO UNTIL TO SCAN ACROSS BLANKS)  */           03S0293
@RF00288 DS    0H                                               03S0293
@DE00285 CLI   NEXTCHAR(CUBIPT),X'40'                           03S0293
         BE    @DL00285                                         03S0293
         CLI   NEXTCHAR(CUBIPT),X'05'                           03S0293
         BE    @DL00285                                         03S0293
*#DC00009:;                                                     03S0294
#DC00009 DS    0H                                               03S0295
* END;                                                          03S0295
*ELSE;                                                          03S0296
@RF00277 DS    0H                                               03S0297
*END;                                  /*                    @DM14066*/
*DO UNTIL (CPWRTC^=CPWCON);            /* SCAN INPUT AND BUILD       */
@RF00275 DS    0H                                               02S0298
@DL00298 DS    0H                                               02S0299
*                                      /* VALUE AS LONG AS CODE =    */
*                                      /* 'CONTINUE'                 */
* IF CPWEOR = CPWONE                   /* IF END-OF-RECORD FOUND     */
*  THEN                                                         02S0299
         TM    CPWEOR(CUBCPWA),B'00100000'                      02S0299
         BNO   @RF00299                                         02S0299
*  IF CPWLCL^=0×CPWPNC>0×              /* IF VALUE NOT ENDED         */
*     (CPWFMT=CPWPL1&CPWTYP=CPWPPP)                             02S0300
*   THEN CPWRTC = CPWERR;              /* CODE = 'SYNTAX ERROR'      */
         LH    @08,CPWLCL(,CUBCPWA)                             02S0300
         N     @08,@CF02347                                     02S0300
         LTR   @08,@08                                          02S0300
         BNZ   @RT00300                                         02S0300
         CLI   CPWPNC(CUBCPWA),0                                02S0300
         BH    @RT00300                                         02S0300
         TM    CPWFMT(CUBCPWA),B'11000000'                      02S0300
         BNZ   @RF00300                                         02S0300
@RT00300 DS    0H                                               02S0301
         MVI   CPWRTC(CUBCPWA),X'08'                            02S0301
*  ELSE CPWRTC = CPWEOV;               /* ELSE 'END-OF-VALUE'        */
         B     @RC00300                                         02S0302
@RF00300 MVI   CPWRTC(CUBCPWA),X'0C'                            02S0302
* ELSE DO;                             /* ELSE NOT END-OF-RECORD     */
         B     @RC00299                                         02S0303
@RF00299 DS    0H                                               02S0304
*/********************************************************************/
*/*                                                                  */
