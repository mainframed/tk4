*            DCL GPR01F FIXED(31) REG(01);                         0130
*            DCL GPR01P PTR(31)   REG(01);                         0131
*            DCL GPR02F FIXED(31) REG(02);                         0132
*            DCL GPR02P PTR(31)   REG(02);                         0133
*            DCL GPR03F FIXED(31) REG(03);                         0134
*            DCL GPR03P PTR(31)   REG(03);                         0135
*            DCL GPR04F FIXED(31) REG(04);                         0136
*            DCL GPR04P PTR(31)   REG(04);                         0137
*            DCL GPR05F FIXED(31) REG(05);                         0138
*            DCL GPR05P PTR(31)   REG(05);                         0139
*            DCL GPR06F FIXED(31) REG(06);                         0140
*            DCL GPR06P PTR(31)   REG(06);                         0141
*            DCL GPR07F FIXED(31) REG(07);                         0142
*            DCL GPR07P PTR(31)   REG(07);                         0143
*            DCL GPR08F FIXED(31) REG(08);                         0144
*            DCL GPR08P PTR(31)   REG(08);                         0145
*            DCL GPR09F FIXED(31) REG(09);                         0146
*            DCL GPR09P PTR(31)   REG(09);                         0147
*            DCL GPR10F FIXED(31) REG(10);                         0148
*            DCL GPR10P PTR(31)   REG(10);                         0149
*            DCL GPR11F FIXED(31) REG(11);                         0150
*            DCL GPR11P PTR(31)   REG(11);                         0151
*            DCL GPR12F FIXED(31) REG(12);                         0152
*            DCL GPR12P PTR(31)   REG(12);                         0153
*            DCL GPR13F FIXED(31) REG(13);                         0154
*            DCL GPR13P PTR(31)   REG(13);                         0155
*            DCL GPR14F FIXED(31) REG(14);                         0156
*            DCL GPR14P PTR(31)   REG(14);                         0157
*            DCL GPR15F FIXED(31) REG(15);                         0158
*            DCL GPR15P PTR(31)   REG(15);                         0159
*            RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;               0160
*            RFY ISTRPH BASED(GPR01P);                             0161
*            GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,                 0162
*                     GPR06P,GPR07P,GPR08P,GPR09P,                 0162
*                     GPR10P,GPR11P,GPR12P,GPR13P)                 0162
*                SETS(RPHWORK);                                    0162
*                                                                  0162
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS
*            GPR00F = RULENGTH;                        /*SIZE IN REG0*/
         LR    GPR00F,RULENGTH                                     0163
*            DCL 1 CMZA0014 FIXED(31) STATIC LOCAL,                0164
*                  2 * FIXED(8) INIT(64),            /* OPTION FLAGS */
*                  2 * FIXED(8) INIT(96),             /* STORAGE KEY */
*                  2 * FIXED(8) INIT(241),         /* SUBPOOL NUMBER */
*                  2 * FIXED(8) INIT('00'X);         /* REQUEST TYPE */
*            GPR03F = CMZA0014;                                    0165
*                                                                  0165
         L     GPR03F,CMZA0014                                     0165
*            DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR02P)) */         0166
*              GPR02P = ATCLCPTR; /*SET UP ATCVT POINTER*/         0167
         L     GPR02P,ATCLCPTR                                     0167
