*/********************************************************************/
*/*                                                                  */
*/* BUILDRU: CONSTRUCTS AN INITIATE SELF RU IN THE SPACE OBTAINED BY */
*/*        GETRU.  FIRST, THE MINIMUM INITIATE SELF RU IS BUILT,     */
*/*        AFTER WHICH THE VARIABLE FIELDS (APPLID, LOGMODE, DATA    */
*/*        AND THEIR LENGTHS) ARE INSERTED VIA CALLS TO THE          */
*/*        GETVAL SUBROUTINE.                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0190
*    /*SEGMENT(BUILDRU)START*/DO;GOTO BUILDRU@;BUILDRU:END;        0190
         B     BUILDRU@                                            0191
BUILDRU  DS    0H                                                  0193
*   RFY                                                            0193
*     RUPTR RSTD;                                                  0193
*                                                                  0193
*/*    BUILD FIXED AND DEFAULT INITIATE SELF RU FIELDS               */
*                                                                  0194
*   RUPTR=NCSPLFSB+2;               /* SET BASEFOR RU CONSTRUCTION   */
         LA    RUPTR,2                                             0194
         AL    RUPTR,NCSPLFSB(,NCSPLPTR)                           0194
*   RU0TO3=HEXCONST;                                               0195
         MVC   RU0TO3(4,RUPTR),@CB00091                            0195
*   RUKEY=FOX3;                                                    0196
         MVI   RUKEY(RUPTR),X'F3'                                  0196
*   RUZEROS=ZERO4;                                                 0197
         MVC   RUZEROS(4,RUPTR),@CB00095                           0197
*   LOGMODU4=BLANKS8;                                              0198
*                                                                  0198
         MVC   LOGMODU4(8,RUPTR),@CC00097                          0198
*/*    FILL IN VARIABLE RU FIELDS                                    */
*                                                                  0199
*   TARGPTR=RUPTR+LENGTH(RU0TO3);   /* POINT AT LOGMODE FIELD IN RU  */
         LA    TARGPTR,4                                           0199
         ALR   TARGPTR,RUPTR                                       0199
*   R1EPTR=ADDR(CU4LOGMD);          /* SET R1 TO DEFAULT VALUE FOR 0200
*                                      CPGVAL                        */
         LA    R1EPTR,CU4LOGMD                                     0200
*   CALL GETVAL;                    /* AND MOVE LOGMODE PARM VALUE TO
*                                      RU                            */
         BAL   @14,GETVAL                                          0201
*   TARGPTR=TARGPTR+LENGTH(LOGMODU4)/* POINT AT APPLID FIELD IN RU   */
*           +LENGTH(RUKEY)+LENGTH(APLIDLEN);                       0202
         AL    TARGPTR,@CF02019                                    0202
*   R1EPTR=ADDR(CU4APLID);          /* SET R1 TO DEFAULT VALUE FOR 0203
*                                      CPGVAL                        */
         LA    R1EPTR,CU4APLID                                     0203
*   CALL GETVAL;                    /* AND MOVE APPLID PARM VALUE TO
*                                      RU                            */
         BAL   @14,GETVAL                                          0204
*   APLIDLEN=CPWVLN;                /* PUT APPLID VALUE LENGTH IN RU */
         SLR   @15,@15                                             0205
         IC    @15,CPWVLN(,CPWPTR)                                 0205
         STC   @15,APLIDLEN(,RUPTR)                                0205
*   TARGPTR=TARGPTR+CPWVLN;         /* POINT PASSED VALUE OF APPLID
*                                      PARM                          */
         ALR   TARGPTR,@15                                         0206
*   THREE0=ZERO3;                   /* MOVE IN DEFAULT VALUES        */
         MVC   THREE0(3,TARGPTR),@CB00100                          0207
*   TARGPTR=TARGPTR+LENGTH(THREE0); /* POINT AT DATA FIELD IN RU     */
         AL    TARGPTR,@CF00077                                    0208
*   IF TARGPTR=RUPTR+(NCSPLFSB->NCSRULEN)    /* NO MORE SPACE IN RU  */
*     THEN                                                         0209
         L     @15,NCSPLFSB(,NCSPLPTR)                             0209
         LR    @00,RUPTR                                           0209
         AH    @00,NCSRULEN(,@15)                                  0209
         CR    TARGPTR,@00                                         0209
         BE    @RT00209                                            0209
*     ;                                                            0210
*   ELSE                            /* DATA PARM EXISTS              */
*     DO;                                                          0211
