         L     @15,ATCGTSTR(,GPR02P)                               0169
         BALR  @14,@15                                             0169
*            RFY (GPR00P,GPR01P,GPR02P,GPR03P) UNRSTD;             0170
*            END ;                                   /*END OF GETSTOR*/
*           RFY                                                    0172
*             RETCODE RSTD;                                        0172
