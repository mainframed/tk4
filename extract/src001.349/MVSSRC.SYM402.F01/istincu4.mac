         LA    @00,CU4ERRLM                                        0102
         ST    @00,MGLVDATA(,@09)                                  0102
*       MGLNS1=HEX10;               /* SENS DATA = REQUEST ERROR     */
         MVI   MGLNS1(@09),X'10'                                   0103
