         SLR   @09,@09                                             0112
         IC    @09,CPWVLN(,CPWPTR)                                 0112
         ALR   RULENGTH,@09                                        0112
*       R1EPTR=ADDR(CU4NEXT);       /* SET R1 TO DEFAULT VALUE FOR 0113
