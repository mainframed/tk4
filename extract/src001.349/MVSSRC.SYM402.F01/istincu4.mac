         LA    @03,MSG003                                          0118
         ST    @03,MGLMSGAD(,@09)                                  0118
*           MGLVDATA=CU4REG2;       /* POINT AT KEYWORD FOUND BY   0119
*                                      CPGVAL                        */
         ST    CU4REG2,MGLVDATA(,@09)                              0119
*           MGLNS1=HEX10;           /* SENS DATA = REQUEST ERROR     */
         MVI   MGLNS1(@09),X'10'                                   0120
*           MGLNS2=HEX05;           /* SENSE DATA = PARAMETER INVALID*/
         MVI   MGLNS2(@09),X'05'                                   0121
*           RFY                                                    0122
*             CU4REG2 UNRSTD;                                      0122
*         END;                                                     0123
*       ELSE                                                       0124
