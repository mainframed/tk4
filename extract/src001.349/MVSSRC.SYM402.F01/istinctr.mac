         XC    CTRPARMS(24),CTRPARMS                            01S0452
*SWITCHES = SWITCHES&&SWITCHES;        /* RESET ALL FLAGS*/     01S0453
         XC    SWITCHES(2),SWITCHES                             01S0453
*RFY R2 RSTD;                                                   01S0454
*                                                               01S0454
*                                                               01S0454
* DO; /* LISATCVT CVTBASE(PAGEABLE,(R2)) */                     01S0455
*   R2 = ATCLCPTR; /*SET UP ATCVT POINTER*/                     01S0456
         L     R2,ATCLCPTR                                      01S0456
* END; /*LISATCVT*/     /* VTAM CVT ADDRESSABILITY   */         01S0457
*                                                               01S0457
*CONID = CONID && CONID;               /* CLEAR CONSOLE ID   @X05CKZA*/
         XC    CONID(9),CONID                                   01S0458
*VTAMCVTA = R2;                        /* SAVE VTAM CVT PTR           *
         ST    R2,VTAMCVTA                                      01S0459
*BPDPTR = ATCBPDA;                     /* BUF POOL DIR ADDR */  01S0460
         L     @03,VTAMCVTA                                     01S0460
         L     @03,ATCBPDA(,@03)                                01S0460
         ST    @03,BPDPTR                                       01S0460
*RFY R2 UNRSTD;                                                 01S0461
*IF R1 > 0                             /* NCSPL PTR TEST              *
*  THEN DO;                            /* ENTERED FOR MODIFY COMMAND  *
         LTR   R1,R1                                            01S0462
         BNP   @RF00462                                         01S0462
*                                                               02S0464
*/*********************************************************************
*/* INITIALIZE TO PROCESS MODIFY TRACE COMMAND                        *
*/*  1. SAVE NCSPLPTR FROM REG.1                                      *
*/*  2. SET SW TO INDICATE MODIFY TRACE                               *
*/*  3. COPY TRACE PARAMETERS FROM NCSPL                              *
*/*  4. SAVE ADDR OF NODE NAME COPY                                   *
*/*  5. SAVE CONSOLE ID                                      @X05CKZA *
*/*********************************************************************
*                                                               02S0464
*NCSPLPTR = R1;                        /* SAVE NCSPL PTR              *
         ST    R1,NCSPLPTR                                      02S0464
*RFY R1 UNRSTD;                        /* RELEASE REG.1               *
*MODSW = ON;                           /* SET MODIFY COMMAND SW       *
         OI    MODSW,B'10000000'                                02S0466
*IF NCSVBF05 = ON                                               02S0467
*  THEN LINETR = ON;                   /* LINE TRACE                  *
         L     @15,NCSPLPTR                                     02S0467
         TM    NCSVBF05(@15),B'00001000'                        02S0467
         BNO   @RF00467                                         02S0467
         OI    LINETR,B'10000000'                               02S0468
*  ELSE IF NCSVBF06 = ON                                        02S0469
*         THEN IOTR = ON;              /* I/O TRACE                   *
         B     @RC00467                                         02S0469
@RF00467 L     @15,NCSPLPTR                                     02S0469
         TM    NCSVBF06(@15),B'00000100'                        02S0469
         BNO   @RF00469                                         02S0469
         OI    IOTR,B'01000000'                                 02S0470
*         ELSE IF NCSVBF07 = ON                                 02S0471
*                THEN BUFRTR = ON;     /* BUFFER TRACE                *
         B     @RC00469                                         02S0471
@RF00469 L     @15,NCSPLPTR                                     02S0471
         TM    NCSVBF07(@15),B'00000010'                        02S0471
         BNO   @RF00471                                         02S0471
         OI    BUFRTR,B'00100000'                               02S0472
*                ELSE IF NCSVBF04 = ON                          02S0473
*                       THEN SMSTR = ON;     /* SMS TRACE */    02S0473
         B     @RC00471                                         02S0473
@RF00471 L     @15,NCSPLPTR                                     02S0473
         TM    NCSVBF04(@15),B'00010000'                        02S0473
         BNO   @RF00473                                         02S0473
         OI    SMSTR,B'00010000'                                02S0474
*                       ELSE;                                   02S0475
@RF00473 DS    0H                                               02S0476
*IF NCSVBF08 = ON                      /* COPY MODIFY TPRINT PARM     *
*  THEN TPRINT = ON;                                            02S0476
@RC00471 DS    0H                                               02S0476
@RC00469 DS    0H                                               02S0476
@RC00467 L     @15,NCSPLPTR                                     02S0476
         TM    NCSVBF08(@15),B'00000001'                        02S0476
         BNO   @RF00476                                         02S0476
         OI    TPRINT,B'10000000'                               02S0477
*  ELSE;                                                        02S0478
@RF00476 DS    0H                                               02S0479
*IF NCSVBF10 = ON                      /* COPY NO MODIFY PARM         *
*  THEN NOTRACE = ON;                                           02S0479
         L     @15,NCSPLPTR                                     02S0479
         TM    NCSVBF10(@15),B'01000000'                        02S0479
         BNO   @RF00479                                         02S0479
         OI    NOTRACE,B'00100000'                              02S0480
*  ELSE;                                                        02S0481
@RF00479 DS    0H                                               02S0482
*IF NCSVBF11 = ON                      /* COPY MODIFY TRACE PARM      *
*  THEN TRACE = ON;                                             02S0482
         L     @15,NCSPLPTR                                     02S0482
         TM    NCSVBF11(@15),B'00100000'                        02S0482
         BNO   @RF00482                                         02S0482
         OI    TRACE,B'01000000'                                02S0483
*  ELSE;                                                        02S0484
@RF00482 DS    0H                                               02S0485
*NODENAME = NCSPLRID;                  /* COPY DEST NODE NAME         *
         L     @15,NCSPLPTR                                     02S0485
         MVC   NODENAME(8),NCSPLRID(@15)                        02S0485
*NAMEPTR = ADDR(NODENAME);             /* SAVE ADDR OF NODE NAME      *
         LA    @00,NODENAME                                     02S0486
         ST    @00,NAMEPTR                                      02S0486
*CONID = NCSPLSID;                     /* SAVE CONSOLE ID    @X05CKZA*/
*                                                               02S0487
         MVC   CONID(9),NCSPLSID(@15)                           02S0487
*/*********************************************************************
*        CALL TRASCHED;                /* SCHED ISTINCER FOR PSS      *
         BAL   @14,TRASCHED                                     01S0488
*        CALL MSGMOD;                  /* WRITE ANY MSGS              *
         BAL   @14,MSGMOD                                       01S0489
*       END;                                                    01S0490
*  ELSE DO WHILE ATCTRCPT > 0;         /* ENTERED FOR START COMMAND */
         B     @RC00462                                         01S0491
@RF00462 B     @DE00491                                         01S0491
@DL00491 DS    0H                                               01S0492
*                                                               02S0492
*/*********************************************************************
*/* 1. SAVE TRACE PARM LIST PTR FROM REG.1 & REMOVE PARM LIST FROM Q  *
*/* 2. COPY TRACE PARAMETERS                                          *
*/* 3. SAVE ADDR OF NODE NAME COPY                                    *
*/*********************************************************************
*                                                               02S0492
*WRKELMNT = WRKELMNT&&WRKELMNT;/*INIT CTR'S WORK ELMNT       @DM14078*/
         XC    WRKELMNT(100),WRKELMNT                           02S0492
*CTRPARMS = CTRPARMS&&CTRPARMS;/*INIT TRACE PARM COPY AREA   @DM14078*/
         XC    CTRPARMS(24),CTRPARMS                            02S0493
*SWITCHES = SWITCHES&&SWITCHES;        /* RESET ALL FLAGS    @DM14078*/
         XC    SWITCHES(2),SWITCHES                             02S0494
*TRCPTR = ATCTRCPT;                    /* SAVE TRACE PARM LIST PTR    *
         L     @15,VTAMCVTA                                     02S0495
         L     @00,ATCTRCPT(,@15)                               02S0495
         ST    @00,TRCPTR                                       02S0495
*ATCTRCPT = TRCPT;                     /* REMOVE PARM LIST FROM QUE   *
         LR    @14,@00                                          02S0496
         L     @14,TRCPT(,@14)                                  02S0496
         ST    @14,ATCTRCPT(,@15)                               02S0496
*IF TRCLINE = ON                       /* LINE TRACE                  *
*  THEN LINETR = ON;                                            02S0497
         LR    @15,@00                                          02S0497
         TM    TRCLINE(@15),B'00001000'                         02S0497
         BNO   @RF00497                                         02S0497
         OI    LINETR,B'10000000'                               02S0498
*  ELSE IF TRCIO = ON                  /* I/O TRACE                   *
*         THEN IOTR = ON;                                       02S0499
         B     @RC00497                                         02S0499
@RF00497 L     @15,TRCPTR                                       02S0499
         TM    TRCIO(@15),B'00010000'                           02S0499
         BNO   @RF00499                                         02S0499
         OI    IOTR,B'01000000'                                 02S0500
*         ELSE IF TRCBUF = ON          /* BUFFER TRACE                *
*                THEN BUFRTR = ON;                              02S0501
         B     @RC00499                                         02S0501
@RF00499 L     @15,TRCPTR                                       02S0501
         TM    TRCBUF(@15),B'00100000'                          02S0501
         BNO   @RF00501                                         02S0501
         OI    BUFRTR,B'00100000'                               02S0502
*                ELSE IF TRCSMS = ON                            02S0503
*                       THEN SMSTR = ON;     /* SMS TRACE */    02S0503
         B     @RC00501                                         02S0503
@RF00501 L     @15,TRCPTR                                       02S0503
         TM    TRCSMS(@15),B'00000010'                          02S0503
         BNO   @RF00503                                         02S0503
         OI    SMSTR,B'00010000'                                02S0504
*                       ELSE;                                   02S0505
@RF00503 DS    0H                                               02S0506
*IF TRCON = ON                         /* COPY ACTIVATE TRACE PARM    *
*  THEN TRACE = ON;                                             02S0506
@RC00501 DS    0H                                               02S0506
@RC00499 DS    0H                                               02S0506
@RC00497 L     @15,TRCPTR                                       02S0506
         TM    TRCON(@15),B'10000000'                           02S0506
         BNO   @RF00506                                         02S0506
         OI    TRACE,B'01000000'                                02S0507
*  ELSE;                                                        02S0508
@RF00506 DS    0H                                               02S0509
*IF TRCOF = ON                         /* COPY DEACTIVATE TRACE PARM */
*  THEN NOTRACE = ON;                                           02S0509
         L     @15,TRCPTR                                       02S0509
         TM    TRCOF(@15),B'01000000'                           02S0509
         BNO   @RF00509                                         02S0509
         OI    NOTRACE,B'00100000'                              02S0510
*  ELSE;                                                        02S0511
@RF00509 DS    0H                                               02S0512
*NODENAME = TRCNN;                     /* COPY DEST NODE NAME         *
         L     @15,TRCPTR                                       02S0512
         MVC   NODENAME(8),TRCNN(@15)                           02S0512
*NAMEPTR = ADDR(NODENAME);             /* SAVE NODE NAME ADDR         *
*                                                               02S0513
         LA    @00,NODENAME                                     02S0513
         ST    @00,NAMEPTR                                      02S0513
*/*********************************************************************
*        CALL TRASCHED;                /* SCHED ISTINCER FOR PSS      *
*                                                               01S0514
*                                                               01S0514
         BAL   @14,TRASCHED                                     01S0514
*/*VFRMAIN  SIZE(LENGTH(ISTTRCPL))STORAGE((TRCPTR))SUBPOOL(241)*/
*         DO;                                                   01S0515
*                                                               01S0515
*         RFY (GPR00P) RSTD;             /*PUT VALUE PASSED AS SIZE  */
*                                        /*PARM IN  THREE LOW-ORDER  */
*                                        /*BYTES OF REG 0            */
*           GPR00F=LENGTH(ISTTRCPL) & '00FFFFFF'X;              01S0517
*                                                               01S0517
         LA    GPR00F,16                                        01S0517
*         RFY (GPR15P) RSTD;             /*PUT SUBPOOL PARM IN HIGH- */
*                                        /*ORDER BYTE OF REG 15      */
*           GPR15F=241;                                         01S0519
         LA    GPR15F,241                                       01S0519
*           SLL (GPR15P,24);             /*COMBINE SUBPOOL AND SIZE  */
         SLL   GPR15P,24                                        01S0520
*                                        /*IN REGISTER 0             */
*           GPR00P=GPR00P × GPR15P;                             01S0521
*                                                               01S0521
         OR    GPR00P,GPR15P                                    01S0521
*         RFY (GPR15P) UNRSTD;           /*DECLARE AND RESTRICT REG 1*/
*                                                               01S0522
*         RFY (GPR01P) RSTD;             /*INVOKE XREGSET MACRO TO   */
*                                        /*CHECK STORAGE PARM AND PUT*/
*                                        /*ADDR IN REGISTER 1        */
*                                                               01S0524
*/*XREGSET  PID('STORAGE')PPARM((TRCPTR))RREG(1,GPR01P)*/       01S0524
*         DO;                                                   01S0524
*           GPR01P=TRCPTR;               /*SET RREG SYMBOL TO VALUE*/
         L     GPR01P,TRCPTR                                    01S0525
*                                        /*OF PPARM OPERAND        */
*         END;  /* XREGSET */            /*INVOKE FREEMAIN MACRO     */
*           GEN;                                                01S0527
         FREEMAIN R,LV=(0),A=(1)
*                                        /*UNRESTRICT REG 1          */
*                                                               01S0528
*         RFY (GPR01P) UNRSTD;           /* UNRESTRICT REG 0         */
*                                                               01S0528
*         RFY (GPR00P) UNRSTD;                                  01S0529
*         END;  /* VFRMAIN */                                   01S0530
*        CALL MSGMOD;                  /* WRITE ANY MSGS              *
         BAL   @14,MSGMOD                                       01S0531
*       END;                                                    01S0532
@DE00491 L     @15,VTAMCVTA                                     01S0532
         L     @00,ATCTRCPT(,@15)                               01S0532
         LTR   @00,@00                                          01S0532
         BP    @DL00491                                         01S0532
*RETURN;                               /* RESTORE REGS & RETURN       *
@EL00001 L     @13,4(,@13)                                      01S0533
@EF00001 DS    0H                                               01S0533
@ER00001 LM    @14,@12,12(@13)                                  01S0533
         BR    @14                                              01S0533
*TRASCHED: PROC OPTIONS(NOSAVE,NOSAVEAREA);                     01S0534
         B     @EL00001                                         01S0534
TRASCHED DS    0H                                               01S0535
*/*********************************************************************
*/* 1. INIT CTR'S DYNAMIC PAB WITH                                    *
*/*     A.      TASK ID FROM ACB DEB                                  *
*/*     B. PAB OFFSET                                                 *
*/*     C. PTR TO ADDR OF PROCESS TO BE EXECUTED UNDER PSS            *
*/* 2. SAVE REGS 2-14 IN CTR'S WORK ELEMENT                           *
*/* 3. TPQUE CTR'S WORK ELEMENT TO RUN ISTINCER UNDER PSS             *
*/* 4. RESTORE REGS 2-14 FROM CTR'S WORK ELMNT                        *
*/* 5. WAIT FOR ISTINCER PROCESSING TO COMPLETE                       *
*/* 6. RETURN TO CALLER                                               *
*/*********************************************************************
*                                                               01S0535
*RFY R14 RSTD;              /* RSTRCT REG.14 FOR RTRN ADDR */   01S0535
*CTRWEA = ADDR(WRKELMNT);              /* ADDR OR CTR WORK ELMNT      *
         LA    @15,WRKELMNT                                     01S0536
         ST    @15,CTRWEA                                       01S0536
*CTRDVT1 = ADDR(ISTINCER);             /* ADDR OF PROC TO BE EXECUTED *
         LA    @00,ISTINCER                                     01S0537
         ST    @00,CTRDVT1(,@15)                                01S0537
*PABDVTA = ADDR(CTRDVT1);              /* PTR TO PROC ADDR            *
         LA    @11,32                                           01S0538
         ALR   @11,@15                                          01S0538
         LA    @00,CTRDVT1(,@15)                                01S0538
         STCM  @00,7,PABDVTA(@11)                               01S0538
*DYPTSKID = ACDTSKID;                  /* TASK ID                     *
         LA    @10,16                                           01S0539
         ALR   @10,@15                                          01S0539
         L     @09,VTAMCVTA                                     01S0539
         L     @09,ATCCONFT(,@09)                               01S0539
         L     @09,RAPACDEB+556(,@09)                           01S0539
         L     @00,ACDTSKID(,@09)                               01S0539
         ST    @00,DYPTSKID(,@10)                               01S0539
*PABOFFST = ADDR(DYPPAB) - ADDR(CTRDYPAB);  /* PAB OFFSET             *
         LA    @00,DYPPAB(,@10)                                 01S0540
         LA    @15,CTRDYPAB(,@15)                               01S0540
         SLR   @00,@15                                          01S0540
         STC   @00,PABOFFST(,@11)                               01S0540
*PABSSN = ON;    /* DO NOT RUN IN SRB MODE                   @ZM42049*/
*                                                               01S0541
         OI    PABSSN(@11),B'00001000'                          01S0541
*GEN REFS(R2,R14,R15,CTRWEA) SETS(CTRSAVE);                     01S0542
*                                                               01S0542
         L     R15,CTRWEA
         STM   R2,R14,CTRSAVE(R15)      SAVE REGS 2 - 14
*RFY R14 UNRSTD;            /* RELEASE REG.14 */                01S0543
*                                                               01S0543
*/*  TPQUE (CTRWEA,DYPPAB) NORM NSTR */                         01S0544
*                                                               01S0544
* DO;                                                           01S0544
* RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;                01S0545
* GPR00P=0;                                                     01S0546
         SLR   GPR00P,GPR00P                                    01S0546
* GPR15P=ADDR(DYPPAB); /*HOLD PAB PTR*/                         01S0547
         L     @14,CTRWEA                                       01S0547
         LA    GPR15P,DYPPAB+16(,@14)                           01S0547
* GPR04P=CTRWEA; /*WORK ELE PTR*/                               01S0548
         LR    GPR04P,@14                                       01S0548
* GPR05P=GPR15P; /*PASS PAB PTR*/                               01S0549
         LR    GPR05P,GPR15P                                    01S0549
* CALL ATCLCPTR->ATCAP37->ISTAPC37;                             01S0550
         L     @14,ATCLCPTR                                     01S0550
         L     @15,ATCAP37(,@14)                                01S0550
         BALR  @14,@15                                          01S0550
* GEN(NOPR  8)NOSETS NOREFS;                                    01S0551
         NOPR  8
* RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) UNRSTD;              01S0552
* END; /**** END OF TPQUE GENERATION ****/    /* SCHED ISTINCER 01S0553
*     */                                                        01S0553
*                                                               01S0553
*GEN REFS(CTRSAVE,CTRWEA) SETS(R2,R14,R15) FLOWS(ISTINCER);     01S0554
*                                                               01S0554
         L     R15,CTRWEA
         LM R2,R14,CTRSAVE(R15)           RESTORE REGS 2 - 14
*/*VWAIT  ECB(CTRECB)TASK(VTAM)*/                               01S0555
* DO;                                                           01S0555
*                                        /*DECLARE AND RESTRICT REG 1*/
*   RESPECIFY GPR01F RSTD;               /*INVOKE XREGSET MACRO TO*/
*                                        /*TEST ECB PARM VALIDITY */
*                                                               01S0557
*/*XREGSET  PPARM(CTRECB)RREG(1,GPR01F)PID('ECB')*/             01S0557
* DO;                                                           01S0557
*   GPR01F=ADDR(CTRECB);                 /*SET RREG SYMBOL TO ADDRESS*/
         L     GPR01F,CTRWEA                                    01S0558
         LA    GPR01F,CTRECB(,GPR01F)                           01S0558
*                                        /*OF PPARM OPERAND          */
* END;  /* XREGSET */                                           01S0559
*   GEN;                                                        01S0560
         WAIT 1,ECB=(1)
*   RESPECIFY GPR01F UNRSTD;             /*UNRESTRICT REG 1          */
* END;  /* VWAIT */                                             01S0562
*                                                               01S0562
*END TRASCHED;                                                  01S0563
@EL00002 DS    0H                                               01S0563
@EF00002 DS    0H                                               01S0563
@ER00002 BR    @14                                              01S0563
*MSGMOD:PROC OPTIONS(SAVE(13,14),SAVEAREA);                     01S0564
MSGMOD   ST    @14,12(,@13)                                     01S0564
         ST    @13,@SA00003+4                                   01S0564
         LA    @14,@SA00003                                     01S0564
         ST    @14,8(,@13)                                      01S0564
         LR    @13,@14                                          01S0564
*/*********************************************************************
*/* WRITE ANY PENDING MSG & RESET MSG SWITCHES                        *
*/*********************************************************************
*                                                               01S0565
*IF MSGSW = ON                                                  01S0565
*  THEN DO;                                                     01S0565
         TM    MSGSW,B'10000000'                                01S0565
         BNO   @RF00565                                         01S0565
*        MSGPTR = NAMEPTR;                                      01S0567
         L     @00,NAMEPTR                                      01S0567
         ST    @00,MSGPTR                                       01S0567
*        IF INITOKSW = ON                                       01S0568
*          THEN DO;                                             01S0568
         TM    INITOKSW,B'01000000'                             01S0568
         BNO   @RF00568                                         01S0568
