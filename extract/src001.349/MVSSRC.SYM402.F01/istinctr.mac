*                  THEN CTRMSGID = '513I'; /* TRACE ACTIVATED */
         TM    TRACE,B'01000000'                                01S0570
         BNO   @RF00570                                         01S0570
         MVC   CTRMSGID(4),@CC03743                             01S0571
*                  ELSE CTRMSGID = '512I'; /* TRACE DEACTIVATED */
         B     @RC00570                                         01S0572
@RF00570 MVC   CTRMSGID(4),@CC03744                             01S0572
*                INITOKSW = OFF;                                01S0573
@RC00570 NI    INITOKSW,B'10111111'                             01S0573
*               END;                                            01S0574
*          ELSE                                                 01S0575
*           IF CANCLSW = ON                                     01S0575
*             THEN DO;                                          01S0575
         B     @RC00568                                         01S0575
@RF00568 TM    CANCLSW,B'00100000'                              01S0575
         BNO   @RF00575                                         01S0575
*                   IF MODSW = ON                               01S0577
*                     THEN MSGPTR = ADDR(MODIFY);               01S0577
         TM    MODSW,B'10000000'                                01S0577
         BNO   @RF00577                                         01S0577
         LA    @00,@CC03664                                     01S0578
         ST    @00,MSGPTR                                       01S0578
*                     ELSE MSGPTR = ADDR(START);                01S0579
         B     @RC00577                                         01S0579
@RF00577 LA    @00,@CC03662                                     01S0579
         ST    @00,MSGPTR                                       01S0579
*                   CANCLSW = OFF;                              01S0580
@RC00577 NI    CANCLSW,B'11011111'                              01S0580
*                  CTRMSGID = '033I'; /* CANCEL TRACE COMMAND */
         MVC   CTRMSGID(4),@CC03745                             01S0581
*                  END;                                         01S0582
*             ELSE                                              01S0583
*              IF INVLDSW = ON                                  01S0583
*                THEN DO;                                       01S0583
         B     @RC00575                                         01S0583
@RF00575 TM    INVLDSW,B'00010000'                              01S0583
         BNO   @RF00583                                         01S0583
*                      CTRMSGID = '511I'; /* INVALID NODE */    01S0585
         MVC   CTRMSGID(4),@CC03746                             01S0585
*                      INVLDSW = OFF;                           01S0586
         NI    INVLDSW,B'11101111'                              01S0586
*                     END;                                      01S0587
*                ELSE                                           01S0588
*                 IF LNACTSW = ON                               01S0588
*                   THEN DO;                                    01S0588
         B     @RC00583                                         01S0588
@RF00583 TM    LNACTSW,B'00000100'                              01S0588
         BNO   @RF00588                                         01S0588
*                         CTRMSGID =  '625I'; /* LINE ALREADY ACTIVE */
         MVC   CTRMSGID(4),@CC03747                             01S0590
*                         MSGPTR = ADDR(LINE8);            /*@D32CKZA*/
         LA    @00,@CC03666                                     01S0591
         ST    @00,MSGPTR                                       01S0591
