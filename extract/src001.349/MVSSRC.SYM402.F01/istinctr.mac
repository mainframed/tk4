         TITLE 'ISTINCTR - TRACE INITIALIZATION                        *
                        '
ISTINCTR CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'ISTINCTR  75.323'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         ST    @13,@SA00001+4                                   01S0001
         LA    @14,@SA00001                                     01S0001
         ST    @14,8(,@13)                                      01S0001
         LR    @13,@14                                          01S0001
*WRKELMNT = WRKELMNT&&WRKELMNT;/*INIT CTR'S WORK ELMNT*/        01S0451
         XC    WRKELMNT(100),WRKELMNT                           01S0451
