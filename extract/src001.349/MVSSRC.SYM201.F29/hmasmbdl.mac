         MH    @15,@CH00311                                        0081
         L     @14,CCADBLDL(,CCAPTR)                               0081
         LA    @01,0(@15,@14)                                      0081
         AL    @01,@CF00722                                        0081
         LH    @09,CCABPOS-12(,@01)                                0081
         MH    @09,@CH00719                                        0081
         L     @01,CCABLDLP(,CCAPTR)                               0081
