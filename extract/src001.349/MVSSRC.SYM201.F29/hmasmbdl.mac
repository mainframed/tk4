         ST    @03,@TF00002                                        0074
         ALR   @03,@15                                             0074
         AL    @03,@CF00721                                        0074
         CLC   CCABNAME-4(8,@01),CCABNAME-4(@03)                   0074
         BNH   @RF00074                                            0074
*         DO;                       /* FOR BUBBLE SORT               */
*           TEMP=CCABMPOS(I);       /* SAVE ONE FOR SWITCH           */
         LR    @10,@15                                             0076
