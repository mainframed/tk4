         MVC   @TS00001(10),CCABMPOS-4(@14)                        0077
         MVC   CCABMPOS-4(10,@10),@TS00001                         0077
*           CCABMPOS(J)=TEMP;       /* SWITCH THE OTHER              */
         AL    @15,@TF00002                                        0078
         AL    @15,@CF00721                                        0078
         MVC   CCABMPOS-4(10,@15),TEMP                             0078
