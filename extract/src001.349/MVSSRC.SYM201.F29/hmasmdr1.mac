*   ELSE                            /*                               */
*     IF CCAREJ=ON                  /*                               */
*       THEN                        /*                               */
         B     @RC00257                                            0260
@RF00257 TM    CCAREJ(CCAPTR),B'00010000'                          0260
         BNO   @RF00260                                            0260
