         B     @RC00283                                            0285
@RF00283 MVI   DDACC+5,C'N'                                        0285
*             END;                  /*                               */
*           ELSE                    /*                               */
*             IF CCAUPDJ=ON         /*                               */
*               THEN                /*                               */
         B     @RC00269                                            0287
@RF00269 TM    CCAUPDJ(CCAPTR),B'00000100'                         0287
         BNO   @RF00287                                            0287
*               DO;                 /*                               */
*               END;                /*                               */
*             ELSE                  /*                               */
*               IF CCAUPDU=ON       /*                               */
*                 THEN              /*                               */
         B     @RC00287                                            0290
@RF00287 TM    CCAUPDU(CCAPTR),B'00000010'                         0290
         BNO   @RF00290                                            0290
*                 DO;               /*                               */
*                   IF TBLCDS=ON    /* DD BASED ON DATASET           */
