         TITLE 'HMASMDR1 - DETERMINE DATASETS REQUIRED'
HMASMDR1 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMDR1  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  LA    @10,4095(,@12)                                      0001
         USING @PSTART,@12                                         0001
         USING @PSTART+4095,@10                                    0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(12),0(@01)                                 0001
*   DR1RTNCD=ZERO;                  /*                               */
         SLR   DR1RTNCD,DR1RTNCD                                   0161
*   IOPPTR=CCAIOPTR;                /* PTR TO IOP                    */
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0162
