*       END;                        /*                               */
*     ELSE                          /*                               */
*       IF CCAAPPLY=ON              /*                               */
*         THEN                      /*                               */
         B     @RC00260                                            0263
@RF00260 TM    CCAAPPLY(CCAPTR),B'01000000'                        0263
         BNO   @RF00263                                            0263
*         DO;                       /*                               */
*         END;                      /*                               */
*       ELSE                        /*                               */
*         IF CCARES=ON              /*                               */
*           THEN                    /*                               */
         B     @RC00263                                            0266
@RF00263 TM    CCARES(CCAPTR),B'00001000'                          0266
         BNO   @RF00266                                            0266
*           DO;                     /*                               */
*           END;                    /*                               */
*         ELSE                      /*                               */
*           IF CCAACCPT=ON          /*                               */
*             THEN                  /*                               */
         B     @RC00266                                            0269
@RF00266 TM    CCAACCPT(CCAPTR),B'00100000'                        0269
         BNO   @RF00269                                            0269
*             DO;                   /*                               */
*               IF TBLNOAPP=OFF     /* IF NOT NOAPPLY                */
*                 THEN              /*                               */
         L     @15,@PC00001                                        0271
         TM    TBLNOAPP(@15),B'10000000'                           0271
