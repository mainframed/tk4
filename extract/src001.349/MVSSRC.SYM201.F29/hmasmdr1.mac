*   MGPPRINT=ON;                    /*                               */
*                                                                  0165
         OI    MGPHLDS,B'10010000'                                 0165
*   /*****************************************************************/
*   /*                                                               */
*   /* SET INDEX INTO ARRAYS BASED ON FUNCTION REQUESTED             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0166
*   CALL DR1FNCST;                  /* SET FUNCTION INDEX            */
         BAL   @14,DR1FNCST                                        0166
*   RFY                                                            0167
*     RTNCODE RSTD;                 /*                               */
*   DR1RTNCD=MAX(DR1RTNCD,RTNCODE); /*                               */
         CR    DR1RTNCD,RTNCODE                                    0168
         BNL   *+6
         LR    DR1RTNCD,RTNCODE                                    0168
*   RFY                                                            0169
*     RTNCODE UNRSTD;               /*                               */
*                                                                  0170
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR OPTIONALLY REQUIRED (BASED ON OPTIONS ON FUNCTION   */
*   /* STATEMENT) REQUIRED AND SET APPROPRIATE ENTRIES IN VARIOUS    */
*   /* ARRAYS                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0170
*   IF DR1RTNCD=ZERO                /* IF FUNCTION FOUND OK          */
*     THEN                          /*                               */
         CH    DR1RTNCD,@CH00353                                   0170
         BNE   @RF00170                                            0170
*     DO;                           /*                               */
*       CALL DR1OPTDD;              /* SET FUNCTION INDEX            */
         BAL   @14,DR1OPTDD                                        0172
*       RFY                                                        0173
*         RTNCODE RSTD;             /*                               */
*       DR1RTNCD=MAX(DR1RTNCD,RTNCODE);/*                            */
         CR    DR1RTNCD,RTNCODE                                    0174
         BNL   *+6
         LR    DR1RTNCD,RTNCODE                                    0174
*       RFY                                                        0175
*         RTNCODE UNRSTD;           /*                               */
*                                                                  0175
*       /*************************************************************/
*       /*                                                           */
*       /* NOW THAT ARRAYS HAVE BEEN SET UP CHECK TO INSURE THAT     */
*       /* REQUIRED DD STATEMENTS ARE PRESENT                        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0176
*       CALL DR1DDCHK;              /* CHECK FOR DD STATEMTS PRESENT */
         BAL   @14,DR1DDCHK                                        0176
*       RFY                                                        0177
*         RTNCODE RSTD;             /*                               */
*       DR1RTNCD=MAX(DR1RTNCD,RTNCODE);/*                            */
         CR    DR1RTNCD,RTNCODE                                    0178
         BNL   *+6
         LR    DR1RTNCD,RTNCODE                                    0178
*       RFY                                                        0179
*         RTNCODE UNRSTD;           /*                               */
*                                                                  0179
*       /*************************************************************/
*       /*                                                           */
*       /* NOW CHECK THAT ALL REQUIRED SYSTEM ENTRIES ARE PRESENT AND*/
*       /* THAT THE SREL OF EACH MATCHES                             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0180
*       CALL DR1SYSCK;              /*                               */
         BAL   @14,DR1SYSCK                                        0180
*       RFY                                                        0181
*         RTNCODE RSTD;             /*                               */
*       DR1RTNCD=MAX(DR1RTNCD,RTNCODE);/*                            */
         CR    DR1RTNCD,RTNCODE                                    0182
         BNL   *+6
         LR    DR1RTNCD,RTNCODE                                    0182
*       RFY                                                        0183
*         RTNCODE UNRSTD;           /*                               */
*     END;                          /*                               */
*                                                                  0185
*   /*****************************************************************/
*   /*                                                               */
*   /* IF PREVIOUS FUNCTION WAS OK AND NOT IN CHECK MODE FOR THIS    */
*   /* FUNCTION THEN FREE UP ANY SECONDARY IN-STORAGE DIRECTORIES    */
*   /* THAT MIGH HAVE BEEN THERE FROM LAST FUNCTION                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0185
*   IF DR1RTNCD=ZERO                /*                               */
*       &SAVECODE=ZERO              /* AND FUNCTION TO BE EXECUTED   */
*       &CCACHECK=OFF               /*                               */
*     THEN                          /*                               */
@RF00170 SLR   @15,@15                                             0185
         CR    DR1RTNCD,@15                                        0185
         BNE   @RF00185                                            0185
         L     @14,@PC00001+8                                      0185
         C     @15,SAVECODE(,@14)                                  0185
         BNE   @RF00185                                            0185
         TM    CCACHECK(CCAPTR),B'00010000'                        0185
         BNZ   @RF00185                                            0185
*     DO;                           /*                               */
*       CCACHECK=ON;                /* TURN ON CHECK INDICATOR TO  0187
*                                      FORCE IO TO FREE ONLY       0187
*                                      SECONDARY DIS FILES           */
         OI    CCACHECK(CCAPTR),B'00010000'                        0187
*       RFY                                                        0188
*         RTNCODE UNRSTD;           /*                               */
*       CALL DR1FREEA;              /* FREE SECONDARY DIS FILES      */
         BAL   @14,DR1FREEA                                        0189
*       RFY                                                        0190
*         RTNCODE RSTD;             /*                               */
*       DR1RTNCD=MAX(DR1RTNCD,RTNCODE);/*                            */
         CR    DR1RTNCD,RTNCODE                                    0191
         BNL   *+6
         LR    DR1RTNCD,RTNCODE                                    0191
*       RFY                                                        0192
*         RTNCODE UNRSTD;           /*                               */
*       CCACHECK=OFF;               /*                               */
         NI    CCACHECK(CCAPTR),B'11101111'                        0193
*     END;                          /*                               */
*                                                                  0194
*   /*****************************************************************/
*   /*                                                               */
*   /* IF PREVIOUS FUNCTION WAS OK AND NODIS SPECIFIED THEN CALL     */
*   /* ROUTINE TO FREE ALL PRIMARY AND SECONDARY DIS FILES           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0195
*   IF DR1RTNCD=ZERO                /*                               */
*       &SAVECODE=ZERO              /* AND FUNCTION TO BE EXECUTED   */
*       &TBLDISN=ON                 /* AND NODIS SPECIFIED.          */
*     THEN                          /*                               */
@RF00185 SLR   @15,@15                                             0195
         CR    DR1RTNCD,@15                                        0195
         BNE   @RF00195                                            0195
         L     @14,@PC00001+8                                      0195
         C     @15,SAVECODE(,@14)                                  0195
         BNE   @RF00195                                            0195
         L     @15,@PC00001                                        0195
         TM    TBLDISN(@15),B'10000000'                            0195
         BNO   @RF00195                                            0195
*     DO;                           /*                               */
*       RFY                                                        0197
*         RTNCODE UNRSTD;           /*                               */
*       CALL DR1FREEA;              /* FREE ALL DIS FILES            */
         BAL   @14,DR1FREEA                                        0198
*       RFY                                                        0199
*         RTNCODE RSTD;             /*                               */
*       DR1RTNCD=MAX(DR1RTNCD,RTNCODE);/*                            */
         CR    DR1RTNCD,RTNCODE                                    0200
         BNL   *+6
         LR    DR1RTNCD,RTNCODE                                    0200
*       RFY                                                        0201
*         RTNCODE UNRSTD;           /*                               */
*     END;                          /*                               */
*                                                                  0203
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW THAT ALL DD CARDS HAVE BEEN CHECKED FREE UP ANY NON-      */
*   /* REQUIRED/NON-OPTIONAL DIS FILES                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0203
*   IF DR1RTNCD=ZERO                /* PREV FUNCTIONS OK             */
*       &SAVECODE=ZERO              /* AND FUNCTION TO BE EXECUTED   */
*       &TBLDISN=OFF                /* ALREADY FREED IF NODIS        */
*     THEN                          /*                               */
@RF00195 SLR   @15,@15                                             0203
         CR    DR1RTNCD,@15                                        0203
         BNE   @RF00203                                            0203
         L     @14,@PC00001+8                                      0203
         C     @15,SAVECODE(,@14)                                  0203
         BNE   @RF00203                                            0203
         L     @15,@PC00001                                        0203
         TM    TBLDISN(@15),B'10000000'                            0203
         BNZ   @RF00203                                            0203
*     DO;                           /*                               */
*       CALL DR1FREEN(DISREQ,DISOPT);/* FREE NO-REQ NON-OPT DIS    0205
*                                      RETURN CODE COMING BACK IS  0205
*                                      EITHER 0 INDICATING ALL FREED
*                                      OK OR 12 INDICATING NONE    0205
*                                      FREED. AT THIS TIME NEITHER IS
*                                      SIGNIFICANT                   */
         LA    @01,@AL00205                                        0205
         BAL   @14,DR1FREEN                                        0205
*     END;                          /*                               */
*                                                                  0207
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW ATTEMPT TO LOAD ALL REQUIRED DIS FILES                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0207
*   IF DR1RTNCD=ZERO                /* PREV FUNCTIONS OK             */
*       &SAVECODE=ZERO              /* AND FUNCTION TO BE EXECUTED   */
*     THEN                          /*                               */
@RF00203 SLR   @15,@15                                             0207
         CR    DR1RTNCD,@15                                        0207
         BNE   @RF00207                                            0207
         L     @14,@PC00001+8                                      0207
         C     @15,SAVECODE(,@14)                                  0207
         BNE   @RF00207                                            0207
*     DO;                           /*                               */
*       CCACHECK=OFF;               /* TURN OFF CHECK TO CAUSE LOAD  */
         NI    CCACHECK(CCAPTR),B'11101111'                        0209
*       CALL DR1LOADR;              /* LOAD REQUIRED DIS FILES       */
         BAL   @14,DR1LOADR                                        0210
*       RFY                                                        0211
*         RTNCODE RSTD;             /*                               */
*       DR1RTNCD=MAX(DR1RTNCD,RTNCODE);/*                            */
         CR    DR1RTNCD,RTNCODE                                    0212
         BNL   *+6
         LR    DR1RTNCD,RTNCODE                                    0212
*       RFY                                                        0213
*         RTNCODE UNRSTD;           /*                               */
*       IF TBLCHECK=ON              /* IF CHECK MODE                 */
*         THEN                      /*                               */
         L     @15,@PC00001                                        0214
         TM    TBLCHECK(@15),B'01000000'                           0214
         BNO   @RF00214                                            0214
*         CCACHECK=ON;              /* SET CHECK IN CCA              */
         OI    CCACHECK(CCAPTR),B'00010000'                        0215
*     END;                          /*                               */
*                                                                  0216
@RF00214 DS    0H                                                  0217
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW ATTEMPT TO LOAD ALL OPTIONAL DIS FILES                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0217
*   IF DR1RTNCD=ZERO                /* PREV FUNCTIONS OK             */
*       &SAVECODE=ZERO              /* AND FUNCTION TO BE EXECUTED   */
*     THEN                          /*                               */
@RF00207 SLR   @15,@15                                             0217
         CR    DR1RTNCD,@15                                        0217
         BNE   @RF00217                                            0217
         L     @14,@PC00001+8                                      0217
         C     @15,SAVECODE(,@14)                                  0217
         BNE   @RF00217                                            0217
*     DO;                           /*                               */
*       CCACHECK=OFF;               /* TURN OFF CHECK TO CAUSE LOAD  */
         NI    CCACHECK(CCAPTR),B'11101111'                        0219
*       CALL DR1LOADO;              /* LOAD OPTIONAL DIS FILES       */
         BAL   @14,DR1LOADO                                        0220
*       RFY                                                        0221
*         RTNCODE RSTD;             /*                               */
*       DR1RTNCD=MAX(DR1RTNCD,RTNCODE);/*                            */
         CR    DR1RTNCD,RTNCODE                                    0222
         BNL   *+6
         LR    DR1RTNCD,RTNCODE                                    0222
*       RFY                                                        0223
*         RTNCODE UNRSTD;           /*                               */
*       IF TBLCHECK=ON              /* IF CHECK MODE                 */
*         THEN                      /*                               */
         L     @03,@PC00001                                        0224
         TM    TBLCHECK(@03),B'01000000'                           0224
         BNO   @RF00224                                            0224
*         CCACHECK=ON;              /* SET CHECK IN CCA              */
         OI    CCACHECK(CCAPTR),B'00010000'                        0225
*     END;                          /*                               */
*                                                                  0226
@RF00224 DS    0H                                                  0227
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO CALLER                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0227
*   RETURN CODE(DR1RTNCD);          /*                               */
@RF00217 LR    @15,DR1RTNCD                                        0227
         L     @13,4(,@13)                                         0227
         L     @14,12(,@13)                                        0227
         LM    @00,@12,20(@13)                                     0227
         BR    @14                                                 0227
         EJECT
*                                                                  0228
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINE TO SET FUNCTION INDEX BASED ON CCA VALUES             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0228
*DR1FNCST:                          /*                               */
*   PROC;                           /*                               */
DR1FNCST STM   @14,@12,12(@13)                                     0228
*   DECLARE                         /*                               */
*     FNCRC FIXED(8);               /*                               */
*   FNCRC=ZERO;                     /*                               */
         SLR   FNCRC,FNCRC                                         0230
*   IF CCAEOSMP=ON                  /* IF END OF SMP INDICATED       */
*     THEN                                                         0231
         TM    CCAEOSMP(CCAPTR),B'00000001'                        0231
         BNO   @RF00231                                            0231
*     FNCINDX=SMP;                  /* THEN SET FUNCTION FOR SMP TERM*/
         MVI   FNCINDX,X'01'                                       0232
*   ELSE                            /*                               */
*     IF CCAREC=ON                  /*                               */
*       THEN                                                       0233
         B     @RC00231                                            0233
@RF00231 TM    CCAREC(CCAPTR),B'10000000'                          0233
         BNO   @RF00233                                            0233
*       FNCINDX=REC;                /*                               */
         MVI   FNCINDX,X'02'                                       0234
*     ELSE                          /*                               */
*       IF CCAREJ=ON                /*                               */
*         THEN                                                     0235
         B     @RC00233                                            0235
@RF00233 TM    CCAREJ(CCAPTR),B'00010000'                          0235
         BNO   @RF00235                                            0235
*         FNCINDX=REJ;              /*                               */
         MVI   FNCINDX,X'03'                                       0236
*       ELSE                        /*                               */
*         IF CCAAPPLY=ON            /*                               */
*           THEN                                                   0237
         B     @RC00235                                            0237
@RF00235 TM    CCAAPPLY(CCAPTR),B'01000000'                        0237
         BNO   @RF00237                                            0237
*           FNCINDX=APP;            /*                               */
         MVI   FNCINDX,X'04'                                       0238
*         ELSE                      /*                               */
*           IF CCARES=ON            /*                               */
*             THEN                                                 0239
         B     @RC00237                                            0239
@RF00237 TM    CCARES(CCAPTR),B'00001000'                          0239
         BNO   @RF00239                                            0239
*             FNCINDX=RES;          /*                               */
         MVI   FNCINDX,X'05'                                       0240
*           ELSE                    /*                               */
*             IF CCAACCPT=ON        /*                               */
*               THEN                                               0241
         B     @RC00239                                            0241
@RF00239 TM    CCAACCPT(CCAPTR),B'00100000'                        0241
         BNO   @RF00241                                            0241
*               FNCINDX=ACC;        /*                               */
         MVI   FNCINDX,X'06'                                       0242
*             ELSE                  /*                               */
*               IF CCAUPDJ=ON       /*                               */
*                 THEN                                             0243
         B     @RC00241                                            0243
@RF00241 TM    CCAUPDJ(CCAPTR),B'00000100'                         0243
         BNO   @RF00243                                            0243
*                 FNCINDX=JCLIN;    /*                               */
         MVI   FNCINDX,X'07'                                       0244
*               ELSE                /*                               */
*                 IF CCAUPDU=ON     /*                               */
*                   THEN                                           0245
         B     @RC00243                                            0245
@RF00243 TM    CCAUPDU(CCAPTR),B'00000010'                         0245
         BNO   @RF00245                                            0245
*                   FNCINDX=UCLIN;  /*                               */
         MVI   FNCINDX,X'08'                                       0246
*                 ELSE              /*                               */
*                   IF CCALIST=ON   /*                               */
*                     THEN                                         0247
         B     @RC00245                                            0247
@RF00245 TM    CCALIST(CCAPTR),B'00000001'                         0247
         BNO   @RF00247                                            0247
*                     FNCINDX=LIST; /*                               */
         MVI   FNCINDX,X'09'                                       0248
*                   ELSE            /*                               */
*                     IF CCALOG=ON  /*                               */
*                       THEN        /*                               */
         B     @RC00247                                            0249
@RF00247 TM    CCALOG(CCAPTR),B'01000000'                          0249
         BNO   @RF00249                                            0249
*                       FNCINDX=LOG;/*                               */
         MVI   FNCINDX,X'0A'                                       0250
*                     ELSE          /*                               */
*                       IF CCARESET=ON/*                             */
*                         THEN      /*                               */
         B     @RC00249                                            0251
@RF00249 TM    CCARESET(CCAPTR),B'00100000'                        0251
         BNO   @RF00251                                            0251
*                         FNCINDX=RESET;/*                           */
         MVI   FNCINDX,X'0B'                                       0252
*                       ELSE        /*                               */
*                         FNCRC=STOPFUNC;/* STOP UNKNOWN FUNCTION    */
         B     @RC00251                                            0253
@RF00251 LA    FNCRC,12                                            0253
*   RETURN CODE(FNCRC);             /*                               */
@RC00251 DS    0H                                                  0254
@RC00249 DS    0H                                                  0254
@RC00247 DS    0H                                                  0254
@RC00245 DS    0H                                                  0254
@RC00243 DS    0H                                                  0254
@RC00241 DS    0H                                                  0254
@RC00239 DS    0H                                                  0254
@RC00237 DS    0H                                                  0254
@RC00235 DS    0H                                                  0254
@RC00233 DS    0H                                                  0254
@RC00231 LR    @15,FNCRC                                           0254
         L     @14,12(,@13)                                        0254
         LM    @00,@12,20(@13)                                     0254
         BR    @14                                                 0254
