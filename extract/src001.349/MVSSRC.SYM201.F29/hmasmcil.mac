         TITLE 'HMASMCIL, RELFILE PRE-PROCESSOR'
HMASMCIL CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMCIL  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(8),0(@01)                                  0001
         EJECT
*                                                                  0198
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE ERROR CODES SWITCHES, ETC.                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0198
*   HIGHCIL=ZERO;                   /*                               */
         SLR   HIGHCIL,HIGHCIL                                     0198
*   SWITCHES=''B;                   /*                               */
         MVI   SWITCHES,X'00'                                      0199
*   IOPPTR=CCAIOPTR;                /* GENERAL USE IOP               */
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0200
*   SPLPMAD=ADDR(CILPDSN);          /*                               */
         LA    @10,CILPDSN                                         0201
         ST    @10,SPLPMAD                                         0201
*   SPLFUNCT=SPLBDDSN;              /* SUBROUTINE FUNCTION           */
         MVI   SPLFUNCT,X'05'                                      0202
*   CILODSN=BLANK;                  /* INIT RELFILE SAVE AREA        */
         MVI   CILODSN+1,C' '                                      0203
         MVC   CILODSN+2(42),CILODSN+1                             0203
         MVI   CILODSN,C' '                                        0203
*   CILPDSN=BLANK;                  /* INIT RELFILE FOR SUB          */
         MVI   CILPDSN+1,C' '                                      0204
         MVC   CILPDSN+2(42),CILPDSN+1                             0204
         MVI   CILPDSN,C' '                                        0204
*   CILTLIB=BLANK;                  /* INIT RELFILE DSN FOR SMPTLIB  */
         MVI   CILTLIB+1,C' '                                      0205
         MVC   CILTLIB+2(42),CILTLIB+1                             0205
         MVI   CILTLIB,C' '                                        0205
*   RFY                                                            0206
*     RVAPARM BASED(CCARVAD);       /* BASE FOR STAE PARM            */
*                                                                  0206
*   /*****************************************************************/
*   /*                                                               */
*   /* SEARCH THE ICT AND DETERMINE WHICH ELEMENTS SHOULD BE         */
*   /* TRANSFERED TO SMPWRK5                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0207
*   CALL CILCHK;                    /*                               */
*                                                                  0207
         BAL   @14,CILCHK                                          0207
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD IEBCOPY CONTROL STMTS FOR ALL ELEMENTS TO BE TRANSFERED */
*   /* AND INVOKE IEBCOPY.                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0208
*   CALL CILCOPY;                   /*                               */
*                                                                  0208
         BAL   @14,CILCOPY                                         0208
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO CALLER. ANY IEBCOPY ERROR WILL TERMINATE THE        */
*   /* FUNCTION.                                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0209
*   RETURN CODE(HIGHCIL);           /*                               */
         LR    @15,HIGHCIL                                         0209
         L     @13,4(,@13)                                         0209
         L     @14,12(,@13)                                        0209
         LM    @00,@12,20(@13)                                     0209
         BR    @14                                                 0209
         EJECT
*                                                                  0210
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOK AT EACH LMOD IN THE ICT AND FOR EACH WHICH WILL BE       */
*   /* LINKEDITED SEARCH CHECK ITS CHAINED MODS TO DETERMINE WHICH   */
*   /* ELEMENTS SHOULD BE TRANSFERED TO SMPWRK5. NO TRANSFERS WILL BE*/
*   /* DONE UNLESS MORE THAN ONE SMPTLIB DATA SET IS PRESENT. ONLY   */
*   /* SELECTED MODULE REPLACEMENTS WHICH ARE TO BE LINKEDITED AND   */
*   /* FROM OTHER THAN THE FIRST SMPTLIB WILL BE MARKED FOR          */
*   /* PROCESSING .                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0210
*CILCHK:                                                           0210
*   PROC;                           /*                               */
@EL00001 L     @13,4(,@13)                                         0210
@EF00001 DS    0H                                                  0210
@ER00001 LM    @14,@12,12(@13)                                     0210
         BR    @14                                                 0210
CILCHK   STM   @14,@12,@SA00002                                    0210
*   RFY                                                            0211
*     ICTLMOD BASED(CCAICLMD);      /* ICT LMOD BASE                 */
*   RFY                                                            0212
*     ICTIXLF BASED(ICTLCHN(L));    /* CAIN PTRS TO MODS             */
*   RFY                                                            0213
*     ICTMOD BASED(ICTIXL(A));      /* MODS BASED ON LMOD CHAIN      */
*   RFY                                                            0214
*     ICTPTF BASED(ICTPPTR(1));     /* SYSMOD OWNER OF THE MOD       */
*   DECLARE                                                        0215
*     A FIXED(31);                  /* LOOP COUNTER                  */
*   DECLARE                                                        0216
*     L FIXED(31);                  /* LOOP COUNTER                  */
*                                                                  0216
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOK AT ICT LMOD ENTRIES. IF THE LMOD IS TO BE LINKEDITED THEN*/
*   /* CHECK ITS MODS.                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0217
*   DO L=1 BY 1 WHILE ICTLEND(L)^=ICTENDNT;/* LMOD LOOP              */
         LA    L,1                                                 0217
         B     @DE00217                                            0217
@DL00217 DS    0H                                                  0218
*     IF ICTLLKD(L)=ON              /* LOAD MOD WILL BE LINKEDITED   */
*         &ICTLCPL(L)=OFF THEN      /* AND NOT COMPLETED             */
*                                                                  0218
         LR    @10,L                                               0218
         MH    @10,@CH01010                                        0218
         L     @02,CCAICLMD(,CCAPTR)                               0218
         ST    @10,@TF00001                                        0218
         ALR   @10,@02                                             0218
         AL    @10,@CF01828                                        0218
         TM    ICTLLKD-14(@10),B'00001000'                         0218
         BNO   @RF00218                                            0218
         AL    @02,@TF00001                                        0218
         AL    @02,@CF01829                                        0218
         TM    ICTLCPL-8(@02),B'00100000'                          0218
         BNZ   @RF00218                                            0218
*       /*************************************************************/
*       /*                                                           */
*       /* LOOK AT MODS FOR THIS LMOD                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0219
*       DO A=1 BY 1 WHILE ICTIXL(A)^=ICTENDIX;/*                     */
*                                                                  0219
         LA    A,1                                                 0219
         B     @DE00219                                            0219
@DL00219 DS    0H                                                  0220
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK FOR ONLY MOD REPLACEMENTS FROM RELFILES(SMPPTLIB) */
*         /* WHICH ARE TO BE LINKED AND ARE SELECTED.                */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0220
*         IF ICTMMOD(1)=ON          /* ELEMENT IS REPLACENET         */
*             &ICTMRELF(1)=ON       /* ELEMENT IS FROM RELFILE       */
*             &ICTMISEL(1)=ON       /* SELECTED                      */
*             &ICTMCPL(1)=OFF THEN  /* NOT COMPLETED                 */
         LR    @10,A                                               0220
         SLA   @10,2                                               0220
         LR    @05,L                                               0220
         MH    @05,@CH01010                                        0220
         L     @02,CCAICLMD(,CCAPTR)                               0220
         AL    @02,@CF01830                                        0220
         L     @05,ICTLCHN-34(@05,@02)                             0220
         AL    @05,@CF01831                                        0220
         L     @10,ICTIXL(@10,@05)                                 0220
         TM    ICTMMOD(@10),B'10000000'                            0220
         BNO   @RF00220                                            0220
         TM    ICTMRELF(@10),B'00100000'                           0220
         BNO   @RF00220                                            0220
         TM    ICTMISEL(@10),B'10000000'                           0220
         BNO   @RF00220                                            0220
         TM    ICTMCPL(@10),B'00100000'                            0220
         BNZ   @RF00220                                            0220
*           DO;                     /*                               */
*             SPLIPFX=ICTPTPFX(1);  /* RELFILE DSN PREFIX            */
         L     @05,SPLPMAD                                         0222
         L     @02,ICTPPTR(,@10)                                   0222
         MVC   SPLIPFX(26,@05),ICTPTPFX(@02)                       0222
*             SPLISMD=ICTPTFS(1);   /*                               */
         MVC   SPLISMD(7,@05),ICTPTFS(@02)                         0223
*             SPLIFIL=ICTMRELN(1);  /*                               */
         MVC   SPLIFIL(2,@05),ICTMRELN(@10)                        0224
*             CALL HMASMSUB(HMASMSPL);/*                             */
*                                                                  0225
         L     @15,@CV01681                                        0225
         LA    @01,@AL00225                                        0225
         BALR  @14,@15                                             0225
*             /*******************************************************/
*             /*                                                     */
*             /* DETERMINE FIRST RELFILE DATA SET (SMPTLIB). ELEMENTS*/
*             /* IN OTHER THAN THIS ONE WILL BE TRANSFERED.          */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0226
*             IF CILTLIB(1)=BLANK   /* FIRST SMPTLIB NOT FOUND YET   */
*               THEN                /*                               */
         CLI   CILTLIB,C' '                                        0226
         BNE   @RF00226                                            0226
*               DO;                 /*                               */
*                 CILTLIB=SPLODSN;  /* THIS IS THE FIRST, SAVE IT    */
         L     @10,SPLPMAD                                         0228
         MVC   CILTLIB(44),SPLODSN(@10)                            0228
*                 CILPTFNO=ICTPTFS(1);/* SAVE PTF NUMBER             */
         L     @05,@PC00001                                        0229
         LR    @02,A                                               0229
         SLA   @02,2                                               0229
         LR    @15,L                                               0229
         MH    @15,@CH01010                                        0229
         L     @01,CCAICLMD(,CCAPTR)                               0229
         AL    @01,@CF01830                                        0229
         L     @01,ICTLCHN-34(@15,@01)                             0229
         AL    @01,@CF01831                                        0229
         L     @02,ICTIXL(@02,@01)                                 0229
         L     @02,ICTPPTR(,@02)                                   0229
         MVC   CILPTFNO(7,@05),ICTPTFS(@02)                        0229
*                 CILPTFDS=SPLODSN; /* AND DSN                       */
         L     @05,@PC00001+4                                      0230
         MVC   CILPTFDS(44,@05),SPLODSN(@10)                       0230
*               END;                /*                               */
*                                                                  0231
*             /*******************************************************/
*             /*                                                     */
*             /* IF RELFILE (SMPTLIB) NAME OF THIS ELEMENT IS NOT THE*/
*             /* ONE SAVED THEN THIS ELEMENT IS MARKED FOR TRANSFER. */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0232
*             IF SPLODSN^=CILTLIB THEN/*                             */
@RF00226 L     @10,SPLPMAD                                         0232
         CLC   SPLODSN(44,@10),CILTLIB                             0232
         BE    @RF00232                                            0232
*               DO;                 /*                               */
*                 ICTFMLIB(1)=SMPWRK5;/* FOR LINKEDIT INCLUDE        */
         LR    @10,A                                               0234
         SLA   @10,2                                               0234
         LR    @05,L                                               0234
         MH    @05,@CH01010                                        0234
         L     @02,CCAICLMD(,CCAPTR)                               0234
         AL    @02,@CF01830                                        0234
         L     @05,ICTLCHN-34(@05,@02)                             0234
         AL    @05,@CF01831                                        0234
         L     @10,ICTIXL(@10,@05)                                 0234
         MVC   ICTFMLIB(8,@10),@CC01721                            0234
*                 ICTMTRX(1)=ON;    /* MOVE THIS TO SMPWRK5          */
         OI    ICTMTRX(@10),B'00010000'                            0235
*               END;                /*                               */
*           END;                    /*                               */
@RF00232 DS    0H                                                  0238
*       END;                        /* END ICT MOD LOOP              */
@RF00220 AH    A,@CH00089                                          0238
@DE00219 LR    @10,A                                               0238
         SLA   @10,2                                               0238
         LR    @05,L                                               0238
         MH    @05,@CH01010                                        0238
         L     @02,CCAICLMD(,CCAPTR)                               0238
         AL    @02,@CF01830                                        0238
         L     @05,ICTLCHN-34(@05,@02)                             0238
         AL    @05,@CF01831                                        0238
         L     @10,ICTIXL(@10,@05)                                 0238
         CL    @10,ICTENDIX                                        0238
         BNE   @DL00219                                            0238
*   END;                            /* END LMOD LOOP                 */
@RF00218 AH    L,@CH00089                                          0239
@DE00217 LR    @10,L                                               0239
         MH    @10,@CH01010                                        0239
         L     @02,CCAICLMD(,CCAPTR)                               0239
         AL    @02,@CF01833                                        0239
         L     @10,ICTLEND(@10,@02)                                0239
         CL    @10,ICTENDNT                                        0239
         BNE   @DL00217                                            0239
*   END CILCHK;                     /*                               */
@EL00002 DS    0H                                                  0240
@EF00002 DS    0H                                                  0240
@ER00002 LM    @14,@12,@SA00002                                    0240
         BR    @14                                                 0240
         EJECT
*                                                                  0241
*   /*****************************************************************/
*   /*                                                               */
*   /* SEARCH THRU ICT MOD SECTION FOR THOSE SELECTED FOR TRANSFER.  */
*   /* SET UP THE SMPTLIB DD NAME WITH THE RELFILE DATA SET NAME.    */
*   /* WRITE THE IEBCOPY 'COPY' STATEMENT AND MEMBER 'SELECT'        */
*   /* STATEMENTS. INVOKE IEBCOPY FOR EACH RELFILE DATA SET WHICH HAS*/
*   /* ELEMEMTS TO BE TRANSFERED.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0241
*CILCOPY:                                                          0241
*   PROC;                           /*                               */
CILCOPY  STM   @14,@04,@SA00003                                    0241
         STM   @06,@12,@SA00003+28                                 0241
*   RFY                                                            0242
*     ICTMOD BASED(CCAICMOD);       /*                               */
*   DECLARE                                                        0243
*     B FIXED(31);                  /* LOOP COUNTER                  */
*   DECLARE                                                        0244
*     SAVENDX FIXED(31);            /* INDEX FOR MOD REFERENCE       */
*                                                                  0244
*   /*****************************************************************/
*   /*                                                               */
*   /* THE PROCESSING IS REPEATED UNTIL ALL RELFILE DATA SETS HAVING */
*   /* MEMEBERS TO TRANSFER HAVE BEEN PROCESSED.                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0245
*   DOCOPY=ON;                      /* INDICATES NEED FOR COPIES     */
         OI    DOCOPY,B'00100000'                                  0245
*   DO WHILE DOCOPY=ON&HIGHCIL=ZERO;/*                               */
         B     @DE00246                                            0246
@DL00246 DS    0H                                                  0247
*     SAVENDX=ZERO;                 /* INIT INDEX FOR REFERENCE      */
         SLR   SAVENDX,SAVENDX                                     0247
*     DOCOPY=OFF;                   /* RESET COTROL SWITCH           */
         NI    DOCOPY,B'11011111'                                  0248
*     CILODSN=BLANK;                /* CURRENT RELFILE               */
         MVI   CILODSN+1,C' '                                      0249
         MVC   CILODSN+2(42),CILODSN+1                             0249
         MVI   CILODSN,C' '                                        0249
*     FIRSTSW=ON;                   /* FIRST PASS FOR RELFILE        */
*                                                                  0250
         OI    FIRSTSW,B'10000000'                                 0250
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK FOR ELIGIBLE ELEMENTS IN THE ICT MOD SECTION AND WRITE*/
*     /* IEBCOPY STATEMENTS.                                         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0251
*     DO B=1 BY 1 WHILE ICTMEND(B)^=ICTENDNT/* LOOP THRU MODS        */
*           &HIGHCIL=ZERO;          /*                               */
         LA    B,1                                                 0251
         B     @DE00251                                            0251
@DL00251 DS    0H                                                  0252
*       IF ICTMTRX(B)=ON THEN       /* TO BE TRANSFERED              */
         LR    @10,B                                               0252
         MH    @10,@CH01826                                        0252
         L     @07,CCAICMOD(,CCAPTR)                               0252
         ST    @10,@TF00001                                        0252
         ALR   @10,@07                                             0252
         AL    @10,@CF01837                                        0252
         TM    ICTMTRX-10(@10),B'00010000'                         0252
         BNO   @RF00252                                            0252
*         DO;                       /*                               */
*           SELSW=OFF;              /* INIT ELEMENT SELECT SW        */
*                                                                  0254
         NI    SELSW,B'10111111'                                   0254
*           /*********************************************************/
*           /*                                                       */
*           /* CHECK THIS ELEMENTS RELFILE. IF FROM ONE BEING        */
*           /* PROCESSED IN THIS PASS THEN BUILD STATEMENT FOR IT. IF*/
*           /* SELSW IS SET ON THEN THE ELEMENT GETS SELECTED.       */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0255
*           CALL NAMECMPR(ADDR(ICTMNAME(B)));/*                      */
         L     @10,@TF00001                                        0255
         AL    @07,@CF01839                                        0255
         LA    @10,ICTMNAME(@10,@07)                               0255
         ST    @10,@AFTEMPS                                        0255
         LA    @01,@AL00255                                        0255
         BAL   @14,NAMECMPR                                        0255
*           IF SELSW=ON THEN        /* SAME DATASET                  */
         TM    SELSW,B'01000000'                                   0256
         BNO   @RF00256                                            0256
*             DO;                   /*                               */
*               CALL STATEMNT(ADDR(ICTMNAME(B)));/* WRITE STMTS      */
         LR    @10,B                                               0258
         MH    @10,@CH01826                                        0258
         L     @02,CCAICMOD(,CCAPTR)                               0258
         AL    @02,@CF01839                                        0258
         LA    @10,ICTMNAME(@10,@02)                               0258
         ST    @10,@AFTEMPS                                        0258
         LA    @01,@AL00258                                        0258
         BAL   @14,STATEMNT                                        0258
*               FIRSTSW=OFF;        /* FIRST HAS BEEN PROCESSED      */
*                                                                  0259
         NI    FIRSTSW,B'01111111'                                 0259
*               /*****************************************************/
*               /*                                                   */
*               /* WRITE SELECT STMSTS FOR ALIAS NAMES IF ANY        */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0260
*               CALL ALIAS(ICTMTCHN(B));/* PTR TO ALIAS CHAIN IS PARM*/
         LR    @10,B                                               0260
         MH    @10,@CH01826                                        0260
         L     @02,CCAICMOD(,CCAPTR)                               0260
         AL    @02,@CF01841                                        0260
         LA    @10,ICTMTCHN-116(@10,@02)                           0260
         ST    @10,@AL00001                                        0260
         LA    @01,@AL00001                                        0260
         BAL   @14,ALIAS                                           0260
*               ICTMTRX(B)=OFF;     /* THIS ELEMENT PROCESSED        */
         LR    @10,B                                               0261
         MH    @10,@CH01826                                        0261
         L     @07,CCAICMOD(,CCAPTR)                               0261
         ALR   @07,@10                                             0261
         AL    @07,@CF01837                                        0261
         NI    ICTMTRX-10(@07),B'11101111'                         0261
*               DOCOPY=ON;          /* INDICATE ELEMENTS TO COPY     */
         OI    DOCOPY,B'00100000'                                  0262
*               SAVENDX=B;          /* FOR ELEMENT REFERENCE         */
         LR    SAVENDX,B                                           0263
*             END;                  /*                               */
*         END;                      /* ELIGIBLE MOD DO               */
@RF00256 DS    0H                                                  0266
*     END;                          /* ICT MOD LOOP                  */
@RF00252 AH    B,@CH00089                                          0266
@DE00251 LR    @10,B                                               0266
         MH    @10,@CH01826                                        0266
         L     @07,CCAICMOD(,CCAPTR)                               0266
         AL    @07,@CF01839                                        0266
         L     @10,ICTMEND(@10,@07)                                0266
         CL    @10,ICTENDNT                                        0266
         BE    @DC00251                                            0266
         CH    HIGHCIL,@CH00254                                    0266
         BE    @DL00251                                            0266
@DC00251 DS    0H                                                  0267
*                                                                  0267
*     /***************************************************************/
*     /*                                                             */
*     /* ALL POSSIBLE ELEMETS FROM A PARTICULAR RELFILE DATA SET HAVE*/
*     /* HAD THEIR COPY STATEMENTS PREPARED                          */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0267
*     IF DOCOPY=ON                  /* MUST DO A COPY                */
*         &HIGHCIL=ZERO THEN        /*                               */
         TM    DOCOPY,B'00100000'                                  0267
         BNO   @RF00267                                            0267
         CH    HIGHCIL,@CH00254                                    0267
         BNE   @RF00267                                            0267
*       DO;                         /*                               */
*                                                                  0268
*         /***********************************************************/
*         /*                                                         */
*         /* INTERFACE WITH IEBCOPY. SYSMOD NUMBER FOR MESSAGE       */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0269
*         CALL COPYLINK(ICTPPTR(SAVENDX));/* COPY INTERFACE          */
*                                                                  0269
         LR    @10,SAVENDX                                         0269
         MH    @10,@CH01826                                        0269
         L     @07,CCAICMOD(,CCAPTR)                               0269
         AL    @07,@CF01843                                        0269
         LA    @10,ICTPPTR-96(@10,@07)                             0269
         ST    @10,@AL00001                                        0269
         LA    @01,@AL00001                                        0269
         BAL   @14,COPYLINK                                        0269
*         /***********************************************************/
*         /*                                                         */
*         /* IF ERRORS THEN DO COMPLETION PROCESSING                 */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0270
*         IF HIGHCIL^=ZERO THEN     /*                               */
         CH    HIGHCIL,@CH00254                                    0270
         BE    @RF00270                                            0270
*           DO;                     /*                               */
*             ICTMPROC(SAVENDX)=ON; /* PROCESS INDICATOR             */
         LR    @10,SAVENDX                                         0272
         MH    @10,@CH01826                                        0272
         L     @07,CCAICMOD(,CCAPTR)                               0272
         ALR   @07,@10                                             0272
         AL    @07,@CF01844                                        0272
         OI    ICTMPROC-8(@07),B'10000000'                         0272
*             CALL HMASMCPL('B');   /* DO COMPLETION                 */
         L     @15,@CV01680                                        0273
         LA    @01,@AL00273                                        0273
         BALR  @14,@15                                             0273
*             RFY                                                  0274
*               RTNCODE RSTD;       /*                               */
*             HIGHCIL=RTNCODE;      /* SET ERROR CODE                */
         LR    HIGHCIL,RTNCODE                                     0275
*             RFY                                                  0276
*               RTNCODE UNRSTD;     /*                               */
*           END;                    /*                               */
*       END;                        /*                               */
@RF00270 DS    0H                                                  0279
*   END;                            /* COPY PROCESS LOOP             */
@RF00267 DS    0H                                                  0279
@DE00246 TM    DOCOPY,B'00100000'                                  0279
         BNO   @DC00246                                            0279
         CH    HIGHCIL,@CH00254                                    0279
         BE    @DL00246                                            0279
@DC00246 DS    0H                                                  0280
*   END CILCOPY;                    /*                               */
@EL00003 DS    0H                                                  0280
@EF00003 DS    0H                                                  0280
@ER00003 LM    @14,@04,@SA00003                                    0280
         LM    @06,@12,@SA00003+28                                 0280
         BR    @14                                                 0280
         EJECT
*                                                                  0281
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINATION IS MADE WHETHER THIS ELEMENT SHOULD BE COPIED IN*/
*   /* THIS PASS: 1. THIS IS THE FIRST ELEMENT ENCOUNTERED IN THIS   */
*   /* PASS 2. THIS IS NOT FIRST ELEMENT BUT HAS SAME RELFILE AS THAT*/
*   /* BEING PROCESSED IN THIS PASS IF THE ELEMENT IS TO BE COPIED   */
*   /* THEN A SWITCH IS SET AND PASSED TO CALLER.                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0281
*NAMECMPR:                          /*                               */
*   PROC(ELEMPTR);                  /*                               */
NAMECMPR STM   @14,@12,@SA00004                                    0281
         MVC   @PC00004(4),0(@01)                                  0281
*   DECLARE                                                        0282
*     ELEMPTR PTR(31);              /* ADDRESS OF ICT MOD PASSED     */
*   RFY                                                            0283
*     ICTMOD BASED(ELEMPTR);        /* ICT MOD ENTRY BASE            */
*   RFY                                                            0284
*     ICTPTF BASED(ICTPPTR(1));     /* POINTS TO SYSMOD ENTRY        */
*                                                                  0284
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THIS IS FIRST ELEMENT FOR THIS PASS THEN SAVE ITS RELFILE  */
*   /* DATASET NAME.                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0285
*   IF FIRSTSW=ON THEN              /*                               */
         TM    FIRSTSW,B'10000000'                                 0285
         BNO   @RF00285                                            0285
*     DO;                           /*                               */
*                                                                  0286
*       /*************************************************************/
*       /*                                                           */
*       /* CALL HMASMSUB TO CONSTRUCT NAME                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0287
*       SPLIPFX=ICTPTPFX(1);        /* RELFILE DSN PREFIX            */
         L     @10,SPLPMAD                                         0287
         L     @15,@PC00004                                        0287
         L     @15,ELEMPTR(,@15)                                   0287
         L     @14,ICTPPTR(,@15)                                   0287
         MVC   SPLIPFX(26,@10),ICTPTPFX(@14)                       0287
*       SPLISMD=ICTPTFS(1);         /*                               */
         MVC   SPLISMD(7,@10),ICTPTFS(@14)                         0288
*       SPLIFIL=ICTMRELN(1);        /*                               */
         MVC   SPLIFIL(2,@10),ICTMRELN(@15)                        0289
*       CALL HMASMSUB(HMASMSPL);    /*                               */
         L     @15,@CV01681                                        0290
         LA    @01,@AL00290                                        0290
         BALR  @14,@15                                             0290
*       CILODSN=SPLODSN;            /* SAVE DSN FOR COMPARES         */
         L     @10,SPLPMAD                                         0291
         MVC   CILODSN(44),SPLODSN(@10)                            0291
*     END;                          /*                               */
*                                                                  0292
*   /*****************************************************************/
*   /*                                                               */
*   /* COMPARE THE RELFILE DSNAME OF ELEMENT BEING PROCESSED TO SAVED*/
*   /* RELFILE DSNAME. IF EQUAL THEN THIS ELEMENT WILL BE COPIED.    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0293
*   SPLIPFX=ICTPTPFX(1);            /* RELFILE DSN PREFIX            */
@RF00285 L     @10,SPLPMAD                                         0293
         L     @15,@PC00004                                        0293
         L     @15,ELEMPTR(,@15)                                   0293
         L     @14,ICTPPTR(,@15)                                   0293
         MVC   SPLIPFX(26,@10),ICTPTPFX(@14)                       0293
*   SPLISMD=ICTPTFS(1);             /*                               */
         MVC   SPLISMD(7,@10),ICTPTFS(@14)                         0294
*   SPLIFIL=ICTMRELN(1);            /*                               */
         MVC   SPLIFIL(2,@10),ICTMRELN(@15)                        0295
*   CALL HMASMSUB(HMASMSPL);        /* GET THIS ELEMENTS NAME        */
         L     @15,@CV01681                                        0296
         LA    @01,@AL00296                                        0296
         BALR  @14,@15                                             0296
*   IF SPLODSN=CILODSN THEN         /*                               */
         L     @10,SPLPMAD                                         0297
         CLC   SPLODSN(44,@10),CILODSN                             0297
         BNE   @RF00297                                            0297
*     SELSW=ON;                     /* TELL CALLER TO PROCESS ELEMENT*/
         OI    SELSW,B'01000000'                                   0298
*   END NAMECMPR;                   /*                               */
@EL00004 DS    0H                                                  0299
@EF00004 DS    0H                                                  0299
@ER00004 LM    @14,@12,@SA00004                                    0299
         BR    @14                                                 0299
         EJECT
*                                                                  0300
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS PROCEDURE SETS UP THE SELECT MEMBER= STATEMENT FOR MODULE*/
*   /* AND LOAD MODULE WITH IDENTICAL NAMES.                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0300
*STATEMNT:                                                         0300
*   PROC(ADRMOD);                   /*                               */
STATEMNT STM   @14,@04,@SA00005                                    0300
         STM   @06,@12,@SA00005+28                                 0300
         MVC   @PC00005(4),0(@01)                                  0300
*   DECLARE                                                        0301
*     ADRMOD PTR(31);               /* ELEMENT NAME TO SELECT        */
*   DECLARE                                                        0302
*     SELMOD CHAR(8);               /* SAVE AREA FOR ELEMENT NAME    */
*   RFY                                                            0303
*     ICTMOD BASED(ADRMOD);         /*                               */
*   RFY                                                            0304
*     ICTPTF BASED(ICTPPTR(1));     /*                               */
*                                                                  0304
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THIS IS THE FIRST ELEMENT TO BE PROCESSED FROM A RELFILE   */
*   /* DATA SET THEN A IEBCOPY COPY STATEMENT MUST BE PREPARED.      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0305
*   IF FIRSTSW=ON THEN              /*                               */
         TM    FIRSTSW,B'10000000'                                 0305
         BNO   @RF00305                                            0305
*     DO;                           /*                               */
*       BUFFER=TLIBCPY;             /*                               */
         L     @10,IOPBUFAD(,IOPPTR)                               0307
         MVC   BUFFER(80,@10),TLIBCPY                              0307
*       IOPDSID=IOPSCR1;            /* SET DATA SET ID               */
         MVI   IOPDSID(IOPPTR),X'03'                               0308
*       IOPFUNCT=IOPWRITE;          /* SET I/O FUNCTION FOR WRITE    */
         MVI   IOPFUNCT(IOPPTR),X'51'                              0309
*       CALL HMASMIO(HMASMIOP);     /* CALL I/O ROUTINE              */
         ST    IOPPTR,@AL00001                                     0310
         L     @15,@CV01679                                        0310
         LA    @01,@AL00001                                        0310
         BALR  @14,@15                                             0310
*       HIGHCIL=MAX(HIGHCIL,IOPRETRN);/*                             */
         SLR   @10,@10                                             0311
         IC    @10,IOPRETRN(,IOPPTR)                               0311
         CR    HIGHCIL,@10                                         0311
         BNL   *+6
         LR    HIGHCIL,@10                                         0311
*     END;                          /*                               */
*                                                                  0312
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW BUILD SELECT STATEMENTS FOR THE ELEMTENT.                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0313
*   SELMOD=ICTMNAME(1);             /* EASIER TO WORK WITR           */
@RF00305 L     @10,@PC00005                                        0313
         L     @10,ADRMOD(,@10)                                    0313
         MVC   SELMOD(8),ICTMNAME(@10)                             0313
*   BUFFER=BLANK;                   /* CLEAR BUFFER AREA             */
         L     @10,IOPBUFAD(,IOPPTR)                               0314
         MVI   BUFFER+1(@10),C' '                                  0314
         MVC   BUFFER+2(78,@10),BUFFER+1(@10)                      0314
         MVI   BUFFER(@10),C' '                                    0314
*   BUFFER(L2:L17)=SELECT;          /* MOVE SELECT VERB              */
         MVC   BUFFER+1(16,@10),SELECT                             0315
*   LX=L8;                          /* SET LENGTH FIELD              */
         LA    LX,8                                                0316
*CHECK:                                                            0317
*   IF SELMOD(LX)=BLANK             /* LAST POSITION BLANK           */
*     THEN                                                         0317
CHECK    LA    @10,SELMOD-1(LX)                                    0317
         CLI   0(@10),C' '                                         0317
         BNE   @RF00317                                            0317
*     DO;                                                          0318
*       LX=LX-L1;                   /* CK NEXT POSITION              */
         BCTR  LX,0                                                0319
*       GOTO CHECK;                 /* BRANCH BACK TO TEST           */
         B     CHECK                                               0320
*     END;                                                         0321
*   LY=L17+LX;                      /* CALC DISP IN BUFFER           */
@RF00317 LA    LY,17                                               0322
         ALR   LY,LX                                               0322
*   BUFFER(L18:LY)=SELMOD;          /* MOVE ELEMENT NAME INTO STMT   */
         L     @10,IOPBUFAD(,IOPPTR)                               0323
         LR    @07,LY                                              0323
         SH    @07,@CH00451                                        0323
         EX    @07,@SM01846                                        0323
*   LZ=LY+L5;                       /* CALC PLACE TO MOVE FIELDS     */
         LA    LZ,5                                                0324
         ALR   LZ,LY                                               0324
*   LY=LY+L1;                       /* CALC PLACE TO MOVE FIELDS     */
         AH    LY,@CH01686                                         0325
*   BUFFER(LY:LZ)=REPLACE;          /* MOVE IN SELECT PARM FIELDS    */
         LR    @07,@10                                             0326
         ALR   @07,LY                                              0326
         BCTR  @07,0                                               0326
         LR    @02,LZ                                              0326
         SLR   @02,LY                                              0326
         EX    @02,@SM01848                                        0326
*   IF ALIASW=OFF THEN              /* IF NOT ALIAS NAME             */
         TM    ALIASW,B'00010000'                                  0327
         BNZ   @RF00327                                            0327
*     BUFFER(41:47)=ICTPTFS(1);     /* SYSMOD NAME INTO STMT         */
         L     @04,@PC00005                                        0328
         L     @04,ADRMOD(,@04)                                    0328
         L     @04,ICTPPTR(,@04)                                   0328
         MVC   BUFFER+40(7,@10),ICTPTFS(@04)                       0328
*   IOPDSID=IOPSCR1;                /* SET DATA SET ID               */
@RF00327 MVI   IOPDSID(IOPPTR),X'03'                               0329
*   IOPFUNCT=IOPWRITE;              /* SET I/O FUNCTION FOR WRITE    */
         MVI   IOPFUNCT(IOPPTR),X'51'                              0330
*   CALL HMASMIO(HMASMIOP);         /* CALL I/O ROUTINE              */
         ST    IOPPTR,@AL00001                                     0331
         L     @15,@CV01679                                        0331
         LA    @01,@AL00001                                        0331
         BALR  @14,@15                                             0331
*   HIGHCIL=MAX(HIGHCIL,IOPRETRN);  /*                               */
         SLR   @10,@10                                             0332
         IC    @10,IOPRETRN(,IOPPTR)                               0332
         CR    HIGHCIL,@10                                         0332
         BNL   *+6
         LR    HIGHCIL,@10                                         0332
*   END STATEMNT;                                                  0333
@EL00005 DS    0H                                                  0333
@EF00005 DS    0H                                                  0333
@ER00005 LM    @14,@04,@SA00005                                    0333
         LM    @06,@12,@SA00005+28                                 0333
         BR    @14                                                 0333
         EJECT
*                                                                  0334
*   /*****************************************************************/
*   /*                                                               */
*   /* THE POINTER TO THE ELEMLENTS ALIAS LIST IS PASSED             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0334
*ALIAS:                                                            0334
*   PROCEDURE(ALIASCHN);                                           0334
ALIAS    STM   @14,@04,@SA00006                                    0334
         STM   @06,@12,@SA00006+28                                 0334
         MVC   @PC00006(4),0(@01)                                  0334
*   DECLARE                                                        0335
*     ALIASCHN PTR(31);             /* POINTER TO ALIAS LIST         */
*   DECLARE                                                        0336
*     AL FIXED(31);                 /* LOOP COUNTER                  */
*   RFY                                                            0337
*     ICTTALLS BASED(ALIASCHN);     /* BASE OF ALIAS LIST            */
*                                                                  0337
*   /*****************************************************************/
*   /*                                                               */
*   /* GO THRU THE LIST OF ALIAS NAMES AND BUILD IEBCOPY STATEMENTS  */
*   /* FOR EACH                                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0338
*   ALIASW=ON;                      /* FOR USE BY CALLED ROUTINE     */
         OI    ALIASW,B'00010000'                                  0338
*   DO AL=1 BY 1 WHILE(ICTTALND(AL)^=ICTENDIX/*                      */
*         &HIGHCIL=ZERO);           /*                               */
*                                                                  0339
         LA    AL,1                                                0339
         B     @DE00339                                            0339
@DL00339 DS    0H                                                  0340
*     /***************************************************************/
*     /*                                                             */
*     /* CALL ROUTINE TO BUILD THE CONTROL STATEMENT. INDICATE NAME  */
*     /* AND PASS IT ON .                                            */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0340
*     CALL STATEMNT(ADDR(ICTTALNM(AL)));/*                           */
         LR    @10,AL                                              0340
         SLA   @10,3                                               0340
         L     @02,@PC00006                                        0340
         L     @02,ALIASCHN(,@02)                                  0340
         AL    @02,@CF01850                                        0340
         LA    @10,ICTTALNM(@10,@02)                               0340
         ST    @10,@AFTEMPS+4                                      0340
         LA    @01,@AL00340                                        0340
         BAL   @14,STATEMNT                                        0340
*   END;                            /*                               */
         AH    AL,@CH00089                                         0341
@DE00339 LR    @10,AL                                              0341
         SLA   @10,3                                               0341
         L     @02,@PC00006                                        0341
         L     @02,ALIASCHN(,@02)                                  0341
         AL    @02,@CF01850                                        0341
         L     @10,ICTTALND(@10,@02)                               0341
         CL    @10,ICTENDIX                                        0341
         BE    @DC00339                                            0341
         CH    HIGHCIL,@CH00254                                    0341
         BE    @DL00339                                            0341
@DC00339 DS    0H                                                  0342
*   ALIASW=OFF;                     /*                               */
         NI    ALIASW,B'11101111'                                  0342
*   END ALIAS;                                                     0343
@EL00006 DS    0H                                                  0343
@EF00006 DS    0H                                                  0343
@ER00006 LM    @14,@04,@SA00006                                    0343
         LM    @06,@12,@SA00006+28                                 0343
         BR    @14                                                 0343
         EJECT
*                                                                  0344
*   /*****************************************************************/
*   /*                                                               */
*   /* IEBCOPY PARAMETERS ARE SET UP AND A LINK IS GENERATED         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0344
*COPYLINK:                                                         0344
*   PROC(PPTR);                     /*                               */
COPYLINK STM   @14,@04,@SA00007                                    0344
         STM   @06,@12,@SA00007+28                                 0344
         MVC   @PC00007(4),0(@01)                                  0344
*   DECLARE                                                        0345
*     P FIXED(8);                   /* LOOP COUNTER                  */
*   DECLARE                                                        0346
*     PPTR PTR(31);                 /* PTR TO SYSMOD NUMBER          */
*   RFY                                                            0347
*     ICTPTF BASED(PPTR);           /*                               */
*   RFY                                                            0348
*     HMASMIOP BASED(IOPPTR);       /*                               */
*                                                                  0348
*   /*****************************************************************/
*   /*                                                               */
*   /* THE JFCB OF THE SMPTLIB DATA SET IS SET UP FOR THE DSN OF THE */
*   /* RELFILE DATA SET BEING PROCESSED.                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0349
*   IOPFUNCT=IOPALC;                /*                               */
         MVI   IOPFUNCT(IOPPTR),X'0D'                              0349
*   IOPDSID=IOPLRF;                 /*                               */
         MVI   IOPDSID(IOPPTR),X'20'                               0350
*   IOPDSN=CILODSN;                 /* FULL DSN OF RELFILE           */
         MVC   IOPDSN(44,IOPPTR),CILODSN                           0351
*   IOPSPACE=''B;                   /* INDICATES OLD DATA SET        */
         XC    IOPSPACE(6,IOPPTR),IOPSPACE(IOPPTR)                 0352
*   CALL HMASMIO(HMASMIOP);         /*                               */
         ST    IOPPTR,@AL00001                                     0353
         L     @15,@CV01679                                        0353
         LA    @01,@AL00001                                        0353
         BALR  @14,@15                                             0353
*   HIGHCIL=MAX(HIGHCIL,IOPRETRN);  /*                               */
         SLR   @10,@10                                             0354
         IC    @10,IOPRETRN(,IOPPTR)                               0354
         CR    HIGHCIL,@10                                         0354
         BNL   *+6
         LR    HIGHCIL,@10                                         0354
*   IF IOPRETRN=IOPGOOD             /* IF ALLOCATE DONE OK           */
*     THEN                          /* THEN SET SW FOR               */
         CLI   IOPRETRN(IOPPTR),0                                  0355
         BNE   @RF00355                                            0355
*     ALCDONE=ON;                   /* FREE REQUIRED                 */
         OI    ALCDONE,B'00001000'                                 0356
*   ELSE                            /*                               */
*     ALCDONE=OFF;                  /*                               */
*                                                                  0357
         B     @RC00355                                            0357
@RF00355 NI    ALCDONE,B'11110111'                                 0357
*   /*****************************************************************/
*   /*                                                               */
*   /* CLOSE SYSUT1 WHICH CONTAINS THE CONTROL STATEMENTS            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0358
*   IOPDSID=IOPSCR1;                /*                               */
@RC00355 MVI   IOPDSID(IOPPTR),X'03'                               0358
*   IOPFUNCT=IOPCLOSE;              /* SET TO DO CLOSE               */
         MVI   IOPFUNCT(IOPPTR),X'50'                              0359
*   CALL HMASMIO(HMASMIOP);         /* CALL I/O ROUTINE              */
         ST    IOPPTR,@AL00001                                     0360
         L     @15,@CV01679                                        0360
         LA    @01,@AL00001                                        0360
         BALR  @14,@15                                             0360
*   HIGHCIL=MAX(HIGHCIL,IOPRETRN);  /* SET ERROR CODE                */
*                                                                  0361
         SLR   @10,@10                                             0361
         IC    @10,IOPRETRN(,IOPPTR)                               0361
         CR    HIGHCIL,@10                                         0361
         BNL   *+6
         LR    HIGHCIL,@10                                         0361
*   /*****************************************************************/
*   /*                                                               */
*   /* GET IEBCOPY PARMS FROM THE CCA                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0362
*   IF HIGHCIL=ZERO THEN            /*                               */
         CH    HIGHCIL,@CH00254                                    0362
         BNE   @RF00362                                            0362
*     DO;                           /*                               */
*       SYSPRINT=CCACPYSP;          /* SYSPRINT DD NAME              */
         L     @10,CCABLDLP(,CCAPTR)                               0364
         MVC   SYSPRINT(8),CCACPYSP(@10)                           0364
*       DO P=1 BY 1 WHILE CCACPYPM(P)^=BLANK;/* MOVE LIST            */
         LA    P,1                                                 0365
         B     @DE00365                                            0365
@DL00365 DS    0H                                                  0366
*         OPTVAR(P)=CCACPYPM(P);    /*                               */
         LA    @10,OPTVAR-1(P)                                     0366
         L     @02,CCABLDLP(,CCAPTR)                               0366
         ALR   @02,P                                               0366
         MVC   0(1,@10),CCACPYPM-1(@02)                            0366
*       END;                        /*                               */
         AH    P,@CH00089                                          0367
@DE00365 L     @10,CCABLDLP(,CCAPTR)                               0367
         LR    @02,@10                                             0367
         ALR   @02,P                                               0367
         CLI   CCACPYPM-1(@02),C' '                                0367
         BNE   @DL00365                                            0367
*       OPTLGTH=P-1;                /* LENGTH OF PARM LIST           */
*                                                                  0368
         LR    @02,P                                               0368
         BCTR  @02,0                                               0368
         STH   @02,OPTLGTH                                         0368
*       /*************************************************************/
*       /*                                                           */
*       /* STAE PARM SETUP                                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0369
*       SAVRVANM=RVAEPRNM;          /* SAVE RVA PROC NAME            */
         L     @04,CCARVAD(,CCAPTR)                                0369
         MVC   SAVRVANM(8),RVAEPRNM(@04)                           0369
*       RVAEPRNM=CCACPYNM;          /* IEBCOPY NMAE                  */
*                                                                  0370
         MVC   RVAEPRNM(8,@04),CCACPYNM(@10)                       0370
*       /*************************************************************/
*       /*                                                           */
*       /* LINK TO IEBCOPY                                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0371
*       RFY                                                        0371
*         LINKREG RSTD;                                            0371
*       LINKREG=CCACOPY;            /* ADDR OF BLDL ENTRY-IEBCOPY    */
         L     LINKREG,CCACOPY(,@10)                               0372
*       GENERATE;                                                  0373
        LINK DE=(LINKREG),PARAM=(OPTLST,DDNAMES),VL=1 EXEC IEBCOPY  */
*       RFY                                                        0374
*         RTNCODE RSTD;             /*                               */
*       RFY                                                        0375
*         LINKREG UNRSTD;                                          0375
*       RVAEPRNM=SAVRVANM;          /* RESTORE RVA PROC NAME         */
         L     @14,CCARVAD(,CCAPTR)                                0376
         MVC   RVAEPRNM(8,@14),SAVRVANM                            0376
*       IF RTNCODE>CCARCOPY THEN    /* CHECK FOR COPY ERRORS         */
         L     @14,CCABLDLP(,CCAPTR)                               0377
         SLR   @10,@10                                             0377
         IC    @10,CCARCOPY(,@14)                                  0377
         CR    RTNCODE,@10                                         0377
         BNH   @RF00377                                            0377
*         DO;                       /*                               */
*           HIGHCIL=MAX(HIGHCIL,STOPPTF);/*                          */
*                                                                  0379
         LA    @14,8                                               0379
         CR    HIGHCIL,@14                                         0379
         BNL   *+6
         LR    HIGHCIL,@14                                         0379
*           /*********************************************************/
*           /*                                                       */
*           /* ISSUE ERROR MESSAGE                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0380
*           CVD(RTNCODE,SAVE1);     /* TO DECIMAL                    */
         CVD   RTNCODE,SAVE1                                       0380
*           UNPK(SAVE2,SAVE1);      /* UNPACK                        */
         UNPK  SAVE2(8),SAVE1(8)                                   0381
*           SIGN=MASK;              /* ELIMINATE SIGN                */
         OI    SIGN,B'11110000'                                    0382
*           DO;                     /* HMASMPMG M1(HMA376I)TYPE(ERR)T
*                                      ARGET(SYSOUT,LOG)VAR(ICTPTFS(1
*                                      ),LINKRTN)                    */
*             HMASMMGP=HMASMMGP&&HMASMMGP;/* SET PARMS TO ZEROS      */
         XC    HMASMMGP(24),HMASMMGP                               0384
*             MGPMGNO1=HMA376I;     /* SET UP MSG 1 CODE             */
         SLR   @14,@14                                             0385
         IC    @14,HMA376I                                         0385
         STH   @14,MGPMGNO1                                        0385
*             MGPTYPE=MGPERR;       /* SET MESSAGE TYPE              */
         MVI   MGPTYPE,X'03'                                       0386
*             MGPPRINT='1'B;        /* SET TARGET FLAG               */
*             MGPHLDS='1'B;         /* SET TARGET FLAG               */
         OI    MGPPRINT,B'10010000'                                0388
*             MGPVARPT(1)=ADDR(ICTPTFS(1));/* SET VARIABLE PTR       */
         L     @14,@PC00007                                        0389
         L     @14,PPTR(,@14)                                      0389
         ST    @14,MGPVARPT                                        0389
*             MGPVARPT(2)=ADDR(LINKRTN);/* SET VARIABLE PTR          */
         LA    @14,LINKRTN                                         0390
         ST    @14,MGPVARPT+4                                      0390
*             CALL HMASMMSG(HMASMMGP);/* HAVE MESSAGE ISSUED         */
         L     @15,@CV01678                                        0391
         LA    @01,@AL00391                                        0391
         BALR  @14,@15                                             0391
*           END;                    /*                               */
*           ICTPNOGO(1)=ON;         /*                               */
         L     @14,@PC00007                                        0393
         L     @14,PPTR(,@14)                                      0393
         OI    ICTPNOGO(@14),B'01000000'                           0393
*         END;                      /*                               */
*     END;                          /*                               */
@RF00377 DS    0H                                                  0396
*   IF ALCDONE=ON                   /* IF RELFILE ALLOCTED           */
*     THEN                          /* THE FREE IT NOW               */
@RF00362 TM    ALCDONE,B'00001000'                                 0396
         BNO   @RF00396                                            0396
*     DO;                           /*                               */
*       IOPFUNCT=IOPDALC;           /*                               */
         MVI   IOPFUNCT(IOPPTR),X'0E'                              0398
