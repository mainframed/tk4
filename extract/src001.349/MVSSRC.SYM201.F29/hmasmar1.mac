         CH    INDX,@CH00057                                       0494
         BE    @RT00494                                            0494
         CH    INDX,@CH00248                                       0494
         BE    @RT00494                                            0494
         CH    INDX,@CH00059                                       0494
         BNE   @RF00494                                            0494
