*   DCL                                                            0514
*     1 RSTC CHAR(24),              /* RESTORE CHECK HEADING INSERT  */
*       3 * CHAR(8) INIT('RESTORE '),/*                              */
*       3 * CHAR(6) INIT('CHECK '), /*                               */
*       3 * CHAR(10) INIT('PROCESSING');/*                           */
*   DCL                                                            0515
*     1 RST CHAR(24),               /* RESTORE HEADING INSERT        */
*       3 * CHAR(8) INIT('RESTORE '),/*                              */
*       3 * CHAR(11) INIT('PROCESSING '),/*                          */
*       3 *(5) CHAR(1) INIT((5)' ');/*                               */
*                                                                  0515
*   /*****************************************************************/
*   /*                                                               */
*   /* START OF HDINSERT CODE                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0516
*   IF CCAAPPLY=ON                  /* IF APPLY IN PROGRESS          */
*     THEN                                                         0516
         TM    CCAAPPLY(CCAPTR),B'01000000'                        0516
         BNO   @RF00516                                            0516
*     DO;                           /*                       @Z67SP00*/
