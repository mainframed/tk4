*           DO;                     /* PASS HEADING ADDRESS          */
         B     @RC00494                                            0499
@RF00494 DS    0H                                                  0500
*             PRLBLANK=OFF;         /* NOT A BLANK LINE              */
         L     @12,IOPPTR                                          0500
         L     @12,IOPBUFAD(,@12)                                  0500
         NI    PRLBLANK(@12),B'11011111'                           0500
