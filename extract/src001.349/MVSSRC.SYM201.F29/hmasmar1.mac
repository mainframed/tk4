         LR    @08,INDX                                            0501
         SLA   @08,2                                               0501
         L     @08,HEADPTR-4(@08)                                  0501
         STCM  @08,7,PRLHDADR(@12)                                 0501
*           END;                    /*                               */
*         CALL HMASMIO(HMASMIOP);   /* DO IO                         */
@RC00494 L     @12,IOPPTR                                          0503
         ST    @12,@AL00001                                        0503
         L     @15,@CV01837                                        0503
         LA    @01,@AL00001                                        0503
         BALR  @14,@15                                             0503
*         HEADRC=IOPRETRN;          /* SAVE RETURN CODE              */
         L     @12,IOPPTR                                          0504
         SLR   HEADRC,HEADRC                                       0504
         IC    HEADRC,IOPRETRN(,@12)                               0504
