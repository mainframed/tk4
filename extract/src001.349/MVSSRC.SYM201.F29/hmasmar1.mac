         L     @12,IOPPTR                                          0496
         L     @12,IOPBUFAD(,@12)                                  0496
         OI    PRLBLANK(@12),B'00100000'                           0496
*             PRLHDADR=ZERO;        /* CLEAR HEADING ADDRESS         */
         SLR   @08,@08                                             0497
         STCM  @08,7,PRLHDADR(@12)                                 0497
