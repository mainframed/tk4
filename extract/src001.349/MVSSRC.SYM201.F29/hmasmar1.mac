         TITLE 'HMASMAR1 - SYSMOD STATUS REPORT GENERATOR'
HMASMAR1 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMAR1  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @09,0                                               0001
@PSTART  LA    @10,4095(,@09)                                      0001
         USING @PSTART,@09                                         0001
         USING @PSTART+4095,@10                                    0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*                                                                  0216
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT OF CODE WILL OUTPUT THE SYSMOD STATUS REPORT. THE*/
*   /* REPORT IS CREATED IN TWO PHASES. THE FIRST PHASE WILL EXTRACT */
*   /* THE DATA FROM THE PTF SECTION OF THE ICT AND SAVE THE         */
*   /* EXTRACTED DATA IN THE GTA FILE. THE DATA MUST BE SAVED IN     */
*   /* ORDER TO OUTPUT THE SYSMOD INFORMATION IN ALPHANUMERICAL      */
*   /* SEQUENCE. THE SECOND PHASE WILL SEQUENTIALLY RETRIEVE GTA FILE*/
*   /* RECORDS. EACH RECORD WILL BE FORMATTED AND PRINTED            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0216
*                                                                  0216
*   /*****************************************************************/
*   /*                                                               */
*   /* START OF HMASMAR1 CODE                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0216
*   AR1RTNCD=ZERO;                  /* INITIALIZE RETURN CODE        */
         SLR   AR1RTNCD,AR1RTNCD                                   0216
*   CALL AR1INIT;                   /* INIT PARM LIST FOR HMASMGTA   */
         BAL   @14,AR1INIT                                         0217
*   CALL AR1TBLD;                   /* PHASE ONE-EXTRACT AND SEQUENCE
*                                      DATA                          */
         BAL   @14,AR1TBLD                                         0218
*   RFY                                                            0219
*     RTNCODE RSTD;                 /*                               */
*   AR1RTNCD=RTNCODE;               /* SAVE RETURN CODE              */
         LR    AR1RTNCD,RTNCODE                                    0220
*   RFY                                                            0221
*     RTNCODE UNRSTD;               /*                               */
*   IF AR1RTNCD=ZERO                /* IF PHASE ONE SUCCESSFUL       */
*     THEN                                                         0222
         LTR   AR1RTNCD,AR1RTNCD                                   0222
         BNZ   @RF00222                                            0222
*     DO;                           /* THEN DO                       */
*       CALL AR1RPT;                /* PHASE TWO-RETRIEVE DATA,FORMAT
*                                      AND PRINT                     */
         BAL   @14,AR1RPT                                          0224
*       RFY                                                        0225
*         RTNCODE RSTD;             /*                               */
*       AR1RTNCD=RTNCODE;           /* SAVE RETURN CODE              */
         LR    AR1RTNCD,RTNCODE                                    0226
*       RFY                                                        0227
*         RTNCODE UNRSTD;           /*                               */
*     END;                          /*                               */
*   CALL AR1CLEAN;                  /* FREE GOTTEN STORAGE AND RESET
*                                      HEADERS                       */
@RF00222 BAL   @14,AR1CLEAN                                        0229
*   RFY                                                            0230
*     RTNCODE RSTD;                 /*                               */
*   AR1RTNCD=MAX(AR1RTNCD,RTNCODE); /* SAVE MAXIMUM RETURN CODE      */
         CR    AR1RTNCD,RTNCODE                                    0231
         BNL   *+6
         LR    AR1RTNCD,RTNCODE                                    0231
*   RFY                                                            0232
*     RTNCODE UNRSTD;               /*                               */
*   RETURN CODE(AR1RTNCD);          /* RETURN TO CALLER              */
         LR    @15,AR1RTNCD                                        0233
         L     @13,4(,@13)                                         0233
         L     @14,12(,@13)                                        0233
         LM    @00,@12,20(@13)                                     0233
         BR    @14                                                 0233
         EJECT
*AR1INIT:                                                          0234
*   PROC;                           /*                               */
*                                                                  0234
AR1INIT  STM   @14,@12,12(@13)                                     0234
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT OF CODE INITIALIZES THE PARAMETER LIST PASSED TO */
*   /* HMASMGTA TO STORE AND SEQUENCE EXTRACTED SYSMOD DATA          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0235
*   HMASMGTP=''B;                   /* ZERO OUT THE GTP      @Z67SP00*/
         XC    HMASMGTP(28),HMASMGTP                               0235
*   GTPKEYLN=LENGTH(GTAKEYLN);      /* GTA KEY LENGTH (15)   @Z67SP00*/
         MVC   GTPKEYLN(2),@CH00117                                0236
*   GTPRCDLN=LENGTH(PTFBUFF);       /* SET GTA FILE RECORD LENGTH    */
         MVC   GTPRCDLN(2),@CH00492                                0237
*   GTPBUFAD=ADDR(PTFBUFF);         /* INIT PTR TO GTA FILE RECORD 0238
*                                      BUFFER                        */
         LA    @12,PTFBUFF                                         0238
         ST    @12,GTPBUFAD                                        0238
*   GTPWKAD=ZERO;                   /* SIGNAL NEW TABLE TO BE BUILT  */
         SLR   @12,@12                                             0239
         ST    @12,GTPWKAD                                         0239
*   GTPPSIZE=GTP512B;               /* SET PAGE SIZE TO 512  @Z67SP00*/
         NI    GTPPSIZE,B'11111001'                                0240
         OI    GTPPSIZE,B'00000001'                                0240
*   GTPFUNCT=GTPADD;                /* INIT FUNCTION TO ADD RECORDS  */
         MVI   GTPFUNCT,X'04'                                      0241
*   RETURN;                         /* RETURN TO CALLER              */
@EL00002 DS    0H                                                  0242
@EF00002 DS    0H                                                  0242
@ER00002 LM    @14,@12,12(@13)                                     0242
         BR    @14                                                 0242
*   END AR1INIT;                    /*                               */
         EJECT
*AR1TBLD:                                                          0244
*   PROC;                           /*                               */
*                                                                  0244
AR1TBLD  STM   @14,@12,@SA00003                                    0244
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT OF CODE WILL EXTRACT SYSMOD DATA TO BE REPORTED. */
*   /* FOR EACH SYSMOD IN THE PTF SECTION OF THE ICT, A MAJOR SYSMOD */
*   /* RECORD WILL BE CONSTUCTED AND STORED IN THE GTA FILE. IN      */
*   /* ADDITION, A GTA FILE RECORD WILL BE CONSTRUCTED AND STORED FOR*/
*   /* SYSMODS IN THE PTF IFREQ, REQ AND PRE LISTS FOR SYSMODS THAT  */
*   /* ARE NOT DELETED IN THE PTF SECTION OF THE ICT. STORING THE    */
*   /* RECORDS VIA HMASMGTA( GIVEN THE DESIGN OF THE SYSMOD RECORD   */
*   /* KEY ) WILL SEQUENCE THE SYSMODS IN THE ICT IN ALPHANUMERICAL  */
*   /* ORDER.                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0245
*                                                                  0245
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCAL DECLARES                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0245
*   DCL                                                            0245
*     TBLDRC FIXED(31);             /* INTERNAL RETURN CODE          */
*   RFY                                                            0246
*     ICTPTF BASED(CCAICPTF);       /* ADDRESS PTF SECTION OF ICT    */
*                                                                  0246
*   /*****************************************************************/
*   /*                                                               */
*   /* START OF AR1TBLD CODE                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0247
*   TBLDRC=ZERO;                    /* INITIALIZE RETURN CODE        */
         SLR   TBLDRC,TBLDRC                                       0247
*   DO PTF=ONE BY ONE               /* LOOP THRU PTF SECTION         */
*         WHILE(ICTPEND(PTF)^=ICTENDNT/* UNTIL END OF SYSMODS        */
*         &TBLDRC=ZERO);            /* OR NO MORE SPACE FOR GTA FILE */
         LA    PTF,1                                               0248
         B     @DE00248                                            0248
@DL00248 DS    0H                                                  0249
*     PTFBUFF=' ';                  /* INIT SYSMOD RECORD BUFFER     */
         MVI   PTFBUFF+1,C' '                                      0249
         MVC   PTFBUFF+2(15),PTFBUFF+1                             0249
         MVI   PTFBUFF,C' '                                        0249
*     IF ICTPGELM(PTF)=OFF          /* NOT ELIMINATED, OR    @Z67SP00*/
*         ×(ICTPGELM(PTF)=ON        /* ELIMINATED AND IN THE @Z67SP00*/
*         &ICTPSLST(PTF)=ON) THEN   /* SELECT LIST THEN--    @Z67SP00*/
         LR    @12,PTF                                             0250
         MH    @12,@CH01955                                        0250
         L     @08,CCAICPTF(,CCAPTR)                               0250
         ST    @12,@TF00001                                        0250
         ALR   @12,@08                                             0250
         AL    @12,@CF01966                                        0250
         TM    ICTPGELM-13(@12),B'00001000'                        0250
         BZ    @RT00250                                            0250
         LR    @12,@08                                             0250
         AL    @12,@TF00001                                        0250
         AL    @12,@CF01966                                        0250
         TM    ICTPGELM-13(@12),B'00001000'                        0250
         BNO   @RF00250                                            0250
         AL    @08,@TF00001                                        0250
         AL    @08,@CF01967                                        0250
         TM    ICTPSLST-8(@08),B'00000010'                         0250
         BNO   @RF00250                                            0250
@RT00250 DS    0H                                                  0251
*       DO;                         /* SAVE FOR REPORT       @Z67SP00*/
*         PTFNUM=ICTPTFS(PTF);      /* INSERT SYSMOD NUM IN BUFFER   */
         LR    @12,PTF                                             0252
         MH    @12,@CH01955                                        0252
         L     @08,CCAICPTF(,CCAPTR)                               0252
         ST    @12,@TF00001                                        0252
         ALR   @12,@08                                             0252
         AL    @12,@CF01968                                        0252
         MVC   PTFNUM(7),ICTPTFS(@12)                              0252
*         PTFDATA=ICTPFMID(PTF);    /* INSERT FMID IN BUFFER @Z67SP00*/
*                                                                  0253
         LR    @12,@08                                             0253
         AL    @12,@TF00001                                        0253
         AL    @12,@CF01969                                        0253
         MVC   PTFDATA(7),ICTPFMID-22(@12)                         0253
*         /***********************************************************/
*         /*                                                         */
*         /* DETERMINE TYPE OF SYSMOD BEING SAVED IN GTA FILE        */
*         /*                                                 @Z67SP00*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0254
*         IF ICTPTYPE(PTF)=IOPPFUNC THEN/* SYSMOD A FUNCTION?      0254
*                                                            @Z67SP00*/
         L     @12,@TF00001                                        0254
         AL    @08,@CF01970                                        0254
         SLR   @03,@03                                             0254
         IC    @03,ICTPTYPE-17(@12,@08)                            0254
         CH    @03,@CH00091                                        0254
         BNE   @RF00254                                            0254
*           PTFTYPE=TYPE00;         /* YES- SHOW FUNCTION    @Z67SP00*/
         MVI   PTFTYPE,X'00'                                       0255
*         ELSE                                                     0256
*           IF ICTPTYPE(PTF)=IOPPPTF THEN/* SYSMOD A PTF?    @Z67SP00*/
         B     @RC00254                                            0256
@RF00254 LR    @12,PTF                                             0256
         MH    @12,@CH01955                                        0256
         L     @08,CCAICPTF(,CCAPTR)                               0256
         AL    @08,@CF01970                                        0256
         SLR   @03,@03                                             0256
         IC    @03,ICTPTYPE-17(@12,@08)                            0256
         CH    @03,@CH00041                                        0256
         BNE   @RF00256                                            0256
*             PTFTYPE=TYPE01;       /* YES- SHOW PTF         @Z67SP00*/
         MVI   PTFTYPE,X'01'                                       0257
*           ELSE                                                   0258
*             IF ICTPTYPE(PTF)=IOPPAPAR THEN/* SYSMOD AN APAR?     0258
*                                                            @Z67SP00*/
         B     @RC00256                                            0258
@RF00256 LR    @12,PTF                                             0258
         MH    @12,@CH01955                                        0258
         L     @08,CCAICPTF(,CCAPTR)                               0258
         AL    @08,@CF01970                                        0258
         SLR   @03,@03                                             0258
         IC    @03,ICTPTYPE-17(@12,@08)                            0258
         CH    @03,@CH00232                                        0258
         BNE   @RF00258                                            0258
*               PTFTYPE=TYPE02;     /* YES- SHOW APAR        @Z67SP00*/
         MVI   PTFTYPE,X'02'                                       0259
*             ELSE                                                 0260
*               IF ICTPTYPE(PTF)=IOPPUSER THEN/* SYSMOD A USERMOD? 0260
*                                                            @Z67SP00*/
         B     @RC00258                                            0260
@RF00258 LR    @12,PTF                                             0260
         MH    @12,@CH01955                                        0260
         L     @08,CCAICPTF(,CCAPTR)                               0260
         AL    @08,@CF01970                                        0260
         SLR   @03,@03                                             0260
         IC    @03,ICTPTYPE-17(@12,@08)                            0260
         CH    @03,@CH00039                                        0260
         BNE   @RF00260                                            0260
*                 PTFTYPE=TYPE03;   /* YES- SHOW USERMOD     @Z67SP00*/
         MVI   PTFTYPE,X'03'                                       0261
*               ELSE                                               0262
*                 PTFTYPE=TYPE04;   /* TYPE UNKNOWN          @Z67SP00*/
*                                                                  0262
         B     @RC00260                                            0262
@RF00260 MVI   PTFTYPE,X'04'                                       0262
*         /***********************************************************/
*         /*                                                         */
*         /* DETERMINE STATUS OF SYSMOD BEING SAVED IN GTA FILE      */
*         /*                                                 @Z67SP00*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0263
*         IF ICTPCPL(PTF)=OFF       /* SYSMOD INCOMPLETE AND @Z67SP00*/
*             &ARLCHECK=OFF THEN    /* NOT A CHECK RUN?      @Z67SP00*/
@RC00260 DS    0H                                                  0263
@RC00258 DS    0H                                                  0263
@RC00256 DS    0H                                                  0263
@RC00254 LR    @12,PTF                                             0263
         MH    @12,@CH01955                                        0263
         L     @08,CCAICPTF(,CCAPTR)                               0263
         ALR   @08,@12                                             0263
         AL    @08,@CF01967                                        0263
         TM    ICTPCPL-8(@08),B'00100000'                          0263
         BNZ   @RF00263                                            0263
         L     @12,@PC00001                                        0263
         TM    ARLCHECK(@12),B'01000000'                           0263
         BNZ   @RF00263                                            0263
*           PTFSTAT=STAT06;         /* YES- SHOW INCOMPLETE  @Z67SP00*/
         MVI   PTFSTAT,X'06'                                       0264
*         ELSE                                                     0265
*           IF(ICTPDEXP(PTF)=ON     /* SYSMOD EXPLICIT DEL,  @Z67SP00*/
*               ×ICTPDIMP(PTF)=ON)  /* SYSMOD IMPLICIT DEL,  @Z67SP00*/
*               &ICTERROR(PTF)=OFF  /* SYSMOD NOT ERROR,     @Z67SP00*/
*               &ICTPNOGO(PTF)=OFF THEN/* SYSMOD NOT NOGO THEN     0265
*                                                            @Z67SP00*/
         B     @RC00263                                            0265
@RF00263 LR    @12,PTF                                             0265
         MH    @12,@CH01955                                        0265
         L     @08,CCAICPTF(,CCAPTR)                               0265
         ST    @12,@TF00001                                        0265
         ALR   @12,@08                                             0265
         AL    @12,@CF01966                                        0265
         TM    ICTPDEXP-13(@12),B'00110000'                        0265
         BZ    @RF00265                                            0265
         LR    @12,@08                                             0265
         AL    @12,@TF00001                                        0265
         AL    @12,@CF01971                                        0265
         TM    ICTERROR-7(@12),B'00010000'                         0265
         BNZ   @RF00265                                            0265
         AL    @08,@TF00001                                        0265
         AL    @08,@CF01967                                        0265
         TM    ICTPNOGO-8(@08),B'01000000'                         0265
         BNZ   @RF00265                                            0265
*             PTFSTAT=STAT03;       /* YES- SHOW DELETED     @Z67SP00*/
         MVI   PTFSTAT,X'03'                                       0266
*           ELSE                                                   0267
*             IF ICTPNOGO(PTF)=ON   /* SYSMOD NOGO?          @Z67SP00*/
*                 &ICTPCDSE(PTF)=ON /* (A)CDS ENTRY BUILT    @Z67SP00*/
*               THEN                                               0267
         B     @RC00265                                            0267
@RF00265 LR    @12,PTF                                             0267
         MH    @12,@CH01955                                        0267
         L     @08,CCAICPTF(,CCAPTR)                               0267
         ST    @12,@TF00001                                        0267
         ALR   @12,@08                                             0267
         AL    @12,@CF01967                                        0267
         TM    ICTPNOGO-8(@12),B'01000000'                         0267
         BNO   @RF00267                                            0267
         AL    @08,@TF00001                                        0267
         AL    @08,@CF01966                                        0267
         TM    ICTPCDSE-13(@08),B'10000000'                        0267
         BNO   @RF00267                                            0267
*               PTFSTAT=STAT02;     /* YES- SHOW ERROR       @Z67SP00*/
         MVI   PTFSTAT,X'02'                                       0268
*             ELSE                                                 0269
*               IF ICTERROR(PTF)=ON THEN/* SYSMOD IN ERROR?  @Z67SP00*/
         B     @RC00267                                            0269
@RF00267 LR    @12,PTF                                             0269
         MH    @12,@CH01955                                        0269
         L     @08,CCAICPTF(,CCAPTR)                               0269
         ALR   @08,@12                                             0269
         AL    @08,@CF01971                                        0269
         TM    ICTERROR-7(@08),B'00010000'                         0269
         BNO   @RF00269                                            0269
*                 PTFSTAT=STAT02;   /* YES- SHOW ERROR       @Z67SP00*/
         MVI   PTFSTAT,X'02'                                       0270
*               ELSE                                               0271
*                 IF ICTPNOGO(PTF)=ON/* SYSMOD NOGO?         @Z67SP00*/
*                     &ICTPCDSE(PTF)=OFF/* NO (A)CDS ENTRY BUILT   0271
*                                                            @Z67SP00*/
*                   THEN                                           0271
         B     @RC00269                                            0271
@RF00269 LR    @12,PTF                                             0271
         MH    @12,@CH01955                                        0271
         L     @08,CCAICPTF(,CCAPTR)                               0271
         ST    @12,@TF00001                                        0271
         ALR   @12,@08                                             0271
         AL    @12,@CF01967                                        0271
         TM    ICTPNOGO-8(@12),B'01000000'                         0271
         BNO   @RF00271                                            0271
         AL    @08,@TF00001                                        0271
         AL    @08,@CF01966                                        0271
         TM    ICTPCDSE-13(@08),B'10000000'                        0271
         BNZ   @RF00271                                            0271
*                   PTFSTAT=STAT01; /* YES- SHOW NOGO        @Z67SP00*/
         MVI   PTFSTAT,X'01'                                       0272
*                 ELSE                                             0273
*                   PTFSTAT=STAT00; /* STATUS IS GO          @Z67SP00*/
         B     @RC00271                                            0273
@RF00271 MVI   PTFSTAT,X'00'                                       0273
*         PTFIND=MAJOR;             /* SYSMOD RECORD TYPE TO MAJOR   */
@RC00271 DS    0H                                                  0274
@RC00269 DS    0H                                                  0274
@RC00267 DS    0H                                                  0274
@RC00265 DS    0H                                                  0274
@RC00263 MVI   PTFIND,X'00'                                        0274
*         CALL HMASMGTA(HMASMGTP);  /* STORE AND SEQUENCE RECORD     */
         L     @15,@CV01838                                        0275
         LA    @01,@AL00275                                        0275
         BALR  @14,@15                                             0275
*         TBLDRC=GTPRETRN;          /* SAVE RETURN CODE              */
         SLR   TBLDRC,TBLDRC                                       0276
         IC    TBLDRC,GTPRETRN                                     0276
*         IF TBLDRC=ZERO            /* IF OK TO CONTINUE             */
*           THEN                                                   0277
         LTR   TBLDRC,TBLDRC                                       0277
         BNZ   @RF00277                                            0277
*           DO;                     /* THEN EXTRACT REQ SYSMOD INFO  */
*             IF ICTPDIMP(PTF)=OFF  /* IF NOT DELETED THEN   @Z67SP00*/
*                 &ICTPDEXP(PTF)=OFF THEN/* ACCESS REQ LISTS @Z67SP00*/
         LR    @12,PTF                                             0279
         MH    @12,@CH01955                                        0279
         L     @08,CCAICPTF(,CCAPTR)                               0279
         ALR   @08,@12                                             0279
         AL    @08,@CF01966                                        0279
         TM    ICTPDIMP-13(@08),B'00110000'                        0279
         BNZ   @RF00279                                            0279
*               DO;                 /*                       @Z67SP00*/
*                 CALL AR1RELP;     /* VIA AR1RELP ROUTINE           */
         BAL   @14,AR1RELP                                         0281
*                 RFY                                              0282
*                   RTNCODE RSTD;   /*                               */
*                 TBLDRC=RTNCODE;   /* SAVE RETURN CODE              */
         LR    TBLDRC,RTNCODE                                      0283
*                 RFY                                              0284
*                   RTNCODE UNRSTD; /*                               */
*               END;                /* END NOT DELETED SYS   @Z67SP00*/
*           END;                    /* REQUISITE SYSMOD INFO SAVED   */
@RF00279 DS    0H                                                  0287
*       END;                        /* END NOT ELEIMINATED   @Z67SP00*/
@RF00277 DS    0H                                                  0288
*   END;                            /* ALL SYSMOD INFO EXTRACTED     */
@RF00250 AH    PTF,@CH00039                                        0288
@DE00248 LR    @12,PTF                                             0288
         MH    @12,@CH01955                                        0288
         L     @08,CCAICPTF(,CCAPTR)                               0288
         AL    @08,@CF01968                                        0288
         L     @12,ICTPEND(@12,@08)                                0288
         CL    @12,ICTENDNT                                        0288
         BE    @DC00248                                            0288
         LTR   TBLDRC,TBLDRC                                       0288
         BZ    @DL00248                                            0288
@DC00248 DS    0H                                                  0289
*   RETURN CODE(TBLDRC);            /* RETURN TO CALLER              */
         LR    @15,TBLDRC                                          0289
         L     @14,@SA00003                                        0289
         LM    @00,@12,@SA00003+8                                  0289
         BR    @14                                                 0289
*   END AR1TBLD;                    /*                               */
         EJECT
*AR1RELP:                                                          0291
*   PROC;                           /*                               */
*                                                                  0291
AR1RELP  STM   @14,@12,@SA00004                                    0291
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT OF CODE EXTRACTS AND SEQUENCES THE REQUISITE     */
*   /* SYSMOD FOR THE CURRENT SYSMOD                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0292
*   DCL                                                            0292
*     RELPRC FIXED(31);             /* INTERNAL RETURN CODE          */
*   DCL                                                            0293
*     REQL FIXED(15);               /* LOOP CTR FOR REQ SYSMOD LIST  */
*   DCL                                                            0294
*     IREQL FIXED(15);              /* LOOP CTR FOR IFREQ SYSMOD LIST
*                                                            @Z67SP00*/
*   DCL                                                            0295
*     PREL FIXED(15);               /* LOOP CTR FOR PRE SYSMOD LIST  */
*   RFY                                                            0296
*     ICTREQLS BASED(ICTPRCHN(PTF));/* BASE CURRENT SYSMOD REQ LIST  */
*   RFY                                                            0297
*     ICTPRELS BASED(ICTPPCHN(PTF));/* BASE CURRENT SYSMOD PRE LIST  */
*   RFY                                                            0298
*     ICTIRQLS BASED(ICTPICHN(PTF));/* BASE CURRENT SYSMOD IFREQ LIST
*                                                            @Z67SP00*/
*                                                                  0298
*   /*****************************************************************/
*   /*                                                               */
*   /* START OF AR1RELP CODE                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0299
*   RELPRC=ZERO;                    /* INITIALIZE RETURN CODE        */
*                                                                  0299
         SLR   RELPRC,RELPRC                                       0299
*   /*****************************************************************/
*   /*                                                               */
*   /* EXTRACT AND SEQUENCE REQ SYSMOD NUMBERS                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0300
*   DO REQL=ONE BY ONE              /* LOOP THRU REQ LIST    @Z67SP00*/
*         WHILE(ICTREQND(REQL)^=ICTENDIX/* UNTIL END OF LIST         */
*         &ICTPSUPN(PTF)=ZERO       /* AND NOT SUPERCEDED    @Z67SP00*/
*         &RELPRC=ZERO);            /* OR NO MORE STORAGE            */
         LA    REQL,1                                              0300
         B     @DE00300                                            0300
@DL00300 DS    0H                                                  0301
