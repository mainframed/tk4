         ST    @12,@AL00001                                        0485
         L     @15,@CV01837                                        0485
         LA    @01,@AL00001                                        0485
         BALR  @14,@15                                             0485
*   HEADRC=IOPRETRN;                /* SAVE RETURN CODE              */
         L     @12,IOPPTR                                          0486
         SLR   HEADRC,HEADRC                                       0486
         IC    HEADRC,IOPRETRN(,@12)                               0486
*   IF HEADRC=ZERO                  /* IF OK TO CONTINUE THEN        */
