         AH    INDX,@CH00039                                       0505
         CH    INDX,@CH00059                                       0505
         BNH   @DL00490                                            0505
@DC00490 DS    0H                                                  0506
*     END;                          /* HEADING OUTPUT COMPLETED      */
*   RETURN CODE(HEADRC);            /* RETURN TO CALLER              */
@RF00487 LR    @15,HEADRC                                          0507
         L     @14,@SA00008                                        0507
         LM    @00,@12,@SA00008+8                                  0507
         BR    @14                                                 0507
