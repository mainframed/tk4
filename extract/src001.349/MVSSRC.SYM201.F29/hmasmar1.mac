*AR1RPTF:                                                          0425
*   PROC;                           /*                               */
*                                                                  0425
AR1RPTF  STM   @14,@12,@SA00007                                    0425
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT OF CODE WILL INSERT REQUISITE SYSMOD NUMBERS,    */
*   /* THEIR STATUS (MAX OF 6 ENTRIES PER LINE) INTO THE REPORT      */
*   /* OUTPUT BUFFER AND PRINT REPORT LINES IF REQUIRED              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0426
*   DCL                                                            0426
*     RPTFRC FIXED(31);             /* INTERNAL RETURN CODE          */
*   DCL                                                            0427
*     1 TYPETAB(3) CHAR(6),         /* TABLE USED TO CONVERT       0427
*                                      REQUISITE SYSMOD TYPE TO    0427
*                                      PRINTABLE FORMAT              */
*       3 ACTTYPE CHAR(1) INIT(     /* REQUISITE SYSMOD TYPE         */
*             '01'X,                /* IFREQ SYSMOD NUMBER           */
*             '02'X,                /* REQ SYSMOD NUMBER             */
*             '03'X),               /* PRE SYSMOD NUMBER             */
*       3 FMTTYPE CHAR(5) INIT(     /* PRINTABLE REQUISITE SYSMOD TYP*/
*             'IFREQ',              /* IFREQ SYSMOD                  */
*             'REQ  ',              /* REQ SYSMOD                    */
*             'PRE  ');             /* PRE SYSMOD                    */
*   DCL                                                            0428
*     INDXT FIXED(15);              /* TYPETAB LOOP VARIABLE         */
*                                                                  0428
*   /*****************************************************************/
*   /*                                                               */
*   /* START OF AR1RPTF CODE                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0429
*   RPTFRC=ZERO;                    /* INITIALIZE RETURN CODE        */
*                                                                  0429
         SLR   RPTFRC,RPTFRC                                       0429
*   /*****************************************************************/
*   /*                                                               */
*   /* IF REQUISITE SYSMOD TYPE IS DIFFERENT THAN TYPE OF THE LAST   */
*   /* SYSMOD THAT WAS INSERTED IN THE REPORT OUTPUT BUFFER, THEN    */
*   /* PRINTABLE FORM OF CURRENT REQUISITE SYSMOD TYPE MUST BE       */
*   /* INSERTED IN OUTPUT BUFFER.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0430
*   IF PTFIND^=CURIND               /* IF DIFFERENT TYPE THAN LAST 0430
*                                      REQUISITE SYSMOD INSERTED     */
*     THEN                                                         0430
         CLC   PTFIND(1),CURIND                                    0430
         BE    @RF00430                                            0430
*     DO;                           /* THEN SEE IF PRIOR REPORT LINE */
*       IF RECPRINT=OFF             /* NEEDS TO BE PRINTED           */
*           &CURIND^=MAJOR          /*                               */
*         THEN                                                     0432
         TM    RECPRINT,B'10000000'                                0432
         BNZ   @RF00432                                            0432
         CLI   CURIND,X'00'                                        0432
         BE    @RF00432                                            0432
*         DO;                       /* PRINT IT VIA                  */
*           CALL AR1PREC;           /* CALL TO AR1PREC ROUTINE       */
         BAL   @14,AR1PREC                                         0434
*           RFY                                                    0435
*             RTNCODE RSTD;         /*                               */
*           RPTFRC=RTNCODE;         /* SAVE RETURN CODE              */
         LR    RPTFRC,RTNCODE                                      0436
*           RFY                                                    0437
*             RTNCODE UNRSTD;       /*                               */
*         END;                      /* PRIOR REPORT LINE PRINTED     */
*                                                                  0438
*       /*************************************************************/
*       /*                                                           */
*       /* INSERT PRINTABLE FORMAT OF CURRENT REQUISITE SYSMOD TYPE. */
*       /* TYPES ARE REQ, IFREQ AND PRE.                             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0439
*       IF RPTFRC=ZERO              /* IF NO PRIOR IO ERRORS         */
*         THEN                                                     0439
@RF00432 LTR   RPTFRC,RPTFRC                                       0439
         BNZ   @RF00439                                            0439
*         DO;                       /* THEN CONTINUE                 */
*           DO INDXT=ONE TO DIM(TYPETAB);/* FIND TYPE IN TYPE TABLE  */
         LA    INDXT,1                                             0441
@DL00441 DS    0H                                                  0442
*             IF PTFIND=ACTTYPE(INDXT)/* IF MATCH                    */
*               THEN                                               0442
         LR    @12,INDXT                                           0442
         MH    @12,@CH00248                                        0442
         LA    @08,ACTTYPE-6(@12)                                  0442
         CLC   PTFIND(1),0(@08)                                    0442
         BNE   @RF00442                                            0442
*               OUTRELPT=FMTTYPE(INDXT);/* PICK UP PRINTABLE TYPE AND
*                                      INSERT IN BUFFER              */
         LA    @08,FMTTYPE-6(@12)                                  0443
         MVC   OUTRELPT(5),0(@08)                                  0443
*           END;                    /* PRINTABLE TYPE INSERTED       */
@RF00442 AH    INDXT,@CH00039                                      0444
         CH    INDXT,@CH00041                                      0444
         BNH   @DL00441                                            0444
*           RELPINDX=ZERO;          /* RESET COUNT INSERTED SYSMODS  */
         SLR   @12,@12                                             0445
         STH   @12,RELPINDX                                        0445
*           CURIND=PTFIND;          /* RESET LAST REQUISITE SYSMOD 0446
*                                      TYPE INSERTED                 */
         MVC   CURIND(1),PTFIND                                    0446
*         END;                      /*                               */
*     END;                          /* END OF NEW REQUISITE SYSMOD   */
*                                                                  0448
@RF00439 DS    0H                                                  0449
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FOLLOWING CODE WILL INSERT THE REQUISITE SYSMOD NUMBER A  */
*   /* STATUS INDICATOR FOR THE REQUISITE SYSMOD (EITHER - BLANK =   */
*   /* GO, '-' = NOGO, '*' = BYPASS NOGO) IN THE REPORT OUTPUT BUFFER*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0449
*   IF RPTFRC=ZERO                  /* IF NO PRIOR IO ERRORS         */
*     THEN                                                         0449
@RF00430 LTR   RPTFRC,RPTFRC                                       0449
         BNZ   @RF00449                                            0449
*     DO;                           /* THEN CONTINUE WITH REPORT     */
*       RELPINDX=RELPINDX+ONE;      /* INCREMENT COUNT OF INSERTED 0451
*                                      SYSMODS                       */
         LA    @12,1                                               0451
         AH    @12,RELPINDX                                        0451
         STH   @12,RELPINDX                                        0451
*       IF RELPINDX>DIM(OUTRELPS)   /* IF NO MORE ROOM IN BUFFER     */
*         THEN                                                     0452
         CH    @12,@CH00248                                        0452
         BNH   @RF00452                                            0452
*         DO;                       /* THEN PRINT CURRENT CONTENTS   */
*           CALL AR1PREC;           /* VIA CALL TO AR1PREC ROUTINE   */
         BAL   @14,AR1PREC                                         0454
*           RFY                                                    0455
*             RTNCODE RSTD;         /*                               */
*           RPTFRC=RTNCODE;         /* SAVE RETURN CODE              */
         LR    RPTFRC,RTNCODE                                      0456
*           RFY                                                    0457
*             RTNCODE UNRSTD;       /*                               */
*           RELPINDX=ONE;           /* SET INSRTED SYSMOD COUNT TO 1 */
         MVC   RELPINDX(2),@CH00039                                0458
*         END;                      /* BUFFER CONTENTS PRINTED       */
*       IF RPTFRC=ZERO              /* IF OK TO CONTINUE             */
*         THEN                                                     0460
@RF00452 LTR   RPTFRC,RPTFRC                                       0460
         BNZ   @RF00460                                            0460
*         DO;                       /*                               */
*           IF PTFSTAT=STAT04 THEN  /* REQ NOT SATISFIED?    @Z67SP00*/
         CLI   PTFSTAT,X'04'                                       0462
         BNE   @RF00462                                            0462
*             REQSTAT(RELPINDX)=DASH;/* SHOW ^ SATISFIED '-' @Z67SP00*/
         LH    @12,RELPINDX                                        0463
         MH    @12,@CH00034                                        0463
         LA    @08,REQSTAT-10(@12)                                 0463
         MVI   0(@08),C'-'                                         0463
*           ELSE                                                   0464
*             IF PTFSTAT=STAT05 THEN/* REQ NOT SATISFIED BUT BYPASS
*                                      WAS USED FOR THIS REQ @Z67SP00*/
         B     @RC00462                                            0464
@RF00462 CLI   PTFSTAT,X'05'                                       0464
         BNE   @RF00464                                            0464
*               REQSTAT(RELPINDX)=STAR;/* SATISFIED W/BYPASS'*'    0465
*                                                            @Z67SP00*/
         LH    @12,RELPINDX                                        0465
         MH    @12,@CH00034                                        0465
         LA    @08,REQSTAT-10(@12)                                 0465
         MVI   0(@08),C'*'                                         0465
*             ELSE                                                 0466
*               REQSTAT(RELPINDX)=BLANK;/* SHOW GO STATUS    @Z67SP00*/
         B     @RC00464                                            0466
@RF00464 LH    @12,RELPINDX                                        0466
         MH    @12,@CH00034                                        0466
         LA    @08,REQSTAT-10(@12)                                 0466
         MVI   0(@08),C' '                                         0466
*           OUTRELP(RELPINDX)=PTFDATA;/* INSERT REQUISITE SYSMOD   0467
*                                      NUMBER IN OUTPUT BUFFER       */
@RC00464 DS    0H                                                  0467
@RC00462 LH    @12,RELPINDX                                        0467
         MH    @12,@CH00034                                        0467
         LA    @08,OUTRELP-10(@12)                                 0467
         MVC   0(7,@08),PTFDATA                                    0467
*           RECPRINT=OFF;           /* SIGNAL PRINT NEW REPORT LINE  */
         NI    RECPRINT,B'01111111'                                0468
*         END;                      /* END OF SYSMOD NO. INTO BUFFER */
*     END;                          /* END REQUISITE SYSMOD PROCESS  */
@RF00460 DS    0H                                                  0471
*   RETURN CODE(RPTFRC);            /* RETURN TO CALLER              */
@RF00449 LR    @15,RPTFRC                                          0471
         L     @14,@SA00007                                        0471
         LM    @00,@12,@SA00007+8                                  0471
         BR    @14                                                 0471
*   END AR1RPTF;                    /*                               */
         EJECT
*AR1HEAD:                                                          0473
