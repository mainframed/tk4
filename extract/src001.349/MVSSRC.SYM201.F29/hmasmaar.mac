*   /*****************************************************************/
*                                                                  0252
*   IF HIGHAAR<STOPFUNC             /* NO TERMINAL ERRORS            */
*     THEN                                                         0252
@RF00245 CH    HIGHAAR,@CH00033                                    0252
         BNL   @RF00252                                            0252
*     DO;                           /* CALL PROGRAM                  */
*       CALL HMASMCMP;              /* DO ASSEMBLIES                 */
         L     @15,@CV01710                                        0254
         BALR  @14,@15                                             0254
*       RFY                                                        0255
*         RTNCODE RSTD;             /* RESTRICT CODE REG             */
*       HIGHAAR=MAX(HIGHAAR,RTNCODE);/* AAR CODE                     */
         CR    HIGHAAR,RTNCODE                                     0256
         BNL   *+6
         LR    HIGHAAR,RTNCODE                                     0256
*       RFY                                                        0257
*         RTNCODE UNRSTD;           /* UNRESTRICT CODE REG           */
*     END;                          /* END ASSEMBLY PROCESSING       */
*                                                                  0258
*   /*****************************************************************/
*   /*                                                               */
*   /* EXIT IS MADE TO HMASMLKI IF THERE ARE ANY LINKEDITS TO DO     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0259
*   IF HIGHAAR<STOPFUNC             /* NO TERMINAL ERRORS            */
*     THEN                                                         0259
@RF00252 CH    HIGHAAR,@CH00033                                    0259
         BNL   @RF00259                                            0259
*     DO;                           /* CALL PROGRAM                  */
*       CALL HMASMLKI;              /* DO LINKS                      */
         L     @15,@CV01708                                        0261
         BALR  @14,@15                                             0261
*       RFY                                                        0262
*         RTNCODE RSTD;             /* RESTRICT CODE REG             */
*       HIGHAAR=MAX(HIGHAAR,RTNCODE);/* AAR CODE                     */
         CR    HIGHAAR,RTNCODE                                     0263
         BNL   *+6
         LR    HIGHAAR,RTNCODE                                     0263
*       RFY                                                        0264
*         RTNCODE UNRSTD;           /* UNRESTRICT CODE REG           */
*     END;                          /* END ASSEMBLY PROCESSING       */
*                                                                  0265
*   /*****************************************************************/
*   /*                                                               */
*   /* EXIT IS MADE TO HMASMZAP TO PROCESS ZAPS BY TESTING THE ZAP   */
*   /* BIT IN THE CCA. ALL ZAPS WILL BE DONE BY HMASMZAP WITH ONE    */
*   /* CALL.                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0266
*   IF HIGHAAR<STOPFUNC             /* AND NOT TERMINATING           */
*     THEN                                                         0266
@RF00259 CH    HIGHAAR,@CH00033                                    0266
         BNL   @RF00266                                            0266
*     DO;                                                          0267
*       CALL HMASMZAP;              /* CALL THE ZAP INTERFACE SUBROUT*/
         L     @15,@CV01707                                        0268
         BALR  @14,@15                                             0268
*       RFY                                                        0269
*         RTNCODE RSTD;             /* RESTRICT CODE REG             */
*       HIGHAAR=MAX(HIGHAAR,RTNCODE);/* SETT AAR CODE                */
         CR    HIGHAAR,RTNCODE                                     0270
         BNL   *+6
         LR    HIGHAAR,RTNCODE                                     0270
*       RFY                                                        0271
*         RTNCODE UNRSTD;           /* UNRESTRICT CODE REG           */
*       CCAZAPP=OFF;                /* RESET ZAP BIT WHEN ZAPS DONE  */
         NI    CCAZAPP(CCAPTR),B'11110111'                         0272
*     END;                                                         0273
*   END MODIFY;                     /* END OF PROC                   */
@EL00002 DS    0H                                                  0274
@EF00002 DS    0H                                                  0274
@ER00002 LM    @14,@03,@SA00002                                    0274
         LM    @05,@12,@SA00002+24                                 0274
         BR    @14                                                 0274
*                                                                  0275
*   /*****************************************************************/
*   /*                                                               */
*   /* IOSUP IS INVOKED TO PROCESS THE SVCLIB DATASET                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0275
*IOSUPLNK:                                                         0275
*   PROC OPTIONS(SAVEAREA);                                        0275
*                                                                  0275
IOSUPLNK STM   @14,@03,12(@13)                                     0275
         STM   @05,@12,40(@13)                                     0275
         ST    @13,@SA00003+4                                      0275
         LA    @14,@SA00003                                        0275
         ST    @14,8(,@13)                                         0275
         LR    @13,@14                                             0275
*   /*****************************************************************/
*   /*                                                               */
*   /* STAE PARMS                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0276
*   SAVRVANM=RVAEPRNM;              /* SAVE RVA PROC NAME            */
         L     @10,CCARVAD(,CCAPTR)                                0276
         MVC   SAVRVANM(8),RVAEPRNM(@10)                           0276
*   RVAEPRNM=CCASUPNM;              /* IEHIOSUP NAME                 */
         L     @05,CCABLDLP(,CCAPTR)                               0277
         MVC   RVAEPRNM(8,@10),CCASUPNM(@05)                       0277
*   RFY                                                            0278
*     LINKREG RSTD;                                                0278
*   LINKREG=CCAIOSUP;               /* ADDR OF IOSUP BLDL            */
         L     LINKREG,CCAIOSUP(,@05)                              0279
*   IF CCASPDCB=ZERO                /* IF NO DCB FOR IOSUP LINK      */
*     THEN                          /* GO AHEAD AND LINK             */
         L     @10,CCASPDCB(,CCAPTR)                               0280
         CH    @10,ZERO                                            0280
         BNE   @RF00280                                            0280
*     GEN( LINK  DE=(LINKREG),PARAM=(0,PARMLIST),VL=1);            0281
          LINK  DE=(LINKREG),PARAM=(0,PARMLIST),VL=1
*   ELSE                            /* OTHERWISE - MUST PASS DCB TO
*                                      LINK                          */
*     DO;                                                          0282
         B     @RC00280                                            0282
@RF00280 DS    0H                                                  0283
*       RFY                                                        0283
*         SPDCBRG RSTD;                                            0283
*       SPDCBRG=CCASPDCB;           /* GET DCB ADDR FROM CCA         */
         L     SPDCBRG,CCASPDCB(,CCAPTR)                           0284
*       GEN( LINK  DE=(LINKREG),PARAM=(0,PARMLIST),DCB=(SPDCBRG),VL=1);
          LINK  DE=(LINKREG),PARAM=(0,PARMLIST),DCB=(SPDCBRG),VL=1
*       RFY                                                        0286
*         SPDCBRG UNRSTD;                                          0286
*     END;                                                         0287
*   RFY                                                            0288
*     LINKREG UNRSTD;                                              0288
@RC00280 DS    0H                                                  0289
*   RFY                                                            0289
*     RTNCODE RSTD;                 /* RESTRICD CODE REG             */
*   RVAEPRNM=SAVRVANM;              /* RESTORE RVA                   */
         L     @14,CCARVAD(,CCAPTR)                                0290
         MVC   RVAEPRNM(8,@14),SAVRVANM                            0290
*   CVD(RTNCODE,DBLWRD);            /* CONVERT RETURN CODE TO DECIMAL*/
         CVD   RTNCODE,DBLWRD                                      0291
*   MGPCLEAR=''B;                   /* CLEAR MSG PARMS               */
         XC    MGPCLEAR(6),MGPCLEAR                                0292
*   MGPTYPE=MGPINFO;                /* MESSAGE SEVERITY              */
         MVI   MGPTYPE,X'01'                                       0293
*   IF RTNCODE^=ZERO                /* WAS RETURN CODE BAD?          */
*     THEN                          /* YES - SET BAD RETURN INDICATOR*/
         CH    RTNCODE,ZERO                                        0294
         BE    @RF00294                                            0294
*     DO;                           /*                               */
*       MGPTYPE=MGPTERM;            /* MESSAGE SEVERITY              */
         MVI   MGPTYPE,X'05'                                       0296
*       HIGHAAR=STOPSMP;            /* STOP SMP                      */
         LA    HIGHAAR,16                                          0297
*     END;                          /*                               */
*   RFY                                                            0299
*     RTNCODE UNRSTD;               /* UNRESTRICT CODE REG           */
@RF00294 DS    0H                                                  0300
*   UNPK(SUPCODE,DBLWRD);           /* UNPACK THE RETURN CODE        */
         UNPK  SUPCODE(2),DBLWRD(8)                                0300
*   SUPDIG=SUPDIG×MAKPRINT;         /* MAKE IT PRINTABLE             */
         OI    SUPDIG,X'F0'                                        0301
*   MGPVARPT(1)=ADDR(SUPCODE);      /* SET VARIABLE SECTION          */
         LA    @10,SUPCODE                                         0302
         ST    @10,MGPVARPT                                        0302
*   MGPMGNO1=HMA230I;               /* IEHIOSUP EXECUTED             */
         MVC   MGPMGNO1(2),@CH00036                                0303
*   MGPMGNO2=PART009;               /* RETURN CODE                   */
         MVC   MGPMGNO2(2),@CH00479                                0304
*   CALL HMASMMSG(HMASMMGP);        /* WRITE MESSAGE                 */
         L     @15,@CV01703                                        0305
         LA    @01,@AL00305                                        0305
         BALR  @14,@15                                             0305
*   CCASVCLB=OFF;                   /* RESET IOSUP INDICATOR         */
         NI    CCASVCLB(CCAPTR),B'10111111'                        0306
*   END IOSUPLNK;                                                  0307
@EL00003 L     @13,4(,@13)                                         0307
@EF00003 DS    0H                                                  0307
@ER00003 LM    @14,@03,12(@13)                                     0307
         LM    @05,@12,40(@13)                                     0307
         BR    @14                                                 0307
*                                                                  0308
*   /*****************************************************************/
*   /*                                                               */
*   /* THE ICT MOD ENTRIES ARE CHECKED FOR MAC OR SRC TYPES. IF THE  */
*   /* CCA INDICATES MTS OR STS DELETES AND THIS IS ACCEPT WITHOUT   */
*   /* NOAPPLY AND THE MODID SUBENTRIES FROM THE ELEMENTS CDS ENTRY  */
*   /* MATCH ITS ACDS ENTRY MODID LIST , OR THIS IS THE RESTORE      */
*   /* FUNCTION THEN CALL HMASMIO TO DELETE THE MTS OR STS MEMBER.   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0308
*MTSSTS:                                                           0308
*   PROC;                           /*                               */
MTSSTS   STM   @14,@03,@SA00004                                    0308
         STM   @05,@12,@SA00004+24                                 0308
*   RFY                                                            0309
*     ICTMOD BASED(CCAICMOD);       /*                               */
*                                                                  0309
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR RESTORE OR ACCEPT AND NOAPPLY FUNCTION AND MTS STS  */
*   /* DELETE OPTION                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0310
*   IF((CCAACCPT=ON&TBLNOAPP=OFF)   /* ACCEPT NOAPPLY OR             */
*       ×CCARES=ON)                 /* .... RESTORE AND              */
*       &(CCAMTSNP=OFF×CCASTSNP=OFF)/* ......DELETING MTS, STS       */
*     THEN                          /*                               */
*                                                                  0310
         TM    CCAACCPT(CCAPTR),B'00100000'                        0310
         BNO   @GL00003                                            0310
         L     @10,@PC00001                                        0310
         TM    TBLNOAPP(@10),B'10000000'                           0310
         BZ    @GL00002                                            0310
@GL00003 TM    CCARES(CCAPTR),B'00001000'                          0310
         BNO   @RF00310                                            0310
@GL00002 TM    CCAMTSNP(CCAPTR),B'01100000'                        0310
         BO    @RF00310                                            0310
*     /***************************************************************/
*     /*                                                             */
*     /* SEARCH THRU THE ICT MOD SECTION                             */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0311
*     DO A=1 BY 1 WHILE ICTMEND(A)^=ICTENDNT/*                       */
*           &HIGHAAR<STOPFUNC;      /* STOP IF ERROR                 */
*                                                                  0311
         LA    A,1                                                 0311
         B     @DE00311                                            0311
@DL00311 DS    0H                                                  0312
*       /*************************************************************/
*       /*                                                           */
*       /* SEE IF ELEMENT IS ELIGIBLE FOR PROCESSING                 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0312
*       IF ICTMNOGO(A)=OFF          /*                               */
*           &ICTMCPL(A)=ON          /*                               */
*           &ICTMISEL(A)=ON THEN    /*                               */
*                                                                  0312
         LR    @10,A                                               0312
         MH    @10,@CH01841                                        0312
         L     @07,CCAICMOD(,CCAPTR)                               0312
         ST    @10,@TF00001                                        0312
         ALR   @10,@07                                             0312
         AL    @10,@CF01852                                        0312
         TM    ICTMCPL-8(@10),B'00100000'                          0312
         BNO   @RF00312                                            0312
         TM    ICTMNOGO-8(@10),B'01000000'                         0312
         BNZ   @RF00312                                            0312
         LR    @10,@07                                             0312
         AL    @10,@TF00001                                        0312
         AL    @10,@CF01853                                        0312
         TM    ICTMISEL-11(@10),B'10000000'                        0312
         BNO   @RF00312                                            0312
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK MACRO OR SOURCE ELIGIBILITY                       */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0313
*         IF((ICTMMACR(A)=ON×ICTMMACU(A)=ON)/* MACRO TYPE            */
*             &CCAMTSNP=OFF)        /* AND DELETE MACS               */
*             ×((ICTMSRCR(A)=ON×ICTMSRCU(A)=ON)/* SOURCE TYPE        */
*             &CCASTSNP=OFF)        /* AND DELETE SRC                */
*           THEN                    /*                               */
         AL    @07,@TF00001                                        0313
         AL    @07,@CF01854                                        0313
         TM    ICTMMACR-9(@07),B'00001100'                         0313
         BZ    @GL00006                                            0313
         TM    CCAMTSNP(CCAPTR),B'01000000'                        0313
         BZ    @RT00313                                            0313
@GL00006 LR    @10,A                                               0313
         MH    @10,@CH01841                                        0313
         L     @07,CCAICMOD(,CCAPTR)                               0313
         ALR   @07,@10                                             0313
         AL    @07,@CF01854                                        0313
         TM    ICTMSRCR-9(@07),B'00000011'                         0313
         BZ    @RF00313                                            0313
         TM    CCASTSNP(CCAPTR),B'00100000'                        0313
         BNZ   @RF00313                                            0313
@RT00313 DS    0H                                                  0314
*           DO;                     /*                               */
