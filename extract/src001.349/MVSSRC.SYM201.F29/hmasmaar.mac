@RF00219 TM    CCASVCLB(CCAPTR),B'01000000'                        0221
         BNO   @RF00221                                            0221
*     CALL IOSUPLNK;                /* CALL ROUTINE                  */
*                                                                  0222
         BAL   @14,IOSUPLNK                                        0222
