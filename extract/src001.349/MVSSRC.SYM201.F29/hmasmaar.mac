*     THEN                          /*                               */
@RF00231 CH    HIGHAAR,@CH00033                                    0238
         BNL   @RF00238                                            0238
*     DO;                                                          0239
*       CALL HMASMCPI;              /* CALL THE COPY INTERFACE ROUT. */
         L     @15,@CV01712                                        0240
         BALR  @14,@15                                             0240
*       RFY                                                        0241
*         RTNCODE RSTD;             /* RESTRICT CODE REG             */
*       HIGHAAR=MAX(HIGHAAR,RTNCODE);/* SET AAR CODE                 */
         CR    HIGHAAR,RTNCODE                                     0242
         BNL   *+6
         LR    HIGHAAR,RTNCODE                                     0242
*       RFY                                                        0243
*         RTNCODE UNRSTD;           /* UNRESTRICT CODE REG           */
*     END;                                                         0244
*                                                                  0244
*   /*****************************************************************/
*   /*                                                               */
