         TITLE 'HMASMDR2 REWRITE INSTORAGE DIRECTORIES'
HMASMDR2 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMDR2  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(12),0(@01)                                 0001
*   IOPPTR=CCAIOPTR;                /*                               */
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0138
*   DR2RTNCD=ZERO;                  /*                               */
         SLR   DR2RTNCD,DR2RTNCD                                   0139
*   DR2SW=''B;                      /*                               */
         MVI   DR2SW,X'00'                                         0140
*   HMASMMGP=''B;                   /* ZERO MGP THEN                 */
         XC    HMASMMGP(16),HMASMMGP                               0141
*   MGPPRINT=ON;                    /* SET TO WRITE MSG TO SMPOUT    */
*   MGPHLDS=ON;                     /* AND LOG                       */
*                                                                  0143
         OI    MGPPRINT,B'10010000'                                0143
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE WHETHER THE DIS FILE SHOULD BE REWRITTEN OR FREED   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0144
*   IF CCAUPDJ=ON                   /* IF JCLIN FUNCTION             */
*       &SAVECODE>=STOPPTF          /* AND RETURN CODE IS STOPPTF    */
*     THEN                          /* OR GREATER THEN DO NOT REWRITE*/
         TM    CCAUPDJ(CCAPTR),B'00000100'                         0144
         BNO   @RF00144                                            0144
         L     @10,@PC00001+8                                      0144
         L     @10,SAVECODE(,@10)                                  0144
         CH    @10,@CH00061                                        0144
         BL    @RF00144                                            0144
*     DR2FUNCT=IOPFREE;             /* DIS. JUST FREE IT.            */
         LA    DR2FUNCT,9                                          0145
*   ELSE                            /* FOR ALL OTHER FUNCTION AND    */
*     DR2FUNCT=IOPWRDIR;            /* SUCCESSFULL JCLIN REWRITE THE
*                                      INSTORAGE DIRECTORY           */
*                                                                  0146
         B     @RC00144                                            0146
@RF00144 LA    DR2FUNCT,89                                         0146
*   /*****************************************************************/
*   /*                                                               */
*   /* REMEMBER IF JCLIN BEING RUN                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0147
*   FJCLIN=OFF;                     /*                               */
@RC00144 NI    FJCLIN,B'01111111'                                  0147
*   IF CCAUPDJ=ON                   /*                               */
*     THEN                          /*                               */
         TM    CCAUPDJ(CCAPTR),B'00000100'                         0148
         BNO   @RF00148                                            0148
*     FJCLIN=ON;                    /*                               */
         OI    FJCLIN,B'10000000'                                  0149
         EJECT
*                                                                  0150
*   /*****************************************************************/
*   /*                                                               */
*   /* IF CDS UPDATED IN STORAGE THEN PROCESS IT BY SETTING SYSTEM   */
*   /* ENTRY INDICATOR IR REWRITE TO BE DONE, DO REWRITE OR FREE,    */
*   /* RESET SYSTEM ENTRY ERROR INDICATOR, AND ISSUE MESSAGE         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0150
*   IF CCACDSOS=ON                  /* CDS IN STORAGE FOR UPDATE     */
*     THEN                          /*                               */
@RF00148 TM    CCACDSOS(CCAPTR),B'10000000'                        0150
         BNO   @RF00150                                            0150
*     DO;                           /*                               */
*       IF DR2FUNCT=IOPWRDIR        /* IF DIRECTORY REWRITE          */
*           &CCACSYSF=ON            /* AND SYSTEM ENTRY FOUND        */
*           &CCACDSUP=ON            /* AND HAS BEEN UPDATED          */
*         THEN                      /*                               */
         CH    DR2FUNCT,@CH00632                                   0152
         BNE   @RF00152                                            0152
         TM    CCACSYSF(CCAPTR),B'10000000'                        0152
         BNO   @RF00152                                            0152
         TM    CCACDSUP(CCAPTR),B'10000000'                        0152
         BNO   @RF00152                                            0152
*         DO;                       /*                               */
*           HMASMIOP=CCAIOPCS->HMASMIOP;/*                           */
         L     @10,CCAIOPCS(,CCAPTR)                               0154
         MVC   HMASMIOP(68,IOPPTR),HMASMIOP(@10)                   0154
*           IOPCDMAP=CCAIOPCS->IOPCDMAP;/*                           */
         MVC   IOPCDMAP(36,IOPPTR),IOPCDMAP(@10)                   0155
*           IOPFUNCT=IOPLOC;        /*                               */
         MVI   IOPFUNCT(IOPPTR),X'03'                              0156
*           CALL HMASMIO(HMASMIOP); /*                               */
         ST    IOPPTR,@AL00001                                     0157
         L     @15,@CV01343                                        0157
         LA    @01,@AL00001                                        0157
         BALR  @14,@15                                             0157
*           IF IOPRETRN=IOPGOOD     /*                               */
*             THEN                  /*                               */
         CLI   IOPRETRN(IOPPTR),0                                  0158
         BNE   @RF00158                                            0158
*             DO;                   /*                               */
*               CCACDSOS=OFF;       /* UPDATE ACTUAL CDS             */
         NI    CCACDSOS(CCAPTR),B'01111111'                        0160
*               IOPFUNCT=IOPSTOWR;  /*                               */
         MVI   IOPFUNCT(IOPPTR),X'54'                              0161
*               IOPSYNG=ON;         /*                               */
         OI    IOPSYNG(IOPPTR),B'00010000'                         0162
*               CALL HMASMIO(HMASMIOP);/* RESTOW CDS SYSTEM ENTRY    */
         ST    IOPPTR,@AL00001                                     0163
         L     @15,@CV01343                                        0163
         LA    @01,@AL00001                                        0163
         BALR  @14,@15                                             0163
*               CCACDSOS=ON;        /*                               */
         OI    CCACDSOS(CCAPTR),B'10000000'                        0164
*               CCAIOPCS->IOPCDMAP=IOPCDMAP;/*                       */
         L     @10,CCAIOPCS(,CCAPTR)                               0165
         MVC   IOPCDMAP(36,@10),IOPCDMAP(IOPPTR)                   0165
*             END;                  /*                               */
*           DR2RTNCD=MAX(DR2RTNCD,IOPRETRN);/*                       */
@RF00158 SLR   @10,@10                                             0167
         IC    @10,IOPRETRN(,IOPPTR)                               0167
         CR    DR2RTNCD,@10                                        0167
         BNL   *+6
         LR    DR2RTNCD,@10                                        0167
*         END;                      /*                               */
*       IF DR2RTNCD=ZERO            /*                               */
*           &CCACDSUP=ON            /* AND HAS BEEN UPDATED          */
*         THEN                      /*                               */
@RF00152 CH    DR2RTNCD,@CH00353                                   0169
         BNE   @RF00169                                            0169
         TM    CCACDSUP(CCAPTR),B'10000000'                        0169
         BNO   @RF00169                                            0169
*         DO;                       /*                               */
*           IOPFUNCT=DR2FUNCT;      /* WRITE OR FREE                 */
         STC   DR2FUNCT,IOPFUNCT(,IOPPTR)                          0171
*           IOPDSID=IOPCDS;         /* CDS DIS                       */
         MVI   IOPDSID(IOPPTR),X'02'                               0172
*           CALL HMASMIO(HMASMIOP); /* CALL HMASMIO TO EITHER FREE OR
*                                      REWRITE CDS DIRECTORY.      0173
*                                      FUNCTION ALREADY SET UP IN  0173
*                                      PREVIOUS ROUTINE              */
         ST    IOPPTR,@AL00001                                     0173
         L     @15,@CV01343                                        0173
         LA    @01,@AL00001                                        0173
         BALR  @14,@15                                             0173
*           DR2RTNCD=MAX(DR2RTNCD,IOPRETRN);/* SAVE RETURN CODE      */
         SLR   @10,@10                                             0174
         IC    @10,IOPRETRN(,IOPPTR)                               0174
         CR    DR2RTNCD,@10                                        0174
         BNL   *+6
         LR    DR2RTNCD,@10                                        0174
*         END;                      /*                               */
*       CCACDSOS=OFF;               /* UPDATE ACTUAL CDS             */
*                                                                  0176
@RF00169 NI    CCACDSOS(CCAPTR),B'01111111'                        0176
*       /*************************************************************/
*       /*                                                           */
*       /* IF DIRECTORY REWRITE OK THEN RESET SYSTEM ENTRY INDICATOR */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0177
*       IF DR2FUNCT=IOPWRDIR        /* IF DIRECTORY REWRITE          */
*           &CCACSYSF=ON            /* AND SYSTEM ENTRY FOUND        */
*           &DR2RTNCD=ZERO          /*                               */
*         THEN                      /*                               */
         CH    DR2FUNCT,@CH00632                                   0177
         BNE   @RF00177                                            0177
         TM    CCACSYSF(CCAPTR),B'10000000'                        0177
         BNO   @RF00177                                            0177
         CH    DR2RTNCD,@CH00353                                   0177
         BNE   @RF00177                                            0177
*         DO;                       /*                               */
*           RFY                                                    0179
*             HMASMIOP BASED(CCAIOPCS);/*                            */
*           CCAUPDJ=OFF;            /* SET SO MSG WILL GO TO LOG     */
         NI    CCAUPDJ(CCAPTR),B'11111011'                         0180
*           IOPFUNCT=IOPUDEO;       /*                               */
         L     @10,CCAIOPCS(,CCAPTR)                               0181
         MVI   IOPFUNCT(@10),X'56'                                 0181
*           IOPSYNG=OFF;            /*                               */
*           IOPSYDL=OFF;            /*                               */
         NI    IOPSYNG(@10),B'11001111'                            0183
*           CALL HMASMIO(HMASMIOP); /* RESTOW CDS SYSTEM ENTRY       */
         ST    @10,@AL00001                                        0184
         L     @15,@CV01343                                        0184
         LA    @01,@AL00001                                        0184
         BALR  @14,@15                                             0184
*           DR2RTNCD=MAX(DR2RTNCD,IOPRETRN);/*                       */
         L     @10,CCAIOPCS(,CCAPTR)                               0185
         SLR   @05,@05                                             0185
         IC    @05,IOPRETRN(,@10)                                  0185
         CR    DR2RTNCD,@05                                        0185
         BNL   *+6
         LR    DR2RTNCD,@05                                        0185
*           RFY                                                    0186
*             HMASMIOP BASED(IOPPTR);/*                              */
*           IF FJCLIN=ON            /*                               */
*             THEN                  /*                               */
         TM    FJCLIN,B'10000000'                                  0187
         BNO   @RF00187                                            0187
*             CCAUPDJ=ON;           /*                               */
         OI    CCAUPDJ(CCAPTR),B'00000100'                         0188
*         END;                      /*                               */
*                                                                  0189
@RF00187 DS    0H                                                  0190
*       /*************************************************************/
*       /*                                                           */
*       /* IF DIRECTORY REWRITE OK THEN ISSUE MESSAGE                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0190
*       IF DR2RTNCD=ZERO            /*                               */
*           &DR2FUNCT=IOPWRDIR      /*                               */
*         THEN                      /*                               */
@RF00177 CH    DR2RTNCD,@CH00353                                   0190
         BNE   @RF00190                                            0190
         CH    DR2FUNCT,@CH00632                                   0190
         BNE   @RF00190                                            0190
*         DO;                       /*                               */
*           MGPCLEAR=''B;           /*                               */
         XC    MGPCLEAR(6),MGPCLEAR                                0192
*           MGPMGNO1=HMA368I;       /* XXXXXXXX IN STORAGE DIRECTORY
*                                      SUCCESSFULLY REWRITTEN        */
         MVC   MGPMGNO1(2),@CH01166                                0193
*           MGPVARPT(1)=ADDR(TXTCDS);/*                              */
         LA    @10,@CC01347                                        0194
         ST    @10,MGPVARPT                                        0194
*           MGPTYPE=MGPINFO;        /*                               */
         MVI   MGPTYPE,X'01'                                       0195
*           CALL HMASMMSG(HMASMMGP);/*                               */
         L     @15,@CV01344                                        0196
         LA    @01,@AL00196                                        0196
         BALR  @14,@15                                             0196
*         END;                      /*                               */
*     END;                          /*                               */
@RF00190 DS    0H                                                  0199
         EJECT
