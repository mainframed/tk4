         TITLE 'HMASMASI - SMP ASSEMBLER INTERFACE MODULE'
HMASMASI CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMASI  78.335'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*                                                                  0194
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE VARIABLES                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0195
*   IOPPTR=CCAIOPTR;                /* INITIALIZE IOP BASE REGISTER  */
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0195
*   ASIRTNCD=ZERO;                  /* INITIALIZE RETURN CODE        */
         SLR   ASIRTNCD,ASIRTNCD                                   0196
*   HMASMMGP=HMASMMGP&&HMASMMGP;    /* ZERO MSG PARM         @Z40SP00*/
         XC    HMASMMGP(36),HMASMMGP                               0197
*   MGPPRINT=ON;                    /* ALL MSG TO PRINTER    @Z40SP00*/
*   MGPHLDS=ON;                     /* AND TO LOG            @Z40SP00*/
         OI    MGPPRINT,B'10010000'                                0199
*   SYSIN=ICTFMLIB(1);              /* SET UP SYSIN FOR ASM  @ZD00005*/
         L     @10,@PC00001                                        0200
         MVC   SYSIN(8),ICTFMLIB(@10)                              0200
*   SYSPUNCH=SMPWRK3;               /* SET UP SYSPUNCH FOR ASM     0201
*                                                            @Z67SP00*/
         MVC   SYSPUNCH(8),@CC01675                                0201
*   CRNTSMD=ICTPTFS(1);             /* SAVE CURRENT SYSMOD NUMBER  0202
*                                                            @Z67SP00*/
*                                                                  0202
         L     @05,ICTPPTR(,@10)                                   0202
         MVC   CRNTSMD(7),ICTPTFS(@05)                             0202
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCATE ASSEMBLY MEMBER                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0203
*   IOPFUNCT=IOPLOC;                /* INDICATE LOCATE               */
         MVI   IOPFUNCT(IOPPTR),X'03'                              0203
*   IOPNAME=ICTMNAME(1);            /* INDICATE ASSEM MEMBER @Z40SP00*/
         MVC   IOPNAME(8,IOPPTR),ICTMNAME(@10)                     0204
*   IF ICTFMLIB(1)=SMPCDS           /* IS FROMLIB CDS?       @ZD00005*/
*     THEN                                                         0205
         CLC   ICTFMLIB(8,@10),@CC01669                            0205
         BNE   @RF00205                                            0205
*     DO;                           /* YES, IT IS                    */
*       IOPDSID=IOPCDSM;            /* INDICATE CDS DATASET          */
         MVI   IOPDSID(IOPPTR),X'02'                               0207
*       IOPTYPE=IOPASM;             /* INDICATE ASSEM TYPE CDS MEMBER
*                                                            @ZD00008*/
         MVI   IOPTYPE(IOPPTR),X'01'                               0208
*     END;                                                         0209
*   ELSE                            /* NO, FROMLIB NOT CDS           */
*     IF ICTFMLIB(1)=SMPSTS         /* IS FROMLIB STS?       @ZD00005*/
*       THEN                                                       0210
         B     @RC00205                                            0210
@RF00205 L     @10,@PC00001                                        0210
         CLC   ICTFMLIB(8,@10),@CC01671                            0210
         BNE   @RF00210                                            0210
*       DO;                         /* THEN DO               @ZD00008*/
*         IOPDSID=IOPSTS;           /* INDICATE STS DATASET          */
         MVI   IOPDSID(IOPPTR),X'1A'                               0212
*         IOPTYPE=IOPSRC;           /* SET SOURCE TYPE IN IOP@ZD00008*/
         MVI   IOPTYPE(IOPPTR),X'08'                               0213
*       END;                        /*                       @ZD00008*/
*     ELSE                          /* OTHERWISE, INDICATE JUST A  0215
*                                      SOURCE LIBRARY                */
*       DO;                         /*                       @Z40SP00*/
         B     @RC00210                                            0215
@RF00210 DS    0H                                                  0216
*         IOPDSID=IOPSRCL;          /* USE SOURCE LIB        @Z40SP00*/
         MVI   IOPDSID(IOPPTR),X'16'                               0216
*         IOPDDNAM=ICTFMLIB(1);     /* SET LIBRARY NAME      @ZD00005*/
         L     @10,@PC00001                                        0217
         MVC   IOPDDNAM(8,IOPPTR),ICTFMLIB(@10)                    0217
*       END;                        /*                       @Z40SP00*/
*   CALL HMASMIO(HMASMIOP);         /* LOCATE ASSEMBLY               */
@RC00210 DS    0H                                                  0219
@RC00205 ST    IOPPTR,@AL00001                                     0219
         L     @15,@CV01638                                        0219
         LA    @01,@AL00001                                        0219
         BALR  @14,@15                                             0219
*   ASIRTNCD=MAX(ASIRTNCD,IOPRETRN);/* SET MAX RETURN CODE           */
*                                                                  0220
         SLR   @10,@10                                             0220
         IC    @10,IOPRETRN(,IOPPTR)                               0220
         CR    ASIRTNCD,@10                                        0220
         BNL   *+6
         LR    ASIRTNCD,@10                                        0220
*   /*****************************************************************/
*   /*                                                               */
*   /* IF NOT FOUND OR UNABLE TO OPEN LIBRARY PUT OUT MESSAGE AND SET*/
*   /* APPLICABLE RETURN CODE                                @Z40SP00*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0221
*   IF IOPRETRN=IOPNOTFD            /* ASSEMBLY FOUND?               */
*     THEN                                                         0221
         CLI   IOPRETRN(IOPPTR),4                                  0221
         BNE   @RF00221                                            0221
*     DO;                           /* NO, IT WASN'T SO PRINT MESSAGE*/
*       MGPCLEAR=MGPCLEAR&&MGPCLEAR;/* ZERO MSG NUMBERS      @Z40SP00*/
         XC    MGPCLEAR(6),MGPCLEAR                                0223
*       MGPTYPE=MGPWARN;            /* SET SEVERITY          @MS00001*/
         MVI   MGPTYPE,X'02'                                       0224
*       MGPMGNO1=HMA246I;           /* XXXXXXXX XXXXXXXX NOT FOUND ON
*                                      XXXXXXXX LIBRARY      @Z40SP00*/
         MVC   MGPMGNO1(2),@CH01655                                0225
*       MGPMGNO2=PART011;           /* FOR SYSMOD XXXXXXX    @Z67SP00*/
         MVC   MGPMGNO2(2),@CH00437                                0226
*       MGPVARPT(1)=ADDR(TXTASM);   /* ASSEMBLY              @Z40SP00*/
         LA    @10,@CC01677                                        0227
         ST    @10,MGPVARPT                                        0227
*       MGPVARPT(2)=ADDR(ICTMNAME(1));/* SET MSG NAME        @ZD00005*/
         L     @10,@PC00001                                        0228
         ST    @10,MGPVARPT+4                                      0228
*       MGPVARPT(3)=ADDR(ICTFMLIB(1));/* SET LIB MSG NAME    @ZD00005*/
         LA    @05,ICTFMLIB(,@10)                                  0229
         ST    @05,MGPVARPT+8                                      0229
*       MGPVARPT(4)=ADDR(ICTPTFS(1));/* SET SYSMOD# FOR MSG  @Z67SP00*/
         L     @10,ICTPPTR(,@10)                                   0230
         ST    @10,MGPVARPT+12                                     0230
*       CALL HMASMMSG(HMASMMGP);    /* PRINT MESSAGE                 */
         L     @15,@CV01639                                        0231
         LA    @01,@AL00231                                        0231
         BALR  @14,@15                                             0231
*       ASIRTNCD=ENTNOTFD;          /* IF MOD NOT FOUND THEN SET RTN
*                                      TO INDICATE NOT FOUND AND TO
*                                      CONTINUE PROCESSING( IN CASE
*                                      NO PART OF GEN)       @Z40SP00*/
         LA    ASIRTNCD,6                                          0232
*     END;                                                         0233
*   ELSE                                                           0234
*     IF IOPRETRN=IOPNTOPN          /* WAS THERE AN OPEN/CLOSE ERROR?*/
*       THEN                                                       0234
         B     @RC00221                                            0234
@RF00221 CLI   IOPRETRN(IOPPTR),12                                 0234
         BNE   @RF00234                                            0234
*       DO;                         /* YES, SO WRITE OUT ERROR     0235
*                                      MESSAGE                       */
*         IF ICTFMLIB(1,1:3)=SMP    /* WAS ERROR ON AN SMP DATASET?
*                                                            @ZD00005*/
*           THEN                                                   0236
         L     @10,@PC00001                                        0236
         CLC   ICTFMLIB(3,@10),@CC01667                            0236
         BNE   @RF00236                                            0236
*           ASIRTNCD=STOPSMP;       /* IF SO, STOP SMP               */
         LA    ASIRTNCD,16                                         0237
*         ELSE                                                     0238
*           ASIRTNCD=STOPPTF;       /* ELSE STOP PTF                 */
         B     @RC00236                                            0238
@RF00236 LA    ASIRTNCD,8                                          0238
*       END;                                                       0239
@RC00236 DS    0H                                                  0240
*                                                                  0240
*   /*****************************************************************/
*   /*                                                               */
*   /* MODIFY JFCB OF ICTFMLIB(1) FROM PDS TO MEMBER OF PDS          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0240
*   IF ASIRTNCD=ZERO                /* ANY PREVIOUS ERRORS?          */
*     THEN                                                         0240
@RF00234 DS    0H                                                  0240
@RC00221 CH    ASIRTNCD,@CH00251                                   0240
         BNE   @RF00240                                            0240
*     DO;                           /* NO, SO CONTINUE               */
*       IOPFUNCT=IOPJFCSI;          /* INDICATE ALTER JFCB   @Z40SP00*/
         MVI   IOPFUNCT(IOPPTR),X'05'                              0242
*       IOPDSID=IOPJFCB;            /* INDICATE JFCB                 */
         MVI   IOPDSID(IOPPTR),X'19'                               0243
*       IOPDDNAM=ICTFMLIB(1);       /* SPECIFY DDNAME        @ZD00005*/
         L     @10,@PC00001                                        0244
         MVC   IOPDDNAM(8,IOPPTR),ICTFMLIB(@10)                    0244
*       IOPNAME=ICTMNAME(1);        /* INDICATE MEMBER       @ZD00005*/
         MVC   IOPNAME(8,IOPPTR),ICTMNAME(@10)                     0245
*       IF ICTFMLIB(1)=SMPCDS       /* IS FROMLIB CDS?       @ZD00005*/
*         THEN                                                     0246
         CLC   ICTFMLIB(8,@10),@CC01669                            0246
         BNE   @RF00246                                            0246
*         IOPTYPE=IOPASM;           /* YES, IT IS SO INDICATE ASSEM
*                                      TYPE CDS MEMBER       @ZD00008*/
         MVI   IOPTYPE(IOPPTR),X'01'                               0247
*       ELSE                        /*                       @Z67SP00*/
*         IF ICTFMLIB(1)=SMPSTS     /* IS FROMLIB STS?       @ZD00005*/
*           THEN                                                   0248
         B     @RC00246                                            0248
@RF00246 L     @10,@PC00001                                        0248
         CLC   ICTFMLIB(8,@10),@CC01671                            0248
         BNE   @RF00248                                            0248
*           IOPTYPE=IOPSRC;         /* YES, IT IS SO INDICATE SOURCE
*                                      TYPE CDS MEMBER       @ZD00008*/
         MVI   IOPTYPE(IOPPTR),X'08'                               0249
*         ELSE                                                     0250
*           IOPTYPE=IOPMEM;         /* YES, IT IS SO INDICATE NON- 0250
*                                      SMP TYPE MEMBER       @Z67SP00*/
         B     @RC00248                                            0250
@RF00248 MVI   IOPTYPE(IOPPTR),X'11'                               0250
*       CALL HMASMIO(HMASMIOP);     /* ALTER ICTFMLIB(1)JFCB @ZD00005*/
@RC00248 DS    0H                                                  0251
@RC00246 ST    IOPPTR,@AL00001                                     0251
         L     @15,@CV01638                                        0251
         LA    @01,@AL00001                                        0251
         BALR  @14,@15                                             0251
*       IF IOPRETRN>IOPGOOD         /* WAS THERE I/O ERROR?          */
*         THEN                      /* YES                           */
         CLI   IOPRETRN(IOPPTR),0                                  0252
         BNH   @RF00252                                            0252
*         IF ICTFMLIB(1,1:3)=SMP    /* DID IT OCCUR ON SMP DATASET?
*                                                            @ZD00005*/
*           THEN                                                   0253
         L     @10,@PC00001                                        0253
         CLC   ICTFMLIB(3,@10),@CC01667                            0253
         BNE   @RF00253                                            0253
*           ASIRTNCD=IOPIOERR;      /* YES, THEN INDICATE SEVERE I/O
*                                      ERROR                         */
         LA    ASIRTNCD,16                                         0254
*         ELSE                                                     0255
*           ASIRTNCD=STOPPTF;       /* ELSE STOP PTF                 */
         B     @RC00253                                            0255
@RF00253 LA    ASIRTNCD,8                                          0255
*     END;                                                         0256
@RC00253 DS    0H                                                  0256
@RF00252 DS    0H                                                  0257
*                                                                  0257
*   /*****************************************************************/
*   /*                                                               */
*   /* MODIFY JFCB OF CMPTOLIB FROM PDS TO MEMBER OF PDS             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0257
*   IF ASIRTNCD=ZERO                /* ANY PREVIOUS ERRORS?          */
*     THEN                                                         0257
@RF00240 CH    ASIRTNCD,@CH00251                                   0257
         BNE   @RF00257                                            0257
*     DO;                           /* NO, SO CONTINUE               */
*       IOPTYPE=IOPASM;             /* SMPWRK3 ASSEMBLY TYPE @Z67SP00*/
         MVI   IOPTYPE(IOPPTR),X'01'                               0259
*       IOPNAME=ICTMNAME(1);        /* INDICATE MEMBER NAME  @ZD00005*/
         L     @10,@PC00001                                        0260
         MVC   IOPNAME(8,IOPPTR),ICTMNAME(@10)                     0260
*       IOPFUNCT=IOPJFCSO;          /* INDICATE ALTER JFCB   @Z40SP00*/
         MVI   IOPFUNCT(IOPPTR),X'58'                              0261
*       IOPDSID=IOPJFCB;            /* INDICATE JFCB                 */
         MVI   IOPDSID(IOPPTR),X'19'                               0262
*       IOPDDNAM=SYSPUNCH;          /* INDICATE WORK DATA SET AS OUT
*                                                            @Z67SP00*/
         MVC   IOPDDNAM(8,IOPPTR),SYSPUNCH                         0263
*       CALL HMASMIO(HMASMIOP);     /* ALTER CMPTOLIB JFCB           */
         ST    IOPPTR,@AL00001                                     0264
         L     @15,@CV01638                                        0264
         LA    @01,@AL00001                                        0264
         BALR  @14,@15                                             0264
*       IF IOPRETRN^=IOPGOOD        /* WAS THERE I/O ERROR?          */
*         THEN                                                     0265
         CLI   IOPRETRN(IOPPTR),0                                  0265
         BE    @RF00265                                            0265
*         DO;                       /* YES, THERE WAS                */
*           ASIRTNCD=IOPIOERR;      /* INDICATE SEVERE I/O ERROR     */
         LA    ASIRTNCD,16                                         0267
*           CALL RESETFM;           /* RESET FROM LIB BACK           */
         BAL   @14,RESETFM                                         0268
*         END;                                                     0269
*     END;                          /* END ALTER TOLIB JFCB          */
@RF00265 DS    0H                                                  0271
*                                                                  0271
*   /*****************************************************************/
*   /*                                                               */
*   /* LINK TO ASSEMBLER, PROCESS RETURN CODE, AND PUT OUT MESSAGE   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0271
*   IF ASIRTNCD=ZERO                /* ANY PREVIOUS ERRORS?          */
*     THEN                                                         0271
@RF00257 CH    ASIRTNCD,@CH00251                                   0271
         BNE   @RF00271                                            0271
*     DO;                           /* NO, SO CONTINUE               */
*                                                                  0272
*       /*************************************************************/
*       /*                                                           */
*       /* GET THE USER SUPPLIED PARMS                               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0273
*       DO X=1 BY 1 WHILE CCAASMPM(X)^=BLANK;/* MOVE ANY AND ALL   0273
*                                                            @Z67SP00*/
         LA    X,1                                                 0273
         B     @DE00273                                            0273
@DL00273 DS    0H                                                  0274
*         AOPTIONS(X)=CCAASMPM(X);  /* PARMS IN              @Z67SP00*/
         LA    @10,AOPTIONS-1(X)                                   0274
         L     @01,CCABLDLP(,CCAPTR)                               0274
         ALR   @01,X                                               0274
         MVC   0(1,@10),CCAASMPM-1(@01)                            0274
*       END;                        /*                       @Z67SP00*/
         AH    X,@CH00085                                          0275
@DE00273 L     @10,CCABLDLP(,CCAPTR)                               0275
         LR    @01,@10                                             0275
         ALR   @01,X                                               0275
         CLI   CCAASMPM-1(@01),C' '                                0275
         BNE   @DL00273                                            0275
*       OPTLNG=X-1;                 /* DETERMINE LENGTH OF PARM    0276
*                                      PASSED                @Z67SP00*/
         LR    @15,X                                               0276
         BCTR  @15,0                                               0276
         STH   @15,OPTLNG                                          0276
*       SYSPRINT=CCAASMSP;          /* SYSPRINT DD NAME      @Z67SP00*/
*                                                                  0277
         MVC   SYSPRINT(8),CCAASMSP(@10)                           0277
*       /*************************************************************/
*       /*                                                           */
*       /* TURN ON THE STAE PARAMETER BEFORE DOING ACTUAL ASSEMBLY   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0278
*       SAVRVANM=RVAEPRNM;          /* SAVE PROC NM FROM RVA @Z67SP00*/
         L     @05,CCARVAD(,CCAPTR)                                0278
         MVC   SAVRVANM(8),RVAEPRNM(@05)                           0278
*       RVAEPRNM=CCAASMNM;          /* ASSEMBLER NM FROM CCA@Z67SP00 */
         MVC   RVAEPRNM(8,@05),CCAASMNM(@10)                       0279
*       RFY                                                        0280
*         LNKREG RSTD;              /*                       @Z67SP00*/
*       LNKREG=CCAASM;              /* POINT TO ADDRESS OF ASSEMBLER */
         L     LNKREG,CCAASM(,@10)                                 0281
*       GENERATE CODE REFS(LNKREG,DDLIST,OPLIST) EXIT SEQFLOW;     0282
*                                                                  0282
*                                   /* CALL ASSEMBLER                */
          LINK  DE=(LNKREG),PARAM=(OPLIST,DDLIST),VL=1 LINK TO ASSEM
*       /*************************************************************/
*       /*                                                           */
*       /* TURN OFF THE STAE PARAMETER AFTER DOING THE ASSEMBLY      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0283
*       RFY                                                        0283
*         RTNCODE RSTD;             /* RSTD RETURN CODE REG          */
*       RFY                                                        0284
*         LNKREG UNRSTD;            /* UNRSTD RETURN CODE REG        */
*       RVAEPRNM=SAVRVANM;          /*                       @Z67SP00*/
         L     @14,CCARVAD(,CCAPTR)                                0285
         MVC   RVAEPRNM(8,@14),SAVRVANM                            0285
*       IF RTNCODE>CCARASM          /* WAS RETURN CODE GREATER THAN
*                                      THRESHOLD?                    */
*         THEN                      /* YES, SO STOP PTF              */
         L     @14,CCABLDLP(,CCAPTR)                               0286
         SLR   @10,@10                                             0286
         IC    @10,CCARASM(,@14)                                   0286
         CR    RTNCODE,@10                                         0286
         BNH   @RF00286                                            0286
*         DO;                       /*                       @Z40SP00*/
*           ASIRTNCD=STOPPTF;       /* SET RETURN CODE       @Z40SP00*/
         LA    ASIRTNCD,8                                          0288
*           MGPTYPE=MGPERR;         /* SET SEVERITY          @MS00001*/
         MVI   MGPTYPE,X'03'                                       0289
*           MGPVARPT(1)=ADDR(TXTFAIL);/* SET MSG PART        @Z40SP00*/
         LA    @14,@CC01682                                        0290
         ST    @14,MGPVARPT                                        0290
*         END;                      /*                       @Z40SP00*/
*       ELSE                        /* NO IT WAS LESS                */
*         DO;                       /*                       @Z40SP00*/
         B     @RC00286                                            0292
@RF00286 DS    0H                                                  0293
*           MGPVARPT(1)=ADDR(TXTSUCC);/* SET MSG INDICATOR   @Z40SP00*/
         LA    @14,@CC01679                                        0293
         ST    @14,MGPVARPT                                        0293
*           IF RTNCODE^=IOPGOOD     /* BUT IF IT WASN'T GOOD         */
*             THEN                  /* THEN FLAG WARNING TO SYSMOD   */
         LTR   RTNCODE,RTNCODE                                     0294
         BZ    @RF00294                                            0294
*             DO;                   /* AND SET MSG SEVERITY  @MS00001*/
*               MGPTYPE=MGPWARN;    /* SET SEVERITY          @MS00001*/
         MVI   MGPTYPE,X'02'                                       0296
*               ASIRTNCD=PTFWARN;   /*                       @Z40SP00*/
         LA    ASIRTNCD,4                                          0297
*             END;                  /* SEV&FLAG SET          @MS00001*/
*           ELSE                                                   0299
*             MGPTYPE=MGPINFO;      /* SET SEVERITY OF MSG   @MS00001*/
         B     @RC00294                                            0299
@RF00294 MVI   MGPTYPE,X'01'                                       0299
*         END;                      /*                       @Z40SP00*/
@RC00294 DS    0H                                                  0301
*       CVD(RTNCODE,DECNO);         /* CONVERT THE RETURN CODE TO  0301
*                                      PACKED DECIMAL                */
@RC00286 CVD   RTNCODE,DECNO                                       0301
*       RFY                                                        0302
*         RTNCODE UNRSTD;           /* UNRSTD RETRN CODE REG         */
*       UNPK(RETURNCD(1:2),DECNO(7:8));/* UNPACK RETURN CODE FOR   0303
*                                      PRINTING IN MSG               */
         UNPK  RETURNCD(2),DECNO+6(2)                              0303
*       RETURNCD(2)=RETURNCD(2)×ZONE;/* OR IN ZONE                   */
         OI    RETURNCD+1,C'0'                                     0304
*       MGPCLEAR=MGPCLEAR&&MGPCLEAR;/* ZERO MSG NOS          @MS00001*/
         XC    MGPCLEAR(6),MGPCLEAR                                0305
*       MGPMGNO1=HMA240I;           /* ASSEMBLY XXXXXXXXXX -       0306
*                                      MOD=XXXXXXXX -              0306
*                                      LIBRARY=XXXXXXXX -          0306
*                                      SYSMOD=XXXXXXX - RETURN     0306
*                                      CODE=XX               @Z67SP00*/
         MVC   MGPMGNO1(2),@CH00491                                0306
*       MGPVARPT(2)=ADDR(ICTMNAME(1));/* SET MOD MSG NAME    @ZD00005*/
         L     @10,@PC00001                                        0307
         ST    @10,MGPVARPT+4                                      0307
*       MGPVARPT(3)=ADDR(ICTFMLIB(1));/* SET LIBRARY INSERT  @ZD00001*/
         LA    @05,ICTFMLIB(,@10)                                  0308
         ST    @05,MGPVARPT+8                                      0308
*       MGPVARPT(4)=ADDR(ICTPTFS(1));/* SET SYSMOD NO. INSERT      0309
*                                                            @Z67SP00*/
         L     @10,ICTPPTR(,@10)                                   0309
         ST    @10,MGPVARPT+12                                     0309
*       MGPVARPT(5)=ADDR(RETURNCD); /* SET RET CODE FOR MSG  @ZD00005*/
         LA    @10,RETURNCD                                        0310
         ST    @10,MGPVARPT+16                                     0310
*       CALL HMASMMSG(HMASMMGP);    /* PRINT MESSAGE                 */
         L     @15,@CV01639                                        0311
         LA    @01,@AL00311                                        0311
         BALR  @14,@15                                             0311
         EJECT
*                                                                  0312
*       /*************************************************************/
*       /*                                                           */
*       /* RESET JFCB FOR CMPTOLIB                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0312
*       IOPFUNCT=IOPJFCBP;          /* INDICATE ALTER JFCB           */
         MVI   IOPFUNCT(IOPPTR),X'06'                              0312
*       IOPDSID=IOPJFCB;            /* INDICATE JFCB                 */
         MVI   IOPDSID(IOPPTR),X'19'                               0313
*       IOPDDNAM=SYSPUNCH;          /* INDICATE DDNAME               */
         MVC   IOPDDNAM(8,IOPPTR),SYSPUNCH                         0314
*       CALL HMASMIO(HMASMIOP);     /* ALTER JFCB                    */
         ST    IOPPTR,@AL00001                                     0315
         L     @15,@CV01638                                        0315
         LA    @01,@AL00001                                        0315
         BALR  @14,@15                                             0315
*       IF IOPRETRN^=IOPGOOD        /* WAS THERE I/O ERROR?          */
*         THEN                                                     0316
         CLI   IOPRETRN(IOPPTR),0                                  0316
         BE    @RF00316                                            0316
*         ASIRTNCD=IOPIOERR;        /* YES THERE SO INDICATE SEVERE
*                                      I/O ERROR                     */
*                                                                  0317
         LA    ASIRTNCD,16                                         0317
*       /*************************************************************/
*       /*                                                           */
