         TITLE 'HMASMLCP - LIST PTS ROUTINE'
HMASMLCP CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMLCP  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(12),0(@01)                                 0001
*                                                                  0176
*   /*****************************************************************/
*   /*                                                               */
*   /* DO INITIALIZATION                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0176
*   IOPPTR=ADDR(PASSIOP);           /* SET PTR TO PASSED IOP         */
         L     @10,@PC00001+4                                      0176
         ST    @10,IOPPTR                                          0176
*   SAVBUFAD=IOPBUFAD;              /*                               */
         L     @15,IOPBUFAD(,@10)                                  0177
         ST    @15,SAVBUFAD                                        0177
*   RFY                                                            0178
*     RTNCODE RSTD;                 /*                               */
*   RTNCODE=ZERO;                   /* INIT RETURN CODE              */
         SLR   RTNCODE,RTNCODE                                     0179
*   IF IOPTYPE^=LASTTYPE            /*                               */
*       &IOPTYPE^=IOPSMD            /*                               */
*       &RTNCODE=ZERO               /*                               */
*     THEN                          /*                               */
         L     @14,@PC00001+8                                      0180
         CLC   IOPTYPE(1,@10),LASTTYPE(@14)                        0180
         BE    @RF00180                                            0180
         CLI   IOPTYPE(@10),11                                     0180
         BE    @RF00180                                            0180
         LTR   RTNCODE,RTNCODE                                     0180
         BNZ   @RF00180                                            0180
*     CALL LCPSETHD;                /*                               */
*                                                                  0181
         BAL   @14,LCPSETHD                                        0181
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL PROPER SUBROUTINE OR MODULE TO FORMAT SPECIFIED ENTRY    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0182
*   IF RTNCODE=ZERO                 /*                       @Z67SP00*/
*     THEN                          /*                       @Z67SP00*/
@RF00180 LTR   RTNCODE,RTNCODE                                     0182
         BNZ   @RF00182                                            0182
*     IF IOPTYPE=IOPSMD             /* IF SYSMOD TYPE ENTRY          */
*       THEN                        /*                               */
         L     @14,IOPPTR                                          0183
         CLI   IOPTYPE(@14),11                                     0183
         BNE   @RF00183                                            0183
*       CALL HMASMFPT(LISTPARM,HMASMIOP,LASTTYPE);/* FORMAT SYSMOD   */
         L     @10,@PC00001                                        0184
         ST    @10,@AL00001                                        0184
         ST    @14,@AL00001+4                                      0184
         L     @14,@PC00001+8                                      0184
         ST    @14,@AL00001+8                                      0184
         L     @15,@CV01393                                        0184
         LA    @01,@AL00001                                        0184
         BALR  @14,@15                                             0184
*     ELSE                          /* IF NOT SYSMOD ENTRY           */
*       IF IOPTYPE=IOPMCS           /* IF MCS ENTRY                  */
*         THEN                      /*                               */
         B     @RC00183                                            0185
@RF00183 L     @14,IOPPTR                                          0185
         CLI   IOPTYPE(@14),16                                     0185
         BNE   @RF00185                                            0185
*         CALL LCPMCS;              /* FORMAT MCS ENTRY              */
         BAL   @14,LCPMCS                                          0186
*       ELSE                        /* IF NOT MCS ENTRY              */
*         IF IOPTYPE=IOPSYS         /* IF SYSTEM ENTRY               */
*           THEN                    /*                               */
         B     @RC00185                                            0187
@RF00185 L     @14,IOPPTR                                          0187
         CLI   IOPTYPE(@14),7                                      0187
         BNE   @RF00187                                            0187
*           CALL LCPSYS;            /* FORMAT SYSTEM ENTRY           */
         BAL   @14,LCPSYS                                          0188
*         ELSE                      /*                               */
*           RTNCODE=ZERO;           /* MUST BE ENTRY TYPE NOT      0189
*                                      FORMATTED FOR USER            */
*                                                                  0189
         B     @RC00187                                            0189
@RF00187 SLR   RTNCODE,RTNCODE                                     0189
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ENTRY WAS FORMATTED PRINT BLANK LINE TO SEPERATE           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0190
*   IF RTNCODE=ZERO                 /*                               */
*     THEN                          /*                               */
@RC00187 DS    0H                                                  0190
@RC00185 DS    0H                                                  0190
@RC00183 DS    0H                                                  0190
@RF00182 LTR   RTNCODE,RTNCODE                                     0190
         BNZ   @RF00190                                            0190
*     CALL LCPWRITE;                /*                               */
*                                                                  0191
         BAL   @14,LCPWRITE                                        0191
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY FORMATTED NOW RETURN TO CALLER                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0192
*   RETURN CODE(RTNCODE);           /*                               */
@RF00190 L     @13,4(,@13)                                         0192
         L     @14,12(,@13)                                        0192
         LM    @00,@12,20(@13)                                     0192
         BR    @14                                                 0192
*   RFY                                                            0193
*     RTNCODE UNRSTD;               /*                               */
         EJECT
*                                                                  0194
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINE USED TO RESET THE HMASMIO HEADING BUFFERS             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0194
*LCPSETHD:                                                         0194
*   PROCEDURE;                                                     0194
@EL00001 L     @13,4(,@13)                                         0194
@EF00001 DS    0H                                                  0194
@ER00001 LM    @14,@12,12(@13)                                     0194
         BR    @14                                                 0194
LCPSETHD STM   @14,@12,@SA00002                                    0194
*   RFY                                                            0195
*     HMASMIOP BASED(IOPPTR);       /*                               */
*   DECLARE                         /*                               */
*     HDINDX FIXED(31);             /* LOOP CONTER                   */
*   DECLARE                         /*                               */
*     SETDSID FIXED(8);             /* SAVE AREA FOR IOPDSID         */
*                                                                  0197
*   /*****************************************************************/
*   /*                                                               */
*   /* SET ENTRY TYPE IN HEADING LINE                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0198
*   SETDSID=IOPDSID;                /* SAVE DSID                     */
         L     @10,IOPPTR                                          0198
         IC    @15,IOPDSID(,@10)                                   0198
         STC   @15,SETDSID                                         0198
*   IF IOPTYPE=IOPSYS               /*                               */
*     THEN                          /*                               */
         CLI   IOPTYPE(@10),7                                      0199
         BNE   @RF00199                                            0199
*     HDTYPE=TXTSYSHD;              /*                               */
         MVC   HDTYPE(19),TXTSYSHD                                 0200
*   ELSE                            /*                               */
*     IF IOPTYPE=IOPMCS             /*                               */
*       THEN                        /*                               */
         B     @RC00199                                            0201
@RF00199 L     @10,IOPPTR                                          0201
         CLI   IOPTYPE(@10),16                                     0201
         BNE   @RF00201                                            0201
*       HDTYPE=TXTMCSHD;            /*                               */
*                                                                  0202
         MVC   HDTYPE(19),TXTMCSHD                                 0202
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP IOP TO DELETE EXISTING HEADINGS AND CALL IO TO DELETE  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0203
*   IOPBUFAD=ADDR(OUTAREA);         /* AND AIM AT THE PRINT LINE     */
@RF00201 DS    0H                                                  0203
@RC00199 L     @10,IOPPTR                                          0203
         LA    @15,OUTAREA                                         0203
         ST    @15,IOPBUFAD(,@10)                                  0203
*   IOPDSID=CCALSTDS;               /* INDICATE PRINTER DATA SET     */
         IC    @14,CCALSTDS(,CCAPTR)                               0204
         STC   @14,IOPDSID(,@10)                                   0204
*   IOPFUNCT=IOPWRITE;              /* FOR OUTPUT                    */
         MVI   IOPFUNCT(@10),X'51'                                 0205
*   PRLHDADR=ZERO;                  /* ZERO OUT HEADING ADDRESS      */
         SLR   @14,@14                                             0206
         STCM  @14,7,PRLHDADR(@15)                                 0206
*   PRLRLEN=ZERO;                   /* INDICATE ZERO RECORD LENGTH 0207
*                                      USED TO RESET HEADINGS        */
         STCM  @14,3,PRLRLEN(@15)                                  0207
*   PRLFLGS=OFF;                    /* RESET ALL HEADINGS            */
         MVI   PRLFLGS(@15),B'00000000'                            0208
*   PRLHEADO=ON;                    /* WITH THESE TWO PARMS          */
         OI    PRLHEADO(@15),B'01000000'                           0209
*   CALL HMASMIO(HMASMIOP);         /* CALL ON THE I/O ROUTINE       */
*                                                                  0210
         ST    @10,@AL00001                                        0210
         L     @15,@CV01395                                        0210
         LA    @01,@AL00001                                        0210
         BALR  @14,@15                                             0210
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP ADDING ALL SYSMOD HEADINGS                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0211
*   PRLRLEN=LENGTH(PRNTBUF);        /* SET STANDARD LENGTH           */
         L     @10,IOPPTR                                          0211
         L     @10,IOPBUFAD(,@10)                                  0211
         MVC   PRLRLEN(2,@10),@CH01414                             0211
*   PRLHEADO=OFF;                   /* KILL THE RESET SWITCH         */
*   PRLHEAD=ON;                     /* SET IN THE NEW HEADING        */
         OI    PRLHEAD(@10),B'10000000'                            0213
         NI    PRLHEADO(@10),B'10111111'                           0213
*   DO HDINDX=1 TO DIM(HEADPTR)     /* LOOP USING ALL THE HD PTRS    */
*         WHILE(IOPRETRN=IOPGOOD);  /*                               */
         LA    @10,1                                               0214
         ST    @10,HDINDX                                          0214
@DL00214 L     @15,IOPPTR                                          0214
         CLI   IOPRETRN(@15),0                                     0214
         BNE   @DC00214                                            0214
*     IF HEADPTR(HDINDX)=ZERO       /* BLANK HEADING?                */
*       THEN                                                       0215
         SLR   @14,@14                                             0215
         SLA   @10,2                                               0215
         C     @14,HEADPTR-4(@10)                                  0215
         BNE   @RF00215                                            0215
*       DO;                                                        0216
*         PRLBLANK=ON;              /* INDICATE BLANK HEADING        */
         L     @10,IOPBUFAD(,@15)                                  0217
         OI    PRLBLANK(@10),B'00100000'                           0217
*         PRLHDADR=ZERO;            /* CLEAR HEADING ADDRESS         */
         STCM  @14,7,PRLHDADR(@10)                                 0218
*       END;                                                       0219
*     ELSE                                                         0220
*       DO;                         /* NO PASS A HEADING ADDRESS     */
         B     @RC00215                                            0220
@RF00215 DS    0H                                                  0221
*         PRLBLANK=OFF;             /* NOT A BLANK LINE              */
         L     @10,IOPPTR                                          0221
         L     @10,IOPBUFAD(,@10)                                  0221
         NI    PRLBLANK(@10),B'11011111'                           0221
*         PRLHDADR=HEADPTR(HDINDX); /* INSERT THE HEAD ADDR          */
         L     @15,HDINDX                                          0222
         SLA   @15,2                                               0222
         L     @15,HEADPTR-4(@15)                                  0222
         STCM  @15,7,PRLHDADR(@10)                                 0222
*       END;                                                       0223
*     CALL HMASMIO(HMASMIOP);       /* PASS TO I/O ROUTINE           */
@RC00215 L     @10,IOPPTR                                          0224
         ST    @10,@AL00001                                        0224
         L     @15,@CV01395                                        0224
         LA    @01,@AL00001                                        0224
         BALR  @14,@15                                             0224
*   END;                                                           0225
         LA    @10,1                                               0225
         AL    @10,HDINDX                                          0225
         ST    @10,HDINDX                                          0225
         C     @10,@CF00066                                        0225
         BNH   @DL00214                                            0225
@DC00214 DS    0H                                                  0226
*   PRLFLGS=OFF;                    /* RESET FLAGS                   */
         L     @10,IOPPTR                                          0226
         L     @01,IOPBUFAD(,@10)                                  0226
         MVI   PRLFLGS(@01),B'00000000'                            0226
*   IOPBUFAD=SAVBUFAD;              /* RESTORE BUFFER ADDRESS        */
         L     @15,SAVBUFAD                                        0227
         ST    @15,IOPBUFAD(,@10)                                  0227
*   IOPDSID=SETDSID;                /*                               */
         IC    @15,SETDSID                                         0228
         STC   @15,IOPDSID(,@10)                                   0228
*   RETURN CODE(IOPRETRN);          /*                               */
         SLR   @15,@15                                             0229
         IC    @15,IOPRETRN(,@10)                                  0229
         L     @14,@SA00002                                        0229
         LM    @00,@12,@SA00002+8                                  0229
         BR    @14                                                 0229
*   END LCPSETHD;                   /* RETURN TO CALLER              */
         EJECT
*                                                                  0231
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINE CALLED TO FORMAT PTS MCS ENTRIES                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0231
*LCPMCS:                            /*                               */
*   PROCEDURE;                      /*                               */
LCPMCS   STM   @14,@12,@SA00003                                    0231
*   RFY                                                            0232
*     HMASMIOP BASED(IOPPTR);       /*                               */
*   RFY                                                            0233
*     RTNCODE RSTD;                 /*                               */
*   RTNCODE=ZERO;                   /*                               */
*                                                                  0234
         SLR   RTNCODE,RTNCODE                                     0234
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP SCP AND CRP PARAMETER LISTS                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0235
*   HMASMSCP=''B;                   /*                               */
         XC    HMASMSCP(26),HMASMSCP                               0235
*   SCPCHAR=ADDR(CRPBUFFR);         /* BUFFER ADDR                   */
         LA    @14,CRPBUFFR                                        0236
         ST    @14,SCPCHAR                                         0236
*   SCPSRCH=ADDR(KPER1);            /* FIRST DSCAN ADDR              */
         LA    @14,KPER1                                           0237
         ST    @14,SCPSRCH                                         0237
*   SCPIORTN=ADDR(LCPIORTN);        /* IO ROUTINE                    */
         LA    @14,LCPIORTN                                        0238
         ST    @14,SCPIORTN                                        0238
*   SCPIOPAD=ADDR(HMASMCRP);        /* PARM TO IO ROUTINE            */
         LA    @14,HMASMCRP                                        0239
         ST    @14,SCPIOPAD                                        0239
*   SCPINLN=LENGTH(CRPBFDTA);       /* LENGTH OF DATA IN BUFFER      */
         MVC   SCPINLN(2),@CH01094                                 0240
*   SCPCOMNT=ON;                    /* SMP COMMENTS VALID            */
         OI    SCPCOMNT,B'00100000'                                0241
*   HMASMCRP=''B;                   /*                               */
         XC    HMASMCRP(106),HMASMCRP                              0242
*   CRPSCPAD=ADDR(HMASMSCP);        /* ADDR OF SCP                   */
         LA    @14,HMASMSCP                                        0243
         ST    @14,CRPSCPAD                                        0243
*   CRPINDS=IOPPTS;                 /* INPUT FROM PTS                */
         MVI   CRPINDS,X'06'                                       0244
*   HDSUBHD=TXTMCSHD;               /*                               */
         MVC   HDSUBHD(15),TXTMCSHD                                0245
*   HDSEPCHR=TXTEQ;                 /*                               */
         MVI   HDSEPCHR,C'='                                       0246
*   HDNAME=IOPNAME;                 /*                               */
*                                                                  0247
         L     @14,IOPPTR                                          0247
         MVC   HDNAME(8),IOPNAME(@14)                              0247
*   /*****************************************************************/
*   /*                                                               */
*   /* FORMAT MCS CONTROL CARDS                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0248
*   IF RTNCODE=ZERO                 /*                               */
*     THEN                          /*                               */
         LTR   RTNCODE,RTNCODE                                     0248
         BNZ   @RF00248                                            0248
*     DO;                           /*                               */
*       IOPFUNCT=IOPPOSI;           /*                               */
         MVI   IOPFUNCT(@14),X'02'                                 0250
*       CALL HMASMIO(HMASMIOP);     /* POSITION FOR INPUT            */
         ST    @14,@AL00001                                        0251
         L     @15,@CV01395                                        0251
         LA    @01,@AL00001                                        0251
         BALR  @14,@15                                             0251
*       IF IOPRETRN=IOPGOOD         /*                               */
*         THEN                      /*                               */
         L     @14,IOPPTR                                          0252
         CLI   IOPRETRN(@14),0                                     0252
         BNE   @RF00252                                            0252
*         DO;                       /*                               */
*           CALL LCPIORTN;          /* READ AND PRINT FIRST CARD     */
         BAL   @14,LCPIORTN                                        0254
*           CRPRETRN=RTNCODE;       /* SET RC                        */
         STC   RTNCODE,CRPRETRN                                    0255
*           DO WHILE(               /* LOOP READING AND PRINTING     */
*                 CRPRETRN=CRPGOOD);/* TILL EOF OR ERR               */
         B     @DE00256                                            0256
@DL00256 DS    0H                                                  0257
*             CALL HMASMSCN(HMASMSCP);/* SCAN CONTROL CARD SO THAT 0257
*                                      CONTINUATION CARDS WILL RESULT
*                                      IN IO EXIT BEING CALLED SO  0257
*                                      THEY CAN ALSO BE PRINTED      */
         L     @15,@CV01396                                        0257
         LA    @01,@AL00257                                        0257
         BALR  @14,@15                                             0257
*             CRPRETRN=SCPRETRN;    /* SAVE RC                       */
         IC    @14,SCPRETRN                                        0258
         STC   @14,CRPRETRN                                        0258
*             DO UNTIL(             /* NOW READ ALL DATA CARDS       */
*                   CRPRETRN^=CRPGOOD/* BETWEEN CONTROL CARDS        */
*                   ×CRPBUFFR(1:2)=PLUSPLUS);/*                      */
@DL00259 DS    0H                                                  0260
*               CALL HMASMCRD(HMASMCRP);/* READ CARD                 */
         L     @15,@CV01392                                        0260
         LA    @01,@AL00260                                        0260
         BALR  @14,@15                                             0260
*             END;                  /*                               */
@DE00259 CLI   CRPRETRN,0                                          0261
         BNE   @DC00259                                            0261
         CLC   CRPBUFFR(2),@CC01400                                0261
         BNE   @DL00259                                            0261
@DC00259 DS    0H                                                  0262
*             IF CRPRETRN=CRPGOOD   /* IF NEXT CARD READ OK          */
*                 &CRPBFDTA(1:2)=PLUSPLUS/* IF CONTROL CARD (NOT EOF)*/
*               THEN                /* THEN                          */
         CLI   CRPRETRN,0                                          0262
         BNE   @RF00262                                            0262
         CLC   CRPBFDTA(2),@CC01400                                0262
         BNE   @RF00262                                            0262
*               DO;                 /*                               */
*                 HDMCS=CRPBUFFR;   /* WRITE IT OUT                  */
         MVC   HDMCS(80),CRPBUFFR                                  0264
*                 CALL LCPWRITE;    /*                               */
         BAL   @14,LCPWRITE                                        0265
*                 CRPRETRN=RTNCODE; /* SAVE RC FOR LOOP CNTL         */
         STC   RTNCODE,CRPRETRN                                    0266
*               END;                /*                               */
*           END;                    /* END PTS READ LOOP             */
@RF00262 DS    0H                                                  0268
@DE00256 CLI   CRPRETRN,0                                          0268
         BE    @DL00256                                            0268
*           IF CRPRETRN<=CRPEOF     /* IF NO ERROR                   */
*             THEN                  /*                               */
         CLI   CRPRETRN,4                                          0269
         BH    @RF00269                                            0269
*             RTNCODE=ZERO;         /* SET RC                        */
         SLR   RTNCODE,RTNCODE                                     0270
*           ELSE                    /*                               */
*             RTNCODE=CRPRETRN;     /* PASS ALONG RC                 */
         B     @RC00269                                            0271
@RF00269 SLR   RTNCODE,RTNCODE                                     0271
         IC    RTNCODE,CRPRETRN                                    0271
*         END;                      /*                               */
*       ELSE                        /*                               */
*         RTNCODE=IOPRETRN;         /* PASS ALONG RC                 */
         B     @RC00252                                            0273
@RF00252 L     @10,IOPPTR                                          0273
         SLR   RTNCODE,RTNCODE                                     0273
         IC    RTNCODE,IOPRETRN(,@10)                              0273
*     END;                          /*                               */
@RC00252 DS    0H                                                  0275
*   RETURN CODE(RTNCODE);           /*                               */
@RF00248 L     @14,@SA00003                                        0275
         LM    @00,@12,@SA00003+8                                  0275
         BR    @14                                                 0275
*   RFY                                                            0276
*     RTNCODE UNRSTD;               /*                               */
*   END LCPMCS;                     /*                               */
         EJECT
*                                                                  0278
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINE CALLED TO FORMAT PTS SYSTEM ENTRY                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0278
*LCPSYS:                            /*                               */
*   PROCEDURE;                      /*                               */
LCPSYS   STM   @14,@12,@SA00004                                    0278
*   RFY                                                            0279
*     HMASMIOP BASED(IOPPTR);       /*                               */
*   RFY                                                            0280
*     RTNCODE RSTD;                 /*                               */
*   RTNCODE=ZERO;                   /*                               */
         SLR   RTNCODE,RTNCODE                                     0281
*   HDNAME=IOPNAME;                 /*                               */
         L     @14,IOPPTR                                          0282
         MVC   HDNAME(8),IOPNAME(@14)                              0282
*   HDSEPCHR=TXTEQ;                 /*                               */
*                                                                  0283
         MVI   HDSEPCHR,C'='                                       0283
*   /*****************************************************************/
*   /*                                                               */
*   /* FORMAT SYSTEM ENTRY OPTION LINE                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0284
*   IF RTNCODE=ZERO                 /*                               */
*     THEN                          /*                               */
         LTR   RTNCODE,RTNCODE                                     0284
         BNZ   @RF00284                                            0284
*     DO;                           /*                               */
*       HDSUBHD=TXTOPT;             /*                               */
         MVC   HDSUBHD(15),@CC01469                                0286
*       HDSEPCHR=TXTEQ;             /*                               */
         MVI   HDSEPCHR,C'='                                       0287
*       HDPURGEH=TXTPURGE;          /*                               */
         MVC   HDPURGEH(6),@CC01476                                0288
*       IF IOPPTSNP=OFF             /*                               */
*         THEN                      /*                               */
         TM    IOPPTSNP(@14),B'10000000'                           0289
         BNZ   @RF00289                                            0289
*         HDPURGEV=TXTYES;          /*                               */
         MVC   HDPURGEV(3),@CC01482                                0290
*       ELSE                        /*                               */
*         HDPURGEV=TXTNO;           /*                               */
         B     @RC00289                                            0291
@RF00289 MVC   HDPURGEV(3),@CC01484                                0291
*       HDREJH=TXTREJ;              /*                               */
@RC00289 MVC   HDREJH(7),@CC01478                                  0292
*       IF IOPPTSNJ=OFF             /*                               */
*         THEN                      /*                               */
         L     @14,IOPPTR                                          0293
         TM    IOPPTSNJ(@14),B'01000000'                           0293
         BNZ   @RF00293                                            0293
*         HDREJV=TXTYES;            /*                               */
         MVC   HDREJV(3),@CC01482                                  0294
*       ELSE                        /*                               */
*         HDREJV=TXTNO;             /*                               */
         B     @RC00293                                            0295
@RF00293 MVC   HDREJV(3),@CC01484                                  0295
*       HDPAGLNH=TXTPAGLN;          /*                               */
@RC00293 MVC   HDPAGLNH(8),@CC01474                                0296
*       IF IOPSYPLN^=ZERO           /*                               */
*         THEN                      /*                               */
         L     @14,IOPPTR                                          0297
         LH    @14,IOPSYPLN(,@14)                                  0297
         LTR   @14,@14                                             0297
         BZ    @RF00297                                            0297
*         DO;                       /*                               */
*           CVD(IOPSYPLN,CVDWORK);  /*                               */
         CVD   @14,@TS00001                                        0299
         MVC   CVDWORK(4),@TS00001+4                               0299
