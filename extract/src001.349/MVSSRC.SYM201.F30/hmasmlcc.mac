         TITLE 'HMASMLCC  ** LIST THE SMPCRQ/SMPACRQ **'
HMASMLCC CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMLCC  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(12),0(@01)                                 0001
*                                                                  0178
*   /*****************************************************************/
*   /*                                                               */
*   /* M A I N R O U T I N E                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0178
*   LCCRC=ZERO;                     /*                               */
         SLR   LCCRC,LCCRC                                         0178
*   IOPPTR=ADDR(PASSIOP);           /* INITIAL SETUP/ADDRESSABILITY  */
         L     IOPPTR,@PC00001+4                                   0179
*   GTPPTR1=CCAGTP1;                /*                               */
         L     GTPPTR1,CCAGTP1(,CCAPTR)                            0180
*   RFY                                                            0181
*     HMASMIOP BASED(IOPPTR);       /*                               */
*   RFY                                                            0182
*     HMASMGTP BASED(GTPPTR1);      /*                               */
*   PRTBUF(1:LENGTH(PRTBUF))=' ';   /* CLEANUP THE PRINTBUFFER       */
         MVI   PRTBUF+1,C' '                                       0183
         MVC   PRTBUF+2(119),PRTBUF+1                              0183
         MVI   PRTBUF,C' '                                         0183
*   IF LASTTYPE^=IOPTYPE THEN       /*                               */
         L     @10,@PC00001+8                                      0184
         CLC   LASTTYPE(1,@10),IOPTYPE(IOPPTR)                     0184
         BE    @RF00184                                            0184
*     DO;                           /*                               */
*       CALL LCCHDR;                /*                               */
         BAL   @14,LCCHDR                                          0186
*       RFY                                                        0187
*         RTNCODE RSTD;             /*                               */
*       LCCRC=MAX(LCCRC,RTNCODE);   /*                               */
         CR    LCCRC,RTNCODE                                       0188
         BNL   *+6
         LR    LCCRC,RTNCODE                                       0188
*       RFY                                                        0189
*         RTNCODE UNRSTD;           /*                               */
*     END;                          /*                               */
*   IF LCCRC=ZERO THEN              /*                               */
@RF00184 CH    LCCRC,@CH00307                                      0191
         BNE   @RF00191                                            0191
*     DO;                           /*                               */
*       CALL LCCWRITE;              /* PRINT OUT A BLANK LINE        */
         BAL   @14,LCCWRITE                                        0193
*       LCCRC=MAX(LCCRC,IOPRETRN);  /* FOR EACH NEW NAME TO BE LISTED*/
         SLR   @10,@10                                             0194
         IC    @10,IOPRETRN(,IOPPTR)                               0194
         CR    LCCRC,@10                                           0194
         BNL   *+6
         LR    LCCRC,@10                                           0194
*     END;                          /*                               */
*                                                                  0196
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE WHICH PROC TO CALL TO PROCESS THE REQUEST           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0196
*   IF LCCRC=ZERO&LISTSMD=ON&IOPTYPE=IOPCSR THEN                   0196
@RF00191 CH    LCCRC,@CH00307                                      0196
         BNE   @RF00196                                            0196
         L     @10,@PC00001                                        0196
         TM    LISTSMD(@10),B'10000000'                            0196
         BNO   @RF00196                                            0196
         CLI   IOPTYPE(IOPPTR),15                                  0196
         BNE   @RF00196                                            0196
*     CALL LCCSMD;                  /*                               */
         BAL   @14,LCCSMD                                          0197
*   ELSE                            /*                               */
*     IF LCCRC=ZERO&LISTFMD=ON&IOPTYPE=IOPFMD THEN                 0198
         B     @RC00196                                            0198
@RF00196 CH    LCCRC,@CH00307                                      0198
         BNE   @RF00198                                            0198
         L     @10,@PC00001                                        0198
         TM    LISTFMD(@10),B'01000000'                            0198
         BNO   @RF00198                                            0198
         CLI   IOPTYPE(IOPPTR),14                                  0198
         BNE   @RF00198                                            0198
*       CALL LCCFMD;                /*                               */
         BAL   @14,LCCFMD                                          0199
*     ELSE                          /*                               */
*       CALL LCCFUL;                /*                               */
         B     @RC00198                                            0200
@RF00198 BAL   @14,LCCFUL                                          0200
*   RFY                                                            0201
*     RTNCODE RSTD;                 /*                               */
@RC00198 DS    0H                                                  0201
@RC00196 DS    0H                                                  0202
*   LCCRC=MAX(LCCRC,RTNCODE);       /*                               */
         CR    LCCRC,RTNCODE                                       0202
         BNL   *+6
         LR    LCCRC,RTNCODE                                       0202
*   RFY                                                            0203
*     RTNCODE UNRSTD;               /*                               */
*   RETURN CODE(LCCRC);             /*                               */
         LR    @15,LCCRC                                           0204
         L     @13,4(,@13)                                         0204
         L     @14,12(,@13)                                        0204
         LM    @00,@12,20(@13)                                     0204
         BR    @14                                                 0204
*                                                                  0205
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS SYSMOD REQUEST                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0205
*LCCSMD:                                                           0205
*   PROCEDURE;                      /*                               */
LCCSMD   STM   @14,@12,@SA00002                                    0205
*   DCL                                                            0206
*     SMDRC FIXED(8);               /*                               */
*   RFY                                                            0207
*     HMASMIOP BASED(IOPPTR);       /*                               */
*   RFY                                                            0208
*     HMASMGTP BASED(GTPPTR1);      /*                               */
*   SMDRC=ZERO;                     /* RESET RETURN CODES AND      0209
*                                      SWITCHES                      */
         SLR   SMDRC,SMDRC                                         0209
*   SWITCH=OFF;                     /*                               */
         NI    SWITCH,B'01111111'                                  0210
*   LASTENV=' ';                    /* RESET THE LAST ENV PROCESSED  */
         MVI   LASTENV+1,C' '                                      0211
         MVC   LASTENV+2(5),LASTENV+1                              0211
         MVI   LASTENV,C' '                                        0211
*   CALL LCCSMSRT;                  /* BUILD A SORTED GTA FILE       */
         BAL   @14,LCCSMSRT                                        0212
*   RFY                                                            0213
*     RTNCODE RSTD;                 /*                               */
*   SMDRC=MAX(SMDRC,RTNCODE);       /*                               */
         CR    SMDRC,RTNCODE                                       0214
         BNL   *+6
         LR    SMDRC,RTNCODE                                       0214
*   RFY                                                            0215
*     RTNCODE UNRSTD;               /*                               */
*   IF SMDRC=ZERO THEN              /*                               */
         CH    SMDRC,@CH00307                                      0216
         BNE   @RF00216                                            0216
*     DO;                           /* RESET POINTERS TO START AT THE*/
*       GTPFUNCT=GTPOPENN;          /* TOP OF THE FILE..GTPRCDLN WAS */
         MVI   GTPFUNCT(GTPPTR1),X'01'                             0218
*       CALL HMASMGTA(HMASMGTP);    /* SET IN LCCSMSRT               */
         ST    GTPPTR1,@AL00001                                    0219
         L     @15,@CV01329                                        0219
         LA    @01,@AL00001                                        0219
         BALR  @14,@15                                             0219
*                                   /* SETUP THE GTP FOR GETNEXT   0220
*                                      PROCESSING..THE GTPRCDLN AND  */
*       GTPFUNCT=GTPGETN;           /* GTPBUFAD WERE SET IN LCCSMSRT */
         MVI   GTPFUNCT(GTPPTR1),X'08'                             0220
*       NAME2=IOPNAME;              /* PUT THE NAME IN THE PRINT BUF */
         MVC   NAME2(8),IOPNAME(IOPPTR)                            0221
*     END;                          /*                               */
*   DO WHILE SMDRC=ZERO&GTPRETRN=GTPGOOD;                          0223
@RF00216 B     @DE00223                                            0223
@DL00223 DS    0H                                                  0224
*     TXT2=TXT02;                   /* PUT IN THE STANDARD IFREQ =   */
         MVC   TXT2(7),@CC01353                                    0224
*     ENV2=SMDENV;                  /* FILL IN ENV                   */
         MVC   ENV2(7),SMDENV                                      0225
*     DO A=1 TO DIM(REQNUM2) WHILE SMDRC=ZERO&GTPRETRN=GTPGOOD;    0226
         LA    A,1                                                 0226
@DL00226 CH    SMDRC,@CH00307                                      0226
         BNE   @DC00226                                            0226
         CLI   GTPRETRN(GTPPTR1),0                                 0226
         BNE   @DC00226                                            0226
*       CALL HMASMGTA(HMASMGTP);    /*                               */
         ST    GTPPTR1,@AL00001                                    0227
         L     @15,@CV01329                                        0227
         LA    @01,@AL00001                                        0227
         BALR  @14,@15                                             0227
*       IF GTPRETRN=GTPGOOD THEN    /*                               */
         CLI   GTPRETRN(GTPPTR1),0                                 0228
         BNE   @RF00228                                            0228
*         DO;                       /*                               */
*           IF SMDENV^=LASTENV THEN /* CHECK IF NOT EQUAL            */
         CLC   SMDENV(7),LASTENV                                   0230
         BE    @RF00230                                            0230
*             DO;                   /*                               */
*               LASTENV=SMDENV;     /* IF NOT FILL IN NAME           */
         MVC   LASTENV(7),SMDENV                                   0232
*               IF SWITCH=ON THEN   /*                               */
         TM    SWITCH,B'10000000'                                  0233
         BNO   @RF00233                                            0233
*                 DO;               /*                               */
*                   CALL LCCWRITE;  /* WRITE OUT THE LINE PICK UP THE
*                                      RETURN CODE                   */
         BAL   @14,LCCWRITE                                        0235
*                   SMDRC=MAX(SMDRC,IOPRETRN);                     0236
         SLR   @10,@10                                             0236
         IC    @10,IOPRETRN(,IOPPTR)                               0236
         CR    SMDRC,@10                                           0236
         BNL   *+6
         LR    SMDRC,@10                                           0236
*                   A=1;            /*                               */
         LA    A,1                                                 0237
*                 END;              /*                               */
*               TXT2=TXT02;         /* PUT IN THE STANDARD IFREQ =   */
@RF00233 MVC   TXT2(7),@CC01353                                    0239
*               ENV2=SMDENV;        /* FILL IN ENV                   */
         MVC   ENV2(7),SMDENV                                      0240
*               NUM2(A)=SMDNUM;     /* FILL IN NUMBER                */
         LR    @10,A                                               0241
         MH    @10,@CH00383                                        0241
         LA    @01,NUM2-10(@10)                                    0241
         MVC   0(7,@01),SMDNUM                                     0241
*               SWITCH=ON;          /*                               */
         OI    SWITCH,B'10000000'                                  0242
*             END;                  /*                               */
*           ELSE                    /*                               */
*             NUM2(A)=SMDNUM;       /* FILL IN THE LINE              */
         B     @RC00230                                            0244
@RF00230 LR    @10,A                                               0244
         MH    @10,@CH00383                                        0244
         LA    @01,NUM2-10(@10)                                    0244
         MVC   0(7,@01),SMDNUM                                     0244
*         END;                      /*                               */
@RC00230 DS    0H                                                  0246
*     END;                          /*                               */
@RF00228 AH    A,@CH00041                                          0246
         CH    A,@CH00084                                          0246
         BNH   @DL00226                                            0246
@DC00226 DS    0H                                                  0247
*     IF A>DIM(REQNUM2)×GTPRETRN=GTPEOF THEN                       0247
         CH    A,@CH00084                                          0247
         BH    @RT00247                                            0247
         CLI   GTPRETRN(GTPPTR1),4                                 0247
         BNE   @RF00247                                            0247
@RT00247 DS    0H                                                  0248
*       DO;                         /* A WILL BE BUMPED TO NINE      */
*         CALL LCCWRITE;            /* PRINT OUT THE BUFFER          */
         BAL   @14,LCCWRITE                                        0249
*         SMDRC=MAX(SMDRC,IOPRETRN);                               0250
         SLR   @10,@10                                             0250
         IC    @10,IOPRETRN(,IOPPTR)                               0250
         CR    SMDRC,@10                                           0250
         BNL   *+6
         LR    SMDRC,@10                                           0250
*       END;                        /*                               */
*   END;                            /* DOWHILEND                     */
@RF00247 DS    0H                                                  0252
@DE00223 CH    SMDRC,@CH00307                                      0252
         BNE   @DC00223                                            0252
         CLI   GTPRETRN(GTPPTR1),0                                 0252
         BE    @DL00223                                            0252
@DC00223 DS    0H                                                  0253
*   GTPFUNCT=GTPFREE;               /* FREE THE GTP                  */
         MVI   GTPFUNCT(GTPPTR1),X'0A'                             0253
*   CALL HMASMGTA(HMASMGTP);        /*                               */
         ST    GTPPTR1,@AL00001                                    0254
         L     @15,@CV01329                                        0254
         LA    @01,@AL00001                                        0254
         BALR  @14,@15                                             0254
*   RETURN CODE(SMDRC);             /*                               */
         LR    @15,SMDRC                                           0255
         L     @14,@SA00002                                        0255
         LM    @00,@12,@SA00002+8                                  0255
         BR    @14                                                 0255
*   END LCCSMD;                     /*                               */
*                                                                  0257
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD A GTA FILE OF IFREQS ***** SORTED BY GTA PROCESSING     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0257
*LCCSMSRT:                                                         0257
*   PROCEDURE;                      /*                               */
LCCSMSRT STM   @14,@12,@SA00003                                    0257
*   DCL                                                            0258
*     SMSRTRC FIXED(8);             /*                               */
*   RFY                                                            0259
*     HMASMIOP BASED(IOPPTR);       /*                               */
*   RFY                                                            0260
*     HMASMGTP BASED(GTPPTR1);      /*                               */
*   SMSRTRC=ZERO;                   /*                               */
         SLR   SMSRTRC,SMSRTRC                                     0261
*   HMASMGTP=''B;                   /*                               */
         XC    HMASMGTP(28,GTPPTR1),HMASMGTP(GTPPTR1)              0262
*   GTPBUFAD=ADDR(SMDRCD);          /* SETUP THE GTP TO ADD RECORDS  */
         LA    @10,SMDRCD                                          0263
         ST    @10,GTPBUFAD(,GTPPTR1)                              0263
*   GTPRCDLN=LENGTH(SMDRCD);        /*                               */
         LA    @10,14                                              0264
         STH   @10,GTPRCDLN(,GTPPTR1)                              0264
*   GTPKEYLN=LENGTH(SMDRCD);        /*                               */
         STH   @10,GTPKEYLN(,GTPPTR1)                              0265
*   GTPFUNCT=GTPADD;                /*                               */
         MVI   GTPFUNCT(GTPPTR1),X'04'                             0266
*   IOPFUNCT=IOPPOSI;               /* POSITION FOR INPUT THE OTHER
*                                      REQUIRED PARMS ARE            */
         MVI   IOPFUNCT(IOPPTR),X'02'                              0267
*   CALL HMASMIO(HMASMIOP);         /* ALREADY IN THE PASSIOP        */
         ST    IOPPTR,@AL00001                                     0268
         L     @15,@CV01328                                        0268
         LA    @01,@AL00001                                        0268
         BALR  @14,@15                                             0268
*   IOPFUNCT=IOPREAD;               /* SET THE IOP FOR A READ AFTER
*                                      POSITION                      */
         MVI   IOPFUNCT(IOPPTR),X'01'                              0269
*   DO WHILE IOPRETRN=IOPGOOD&GTPRETRN=GTPGOOD;                    0270
         B     @DE00270                                            0270
@DL00270 DS    0H                                                  0271
*     CALL HMASMIO(HMASMIOP);       /*                               */
         ST    IOPPTR,@AL00001                                     0271
         L     @15,@CV01328                                        0271
         LA    @01,@AL00001                                        0271
         BALR  @14,@15                                             0271
*     IF IOPRETRN=IOPGOOD THEN      /*                               */
         CLI   IOPRETRN(IOPPTR),0                                  0272
         BNE   @RF00272                                            0272
*       DO;                         /*                               */
*         SMDENV=IOPQSMFM;          /* ADD ENV TO GTA FILE           */
         L     @10,IOPBUFAD(,IOPPTR)                               0274
         MVC   SMDENV(7),IOPQSMFM(@10)                             0274
*         DO B=1 BY 1 WHILE IOPQSMND(B)^=IOPEOLST&GTPRETRN=GTPGOOD&B<=8
*               ;                   /* PROCESS 80 BYTE RECORDS       */
         LA    B,1                                                 0275
         B     @DE00275                                            0275
@DL00275 DS    0H                                                  0276
*           SMDNUM=IOPQSMNM(B);     /* ADD TO GTA FILE               */
         LR    @10,B                                               0276
         MH    @10,@CH00490                                        0276
         L     @01,IOPBUFAD(,IOPPTR)                               0276
         ALR   @01,@10                                             0276
         BCTR  @01,0                                               0276
         MVC   SMDNUM(7),IOPQSMNM-8(@01)                           0276
*           CALL HMASMGTA(HMASMGTP);                               0277
         ST    GTPPTR1,@AL00001                                    0277
         L     @15,@CV01329                                        0277
         LA    @01,@AL00001                                        0277
         BALR  @14,@15                                             0277
*         END;                      /* END OF DO LOOP                */
         AH    B,@CH00041                                          0278
@DE00275 LR    @10,B                                               0278
         MH    @10,@CH00490                                        0278
         L     @01,IOPBUFAD(,IOPPTR)                               0278
         BCTR  @01,0                                               0278
         SLR   @00,@00                                             0278
         IC    @00,IOPQSMND-8(@10,@01)                             0278
         CH    @00,@CH00679                                        0278
         BE    @DC00275                                            0278
         CLI   GTPRETRN(GTPPTR1),0                                 0278
         BNE   @DC00275                                            0278
         CH    B,@CH00084                                          0278
         BNH   @DL00275                                            0278
@DC00275 DS    0H                                                  0279
*       END;                        /*                               */
*   END;                            /*                               */
@RF00272 DS    0H                                                  0280
@DE00270 CLI   IOPRETRN(IOPPTR),0                                  0280
         BNE   @DC00270                                            0280
         CLI   GTPRETRN(GTPPTR1),0                                 0280
         BE    @DL00270                                            0280
@DC00270 DS    0H                                                  0281
*   IF GTPRETRN=GTPSPACE THEN       /* IF SPACE ERROR SETUP RC       */
         CLI   GTPRETRN(GTPPTR1),12                                0281
         BNE   @RF00281                                            0281
*     SMSRTRC=MAX(SMSRTRC,GTPRETRN);                               0282
         SLR   @10,@10                                             0282
         IC    @10,GTPRETRN(,GTPPTR1)                              0282
         CR    SMSRTRC,@10                                         0282
         BNL   *+6
         LR    SMSRTRC,@10                                         0282
*   IF IOPRETRN>IOPEOF THEN         /* IF READ ERROR                 */
@RF00281 CLI   IOPRETRN(IOPPTR),4                                  0283
         BNH   @RF00283                                            0283
*     SMSRTRC=MAX(SMSRTRC,IOPRETRN);                               0284
         SLR   @10,@10                                             0284
         IC    @10,IOPRETRN(,IOPPTR)                               0284
         CR    SMSRTRC,@10                                         0284
         BNL   *+6
         LR    SMSRTRC,@10                                         0284
*   RETURN CODE(SMSRTRC);           /*                               */
@RF00283 LR    @15,SMSRTRC                                         0285
         L     @14,@SA00003                                        0285
         LM    @00,@12,@SA00003+8                                  0285
         BR    @14                                                 0285
*   END LCCSMSRT;                   /*                               */
*                                   /*                               */
*                                                                  0287
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS FMID REQUESTS                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0287
*LCCFMD:                                                           0287
*   PROCEDURE;                      /*                               */
LCCFMD   STM   @14,@12,@SA00004                                    0287
*   DCL                                                            0288
*     FMDRC FIXED(8);               /*                               */
*   RFY                                                            0289
*     HMASMIOP BASED(IOPPTR);       /*                               */
*   RFY                                                            0290
*     HMASMGTP BASED(GTPPTR1);      /*                               */
*   FMDRC=ZERO;                     /*                               */
         SLR   FMDRC,FMDRC                                         0291
*   CALL LCCFMSRT;                  /* BUILD A SORTED GTA FILE       */
         BAL   @14,LCCFMSRT                                        0292
*   RFY                                                            0293
*     RTNCODE RSTD;                 /*                               */
*   FMDRC=MAX(FMDRC,RTNCODE);       /*                               */
         CR    FMDRC,RTNCODE                                       0294
         BNL   *+6
         LR    FMDRC,RTNCODE                                       0294
*   RFY                                                            0295
*     RTNCODE UNRSTD;               /*                               */
*   IF FMDRC=ZERO THEN              /*                               */
         CH    FMDRC,@CH00307                                      0296
         BNE   @RF00296                                            0296
*     DO;                           /* RESET POINTERS TO START AT THE*/
*       GTPFUNCT=GTPOPENN;          /* TOP OF THE FILE..GTPRCDLN WAS */
         MVI   GTPFUNCT(GTPPTR1),X'01'                             0298
*       CALL HMASMGTA(HMASMGTP);    /* SET IN LCCFMSRT               */
         ST    GTPPTR1,@AL00001                                    0299
         L     @15,@CV01329                                        0299
         LA    @01,@AL00001                                        0299
         BALR  @14,@15                                             0299
*                                   /* SETUP THE GTP FOR GETNEXT   0300
*                                      PROCESSING..THE GTPRCDLN AND  */
*       GTPFUNCT=GTPGETN;           /* GTPBUFAD WAS SET IN LCCFMSRT  */
         MVI   GTPFUNCT(GTPPTR1),X'08'                             0300
*       NAME3=IOPNAME;              /* PUT THE NAME IN THE PRINT BUF */
         MVC   NAME3(8),IOPNAME(IOPPTR)                            0301
*       TXT3=TXT03;                 /* PUT IN THE STANDARD CHARS ONCE*/
         MVC   TXT3(17),@CC01376                                   0302
*     END;                          /* FOR EACH NAME                 */
*   DO WHILE FMDRC=ZERO&GTPRETRN=GTPGOOD;                          0304
@RF00296 B     @DE00304                                            0304
@DL00304 DS    0H                                                  0305
*     DO D=1 TO DIM(FMIDSYSM) WHILE FMDRC=ZERO&GTPRETRN=GTPGOOD;   0305
         LA    D,1                                                 0305
@DL00305 CH    FMDRC,@CH00307                                      0305
         BNE   @DC00305                                            0305
         CLI   GTPRETRN(GTPPTR1),0                                 0305
         BNE   @DC00305                                            0305
*       CALL HMASMGTA(HMASMGTP);    /*                               */
         ST    GTPPTR1,@AL00001                                    0306
         L     @15,@CV01329                                        0306
         LA    @01,@AL00001                                        0306
         BALR  @14,@15                                             0306
*       IF GTPRETRN=GTPGOOD THEN    /*                               */
         CLI   GTPRETRN(GTPPTR1),0                                 0307
         BNE   @RF00307                                            0307
*         DO;                       /*                               */
*           NUM3(D)=SMDFMNUM;       /*                               */
         LR    @10,D                                               0309
         MH    @10,@CH00383                                        0309
         LA    @01,NUM3-10(@10)                                    0309
         MVC   0(7,@01),SMDFMNUM                                   0309
*         END;                      /*                               */
*       IF D=DIM(FMIDSYSM)×GTPRETRN=GTPEOF THEN                    0311
@RF00307 CH    D,@CH00084                                          0311
         BE    @RT00311                                            0311
         CLI   GTPRETRN(GTPPTR1),4                                 0311
         BNE   @RF00311                                            0311
@RT00311 DS    0H                                                  0312
*         DO;                       /*                               */
*           CALL LCCWRITE;          /* PRINT OUT THE BUFFER          */
         BAL   @14,LCCWRITE                                        0313
*           FMDRC=MAX(FMDRC,IOPRETRN);                             0314
         SLR   @10,@10                                             0314
         IC    @10,IOPRETRN(,IOPPTR)                               0314
         CR    FMDRC,@10                                           0314
         BNL   *+6
         LR    FMDRC,@10                                           0314
*         END;                      /*                               */
*     END;                          /*                               */
@RF00311 AH    D,@CH00041                                          0316
         CH    D,@CH00084                                          0316
         BNH   @DL00305                                            0316
@DC00305 DS    0H                                                  0317
*   END;                            /*                               */
@DE00304 CH    FMDRC,@CH00307                                      0317
         BNE   @DC00304                                            0317
