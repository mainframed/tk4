         TITLE 'HMASMMPH HEADER PARSE ROUTINE'
HMASMMPH CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMMPH  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*                                      PTF ENTRY IF PTF DATA TO BE 0167
*                                      FILLED IN ELSE 0              */
*   MPHRTNCD=ZERO;                  /* TO BE RETURNED IF NO ERRORS   */
*                                                                  0168
         SLR   @10,@10                                             0168
         ST    @10,MPHRTNCD                                        0168
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE CRP AND MCB                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0169
*   CRPSCPAD=ADDR(HMASMSCP);        /* INIT. CRD PARM. LIST          */
         L     @10,@PC00001                                        0169
         L     @15,MPPCRDPM(,@10)                                  0169
         LA    @14,HMASMSCP                                        0169
         ST    @14,CRPSCPAD(,@15)                                  0169
*   MCBPNO=NULLPTF#;                /* CLEAR SYSMOD-ID FIELD         */
         L     @14,MPPMCBAD(,@10)                                  0170
         MVC   MCBPNO(7,@14),@CC01438                              0170
*   MCBTYPE=MCBPTFTP;               /* SET CONSTANT PTF TYPE RECORD
*                                      IN MCB                        */
         MVI   MCBTYPE(@14),X'01'                                  0171
*   MCBPRELN=''B;                   /* INIT. FILES BINARY 0'S        */
*                                                                  0172
         SLR   @09,@09                                             0172
         STH   @09,MCBPRELN(,@14)                                  0172
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE SCAN PARMS                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0173
*   SCPRET=SCPGOOD;                 /* GOOD RETURN CODE              */
         MVI   SCPRET,X'00'                                        0173
*   SCPCHAR=ADDR(CRPBUFFR);         /* CHAR STRING TO BE SCANNED     */
         LA    @14,CRPBUFFR(,@15)                                  0174
         ST    @14,SCPCHAR                                         0174
*   SCPSRCH=ADDR(KPLPL);            /* FIRST DSCAN MACRO USED IN SCAN*/
         LA    @14,KPLPL                                           0175
         ST    @14,SCPSRCH                                         0175
*   SCPIORTN=MPPCRDAD;              /* ADDR OF USER I/O ROUTINE      */
         MVC   SCPIORTN(4),MPPCRDAD(@10)                           0176
*   SCPIOPAD=MPPCRDPM;              /* ADDR OF PARM I/O ROUTINE      */
         ST    @15,SCPIOPAD                                        0177
*   SCPINLN=LENGTH(CRPBFCTL);       /* INPUT RECORD LENGTH 72 COLUMNS*/
         MVC   SCPINLN(2),@CH00839                                 0178
*   SCPEOR=ZERO;                    /* FLAG BYTE                     */
         MVI   SCPEOR,X'00'                                        0179
*   SCPCOMNT=ON;                    /* CHECK FOR COMMENT DELIMITERS  */
*   SCPCONT=ON;                     /* GO TO NEXT CARD               */
*                                                                  0181
         OI    SCPCOMNT,B'10100000'                                0181
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE MESSAGE PARMS                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0182
*   HMASMMGP=''B;                   /* CLEAR MGP                     */
         XC    HMASMMGP(24),HMASMMGP                               0182
*   MGPHLDS=ON;                     /* MESSAGES TO LOG               */
*   MGPPRINT=ON;                    /* SET FOR MSG TO PRINTER        */
         OI    MGPHLDS,B'10010000'                                 0184
*                                                                  0185
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN CONTROL CARD FOR TYPE SYSMOD                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0185
*   CALL HMASMSCN(HMASMSCP);                                       0185
*                                                                  0185
         L     @15,@CV01421                                        0185
         LA    @01,@AL00185                                        0185
         BALR  @14,@15                                             0185
*   /*****************************************************************/
*   /*                                                               */
*   /* IF END OF FILE BEFORE END OF COMPLETE STATEMENT CALL PROC TO  */
*   /* ISSUE MSG HMA252I RETRN 8 TO CALLER                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0186
*   IF CRPRETRN=CRPEOF              /*                               */
*     THEN                          /*                               */
         L     @10,@PC00001                                        0186
         L     @10,MPPCRDPM(,@10)                                  0186
         CLI   CRPRETRN(@10),4                                     0186
         BNE   @RF00186                                            0186
*     DO;                           /*                               */
*       CALL INCOMPMG;              /*                               */
         BAL   @14,INCOMPMG                                        0188
*       MPHRTNCD=STOPPTF;           /*                               */
         MVC   MPHRTNCD(4),@CF00116                                0189
*     END;                          /*                               */
*                                                                  0190
*   /*****************************************************************/
*   /*                                                               */
*   /* IF SYNTAX ERROR PUT OUT MESSAGE. IF SEVERE ERROR JUST SET RC  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0191
*   ELSE                            /*                               */
*     IF SCPRET>=SCPBAD             /* IF ERROR IN SCAN              */
*       THEN                        /*                               */
         B     @RC00186                                            0191
@RF00186 CLI   SCPRET,4                                            0191
         BL    @RF00191                                            0191
*       DO;                         /*                               */
*         IF SCPRET=SCPBAD          /*                               */
*           THEN                    /*                               */
         CLI   SCPRET,4                                            0193
         BNE   @RF00193                                            0193
*           CALL SYNTAXMG;          /*                               */
         BAL   @14,SYNTAXMG                                        0194
*         MPHRTNCD=MAX(SCPRET,STOPPTF,MPHRTNCD);                   0195
@RF00193 SLR   @10,@10                                             0195
         IC    @10,SCPRET                                          0195
         LA    @15,8                                               0195
         CR    @10,@15                                             0195
         BNL   *+6
         LR    @10,@15                                             0195
         L     @15,MPHRTNCD                                        0195
         CR    @10,@15                                             0195
         BNL   *+6
         LR    @10,@15                                             0195
         ST    @10,MPHRTNCD                                        0195
*       END;                                                       0196
*   RETURN CODE(MPHRTNCD);                                         0197
@RF00191 DS    0H                                                  0197
@RC00186 L     @15,MPHRTNCD                                        0197
         L     @13,4(,@13)                                         0197
         L     @14,12(,@13)                                        0197
         LM    @00,@12,20(@13)                                     0197
         BR    @14                                                 0197
         EJECT
*   RFY                                                            0198
*    (RTNCODE,                                                     0198
*     REG1,                                                        0198
*     RTNREG) RSTD;                                                0198
*                                                                  0198
*   /*****************************************************************/
*   /*                                                               */
*   /* MCB BIT SETTING FOR PTF                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0199
*RPTF:                                                             0199
*   MCBPTP=IOPPPTF;                 /* SET PTF BIT IN MCB            */
RPTF     L     @10,@PC00001                                        0199
         L     @10,MPPMCBAD(,@10)                                  0199
         MVI   MCBPTP(@10),X'03'                                   0199
*   GO TO GOODSCN1;                 /* RETURN NORMALLY               */
*                                                                  0200
         B     GOODSCN1                                            0200
*   /*****************************************************************/
*   /*                                                               */
*   /* MCB BIT SETTING FOR FUNCTION                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0201
*RFUNCT:                                                           0201
*   MCBPTP=IOPPFUNC;                /* SET FUNC BIT IN MCB           */
RFUNCT   L     @10,@PC00001                                        0201
         L     @10,MPPMCBAD(,@10)                                  0201
         MVI   MCBPTP(@10),X'04'                                   0201
*   GO TO GOODSCN1;                 /* RETURN NORMALLY               */
*                                                                  0202
         B     GOODSCN1                                            0202
*   /*****************************************************************/
*   /*                                                               */
*   /* MCB BIT SETTING FOR USERMOD                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0203
*RUSER:                                                            0203
*   MCBPTP=IOPPUSER;                /* SET USERMOD BIT IN MCB        */
RUSER    L     @10,@PC00001                                        0203
         L     @10,MPPMCBAD(,@10)                                  0203
         MVI   MCBPTP(@10),X'01'                                   0203
*   GO TO GOODSCN1;                 /* RETURN NORMALLY               */
*                                                                  0204
         B     GOODSCN1                                            0204
*   /*****************************************************************/
*   /*                                                               */
*   /* MCB BIT SETTING FOR APAR                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0205
*RAPAR:                                                            0205
*   MCBPTP=IOPPAPAR;                /* SET APAR BIT IN MCB           */
RAPAR    L     @10,@PC00001                                        0205
         L     @10,MPPMCBAD(,@10)                                  0205
         MVI   MCBPTP(@10),X'02'                                   0205
*   GO TO GOODSCN1;                 /* RETURN NORMALLY               */
         B     GOODSCN1                                            0206
         EJECT
*                                                                  0207
*   /*****************************************************************/
*   /*                                                               */
*   /* PICK UP SYSMOD ID WHICH IS 7 ALPHA NUMERIC CHARS              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0207
*RSMDNO:                                                           0207
*                                                                  0207
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR IOP, IF PASSED FILL IN IOP TYPE BIT AND PTF #       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0207
*   IF MPPIOPAD^=0 THEN                                            0207
RSMDNO   L     @10,@PC00001                                        0207
         L     @09,MPPIOPAD(,@10)                                  0207
         LTR   @09,@09                                             0207
         BZ    @RF00207                                            0207
*     DO;                                                          0208
*       IOPPTYPE=MCBPTP;                                           0209
         L     @10,MPPMCBAD(,@10)                                  0209
         MVC   IOPPTYPE(1,@09),MCBPTP(@10)                         0209
*       IOPNAM17=SCPSTRNG(1:LENGTH(IOPNAM17));/*                     */
         L     @10,SCPCHAR                                         0210
         MVC   IOPNAM17(7,@09),SCPSTRNG(@10)                       0210
*     END;                                                         0211
*                                                                  0211
*   /*****************************************************************/
*   /*                                                               */
*   /* FILL IN PTF# FOR MCB                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0212
*   MCBPNO=SCPSTRNG(1:LENGTH(MCBPNO));/* MOVE SYSMOD NUMBER          */
@RF00207 L     @10,@PC00001                                        0212
         L     @10,MPPMCBAD(,@10)                                  0212
         L     @09,SCPCHAR                                         0212
         MVC   MCBPNO(7,@10),SCPSTRNG(@09)                         0212
*   GO TO GOODSCN1;                 /* RETURN NORMALLY               */
         B     GOODSCN1                                            0213
         EJECT
*                                                                  0214
*   /*****************************************************************/
*   /*                                                               */
*   /* PUT NUM OF FILES IN MCB AND IOP IF PASSED                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0214
*RRELFNO:                                                          0214
*   DECLARE                                                        0214
*     PKWKAREA CHAR(8) BDY(DWORD);  /* CONVERT CHAR FILES# TO BINARY */
RRELFNO  DS    0H                                                  0215
*   DECLARE                                                        0215
*     FILEWKA CHAR(8);                                             0215
*   DECLARE                                                        0216
*     FILESNO FIXED(15);                                           0216
*   DECLARE                                                        0217
*     FOPERAND CHAR(*) BASED(SCPCHAR);                             0217
*   DECLARE                                                        0218
*     INVALID CHAR(1) INIT('0');                                   0218
*                                                                  0218
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR INVALID 0 PRECEEDING FILES NUMBER                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0219
*   IF FOPERAND(1:1)=INVALID THEN                                  0219
         L     @10,SCPCHAR                                         0219
         CLC   FOPERAND(1,@10),INVALID                             0219
         BNE   @RF00219                                            0219
*     DO;                           /* ZERO PRECEDING FILES          */
*       SCPRET=SCPSTOP;                                            0221
         MVI   SCPRET,X'08'                                        0221
*     END;                                                         0222
*                                                                  0222
*   /*****************************************************************/
*   /*                                                               */
*   /* CONVERT TO BINERY FILES NUMBER MOVE FILE # IN MCB AND IOP IF  */
*   /* PASSED                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0223
*   ELSE                                                           0223
*     DO;                                                          0223
         B     @RC00219                                            0223
@RF00219 DS    0H                                                  0224
*       FILEWKA=' ';                                               0224
         MVI   FILEWKA+1,C' '                                      0224
         MVC   FILEWKA+2(6),FILEWKA+1                              0224
         MVI   FILEWKA,C' '                                        0224
*       FILEWKA((9-SCPPMLN):8)=FOPERAND(1:SCPPMLN);                0225
         LA    @10,9                                               0225
         SH    @10,SCPPMLN                                         0225
         LA    @09,FILEWKA-1(@10)                                  0225
         LCR   @10,@10                                             0225
         AL    @10,@CF00116                                        0225
         L     @08,SCPCHAR                                         0225
         EX    @10,@SM01488                                        0225
*       PACK(PKWKAREA,FILEWKA);                                    0226
         PACK  PKWKAREA(8),FILEWKA(8)                              0226
*       CVB(FILESNO,PKWKAREA);                                     0227
         CVB   @10,PKWKAREA                                        0227
         STH   @10,FILESNO                                         0227
*       MCBPRELN=FILESNO;           /* NUM FILES TO MCB              */
         L     @09,@PC00001                                        0228
         L     @08,MPPMCBAD(,@09)                                  0228
         STH   @10,MCBPRELN(,@08)                                  0228
*       IF MPPIOPAD^=0 THEN                                        0229
         L     @09,MPPIOPAD(,@09)                                  0229
         LTR   @09,@09                                             0229
         BZ    @RF00229                                            0229
*         DO;                       /*                               */
*           IOPPRELN=MCBPRELN;      /* NUM FILES TO IOP              */
         STH   @10,IOPPRELN(,@09)                                  0231
*           IOPLRFP=ON;             /*                               */
         OI    IOPLRFP(@09),B'00000001'                            0232
*         END;                      /*                               */
*     END;                                                         0234
*   GO TO GOODSCN1;                 /* RETURN NORMALLY               */
         B     GOODSCN1                                            0235
*                                                                  0236
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO SCAN WITH NORMAL RETURN CODE                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0236
*RXTRA:                             /*                               */
*   SCPRETRN=SCPSTOP;               /* SYNTAX ERROR                  */
RXTRA    MVI   SCPRETRN,X'08'                                      0236
*GOODSCN1:                                                         0237
*   DECLARE                                                        0237
*     SCNAPRET LABEL BASED(RTNREG) VALUERANGE(0);/* RETURN REGISTER
*                                      FROM APPENDAGE TO SCAN ROUTINE*/
GOODSCN1 DS    0H                                                  0238
*   GO TO SCNAPRET;                 /* RETURN TO MAIN SCAN ROUTINE   */
         BR    RTNREG                                              0238
*   RFY                                                            0239
*    (RTNCODE,                                                     0239
*     REG1,                                                        0239
*     RTNREG) UNRSTD;                                              0239
*                                                                  0240
*   /*****************************************************************/
*   /*                                                               */
*   /* SYNTAX ERROR MESSAGE                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0240
*SYNTAXMG:                                                         0240
*   PROC;                                                          0240
@EL00001 L     @13,4(,@13)                                         0240
@EF00001 DS    0H                                                  0240
@ER00001 LM    @14,@12,12(@13)                                     0240
         BR    @14                                                 0240
SYNTAXMG STM   @14,@12,@SA00002                                    0240
*   HEXNO=SCPCHAR-ADDR(CRPBUFFR)+1; /* GET COLUMN NUMBER             */
         L     @10,@PC00001                                        0241
         L     @10,MPPCRDPM(,@10)                                  0241
         LA    @15,CRPBUFFR(,@10)                                  0241
         LCR   @15,@15                                             0241
         AL    @15,SCPCHAR                                         0241
         AH    @15,@CH00091                                        0241
         ST    @15,HEXNO                                           0241
*   CVD(HEXNO,DECNO1);              /* CONVERT NO. TO DECIMAL        */
         CVD   @15,DECNO1                                          0242
*   UNPK(BCDNO,DECNO(COLSTR:COLSTP));/* UNPACK NO.                   */
         UNPK  BCDNO(5),DECNO+6(3)                                 0243
*   MGPMGNO1=HMA203I;               /* SYNTAX ERROR IN               */
         MVC   MGPMGNO1(2),@CH00044                                0244
*   MGPMGNO2=PART021;               /* CONTROL STATEMENT             */
         MVC   MGPMGNO2(2),@CH00459                                0245
*   MGPMGNO3=PART026;               /* AT COLUMN                     */
         MVC   MGPMGNO3(2),@CH00469                                0246
*   MGPVARPT(1)=ADDR(BCDCOL);       /* MESSAGE TEXT = COL NUM        */
         LA    @15,BCDCOL                                          0247
         ST    @15,MGPVARPT                                        0247
*   MGPTYPE=MGPERR;                 /* MESSAGE SEVERITY              */
         MVI   MGPTYPE,X'03'                                       0248
*   IF CRPPRINT=ON                  /* STMT. WAS WRITTEN TO SMPOUT   */
*     THEN                                                         0249
         TM    CRPPRINT(@10),B'00001000'                           0249
         BNO   @RF00249                                            0249
*     CALL HMASMMSG(HMASMMGP);      /* CALL MESSAGE ROUTINE          */
         L     @15,@CV01420                                        0250
         LA    @01,@AL00250                                        0250
         BALR  @14,@15                                             0250
*   END SYNTAXMG;                                                  0251
*                                                                  0251
@EL00002 DS    0H                                                  0251
@EF00002 DS    0H                                                  0251
@ER00002 LM    @14,@12,@SA00002                                    0251
         BR    @14                                                 0251
*   /*****************************************************************/
*   /*                                                               */
*   /* ERROR MESSAGE FOR INCOMPLETE CONTROL STATEMENT                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0252
*INCOMPMG:                                                         0252
*   PROC;                                                          0252
INCOMPMG STM   @14,@12,@SA00003                                    0252
*   MGPMGNO1=HMA252I;               /* INCOMPL SMP CNTRL STMT        */
         MVC   MGPMGNO1(2),@CH01424                                0253
*   MGPTYPE=MGPERR;                 /* MESSAGE SEVERITY              */
         MVI   MGPTYPE,X'03'                                       0254
*   IF CRPPRINT=ON                  /* STMT. WAS WRITTEN TO SMPOUT   */
*     THEN                                                         0255
         L     @10,@PC00001                                        0255
         L     @10,MPPCRDPM(,@10)                                  0255
         TM    CRPPRINT(@10),B'00001000'                           0255
         BNO   @RF00255                                            0255
*     CALL HMASMMSG(HMASMMGP);      /* CALL MESSAGE ROUTINE          */
         L     @15,@CV01420                                        0256
         LA    @01,@AL00256                                        0256
         BALR  @14,@15                                             0256
*   END INCOMPMG;                                                  0257
@EL00003 DS    0H                                                  0257
@EF00003 DS    0H                                                  0257
@ER00003 LM    @14,@12,@SA00003                                    0257
         BR    @14                                                 0257
*   END HMASMMPH                                                   0258
