@DATA    DS    0H
@CH00091 DC    H'1'
@CH00044 DC    H'4'
@CH00459 DC    H'21'
@CH00469 DC    H'26'
@CH01424 DC    H'53'
@CH00839 DC    H'72'
@SM01488 MVC   0(0,@09),FOPERAND(@08)
         DS    0F
@AL00185 DC    A(HMASMSCP)             LIST WITH   1 ARGUMENT(S)
@AL00250 EQU   *                       LIST WITH   1 ARGUMENT(S)
@AL00256 DC    A(HMASMMGP)             LIST WITH   1 ARGUMENT(S)
         DS    0F
@SA00001 DS    18F
@PC00001 DS    1F
@SA00002 DS    0F
@SA00003 DS    0F
         DS    15F
         DS    0F
@CF00116 DC    F'8'
@CV01420 DC    V(HMASMMSG)
@CV01421 DC    V(HMASMSCN)
         DS    0D
IOPPTR   DS    A
