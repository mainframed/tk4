         TITLE 'HMASMLCD - FORMAT CDS/ACDS/SCDS ENTRIES'
HMASMLCD CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMLCD  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  LA    @10,4095(,@12)                                      0001
         USING @PSTART,@12                                         0001
         USING @PSTART+4095,@10                                    0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(16),0(@01)                                 0001
*   LCDRTNCD=ZERO;                  /* INIT RETURN CODE              */
         SLR   LCDRTNCD,LCDRTNCD                                   0173
*   IOPPTR=ADDR(PASSIOP);           /* IOP WITH SYSMOD ENTRY         */
         L     IOPPTR,@PC00001+4                                   0174
*   SAVBUFAD=IOPBUFAD;              /* SAVE BUFFER ADDR              */
         L     SAVBUFAD,IOPBUFAD(,IOPPTR)                          0175
*   PRNTBUF(1:LENGTH(PRNTBUF))=BLANK;/* THEN CLEAR THE PRINT AREA    */
         MVI   PRNTBUF+1,C' '                                      0176
         MVC   PRNTBUF+2(118),PRNTBUF+1                            0176
         MVI   PRNTBUF,C' '                                        0176
*   RFY                                                            0177
*     RTNCODE RSTD;                 /*                               */
*   IF IOPTYPE^=LASTTYPE            /* IF NEW TYPE ENTRY             */
*       &LISTSCDS=OFF               /* AND NOT SCDS                  */
*       &IOPTYPE^=IOPSMD            /* AND NOT SYSMOD                */
*     THEN                          /*                               */
         L     @14,@PC00001+8                                      0178
         CLC   IOPTYPE(1,IOPPTR),LASTTYPE(@14)                     0178
         BE    @RF00178                                            0178
         L     @14,@PC00001                                        0178
         TM    LISTSCDS(@14),B'00010000'                           0178
         BNZ   @RF00178                                            0178
         CLI   IOPTYPE(IOPPTR),11                                  0178
         BE    @RF00178                                            0178
*     DO;                           /*                               */
*       CALL LCDSETHD;              /* SET UP PAGE HEADINGS          */
         BAL   @14,LCDSETHD                                        0180
*       LCDRTNCD=MAX(LCDRTNCD,RTNCODE);/* SAVE RC                    */
         CR    LCDRTNCD,RTNCODE                                    0181
         BNL   *+6
         LR    LCDRTNCD,RTNCODE                                    0181
*     END;                          /*                               */
*                                                                  0182
*   /*****************************************************************/
*   /*                                                               */
*   /* INIT COMMON FIELDS OF HMASMFVL PARAMETER LIST                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0183
*   HMASMFVP=''B;                   /*                               */
@RF00178 XC    HMASMFVP(15),HMASMFVP                               0183
*   FVPLNLEN=CCALNLEN;              /*                               */
         MVC   FVPLNLEN(2),CCALNLEN(CCAPTR)                        0184
*   FVPOFFST=ADDR(HDSUBHD)-ADDR(HDNAME);/*                           */
         MVC   FVPOFFST(2),@CH00037                                0185
*   FVPVERN=ZERO;                   /*                               */
         MVI   FVPVERN,X'00'                                       0186
*   FVPDSID=CCALSTDS;               /* OUTPUT DATASET ID             */
*                                                                  0187
         MVC   FVPDSID(1),CCALSTDS(CCAPTR)                         0187
*   /*****************************************************************/
*   /*                                                               */
*   /* IF LIST SCDS REQUEST THEN IF SYSMOD SAVE SYSMOD ENTRY IN      */
*   /* ALTERNATE IOP. IF NOT SYSMOD THEN FROM ALTERNATE IOP OBTAIN   */
*   /* PRINTABLE VERSION OF ELEMENT NAME                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0188
*   IF LISTSCDS=ON                  /* IF LIST SCDS REQUEST          */
*     THEN                          /*                               */
         L     @14,@PC00001                                        0188
         TM    LISTSCDS(@14),B'00010000'                           0188
         BNO   @RF00188                                            0188
*     DO;                           /*                               */
*       CALL LCDSCDS;               /*                               */
         BAL   @14,LCDSCDS                                         0190
*       LCDRTNCD=MAX(LCDRTNCD,RTNCODE);/*                            */
         CR    LCDRTNCD,RTNCODE                                    0191
         BNL   *+6
         LR    LCDRTNCD,RTNCODE                                    0191
*     END;                          /*                               */
         EJECT
*                                                                  0193
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW PROCESS EACH TYPE ENTRY                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0193
*   IF LCDRTNCD=ZERO                /*                               */
*     THEN                          /*                               */
@RF00188 CH    LCDRTNCD,@CH00310                                   0193
         BNE   @RF00193                                            0193
*     DO;                           /*                               */
*       RFY                                                        0195
*         RTNCODE RSTD;             /*                               */
*       IF IOPTYPE=IOPSMD           /*                               */
*         THEN                      /*                               */
         CLI   IOPTYPE(IOPPTR),11                                  0196
         BNE   @RF00196                                            0196
*         CALL HMASMFPT(LISTPARM,HMASMIOP,LASTTYPE);/*               */
         L     @14,@PC00001                                        0197
         ST    @14,@AL00001                                        0197
         ST    IOPPTR,@AL00001+4                                   0197
         L     @14,@PC00001+8                                      0197
         ST    @14,@AL00001+8                                      0197
         L     @15,@CV01313                                        0197
         LA    @01,@AL00001                                        0197
         BALR  @14,@15                                             0197
*       ELSE                        /*                               */
*         IF IOPTYPE=IOPMOD         /*                               */
*           THEN                    /*                               */
         B     @RC00196                                            0198
@RF00196 CLI   IOPTYPE(IOPPTR),5                                   0198
         BNE   @RF00198                                            0198
*           CALL LCDMOD;            /*                               */
         BAL   @14,LCDMOD                                          0199
*         ELSE                      /*                               */
*           IF IOPTYPE=IOPMAC       /*                               */
*             THEN                  /*                               */
         B     @RC00198                                            0200
@RF00198 CLI   IOPTYPE(IOPPTR),3                                   0200
         BNE   @RF00200                                            0200
*             CALL LCDMAC;          /*                               */
         BAL   @14,LCDMAC                                          0201
*           ELSE                    /*                               */
*             IF IOPTYPE=IOPSRC     /*                               */
*               THEN                /*                               */
         B     @RC00200                                            0202
@RF00200 CLI   IOPTYPE(IOPPTR),8                                   0202
         BNE   @RF00202                                            0202
*               CALL LCDSRC;        /*                               */
         BAL   @14,LCDSRC                                          0203
*             ELSE                  /*                               */
*               IF IOPTYPE=IOPLMD   /*                               */
*                 THEN              /*                               */
         B     @RC00202                                            0204
@RF00202 CLI   IOPTYPE(IOPPTR),2                                   0204
         BNE   @RF00204                                            0204
*                 CALL LCDLMD;      /*                               */
         BAL   @14,LCDLMD                                          0205
*               ELSE                /*                               */
*                 IF IOPTYPE=IOPASM /*                               */
*                   THEN            /*                               */
         B     @RC00204                                            0206
@RF00204 CLI   IOPTYPE(IOPPTR),1                                   0206
         BNE   @RF00206                                            0206
*                   CALL LCDASM;    /*                               */
         BAL   @14,LCDASM                                          0207
*                 ELSE              /*                               */
*                   IF IOPTYPE=IOPDLB/*                              */
*                     THEN          /*                               */
         B     @RC00206                                            0208
@RF00206 CLI   IOPTYPE(IOPPTR),6                                   0208
         BNE   @RF00208                                            0208
*                     CALL LCDDLB;  /*                               */
         BAL   @14,LCDDLB                                          0209
*                   ELSE            /*                               */
*                     IF IOPTYPE=IOPSYS/*                            */
*                       THEN        /*                               */
         B     @RC00208                                            0210
@RF00208 CLI   IOPTYPE(IOPPTR),7                                   0210
         BNE   @RF00210                                            0210
*                       CALL LCDSYS;/*                               */
         BAL   @14,LCDSYS                                          0211
*                     ELSE          /*                               */
*                       RTNCODE=ZERO;/* IF NOT ONE OF ABOVE THEN MUST
*                                      BE SPECIAL TYPE ENTRY THAT IS
*                                      NOT DISPLAYED TO USER         */
         B     @RC00210                                            0212
@RF00210 SLR   RTNCODE,RTNCODE                                     0212
*       LCDRTNCD=MAX(LCDRTNCD,RTNCODE);/*                            */
@RC00210 DS    0H                                                  0213
@RC00208 DS    0H                                                  0213
@RC00206 DS    0H                                                  0213
@RC00204 DS    0H                                                  0213
@RC00202 DS    0H                                                  0213
@RC00200 DS    0H                                                  0213
@RC00198 DS    0H                                                  0213
@RC00196 CR    LCDRTNCD,RTNCODE                                    0213
         BNL   *+6
         LR    LCDRTNCD,RTNCODE                                    0213
*     END;                          /*                               */
         EJECT
*                                                                  0215
*   /*****************************************************************/
*   /*                                                               */
*   /* WRITE BLANK LINE TO SET ENTRY OFF FROM NEXT ONE               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0215
*   IF LCDRTNCD=ZERO                /*                               */
*     THEN                          /*                               */
@RF00193 CH    LCDRTNCD,@CH00310                                   0215
         BNE   @RF00215                                            0215
*     DO;                           /*                               */
*       CALL LCDWRITE;              /*                               */
         BAL   @14,LCDWRITE                                        0217
*       LCDRTNCD=MAX(LCDRTNCD,RTNCODE);/*                            */
         CR    LCDRTNCD,RTNCODE                                    0218
         BNL   *+6
         LR    LCDRTNCD,RTNCODE                                    0218
*     END;                          /*                               */
*                                                                  0219
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW RETURN TO CALLER                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0220
*   RFY                                                            0220
*     RTNCODE UNRSTD;               /*                               */
@RF00215 DS    0H                                                  0221
*   RETURN CODE(LCDRTNCD);          /*                               */
         LR    @15,LCDRTNCD                                        0221
         L     @13,4(,@13)                                         0221
         L     @14,12(,@13)                                        0221
         LM    @00,@12,20(@13)                                     0221
         BR    @14                                                 0221
         EJECT
*                                                                  0222
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINE USED TO RESET THE HMASMIO HEADING BUFFERS             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0222
*LCDSETHD:                                                         0222
*   PROCEDURE;                                                     0222
LCDSETHD STM   @14,@12,@SA00002                                    0222
*   RFY                                                            0223
*     HMASMIOP BASED(IOPPTR);       /*                               */
*   DECLARE                         /*                               */
*     HDINDX FIXED(31);             /* LOOP CONTER                   */
*   DECLARE                         /*                               */
*     SETDSID FIXED(8);             /*                               */
*                                                                  0225
*   /*****************************************************************/
*   /*                                                               */
*   /* SET DATASET ID IN HEADING LINE                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0226
*   SETDSID=IOPDSID;                /*                               */
         SLR   SETDSID,SETDSID                                     0226
         IC    SETDSID,IOPDSID(,IOPPTR)                            0226
*   IF LISTCDS=ON                   /*                               */
*     THEN                          /*                               */
         L     @08,@PC00001                                        0227
         TM    LISTCDS(@08),B'01000000'                            0227
         BNO   @RF00227                                            0227
*     HDDSID=TXTCDS;                /*                               */
         MVC   HDDSID(8),TXTCDS                                    0228
*   ELSE                            /*                               */
*     IF LISTACDS=ON                /*                               */
*       THEN                        /*                               */
         B     @RC00227                                            0229
@RF00227 L     @08,@PC00001                                        0229
         TM    LISTACDS(@08),B'00100000'                           0229
         BNO   @RF00229                                            0229
*       HDDSID=TXTACDS;             /*                               */
         MVC   HDDSID(8),TXTACDS                                   0230
*     ELSE                          /*                               */
*       HDDSID=TXTSCDS;             /*                               */
*                                                                  0231
         B     @RC00229                                            0231
@RF00229 MVC   HDDSID(8),TXTSCDS                                   0231
*   /*****************************************************************/
*   /*                                                               */
*   /* SET ENTRY TYPE IN HEADING LINE                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0232
*   IF IOPTYPE=IOPMOD               /*                               */
*     THEN                          /*                               */
@RC00229 DS    0H                                                  0232
@RC00227 CLI   IOPTYPE(IOPPTR),5                                   0232
         BNE   @RF00232                                            0232
*     HDTYPE=TXTMODHD;              /*                               */
         MVC   HDTYPE(19),TXTMODHD                                 0233
*   ELSE                            /*                               */
*     IF IOPTYPE=IOPMAC             /*                               */
*       THEN                        /*                               */
         B     @RC00232                                            0234
@RF00232 CLI   IOPTYPE(IOPPTR),3                                   0234
         BNE   @RF00234                                            0234
*       HDTYPE=TXTMACHD;            /*                               */
         MVC   HDTYPE(19),TXTMACHD                                 0235
*     ELSE                          /*                               */
*       IF IOPTYPE=IOPSRC           /*                               */
*         THEN                      /*                               */
         B     @RC00234                                            0236
@RF00234 CLI   IOPTYPE(IOPPTR),8                                   0236
         BNE   @RF00236                                            0236
*         HDTYPE=TXTSRCHD;          /*                               */
         MVC   HDTYPE(19),TXTSRCHD                                 0237
*       ELSE                        /*                               */
*         IF IOPTYPE=IOPLMD         /*                               */
*           THEN                    /*                               */
         B     @RC00236                                            0238
@RF00236 CLI   IOPTYPE(IOPPTR),2                                   0238
         BNE   @RF00238                                            0238
*           HDTYPE=TXTLMDHD;        /*                               */
         MVC   HDTYPE(19),TXTLMDHD                                 0239
*         ELSE                      /*                               */
*           IF IOPTYPE=IOPASM       /*                               */
*             THEN                  /*                               */
         B     @RC00238                                            0240
@RF00238 CLI   IOPTYPE(IOPPTR),1                                   0240
         BNE   @RF00240                                            0240
*             HDTYPE=TXTASMHD;      /*                               */
         MVC   HDTYPE(19),TXTASMHD                                 0241
*           ELSE                    /*                               */
*             IF IOPTYPE=IOPDLB     /*                               */
*               THEN                /*                               */
         B     @RC00240                                            0242
@RF00240 CLI   IOPTYPE(IOPPTR),6                                   0242
         BNE   @RF00242                                            0242
*               HDTYPE=TXTDLBHD;    /*                               */
         MVC   HDTYPE(19),TXTDLBHD                                 0243
*             ELSE                  /*                               */
*               IF IOPTYPE=IOPSYS   /*                               */
*                 THEN              /*                               */
         B     @RC00242                                            0244
@RF00242 CLI   IOPTYPE(IOPPTR),7                                   0244
         BNE   @RF00244                                            0244
*                 HDTYPE=TXTSYSHD;  /*                               */
*                                                                  0245
         MVC   HDTYPE(19),TXTSYSHD                                 0245
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP IOP TO DELETE EXISTING HEADINGS AND CALL IO TO DELETE  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0246
*   IOPBUFAD=ADDR(OUTAREA);         /* AND AIM AT THE PRINT LINE     */
@RF00244 DS    0H                                                  0246
@RC00242 DS    0H                                                  0246
@RC00240 DS    0H                                                  0246
@RC00238 DS    0H                                                  0246
@RC00236 DS    0H                                                  0246
@RC00234 DS    0H                                                  0246
@RC00232 LA    @08,OUTAREA                                         0246
         ST    @08,IOPBUFAD(,IOPPTR)                               0246
*   IOPDSID=CCALSTDS;               /* INDICATE PRINTER DATA SET     */
         MVC   IOPDSID(1,IOPPTR),CCALSTDS(CCAPTR)                  0247
*   IOPFUNCT=IOPWRITE;              /* FOR OUTPUT                    */
         MVI   IOPFUNCT(IOPPTR),X'51'                              0248
*   PRLHDADR=ZERO;                  /* ZERO OUT HEADING ADDRESS      */
         SLR   @05,@05                                             0249
         STCM  @05,7,PRLHDADR(@08)                                 0249
*   PRLRLEN=ZERO;                   /* INDICATE ZERO RECORD LENGTH 0250
*                                      USED TO RESET HEADINGS        */
         STH   @05,PRLRLEN(,@08)                                   0250
*   PRLFLGS=OFF;                    /* RESET ALL HEADINGS            */
         MVI   PRLFLGS(@08),B'00000000'                            0251
*   PRLHEADO=ON;                    /* WITH THESE TWO PARMS          */
         OI    PRLHEADO(@08),B'01000000'                           0252
*   CALL HMASMIO(HMASMIOP);         /* CALL ON THE I/O ROUTINE       */
*                                                                  0253
         ST    IOPPTR,@AL00001                                     0253
         L     @15,@CV01316                                        0253
         LA    @01,@AL00001                                        0253
         BALR  @14,@15                                             0253
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP ADDING ALL SYSMOD HEADINGS                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0254
*   PRLRLEN=LENGTH(PRNTBUF);        /* SET STANDARD LENGTH           */
         L     @08,IOPBUFAD(,IOPPTR)                               0254
         MVC   PRLRLEN(2,@08),@CH01365                             0254
*   PRLHEADO=OFF;                   /* KILL THE RESET SWITCH         */
*   PRLHEAD=ON;                     /* SET IN THE NEW HEADING        */
         OI    PRLHEAD(@08),B'10000000'                            0256
         NI    PRLHEADO(@08),B'10111111'                           0256
*   DO HDINDX=1 TO DIM(HEADPTR)     /* LOOP USING ALL THE HD PTRS    */
*         WHILE(IOPRETRN=IOPGOOD);  /*                               */
         LA    HDINDX,1                                            0257
@DL00257 CLI   IOPRETRN(IOPPTR),0                                  0257
         BNE   @DC00257                                            0257
*     IF HEADPTR(HDINDX)=ZERO       /* BLANK HEADING?                */
*       THEN                                                       0258
         SLR   @08,@08                                             0258
         LR    @05,HDINDX                                          0258
         SLA   @05,2                                               0258
         C     @08,HEADPTR-4(@05)                                  0258
         BNE   @RF00258                                            0258
*       DO;                                                        0259
*         PRLBLANK=ON;              /* INDICATE BLANK HEADING        */
         L     @05,IOPBUFAD(,IOPPTR)                               0260
         OI    PRLBLANK(@05),B'00100000'                           0260
*         PRLHDADR=ZERO;            /* CLEAR HEADING ADDRESS         */
         STCM  @08,7,PRLHDADR(@05)                                 0261
*       END;                                                       0262
*     ELSE                                                         0263
*       DO;                         /* NO PASS A HEADING ADDRESS     */
         B     @RC00258                                            0263
@RF00258 DS    0H                                                  0264
*         PRLBLANK=OFF;             /* NOT A BLANK LINE              */
         L     @08,IOPBUFAD(,IOPPTR)                               0264
         NI    PRLBLANK(@08),B'11011111'                           0264
*         PRLHDADR=HEADPTR(HDINDX); /* INSERT THE HEAD ADDR          */
         LR    @05,HDINDX                                          0265
         SLA   @05,2                                               0265
         L     @05,HEADPTR-4(@05)                                  0265
         STCM  @05,7,PRLHDADR(@08)                                 0265
*       END;                                                       0266
*     CALL HMASMIO(HMASMIOP);       /* PASS TO I/O ROUTINE           */
@RC00258 ST    IOPPTR,@AL00001                                     0267
         L     @15,@CV01316                                        0267
         LA    @01,@AL00001                                        0267
         BALR  @14,@15                                             0267
*   END;                                                           0268
         AH    HDINDX,@CH00046                                     0268
         CH    HDINDX,@CH00071                                     0268
         BNH   @DL00257                                            0268
@DC00257 DS    0H                                                  0269
*   PRLFLGS=OFF;                    /* RESET FLAGS                   */
         L     @08,IOPBUFAD(,IOPPTR)                               0269
         MVI   PRLFLGS(@08),B'00000000'                            0269
*   IOPBUFAD=SAVBUFAD;              /* RESTORE BUFFER ADDRESS        */
         ST    SAVBUFAD,IOPBUFAD(,IOPPTR)                          0270
*   IOPDSID=SETDSID;                /*                               */
         STC   SETDSID,IOPDSID(,IOPPTR)                            0271
*   RETURN CODE(IOPRETRN);          /*                               */
         SLR   @15,@15                                             0272
         IC    @15,IOPRETRN(,IOPPTR)                               0272
         L     @14,@SA00002                                        0272
         LM    @00,@12,@SA00002+8                                  0272
         BR    @14                                                 0272
*   END LCDSETHD;                   /* RETURN TO CALLER              */
         EJECT
*                                                                  0274
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINE CALLED FOR SCDS DATASET. IF ENTRY COMING IN IS SYSMOD */
*   /* THEN SAVE SYSMOD ENTRY IN ALTERNATE IOP TO BE USED LATER TO   */
*   /* GET ELEMENT NAMES. IF NOT SYSMOD THEN GET ALTERNATE ELEMENT   */
*   /* NAMES                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0274
*LCDSCDS:                           /*                               */
*   PROCEDURE;                      /*                               */
LCDSCDS  STM   @14,@12,12(@13)                                     0274
*   DECLARE                         /*                               */
*     SCDSX FIXED(31);              /* LOOP INDEX                    */
*   DECLARE                         /*                               */
*     SCDSPTR PTR(31);              /* PTR TO SCDS IOP               */
*   RFY                             /*                               */
*     HMASMIOP BASED(IOPPTR);       /* IOP WITH PASSED ENTRY         */
*   SCDSPTR=ADDR(SCDSIOP);          /* PTR TO XTRA IOP FOR SCDS      */
         L     SCDSPTR,@PC00001+12                                 0278
*   IF IOPTYPE=IOPSMD               /* IF SYSMOD ENTRY               */
*     THEN                          /* THEN SAVE IT                  */
         CLI   IOPTYPE(IOPPTR),11                                  0279
         BNE   @RF00279                                            0279
*     DO;                           /*                               */
*       SCDSPTR->HMASMIOP=HMASMIOP; /* FIXED PART OF IOP             */
         MVC   HMASMIOP(68,SCDSPTR),HMASMIOP(IOPPTR)               0281
*       SCDSPTR->IOPPTFNT=IOPPTFNT; /* FIXED PART OF SYSMOD ENTRY    */
         MVC   IOPPTFNT(104,SCDSPTR),IOPPTFNT(IOPPTR)              0282
*       DO SCDSX=1 BY 1 WHILE       /*                               */
*             IOPPTFND(SCDSX)^=IOPEOLST;/* MOVE ALL SUBENTRIES       */
         LA    SCDSX,1                                             0283
         B     @DE00283                                            0283
@DL00283 DS    0H                                                  0284
*         SCDSPTR->IOPPNTRY(SCDSX)=IOPPNTRY(SCDSX);/* MOVE ENTRY     */
         LR    @08,SCDSX                                           0284
         MH    @08,@CH00492                                        0284
         ST    @08,@TF00001                                        0284
         ALR   @08,SCDSPTR                                         0284
         LR    @01,IOPPTR                                          0284
         AL    @01,@TF00001                                        0284
         MVC   IOPPNTRY-9(9,@08),IOPPNTRY-9(@01)                   0284
*       END;                        /*                               */
         AH    SCDSX,@CH00046                                      0285
@DE00283 LA    @08,255                                             0285
         LR    @15,SCDSX                                           0285
         MH    @15,@CH00492                                        0285
         SLR   @14,@14                                             0285
         IC    @14,IOPPTFND-9(@15,IOPPTR)                          0285
         CR    @08,@14                                             0285
         BNE   @DL00283                                            0285
*       SCDSPTR->IOPPTFND(SCDSX)=IOPEOLST;/* SET END OF LIST         */
         STC   @08,IOPPTFND-9(@15,SCDSPTR)                         0286
*     END;                          /*                               */
*   ELSE                            /* IF NOT SYSMOD                 */
*     IOPNAME=SCDSPTR->IOPPMODS(IOPSEQNO);/* MOVE NAME OVER          */
         B     @RC00279                                            0288
@RF00279 LH    @08,IOPSEQNO(,IOPPTR)                               0288
         MH    @08,@CH00492                                        0288
         ALR   @08,SCDSPTR                                         0288
         MVC   IOPNAME(8,IOPPTR),IOPPMODS-9(@08)                   0288
*   RETURN CODE(ZERO);              /*                               */
@RC00279 SLR   @15,@15                                             0289
         L     @14,12(,@13)                                        0289
         LM    @00,@12,20(@13)                                     0289
         BR    @14                                                 0289
*   END LCDSCDS;                    /*                               */
         EJECT
*                                                                  0291
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINE CALLED TO FORMAT MODULE TYPE ENTRIES                  */
*   /*                                                               */
