         BAL   @14,DS1SYSLC                                        0172
*       RFY                                                        0173
*         RTNCODE RSTD;             /*                               */
*       DS1RTNCD=MAX(DS1RTNCD,RTNCODE);/* SAVE RC                    */
         CR    DS1RTNCD,RTNCODE                                    0174
         BNL   *+6
         LR    DS1RTNCD,RTNCODE                                    0174
*       RFY                                                        0175
*         RTNCODE UNRSTD;           /*                               */
*     END;                          /*                               */
*                                                                  0176
*   /*****************************************************************/
*   /*                                                               */
*   /* IF PREVIOUS FUNCTIONS WERE OK THEN CALL ROUTINE TO SET FOR    */
*   /* THOSE OPTIONS NOT SPECIFIED IN SYSTEM ENTRY                   */
*   /*                                                               */
*   /*****************************************************************/
