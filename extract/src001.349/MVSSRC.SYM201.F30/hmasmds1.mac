         BAL   @14,DS1SLCHK                                        0275
*               RFY                                                0276
*                 RTNCODE RSTD;     /*                               */
*               LCRTNCD=MAX(LCRTNCD,RTNCODE);/* SAVE RC              */
         CR    LCRTNCD,RTNCODE                                     0277
         BNL   *+6
         LR    LCRTNCD,RTNCODE                                     0277
*             END;                  /*                               */
*           IF LCRTNCD=ZERO         /*                               */
*             THEN                  /*                               */
@RF00273 C     LCRTNCD,@CF00253                                    0279
         BNE   @RF00279                                            0279
*             CALL DS1CCAST;        /* SET CCA FIELDS                */
         BAL   @14,DS1CCAST                                        0280
*         END;                      /*                               */
*                                                                  0281
*       /*************************************************************/
*       /*                                                           */
*       /* SYSTEM ENTRY IN CURRENT FORMAT BUT AT HIGHER CONVERSION   */
*       /* LEVEL                                                     */
*       /*                                                           */
*       /*************************************************************/
