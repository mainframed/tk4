*             THEN                  /*                               */
         CLI   IOPDSID(IOPPTR),2                                   0273
         BE    @GL00004                                            0273
         CLI   IOPDSID(IOPPTR),12                                  0273
         BNE   @RF00273                                            0273
@GL00004 L     @05,@CA00039                                        0273
         TM    CCAUPDU-HMASMCCA(@05),B'00000010'                   0273
         BNZ   @RF00273                                            0273
*             DO;                   /*                               */
