         CLI   IOPSCNVL(IOPPTR),2                                  0271
         BNE   @RF00271                                            0271
*         DO;                       /*                               */
*           IF(IOPDSID=IOPCDS       /* IF CDS                        */
*               ×IOPDSID=IOPACDS)   /* OR ACDS BUT NOT UCLIN         */
*               &CCAUPDU=OFF        /* (IE ONLY DURING FIRST CHECK 0273
