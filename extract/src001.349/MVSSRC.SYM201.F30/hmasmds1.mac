         L     @02,@CA00039                                        0203
         NI    CCACSYSF-HMASMCCA(@02),B'01111111'                  0203
*       CCANUCID=BLANK;             /*                               */
         MVI   CCANUCID-HMASMCCA(@02),C' '                         0204
