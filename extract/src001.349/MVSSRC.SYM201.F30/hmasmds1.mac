         MVI   CCACSREL-HMASMCCA+1(@02),C' '                       0205
         MVC   CCACSREL-HMASMCCA+2(2,@02),CCACSREL-HMASMCCA+1(@02) 0205
         MVI   CCACSREL-HMASMCCA(@02),C' '                         0205
*       CCACDSID=BLANK;             /*                               */
         MVI   CCACDSID-HMASMCCA+1(@02),C' '                       0206
         MVC   CCACDSID-HMASMCCA+2(6,@02),CCACDSID-HMASMCCA+1(@02) 0206
         MVI   CCACDSID-HMASMCCA(@02),C' '                         0206
*       CCAMTSNP=OFF;               /*                               */
