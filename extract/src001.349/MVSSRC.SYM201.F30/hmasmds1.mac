         L     @15,@CV01266                                        0193
         BALR  @14,@15                                             0193
*       RFY                                                        0194
*         RTNCODE RSTD;             /*                               */
*       DS1RTNCD=MAX(DS1RTNCD,RTNCODE);/* SAVE RC                    */
         CR    DS1RTNCD,RTNCODE                                    0195
         BNL   *+6
         LR    DS1RTNCD,RTNCODE                                    0195
*       RFY                                                        0196
*         RTNCODE UNRSTD;           /*                               */
*     END;                          /*                               */
*                                                                  0197
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO CALLER                                              */
*   /*                                                               */
