*   /*****************************************************************/
*                                                                  0254
*DS1SYSLC:                          /*                               */
*   PROCEDURE;                      /*                               */
DS1SYSLC STM   @14,@12,@SA00003                                    0254
*   DECLARE                         /*                               */
*     LCRTNCD FIXED(8);             /*                               */
*   RFY                                                            0256
*     HMASMIOP BASED(IOPPTR);       /*                               */
*   LCRTNCD=ZERO;                   /*                               */
         SLR   LCRTNCD,LCRTNCD                                     0257
*   IF PRIMED=NO                    /* IF IOP NOT ALREADY PRIMED     */
*     THEN                          /*                               */
         L     @05,@PC00001+4                                      0258
         CLI   PRIMED(@05),C'N'                                    0258
         BNE   @RF00258                                            0258
*     DO;                           /*                               */
*       IOPFUNCT=IOPOPEN;           /* THEN OPEN DATASET             */
         MVI   IOPFUNCT(IOPPTR),X'00'                              0260
*       CALL HMASMIO(HMASMIOP);     /* OPEN DATA SET                 */
         ST    IOPPTR,@AL00001                                     0261
         L     @15,@CV01265                                        0261
         LA    @01,@AL00001                                        0261
         BALR  @14,@15                                             0261
*       IF IOPRETRN=IOPGOOD         /* IF OPEN DONE OK               */
*         THEN                      /*                               */
         CLI   IOPRETRN(IOPPTR),0                                  0262
         BNE   @RF00262                                            0262
