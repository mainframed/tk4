*   RETURN CODE(ZERO);              /*                               */
@RF00216 DS    0H                                                  0252
@RC00210 DS    0H                                                  0252
@RC00201 SLR   @15,@15                                             0252
         L     @14,12(,@13)                                        0252
         LM    @00,@12,20(@13)                                     0252
         BR    @14                                                 0252
*   END DS1INIT;                    /*                               */
         EJECT
