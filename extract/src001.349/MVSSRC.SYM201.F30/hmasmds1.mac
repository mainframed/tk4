*           IOPFUNCT=IOPLOC;        /*                               */
         MVI   IOPFUNCT(IOPPTR),X'03'                              0264
*           IOPTYPE=IOPSYS;         /*                               */
         MVI   IOPTYPE(IOPPTR),X'07'                               0265
*           CALL HMASMIO(HMASMIOP); /* DATASET SYSTEM                */
         ST    IOPPTR,@AL00001                                     0266
         L     @15,@CV01265                                        0266
         LA    @01,@AL00001                                        0266
         BALR  @14,@15                                             0266
*         END;                      /*                               */
*     END;                          /*                               */
*                                                                  0268
@RF00262 DS    0H                                                  0269
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS DATA IF SYSTEM ENTRY AVAILABLE                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0269
*   IF IOPRETRN=IOPGOOD             /* IF SYS ENTRY AVAILABLE        */
*       &IOPFUNCT^=IOPSTOWD         /* AND NOT STOW DELETE. THIS   0269
*                                      SHOULD BE TREATED AS NOT FOUND
*                                      CONDITION                     */
*     THEN                          /* GET SYSTEM ENTRY              */
@RF00258 CLI   IOPRETRN(IOPPTR),0                                  0269
         BNE   @RF00269                                            0269
         CLI   IOPFUNCT(IOPPTR),83                                 0269
         BE    @RF00269                                            0269
*     DO;                           /*                               */
*                                                                  0270
*       /*************************************************************/
*       /*                                                           */
*       /* SYSTEM ENTRY IN CURRENT FORMAT AND AT CURRENT CONVERSION  */
