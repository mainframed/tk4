         MVC   MGPMGNO1(2),@CH00131                                0284
*           IF IOPDSID=IOPCDS       /*                               */
*             THEN                  /*                               */
         CLI   IOPDSID(IOPPTR),2                                   0285
         BNE   @RF00285                                            0285
*             MGPVARPT(1)=ADDR(CCADSIDA(IOPCDS));/*                  */
         L     @14,@CA00039                                        0286
         L     @14,CCADSAD-HMASMCCA(,@14)                          0286
         LA    @14,CCADSIDA+8(,@14)                                0286
         ST    @14,MGPVARPT                                        0286
*           ELSE                    /*                               */
*             IF IOPDSID=IOPACDS    /*                               */
*               THEN                /*                               */
         B     @RC00285                                            0287
@RF00285 CLI   IOPDSID(IOPPTR),12                                  0287
         BNE   @RF00287                                            0287
*               MGPVARPT(1)=ADDR(CCADSIDA(IOPACDS));/*               */
         L     @14,@CA00039                                        0288
         L     @14,CCADSAD-HMASMCCA(,@14)                          0288
         LA    @14,CCADSIDA+88(,@14)                               0288
         ST    @14,MGPVARPT                                        0288
*             ELSE                  /*                               */
*               MGPVARPT(1)=ADDR(CCADSIDA(IOPPTS));/*                */
         B     @RC00287                                            0289
@RF00287 L     @14,@CA00039                                        0289
         L     @14,CCADSAD-HMASMCCA(,@14)                          0289
         LA    @14,CCADSIDA+40(,@14)                               0289
         ST    @14,MGPVARPT                                        0289
*           IF IOPSCNVL>IOPSCNVC    /* IF AT HIGHER LVL              */
*             THEN                  /*                               */
@RC00287 DS    0H                                                  0290
@RC00285 CLI   IOPSCNVL(IOPPTR),2                                  0290
         BNH   @RF00290                                            0290
*             MGPVARPT(2)=ADDR(TXTHIGH);/* HIGHER                    */
         LA    @14,@CC01273                                        0291
         ST    @14,MGPVARPT+4                                      0291
*           ELSE                    /*                               */
*             MGPVARPT(2)=ADDR(TXTLOW);/* AT LOWER                   */
         B     @RC00290                                            0292
@RF00290 LA    @14,@CC01275                                        0292
         ST    @14,MGPVARPT+4                                      0292
*           MGPTYPE=MGPTERM;        /* SEVER ERROR                   */
@RC00290 MVI   MGPTYPE,X'05'                                       0293
*           CALL HMASMMSG(HMASMMGP);/*                               */
         L     @15,@CV01267                                        0294
         LA    @01,@AL00294                                        0294
