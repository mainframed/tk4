*       /*                                                           */
*       /* INIT FIELDS TO BE SET FROM PTS SYTEM ENTRY                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0216
*       IF IOPDSID=IOPPTS           /*                               */
*         THEN                      /*                               */
         B     @RC00210                                            0216
@RF00210 CLI   IOPDSID(IOPPTR),6                                   0216
         BNE   @RF00216                                            0216
*         DO;                       /*                               */
*           CCAPSYSF=OFF;           /* SYSTEM ENTRY NOT FOUND        */
         L     @02,@CA00039                                        0218
         NI    CCAPSYSF-HMASMCCA(@02),B'11011111'                  0218
*           CCAPTSNP=OFF;           /* PURGE AT ACCEPT               */
*           CCAPTSNJ=OFF;           /* REJECT AT RESTORE             */
         NI    CCAPTSNP-HMASMCCA(@02),B'01101111'                  0220
*           CCALKDNM(1)=BLANK;      /* INIT UTILITY NAMES TO BLANK   */
         L     @15,CCABLDLP-HMASMCCA(,@02)                         0221
         MVI   CCALKDNM(@15),C' '                                  0221
*           CCAASMNM(1)=BLANK;      /*                               */
         MVI   CCAASMNM(@15),C' '                                  0222
*           CCACPYNM(1)=BLANK;      /*                               */
         MVI   CCACPYNM(@15),C' '                                  0223
*           CCACOMNM(1)=BLANK;      /*                               */
         MVI   CCACOMNM(@15),C' '                                  0224
*           CCAZAPNM(1)=BLANK;      /*                               */
         MVI   CCAZAPNM(@15),C' '                                  0225
*           CCASUPNM(1)=BLANK;      /*                               */
         MVI   CCASUPNM(@15),C' '                                  0226
*           CCAUPDNM(1)=BLANK;      /*                               */
         MVI   CCAUPDNM(@15),C' '                                  0227
*           CCALKDPM(1)=BLANK;      /* INIT UTILITY PARMS TO BLANK   */
         MVI   CCALKDPM(@15),C' '                                  0228
*           CCAASMPM(1)=BLANK;      /*                               */
         MVI   CCAASMPM(@15),C' '                                  0229
*           CCACPYPM(1)=BLANK;      /*                               */
         MVI   CCACPYPM(@15),C' '                                  0230
*           CCACOMPM(1)=BLANK;      /*                               */
         MVI   CCACOMPM(@15),C' '                                  0231
*           CCAZAPPM(1)=BLANK;      /*                               */
         MVI   CCAZAPPM(@15),C' '                                  0232
*           CCASUPPM(1)=BLANK;      /*                               */
         MVI   CCASUPPM(@15),C' '                                  0233
*           CCAUPDPM(1)=BLANK;      /*                               */
         MVI   CCAUPDPM(@15),C' '                                  0234
*           CCALKDSP(1)=BLANK;      /* INIT UTILITY SYSPRINT DDNAME  */
         MVI   CCALKDSP(@15),C' '                                  0235
*           CCAASMSP(1)=BLANK;      /*                               */
         MVI   CCAASMSP(@15),C' '                                  0236
*           CCACPYSP(1)=BLANK;      /*                               */
         MVI   CCACPYSP(@15),C' '                                  0237
*           CCACOMSP(1)=BLANK;      /*                               */
         MVI   CCACOMSP(@15),C' '                                  0238
*           CCAZAPSP(1)=BLANK;      /*                               */
         MVI   CCAZAPSP(@15),C' '                                  0239
*           CCASUPSP(1)=BLANK;      /*                               */
         MVI   CCASUPSP(@15),C' '                                  0240
*           CCAUPDSP(1)=BLANK;      /*                               */
         MVI   CCAUPDSP(@15),C' '                                  0241
*           CCAPGLEN=PGLENDFT;      /* SET DEFAULT SMPOUT PAGE LEN   */
         MVC   CCAPGLEN-HMASMCCA(2,@02),@CH00807                   0242
*           CCALNLEN=LNLENDFT;      /* SET DEFAULT SMPOUT LINE LEN   */
         MVC   CCALNLEN-HMASMCCA(2,@02),@CH01284                   0243
*           CCARASM=ASMRCDFT;       /* SET DEFAULT RETURN CODES      */
         MVI   CCARASM(@15),X'04'                                  0244
*           CCARUPDT=UPDRCDFT;      /*                               */
         MVI   CCARUPDT(@15),X'00'                                 0245
*           CCARLINK=LKDRCDFT;      /*                               */
         MVI   CCARLINK(@15),X'08'                                 0246
*           CCARZAP=ZAPRCDFT;       /*                               */
         MVI   CCARZAP(@15),X'04'                                  0247
*           CCARCOPY=CPYRCDFT;      /*                               */
         MVI   CCARCOPY(@15),X'00'                                 0248
*           CCARCOMP=COMRCDFT;      /*                               */
         MVI   CCARCOMP(@15),X'00'                                 0249
*           CCARSUP=SUPRCDFT;       /*                               */
         MVI   CCARSUP(@15),X'00'                                  0250
*         END;                      /*                               */
*                                                                  0251
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN                                                        */
*   /*                                                               */
*   /*****************************************************************/
