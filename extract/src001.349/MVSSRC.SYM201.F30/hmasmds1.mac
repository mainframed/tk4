@RF00177 C     DS1RTNCD,@CF00253                                   0184
         BNE   @RF00184                                            0184
         L     @02,@CA00039                                        0184
         CH    OLDPEMAX,CCAPEMAX-HMASMCCA(,@02)                    0184
         BNL   @RF00184                                            0184
*     DO;                           /*                               */
*       CALL DS1GMIOP;              /*                               */
         BAL   @14,DS1GMIOP                                        0186
*       RFY                                                        0187
*         RTNCODE RSTD;             /*                               */
*       DS1RTNCD=MAX(DS1RTNCD,RTNCODE);/* SAVE RC                    */
         CR    DS1RTNCD,RTNCODE                                    0188
         BNL   *+6
         LR    DS1RTNCD,RTNCODE                                    0188
*       RFY                                                        0189
*         RTNCODE UNRSTD;           /*                               */
*     END;                          /*                               */
*                                                                  0190
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ALL PREVIOUS FUNCTIONS DONE OK AND PTS ENTRY PROCESSED CALL*/
