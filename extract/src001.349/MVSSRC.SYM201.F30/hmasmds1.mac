         B     @RC00271                                            0282
@RF00271 DS    0H                                                  0283
*           MGPCLEAR=MGPCLEAR&&MGPCLEAR;/* INIT                      */
         XC    MGPCLEAR(6),MGPCLEAR                                0283
*           MGPMGNO1=HMA204I;       /* XXXXXXXX AT XXXXXX FUNC LEVEL
