         BAL   @14,DS1DFLT                                         0179
*       RFY                                                        0180
*         RTNCODE RSTD;             /*                               */
*       DS1RTNCD=MAX(DS1RTNCD,RTNCODE);/* SAVE RC                    */
         CR    DS1RTNCD,RTNCODE                                    0181
         BNL   *+6
         LR    DS1RTNCD,RTNCODE                                    0181
*       RFY                                                        0182
*         RTNCODE UNRSTD;           /*                               */
*     END;                          /*                               */
*                                                                  0184
