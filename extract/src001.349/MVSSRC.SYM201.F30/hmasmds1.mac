*                                                                  0162
         L     @02,@CA00039                                        0162
         LH    OLDPEMAX,CCAPEMAX-HMASMCCA(,@02)                    0162
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL ROUTINE TO RE-INITIALIZE APPRIPRIATE FIELDS IN CCA BASED */
*   /* ON DATASET BEING PROCESSED                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0163
*   IF DS1RTNCD=ZERO                /* IF PREVIOUS FUNCTION OK       */
*     THEN                          /*                               */
         C     DS1RTNCD,@CF00253                                   0163
         BNE   @RF00163                                            0163
*     DO;                           /*                               */
*       CALL DS1INIT;               /* INIT CCA FIELDS               */
         BAL   @14,DS1INIT                                         0165
*       RFY                                                        0166
*         RTNCODE RSTD;             /*                               */
*       DS1RTNCD=MAX(DS1RTNCD,RTNCODE);/* SAVE RC                    */
         CR    DS1RTNCD,RTNCODE                                    0167
         BNL   *+6
         LR    DS1RTNCD,RTNCODE                                    0167
*       RFY                                                        0168
*         RTNCODE UNRSTD;           /*                               */
*     END;                          /*                               */
*                                                                  0170
