         B     @RC00201                                            0210
@RF00201 CLI   IOPDSID(IOPPTR),12                                  0210
         BNE   @RF00210                                            0210
*       DO;                         /*                               */
*         CCAASYSF=OFF;             /*                               */
         L     @02,@CA00039                                        0212
         NI    CCAASYSF-HMASMCCA(@02),B'10111111'                  0212
*         CCAASREL=BLANK;           /*                               */
         MVI   CCAASREL-HMASMCCA+1(@02),C' '                       0213
         MVC   CCAASREL-HMASMCCA+2(2,@02),CCAASREL-HMASMCCA+1(@02) 0213
         MVI   CCAASREL-HMASMCCA(@02),C' '                         0213
*         CCAACDID=BLANK;           /*                               */
         MVI   CCAACDID-HMASMCCA+1(@02),C' '                       0214
         MVC   CCAACDID-HMASMCCA+2(6,@02),CCAACDID-HMASMCCA+1(@02) 0214
         MVI   CCAACDID-HMASMCCA(@02),C' '                         0214
*       END;                        /*                               */
*     ELSE                          /* IF NOT ACDS                   */
