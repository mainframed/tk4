         TITLE 'HMASMDS1 - SET CCA FROM SYSTEM ENTRY'
HMASMDS1 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMDS1  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  LA    @10,4095(,@12)                                      0001
         USING @PSTART,@12                                         0001
         USING @PSTART+4095,@10                                    0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(8),0(@01)                                  0001
*   DS1RTNCD=ZERO;                  /*                               */
         SLR   DS1RTNCD,DS1RTNCD                                   0157
*   IOPPTR=ADDR(DS1IOP);            /* PTR TO IOP PASSED             */
         L     IOPPTR,@PC00001                                     0158
