*   VLPDATA(1:SCPPMLN)=SCPSTRNG(1:SCPPMLN);/* PUT OPERAND SCANNED IN
*                                      VLU DATA FIELD                */
         LH    @15,SCPPMLN                                         0337
         BCTR  @15,0                                               0337
         L     @10,SCPCHAR                                         0337
         EX    @15,@SM01571                                        0337
*   VLPDATA(8)=VERCNTR+1;           /* PUT ++VER NUMBER IN VLU DATA  */
*                                                                  0338
         LA    @15,1                                               0338
