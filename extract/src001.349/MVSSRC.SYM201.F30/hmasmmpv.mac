         TITLE 'HMASMMPV - ++VER PARSE MODULE'
HMASMMPV CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMMPV  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*   MPVRTNCD=ZERO;                  /* ERROR CODE                    */
         SLR   @10,@10                                             0175
         ST    @10,MPVRTNCD                                        0175
*   MCBTYPE=MCBVERTP;               /* MCB TYPE                      */
*                                                                  0176
         L     @10,@PC00001                                        0176
         L     @10,MPPMCBAD(,@10)                                  0176
         MVI   MCBTYPE(@10),X'02'                                  0176
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE MESSAGE AND SCAN PARAMETERS                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0177
*   RFY                             /*                               */
*     RTNCODE RSTD;                 /*                               */
*   CALL SETUP;                     /*                               */
         BAL   @14,SETUP                                           0178
*   MPVRTNCD=MAX(MPVRTNCD,RTNCODE); /*                               */
*                                                                  0179
         L     @14,MPVRTNCD                                        0179
         CR    @14,RTNCODE                                         0179
         BNL   *+6
         LR    @14,RTNCODE                                         0179
         ST    @14,MPVRTNCD                                        0179
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL ROUTINE TO HANDLE SCN INTERFACE                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0180
*   IF MPVRTNCD<STOPPTF             /*                               */
*     THEN                          /*                               */
         CH    @14,@CH00116                                        0180
         BNL   @RF00180                                            0180
*     DO;                           /*                               */
*       CALL SCNCALL;               /*                               */
         BAL   @14,SCNCALL                                         0182
*       MPVRTNCD=MAX(MPVRTNCD,RTNCODE);/*                            */
         L     @14,MPVRTNCD                                        0183
         CR    @14,RTNCODE                                         0183
         BNL   *+6
         LR    @14,RTNCODE                                         0183
         ST    @14,MPVRTNCD                                        0183
*     END;                          /*                               */
*                                                                  0184
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO CALLER WITH HIGHEST ERROR CODE                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0185
*   RFY                             /*                               */
*     RTNCODE UNRSTD;               /*                               */
@RF00180 DS    0H                                                  0186
*   RETURN CODE(MPVRTNCD);                                         0186
         L     @15,MPVRTNCD                                        0186
         L     @13,4(,@13)                                         0186
         L     @14,12(,@13)                                        0186
         LM    @00,@12,20(@13)                                     0186
         BR    @14                                                 0186
         EJECT
*                                                                  0187
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE SCN , MSG AND MCB PARMS                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0187
*SETUP:                                                            0187
*   PROC;                           /*                               */
@EL00001 L     @13,4(,@13)                                         0187
@EF00001 DS    0H                                                  0187
@ER00001 LM    @14,@12,12(@13)                                     0187
         BR    @14                                                 0187
SETUP    STM   @14,@12,12(@13)                                     0187
*   CRPSCPAD=ADDR(HMASMSCP);        /* FOR READ ROUTINE              */
         L     @10,@PC00001                                        0188
         L     @15,MPPCRDPM(,@10)                                  0188
         LA    @14,HMASMSCP                                        0188
         ST    @14,CRPSCPAD(,@15)                                  0188
*   TYPES=''B;                      /* KEYWORD TYPE BITS             */
         XC    TYPES(2),TYPES                                      0189
*   MPVSW=''B;                      /* CLEAR INTERNAL SWITCH         */
         MVI   MPVSW,X'00'                                         0190
*   IOPPTR=MPPIOPAD;                /* IOP PASSED FROM CALLER        */
         L     @14,MPPIOPAD(,@10)                                  0191
         ST    @14,IOPPTR                                          0191
*   MCBSFMID=BLANK;                 /* BLANK THE FIELD               */
*                                                                  0192
         L     @09,MPPMCBAD(,@10)                                  0192
         MVI   MCBSFMID+1(@09),C' '                                0192
         MVC   MCBSFMID+2(5,@09),MCBSFMID+1(@09)                   0192
         MVI   MCBSFMID(@09),C' '                                  0192
*   /*****************************************************************/
*   /*                                                               */
*   /* MCB INITIALIZATION                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0193
*   MCBVND(1)=IOPEOLST;             /* INIT FIRST ENTRY TO END LIST  */
*                                                                  0193
         MVI   MCBVND(@09),X'FF'                                   0193
*   /*****************************************************************/
*   /*                                                               */
*   /* SCN PARAMETERS                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0194
*   SCPCHAR=ADDR(CRPBUFFR);         /* ADDRESS OF BUFFER CONTAINING
*                                      INPUT                         */
         LA    @09,CRPBUFFR(,@15)                                  0194
         ST    @09,SCPCHAR                                         0194
*   SCPSRCH=ADDR(K2PLUS);           /* DSCANS FOR VER SEARCH         */
         LA    @09,K2PLUS                                          0195
         ST    @09,SCPSRCH                                         0195
*   SCPIORTN=MPPCRDAD;              /* IO ROUTINE FOR CONTINUATION   */
         MVC   SCPIORTN(4),MPPCRDAD(@10)                           0196
*   SCPIOPAD=MPPCRDPM;              /* PARAMETER LIST FOR ABOVE      */
         ST    @15,SCPIOPAD                                        0197
*   SCPINLN=LENGTH(CRPBFCTL);       /* LENGTN OF INPUT RECORD        */
         MVC   SCPINLN(2),@CH00838                                 0198
*   SCPEOR=''B;                     /* CLEAR FLAGS                   */
         MVI   SCPEOR,X'00'                                        0199
*   SCPCONT=ON;                     /* CONTINUATION ALLOWED          */
*   SCPCOMNT=ON;                    /* SKIP OVER COMMENTS            */
*                                                                  0201
         OI    SCPCONT,B'10100000'                                 0201
*   /*****************************************************************/
*   /*                                                               */
*   /* MESSAGE PARMS, ALL MSGS TO LOG AND PRINTER                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0202
*   HMASMMGP=''B;                   /*                               */
         XC    HMASMMGP(24),HMASMMGP                               0202
*   MGPHLDS=ON;                     /*                               */
*   MGPPRINT=ON;                    /*                               */
         OI    MGPHLDS,B'10010000'                                 0204
*                                                                  0205
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE THE NUMBER OF ++VERS IN THE IOP PASSED              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0205
*   VERCNTR=0;                      /* CLEAR COUNT OF ++VERS         */
         SLR   @10,@10                                             0205
         STH   @10,VERCNTR                                         0205
*   ASV#=0;                         /* CLEAR ASSOC. ++VER NUMBER     */
         MVI   ASV#,X'00'                                          0206
*   IF MPPIOPAD^=ZERO               /* IOP PASSED                    */
*     THEN                                                         0207
         CR    @14,@10                                             0207
         BE    @RF00207                                            0207
*     DO;                                                          0208
*       DO JJ=1 BY 1 WHILE IOPPTFND(JJ)^=IOPEOLST;                 0209
         LA    @10,1                                               0209
         STH   @10,JJ                                              0209
         B     @DE00209                                            0209
@DL00209 DS    0H                                                  0210
*         IF IOPPIND(JJ)=IOPPSREL   /* SREL SUB-ENTRY                */
*             &IOPSRLST(JJ)^=ASV#   /* WITH DIFFERENT ASSOC. ++VER # */
*           THEN                                                   0210
         L     @10,IOPPTR                                          0210
         LH    @15,JJ                                              0210
         MH    @15,@CH00438                                        0210
         SLR   @14,@14                                             0210
         IC    @14,IOPPIND-9(@15,@10)                              0210
         CH    @14,@CH01053                                        0210
         BNE   @RF00210                                            0210
         ST    @15,@TF00001                                        0210
         ALR   @15,@10                                             0210
         CLC   IOPSRLST-9(1,@15),ASV#                              0210
         BE    @RF00210                                            0210
*           DO;                                                    0211
*             VERCNTR=VERCNTR+1;    /* INCREMENT ++VER COUNTER       */
         LH    @01,VERCNTR                                         0212
         LA    @01,1(,@01)                                         0212
         STH   @01,VERCNTR                                         0212
*             ASV#=IOPSRLST(JJ);    /* SAVE ASSOC. ++VER NUMBER      */
         AL    @10,@TF00001                                        0213
         MVC   ASV#(1),IOPSRLST-9(@10)                             0213
*           END;                                                   0214
*       END;                                                       0215
@RF00210 LA    @10,1                                               0215
         AH    @10,JJ                                              0215
         STH   @10,JJ                                              0215
@DE00209 LH    @10,JJ                                              0215
         MH    @10,@CH00438                                        0215
         L     @01,IOPPTR                                          0215
         SLR   @00,@00                                             0215
         IC    @00,IOPPTFND-9(@10,@01)                             0215
         CH    @00,@CH00627                                        0215
         BNE   @DL00209                                            0215
*     END;                                                         0216
*   RETURN CODE(ZERO);              /*                               */
@RF00207 SLR   @15,@15                                             0217
         L     @14,12(,@13)                                        0217
         LM    @00,@12,20(@13)                                     0217
         BR    @14                                                 0217
*   END SETUP;                      /*                               */
         EJECT
*                                                                  0219
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL SCN TO PARSE THE ++VER STATEMENT                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0219
*SCNCALL:                                                          0219
*   PROC;                           /*                               */
SCNCALL  STM   @14,@12,@SA00003                                    0219
*   DECLARE                         /*                               */
*     SCNRC FIXED(8);               /*                               */
*   SCNRC=ZERO;                     /*                               */
*                                                                  0221
         MVI   SCNRC,X'00'                                         0221
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP PARM FOR VLU                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0222
*   HMASMVLP=''B;                   /* SET UP VLU PARM               */
         XC    HMASMVLP(52),HMASMVLP                               0222
*   VLPFUNCT=VLPADD;                /*                               */
         MVI   VLPFUNCT,X'01'                                      0223
*   VLPFMSG=OFF;                    /*                               */
         NI    VLPFMSG,B'01111111'                                 0224
*   VLPMTYPE=CCATYPEA(IOPSMD);      /*                               */
         L     @10,@CA00042                                        0225
         L     @10,CCATPAD-HMASMCCA(,@10)                          0225
         MVC   VLPMTYPE(8),CCATYPEA+80(@10)                        0225
*   VLPMNAME=MPPSMDNM;              /*                               */
*                                                                  0226
         MVI   VLPMNAME+7,C' '                                     0226
         L     @10,@PC00001                                        0226
         MVC   VLPMNAME(7),MPPSMDNM(@10)                           0226
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN MCS CARD                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0227
*   CALL HMASMSCN(HMASMSCP);        /* SCAN VER HEADER RECORD        */
*                                                                  0227
         L     @15,@CV01463                                        0227
         LA    @01,@AL00227                                        0227
         BALR  @14,@15                                             0227
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK RETURN CODE. SCN RETURN FOR SYNTAX AND ROUT ERRORS. CRD */
*   /* PARMS FOR PREMATURE END OF FILE AND IO ERRORS.                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0228
*   IF SCPRET=SCPBAD THEN           /* SYNTAX ERROR                  */
         CLI   SCPRET,4                                            0228
         BNE   @RF00228                                            0228
*     DO;                           /*                               */
*       SCNRC=STOPPTF;              /* FLUSH THE SYSMOD              */
         MVI   SCNRC,X'08'                                         0230
*       IF SCPRTERR=OFF             /* SCAN FOUND SYNTAX ERROR       */
*         THEN                      /*                               */
         TM    SCPRTERR,B'00010000'                                0231
         BNZ   @RF00231                                            0231
*         DO;                       /*                               */
*           CALL SYNTAXMG;          /* ISSUE MESSAGE                 */
         BAL   @14,SYNTAXMG                                        0233
*           RFY                     /*                               */
*             RTNCODE RSTD;         /*                               */
*           SCNRC=MAX(SCNRC,RTNCODE);/*                              */
         SLR   @14,@14                                             0235
         IC    @14,SCNRC                                           0235
         CR    @14,RTNCODE                                         0235
         BNL   *+6
         LR    @14,RTNCODE                                         0235
         STC   @14,SCNRC                                           0235
*           RFY                     /*                               */
*             RTNCODE UNRSTD;       /*                               */
*         END;                      /*                               */
*     END;                          /*                               */
@RF00231 DS    0H                                                  0239
*   IF CRPRETRN^=CRPGOOD THEN       /* READ CONTINUATION ERROR       */
@RF00228 L     @10,@PC00001                                        0239
         L     @10,MPPCRDPM(,@10)                                  0239
         CLI   CRPRETRN(@10),0                                     0239
         BE    @RF00239                                            0239
*     DO;                           /*                               */
*       IF CRPRETRN=CRPEOF THEN     /* PREMATURE EOF                 */
         CLI   CRPRETRN(@10),4                                     0241
         BNE   @RF00241                                            0241
*         DO;                       /*                               */
*           MGPMGNO1=HMA252I;       /* INCOMPLETE CONTROL STATEMENT  */
         MVC   MGPMGNO1(2),@CH01467                                0243
*           MGPTYPE=MGPERR;         /*                               */
         MVI   MGPTYPE,X'03'                                       0244
*           CALL HMASMMSG(HMASMMGP);/*                               */
         L     @15,@CV01462                                        0245
         LA    @01,@AL00245                                        0245
         BALR  @14,@15                                             0245
*           SCNRC=STOPPTF;          /*                               */
         MVI   SCNRC,X'08'                                         0246
*         END;                      /*                               */
*       ELSE                        /* OR IO ERROR                   */
*         SCNRC=CRPRETRN;           /*                               */
         B     @RC00241                                            0248
@RF00241 L     @10,@PC00001                                        0248
         L     @10,MPPCRDPM(,@10)                                  0248
         MVC   SCNRC(1),CRPRETRN(@10)                              0248
*     END;                          /*                               */
@RC00241 DS    0H                                                  0250
*   RETURN CODE(SCNRC);             /*                               */
@RF00239 SLR   @15,@15                                             0250
         IC    @15,SCNRC                                           0250
         L     @14,@SA00003                                        0250
         LM    @00,@12,@SA00003+8                                  0250
         BR    @14                                                 0250
*   END SCNCALL;                    /*                               */
         EJECT
*                                                                  0252
*   /*****************************************************************/
*   /*                                                               */
*   /* SYNTAX ERROR MESSAGE ROUTINE                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0252
*SYNTAXMG:                                                         0252
*   PROC;                                                          0252
SYNTAXMG STM   @14,@12,@SA00004                                    0252
*   DECLARE                                                        0253
*     HEXNO FIXED(8);               /* HEXADECIMAL NUMBER            */
*   DECLARE                                                        0254
*     1 DECNO BDY(DWORD),           /* CONVERT DECIMAL NUMBER        */
*       3 DECNO1 CHAR(8),           /* ACTUAL DECIMAL NUMBER         */
*       3 * CHAR(1);                /* EXTRA FOR UNPACK              */
*   DECLARE                                                        0255
*     1 BCDNO BDY(WORD) CHAR(5),    /* EBCDIC NUMBER                 */
*       3 * CHAR(1),                /* PADDING                       */
*       3 BCDCOL CHAR(2),           /* COLUMN NUMBER                 */
*       3 * CHAR(2);                /* FOR SIGN                      */
*   DECLARE                                                        0256
*     COLSTR FIXED(8) CONSTANT(7);  /* START FOR UNPACK              */
*   DECLARE                                                        0257
*     COLSTP FIXED(8) CONSTANT(9);  /* END FOR UNPACK                */
*   HEXNO=SCPCHAR-ADDR(CRPBUFFR)+1; /* GET COLUMN NUMBER             */
         L     @10,@PC00001                                        0258
         L     @10,MPPCRDPM(,@10)                                  0258
         LA    @10,CRPBUFFR(,@10)                                  0258
         LCR   @10,@10                                             0258
         AL    @10,SCPCHAR                                         0258
         LA    @10,1(,@10)                                         0258
         STC   @10,HEXNO                                           0258
*   CVD(HEXNO,DECNO1);              /* CONVERT NO. TO DECIMAL        */
         CVD   @10,DECNO1                                          0259
*   UNPK(BCDNO,DECNO(COLSTR:COLSTP));/* UNPACK NO.                   */
         UNPK  BCDNO(5),DECNO+6(3)                                 0260
*   MGPMGNO1=HMA203I;               /* SYNTAX ERROR IN               */
         MVC   MGPMGNO1(2),@CH00044                                0261
*   MGPMGNO2=PART021;               /* CONTROL STATMENT              */
         MVC   MGPMGNO2(2),@CH00458                                0262
*   MGPMGNO3=PART026;               /* AT COL                        */
         MVC   MGPMGNO3(2),@CH00468                                0263
*   MGPVARPT(1)=ADDR(BCDCOL);       /* MESSAGE TEXT = COLUMN NUMBER  */
         LA    @10,BCDCOL                                          0264
         ST    @10,MGPVARPT                                        0264
*   MGPTYPE=MGPERR;                 /* MESSAGE SEVERITY              */
         MVI   MGPTYPE,X'03'                                       0265
*   CALL HMASMMSG(HMASMMGP);        /* CALL MESSAGE ROUTINE          */
         L     @15,@CV01462                                        0266
         LA    @01,@AL00266                                        0266
         BALR  @14,@15                                             0266
*   RETURN CODE(ZERO);              /*                               */
         SLR   @15,@15                                             0267
         L     @14,@SA00004                                        0267
         LM    @00,@12,@SA00004+8                                  0267
         BR    @14                                                 0267
*   END SYNTAXMG;                                                  0268
         EJECT
*                                                                  0269
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINES USED DURING 'K2PLUS' DSCAN CHAIN                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0269
*MPVPROCS:                          /*                               */
*   PROC;                           /*                               */
MPVPROCS STM   @14,@12,@SA00005                                    0269
*   RFY                                                            0270
*     RTNREG RSTD;                                                 0270
*   RFY                                                            0271
*     REG1 RSTD;                                                   0271
*   DECLARE                                                        0272
*     SAV14 PTR(31);                /*                               */
*   DECLARE                                                        0273
*     SAV1 PTR(31);                 /*                               */
*                                                                  0273
*   /*****************************************************************/
*   /*                                                               */
*   /* PREPARE FOR PROCESSING ++VER WITH SREL                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0274
*RVER:                                                             0274
*   ENTRY;                          /*                               */
         B     @EC00274                                            0274
RVER     STM   @14,@12,@SA00005                                    0274
@EC00274 DS    0H                                                  0275
*   SREL=ON;                        /* FOR CONTROL OF RVERV          */
         OI    SREL,B'10000000'                                    0275
*   VLPTYPE=IOPPSREL;               /*                               */
         MVI   VLPTYPE,X'A1'                                       0276
*   VLPSTYPE=SCPSTRNG(1:SCPPMLN);   /*                               */
         MVI   VLPSTYPE+1,C' '                                     0277
         MVC   VLPSTYPE+2(14),VLPSTYPE+1                           0277
         LH    @15,SCPPMLN                                         0277
         BCTR  @15,0                                               0277
         L     @10,SCPCHAR                                         0277
         EX    @15,@SM01562                                        0277
*   SCPRET=SCPGOOD;                 /*                               */
*                                                                  0278
         MVI   SCPRET,X'00'                                        0278
*   /*****************************************************************/
*   /*                                                               */
*   /* IF AN IOP IS BEING PROCESSED THEN CHECK TO SEE IF THERE ARE   */
*   /* TOO MANY ++VERS SUPPLIED FOR THE SYSMOD BEING RECEIVED        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0279
*   IF MPPIOPAD^=ZERO THEN          /* IF IOP BEING PROCESSED        */
         L     @15,@PC00001                                        0279
         ICM   @10,15,MPPIOPAD(@15)                                0279
         BZ    @RF00279                                            0279
*     DO;                           /*                               */
*       IF VERCNTR>=VERMAX          /* TOO MANY VER STATEMENTS       */
*         THEN                                                     0281
         LH    @15,VERCNTR                                         0281
         CH    @15,@CH00627                                        0281
         BL    @RF00281                                            0281
*         SCPRET=SCPSTOP;           /* GET SYNTAX ERROR              */
         MVI   SCPRET,X'08'                                        0282
*     END;                          /*                               */
*   GOTO GOODSCAN;                  /* COMMO EXIT TO SC              */
         B     GOODSCAN                                            0284
*                                                                  0285
*   /*****************************************************************/
*   /*                                                               */
*   /* FMID FOUND, SAVE TYPE FOR LATER USE WHEN INSERTING VALUE AND  */
*   /* TYPE INTO MCB LIST                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0285
*RFMID:                                                            0285
*   ENTRY;                          /*                               */
RFMID    STM   @14,@12,@SA00005                                    0285
*   VLPTYPE=IOPPFID;                /*                               */
         MVI   VLPTYPE,X'A7'                                       0286
*   VLPSTYPE=SCPSTRNG(1:SCPPMLN);   /*                               */
         MVI   VLPSTYPE+1,C' '                                     0287
         MVC   VLPSTYPE+2(14),VLPSTYPE+1                           0287
         LH    @15,SCPPMLN                                         0287
         BCTR  @15,0                                               0287
         L     @10,SCPCHAR                                         0287
         EX    @15,@SM01562                                        0287
*   IF FMID=ON THEN                 /* DUPLICATE                     */
         TM    FMID,B'01000000'                                    0288
         BNO   @RF00288                                            0288
*     SCPRET=SCPSTOP;               /*                               */
         MVI   SCPRET,X'08'                                        0289
*   FMID=ON;                        /*                               */
@RF00288 OI    FMID,B'01000000'                                    0290
*   GOTO GOODSCAN;                  /* COMMO EXIT TO SC              */
*                                                                  0291
         B     GOODSCAN                                            0291
*   /*****************************************************************/
*   /*                                                               */
*   /* PRE FOUND, INDICATE TYPE FOR USE IN MCB LIST                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0292
*RPRE:                                                             0292
*   ENTRY;                          /*                               */
RPRE     STM   @14,@12,@SA00005                                    0292
*   VLPTYPE=IOPPPRE;                /*                               */
         MVI   VLPTYPE,X'A2'                                       0293
*   VLPSTYPE=SCPSTRNG(1:SCPPMLN);   /*                               */
         MVI   VLPSTYPE+1,C' '                                     0294
         MVC   VLPSTYPE+2(14),VLPSTYPE+1                           0294
         LH    @15,SCPPMLN                                         0294
         BCTR  @15,0                                               0294
         L     @10,SCPCHAR                                         0294
         EX    @15,@SM01562                                        0294
*   IF PRE=ON THEN                  /* DUPLICATE SPECIFICATION OF    */
         TM    PRE,B'00100000'                                     0295
         BNO   @RF00295                                            0295
*     SCPRET=SCPSTOP;               /* KEYWORD, SYNTAX ERROR         */
         MVI   SCPRET,X'08'                                        0296
*   PRE=ON;                         /*                               */
@RF00295 OI    PRE,B'00100000'                                     0297
*   GOTO GOODSCAN;                  /* COMMO EXIT TO SC              */
*                                                                  0298
         B     GOODSCAN                                            0298
*   /*****************************************************************/
*   /*                                                               */
*   /* NPRE FOUND, INDICATE TYPE FOR USE IN MCB LIST                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0299
*RNPRE:                                                            0299
*   ENTRY;                          /*                               */
RNPRE    STM   @14,@12,@SA00005                                    0299
*   VLPTYPE=IOPPNPR;                /*                               */
         MVI   VLPTYPE,X'A3'                                       0300
*   VLPSTYPE=SCPSTRNG(1:SCPPMLN);   /*                               */
         MVI   VLPSTYPE+1,C' '                                     0301
