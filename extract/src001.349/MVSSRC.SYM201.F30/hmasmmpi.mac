         TITLE 'HMASMMPI - ++IF  PARSE MODULE'
HMASMMPI CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMMPI  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*   MPIRTNCD=ZERO;                  /*                               */
         SLR   @10,@10                                             0097
         ST    @10,MPIRTNCD                                        0097
*   MCBTYPE=MCBIFTP;                /* INDICATE IF TYPE MCB          */
*                                                                  0098
         L     @10,@PC00001                                        0098
         L     @10,MPPMCBAD(,@10)                                  0098
         MVI   MCBTYPE(@10),X'03'                                  0098
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL ROUTINE TO INITIALIZE PARMS ETC.                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0099
*   RFY                             /*                               */
*     RTNCODE RSTD;                 /*                               */
*   CALL SETUP;                     /*                               */
         BAL   @14,SETUP                                           0100
*   MPIRTNCD=MAX(MPIRTNCD,RTNCODE); /*                               */
*                                                                  0101
         L     @14,MPIRTNCD                                        0101
         CR    @14,RTNCODE                                         0101
         BNL   *+6
         LR    @14,RTNCODE                                         0101
         ST    @14,MPIRTNCD                                        0101
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL SCN TO PARSE THE ++IF STATEMENT                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0102
*   IF MPIRTNCD<STOPSMD             /*                               */
*     THEN                          /*                               */
         CH    @14,@CH00116                                        0102
         BNL   @RF00102                                            0102
*     DO;                           /*                               */
*       CALL SCNCALL;               /*                               */
         BAL   @14,SCNCALL                                         0104
*       MPIRTNCD=MAX(MPIRTNCD,RTNCODE);/*                            */
         L     @14,MPIRTNCD                                        0105
         CR    @14,RTNCODE                                         0105
         BNL   *+6
         LR    @14,RTNCODE                                         0105
         ST    @14,MPIRTNCD                                        0105
*     END;                          /*                               */
*                                                                  0106
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO CALLER WITH HIGHEST ERROR CODE                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0107
*   RFY                             /*                               */
*     RTNCODE UNRSTD;               /*                               */
@RF00102 DS    0H                                                  0108
*   RETURN CODE(MPIRTNCD);                                         0108
         L     @15,MPIRTNCD                                        0108
         L     @13,4(,@13)                                         0108
         L     @14,12(,@13)                                        0108
         LM    @00,@12,20(@13)                                     0108
         BR    @14                                                 0108
         EJECT
*                                                                  0109
*   /*****************************************************************/
*   /*                                                               */
*   /* INIT MESSAGE, SCAN, READ PARMS                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0109
*SETUP:                                                            0109
*   PROC;                           /*                               */
@EL00001 L     @13,4(,@13)                                         0109
@EF00001 DS    0H                                                  0109
@ER00001 LM    @14,@12,12(@13)                                     0109
         BR    @14                                                 0109
SETUP    STM   @14,@12,12(@13)                                     0109
*   TYPES=''B;                      /* KEYWORD TYPE BITS             */
         XC    TYPES(2),TYPES                                      0110
*   CRPSCPAD=ADDR(HMASMSCP);        /* FOR SCN CRD INTERFACE         */
*                                                                  0111
         L     @10,@PC00001                                        0111
         L     @15,MPPCRDPM(,@10)                                  0111
         LA    @14,HMASMSCP                                        0111
         ST    @14,CRPSCPAD(,@15)                                  0111
*   /*****************************************************************/
*   /*                                                               */
*   /* SCN PARAMETERS                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0112
*   SCPCHAR=ADDR(CRPBUFFR);         /* ADDRESS OF BUFFER CONTAINING
*                                      INPUT                         */
         LA    @14,CRPBUFFR(,@15)                                  0112
         ST    @14,SCPCHAR                                         0112
*   SCPSRCH=ADDR(K2PLUS);           /* DSCANS FOR IF SEARCH          */
         LA    @14,K2PLUS                                          0113
         ST    @14,SCPSRCH                                         0113
*   SCPIORTN=MPPCRDAD;              /* IO ROUTINE FOR CONTINUATION   */
         MVC   SCPIORTN(4),MPPCRDAD(@10)                           0114
*   SCPIOPAD=MPPCRDPM;              /* PARAMETER LIST FOR ABOVE      */
         ST    @15,SCPIOPAD                                        0115
*   SCPINLN=LENGTH(CRPBFCTL);       /* LENGTN OF INPUT RECORD        */
         MVC   SCPINLN(2),@CH00742                                 0116
*   SCPEOR=''B;                     /* CLEAR FLAGS                   */
         MVI   SCPEOR,X'00'                                        0117
*   SCPCONT=ON;                     /* CONTINUATION ALLOWED          */
*   SCPCOMNT=ON;                    /* SKIP OVER COMMENTS            */
*                                                                  0119
         OI    SCPCONT,B'10100000'                                 0119
*   /*****************************************************************/
*   /*                                                               */
*   /* MESSAGE PARMS                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0120
*   HMASMMGP=''B;                   /* CLEAR HMASMMSG PARM LIST      */
         XC    HMASMMGP(24),HMASMMGP                               0120
*   MGPHLDS=ON;                     /* SEND MESSAGES TO SMPLOG       */
*   MGPPRINT=ON;                    /* SEND MESSAGES TO SMPOUT       */
*                                                                  0122
         OI    MGPHLDS,B'10010000'                                 0122
*   /*****************************************************************/
*   /*                                                               */
*   /* MCB PARMS                                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0123
*   MCBIND(1)=MPIEOLST;             /* FIRST MEMBER IS LAST          */
         L     @10,MPPMCBAD(,@10)                                  0123
         MVI   MCBIND(@10),X'FF'                                   0123
*   RETURN CODE(ZERO);              /*                               */
         SLR   @15,@15                                             0124
         L     @14,12(,@13)                                        0124
         LM    @00,@12,20(@13)                                     0124
         BR    @14                                                 0124
*   END SETUP;                      /*                               */
         EJECT
*                                                                  0126
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL SCN WITH ADDR OF FIRST ++IF DSCAN. UPON RETURN CHECK THE */
*   /* SCAN RETURN CODE FOR SYNTAX ERROR. CHECK CRP RETURN CODE FOR  */
*   /* READ ERRORS WHEN HANDLING STATEMENTS WHICH WERE CONTAINED ON  */
*   /* MORE THAN ONE RECORD.                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0126
*SCNCALL:                                                          0126
*   PROC;                           /*                               */
SCNCALL  STM   @14,@12,@SA00003                                    0126
*   DECLARE                         /*                               */
*     SCNRC FIXED(8);               /*                               */
*   SCNRC=ZERO;                     /*                               */
*                                                                  0128
         MVI   SCNRC,X'00'                                         0128
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP PARM TO HMASMVLU                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0129
*   HMASMVLP=''B;                   /*                               */
         XC    HMASMVLP(52),HMASMVLP                               0129
*   VLPFUNCT=VLPADD;                /*                               */
         MVI   VLPFUNCT,X'01'                                      0130
*   VLPFMSG=OFF;                    /*                               */
         NI    VLPFMSG,B'01111111'                                 0131
*   VLPLPTR=ADDR(MCBIVLST(1));      /*                               */
         L     @10,@PC00001                                        0132
         L     @01,MPPMCBAD(,@10)                                  0132
         LA    @15,MCBIVLST(,@01)                                  0132
         ST    @15,VLPLPTR                                         0132
*   VLPMTYPE=CCATYPEA(IOPSMD);      /*                               */
         L     @15,@CA00042                                        0133
         L     @01,CCATPAD-HMASMCCA(,@15)                          0133
         MVC   VLPMTYPE(8),CCATYPEA+80(@01)                        0133
*   VLPMNAME=MPPSMDNM;              /*                               */
         MVI   VLPMNAME+7,C' '                                     0134
         MVC   VLPMNAME(7),MPPSMDNM(@10)                           0134
*   CALL HMASMSCN(HMASMSCP);        /* SCAN VER HEADER RECORD        */
*                                                                  0135
         L     @15,@CV00781                                        0135
         LA    @01,@AL00135                                        0135
         BALR  @14,@15                                             0135
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK SCN RETRURN FOR SYNTAX ERROR                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0136
*   IF SCPRET=SCPBAD THEN           /* SCN INDICATED AN ERROR        */
         CLI   SCPRET,4                                            0136
         BNE   @RF00136                                            0136
*     DO;                           /*                               */
*       SCNRC=STOPPTF;              /* FLUSH THE SYSMOD              */
         MVI   SCNRC,X'08'                                         0138
*       IF SCPRTERR=OFF             /* SCAN FOUND SYNTAX ERROR       */
*         THEN                      /*                               */
         TM    SCPRTERR,B'00010000'                                0139
         BNZ   @RF00139                                            0139
*         DO;                       /*                               */
*           CALL SYNTAXMG;          /* ISSUE MESSAGE                 */
         BAL   @14,SYNTAXMG                                        0141
*           RFY                     /*                               */
*             RTNCODE RSTD;         /*                               */
*           SCNRC=MAX(SCNRC,RTNCODE);/*                              */
         SLR   @14,@14                                             0143
         IC    @14,SCNRC                                           0143
         CR    @14,RTNCODE                                         0143
         BNL   *+6
         LR    @14,RTNCODE                                         0143
         STC   @14,SCNRC                                           0143
*           RFY                     /*                               */
*             RTNCODE UNRSTD;       /*                               */
*         END;                      /*                               */
*     END;                          /*                               */
@RF00139 DS    0H                                                  0147
*   IF CRPRETRN^=CRPGOOD THEN       /* ERROR IN CONTINUATION READ    */
@RF00136 L     @10,@PC00001                                        0147
         L     @10,MPPCRDPM(,@10)                                  0147
         CLI   CRPRETRN(@10),0                                     0147
         BE    @RF00147                                            0147
*     DO;                           /*                               */
*       IF CRPRETRN=CRPEOF THEN     /* PREMATURE END OF FILE         */
         CLI   CRPRETRN(@10),4                                     0149
         BNE   @RF00149                                            0149
*         DO;                       /*                               */
*           SCNRC=STOPPTF;          /*                               */
         MVI   SCNRC,X'08'                                         0151
*           MGPCLEAR=''B;           /* CLEAR ALL MESSAGE NUMBERS     */
         XC    MGPCLEAR(6),MGPCLEAR                                0152
*           MGPMGNO1=HMA252I;       /* INCOMPLETE CONTROL STATEMENT  */
         MVC   MGPMGNO1(2),@CH00786                                0153
*           MGPTYPE=MGPERR;         /*                               */
         MVI   MGPTYPE,X'03'                                       0154
*           CALL HMASMMSG(HMASMMGP);/*                               */
         L     @15,@CV00780                                        0155
         LA    @01,@AL00155                                        0155
         BALR  @14,@15                                             0155
*         END;                      /*                               */
*       ELSE                        /* IO ERROR                      */
*         SCNRC=CRPRETRN;           /*                               */
         B     @RC00149                                            0157
@RF00149 L     @10,@PC00001                                        0157
         L     @10,MPPCRDPM(,@10)                                  0157
         MVC   SCNRC(1),CRPRETRN(@10)                              0157
*     END;                          /*                               */
@RC00149 DS    0H                                                  0159
*   RETURN CODE(SCNRC);             /*                               */
@RF00147 SLR   @15,@15                                             0159
         IC    @15,SCNRC                                           0159
         L     @14,@SA00003                                        0159
         LM    @00,@12,@SA00003+8                                  0159
         BR    @14                                                 0159
*   END SCNCALL;                    /*                               */
         EJECT
*                                                                  0161
*   /*****************************************************************/
*   /*                                                               */
*   /* SYNTAX ERROR MESSAGE ROUTINE                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0161
*SYNTAXMG:                                                         0161
*   PROC;                                                          0161
SYNTAXMG STM   @14,@12,@SA00004                                    0161
*   DECLARE                                                        0162
*     HEXNO FIXED(8);               /* HEXADECIMAL NUMBER            */
*   DECLARE                                                        0163
*     1 DECNO BDY(DWORD),           /* CONVERT DECIMAL NUMBER        */
*       3 DECNO1 CHAR(8),           /* ACTUAL DECIMAL NUMBER         */
*       3 * CHAR(1);                /* EXTRA FOR UNPACK              */
*   DECLARE                                                        0164
*     1 BCDNO BDY(WORD) CHAR(5),    /* EBCDIC NUMBER                 */
*       3 * CHAR(1),                /* PADDING                       */
*       3 BCDCOL CHAR(2),           /* COLUMN NUMBER                 */
*       3 * CHAR(2);                /* FOR SIGN                      */
*   DECLARE                                                        0165
*     COLSTR FIXED(8) CONSTANT(7);  /* START FOR UNPACK              */
*   DECLARE                                                        0166
*     COLSTP FIXED(8) CONSTANT(9);  /* END FOR UNPACK                */
*   HEXNO=SCPCHAR-ADDR(CRPBUFFR)+1; /* GET COLUMN NUMBER             */
         L     @10,@PC00001                                        0167
         L     @10,MPPCRDPM(,@10)                                  0167
         LA    @10,CRPBUFFR(,@10)                                  0167
         LCR   @10,@10                                             0167
         AL    @10,SCPCHAR                                         0167
         LA    @10,1(,@10)                                         0167
         STC   @10,HEXNO                                           0167
*   CVD(HEXNO,DECNO1);              /* CONVERT NO. TO DECIMAL        */
         CVD   @10,DECNO1                                          0168
*   UNPK(BCDNO,DECNO(COLSTR:COLSTP));/* UNPACK NO.                   */
         UNPK  BCDNO(5),DECNO+6(3)                                 0169
*   MGPCLEAR=''B;                   /* CLEAR ALL MESSAGE NUMBERS     */
         XC    MGPCLEAR(6),MGPCLEAR                                0170
*   MGPMGNO1=HMA203I;               /* SYNTAX ERROR IN               */
         MVC   MGPMGNO1(2),@CH00044                                0171
*   MGPMGNO2=PART021;               /* CONTROL STATMENT              */
         MVC   MGPMGNO2(2),@CH00416                                0172
*   MGPMGNO3=PART026;               /* AT COL                        */
         MVC   MGPMGNO3(2),@CH00426                                0173
*   MGPVARPT(1)=ADDR(BCDCOL);       /* MESSAGE TEXT = COLUMN NUMBER  */
         LA    @10,BCDCOL                                          0174
         ST    @10,MGPVARPT                                        0174
*   MGPTYPE=MGPERR;                 /* MESSAGE SEVERITY              */
         MVI   MGPTYPE,X'03'                                       0175
*   CALL HMASMMSG(HMASMMGP);        /* CALL MESSAGE ROUTINE          */
         L     @15,@CV00780                                        0176
         LA    @01,@AL00176                                        0176
         BALR  @14,@15                                             0176
*   RETURN CODE(ZERO);                                             0177
         SLR   @15,@15                                             0177
         L     @14,@SA00004                                        0177
         LM    @00,@12,@SA00004+8                                  0177
         BR    @14                                                 0177
*   END SYNTAXMG;                                                  0178
         EJECT
*                                                                  0179
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINES FOR DSCAN ROUTS                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0179
*MPIPROCS:                          /*                               */
*   PROC;                           /*                               */
MPIPROCS STM   @14,@12,@SA00005                                    0179
*   RFY                                                            0180
*     RTNREG RSTD;                                                 0180
*   RFY                                                            0181
*     REG1 RSTD;                                                   0181
*   DECLARE                                                        0182
*     SAV1 PTR(31);                 /* FOR SAVING REG 1              */
*   DECLARE                                                        0183
*     SAV14 PTR(31);                /* FOR SAVING REG 14             */
*                                                                  0183
*   /*****************************************************************/
*   /*                                                               */
*   /* PLACE THE FMID VALUE INTO THE MCB                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0184
*RFMIDV:                            /*                               */
*   ENTRY;                          /*                               */
         B     @EC00184                                            0184
RFMIDV   STM   @14,@12,@SA00005                                    0184
@EC00184 DS    0H                                                  0185
*   MCBIFENV=SCPSTRNG(1:7);         /* SAVE CHARACTERS               */
         L     @15,@PC00001                                        0185
         L     @15,MPPMCBAD(,@15)                                  0185
         L     @10,SCPCHAR                                         0185
         MVC   MCBIFENV(7,@15),SCPSTRNG(@10)                       0185
*   GOTO GOODSCAN;                  /* COMMOM EXIT                   */
         B     GOODSCAN                                            0186
*RREQV:                                                            0187
*   ENTRY;                          /*                               */
*                                                                  0187
RREQV    STM   @14,@12,@SA00005                                    0187
*   /*****************************************************************/
*   /*                                                               */
*   /* COMMON ROUTINE TO EXTRACT THE CHARACTERS FROM A SYSMOD NAME   */
*   /* AND SAVE THEM IN MCB                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0188
*   VLPDATA=SCPSTRNG(1:SCPPMLN);    /* MOVE DATA TO VLP DATA AREA    */
         MVI   VLPDATA+1,C' '                                      0188
         MVC   VLPDATA+2(6),VLPDATA+1                              0188
         LH    @15,SCPPMLN                                         0188
         BCTR  @15,0                                               0188
         L     @10,SCPCHAR                                         0188
         EX    @15,@SM00848                                        0188
*   VLPDATA(8)=''B;                 /*                               */
         MVI   VLPDATA+7,X'00'                                     0189
*   VLPTYPE=MCBIFREQ;               /* SET ENTRY TYPE                */
         MVI   VLPTYPE,X'01'                                       0190
*   VLPDLEN=LENGTH(MCBINUM);        /* LEN OF KEY IN VLPDATA         */
         MVI   VLPDLEN,X'07'                                       0191
*   VLPSTYPE=TXTIRQ;                /*                               */
         MVC   VLPSTYPE(16),@CC00777                               0192
*   SAV1=REG1;                      /* SAVE REG 1                    */
         ST    REG1,SAV1                                           0193
*   SAV14=RTNREG;                   /* SAVE REG 14                   */
         ST    RTNREG,SAV14                                        0194
*   RFY                                                            0195
*     REG1 UNRSTD;                  /*                               */
*   RFY                                                            0196
*     RTNREG UNRSTD;                /*                               */
*   CALL HMASMVLU(HMASMVLP);        /*                               */
         L     @15,@CV00783                                        0197
         LA    @01,@AL00197                                        0197
         BALR  @14,@15                                             0197
*   RFY                                                            0198
*     REG1 RSTD;                    /*                               */
*   RFY                                                            0199
*     RTNREG RSTD;                  /*                               */
*   REG1=SAV1;                      /* RESTORE REG 1                 */
         L     REG1,SAV1                                           0200
*   RTNREG=SAV14;                   /* RESTORE REG 14                */
         L     RTNREG,SAV14                                        0201
*   IF VLPRETRN>VLPWARN THEN        /* PEMAX EXCEEDED                */
         CLI   VLPRETRN,4                                          0202
         BNH   @RF00202                                            0202
*     DO;                           /*                               */
*       SCPRTERR=ON;                /* ERROR NOT SYSTAX              */
         OI    SCPRTERR,B'00010000'                                0204
*       SCPRET=SCPSTOP;             /* FLUSH THE SYSMOD              */
         MVI   SCPRET,X'08'                                        0205
*     END;                          /*                               */
*   GOTO GOODSCAN;                  /* COMMO EXIT TO SCN             */
         B     GOODSCAN                                            0207
*                                                                  0208
*   /*****************************************************************/
*   /*                                                               */
*   /* KXTRA ON CARD AFTER PERIOD                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0208
*RXTRA:                             /*                               */
*   ENTRY;                          /*                               */
RXTRA    STM   @14,@12,@SA00005                                    0208
*   SCPRET=SCPSTOP;                 /*                               */
         MVI   SCPRET,X'08'                                        0209
*   GOTO GOODSCAN;                  /*                               */
*                                                                  0210
         B     GOODSCAN                                            0210
*   /*****************************************************************/
*   /*                                                               */
*   /* COMMON EXIT TO SCN                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0211
*GOODSCAN:                                                         0211
*   END MPIPROCS;                   /* RETURN TO SCAN                */
@EL00005 DS    0H                                                  0211
@EF00005 DS    0H                                                  0211
@ER00005 LM    @14,@12,@SA00005                                    0211
         BR    @14                                                 0211
