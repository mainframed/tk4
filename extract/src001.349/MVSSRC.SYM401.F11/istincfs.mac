*        2 BINPRIML FIXED(8),      /* PRIMARY NAU NAME LENGTH        */
*        2 BINPRIM  CHAR(8),       /* PRIMARY NAU NAME               */
*        2 BINUSEL  FIXED(8),      /* USER DATA LENGTH               */
