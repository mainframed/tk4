         BR    R14                RETURN
PARPUT   STM   R14,R12,12(R13)    SAVE REGS
         SR    R8,R8              CLEAR REG
         SR    R11,R11            CLEAR REG
         IC    R8,4(R15)          REG 8-# ENTRIES TO BE PROCESSED
         SR    R10,R10            CLEAR REG 10
         LR    R3,R0              REG 3 POINTS TO PRINTBUF POS. 0
RETURN1  LR    R4,R3
         IC    R10,5(R15)
         AR    R4,R10             REG 4 HAS LOCATION OF PARITY BIT
         IC    R10,7(R15)         REG 10-# OF TIMES TO REPEAT PROCESS
RETURN2  MVC   KEEPER(L1),0(R4)   PARITY BYTE IN KEEPER
         IC    R11,6(R15)         REG 11-# POSITIONS TO BE PROCESSED
         NI    KEEPER,X'0F'
         XI    KEEPER,X'01'       NOT OF PARITY BIT IN KEEPER
         LA    R5,2(R4)           REG 5-POINTER TO BYTE FOR PROCESSING
         SR    R12,R12            CLEAR REG 12
         IC    R12,KEEPER         REG 12-KEEPS PARITY COUNT
RETURN3  MVC   KEEPER(L1),0(R5)   EBCDIC BYTE TO KEEPER
         CLI   KEEPER,X'C6'       SEE IF DIGIT (0-9)
         BH    DIGIT
         SR    R2,R2              CLEAR REG 2
         IC    R2,KEEPER          ASSUME A-F
         SH    R2,ABCDEF          CONVERT TO HEX BY SUBTRACTING HEX B7
         STC   R2,KEEPER          MAKE BELIEVE NOTHING HAPPENED
DIGIT    IC    R7,KEEPER          READY FOR PARITY ROUTINE
         SLL   R7,28              LEFT-JUSTIFY
RETURN4  SR    R6,R6              CLEAR REG 6
         SLDL  R6,1
         XR    R12,R6             ADD PARITY BIT
         NR    R7,R7              SET CONDITION CODE
         BC    7,RETURN4          BACK IF MORE BITS TO BE TESTED
         LA    R5,1(R5)           BUMP TO NEXT PRINT POSITION
         BCT   R11,RETURN3     BACK IF MORE POSITIONS TO BE CHECKED
         NR    R12,R12            SET CONDITION CODE
         BZ    NOSPLAT         SKIP * MAKER IF GOOD PARITY
         MVI   1(R4),C'*'         SPLAT FOR BAD PARITY
NOSPLAT  LA    R4,2(R5)           BUMP PARITY POS. COUNTER
         BCT   R10,RETURN2        BACK IF MORE PARITY CHECKS
         LA    R15,3(R15)         BUMP CONTROL ENTRY POINTER
         BCT   R8,RETURN1         BACK IF MORE COLUMN ENTRIES
         LM    R14,R12,12(R13)    RESTORE REGS
         BR    R14                RETURN
