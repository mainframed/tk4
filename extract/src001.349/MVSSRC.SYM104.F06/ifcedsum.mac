         TITLE 'IFCEDSUM-CONTROL MODULE FOR EDIT AND SUM ROUTINES      *
                        '
IFCEDSUM CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IFCEDSUM  78.100'                                 0001
         DROP  @15
@PROLOG  ST    @14,12(,@13)                                        0001
         STM   @00,@12,20(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  LA    @11,4095(,@12)                                      0001
         LA    @10,4095(,@11)                                      0001
         USING @PSTART,@12                                         0001
         USING @PSTART+4095,@11                                    0001
         USING @PSTART+8190,@10                                    0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         B     @PB00002                                            0187
MERDIAN  STM   @14,@12,@SA00002                                    0187
* IF DEBUG48 = ON & FIRSTHEX = OFF & FRAMEPTR ^= 0 THEN            0192
         TM    DEBUG48(R2),B'10000000'                             0192
         BNO   @RF00192                                            0192
         TM    FIRSTHEX,B'00010000'                                0192
         BNZ   @RF00192                                            0192
         L     @14,FRAMEPTR(,R2)                                   0192
         LTR   @14,@14                                             0192
         BZ    @RF00192                                            0192
*  DO;                                                     /*@G51DGVS*/
*   GEN(MVC  PRTLINE(26),BUG48); /*MOVE DEBUG48 INTO PRT LINE*/    0194
         MVC  PRTLINE(26),BUG48
*   CALL PRINTIT;  /*GO PRINT THE DISCRIPTION OF DUMP        @G51DGVS*/
         BAL   @14,PRINTIT                                         0195
*   SAVE7 = R7;/*SAVE THE ADDRESS OF THE RECORD              @G51DGVS*/
         ST    R7,SAVE7                                            0196
*   SAVERLN = RECLNGTH; /*SAVE THE RECORD LENGTH FOR RETURN  @G51DGVS*/
         LH    @14,RECLNGTH(,R2)                                   0197
         ST    @14,SAVERLN                                         0197
*   R7 =  FRAMEPTR;  /*SET UP THE ADDRESS OF THE FRAME TABLE @G51DGVS*/
         L     R7,FRAMEPTR(,R2)                                    0198
*   RECLNGTH = 384;  /*SET UP THE LENGTH OF THE TABLE        @G51DGVS*/
         MVC   RECLNGTH(2,R2),@CH01253                             0199
*   CALL HEXDUMP;   /*GO HEX DUMP THE FRAME TABLE            @G51DGVS*/
         BAL   @14,HEXDUMP                                         0200
*   FIRSTHEX = ON;  /*SET THE SWITCH SO WE ONLY DO IT ONCE   @G51DGVS*/
         OI    FIRSTHEX,B'00010000'                                0201
*   R7 = SAVE7;     /*RESTORE THE REG TO THE ORGINAL VALUE   @G51DGVS*/
         L     R7,SAVE7                                            0202
*   RECLNGTH = SAVERLN;  /*RESTORE THE RECORD LENGTH         @G51DGVS*/
         L     @14,SAVERLN                                         0203
         STH   @14,RECLNGTH(,R2)                                   0203
*  END;                                                    /*@G51DGVS*/
* IF CORE ^= YES THEN            /*HAVE WE DONE THE GETMAIN FOR FRMS*/
@RF00192 TM    CORE,B'01000000'                                    0205
         BO    @RF00205                                            0205
*  DO;                                                     /*@G51DGVS*/
*   RESPECIFY (R0,R15) RESTRICTED;                         /*@G51DGVS*/
*   R0 = 2000;   /*THIS IS MIN FOR SUBPOOL  GETMAIN ANWAY    @G51DGVS*/
         LA    R0,2000                                             0208
*   GEN(GETMAIN EC,LV=(0),A=NEWREC,SP=20);                 /*@G51DGVS*/
         GETMAIN EC,LV=(0),A=NEWREC,SP=20
*   IF R15 ^= ZERO THEN          /*CONTINUE IF WE GOT THE CORE*/   0210
         LTR   R15,R15                                             0210
         BZ    @RF00210                                            0210
*    DO;                                                   /*@G51DGVS*/
*     ERROR = R15;               /*THIS WILL STOP THINGS THIS JOB STEP*
         STH   R15,ERROR                                           0212
*     CALL IFCMSG(40,ADDR(PRTLINE));/*GO GET THE NEEDED MESSAGE*/  0213
         LA    @14,PRTLINE                                         0213
         ST    @14,@AFTEMPS                                        0213
         L     @15,ADIFCMSG(,R2)                                   0213
         LA    @01,@AL00213                                        0213
         BALR  @14,@15                                             0213
*     CALL PRINTIT;              /*TELL ABOUT THE LACK OF CORE*/   0214
         BAL   @14,PRINTIT                                         0214
*    END;                                                  /*@G51DGVS*/
*   ELSE                                                           0216
*     CORE = YES;                /*SET HAVE CORE FLAG        @G51DGVS*/
         B     @RC00210                                            0216
@RF00210 OI    CORE,B'01000000'                                    0216
*  END;                                                    /*@G51DGVS*/
@RC00210 DS    0H                                                  0218
*  IF  ERROR = ZERO THEN         /*DONT DO IF FAILED ABOVE   @G51DGVS*/
@RF00205 LH    @14,ERROR                                           0218
         LTR   @14,@14                                             0218
         BNZ   @RF00218                                            0218
*   DO;                                                    /*@G51DGVS*/
*    RESPECIFY (R0,R8,R9) RESTRICTED;                      /*@G51DGVS*/
*    R1 = RECLNGTH;              /*SET  UP TO MOVE THE RECORD TO*/ 0221
         LH    R1,RECLNGTH(,R2)                                    0221
*    R9 = R1;                    /*THE NEW AREA SO WE CAN READ IN THE*/
         LR    R9,R1                                               0222
*    R0 = NEWREC;                /*FRAME RECORD IF ONE EXIST @G51DGVS*/
         L     R0,NEWREC                                           0223
*    RECMOVE = RECMOVE + ONE;    /*# OF RECORDS THAT WERE MOVED*/  0224
         LA    @14,1                                               0224
         AL    @14,RECMOVE                                         0224
         ST    @14,RECMOVE                                         0224
*    R8 = R7;                    /*R8 = WHERE REC IS NOW     @G51DGVS*/
         LR    R8,R7                                               0225
*    GEN(MVCL  R0,R8);           /*MOVE THE RECORD TO NEW AREA*/   0226
         MVCL  R0,R8
*    RESPECIFY (R8,R9) UNRESTRICTED;                       /*@G51DGVS*/
*    RECLNGT1 = RECLNGTH;        /*IFCFRAME WILL NEED THIS VALUE*/ 0228
         LH    @14,RECLNGTH(,R2)                                   0228
         ST    @14,RECLNGT1(,R2)                                   0228
*    R7 = NEWREC;                /*SET POINTER TO DATA RECORD*/    0229
         L     R7,NEWREC                                           0229
*    IF FRAMEIN = NO THEN        /*HAVE WE LOADED IFCFRAME YET*/   0230
         TM    FRAMEIN,B'00100000'                                 0230
         BNZ   @RF00230                                            0230
*     DO;                                                  /*@G51DGVS*/
*    GEN(LOADEREP EPLOC=FRMNAME);    /* LOAD FRAME ROUTINE   @G51DGVS*/
         LOADEREP EPLOC=FRMNAME
*      FRAMEIN = YES;            /*SET FLAG SO AS TO NOT RETURN*/  0233
         OI    FRAMEIN,B'00100000'                                 0233
*      FRAMEADD = R0;            /*SAVE THE ADDRESS OF WHERE OS PUT IT*
         ST    R0,FRAMEADD                                         0234
*     END;                                                 /*@G51DGVS*/
*    CALL FRAME;                 /*GO FIND THE FRAME IF THERE IS ANY*/
@RF00230 BAL   @14,FRAME                                           0236
*   END;                                                   /*@G51DGVS*/
*RESPECIFY (R0,R1,R15)  UNRESTRICTED;                      /*@G51DGVS*/
*END;                                                      /*@G51DGVS*/
@EL00002 DS    0H                                                  0239
@EF00002 DS    0H                                                  0239
@ER00002 LM    @14,@12,@SA00002                                    0239
         BR    @14                                                 0239
*/*******************************************************************/
*/*    THIS PROC WILL FIND THE SET OF FRAMES NEEDED TO PROCESS THIS */
*/*    DATA RECORD.  IF THE MODEL-SERIAL # CHECK DOES NOT MATCH THEN*/
*/*    AN ATTEMPT IS MADE TO FIND A MATCH ON JUST MODEL IF NO MATCH */
*/*    THEN THERE ARE NOT ANY FRAMES TO EDIT THIS REC WITH, IFCFRAME*/
*/*    WILL ONLY FORMAT THE HEADER AND THEN HEX DUMP THE RECORD     */
*/*    DEBUG COUNTS OF THE NUMBER OF FRAMES READ IN ARE KEPT @G51DGVS*/
*/*        VALUES PASSED TO IFCFRAME:                        @G51DGVS*/
*/*           A.  R5 = ADDRESS OF THE FRAME RECORD           @G51DGVS*/
*/*               (0 TELLS IFCFRAME THAT FUTHER FRAMES ARE NOT AVAIBLE*
*/*           B.  R7 = ADDRESS OF THE DATA RECORD            @G51DGVS*/
*/*           C.  R2 = ADDRESS OF THE COMMUNATION TABLE      @G51DGVS*/
*/*           D.  RECLNGT1 = LENGTH OF THE DATA RECORD       @G51DGVS*/
*/*    IT IS LEFT UP TO IFCFRAME TO SUPPORT DEBUG4 FOR ITS SELF AND */
*/*    ANY MODULES LINKED CALLED BY IFCFRAME                 @G51DGVS*/
*/*                                                          @G51DGVS*/
*FRAME: PROC;                                              /*@G51DGVS*/
FRAME    STM   @14,@12,@SA00003                                    0240
*DCL INDEX1 FIXED(31) INIT(0);                             /*@G51DGVS*/
*  RESPECIFY (R15) RESTRICTED;                             /*@G51DGVS*/
* FOUND = NO;                   /*SET FLAG TO NO FRAMES      @G51DGVS*/
         NI    FOUND,B'11110111'                                   0243
* DO A = 1 BY 1 TO FRAMENUM  WHILE(FOUND = NO);            /*@G51DGVS*/
         LA    @14,1                                               0244
         ST    @14,A                                               0244
         B     @DE00244                                            0244
@DL00244 TM    FOUND,B'00001000'                                   0244
         BNZ   @DC00244                                            0244
*  IF LRTCPMOD = FRAMECPU(A)  &    /*DOES THE SERIAL/MODEL MATCH*/ 0245
*     FRAMETYP(A) = 'FF'X THEN   /*ALSO ALL THE FRAMES FOUND BY FILT1*/
         LR    @06,@14                                             0245
         MH    @06,@CH00043                                        0245
         L     @03,FRAMEPTR(,R2)                                   0245
         ST    @06,@TF00001                                        0245
         ALR   @06,@03                                             0245
         AL    @06,@CF01392                                        0245
         CLC   LRTCPMOD(5,R7),FRAMECPU-4(@06)                      0245
         BNE   @RF00245                                            0245
         AL    @03,@TF00001                                        0245
         AL    @03,@CF01393                                        0245
         CLI   FRAMETYP-17(@03),X'FF'                              0245
         BNE   @RF00245                                            0245
*   DO;                                                    /*@G51DGVS*/
*   INDEX1 = A;                /*SAVE FOR INDEXING LATTER WHILE CORREC*
         ST    @14,INDEX1                                          0247
*   FOUND = YES;                  /*FOUND MATCH SO SET TO EXIT*/   0248
         OI    FOUND,B'00001000'                                   0248
*   END;                                                   /*@G51DGVS*/
* END;                                                     /*@G51DGVS*/
@RF00245 LA    @14,1                                               0250
         AL    @14,A                                               0250
         ST    @14,A                                               0250
@DE00244 L     @06,FRAMEPTR(,R2)                                   0250
         C     @14,FRAMENUM(,@06)                                  0250
         BNH   @DL00244                                            0250
@DC00244 DS    0H                                                  0251
* IF FOUND = NO THEN              /*LETS SEE IF ONLY THE MODEL MATCH*/
         TM    FOUND,B'00001000'                                   0251
         BNZ   @RF00251                                            0251
*  DO A = 1 BY 1 TO FRAMENUM WHILE(FOUND = NO);            /*@G51DGVS*/
         LA    @14,1                                               0252
         ST    @14,A                                               0252
         B     @DE00252                                            0252
@DL00252 TM    FOUND,B'00001000'                                   0252
         BNZ   @DC00252                                            0252
*   IF LRTMODEL = FRAMEMOD(A) &    /*DOES THE MODEL MATCH    @G51DGVS*/
*     FRAMETYP(A) = 'FF'X THEN  /*FILT1 FOUND A COMPLETE SET OF FRM*/
         LR    @06,@14                                             0253
         MH    @06,@CH00043                                        0253
         L     @03,FRAMEPTR(,R2)                                   0253
         ST    @06,@TF00001                                        0253
         ALR   @06,@03                                             0253
         AL    @06,@CF01395                                        0253
         CLC   LRTMODEL(2,R7),FRAMEMOD-7(@06)                      0253
         BNE   @RF00253                                            0253
         AL    @03,@TF00001                                        0253
         AL    @03,@CF01393                                        0253
         CLI   FRAMETYP-17(@03),X'FF'                              0253
         BNE   @RF00253                                            0253
*    DO;                                                   /*@G51DGVS*/
*    SPECILCT = SPECILCT + 1;     /*KEEP TRACK OF THE NUMBER OF THESE*/
         LA    @06,1                                               0255
         AL    @06,SPECILCT                                        0255
         ST    @06,SPECILCT                                        0255
*    FOUND = YES;                 /*SET FLAG TO FOUND MATCH  @G51DGVS*/
         OI    FOUND,B'00001000'                                   0256
*    INDEX1 = A;                  /*SAVE FOR INDEXING LATER  @G51DGVS*/
         ST    @14,INDEX1                                          0257
*    END;                                                  /*@G51DGVS*/
*  END;                                                    /*@G51DGVS*/
@RF00253 LA    @14,1                                               0259
         AL    @14,A                                               0259
         ST    @14,A                                               0259
@DE00252 L     @06,FRAMEPTR(,R2)                                   0259
         C     @14,FRAMENUM(,@06)                                  0259
         BNH   @DL00252                                            0259
@DC00252 DS    0H                                                  0260
* IF FOUND = YES THEN             /*HAVE TO SET UP THE ADDRESSABILITY*/
@RF00251 TM    FOUND,B'00001000'                                   0260
         BNO   @RF00260                                            0260
*  DO;                            /*TO THE CCHHRS OF THE FRAME RECORDS*
*   IF MERDTYP = '1F'X THEN       /*COMPARE 1ST BYTE IN THE SORT ENTRY*
         CLI   MERDTYP(R4),X'1F'                                   0262
         BNE   @RF00262                                            0262
*     LISTPTR = FRAMLOCM(INDEX1); /*IS AN MCF TYPE SET BASED ADDRESS*/
         L     @14,INDEX1                                          0263
         MH    @14,@CH00043                                        0263
         L     @06,FRAMEPTR(,R2)                                   0263
         AL    @06,@CF01396                                        0263
         L     @14,FRAMLOCM-20(@14,@06)                            0263
         ST    @14,LISTPTR                                         0263
*    ELSE                                                          0264
*     LISTPTR = FRAMLOCC(INDEX1); /*SET TO AN CCF TYPE       @G51DGVS*/
         B     @RC00262                                            0264
@RF00262 L     @14,INDEX1                                          0264
         MH    @14,@CH00043                                        0264
         L     @06,FRAMEPTR(,R2)                                   0264
         L     @14,FRAMLOCC-24(@14,@06)                            0264
         ST    @14,LISTPTR                                         0264
*  END;                                                    /*@G51DGVS*/
@RC00262 DS    0H                                                  0266
*IF FOUND = YES THEN              /*LET IFCFRAME HANDLE IT NOW*/   0266
@RF00260 TM    FOUND,B'00001000'                                   0266
         BNO   @RF00266                                            0266
* DO A = 1 BY 1 TO LISTNUM;       /*= # OF FRAMES IN THIS SET*/    0267
         LA    @14,1                                               0267
         ST    @14,A                                               0267
         B     @DE00267                                            0267
@DL00267 DS    0H                                                  0268
*  RECCCHHR = LISTCCHR(A);        /*MOVE THE ADDRESS OF THE NEXT FRAME*
         MH    @14,@CH00047                                        0268
         L     @06,LISTPTR                                         0268
         ALR   @06,@14                                             0268
         BCTR  @06,0                                               0268
         MVC   RECCCHHR(5,R2),LISTCCHR-4(@06)                      0268
*  CALL IOHND(READIRCT);          /*ASK I/O TO READ THE FRAME RECORD*/
         L     @15,ADIOHND(,R2)                                    0269
         LA    @01,@AL00269                                        0269
         BALR  @14,@15                                             0269
*  IF R15 = ZERO THEN             /*DID WE GET A GOOD READ DIRECT*/
         LTR   R15,R15                                             0270
         BNZ   @RF00270                                            0270
*   DO;                           /*THEN CONTINUE PROCESSING @G51DGVS*/
*    IF LRTKEY1 = 'A0'X THEN      /*IS IT AN MCH FRAME             D*/
         CLI   LRTKEY1(R7),X'A0'                                   0272
         BNE   @RF00272                                            0272
*     MCHFRAME = MCHFRAME + ONE;  /*COUNT THE NUMBER OF THESE READ*/
         LA    @14,1                                               0273
         AL    @14,MCHFRAME                                        0273
         ST    @14,MCHFRAME                                        0273
*    ELSE                                                          0274
*     CCHFRAME = CCHFRAME + ONE;  /*THEN MUST BE AN CCH FRAME        */
         B     @RC00272                                            0274
@RF00272 LA    @14,1                                               0274
         AL    @14,CCHFRAME                                        0274
         ST    @14,CCHFRAME                                        0274
*    R5 = R7;                     /*IFCFRAME EXPECTS FRAME ADDRESS IN*/
@RC00272 LR    R5,R7                                               0275
*    R7 = NEWREC;                 /*LOAD 7 WITH ADDR OF THE DATA REC */
         L     R7,NEWREC                                           0276
*    R15 = FRAMEADD;              /*LOAD THE ADDRESS OF IFCFRAME*/ 0277
         L     R15,FRAMEADD                                        0277
*    GEN(BALR   R14,R15);                                  /*@VS48732*/
         BALR   R14,R15
*   END;                                                   /*@G51DGVS*/
*  ELSE                                                            0280
*   DO;                                                    /*@G51DGVS*/
         B     @RC00270                                            0280
@RF00270 DS    0H                                                  0281
*   BADFRAME = BADFRAME + ONE;    /*KEEP TRACK OF BAD DIRECT READS*/
         LA    @14,1                                               0281
         AL    @14,BADFRAME                                        0281
         ST    @14,BADFRAME                                        0281
*  IF A = LISTNUM & LRTNLAST ^= OFF  THEN                          0282
         L     @14,LISTPTR                                         0282
         CLC   A(4),LISTNUM(@14)                                   0282
         BNE   @RF00282                                            0282
         TM    LRTNLAST(R7),B'10000000'                            0282
         BZ    @RF00282                                            0282
*   DO;                                                    /*@G51DGVS*/
*    R5 = ZERO;                   /*TELL DONE THIS FRAME SET AND */
         SLR   R5,R5                                               0284
*                                 /*THERE WAS NO END FRAME   @G51DGVS*/
*    R15 = FRAMEADD;              /*LOAD THE ADDRESS OF IFCFRAME*/ 0285
         L     R15,FRAMEADD                                        0285
*    GEN(BALR   R14,R15);                                  /*@VS48732*/
         BALR   R14,R15
*   END;                                                   /*@G51DGVS*/
*  END;                                                    /*@G51DGVS*/
@RF00282 DS    0H                                                  0289
* END;                                                     /*@G51DGVS*/
@RC00270 LA    @14,1                                               0289
         AL    @14,A                                               0289
         ST    @14,A                                               0289
@DE00267 L     @06,LISTPTR                                         0289
         C     @14,LISTNUM(,@06)                                   0289
         BNH   @DL00267                                            0289
* ELSE                                                             0290
* DO;                                                      /*@G51DGVS*/
         B     @RC00266                                            0290
@RF00266 DS    0H                                                  0291
*    R5 = ZERO;                   /*IFCFRAME NOW KNOWS NOTHING FOUND */
         SLR   R5,R5                                               0291
*    R7 = NEWREC;                 /*LOAD 7 WITH ADDR OF THE DATA REC */
         L     R7,NEWREC                                           0292
*    R15 = FRAMEADD;              /*LOAD THE ADDRESS OF IFCFRAME*/ 0293
         L     R15,FRAMEADD                                        0293
*    GEN(BALR  R14,R15);          /*TURN CONTROL OVER TO IFCFRAME*/
         BALR  R14,R15
* END;                                                     /*@G51DGVS*/
* RESPECIFY (R15) UNRESTRICTED;                            /*@G51DGVS*/
*END;                                                      /*@G51DGVS*/
@EL00003 DS    0H                                                  0297
@EF00003 DS    0H                                                  0297
@ER00003 LM    @14,@12,@SA00003                                    0297
         BR    @14                                                 0297
*/********************************************************************/
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * **/
*/*    THIS PROC WILL PRINT ALL DEBUG DATA                   @Z427VLS*/
*/*     A. COUNT OF EACH TYPE OF RECORD                      @Z427VLS*/
*/*     B. COUNT OF THE VARIOUS TYPES OF UNKNOWN RECORDS     @Z427VLS*/
*/*     C. COUNT OF THE NUMBER OF TIMES MODULES ARE CALLED AND NOT   */
*/*        FOUND                                             @Z427VLS*/
*/*     D. COUNT OF THE NUMBER OF TIMES EACH PROC IS CALLED  @Z427VLS*/
*/*                                                          @Z427VLS*/
*PRTDEBUG: PROC;                                           /*@Z427VLS*/
PRTDEBUG STM   @14,@12,@SA00004                                    0298
* SPACING = TRIPLE;                     /*SET SPACING FOR THREE SPACE*/
         MVC   SPACING(1),TRIPLE                                   0299
* MISINFO1 = MISJUNK;                   /*DEBUG0 HEADING     @Z427VLS*/
         MVC   MISINFO1(40),MISJUNK                                0300
* CALL PRINTIT;                                            /*@Z427VLS*/
         BAL   @14,PRINTIT                                         0301
* SPACING = SINGLE;                                        /*@Z427VLS*/
         MVC   SPACING(1),SINGLE                                   0302
* IF NOEDIT ^= ZERO THEN                                           0303
         L     @06,NOEDIT                                          0303
         LTR   @06,@06                                             0303
         BZ    @RF00303                                            0303
*   DO;                                                    /*@Z427VLS*/
*    BINWORD = ENUMFAIL;                /*SET NUMBER TO CONVERT      */
         LH    @06,ENUMFAIL                                        0305
         ST    @06,BINWORD                                         0305
*    BNDECNUM = FIVE;                   /*SET NUMBER OF CHARS TO CONV*/
         MVC   BNDECNUM(4),@CF00047                                0306
*    DECADDR = ADDR(COUNT1);            /*SET ADDRESS TO PUT NUMBER  */
         LA    @06,COUNT1                                          0307
         ST    @06,DECADDR                                         0307
*    CALL CONVBIN;                      /*GO CONVERT THE # TO PRINTAB*/
         BAL   @14,CONVBIN                                         0308
*    PRTL1  =  BLDFAIL;                 /*MOVE THE LINE INTO PRINT LN*/
         MVI   PRTL1+53,C' '                                       0309
         MVC   PRTL1+54(13),PRTL1+53                               0309
         MVC   PRTL1(53),BLDFAIL                                   0309
*    CALL PRINTIT;                      /*GO PRINT THE LINE  @Z427VLS*/
         BAL   @14,PRINTIT                                         0310
*    TYPE1 = BLANK;                                        /*@Z427VLS*/
         MVC   TYPE1(7),BLANK                                      0311
*    DO I = 9 BY -1 TO 1;               /*SET CONTROLS TO PRINT #'S  */
         LA    @06,9                                               0312
         ST    @06,I                                               0312
@DL00312 DS    0H                                                  0313
*      IF EDITFAIL(I) ^= ZERO THEN                                 0313
         LR    @03,@06                                             0313
         SLA   @03,2                                               0313
         L     @03,EDITFAIL-4(@03)                                 0313
         LTR   @03,@03                                             0313
         BZ    @RF00313                                            0313
*        DO;                                               /*@Z427VLS*/
*          TYPE11 = RECNAMES(I);        /*MOVE IN THE REC TYPE       */
         MH    @06,@CH00045                                        0315
         LA    @01,RECNAMES-3(@06)                                 0315
         MVC   TYPE11(3),0(@01)                                    0315
*          BINWORD = EDITFAIL(I);       /*NUMBER TO CONVERT TO DEC   */
         ST    @03,BINWORD                                         0316
*          CALL CONVBIN;                /*GO CONVERT THE BIN TO DEC  */
         BAL   @14,CONVBIN                                         0317
*         PRTL1 = BLDFAIL;  /*MOVE REC TYPE + COUNT TO PRT LINE*/  0318
         MVI   PRTL1+53,C' '                                       0318
         MVC   PRTL1+54(13),PRTL1+53                               0318
         MVC   PRTL1(53),BLDFAIL                                   0318
*          CALL PRINTIT;                /*GO PRINT THE LINE  @Z427VLS*/
         BAL   @14,PRINTIT                                         0319
*        END;                                              /*@Z427VLS*/
*    END;                                                  /*@Z427VLS*/
@RF00313 SLR   @06,@06                                             0321
         BCTR  @06,0                                               0321
         AL    @06,I                                               0321
         ST    @06,I                                               0321
         LTR   @06,@06                                             0321
         BP    @DL00312                                            0321
*    IF ENUMFAIL ^= ZERO THEN           /* ANY NAMES IN BUFFER       */
         LH    @06,ENUMFAIL                                        0322
         LTR   @06,@06                                             0322
         BZ    @RF00322                                            0322
*      DO;                                                 /*@Z427VLS*/
*        PRTL2 =  EXPL1;                /*MOVE MESSAGE INTO THE BUFF */
         MVC   PRTL2(42),EXPL1                                     0324
*        CALL PRINTIT;                  /*GO AND DO IT(PRINT)        */
         BAL   @14,PRINTIT                                         0325
*        DO X = ENUMFAIL BY -1 TO 1;                       /*@Z427VLS*/
         LH    @06,ENUMFAIL                                        0326
         ST    @06,X                                               0326
         B     @DE00326                                            0326
@DL00326 DS    0H                                                  0327
*          EDVAR = IDXEDIT(X);          /*SET NAME TO FAILED TYPE    */
         LR    @03,@06                                             0327
         SLA   @03,2                                               0327
