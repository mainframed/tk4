         L     @10,SAVER2                                          0085
         LR    R2,@10                                              0085
*  MOVETOLN = MOVELINE;           /* OUTPUT LINE             @Z427VLS*/
         MVC   MOVETOLN(133),MOVELINE                              0086
*  PRINTADR = ADDR(DUMBUFF);                               /*@Z427VLS*/
         LA    @15,DUMBUFF                                         0087
         ST    @15,PRINTADR(,@10)                                  0087
*  CALL IOHND(RITEPRTR);                                   /*@Z427VLS*/
         L     @15,ADIOHND(,@10)                                   0088
         LA    @01,@AL00088                                        0088
         BALR  @14,@15                                             0088
*  PRBUFF = DUMBUFF(3:120);                                /*@Z427VLS*/
         MVI   PRBUFF+118,C' '                                     0089
         MVC   PRBUFF+119(14),PRBUFF+118                           0089
         MVC   PRBUFF(118),DUMBUFF+2                               0089
*  R2 = SWAPR2;                                            /*@Z427VLS*/
         L     R2,SWAPR2                                           0090
* END;                                                     /*@Z427VLS*/
@EL00002 DS    0H                                                  0091
@EF00002 DS    0H                                                  0091
@ER00002 LM    @14,@12,@SA00002                                    0091
         BR    @14                                                 0091
@PB00002 DS    0H                                                  0092
*/********************************************************************/
*/*                                                          @Z427VLS*/
*/*      SET UP PRIOR TO PROCESSING                          @Z427VLS*/
*/*                                                          @Z427VLS*/
*/********************************************************************/
*        SAVER2 = R2;                                      /*@Z427VLS*/
         ST    R2,SAVER2                                           0092
