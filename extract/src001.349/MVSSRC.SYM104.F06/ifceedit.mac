         MVC   L1(L1,R1),L0(R7)
*EDHLOOP2:    ;                         /*                   @Z427VLS*/
EDHLOOP2 DS    0H                                                  0323
*        R4=R4+1 ;                      /* BUMP PTR TO NEXT INPUT CHR*/
         AL    R4,@CF00051                                         0323
*        R1=R1+R5+2;                    /* BUMP OUT BUFF PTR BY 2+   */
         LR    @10,R1                                              0324
         ALR   @10,R5                                              0324
         AL    @10,@CF00071                                        0324
         LR    R1,@10                                              0324
*        R6=R6-1;                       /* SPACING VALUE DECREMENT LP*/
         BCTR  R6,0                                                0325
