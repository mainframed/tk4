*/********************************************************************/
*        RESPECIFY (R1,R5,R6,R7) RESTRICTED;               /*@Z427VLS*/
*EDHEX  :     ;                                            /*@Z427VLS*/
EDHEX    DS    0H                                                  0283
*        EDSPSV8=R8;                    /* SAVE RTN POINT    @Z427VLS*/
         ST    R8,EDSPSV8                                          0283
*        R1=R3;                         /* SET PTR FOR WORK  @Z427VLS*/
         LR    R1,R3                                               0284
