         SRL   R7,L20
*        R5=R6 ;                        /* SET WORK REG      @Z427VLS*/
         LR    R5,R6                                               0260
*        R6=R9 ;                        /* SAVE CURRENT ENTRY PTR    */
         LR    R6,R9                                               0261
