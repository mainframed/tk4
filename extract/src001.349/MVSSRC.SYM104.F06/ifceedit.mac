*EDHLOOP1:    ;                         /*                   @Z427VLS*/
EDHLOOP1 DS    0H                                                  0315
*        GEN(TM    L3(R9),HEX02);       /* IS LEFT HALF ONLY ASKED   */
         TM    L3(R9),HEX02
*        GEN(BZ    EDHLOOP2);           /* FOR IF SW OFF-BR-YES      */
         BZ    EDHLOOP2
