         BE    INSERT                   YES - GO PUT IN LABEL
         BR    R14                      RETURN
TSTMPBIT TM    XSWS8,MPBIT              Q/ IS THIS AN MP SYSTEM
         BO    INSERT                   YES - GO PUT IN LABEL
         BR    R14                      RETURN
INSERT   MVC   L0(L2,R3),MPCON          MOVE IN LABEL FOR MP FEATURE
         LA    R3,2(R3)                                        @XA20541
         BR    R14                      RETURN
