*           /*********************************************************/
*           /*                                                       */
*           /* THE EXECUTING CPU MAY HAVE CHANGED DURING RISGNL DUE  */
*           /* TO A MACHINE CHECK AND A SWITCH OF CPUS BY ACR. IF THE*/
*           /* EXECUTING CPU IS NOT THE SIGNALLED CPU, THEN THE      */
*           /* SIGNALLED CPU IS SPINNING. INDICATE THIS FACT         */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0081
*           IF PSACPUSA^=IOCPUID THEN                              0081
         CLC   PSACPUSA(2),IOCPUID                                 0081
         BE    @RF00081                                            0081
*             DO;                                                  0082
*               IOCPUSGL=IOCPUSGL×IOCPUMSK;                        0083
*                                                                  0083
         SR    @12,@12                                             0083
         ICM   @12,3,IOCPUMSK                                      0083
         SR    @15,@15                                             0083
         ICM   @15,3,IOCPUSGL                                      0083
         OR    @12,@15                                             0083
         STH   @12,IOCPUSGL                                        0083
*               /*****************************************************/
*               /*                                                   */
*               /* INCREMENT COUNT OF CPUS SPINNING. USE COMPARE AND */
*               /* SWAP LOGIC MACDATE Y-2 73018                      */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0084
*               RESPECIFY                                          0084
*                (GPR14P,                                          0084
*                 GPR15P)RESTRICTED;                               0084
*IOLAB1:                                                           0085
*               GPR14P=IOCOMCPU;    /* GET CURRENT VALUE             */
*                                                                  0085
IOLAB1   L     @12,IOCOMPTR                                        0085
         L     GPR14P,IOCOMCPU(,@12)                               0085
*               /*****************************************************/
*               /*                                                   */
*               /* ADD ONE                                           */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0086
*               GPR15P=GPR14P+IOONE;                               0086
*                                                                  0086
         LR    GPR15P,GPR14P                                       0086
         AL    GPR15P,IOONE                                        0086
*               /*****************************************************/
*               /*                                                   */
*               /* COMPARE AND SWAP                                  */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0087
*               CS(GPR14P,GPR15P,IOCOMCPU);                        0087
         L     @12,IOCOMPTR                                        0087
         CS    GPR14P,GPR15P,IOCOMCPU(@12)                         0087
*               BC(7,IOLAB1);       /* LOOP IF UNSUCCESSFUL          */
*                                                                  0088
         BC    7,IOLAB1                                            0088
