         RISGNL PARALLEL,CPU=(1),EP=MFAISGNL,PARM=(11)
*           IF GPR15P^=0 THEN       /* CONTINUE IF SIGNAL DID NOT  0078
*                                      TAKE                          */
         LTR   GPR15P,GPR15P                                       0078
         BNZ   @RT00078                                            0078
*             GO TO IOLAB2;         /* SET UP TO SIGNAL NEXT CPU     */
