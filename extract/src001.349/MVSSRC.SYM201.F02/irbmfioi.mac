         B     @RC00096                                            0098
@RF00096 L     @12,CVTPTR                                          0098
         L     @12,CVTIXAVL(,@12)                                  0098
         L     @01,IOCMFHK(,@12)                                   0098
         L     @12,IOCMFCNT(,@12)                                  0098
         MVC   IOINSTRC(4,@01),IOINSTRC+4(@12)                     0098
*     IOFINISH='1'B;                /* INDICATE SUCCESSFUL COMPLETION*/
@RC00096 OI    IOFINISH,B'10000000'                                0099
*IOLAB3:                                                           0100
*                                                                  0100
*     /***************************************************************/
*     /*                                                             */
