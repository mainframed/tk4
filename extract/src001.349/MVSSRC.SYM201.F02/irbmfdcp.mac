*   /*****************************************************************/
*   /*                                                               */
*   /* CAUSE OTHER CPU'S TO EXIT WAIT STATE SO THAT WAIT TIME CAN BE */
*   /* ACCOUNTED FOR BY IEAQWAIT BEFORE IT IS GATHERED BY IRBMFDCP   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0057
*   DCCPUNUM=1;                     /* SET LOW ORDER CPU MASK, CPU #N*/
         STH   @08,DCCPUNUM                                        0057
*   DO I=DCMAXCPU BY-1 TO 1;        /* FOR ALL CPU'S                 */
         LA    I,16                                                0058
@DL00058 DS    0H                                                  0059
*     IF(DCCPUBIT&CSDCPUAL)^=0      /* IF CPU IS ALIVE, SIGNAL IT    */
*       THEN                                                       0059
         L     @12,CVTPTR                                          0059
         L     @11,CVTCSD(,@12)                                    0059
         LH    @11,CSDCPUAL(,@11)                                  0059
         N     @11,@CF01381                                        0059
         LH    @08,DCCPUBIT                                        0059
         N     @08,@CF01381                                        0059
         NR    @11,@08                                             0059
         LTR   @11,@11                                             0059
         BZ    @RF00059                                            0059
*       DO;                         /* DO REMOTE IMMEDIATE INTER-  0060
*                                      PROCESSOR COMMUNICATION       */
*         RFY                                                      0061
*          (GPR11P,                                                0061
*           GPR12P) RSTD;                                          0061
*                                                                  0061
*         /***********************************************************/
*         /*                                                         */
*         /* MACDATE Y-2 73018                                       */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0062
*         RESPECIFY                                                0062
*          (GPR00P,                                                0062
*           GPR01P,                                                0062
*           GPR14P,                                                0062
*           GPR15P) RESTRICTED;                                    0062
*         GPR01P=PCCAT00P(I);       /* PCCA ADDRESS OF CPU           */
         LR    @08,I                                               0063
         SLA   @08,2                                               0063
         L     @07,CVTPTR                                          0063
         L     @06,CVTPCCAT(,@07)                                  0063
         AL    @06,@CF01380                                        0063
         L     GPR01P,PCCAT00P(@08,@06)                            0063
*         GPR12P=ADDR(CVTBRET);     /* ROUTINE EXECUTED IS BCR 15,14 */
         LA    GPR12P,CVTBRET(,@07)                                0064
*         GEN(RISGNL SERIAL,CPU=(1),EP=(12));/* SIGNAL CPU           */
*                                                                  0065
         RISGNL SERIAL,CPU=(1),EP=(12)
*         /***********************************************************/
*         /*                                                         */
*         /* MACDATE Y-2 73018                                       */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0066
*         RESPECIFY                                                0066
*          (GPR00P,                                                0066
*           GPR01P,                                                0066
*           GPR14P,                                                0066
*           GPR15P) UNRESTRICTED;                                  0066
*         RFY                                                      0067
*          (GPR11P,                                                0067
*           GPR12P) UNRSTD;                                        0067
*       END;                        /* END IF                        */
*     DCCPUNUM=DCCPUNUM*2;          /* SHIFT CPU BIT MASK TO LEFT    */
@RF00059 LH    @12,DCCPUNUM                                        0069
         N     @12,@CF01381                                        0069
         ALR   @12,@12                                             0069
         STH   @12,DCCPUNUM                                        0069
*   END;                            /* END I LOOP                    */
*                                                                  0070
         BCTR  I,0                                                 0070
         LTR   I,I                                                 0070
         BP    @DL00058                                            0070
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO CALLER BY SPECIAL RETURN SEQUENCE WHICH DOES NOT    */
*   /* FREE AUTOMATIC DATA OR INVALIDATE ADDRESSING. THE PURPOSE IS  */
*   /* TO ALLOW ALL PROLOGS, MOVES, AND EPILOGS TO EXECUTE TOGETHER. */
*   /* THE EFFECT IN ANY INTERVAL-DRIVEN MG ROUTINE IS AS IF THE     */
*   /* PROLOG, MOVE, AND EPILOG WERE EXECUTED AS SEQUENTIAL CODE     */
*   /* DCMOVE: IRBBREAK MACDATE Y-2 73018                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0071
*DCMOVE:                                                           0071
*                                                                  0071
*   /*****************************************************************/
*   /*                                                               */
*   /* INDICATE NEXT ROUTINE ADDRESS TO SVC MAINLINE FOR REENTRY     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0071
*   STSMENTR=ADDR(DCRT0019);                                       0071
DCMOVE   L     @12,@PC00001                                        0071
         L     @12,DCSMAPTR(,@12)                                  0071
         LA    @11,DCRT0019                                        0071
         ST    @11,STSMENTR(,@12)                                  0071
*   RESPECIFY                                                      0072
*    (GPR00P,                                                      0072
*     GPR01P,                                                      0072
*     GPR14P,                                                      0072
*     GPR15P) RESTRICTED;                                          0072
*   GENERATE;                                                      0073
         STM   @02,@12,28(@13)     SAVE REGISTERS FOR NEXT SEGMENT
         L     @13,4(,@13)         BACK CHAIN TO CALLERS SAVEAREA
         LM    @14,@12,12(@13)     RESTORE CALLERS REGISTERS
         BR    @14                 RETURN TO CALLER
*                                                                  0074
*   /*****************************************************************/
*   /*                                                               */
*   /* REENTRY POINT FROM CALLER, IGX00014.                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0074
*DCRT0019:                                                         0074
*   GENERATE;                                                      0074
DCRT0019 DS    0H                                                  0074
         STM   @14,@12,12(@13)     SAVE CALLERS REGISTERS
         L     @01,0(,@01)         GET ADDRESS OF SAVE AREA ADDRESS
         L     @01,0(,@01)         GET SAVE AREA ADDRESS
         LM    @02,@12,28(@01)     RELOAD REGISTERS
         ST    @13,4(,@01)         BACK CHAIN NEW SAVEAREA TO OLD
         ST    @01,8(,@13)         FORWARD CHAIN FROM OLD TO NEW
         LR    @13,@01             R13 NOW POINTS TO NEW SAVEAREA
*   RESPECIFY                                                      0075
*    (GPR00P,                                                      0075
*     GPR01P,                                                      0075
*     GPR14P,                                                      0075
*     GPR15P) UNRESTRICTED;                                        0075
*                                                                  0075
*   /*****************************************************************/
*   /*                                                               */
*   /* BEGIN MOVE SEGMENT. THIS CODE IS ENTERED DISABLED WITH NO     */
*   /* LOCKS AND EXECUTES WITH NO PAGE FAULTS. THE FUNCTION IS TO    */
*   /* MOVE DATA INTO STORAGE OBTAINED BY THE PROLOG. THE FIRST      */
*   /* SEGMENT OF CODE DEALS WITH THE ONLINE/OFFLINE STATUS OF THE   */
*   /* CPUS. THREE 16 BIT FIELDS ARE INVOLVED. CSDCPUAL IS A SET OF  */
*   /* FLAGS WHICH INDICATE THE CURRENT ONLINE STATUS. CSDMF1CP      */
*   /* INDICATES WHETHER ANY VARY ACTIVITY HAS TAKEN PLACE ON A CPU  */
*   /* SINCE THE LAST INTERVAL. DCCPUVLD INDICATES THOSE CPUS WHICH  */
*   /* HAVE BEEN OBSERVED ONLINE IN AT LEAST ONE INTERVAL            */
*   /* OBSERVATION. SMF RECORD CPU ENTRIES WILL BE CREATED ONLY FOR  */
*   /* THOSE CPUS INDICATED IN DCCPUVLD.                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0076
*   DCCPUAL=CSDCPUAL;               /* MOVE CURRENT ONLINE FLAGS TO
*                                      TEMPORARY STORAGE             */
         L     @11,CVTPTR                                          0076
         L     @11,CVTCSD(,@11)                                    0076
         MVC   DCCPUAL(2),CSDCPUAL(@11)                            0076
*   DCMF1CP=CSDMF1CP;               /* MOVE VARY ACTIVITY FLAGS TO 0077
*                                      TEMPORARY STORAGE             */
         MVC   DCMF1CP(2),CSDMF1CP(@11)                            0077
*   CSDMF1CP=0;                     /* CLEAR CSD FIELD FOR ACTIVITY
*                                      DURING THE NEXT INTERVAL      */
*                                                                  0078
         SLR   @08,@08                                             0078
         STH   @08,CSDMF1CP(,@11)                                  0078
*   /*****************************************************************/
*   /*                                                               */
*   /* IF A CPU IS NOW ONLINE WHOSE FLAG IS NOT SET IN FIELD         */
*   /* DCCPUCVLD, SET ITS FLAG IN DCCPUVLD INDICATING THAT THIS CPU  */
*   /* HAS BEEN OBSERVED TO BE ONLINE.                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0079
*   DCCPUMSK=DCCPUZER;              /* SET MASK FOR CPU 0            */
         L     DCCPUMSK,@CF01353                                   0079
*   DO DCCPUIDX=1 TO CVTMAXMP+1;    /* LOOP TO MAXIMUM CPU ADDRESS   */
         LA    DCCPUIDX,1                                          0080
         B     @DE00080                                            0080
@DL00080 DS    0H                                                  0081
*     IF(DCCPUMSK&DCCPUAL)^=0&(DCCPUMSK&DCCPUVLD)=0 THEN           0081
*                                                                  0081
         SLR   @11,@11                                             0081
         SLR   @08,@08                                             0081
         ICM   @08,3,DCCPUAL                                       0081
         NR    @08,DCCPUMSK                                        0081
         CR    @08,@11                                             0081
         BE    @RF00081                                            0081
         L     @08,@PC00001                                        0081
         L     @08,DCSMAPTR(,@08)                                  0081
         LH    @07,DCCPUVLD(,@08)                                  0081
         N     @07,@CF01381                                        0081
         LR    @03,DCCPUMSK                                        0081
         NR    @03,@07                                             0081
         CLR   @03,@11                                             0081
         BNE   @RF00081                                            0081
*       /*************************************************************/
*       /*                                                           */
*       /* TURN ON FLAG IN VALID FIELD IF THIS CPU IS NOW ONLINE AND */
*       /* HAS NOT PREVIOUSLY BEEN VALID                             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0082
*       DCCPUVLD=DCCPUVLD×DCCPUMSK;                                0082
         OR    @07,DCCPUMSK                                        0082
         STH   @07,DCCPUVLD(,@08)                                  0082
*     DCCPUMSK=DCCPUMSK/2;          /* SHIFT RIGHT FOR NEXT CPU      */
@RF00081 SRL   DCCPUMSK,1                                          0083
*   END;                                                           0084
*                                                                  0084
         AL    DCCPUIDX,@CF00060                                   0084
@DE00080 LA    @11,1                                               0084
         L     @08,CVTPTR                                          0084
         AH    @11,CVTMAXMP(,@08)                                  0084
         CR    DCCPUIDX,@11                                        0084
         BNH   @DL00080                                            0084
*   /*****************************************************************/
*   /*                                                               */
*   /* PARTIALLY INITIALIZE THE SMF RECORD POINTED TO BY THE STSMIADD*/
*   /* FIELD. SET ONLINE STATUS FLAGS FOR ALL VALID CPUS AND MOVE IN */
*   /* WRAP-AROUND WAIT TIME COUNTERS FOR THOSE CPUS PROVIDE         */
*   /* ADDRESSIBILITY TO SMF RECORD                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0085
*   RESPECIFY                                                      0085
*     SMFRCD70 BASED(DCSMIADD);                                    0085
*                                                                  0085
*   /*****************************************************************/
*   /*                                                               */
*   /* MOVE IN STANDARD SMF HEADER                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0086
*   SMFRCD70=DCSMFHDR(1:LENGTH(SMFRCD70));                         0086
*                                                                  0086
         L     @11,@PC00001+16                                     0086
         MVC   SMFRCD70(48,DCSMIADD),DCSMFHDR(@11)                 0086
*   /*****************************************************************/
*   /*                                                               */
*   /* FILL IN UNSET FIELDS                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0087
*   SMF70RTY=DCRCDTYP;              /* RECORD TYPE                   */
         MVI   SMF70RTY(DCSMIADD),X'46'                            0087
*   SMF70CYC=DCNOSAMP;              /* NO SAMPLING                   */
         MVC   SMF70CYC(2,DCSMIADD),@CB01350                       0088
*   SMF70A=SMF70A&&SMF70A;          /* ZERO CONTROL PORTION OF RECORD*/
         LA    @11,SMF70SIZ(,DCSMIADD)                             0089
