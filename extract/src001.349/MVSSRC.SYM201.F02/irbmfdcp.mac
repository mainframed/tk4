*   STSGFREE(STRVNSGT)=GPR00P;      /* GETMAIN CONTROL WORD TO SGC   */
         L     @12,@PC00001                                        0053
         L     @12,DCSMAPTR(,@12)                                  0053
         L     @12,STSMRVT(,@12)                                   0053
         L     @11,STRVNSGT(,@12)                                  0053
         LR    @08,@11                                             0053
         SLA   @08,3                                               0053
         L     @03,STRVSSGT(,@12)                                  0053
         LA    @15,0(@08,@03)                                      0053
         AL    @15,@CF01379                                        0053
         ST    GPR00P,STSGFREE(,@15)                               0053
*   STSGADD(STRVNSGT)=GPR01P;       /* ADDRESS                       */
*                                                                  0054
         AL    @03,@CF01380                                        0054
         ST    GPR01P,STSGADD-4(@08,@03)                           0054
