         TITLE '*/* IRBMFMFC - MEASUREMENT FACILITY CONTROL            *
                       '
IRBMFMFC CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IRBMFMFC  74.087'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*                                                                  0032
*   /*****************************************************************/
*   /*                                                               */
*   /* MACRO AUTOMATIC LIST INITIALIZATION FROM STATIC LIST          */
*   /* EXPANSIONS                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0032
*   MFDCBAPL=MFDCBSPL;              /* DCB LIST                      */
         MVC   MFDCBAPL(96),MFDCBSPL                               0032
*   MFENQAPL=MFENQSPL;              /* ENQ LIST                      */
         MVC   MFENQAPL(16),MFENQSPL                               0033
*   MFESTAPL=MFESTSPL;              /* ESTAE LIST                    */
         MVC   MFESTAPL(16),MFESTSPL                               0034
*   MFEXTAPL=MFEXTSPL;              /* EXTRACT LIST                  */
         MVC   MFEXTAPL(16),MFEXTSPL                               0035
*   MFOPEAPL=MFOPESPL;              /* OPEN LIST                     */
         MVC   MFOPEAPL(4),MFOPESPL                                0036
*   DCBPTR=ADDR(MFDCBAPL);          /* SET DCB POINTER.              */
*                                                                  0037
         LA    @10,MFDCBAPL                                        0037
         LR    DCBPTR,@10                                          0037
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE AUTOMATIC STORAGE FOR MFPCT BEFORE MFMVT,MFCOA, AND*/
*   /* MFSQU                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0038
*   MFPCT=MFPCT&&MFPCT;             /* ZERO ALL PCT FIELDS TO BEGIN  */
         XC    MFPCT(64),MFPCT                                     0038
*   MFPCNAME='MFPC';                /* MFPCT NAME                    */
         MVC   MFPCNAME(4),@CC00679                                0039
*   MFPCMVT=ADDR(MFMVT);            /* SET PTR TO MFMVT              */
         LA    @05,MFMVT                                           0040
         ST    @05,MFPCMVT                                         0040
*   MFPCCOA=ADDR(MFCOA);            /* SET PTR TO MFCOA              */
         LA    @05,MFCOA                                           0041
         ST    @05,MFPCCOA                                         0041
*   MFPCSQU=ADDR(MFSQU);            /* SET PTR TO MFSQU              */
         LA    @05,MFSQU                                           0042
         ST    @05,MFPCSQU                                         0042
*   MFPCMDCB=ADDR(MFDCBAPL);        /* ADDRESS OF MESSAGE-SYSOUT DATA
*                                      SET DCB                       */
         ST    @10,MFPCMDCB                                        0043
*   MFPCMFR=ADDR(IRBMFMFR);         /* RETRY ENTRY POINT             */
         LA    @10,IRBMFMFR                                        0044
         ST    @10,MFPCMFR                                         0044
*   MFMVNUM=CMAXMEAS;               /* SET NUMBER OF PROBLEM STATE 0045
*                                      MEASUREMENTS DEFINED.         */
*                                                                  0045
         MVC   MFMVNUM(4),@CF00178                                 0045
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE AUTOMATIC STORAGE FOR MFMVT BEFORE MFPMA           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0046
*   DO I=1 BY 1 TO CMAXMEAS;        /* FOR EACH MEASUREMENT OPTION   */
         LA    I,1                                                 0046
@DL00046 DS    0H                                                  0047
*     MFMVADD(I)=ADDR(INPMA(I));    /* SET MFMVT PTR S TO PMA S      */
         LR    @10,I                                               0047
         SLA   @10,2                                               0047
         LR    @05,I                                               0047
         MH    @05,@CH00528                                        0047
         LA    @05,INPMA-12(@05)                                   0047
         ST    @05,MFMVADD-4(@10)                                  0047
*     MFMVADD(I)->MFPMOPT=CNULLOPT; /* ZERO OPTION INDICATOR         */
         SR    @10,@10                                             0048
         ST    @10,MFPMOPT(,@05)                                   0048
*     MFMVADD(I)->MFPMCON=CNONE;    /* SET OPTION NOT SPECIFIED      */
*                                                                  0049
         OI    MFPMCON(@05),B'10000000'                            0049
*     /***************************************************************/
*     /*                                                             */
*     /* IF DEVICE OPTION                                            */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0050
*     IF(I-1)*LENGTH(MFMVADD)=ADDR(MFMVDEV)-ADDR(MFMVOPT)THEN      0050
         LR    @10,I                                               0050
         BCTR  @10,0                                               0050
         SLA   @10,2                                               0050
         LA    @02,MFMVDEV                                         0050
         LA    @15,MFMVOPT                                         0050
         SR    @02,@15                                             0050
         CR    @10,@02                                             0050
         BNE   @RF00050                                            0050
*       MFMVADD(I)->MFPMDEVF(CNTRL)=MFPMDEVC;/* SET ALL DEVICE TYPES
*                                      AS NOT SPECIFIED              */
         MVI   MFPMDEVF(@05),X'FC'                                 0051
*   END;                            /* END DO ON I INDEX             */
*                                                                  0052
@RF00050 AH    I,@CH00042                                          0052
         CH    I,@CH00178                                          0052
         BNH   @DL00046                                            0052
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE AUTOMATIC STORAGE FOR MFPMA                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0053
*   MFMVCPU->MFPMSARG='IRBMFRCR';   /* CPU REPORT PROGRAM NAME       */
         L     @10,MFMVCPU                                         0053
         MVC   MFPMSARG(8,@10),@CC00682                            0053
*   MFMVPAG->MFPMSARG='IRBMFRPR';   /* PAGING REPORT PROGRAM NAME    */
         L     @10,MFMVPAG                                         0054
         MVC   MFPMSARG(8,@10),@CC00683                            0054
*   MFMVWKL->MFPMSARG='IRBMFRWR';   /* WORKLOAD REPORT PROGRAM NAME  */
         L     @10,MFMVWKL                                         0055
         MVC   MFPMSARG(8,@10),@CC00684                            0055
*   MFMVCHA->MFPMSARG='IRBMFRHR';   /* CHANNEL REPORT PROGRAM NAME   */
         L     @10,MFMVCHA                                         0056
         MVC   MFPMSARG(8,@10),@CC00685                            0056
*   MFMVDEV->MFPMSARG='IRBMFRDR';   /* DEVICE REPORT PROGRAM NAME    */
*                                                                  0057
         L     @10,MFMVDEV                                         0057
         MVC   MFPMSARG(8,@10),@CC00686                            0057
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE AUTOMATIC STORAGE FOR MFCOA                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0058
*   MFCOA=MFCOA&&MFCOA;             /* ZERO TABLE MFCOA              */
         XC    MFCOA(32),MFCOA                                     0058
*   MFCOTCEC=CNONE;                 /* TRACE OPTION NOT SPECIFIED    */
         OI    MFCOTCEC,B'10000000'                                0059
*   MFCOCYCC=CNONE;                 /* CYCLE OPTION NOT SPECIFIED    */
         OI    MFCOCYCC,B'10000000'                                0060
*   MFCOOUTC=CNONE;                 /* OUTCLASS OPTION NOT SPECIFIED */
         OI    MFCOOUTC,B'10000000'                                0061
*   MFCOINTC=CNONE;                 /* INTERVAL OPTION NOT SPECIFIED */
         OI    MFCOINTC,B'10000000'                                0062
*   MFCOREPC=CNONE;                 /* REPORT OPTION NOT SPECIFIED   */
         OI    MFCOREPC,B'10000000'                                0063
*   MFCOSTPC=CNONE;                 /* STOP OPTION NOT SPECIFIED     */
         OI    MFCOSTPC,B'10000000'                                0064
*   MFCOOPIC=CNONE;                 /* OPI OPTION NOT SPECIFIED      */
         OI    MFCOOPIC,B'10000000'                                0065
*   MFCOMEMC=CNONE;                 /* MEMBER OPTION NOT SPECIFIED   */
*                                                                  0066
         OI    MFCOMEMC,B'10000000'                                0066
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE AUTOMATIC STORAGE FOR MFSQU                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0067
*   MFSQNAME='MFSQ';                /* MFSQU NAME                    */
         MVC   MFSQNAME(4),@CC00687                                0067
*   MFSQFRST=0;                     /* NO QUEUE ELEMENTS EXIST       */
         SR    @10,@10                                             0068
         ST    @10,MFSQFRST                                        0068
*   MFSQLAST=ADDR(MFSQFRST)-        /* ADD 1ST ELEMENT TO THIS PTR   */
*       (ADDR(MFPCMVT->MFSENEXT)-ADDR(MFPCMVT->MFSEL));            0069
         LA    @05,MFSQFRST                                        0069
         L     @15,MFPCMVT                                         0069
         LR    @14,@15                                             0069
         SR    @14,@15                                             0069
         SR    @05,@14                                             0069
         ST    @05,MFSQLAST                                        0069
*   MFSQNUMB=0;                     /* NUMBER OF ELEMENTS            */
         ST    @10,MFSQNUMB                                        0070
*   MFSQSP=0;                       /* SUBPOOL FOR MFSEL STORAGE     */
         MVI   MFSQSP,X'00'                                        0071
*   MFSQLEN=LENGTH(MFSEL);          /* LENGTH OF MFSEL STORAGE       */
         MVC   MFSQLEN(3),@CF00212+1                               0072
*                                                                  0073
*   /*****************************************************************/
*   /*                                                               */
*   /* MFC FUNCTIONAL CODE ENQ TO SERIALIZE MF/1                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0073
*   RFY                                                            0073
*    (GPR00F,                                                      0073
*     GPR01F,                                                      0073
*     GPR14F,                                                      0073
*     GPR15F)RSTD;                                                 0073
*   GEN REFS(MFENQAPL);                                            0074
         ENQ   MF=(E,MFENQAPL)      ENQ ON SYSZRBMF.ACTIVE TO ALLOW
*                                     ONLY ONE MF/1 TASK ACTIVE.
*   RFY                                                            0075
*    (GPR00F,                                                      0075
*     GPR01F,                                                      0075
*     GPR14F)UNRSTD;                /* SAVE GPR15F FOR TEST BELOW    */
*   IF GPR15P^=0 THEN               /* IF ENQ RESOURCE IS NOT AVAIL-
*                                      ABLE, MF/1 IS ALREADY ACTIVE  */
         LTR   GPR15P,GPR15P                                       0076
         BZ    @RF00076                                            0076
*     DO;                           /* ISSUE MESSAGE THAT SAYS MF/1
*                                      IS ALREADY ACTIVE             */
*       RFY                                                        0078
*        (GPR15F)UNRSTD;            /* UNRSTD FROM ENQ ABOVE         */
*                                                                  0078
*       /*************************************************************/
*       /*                                                           */
*       /* MACDATE Y-2 73018                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0079
*       RESPECIFY                                                  0079
*        (GPR00P,                                                  0079
*         GPR01P,                                                  0079
*         GPR14P,                                                  0079
*         GPR15P)RESTRICTED;                                       0079
*       GEN;                                                       0080
         LOAD  EP=IRBMFMPR          LOAD MESSAGE MODULE
*       MFMODEPA=GPR00P;            /* SAVE ENTRY POINT ADDRESS      */
*                                                                  0081
         LR    MFMODEPA,GPR00P                                     0081
*       /*************************************************************/
*       /*                                                           */
*       /* MACDATE Y-2 73018                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0082
*       RESPECIFY                                                  0082
*        (GPR00P,                                                  0082
*         GPR01P,                                                  0082
*         GPR14P,                                                  0082
*         GPR15P)UNRESTRICTED;                                     0082
*       CALL IRBMFMPR(MID200I       /* ISSUE MESSAGE.                */
*           ,CNOTEXT                /* NO VARIABLE TEXT              */
*           ,CNOREPLY               /* NO NO REPLY AREA              */
*           ,CNODCB                 /* NO OUTPUT DATA SET DCB        */
*           );                                                     0083
*                                                                  0083
         LR    @15,MFMODEPA                                        0083
         LA    @01,@AL00083                                        0083
         BALR  @14,@15                                             0083
*       /*************************************************************/
*       /*                                                           */
*       /* MACDATE Y-2 73018                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0084
*       RESPECIFY                                                  0084
*        (GPR00P,                                                  0084
*         GPR01P,                                                  0084
*         GPR14P,                                                  0084
*         GPR15P)RESTRICTED;                                       0084
*       GEN;                                                       0085
*                                                                  0085
         DELETE EP=IRBMFMPR         DELETE MESSAGE MODULE
*       /*************************************************************/
*       /*                                                           */
*       /* MACDATE Y-2 73018                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0086
*       RESPECIFY                                                  0086
*        (GPR00P,                                                  0086
*         GPR01P,                                                  0086
*         GPR14P,                                                  0086
*         GPR15P)UNRESTRICTED;                                     0086
*       RETURN CODE(0);             /* DO NOT ACCEPT ANY INPUTS. DO
*                                      NOT START MF/1.               */
         L     @13,4(,@13)                                         0087
         L     @00,@SIZDATD                                        0087
         LR    @01,@11                                             0087
         FREEMAIN R,LV=(0),A=(1)
         SR    @15,@15                                             0087
         L     @14,12(,@13)                                        0087
         LM    @00,@12,20(@13)                                     0087
         BR    @14                                                 0087
*     END;                          /* END, IF GPR15P^=0             */
*                                                                  0088
*   /*****************************************************************/
*   /*                                                               */
*   /* EXTRACT STOP COMMAND ECB ADDRESS                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0089
*   RFY                                                            0089
*    (GPR00F,                                                      0089
*     GPR01F,                                                      0089
*     GPR14F,                                                      0089
*     GPR15F)RSTD;                                                 0089
@RF00076 DS    0H                                                  0090
*   GEN REFS(MFEXTAPL,MFCSCLA)SETS(MFCSCLA);                       0090
         EXTRACT MFCSCLA,MF=(E,MFEXTAPL) EXTRACT STOP COMMAND ECB ADDR
*   RFY                                                            0091
*    (GPR00F,                                                      0091
*     GPR01F,                                                      0091
*     GPR14F,                                                      0091
*     GPR15F)UNRSTD;                                               0091
*   MFPCSEAD=MFCSCECB;              /* SAVE STOP COMMAND ECB ADDRESS */
*                                                                  0092
         L     @10,MFCSCLA                                         0092
         MVC   MFPCSEAD(4),MFCSCECB(@10)                           0092
*   /*****************************************************************/
*   /*                                                               */
*   /* EXECUTE INPUT/MERGE CONTROL                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0093
*   RFY                                                            0093
*    (GPR00P,                                                      0093
*     GPR01F,                                                      0093
*     GPR14P,                                                      0093
*     GPR15P)RSTD;                                                 0093
*   GEN;                                                           0094
         LOAD  EP=IRBMFINP          LOAD INPUT/MERGE CONTROL
*   RFY                                                            0095
*    (GPR01P,                                                      0095
*     GPR14P,                                                      0095
*     GPR15P)UNRSTD;                /* SAVE GPR00 CONTENTS           */
*   MFMODEPA=GPR00P;                /* SAVE ENTRY POINT FROM LOAD  0096
*                                      MACRO                         */
         LR    MFMODEPA,GPR00P                                     0096
*   RFY                                                            0097
*    (GPR00P)UNRSTD;                /* RSTD FROM LOAD MACRO ABOVE    */
*                                                                  0097
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL INPUT/MERGE CONTROL                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0098
*   CALL IRBMFINP(MFEXECPA          /* EXEC STATEMENT PARMS, INPUT   */
*       ,MFMVT                      /* MEASUREMENT OPTIONS, OUTPUT   */
*       ,MFCOA                      /* COMMON OPTIONS, OUTPUT        */
*       );                                                         0098
         L     @10,@PC00001                                        0098
         ST    @10,@AL00001                                        0098
         LA    @10,MFMVT                                           0098
         ST    @10,@AL00001+4                                      0098
         LA    @10,MFCOA                                           0098
         ST    @10,@AL00001+8                                      0098
         LR    @15,MFMODEPA                                        0098
         LA    @01,@AL00001                                        0098
         BALR  @14,@15                                             0098
*   RFY                                                            0099
*    (GPR00P,                                                      0099
*     GPR01P,                                                      0099
*     GPR14P,                                                      0099
*     GPR15P)RSTD;                                                 0099
*   GEN;                                                           0100
         DELETE EP=IRBMFINP         DELETE INPUT MERGE CONTROL
*   RFY                                                            0101
*    (GPR00P,                                                      0101
*     GPR01P,                                                      0101
*     GPR14P,                                                      0101
*     GPR15P)UNRSTD;                                               0101
*                                                                  0101
*   /*****************************************************************/
*   /*                                                               */
*   /* ALLOCATE AND OPEN MESSAGE-SYSOUT DATA SET                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0102
*   RFY                                                            0102
*    (GPR00P,                                                      0102
*     GPR01P,                                                      0102
*     GPR14P,                                                      0102
*     GPR15P)RSTD;                                                 0102
*   GEN;                                                           0103
         LOAD  EP=IRBMFALL          LOAD DYNAMIC ALLOCATION MODULE
*   RFY                                                            0104
*    (GPR01P,                                                      0104
*     GPR14P,                                                      0104
*     GPR15P)UNRSTD;                /* SAVE GPR00 CONTENTS           */
*   MFMODEPA=GPR00P;                /* SAVE ENTRY POINT FROM LOAD  0105
*                                      MACRO                         */
         LR    MFMODEPA,GPR00P                                     0105
*   RFY                                                            0106
*    (GPR00P)UNRSTD;                /* RSTD FOR LOAD MACRO ABOVE     */
*   CALL IRBMFALL('MFMESSGE'        /* DDNAME 8 BYTES                */
*       ,MFCOOUTV                   /* SYSOUT CLASS FROM INPUT OPTION
*                                      1 BYTE                        */
*       ,'0'B                       /* DO NOT UNALLOCATE AT CLOSE    */
*       );                                                         0107
         MVC   @AL00001(12),@AL00107                               0107
         LA    @10,MFCOOUTV                                        0107
         ST    @10,@AL00001+4                                      0107
         LR    @15,MFMODEPA                                        0107
         LA    @01,@AL00001                                        0107
         BALR  @14,@15                                             0107
*   RFY                                                            0108
*    (GPR00P,                                                      0108
*     GPR01P,                                                      0108
*     GPR14P,                                                      0108
*     GPR15P)RSTD;                                                 0108
*   GEN;                                                           0109
         DELETE EP=IRBMFALL         DELETE ALLOCATION MODULE
*   RFY                                                            0110
*    (GPR00P,                                                      0110
*     GPR01P,                                                      0110
*     GPR14P,                                                      0110
*     GPR15P)UNRSTD;                                               0110
*                                                                  0110
*   /*****************************************************************/
*   /*                                                               */
*   /* MACDATE Y-2 73018                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0111
*   RESPECIFY                                                      0111
*    (GPR00P,                                                      0111
*     GPR01P,                                                      0111
*     GPR14P,                                                      0111
*     GPR15P)RESTRICTED;                                           0111
*   GEN REFS(MFDCBAPL,MFOPEAPL);                                   0112
*                                                                  0112
         OPEN (MFDCBAPL),MF=(E,MFOPEAPL) OPEN MESSAGE-SYSOUT DATA SET
*   /*****************************************************************/
*   /*                                                               */
*   /* MACDATE Y-2 73018                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0113
*   RESPECIFY                                                      0113
*    (GPR00P,                                                      0113
*     GPR01P,                                                      0113
*     GPR14P,                                                      0113
*     GPR15P)UNRESTRICTED;                                         0113
*   IF DCBOFOPN^='1'B THEN          /* IF OPEN NOT OK,               */
         TM    DCBOFOPN+40(DCBPTR),B'00010000'                     0114
         BO    @RF00114                                            0114
*     DO;                                                          0115
*       MFABCODE=COPENERR;          /* ABEND CODE.                   */
         MVC   MFABCODE(4),@CF00662                                0116
*       MFREASON=0;                 /* REASON CODE FOR ABEND IS OPEN */
         SR    @10,@10                                             0117
         ST    @10,MFREASON                                        0117
*       GOTO MF080;                 /* ABEND.                        */
         B     MF080                                               0118
*     END;                                                         0119
*                                                                  0119
*   /*****************************************************************/
*   /*                                                               */
*   /* ESTABLISH ESTAE RECOVERY ROUTINE MACDATE Y-2 73018            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0120
*   RESPECIFY                                                      0120
*    (GPR00P,                                                      0120
*     GPR01P,                                                      0120
*     GPR14P,                                                      0120
*     GPR15P)RESTRICTED;                                           0120
@RF00114 DS    0H                                                  0121
*   RFY                                                            0121
*    (GPR02P)RSTD;                                                 0121
*   MFESTPCT=ADDR(MFPCT);           /* SET UP ESTAE PARAM INPUT      */
         LA    @10,MFPCT                                           0122
         ST    @10,MFESTPCT                                        0122
*   MFESTRUB(CCOD+16)='1'B;         /* ESTAE RTN UPDATES CODE REG.   */
*   MFESTRUB(CDAT+16)='1'B;         /* ESTAE RTN UPDATES DATA REG.   */
         OI    MFESTRUB+3,B'00110000'                              0124
*   MFESTCOD=GPRCOD;                /* SAVE CODE REG VALUE FOR ESTAE.*/
         ST    GPRCOD,MFESTCOD                                     0125
*   MFESTDAT=GPRDAT;                /* SAVE DATA REG VALUE FOR ESTAE.*/
         ST    GPRDAT,MFESTDAT                                     0126
