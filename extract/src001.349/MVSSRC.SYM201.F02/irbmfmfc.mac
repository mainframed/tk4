*                                                                  0127
         ESTAE PARAM=MFESTPPL,MF=(E,MFESTAPL)
*   /*****************************************************************/
*   /*                                                               */
*   /* MACDATE Y-2 73018                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0128
*   RESPECIFY                                                      0128
*    (GPR00P,                                                      0128
*     GPR01P,                                                      0128
*     GPR14P,                                                      0128
*     GPR15P)UNRESTRICTED;                                         0128
*                                                                  0129
*/* RETRY ENTRY POINT                                                */
*                                                                  0129
*IRBMFMFR:                                                         0129
*   ;                               /* ENTER FROM ESTAE ROUTINE WITH
*                                      CODE AND DATA REGS RESTORED.  */
IRBMFMFR DS    0H                                                  0130
*   GEN;                                                           0130
*                                                                  0130
         LR    @13,GPRDAT           RESET SAVE AREA REG FROM DATA REG
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE MFSTART SVC. IT STARTS MF/1 MEASUREMENTS SET BY INPUT/  */
*   /* MERGE CONTROL IN TABLES MFCOA AND MFMVT->MFPMA. CONTROL PASSES*/
*   /* FROM MFSTART TO DATA CONTROL BEFORE RETURNING TO MFC MAINLINE.*/
*   /* SET UP INPUTS TO SVC MFSTART. SET PARAMETER LIST.             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0131
*   MFSTMVT=ADDR(MFMVT);            /* MEASUREMENT OPTIONS           */
         LA    @10,MFMVT                                           0131
         ST    @10,MFSTMVT                                         0131
*   MFSTCOA=ADDR(MFCOA);            /* COMMON OPTIONS                */
         LA    @10,MFCOA                                           0132
         ST    @10,MFSTCOA                                         0132
*   MFSTCON=ADDR(IRBMFDTA);         /* DATA CONTROL ENTRY            */
         L     @10,@CV00614                                        0133
         ST    @10,MFSTCON                                         0133
*   MFSTPCT=ADDR(MFPCT);            /* PASS MFPCT TO DATA CONTROL  0134
*                                      IRBMFDTA                      */
*                                                                  0134
         LA    @10,MFPCT                                           0134
         ST    @10,MFSTPCT                                         0134
*   /*****************************************************************/
*   /*                                                               */
*   /* MACDATE Y-2 73018                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0135
*   RESPECIFY                                                      0135
*    (GPR00P,                                                      0135
