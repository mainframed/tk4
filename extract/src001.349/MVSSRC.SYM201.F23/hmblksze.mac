         TITLE 'HMBLKSZE  -  EXIT ROUTINE FOR SYSPRINT'
         LCLA  &T,&SPN                                            0001
.@001    ANOP                                                     0001
HMBLKSZE CSECT ,                                                  0001
         BC    15,14(0,@F)
         DC    C'©HMBLKSZE'                                       0001
         BALR  @B,0                                               0001
@PSTART  DS    0H                                                 0001
         USING @PSTART+00000,@B                                   0001
         LR    @C,@1                                              0001
         L     @0,@SIZ001                                         0001
         GETMAIN  R,LV=(0)                                        0001
         XR    @C,@1                                              0001
         XR    @1,@C                                              0001
         XR    @C,@1                                              0001
         USING @DATD+00000,@C                                     0001
         XC    @TEMPS(@L),@TEMPS                                  0001
*   GEN(  ST  14,FOURTEEN);             /* SAVE REG 14 CONTENTS       *
           ST  14,FOURTEEN
         DS    0H
*   GEN(  USING IHADCB,R1);             /* USE REG 1 AS BASE FOR DSECT*
           USING IHADCB,R1
         DS    0H
*   /* CHECK TO SEE IF A BLOCKING FACTOR WAS USED                     *
*   IF DCBLRECL > DCBBLKSI              /* IS BLKSIZE = LOGICAL RECORD*
*      THEN DCBBLKSI = DCBLRECL;        /* STORE RECORD SIZE IN
         LH    @F,DCBBLKSI                                        0009
         CH    @F,DCBLRECL                                        0009
         BC    10,@9FF                                            0009
*                                         BLOCKSIZE                   *
         MVC   DCBBLKSI(2),DCBLRECL                               0010
*   GEN(  L     14,FOURTEEN);           /* RESTORE REG 14             *
@9FF     EQU   *                                                  0011
           L     14,FOURTEEN
         DS    0H
*   END;
@EL01    LR    @1,@C                                              0012
         L     @0,@SIZ001                                         0012
         FREEMAIN R,LV=(0),A=(1)                                  0012
         BCR   15,@E                                              0012
@DATA1   EQU   *
@0       EQU   00                  EQUATES FOR REGISTERS 0-15
@1       EQU   01
@2       EQU   02
@3       EQU   03
@4       EQU   04
@5       EQU   05
@6       EQU   06
@7       EQU   07
@8       EQU   08
@9       EQU   09
@A       EQU   10
@B       EQU   11
@C       EQU   12
@D       EQU   13
@E       EQU   14
@F       EQU   15
         DS    0F
@SIZ001  DC    AL1(&SPN)
         DC    AL3(@DATEND-@DATD)
         DS    0D
@DATA    EQU   *
DCBADR   EQU   00000000            FULLWORD INTEGER
R1       EQU   00000001            FULLWORD POINTER REGISTER
MAINT    EQU   @DATA+00000000      600 BYTE(S)
         DS    00000600C
@L       EQU   1
@DATD    DSECT
FOURTEEN EQU   @DATD+00000000      FULLWORD INTEGER
         DS    00000004C
@TEMPS   DS    0F
         DS    C
         DCBD  DSORG=PO
HMBLKSZE CSECT
@DATD    DSECT
@DATEND  EQU   *
HMBLKSZE CSECT ,
         END    ,(C'PL/S',1400,73200)
