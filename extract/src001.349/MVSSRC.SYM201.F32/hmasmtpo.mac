*   CALL TPOERRCK;                  /*                               */
         BAL   @14,TPOERRCK                                        0183
*   RFY                                                            0184
*     RTNCODE RSTD;                 /*                               */
*   TPORTNCD=MAX(TPORTNCD,RTNCODE); /*                               */
         CR    TPORTNCD,RTNCODE                                    0185
         BNL   *+6
         LR    TPORTNCD,RTNCODE                                    0185
*   RFY                                                            0186
*     RTNCODE UNRSTD;               /*                               */
*                                                                  0186
*   /*****************************************************************/
*   /*                                                               */
