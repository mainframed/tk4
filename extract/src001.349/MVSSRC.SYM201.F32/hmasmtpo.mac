*                                                                  0175
         LA    LON,1                                               0175
*   /*****************************************************************/
*   /*                                                               */
*   /* ORDER ALL SERVICE SYSMODS THAT FMID FUNCTIONS THAT ARE NOT IN */
*   /* THE ICT (IE THE FUNCTION IS ALREADY APPLIED OR ACCEPTED) ...  */
*   /* EXCEPT SERVICE SYSMODS WHICH ARE BEING RE-INSTALLED. ORDERING */
*   /* IS DONE FOR PTFS FIRST, THEN APARS, THEN USERMODS SO THAT THE */
*   /* ORDER OF APPLICATION WILL BE IN THAT ORDER                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0176
*   CALL TPOEFSVC(IOPPPTF);         /* PROCESS PTF TYPE SYSMODS      */
         LA    @01,@AL00176                                        0176
         BAL   @14,TPOEFSVC                                        0176
*   CALL TPOEFSVC(IOPPAPAR);        /* PROCESS APAR TYPE SYSMODS     */
         LA    @01,@AL00177                                        0177
         BAL   @14,TPOEFSVC                                        0177
*   CALL TPOEFSVC(IOPPUSER);        /* PROCESS USERMOD TYPE SYSMODS  */
*                                                                  0178
         LA    @01,@AL00178                                        0178
         BAL   @14,TPOEFSVC                                        0178
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW THAT ALL SERVICE TO EXISTING FUNCTIONS HAVE BEEN ORDERED  */
*   /* CALL ROUTINE TO ORDER THE FUNCTION TYPE SYSMODS THAT ARE      */
*   /* PRESENT IN THE ICT                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0179
*   CALL TPONF;                     /* ORDER NEW FUNCTION            */
*                                                                  0179
         BAL   @14,TPONF                                           0179
*   /*****************************************************************/
*   /*                                                               */
*   /* ORDER ANY REMAINING SERVICE SYSMODS IN THE ICT ... THOSE WHICH*/
*   /* REMAIN AT THIS POINT ARE THOSE APPLY TO NEW FUNCTIONS AND     */
*   /* THOSE THAT ARE BEING RE-INSTALLED . ORDERING IS DONE FOR PTFS */
*   /* FIRST, THEN APARS, THEN USERMODS SO THAT THE ORDER OF         */
*   /* APPLICATION WILL BE IN THAT ORDER                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0180
*   CALL TPORESVC(IOPPPTF);         /* PROCESS PTF TYPE SYSMODS      */
         LA    @01,@AL00180                                        0180
         BAL   @14,TPORESVC                                        0180
*   CALL TPORESVC(IOPPAPAR);        /* PROCESS APAR TYPE SYSMODS     */
         LA    @01,@AL00181                                        0181
         BAL   @14,TPORESVC                                        0181
*   CALL TPORESVC(IOPPUSER);        /* PROCESS USERMOD TYPE SYSMODS  */
*                                                                  0182
         LA    @01,@AL00182                                        0182
         BAL   @14,TPORESVC                                        0182
*   /*****************************************************************/
*   /*                                                               */
*   /* AT THIS TIME EACH SYSMOD IN THE ICT SHOULD HAVE BEEN ASSIGNED */
*   /* AN ORDER NUMBER. IF ANY SYSMOD HAS NOT BENN ASSIGNED ONE THEN */
*   /* THERE IS INSUFFICIENT DATA PRESENT IN THE SYSMOD TO PROCESS   */
*   /* IT. ISSUE ERROR MESSAGES                                      */
*   /*                                                               */
*   /*****************************************************************/
