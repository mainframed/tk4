         TITLE 'HMASMTPD ICT SYSMOD SECTION DRIVER'
HMASMTPD CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMTPD  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*                                                                  0144
*   /*****************************************************************/
*   /*                                                               */
*   /* MAIN MODULE SEGMENT                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0144
*                                                                  0144
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE POINTERS SAVE AREAS AND SWITCHES                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0144
*   CALL TPDINIT;                   /*                               */
*                                                                  0144
         BAL   @14,TPDINIT                                         0144
*   /*****************************************************************/
*   /*                                                               */
*   /* SPECIAL INTERFACE FOR RESTORE (HMASMTPR)                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0145
*   IF CCARES=ON THEN               /*                               */
         TM    CCARES(CCAPTR),B'00001000'                          0145
         BNO   @RF00145                                            0145
*     CALL TPDRST;                  /*                               */
*                                                                  0146
         BAL   @14,TPDRST                                          0146
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESSING FOR APPLY AND ACCEPT                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0147
*   IF CCAAPPLY=ON×CCAACCPT=ON THEN /*                               */
@RF00145 TM    CCAAPPLY(CCAPTR),B'01100000'                        0147
         BZ    @RF00147                                            0147
*     CALL APPACC;                  /*                               */
*                                                                  0148
         BAL   @14,APPACC                                          0148
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO CALLER                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0149
*   RETURN CODE(HIGHTPD);           /*                               */
@RF00147 LR    @15,HIGHTPD                                         0149
         L     @13,4(,@13)                                         0149
         L     @14,12(,@13)                                        0149
         LM    @00,@12,20(@13)                                     0149
         BR    @14                                                 0149
*                                                                  0150
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP PARMS SWITCHES ETC.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0150
*TPDINIT:                                                          0150
*   PROC;                           /*                               */
@EL00001 L     @13,4(,@13)                                         0150
@EF00001 DS    0H                                                  0150
@ER00001 LM    @14,@12,12(@13)                                     0150
         BR    @14                                                 0150
TPDINIT  STM   @14,@01,12(@13)                                     0150
         STM   @03,@12,32(@13)                                     0150
*   HIGHTPD=ZERO;                   /*                               */
         SLR   HIGHTPD,HIGHTPD                                     0151
*   IOPPTR=TBLXIOP2;                /* SET UP IOP                    */
         L     @10,@PC00001                                        0152
         L     IOPPTR,TBLXIOP2(,@10)                               0152
*   END TPDINIT;                    /*                               */
@EL00002 DS    0H                                                  0153
@EF00002 DS    0H                                                  0153
@ER00002 LM    @14,@01,12(@13)                                     0153
         LM    @03,@12,32(@13)                                     0153
         BR    @14                                                 0153
*                                                                  0154
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL HMASMTPR FOR RESTORE PROCESSING                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0154
*TPDRST:                                                           0154
*   PROC;                           /*                               */
TPDRST   STM   @14,@01,@SA00003                                    0154
         STM   @03,@12,@SA00003+16                                 0154
*   CALL HMASMTPR(TBLXPARM);        /*                               */
         L     @10,@PC00001                                        0155
         ST    @10,@AL00001                                        0155
         L     @15,@CV01488                                        0155
         LA    @01,@AL00001                                        0155
         BALR  @14,@15                                             0155
*   RFY                                                            0156
*     RTNCODE RSTD;                 /*                               */
*   HIGHTPD=MAX(HIGHTPD,RTNCODE);   /* SET ERROR CODE                */
         CR    HIGHTPD,RTNCODE                                     0157
         BNL   *+6
         LR    HIGHTPD,RTNCODE                                     0157
*   RFY                                                            0158
*     RTNCODE UNRSTD;               /*                               */
*   END TPDRST;                     /*                               */
@EL00003 DS    0H                                                  0159
@EF00003 DS    0H                                                  0159
@ER00003 LM    @14,@01,@SA00003                                    0159
         LM    @03,@12,@SA00003+16                                 0159
         BR    @14                                                 0159
*APPACC:                                                           0160
*   PROC;                           /*                               */
*                                                                  0160
APPACC   STM   @14,@01,@SA00004                                    0160
         STM   @03,@12,@SA00004+16                                 0160
*   /*****************************************************************/
*   /*                                                               */
*   /* HMASMTPA BUILDS THE ENTIRE ICT SYSMOD SECTION                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0161
*   CALL HMASMTPA(TBLXPARM);        /*                               */
         L     @10,@PC00001                                        0161
         ST    @10,@AL00001                                        0161
         L     @15,@CV01489                                        0161
         LA    @01,@AL00001                                        0161
         BALR  @14,@15                                             0161
*   RFY                                                            0162
*     RTNCODE RSTD;                 /*                               */
*   HIGHTPD=MAX(HIGHTPD,RTNCODE);   /* SET ERROR CODE                */
         CR    HIGHTPD,RTNCODE                                     0163
         BNL   *+6
         LR    HIGHTPD,RTNCODE                                     0163
*   RFY                                                            0164
*     RTNCODE UNRSTD;               /*                               */
*                                                                  0164
*   /*****************************************************************/
*   /*                                                               */
*   /* HMASMTCR CHECKS PRE, REQ, MREQ, AND IREQ RELATIONSHIPS. IF    */
*   /* THEY ARE SATISFIED THEN ICTPRQOK IS SET FOR EACH SYSMOD       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0165
*   IF HIGHTPD<STOPFUNC THEN        /* NOT FLUSHING FUNCTION         */
         CH    HIGHTPD,@CH00265                                    0165
         BNL   @RF00165                                            0165
*     DO;                           /*                               */
*       MSGPARM='M';                /* MESSAGES WANTED               */
         MVI   MSGPARM,C'M'                                        0167
*       CALL HMASMTCR(TBLXPARM,MSGPARM);/*                           */
         L     @10,@PC00001                                        0168
         ST    @10,@AL00001                                        0168
         LA    @10,MSGPARM                                         0168
         ST    @10,@AL00001+4                                      0168
         L     @15,@CV01490                                        0168
         LA    @01,@AL00001                                        0168
         BALR  @14,@15                                             0168
*       RFY                                                        0169
*         RTNCODE RSTD;             /*                               */
*       HIGHTPD=MAX(HIGHTPD,RTNCODE);/* SET ERROR CODE               */
         CR    HIGHTPD,RTNCODE                                     0170
         BNL   *+6
         LR    HIGHTPD,RTNCODE                                     0170
*       RFY                                                        0171
*         RTNCODE UNRSTD;           /*                               */
*     END;                          /*                               */
*   IF HIGHTPD<STOPFUNC THEN        /* NOT FLUSHING FUNCTION         */
@RF00165 CH    HIGHTPD,@CH00265                                    0173
         BNL   @RF00173                                            0173
*     DO;                           /*                               */
*                                                                  0174
*       /*************************************************************/
*       /*                                                           */
*       /* EACH SYSMOD ENTRY IS CHECKED TO SEE IF IT HAS BEEN        */
*       /* SUPERSEDED BY ANOTHER SYSMOD NOW IN THE COMPLETED ICT     */
*       /* SYSMOD SECTION.                                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0175
*       CALL SUPCHK;                /*                               */
*                                                                  0175
         BAL   @14,SUPCHK                                          0175
*       /*************************************************************/
*       /*                                                           */
*       /* EACH SYSMOD ENTRY WHICH HAS IREQS IS CHECKED. THESE IREQS */
*       /* ARE CHECKED FOR BEING SUP'ED OR DELETED                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0176
*       CALL IREQCHK;               /*                               */
*                                                                  0176
         BAL   @14,IREQCHK                                         0176
*       /*************************************************************/
*       /*                                                           */
*       /* IF GROUP, THEN SOME SYSMODS MAY HAVE TO BE ELEMINATED FROM*/
*       /* ICT BECAUSE OF THE SUPING OR DELETING BY OTHER SYSMODS.   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0177
*       IF TBLGROUP=ON THEN         /* GROUP MODE                    */
         L     @10,@PC00001                                        0177
         L     @10,TBLXTBL(,@10)                                   0177
         TM    TBLGROUP(@10),B'00100000'                           0177
         BNO   @RF00177                                            0177
*         CALL GROUPCHK;            /*                               */
         BAL   @14,GROUPCHK                                        0178
*     END;                          /*                               */
*   END APPACC;                     /*                               */
@EL00004 DS    0H                                                  0180
@EF00004 DS    0H                                                  0180
@ER00004 LM    @14,@01,@SA00004                                    0180
         LM    @03,@12,@SA00004+16                                 0180
         BR    @14                                                 0180
*                                                                  0181
*   /*****************************************************************/
*   /*                                                               */
*   /* SYSMOD IREQ LISTS ARE CHECKED AND THOSE IREQS WHOSE CAUSERS   */
*   /* ARE SUP'ED, OR NOGO ARE MARKED AS 'ELININATED IREQS'          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0181
*IREQCHK:                                                          0181
*   PROC;                           /*                               */
IREQCHK  STM   @14,@12,@SA00005                                    0181
*   RFY                                                            0182
*     ICTPTF BASED(CCAICPTF);       /* ICT SYSMOD ENTRY BASE         */
*   RFY                                                            0183
*     ICTIRQLS BASED(ICTPICHN(1));  /* SYSMOD IREQ LIST BASE         */
*   DCL                                                            0184
*     A FIXED(31);                  /* LOOP CNTR                     */
*   DCL                                                            0185
*     B FIXED(31);                  /* LOOP CNTR                     */
*                                                                  0185
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP THRU ICT SYSMOD ENTRIES. FOR EACH SYSMOD WITH IREQS, LOOK*/
*   /* AT EACH IREQ                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0186
*   DO A=1 BY 1 WHILE ICTPEND(A)^=ICTENDNT;/*                        */
         LA    A,1                                                 0186
         B     @DE00186                                            0186
@DL00186 DS    0H                                                  0187
*     IF ICTPIRQP(A)=ON THEN        /* SYSMOD HAS IREQS              */
*                                                                  0187
         LR    @10,A                                               0187
         MH    @10,@CH01568                                        0187
         L     @02,CCAICPTF(,CCAPTR)                               0187
         ALR   @02,@10                                             0187
         AL    @02,@CF01575                                        0187
         TM    ICTPIRQP-9(@02),B'01000000'                         0187
         BNO   @RF00187                                            0187
*       /*************************************************************/
*       /*                                                           */
*       /* LOOK AT EACH SYSMODS IREQS                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0188
*       DO B=1 BY 1 WHILE ICTIRQND(B)^=ICTENDIX;/* SEARCH IREQS      */
*                                                                  0188
         LA    B,1                                                 0188
         B     @DE00188                                            0188
@DL00188 DS    0H                                                  0189
*         /***********************************************************/
*         /*                                                         */
*         /* SEE IF THE IREQ CAUSER HAS BEEN INVALIDATED             */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0189
*         CALL CSRCHK(ADDR(ICTIRQLS(B)));/* PASS PTR TO IREQ ENTRY   */
*                                                                  0189
         LR    @10,B                                               0189
         SLA   @10,4                                               0189
         L     @01,CCAICPTF(,CCAPTR)                               0189
         L     @01,ICTPICHN(,@01)                                  0189
         AL    @01,@CF01576                                        0189
         LA    @10,ICTIRQLS(@10,@01)                               0189
         ST    @10,@AFTEMPS                                        0189
         LA    @01,@AL00189                                        0189
         BAL   @14,CSRCHK                                          0189
*         /***********************************************************/
*         /*                                                         */
*         /* IF CAUSER INVLALIDATED THEN MARK IREQ ELIMINATED        */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0190
*         RFY                                                      0190
*           RTNCODE RSTD;           /*                               */
*         IF RTNCODE^=ZERO THEN     /* NON ZERO RTN MEANS BAD CAUSER */
         LTR   RTNCODE,RTNCODE                                     0191
         BZ    @RF00191                                            0191
*           ICTCSRF(B)=ON;          /*                               */
         LR    @14,B                                               0192
         SLA   @14,4                                               0192
         L     @10,CCAICPTF(,CCAPTR)                               0192
         L     @10,ICTPICHN(,@10)                                  0192
         ALR   @10,@14                                             0192
         BCTR  @10,0                                               0192
         OI    ICTCSRF-15(@10),B'00010000'                         0192
*         RFY                                                      0193
*           RTNCODE UNRSTD;         /*                               */
@RF00191 DS    0H                                                  0194
*       END;                        /* IREQ LIST LOOP                */
         AH    B,@CH00061                                          0194
@DE00188 LR    @10,B                                               0194
         SLA   @10,4                                               0194
         L     @01,CCAICPTF(,CCAPTR)                               0194
         L     @01,ICTPICHN(,@01)                                  0194
         AL    @01,@CF01576                                        0194
         L     @10,ICTIRQND(@10,@01)                               0194
         CL    @10,ICTENDIX                                        0194
         BNE   @DL00188                                            0194
*   END;                            /* LOOP THRU ICT SYSMODS         */
@RF00187 AH    A,@CH00061                                          0195
@DE00186 LR    @10,A                                               0195
         MH    @10,@CH01568                                        0195
         L     @02,CCAICPTF(,CCAPTR)                               0195
         AL    @02,@CF01578                                        0195
         L     @10,ICTPEND(@10,@02)                                0195
         CL    @10,ICTENDNT                                        0195
         BNE   @DL00186                                            0195
*   END IREQCHK;                    /*                               */
@EL00005 DS    0H                                                  0196
@EF00005 DS    0H                                                  0196
@ER00005 LM    @14,@12,@SA00005                                    0196
         BR    @14                                                 0196
*                                                                  0197
*   /*****************************************************************/
*   /*                                                               */
*   /* THE IREQ LIST ELEMENT ADDRESS IS PASSED AS A PARM. THE ICT    */
*   /* SYSMOD ENTRIES ARE CHECKED LOOKING FOR THE CAUSER OF THIS     */
*   /* IREQ. IF THE CAUSER IS SUPED THE RETURN CODE TO CALLER IS SET */
*   /* TO FOUR.                                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0197
*CSRCHK:                            /*                               */
*   PROC(IREQADDR);                 /* A SYSMOD CHAINED IREQ ENTRY   */
CSRCHK   STM   @14,@12,12(@13)                                     0197
         MVC   @PC00006(4),0(@01)                                  0197
*   DCL                                                            0198
*     IREQADDR PTR(31);             /* ADDRESS OF IREQ ENTRY         */
*   DCL                                                            0199
*     C FIXED(31);                  /* LOOP COUNTER                  */
*   DCL                                                            0200
*     CSRCODE FIXED(31);            /* SUBROUTINE RETURN CODE        */
*   RFY                                                            0201
*     ICTPTF BASED(CCAICPTF);       /* BASE OF ICT SYSMOD ENTRIES    */
*   RFY                                                            0202
*     ICTIRQLS BASED(IREQADDR);     /* BASE OR IREQ LIST             */
*   CSRCODE=ZERO;                   /* INIT CODE                     */
*                                                                  0203
         SLR   CSRCODE,CSRCODE                                     0203
*   /*****************************************************************/
*   /*                                                               */
*   /* ICT SYSMOD LOOP                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0204
*   DO C=1 BY 1 WHILE ICTPEND(C)^=ICTENDNT;/*                        */
*                                                                  0204
         LA    C,1                                                 0204
         B     @DE00204                                            0204
@DL00204 DS    0H                                                  0205
*     /***************************************************************/
*     /*                                                             */
*     /* LOOK FOR IREQ CAUSER IN THE ICT SYSMOD ENTRIES              */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0205
*     IF ICTCSRNM(1)=ICTPTFS(C) THEN/* IF CAUSER FOUND               */
*                                                                  0205
         L     @10,@PC00006                                        0205
         L     @10,IREQADDR(,@10)                                  0205
         LR    @15,C                                               0205
         MH    @15,@CH01568                                        0205
         L     @14,CCAICPTF(,CCAPTR)                               0205
         ST    @15,@TF00001                                        0205
         ALR   @15,@14                                             0205
         AL    @15,@CF01578                                        0205
         CLC   ICTCSRNM(7,@10),ICTPTFS(@15)                        0205
         BNE   @RF00205                                            0205
*       /*************************************************************/
*       /*                                                           */
*       /* SEE IF CAUSER IS STILL VALID                              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0206
*       DO;                         /*                               */
*         IF ICTPSUPN(C)^=ZERO      /* CAUSER SUPED                  */
*             ×ICTPNOGO(C)=ON THEN  /* OR NOGO                       */
         LR    @10,@14                                             0207
         AL    @10,@TF00001                                        0207
         AL    @10,@CF01579                                        0207
         SLR   @15,@15                                             0207
         ICM   @15,3,ICTPSUPN-20(@10)                              0207
         LTR   @15,@15                                             0207
         BNZ   @RT00207                                            0207
         AL    @14,@TF00001                                        0207
         AL    @14,@CF01580                                        0207
         TM    ICTPNOGO-8(@14),B'01000000'                         0207
         BNO   @RF00207                                            0207
@RT00207 DS    0H                                                  0208
*           CSRCODE=PTFWARN;        /* INDICATE TO CALLER            */
         LA    CSRCODE,4                                           0208
*       END;                        /*                               */
@RF00207 DS    0H                                                  0210
*   END;                            /* ICT SYSMOD LOOP               */
@RF00205 AH    C,@CH00061                                          0210
@DE00204 LR    @10,C                                               0210
         MH    @10,@CH01568                                        0210
         L     @01,CCAICPTF(,CCAPTR)                               0210
         AL    @01,@CF01578                                        0210
         L     @10,ICTPEND(@10,@01)                                0210
         CL    @10,ICTENDNT                                        0210
         BNE   @DL00204                                            0210
*   RETURN CODE(CSRCODE);           /*                               */
         LR    @15,CSRCODE                                         0211
         L     @14,12(,@13)                                        0211
         LM    @00,@12,20(@13)                                     0211
         BR    @14                                                 0211
*   END CSRCHK;                     /*                               */
*                                                                  0213
*   /*****************************************************************/
*   /*                                                               */
*   /* IF IN GROUP MODE THE PRE, REQ, MREQ, AND IREQ LISTS OF SYSMODS*/
*   /* SPECIFIED IN THE GROUP LIST ARE CHECKED AGAINST THE ICT SYSMOD*/
*   /* ENTRIES. IF THE ICT ENTRY FOR THE ASSOCIATED SYSMOD IS FOUND  */
*   /* IN THE ICT AND IS ELIGIBLE FOR PROCESSING, IT IS MARKED AS    */
*   /* RELAVENT. WHEN ALL SYSMODS IN THE SET HAVE BEEN PROCESSED     */
*   /* THERE MAY BE SOME ICT SYSMOD ENTRIES NOT MARKED AS RELAVENT.  */
*   /* EACH OF THESE NON-RELAVENT SYSMODS ARE MARKED AS ELIMATED TO  */
*   /* PREVENT FURTHER APPLY ACCEPT PROCESSING FOR THEM. THEY ARE    */
*   /* SYSMODS WHICH WERE PUT INTO THE ICT BECAUSE OF PRE REQ, MREQ, */
*   /* AND IREQ RELATIONSHIPS WITH THE SYSMODS IN THE SET OR SYSMODS */
*   /* SIMILARLY RELATED TO THEM.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0213
*GROUPCHK:                                                         0213
*   PROC;                           /*                               */
GROUPCHK STM   @14,@12,@SA00007                                    0213
*   RFY                                                            0214
*     ICTPTF BASED(CCAICPTF);       /* PTR TO SYSMOD SECTION         */
*   DCL                                                            0215
*     S FIXED(31);                  /* LOOP COUNTER                  */
*   ADDSW=ON;                       /* LOOP CONTROL SWITCH           */
*                                                                  0216
         OI    ADDSW,B'10000000'                                   0216
*   /*****************************************************************/
*   /*                                                               */
*   /* MAKE SUCCESSIVE CHECKS THRU ENTIRE ICT SYSMOD LIST UNTIL NO   */
*   /* ENTRIES HAVE BEEN MARKED AS VALID FOR THE GROUP (ICTPSLST)    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0217
*   DO WHILE ADDSW=ON;              /*                               */
         B     @DE00217                                            0217
@DL00217 DS    0H                                                  0218
*     ADDSW=OFF;                    /* WILL BE TURNED IF ADDITIONAL
*                                      SYSMODS ARE INDICATED AS IN 0218
*                                      GROUP                         */
*                                                                  0218
         NI    ADDSW,B'01111111'                                   0218
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK ENTIRE ICT LOOKING FOR THOSE IN GROUP LIST. THEN CHECK*/
*     /* EACH RELATED CHAINED SYSMOD(PRE, REQ, IREQ)                 */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0219
*     DO S=1 BY 1 WHILE ICTPEND(S)^=ICTENDNT;/*                      */
         LA    S,1                                                 0219
         B     @DE00219                                            0219
@DL00219 DS    0H                                                  0220
*       IF ICTPSLST(S)=ON           /* GROUP OR PUT IN BY THIS RTN   */
*           &ICTPNOGO(S)=OFF        /*                               */
*           &ICTPPASS(S)=OFF        /* NOT ALREADY CHECKED           */
*         THEN                      /*                               */
         LR    @10,S                                               0220
         MH    @10,@CH01568                                        0220
         L     @03,CCAICPTF(,CCAPTR)                               0220
         ST    @10,@TF00001                                        0220
         ALR   @10,@03                                             0220
         AL    @10,@CF01580                                        0220
         TM    ICTPSLST-8(@10),B'00000010'                         0220
         BNO   @RF00220                                            0220
         TM    ICTPNOGO-8(@10),B'01000000'                         0220
         BNZ   @RF00220                                            0220
         LR    @10,@03                                             0220
         AL    @10,@TF00001                                        0220
         AL    @10,@CF01583                                        0220
         TM    ICTPPASS-16(@10),B'10000000'                        0220
         BNZ   @RF00220                                            0220
*         DO;                       /*                               */
*           ICTPPASS(S)=ON;         /* PROCESSED GROUP TYPE          */
*                                                                  0222
         LR    @10,@03                                             0222
         AL    @10,@TF00001                                        0222
         AL    @10,@CF01583                                        0222
         OI    ICTPPASS-16(@10),B'10000000'                        0222
*           /*********************************************************/
*           /*                                                       */
*           /* CHECK ITS PRE LIST TO ADD SYSMODS AS GROUP TYPES      */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0223
*           CALL CHKCHN1(ICTPPCHN(S));/*                             */
*                                                                  0223
         L     @10,@TF00001                                        0223
         AL    @03,@CF01584                                        0223
         LA    @10,ICTPPCHN-76(@10,@03)                            0223
         ST    @10,@AL00001                                        0223
         LA    @01,@AL00001                                        0223
         BAL   @14,CHKCHN1                                         0223
*           /*********************************************************/
*           /*                                                       */
*           /* CHECK ITS REQ/MREQ LIST TO ADD SYSMODS AS GRUOP TYPES */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0224
*           CALL CHKCHN1(ICTPRCHN(S));/*                             */
*                                                                  0224
         LR    @10,S                                               0224
         MH    @10,@CH01568                                        0224
         L     @03,CCAICPTF(,CCAPTR)                               0224
         AL    @03,@CF01585                                        0224
         LA    @10,ICTPRCHN-68(@10,@03)                            0224
         ST    @10,@AL00001                                        0224
         LA    @01,@AL00001                                        0224
         BAL   @14,CHKCHN1                                         0224
*           /*********************************************************/
*           /*                                                       */
