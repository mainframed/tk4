         TITLE 'HMASMTPC - COMPLETE PTF ENTRY AND SAVE IF DATA'
HMASMTPC CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMTPC  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(20),0(@01)                                 0001
*                                                                  0191
*   /*****************************************************************/
*   /*                                                               */
*   /* HMASMTPC << MAINLINE                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0191
*   TPCRC=ZERO;                     /* CLEAR THE RETURN CODE         */
         SLR   TPCRC,TPCRC                                         0191
*   TPCFLAGS=''B;                   /* CLEAR LOCAL CONTROL FLAGS     */
         MVI   TPCFLAGS,X'00'                                      0192
*   RFY                                                            0193
*     HMASMIOP BASED(ADDR(MCBIOP)); /*                               */
*   SAVBUFAD=IOPBUFAD;              /* SAVE BUFFER ADDR TO BE      0194
*                                      RESTORED AT THE END           */
         L     @10,@PC00001+16                                     0194
         L     SAVBUFAD,IOPBUFAD(,@10)                             0194
*   RFY                                                            0195
*     HMASMIOP BASED(ADDR(SMDIOP)); /*                               */
*   SYSMOD8=' ';                    /* CLEAR SYSMOD NAME             */
         MVI   SYSMOD8+1,C' '                                      0196
         MVC   SYSMOD8+2(6),SYSMOD8+1                              0196
         MVI   SYSMOD8,C' '                                        0196
*   SYSMOD=IOPNAM17;                /* SAVE THE SYSMOD NAME          */
         L     @10,@PC00001+4                                      0197
         MVC   SYSMOD(7),IOPNAM17(@10)                             0197
*   HMASMCRP=''B;                   /* INITIALIZE THE CRP FOR      0198
*                                      SUBSEQUENT PARSING OF THE MCS
*                                      ENTRY                         */
         XC    HMASMCRP(106),HMASMCRP                              0198
*   CRPINDS=IOPPTS;                 /* CRP INPUT IS THE PTS          */
*                                                                  0199
         MVI   CRPINDS,X'06'                                       0199
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCATE MCS ENTRY ON THE SMPPTS                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0200
*   CALL LOCMCS;                    /*                               */
         BAL   @14,LOCMCS                                          0200
*   RFY                                                            0201
*     RTNCODE RSTD;                                                0201
*   TPCRC=MAX(RTNCODE,TPCRC);       /* SET RETURN CODE               */
         CR    TPCRC,RTNCODE                                       0202
         BNL   *+6
         LR    TPCRC,RTNCODE                                       0202
*   RFY                                                            0203
*     RTNCODE UNRSTD;                                              0203
*   IF TPCRC=ZERO THEN              /* NO ERROR SO FAR?              */
         LTR   TPCRC,TPCRC                                         0204
         BNZ   @RF00204                                            0204
*     DO;                           /* YES,                          */
*                                                                  0205
*       /*************************************************************/
*       /*                                                           */
*       /* SEARCH THE MCS PTS ENTRY TO FIND THE ++VER CORRESPONDING  */
*       /* TO THE ++VER INDEX SUPPLIED BY THE CALLER                 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0206
*       CALL VERSRH;                /*                               */
         BAL   @14,VERSRH                                          0206
*       RFY                                                        0207
*         RTNCODE RSTD;                                            0207
*       TPCRC=MAX(RTNCODE,TPCRC);   /* SET RETURN CODE               */
         CR    TPCRC,RTNCODE                                       0208
         BNL   *+6
         LR    TPCRC,RTNCODE                                       0208
*       RFY                                                        0209
*         RTNCODE UNRSTD;                                          0209
*     END;                                                         0210
*   IF TPCRC<STOPFUNC THEN          /* NO SERIOUS ERROR YET?         */
@RF00204 CH    TPCRC,@CH00273                                      0211
         BNL   @RF00211                                            0211
*     DO;                           /* YES,                          */
*                                                                  0212
*       /*************************************************************/
*       /*                                                           */
*       /* BUILD A PTF ENTRY IN THE ICT BASED ON THE DATA IN THE     */
*       /* SYSMOD SMPPTS IOP FOR 'SELECTED' ++VER INDEX              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0213
*       PTFPTR=ENTRYPTR;            /* ASSUME ENTRY POINTER SUPPLIED */
         L     @10,@PC00001+12                                     0213
         L     @10,ENTRYPTR(,@10)                                  0213
         ST    @10,PTFPTR                                          0213
*       IF PTFPTR=ZERO THEN         /* NO ENTRY POINTER PASSED?      */
         LTR   @10,@10                                             0214
         BNZ   @RF00214                                            0214
*         DO;                       /* YES,                          */
*           PTFPTR=TBLXTOP;         /* ENTRY WILL BE AT CURRENT ICT
*                                      TOP                           */
         L     @10,@PC00001                                        0216
         MVC   PTFPTR(4),TBLXTOP(@10)                              0216
*         END;                      /*                               */
*       ELSE                                                       0218
*         ;                         /*                               */
@RF00214 DS    0H                                                  0219
*       CALL BLDSMD;                /*                               */
         BAL   @14,BLDSMD                                          0219
*       RFY                                                        0220
*         RTNCODE RSTD;                                            0220
*       TPCRC=MAX(RTNCODE,TPCRC);   /* SET RETURN CODE               */
         CR    TPCRC,RTNCODE                                       0221
         BNL   *+6
         LR    TPCRC,RTNCODE                                       0221
*       IF RTNCODE<STOPFUNC THEN    /* WAS A SYSMOD ENTRY COMPLETED? */
         CH    RTNCODE,@CH00273                                    0222
         BNL   @RF00222                                            0222
*         DO;                       /* YES,                          */
*           RFY                                                    0224
*             RTNCODE UNRSTD;                                      0224
*           ENTBUILT=ON;            /* ENTRY BUILT FLAG              */
         OI    ENTBUILT,B'10000000'                                0225
*         END;                      /*                               */
*       ELSE                                                       0227
*         ;                         /* SEVERE ERROR                  */
@RF00222 DS    0H                                                  0228
*     END;                                                         0228
*   IF TPCRC=ZERO THEN              /* NO ERROR SO FAR?              */
@RF00211 LTR   TPCRC,TPCRC                                         0229
         BNZ   @RF00229                                            0229
*     DO;                           /* YES,                          */
*                                                                  0230
*       /*************************************************************/
*       /*                                                           */
*       /* SAVE THE ++IF DATA FOLLOWING THE 'SELECTED' ++VER IN THE  */
*       /* SMPPTS MCS ENTRY AS RECORDS IN THE TBLXIFGT GTA FILE      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0231
*       CALL SAVEIF;                /*                               */
         BAL   @14,SAVEIF                                          0231
*       RFY                                                        0232
*         RTNCODE RSTD;                                            0232
*       TPCRC=MAX(RTNCODE,TPCRC);   /* SET RETURN CODE               */
         CR    TPCRC,RTNCODE                                       0233
         BNL   *+6
         LR    TPCRC,RTNCODE                                       0233
*       RFY                                                        0234
*         RTNCODE UNRSTD;                                          0234
*     END;                                                         0235
*   IF TPCRC=STOPPTF                /* SYSMOD FAILED ...             */
*       &ENTBUILT=ON THEN           /* AND ENTRY BUILT ?             */
@RF00229 CH    TPCRC,@CH00061                                      0236
         BNE   @RF00236                                            0236
         TM    ENTBUILT,B'10000000'                                0236
         BNO   @RF00236                                            0236
*     DO;                           /* YES,                          */
*       PTFPTR->ICTPNOGO(1)=ON;     /* MARK SYSMOD 'NOGO'            */
         L     @10,PTFPTR                                          0238
         OI    ICTPNOGO(@10),B'01000000'                           0238
*     END;                          /*                               */
*   ELSE                                                           0240
*     ;                             /* SYSMOD NOT FAILED OR ENTRY NOT
*                                      BUILT                         */
@RF00236 DS    0H                                                  0241
*   RFY                                                            0241
*     HMASMIOP BASED(ADDR(MCBIOP)); /*                               */
*   IOPBUFAD=SAVBUFAD;              /* RESTORE BUFFER ADDR TO IOP  0242
*                                      USED AS MCB                   */
         L     @10,@PC00001+16                                     0242
         ST    SAVBUFAD,IOPBUFAD(,@10)                             0242
*   RFY                                                            0243
*     HMASMIOP BASED(ADDR(SMDIOP)); /*                               */
*   RETURN CODE(TPCRC);             /*                               */
         LR    @15,TPCRC                                           0244
         L     @13,4(,@13)                                         0244
         L     @14,12(,@13)                                        0244
         LM    @00,@12,20(@13)                                     0244
         BR    @14                                                 0244
*   RFY                                                            0245
*     HMASMIOP BASED;                                              0245
*                                                                  0246
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCMCS - ROUTINE TO LOCATE MCS ENTRY ON THE SMPPTS            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0246
*LOCMCS:                                                           0246
*   PROC;                           /*                               */
LOCMCS   STM   @14,@12,@SA00002                                    0246
*   DCL                                                            0247
*     LOCMCSRC FIXED(15);           /* LOCMCS RETURN CODE            */
*   RFY                                                            0248
*     HMASMIOP BASED(ADDR(TPCIOPF));                               0248
*   LOCMCSRC=ZERO;                  /* CLEAR RETURN CODE             */
         SLR   LOCMCSRC,LOCMCSRC                                   0249
*   IOPDSID=IOPPTS;                 /* LOCATE PTS MCS ENTRY FOR ...  */
         LA    @10,TPCIOPF                                         0250
         MVI   IOPDSID(@10),X'06'                                  0250
*   IOPNAME=SYSMOD;                 /* ... SYSMOD BEING PROCESSED    */
         MVI   IOPNAME+7(@10),C' '                                 0251
         MVC   IOPNAME(7,@10),SYSMOD                               0251
*   IOPFUNCT=IOPBLDL;               /* ...                           */
         MVI   IOPFUNCT(@10),X'04'                                 0252
*   IOPTYPE=IOPMCS;                 /* ...                           */
         MVI   IOPTYPE(@10),X'10'                                  0253
*   CALL HMASMIO(HMASMIOP);         /* ...                           */
         ST    @10,@AL00001                                        0254
         L     @15,@CV01718                                        0254
         LA    @01,@AL00001                                        0254
         BALR  @14,@15                                             0254
*   IF IOPRETRN^=IOPGOOD THEN       /* WAS THERE AN ERROR            */
         LA    @10,TPCIOPF                                         0255
         CLI   IOPRETRN(@10),0                                     0255
         BE    @RF00255                                            0255
*     DO;                           /* YES,                          */
*       LOCMCSRC=STOPPTF;           /* STOP THE SYSMOD               */
*                                                                  0257
         LA    LOCMCSRC,8                                          0257
*       /*************************************************************/
*       /*                                                           */
*       /* ISSUE MESSAGE HMA246I XXXXXXXX XXXXXXXX NOT FOUND ON      */
*       /* XXXXXXXX                                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0258
*       DO;                         /* HMASMPMG M1(HMA246I)VAR(TXTMCS
*                                      ,SYSMOD,TXTPTS)TYPE(ERR)TARGET
*                                      (SYSOUT,LOG)                  */
*         HMASMMGP=HMASMMGP&&HMASMMGP;/* SET PARMS TO ZEROS          */
         XC    HMASMMGP(24),HMASMMGP                               0259
*         MGPMGNO1=HMA246I;         /* SET UP MSG 1 CODE             */
         MVC   MGPMGNO1(2),@CH01727                                0260
*         MGPTYPE=MGPERR;           /* SET MESSAGE TYPE              */
         MVI   MGPTYPE,X'03'                                       0261
*         MGPPRINT='1'B;            /* SET TARGET FLAG               */
*         MGPHLDS='1'B;             /* SET TARGET FLAG               */
         OI    MGPPRINT,B'10010000'                                0263
*         MGPVARPT(1)=ADDR(TXTMCS); /* SET VARIABLE PTR              */
         LA    @10,TXTMCS                                          0264
         ST    @10,MGPVARPT                                        0264
*         MGPVARPT(2)=ADDR(SYSMOD); /* SET VARIABLE PTR              */
         LA    @10,SYSMOD                                          0265
         ST    @10,MGPVARPT+4                                      0265
*         MGPVARPT(3)=ADDR(TXTPTS); /* SET VARIABLE PTR              */
         LA    @10,TXTPTS                                          0266
         ST    @10,MGPVARPT+8                                      0266
*         CALL HMASMMSG(HMASMMGP);  /* HAVE MESSAGE ISSUED           */
         L     @15,@CV01719                                        0267
         LA    @01,@AL00267                                        0267
         BALR  @14,@15                                             0267
*       END;                                                       0268
*     END;                          /*                               */
*   ELSE                                                           0270
*     ;                             /* IO WAS OK                     */
@RF00255 DS    0H                                                  0271
*   RETURN CODE(LOCMCSRC);          /* RETURN WITH RETURN CODE       */
         LR    @15,LOCMCSRC                                        0271
         L     @14,@SA00002                                        0271
         LM    @00,@12,@SA00002+8                                  0271
         BR    @14                                                 0271
*   RFY                                                            0272
*     HMASMIOP BASED;                                              0272
*   END LOCMCS;                     /*                               */
*                                                                  0274
*   /*****************************************************************/
*   /*                                                               */
*   /* VERSRH - SEARCH THRU MCS ENTRY FOR SPECIFIED ++VER            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0274
*VERSRH:                                                           0274
*   PROC;                           /*                               */
VERSRH   STM   @14,@12,@SA00003                                    0274
*   DCL                                                            0275
*     VERSRHRC FIXED(15);           /* VERSRH RETURN CODE            */
*   DCL                                                            0276
*     1 VERFLAGS BIT(8),            /* LOCAL CONTROL FLAGS           */
*      2 RIGHTVER BIT(1),           /* ++VER CORRESPONDING TO INPUT
*                                      VER INDEX FOUND               */
*      2 * BIT(7);                  /* UNUSED                        */
*   DCL                                                            0277
*     VERCNT FIXED(8);              /* COUNT OF ++VER'S FOUND SO FAR
*                                      IN MCS ENTRY                  */
*   RFY                                                            0278
*     HMASMIOP BASED(ADDR(TPCIOPF));                               0278
*   RFY                                                            0279
*     MCBVERMP BASED(ADDR(MCBIOP)); /* USE INPUT IOP FOR MCB AREA    */
*   VERSRHRC=ZERO;                  /* CLEAR THE RETURN CODE         */
*                                                                  0280
         SLR   VERSRHRC,VERSRHRC                                   0280
*   /*****************************************************************/
*   /*                                                               */
*   /* POINT TO PTS MCS MEMBER BASED ON LOCATE DATA OBTAINED ABOVE   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0281
*   IOPFUNCT=IOPPOSI;               /* ...                           */
         LA    @10,TPCIOPF                                         0281
         MVI   IOPFUNCT(@10),X'02'                                 0281
*   CALL HMASMIO(HMASMIOP);         /* ...                           */
         ST    @10,@AL00001                                        0282
         L     @15,@CV01718                                        0282
         LA    @01,@AL00001                                        0282
         BALR  @14,@15                                             0282
*   IF IOPRETRN^=IOPGOOD THEN       /* ERROR ON THE POINT?           */
         LA    @10,TPCIOPF                                         0283
         CLI   IOPRETRN(@10),0                                     0283
         BE    @RF00283                                            0283
*     DO;                           /* YES,                          */
*       VERSRHRC=STOPFUNC;          /* STOP THE SMP FUNCTION         */
         LA    VERSRHRC,12                                         0285
*     END;                                                         0286
*   ELSE                            /* NO ERROR ON THE POINT         */
*     DO;                                                          0287
*                                                                  0287
         B     @RC00283                                            0287
@RF00283 DS    0H                                                  0288
*       /*************************************************************/
*       /*                                                           */
*       /* PARSE MCS RECORDS UNTIL THE 'SELECTED' ++VER IS FOUND     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0288
*       RIGHTVER=OFF;               /* INITIALIZE TO VER NOT YET   0288
*                                      FOUND                         */
         NI    RIGHTVER,B'01111111'                                0288
*       VERCNT=ZERO;                /* CLEAR NUMBER OF ++VER       0289
*                                      STATEMENTS SEEN               */
         SLR   VERCNT,VERCNT                                       0289
*       DO WHILE(VERSRHRC=ZERO      /* PARSE UNTIL ERROR             */
*             &RIGHTVER=OFF);       /* OR CORRECT ++VER IS FOUND     */
*                                                                  0290
         B     @DE00290                                            0290
@DL00290 DS    0H                                                  0291
*         /***********************************************************/
*         /*                                                         */
*         /* PARSE MODIFICATION CONTROL STATEMENTS BY CALLING        */
*         /* PARSERTN                                                */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0291
*         CALL PARSERTN;                                           0291
         BAL   @14,PARSERTN                                        0291
*         RFY                                                      0292
*           RTNCODE RSTD;                                          0292
*         VERSRHRC=MAX(VERSRHRC,RTNCODE);/* SAVE RETURN CODE         */
         CR    VERSRHRC,RTNCODE                                    0293
         BNL   *+6
         LR    VERSRHRC,RTNCODE                                    0293
*         RFY                                                      0294
*           RTNCODE UNRSTD;                                        0294
*         IF VERSRHRC=ZERO THEN     /* DID PARSE WORK?               */
         LTR   VERSRHRC,VERSRHRC                                   0295
         BNZ   @RF00295                                            0295
*           DO;                                                    0296
*                                                                  0296
*             /*******************************************************/
*             /*                                                     */
*             /* CHECK FOR 'SELECTED' ++VER, IF FOUND SAVE THE FMID  */
*             /* NOTE THAT THIS ALSO POSITIONS FOR PROCESSING ANY    */
*             /* ++IF DATA                                           */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0297
*             IF ADDR(MCBVERMP)->MCBTYPE=MCBVERTP THEN/* IS IT A   0297
*                                      ++VER CARD?                   */
         L     @10,@PC00001+16                                     0297
         CLI   MCBTYPE(@10),2                                      0297
         BNE   @RF00297                                            0297
*               DO;                 /* YES,                          */
*                 VERCNT=VERCNT+1;  /* INCREAMENT ++VER COUNTER      */
         LA    VERCNT,1(,VERCNT)                                   0299
*                 IF VERCNT=VERNDX THEN/* IS THIS THE CORRECT ++VER? */
         L     @05,@PC00001+8                                      0300
         SLR   @02,@02                                             0300
         IC    @02,VERNDX(,@05)                                    0300
         CR    VERCNT,@02                                          0300
         BNE   @RF00300                                            0300
*                   DO;             /* YES,                          */
*                     RIGHTVER=ON;  /* STOP LOOKING                  */
         OI    RIGHTVER,B'10000000'                                0302
*                     FMID=MCBSFMID;/* SAVE THE FMID, IF ANY, FROM 0303
*                                      ++VER                         */
         MVC   FMID(7),MCBSFMID(@10)                               0303
*                   END;                                           0304
*                 ELSE                                             0305
*                   ;               /* A ++VER BUT NOT THE CORRECT 0305
*                                      ONE                           */
@RF00300 DS    0H                                                  0306
*               END;                                               0306
*             ELSE                                                 0307
*               ;                   /* NOT A ++VER, CONTINUE SEARCH  */
@RF00297 DS    0H                                                  0308
*           END;                                                   0308
*       END;                        /* END OF ++VER PARSE LOOP       */
@RF00295 DS    0H                                                  0309
@DE00290 LTR   VERSRHRC,VERSRHRC                                   0309
         BNZ   @DC00290                                            0309
         TM    RIGHTVER,B'10000000'                                0309
         BZ    @DL00290                                            0309
@DC00290 DS    0H                                                  0310
*       IF VERSRHRC=STOPPTF THEN    /* SYSMOD FAILED?                */
         CH    VERSRHRC,@CH00061                                   0310
         BNE   @RF00310                                            0310
*         DO;                       /* YES,                          */
*                                                                  0311
*           /*********************************************************/
*           /*                                                       */
*           /* ISSUE A MESSAGE HMA355I 'INVALID XXXXXXXX ENTRY FOR   */
*           /* SYSMOD XXXXXXXX ON THE XXXXXXXX'                      */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0312
*           DO;                     /* HMASMPMG M1(HMA355I)TYPE(ERR)V
*                                      AR(TXTMCS,SYSMOD,TXTPTS)TARGET
*                                      (SYSOUT,LOG)                  */
*             HMASMMGP=HMASMMGP&&HMASMMGP;/* SET PARMS TO ZEROS      */
         XC    HMASMMGP(24),HMASMMGP                               0313
*             MGPMGNO1=HMA355I;     /* SET UP MSG 1 CODE             */
         MVC   MGPMGNO1(2),@CH01729                                0314
*             MGPTYPE=MGPERR;       /* SET MESSAGE TYPE              */
         MVI   MGPTYPE,X'03'                                       0315
*             MGPPRINT='1'B;        /* SET TARGET FLAG               */
*             MGPHLDS='1'B;         /* SET TARGET FLAG               */
         OI    MGPPRINT,B'10010000'                                0317
*             MGPVARPT(1)=ADDR(TXTMCS);/* SET VARIABLE PTR           */
         LA    @10,TXTMCS                                          0318
         ST    @10,MGPVARPT                                        0318
*             MGPVARPT(2)=ADDR(SYSMOD);/* SET VARIABLE PTR           */
         LA    @10,SYSMOD                                          0319
         ST    @10,MGPVARPT+4                                      0319
*             MGPVARPT(3)=ADDR(TXTPTS);/* SET VARIABLE PTR           */
         LA    @10,TXTPTS                                          0320
         ST    @10,MGPVARPT+8                                      0320
*             CALL HMASMMSG(HMASMMGP);/* HAVE MESSAGE ISSUED         */
         L     @15,@CV01719                                        0321
         LA    @01,@AL00321                                        0321
         BALR  @14,@15                                             0321
*           END;                    /* ROUTING                       */
*         END;                                                     0323
*       ELSE                                                       0324
*         ;                         /* SYSMOD DID NOT FAIL           */
@RF00310 DS    0H                                                  0325
*     END;                                                         0325
*   RETURN CODE(VERSRHRC);          /* RETURN WITH RETURN CODE       */
@RC00283 LR    @15,VERSRHRC                                        0326
         L     @14,@SA00003                                        0326
         LM    @00,@12,@SA00003+8                                  0326
         BR    @14                                                 0326
*   RFY                                                            0327
*     HMASMIOP BASED;                                              0327
*   RFY                                                            0328
*     MCBVERMP BASED;                                              0328
*   END VERSRH;                     /*                               */
*                                                                  0330
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINE TO BUILD A ICT PTF ENTRY FOR THE CURRENT        SYSMOD*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0330
*BLDSMD:                                                           0330
*   PROC;                           /*                               */
BLDSMD   STM   @14,@12,@SA00004                                    0330
*   DCL                                                            0331
*     BLDPTR PTR(31);               /* PTR TO THE SYSMOD ENTRY BEING
*                                      BUILT                         */
*   DCL                                                            0332
*     BLDPTF CHAR(1) CONSTANT('P'); /* TELL HMASMTAD TO BUILD A    0332
*                                      SYSMOD ENTRY IN THE ICT       */
*   DCL                                                            0333
*     BLDSMDRC FIXED(15);           /* BLDSMD RETURN CODE            */
*   RFY                                                            0334
*     HMASMIOP BASED(ADDR(SMDIOP));                                0334
*   BLDSMDRC=ZERO;                  /* CLEAR THE RETURN CODE         */
*                                                                  0335
         SLR   BLDSMDRC,BLDSMDRC                                   0335
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE ICT PTF ENTRY IF NONE WAS SUPPLIED BY CALLER       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0336
*   BLDPTR=ENTRYPTR;                /* USE ENTRY PASSED IN PARM LIST */
         L     @10,@PC00001+12                                     0336
         L     BLDPTR,ENTRYPTR(,@10)                               0336
*   IF BLDPTR=ZERO THEN             /* WAS AN ENTRY PTR SUPPLIED?    */
         LTR   BLDPTR,BLDPTR                                       0337
         BNZ   @RF00337                                            0337
*     DO;                           /* NO,                           */
*       BLDPTR=TBLXTOP;             /* SET ENTRY POINTER             */
         L     @10,@PC00001                                        0339
         L     BLDPTR,TBLXTOP(,@10)                                0339
*       CALL HMASMTAD(TBLXPARM,     /* BUILD PTF ENTRY               */
*           BLDPTF,                 /* PTF TYPE                      */
*           SYSMOD);                /* NAME OF SYSMOD FOR ENTRY      */
         MVC   @AL00001+4(8),@AL00340                              0340
         ST    @10,@AL00001                                        0340
         L     @15,@CV01723                                        0340
         LA    @01,@AL00001                                        0340
         BALR  @14,@15                                             0340
*       RFY                                                        0341
*         RTNCODE RSTD;                                            0341
*       BLDSMDRC=MAX(BLDSMDRC,RTNCODE);/* SET RETURN CODE            */
         CR    BLDSMDRC,RTNCODE                                    0342
         BNL   *+6
         LR    BLDSMDRC,RTNCODE                                    0342
