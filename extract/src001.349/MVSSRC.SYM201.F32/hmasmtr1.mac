*/********************************************************************/
*/*                                                                  */
*/*    CKICTSUP - CHECK ICT TO DETERMINE WHETHER THE CURRENT         */
*/*               REQUISITE IS SATISFIED BY A SUPERSEDING SYSMOD.    */
*/*                                                                  */
*/*                                                                  */
*/*        NOTE - THE REQUISITE SATISFIED FLAG, ICTEREQS, IS "OFF"   */
*/*               ON ENTRY TO THIS PROCEDURE.                        */
*/*                                                                  */
*/*             - THE REQUISITE SATISFIED FLAG, ICTEREQS, WILL BE    */
*/*               SET ON IF THE REQUISITE CAN BE FOUND (AND CHECKED) */
*/*               IN THE ICT.                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0147
*CKICTSUP:                                                         0147
*   PROC(LEPTR2,REQTYPE2);                                         0147
CKICTSUP STM   @14,@12,@SA00004                                    0147
         MVC   @PC00004(8),0(@01)                                  0147
*   DCL                                                            0148
*     LEPTR2 PTR(31);               /* PTR TO REQ LIST ENTRY FOR   0148
*                                      REQUISITE TO BE CHECKED       */
*   DCL                                                            0149
*     REQTYPE2 FIXED(8);            /* REQUISITE TYPE (PRE REQ IRQ)  */
*   DCL                                                            0150
*     SPTR PTR(31);                 /* PTR TO ICT PTF SECTION ENTRY
*                                      FOR SUPERSEDER OF REQ (IF   0150
*                                      FOUND IN ICT)                 */
*   RESPECIFY                                                      0151
*     ICTENTLS BASED(LEPTR2);       /* ADDRESS REQ LIST ENTRY PASSED */
*   RESPECIFY                                                      0152
*     ICTPTF BASED(PTFPTR);         /* ADDRESS PTF PASSED TO TR1     */
*   SPTR=0;                         /* INITIALIZE PTR SO SUPICT PROC
*                                      STARTS LOOKING FOR SUPS AT THE
*                                      BEGINNING OF THE ICT.         */
         SLR   @15,@15                                             0153
         ST    @15,SPTR                                            0153
*   CALL SUPICT(ICTENTNM(1),SPTR);  /* CHECK ICT FOR SUPING PTF      */
         L     @15,@PC00004                                        0154
         L     @01,LEPTR2(,@15)                                    0154
         ST    @01,@AL00001                                        0154
         LA    @15,SPTR                                            0154
         ST    @15,@AL00001+4                                      0154
         LA    @01,@AL00001                                        0154
         BAL   @14,SUPICT                                          0154
*   DO WHILE ICTEREQS(1)=OFF        /* REQ NOT SATISFIED             */
*         &SPTR^=0;                 /* SUPERSEDER FOUND IN ICT       */
*                                                                  0155
         B     @DE00155                                            0155
@DL00155 DS    0H                                                  0156
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK A SYSMOD (FOUND IN THE ICT) WHICH SUPERSEDES THE      */
*     /* REQUISITE                                                   */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0156
*     IF REQTYPE2=IRQ               /* IFREQ REQUISITE               */
*         &ICTENTNM(2)=ICTPTFS(1)   /* CAUSED BY "SELF"              */
*         &SPTR=PTFPTR              /* SUPERSEDING SYSMOD IS "SELF"  */
*       THEN                                                       0156
         L     @15,@PC00004+4                                      0156
         CLI   REQTYPE2(@15),3                                     0156
         BNE   @RF00156                                            0156
         L     @15,@PC00004                                        0156
         L     @01,LEPTR2(,@15)                                    0156
         L     @15,@PC00001+4                                      0156
         L     @15,PTFPTR(,@15)                                    0156
         CLC   ICTENTNM+8(7,@01),ICTPTFS(@15)                      0156
         BNE   @RF00156                                            0156
         C     @15,SPTR                                            0156
         BE    @RT00156                                            0156
*       ;                           /* DO NOT USE "SELF"             */
*     ELSE                                                         0158
*       DO;                                                        0158
@RF00156 DS    0H                                                  0159
*         IF SPTR->ICTPRQCK(1)=ON   /* SUPERSEDING SYSMOD ALREADY  0159
*                                      CHECKED                       */
*           THEN                                                   0159
         L     @15,SPTR                                            0159
         TM    ICTPRQCK(@15),B'00001000'                           0159
         BNO   @RF00159                                            0159
*           DO;                                                    0160
*             IF SPTR->ICTPRQOK(1)=ON/* CHECKED "OK"                 */
*               THEN                                               0161
         TM    ICTPRQOK(@15),B'00000100'                           0161
         BNO   @RF00161                                            0161
*               ICTEREQS(1)=ON;     /* SET REQ SATISFIED FLAG        */
         L     @15,@PC00004                                        0162
         L     @01,LEPTR2(,@15)                                    0162
         OI    ICTEREQS(@01),B'00010000'                           0162
*           END;                                                   0163
*         ELSE                                                     0164
*           DO;                                                    0164
*                                                                  0164
         B     @RC00159                                            0164
@RF00159 DS    0H                                                  0165
*             /*******************************************************/
*             /*                                                     */
*             /* CHECK THE SUPERSEDING SYSMOD FOUND IN ICT           */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0165
*             IF SPTR->ICTPTR1(1)=ON/* CURRENT REQ CHAIN             */
*                 ×SPTR=PTFPTR      /* SUPERSEDING SYSMOD IS "SELF"  */
*               THEN                                               0165
         L     @15,SPTR                                            0165
         TM    ICTPTR1(@15),B'01000000'                            0165
         BO    @RT00165                                            0165
         L     @14,@PC00001+4                                      0165
         C     @15,PTFPTR(,@14)                                    0165
         BNE   @RF00165                                            0165
@RT00165 DS    0H                                                  0166
*               ICTEREQS(1)=ON;     /* REQ SATISFIED                 */
         L     @15,@PC00004                                        0166
         L     @01,LEPTR2(,@15)                                    0166
         OI    ICTEREQS(@01),B'00010000'                           0166
*             ELSE                                                 0167
*               DO;                                                0167
         B     @RC00165                                            0167
@RF00165 DS    0H                                                  0168
*                 CALL HMASMTR1(TBLXPARM,/* CHECK REQS               */
*                     SPTR,         /* SUPING SYSMOD                 */
*                     TEMPRTN);                                    0168
         L     @15,@PC00001                                        0168
         ST    @15,@AL00001                                        0168
         LA    @15,SPTR                                            0168
         ST    @15,@AL00001+4                                      0168
         LA    @15,TEMPRTN                                         0168
         ST    @15,@AL00001+8                                      0168
         LA    @01,@AL00001                                        0168
         L     @15,@CA00028                                        0168
         BALR  @14,@15                                             0168
*                 IF TEMPRTN<=BYPASSED/* REQS "OK"                   */
*                   THEN                                           0169
         CLI   TEMPRTN,4                                           0169
         BH    @RF00169                                            0169
*                   ICTEREQS(1)=ON; /* REQ SATISFIED                 */
         L     @15,@PC00004                                        0170
         L     @01,LEPTR2(,@15)                                    0170
         OI    ICTEREQS(@01),B'00010000'                           0170
*               END;                                               0171
@RF00169 DS    0H                                                  0172
*           END;                                                   0172
@RC00165 DS    0H                                                  0173
*       END;                                                       0173
@RC00159 DS    0H                                                  0174
*     IF ICTEREQS(1)=OFF THEN                                      0174
*                                                                  0174
@RT00156 L     @15,@PC00004                                        0174
         L     @15,LEPTR2(,@15)                                    0174
         TM    ICTEREQS(@15),B'00010000'                           0174
         BNZ   @RF00174                                            0174
*       /*************************************************************/
*       /*                                                           */
*       /* THE SUPERSEDER DID NOT CHECK SUCCESSFULLY. LOOK FOR       */
*       /* ANOTHER SUPERSEDER IN THE ICT.                            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0175
*       CALL SUPICT(ICTENTNM(1),SPTR);/* GET ANOTHER SUP             */
         ST    @15,@AL00001                                        0175
         LA    @15,SPTR                                            0175
         ST    @15,@AL00001+4                                      0175
         LA    @01,@AL00001                                        0175
         BAL   @14,SUPICT                                          0175
*   END;                            /* END OF LOOP FOR ALL         0176
*                                      SUPERSEDING SYSMODS IN ICT    */
@RF00174 DS    0H                                                  0176
@DE00155 L     @15,@PC00004                                        0176
         L     @01,LEPTR2(,@15)                                    0176
         TM    ICTEREQS(@01),B'00010000'                           0176
         BNZ   @DC00155                                            0176
         L     @15,SPTR                                            0176
         LTR   @15,@15                                             0176
         BNZ   @DL00155                                            0176
@DC00155 DS    0H                                                  0177
*   RETURN;                                                        0177
@EL00004 DS    0H                                                  0177
@EF00004 DS    0H                                                  0177
@ER00004 LM    @14,@12,@SA00004                                    0177
         BR    @14                                                 0177
*   END CKICTSUP;                                                  0178
