*/********************************************************************/
*/*                                                                  */
*/*    SUPICT  -  CHECK THE ICT FOR A NON-NOGO, NON-DELETED, NON-    */
*/*               SUPERSEDED SYSMOD WHICH SUPERSEDES THE SYSMOD      */
*/*               PASSED BY THE CALLER.                              */
*/*                                                                  */
*/*            -  THIS PROCEDURE WILL START IST SEARCH THRU THE ICT  */
*/*               PTF SECTION FROM THE PTF SECTION ENTRY AFTER THE   */
*/*               ENTRY POINTED TO BY THE PARAMETER, "SUPPTR".       */
*/*                                                                  */
*/*               IF THE PARAMETER, "SUPPTR", IS ZERO UPON ENTRY,    */
*/*               THE SEARCH WILL START FROM THE BEGINNING OF THE    */
*/*               ICT PTF SECTION.                                   */
*/*                                                                  */
*/*            -  ON EXIT, "SUPPTR" WILL POINT TO THE NEXT SYSMOD    */
*/*               FOUND IN THE ICT WHICH SUPERSEDES THE SYSMOD       */
*/*               PASSED.  IF "SUPPTR" IS ZERO ON EXIT, NO OTHER     */
*/*               SYSMOD WAS FOUND IN THE ICT WHICH SUPERSEDED THE   */
*/*               SYSMOD PASSED.                                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0192
*SUPICT:                                                           0192
*   PROC(SUPNAME,SUPPTR);                                          0192
SUPICT   STM   @14,@12,12(@13)                                     0192
         MVC   @PC00006(8),0(@01)                                  0192
*   DCL                                                            0193
*     SUPNAME CHAR(7);              /* SYSMOD TO BE FOUND AS       0193
*                                      SUPERSEDED IN ICT             */
*   DCL                                                            0194
*     SUPPTR PTR(31);               /* UPON ENTRY THIS PARAMETER IS
*                                      EITHER 0 OR POINTS TO AN ICT
*                                      PTF SECTION ENTRY. .........
*                                      UPON EXIT THIS PARAMETER IS 0194
*                                      SET TO EITHER 0 OR POINTS TO
*                                      THE ICT PTF SECTION ENTRY FOR
*                                      A SYSMOD WHICH SUPERSEDES   0194
*                                      "SUPNAME".                    */
*   DCL                                                            0195
*     PPTR2 PTR(31);                /* ADDR PTF SECTION ENTRY        */
*   DCL                                                            0196
*     L FIXED(31);                  /* INDEX - SUP LIST ENTRIES      */
*   RESPECIFY                                                      0197
*     ICTPTF BASED(SUPPTR);         /* ADDRESS PTF SECT ENTRY PASSED */
*   IF SUPPTR=0 THEN                                               0198
         L     @15,@PC00006+4                                      0198
         L     @15,SUPPTR(,@15)                                    0198
         LTR   @15,@15                                             0198
         BNZ   @RF00198                                            0198
*     PPTR2=CCAICPTF;               /* START WITH THE FIRST PTF    0199
*                                      SECTION ENTRY.                */
         L     PPTR2,CCAICPTF(,CCAPTR)                             0199
*   ELSE                                                           0200
*     PPTR2=ADDR(ICTPTF(2));        /* START WITH THE NEXT PTF     0200
*                                      SECTION ENTRY.                */
         B     @RC00198                                            0200
@RF00198 L     @15,@PC00006+4                                      0200
         L     PPTR2,SUPPTR(,@15)                                  0200
         LA    PPTR2,ICTPTF+116(,PPTR2)                            0200
*   SUPPTR=0;                       /* CLEAR PTR TO SUPERSEDER       */
@RC00198 L     @03,@PC00006+4                                      0201
         SLR   @15,@15                                             0201
         ST    @15,SUPPTR(,@03)                                    0201
*   RESPECIFY                                                      0202
*     ICTPTF BASED(PPTR2);          /* ADDRESS PTF SECT ENTRY        */
*   RESPECIFY                                                      0203
*     ICTSUPLS BASED(ICTPSCHN(1));  /* ADDRESS PTF ENTRY SUP LIST    */
*                                                                  0203
*   /*****************************************************************/
*   /*                                                               */
*   /* GO THRU ALL REMAINING PTF SECTION ENTRIES                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0204
*   DO PPTR2=PPTR2 BY LENGTH(ICTPTF)/* GO THRU PTF SECTION           */
*         WHILE ICTPEND(1)^=ICTENDNT/* NOT AT END                    */
*         &SUPPTR=0;                /* SUPERSEDING SYSMOD NOT FOUND  */
         B     @DE00204                                            0204
@DL00204 DS    0H                                                  0205
*     IF ICTPNOGO(1)=OFF            /* NOT NOGO                      */
*         &ICTPDEXP(1)=OFF          /* NOT EXPLICITLY DELETED        */
*         &ICTPDIMP(1)=OFF          /* NOT IMPLICITLY DELETED        */
*         &ICTPSUPN(1)=0            /* NOT SUPERSEDED                */
*         &ICTPSCHN(1)^=0           /* THERE IS A LIST OF SUPS       */
*       THEN                                                       0205
         TM    ICTPNOGO(PPTR2),B'01000000'                         0205
         BNZ   @RF00205                                            0205
         TM    ICTPDEXP(PPTR2),B'00110000'                         0205
         BNZ   @RF00205                                            0205
         MVC   @ZT00002+2(2),ICTPSUPN(PPTR2)                       0205
         L     @03,@ZT00002                                        0205
         LTR   @03,@03                                             0205
         BNZ   @RF00205                                            0205
         MVC   @TF00001(4),ICTPSCHN(PPTR2)                         0205
         L     @03,@TF00001                                        0205
         LTR   @03,@03                                             0205
         BZ    @RF00205                                            0205
*       DO;                                                        0206
*                                                                  0206
*         /***********************************************************/
*         /*                                                         */
*         /* GO THRU THE LIST OF SUPERSEDED SYSMODS IN THE PTF       */
*         /* SECTION ENTRY                                           */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0207
*         DO L=1 BY 1 WHILE ICTSUPND(L)^=ICTENDIX/* NOT AT END       */
*               &SUPPTR=0;          /* SUPERSEDING SYSMOD NOT FND    */
         LA    L,1                                                 0207
         B     @DE00207                                            0207
@DL00207 DS    0H                                                  0208
*           IF ICTSUPNM(L)=SUPNAME  /* SUPS THE SYSMOD PASSED        */
*             THEN                                                 0208
         LR    @15,L                                               0208
         SLA   @15,3                                               0208
         MVC   @TF00001(4),ICTPSCHN(PPTR2)                         0208
         L     @01,@TF00001                                        0208
         L     @14,@PC00006                                        0208
         ALR   @01,@15                                             0208
         AL    @01,@CF00930                                        0208
         CLC   ICTSUPNM(7,@01),SUPNAME(@14)                        0208
         BNE   @RF00208                                            0208
*             SUPPTR=ADDR(ICTPTF(1));/* POINT TO SUPERSEDER          */
         L     @15,@PC00006+4                                      0209
         ST    PPTR2,SUPPTR(,@15)                                  0209
*         END;                                                     0210
@RF00208 AH    L,@CH00066                                          0210
@DE00207 LR    @15,L                                               0210
         SLA   @15,3                                               0210
         MVC   @TF00001(4),ICTPSCHN(PPTR2)                         0210
         L     @01,@TF00001                                        0210
         AL    @01,@CF00930                                        0210
         L     @15,ICTSUPND(@15,@01)                               0210
         CL    @15,ICTENDIX                                        0210
         BE    @DC00207                                            0210
         L     @15,@PC00006+4                                      0210
         L     @15,SUPPTR(,@15)                                    0210
         LTR   @15,@15                                             0210
         BZ    @DL00207                                            0210
@DC00207 DS    0H                                                  0211
*       END;                                                       0211
*   END;                                                           0212
@RF00205 AH    PPTR2,@CH00922                                      0212
@DE00204 CLC   ICTPEND(4,PPTR2),ICTENDNT                           0212
         BE    @DC00204                                            0212
         L     @03,@PC00006+4                                      0212
         L     @03,SUPPTR(,@03)                                    0212
         LTR   @03,@03                                             0212
         BZ    @DL00204                                            0212
@DC00204 DS    0H                                                  0213
*   RETURN;                                                        0213
@EL00006 DS    0H                                                  0213
@EF00006 DS    0H                                                  0213
@ER00006 LM    @14,@12,12(@13)                                     0213
         BR    @14                                                 0213
*   END SUPICT;                                                    0214
