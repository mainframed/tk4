*/********************************************************************/
*/*                                                                  */
*/*    CHKICT  -  CHECK THE ICT FOR A NON-NOGO, NON-DELETED, NON-    */
*/*               SUPERSEDED ICT PTF SECTION ENTRY REPRESENTING      */
*/*               THE SYSMOD NAME PASSED BY THE CALLER.              */
*/*                                                                  */
*/*               IF SUCH AN ENTRY IS FOUND, RETURN A POINTER TO IT. */
*/*                                                                  */
*/*               IF SUCH AN ENTRY IS NOT FOUND, RETURN THE POINTER  */
*/*               AS ZERO.                                           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0179
*CHKICT:                                                           0179
*   PROC(SYSMNAME,SRCHPTR);                                        0179
CHKICT   STM   @14,@12,12(@13)                                     0179
         MVC   @PC00005(8),0(@01)                                  0179
*   DCL                                                            0180
*     SYSMNAME CHAR(7);             /* SYSMOD TO BE FOUND IN ICT     */
*   DCL                                                            0181
*     SRCHPTR PTR(31);              /* RETURNS PTR TO SYSMOD FOUND   */
*   DCL                                                            0182
*     PPTR1 PTR(31);                /* ADDR OF ICT PTF SECT ENTRY    */
*   RESPECIFY                                                      0183
*     ICTPTF BASED(PPTR1);          /* SET UP ADDRESSIBILITY FOR PTF
*                                      SECTION ENTRIES               */
*   SRCHPTR=0;                      /* INITIALIZE TO ZERO WHICH    0184
*                                      IMPLIES "NOT FOUND"           */
*                                                                  0184
         L     @15,@PC00005+4                                      0184
         SLR   @14,@14                                             0184
         ST    @14,SRCHPTR(,@15)                                   0184
*   /*****************************************************************/
*   /*                                                               */
*   /* GO THRU THE ICT PTF SECTION ENTIRES LOOKING FOR "SYSMNAME"    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0185
*   DO PPTR1=CCAICPTF BY LENGTH(ICTPTF)/* GO THRU ICT PTF SECTION    */
*         WHILE ICTPEND(1)^=ICTENDNT/* NOT AT END OF PTF SECTION     */
*         &SRCHPTR=0;               /* SYSMOD NOT FOUND              */
         L     PPTR1,CCAICPTF(,CCAPTR)                             0185
         B     @DE00185                                            0185
@DL00185 DS    0H                                                  0186
*     IF ICTPTFS(1)=SYSMNAME THEN                                  0186
*                                                                  0186
         L     @03,@PC00005                                        0186
         CLC   ICTPTFS(7,PPTR1),SYSMNAME(@03)                      0186
         BNE   @RF00186                                            0186
*       /*************************************************************/
*       /*                                                           */
*       /* SYSMNAME FOUND IN ICT                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0187
*       IF ICTPNOGO(1)=OFF          /* NOT NOGO                      */
*           &ICTPDEXP(1)=OFF        /* NOT EXPLICITLY DELETED        */
*           &ICTPDIMP(1)=OFF        /* NOT IMPLICITLY DELETED        */
*           &ICTPSUPN(1)=0          /* NOT SUPERSEDED                */
*         THEN                                                     0187
         TM    ICTPNOGO(PPTR1),B'01000000'                         0187
         BNZ   @RF00187                                            0187
         TM    ICTPDEXP(PPTR1),B'00110000'                         0187
         BNZ   @RF00187                                            0187
         MVC   @ZT00002+2(2),ICTPSUPN(PPTR1)                       0187
         L     @03,@ZT00002                                        0187
         LTR   @03,@03                                             0187
         BNZ   @RF00187                                            0187
*         SRCHPTR=ADDR(ICTPTF(1));  /* RETURN POINTER TO CALLER      */
         L     @03,@PC00005+4                                      0188
         ST    PPTR1,SRCHPTR(,@03)                                 0188
*   END;                                                           0189
@RF00187 DS    0H                                                  0189
@RF00186 AH    PPTR1,@CH00922                                      0189
@DE00185 CLC   ICTPEND(4,PPTR1),ICTENDNT                           0189
         BE    @DC00185                                            0189
         L     @03,@PC00005+4                                      0189
         L     @03,SRCHPTR(,@03)                                   0189
         LTR   @03,@03                                             0189
         BZ    @DL00185                                            0189
@DC00185 DS    0H                                                  0190
*   RETURN;                                                        0190
@EL00005 DS    0H                                                  0190
@EF00005 DS    0H                                                  0190
@ER00005 LM    @14,@12,12(@13)                                     0190
         BR    @14                                                 0190
*   END CHKICT;                                                    0191
