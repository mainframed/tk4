         TITLE 'HMASMTR1 CHECK SYSMOD REQUISITES'
HMASMTR1 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMTR1  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @10,@01                                             0001
         USING @DATD,@10                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @10,8(,@13)                                         0001
         LR    @13,@10                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
         MVC   @PC00001(12),0(@01)                                 0001
*                                      ICT PTF SECTION ENTRY PASSED
*                                      BY THE CALLER                 */
*   TR1RETRN=ZERO;                  /* INITIALIZE CALLER''S PARM     */
         L     @15,@PC00001+8                                      0063
         MVI   TR1RETRN(@15),X'00'                                 0063
*   TR1FLGS=''B;                    /* CLEAR LOCAL FLAGS             */
         MVI   TR1FLGS,X'00'                                       0064
*   IF ICTPTR1(1)=ON THEN           /* CHECK TR1 FLAG IN ENTRY PASSED*/
         L     @15,@PC00001+4                                      0065
         L     @01,PTFPTR(,@15)                                    0065
         TM    ICTPTR1(@01),B'01000000'                            0065
         BNO   @RF00065                                            0065
*     LOCALTR1=ON;                  /* REMEMBER THAT TR1 PROCESSING
*                                      FLAG WAS SET UPON ENTRY       */
         OI    LOCALTR1,B'10000000'                                0066
*   ELSE                                                           0067
*     ICTPTR1(1)=ON;                /* OTHERWISE SET TR1 PROCESSING
*                                      FLAG IN CURRENT PTF ENTRY     */
*                                                                  0067
         B     @RC00065                                            0067
@RF00065 L     @15,@PC00001+4                                      0067
         L     @01,PTFPTR(,@15)                                    0067
         OI    ICTPTR1(@01),B'01000000'                            0067
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK THE REQUISITES OF THIS SYSMOD                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0068
*   IF ICTPPCHN(1)^=0 THEN                                         0068
@RC00065 L     @15,@PC00001+4                                      0068
         L     @15,PTFPTR(,@15)                                    0068
         MVC   @TF00001(4),ICTPPCHN(@15)                           0068
         L     @14,@TF00001                                        0068
         LTR   @14,@14                                             0068
         BZ    @RF00068                                            0068
*     DO;                                                          0069
*                                                                  0069
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK THAT PREREQS ARE SATISFIED                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0070
*       CALL REQCHK(ICTPPCHN(1),PRE);                              0070
         LA    @15,ICTPPCHN(,@15)                                  0070
         ST    @15,@AL00001                                        0070
         LA    @15,@CH00066+1                                      0070
         ST    @15,@AL00001+4                                      0070
         LA    @01,@AL00001                                        0070
         BAL   @14,REQCHK                                          0070
*     END;                                                         0071
*   IF ICTPRCHN(1)^=0 THEN                                         0072
@RF00068 L     @15,@PC00001+4                                      0072
         L     @15,PTFPTR(,@15)                                    0072
         MVC   @TF00001(4),ICTPRCHN(@15)                           0072
         L     @14,@TF00001                                        0072
         LTR   @14,@14                                             0072
         BZ    @RF00072                                            0072
*     DO;                                                          0073
*                                                                  0073
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK THAT REQS ARE SATISFIED                             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0074
*       CALL REQCHK(ICTPRCHN(1),REQ);                              0074
         LA    @15,ICTPRCHN(,@15)                                  0074
         ST    @15,@AL00001                                        0074
         LA    @15,@CH00276+1                                      0074
         ST    @15,@AL00001+4                                      0074
         LA    @01,@AL00001                                        0074
         BAL   @14,REQCHK                                          0074
*     END;                                                         0075
*   IF ICTPICHN(1)^=0 THEN                                         0076
@RF00072 L     @15,@PC00001+4                                      0076
         L     @15,PTFPTR(,@15)                                    0076
         MVC   @TF00001(4),ICTPICHN(@15)                           0076
         L     @14,@TF00001                                        0076
         LTR   @14,@14                                             0076
         BZ    @RF00076                                            0076
*     DO;                                                          0077
*                                                                  0077
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK THAT IREQS ARE SATISFIED                            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0078
*       CALL REQCHK(ICTPICHN(1),IRQ);                              0078
         LA    @15,ICTPICHN(,@15)                                  0078
         ST    @15,@AL00001                                        0078
         LA    @15,@CH00089+1                                      0078
         ST    @15,@AL00001+4                                      0078
         LA    @01,@AL00001                                        0078
         BAL   @14,REQCHK                                          0078
*     END;                                                         0079
*                                                                  0079
*   /*****************************************************************/
*   /*                                                               */
*   /* REQUISITE CHECKING COMPLETED FOR THIS SYSMOD ... RETURN TO THE*/
*   /* CALLER                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0080
*   IF LOCALTR1=OFF                 /* RECURSION FLAG NOT SET ON   0080
*                                      ENTRY                         */
*     THEN                                                         0080
@RF00076 TM    LOCALTR1,B'10000000'                                0080
         BNZ   @RF00080                                            0080
*     ICTPTR1(1)=OFF;               /* TURN OFF TR1 RECURSION FLAG   */
         L     @15,@PC00001+4                                      0081
         L     @01,PTFPTR(,@15)                                    0081
         NI    ICTPTR1(@01),B'10111111'                            0081
*   RETURN CODE(TR1RETRN);                                         0082
@RF00080 L     @09,@PC00001+8                                      0082
         SLR   @08,@08                                             0082
         IC    @08,TR1RETRN(,@09)                                  0082
         L     @13,4(,@13)                                         0082
         L     @00,@SIZDATD                                        0082
         LR    @01,@10                                             0082
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@08                                             0082
         L     @14,12(,@13)                                        0082
         LM    @00,@12,20(@13)                                     0082
         BR    @14                                                 0082
*                                                                  0083
*/********************************************************************/
*/*                                                                  */
*/*  REQCHK  - CHECK THE REQUISITES OF A SYSMOD TO DETERMINE WHETHER */
*/*            THEY ARE SATISFIED BY BEING PRESENT ON EITHER THE     */
*/*            TARGET SYSTEM OR IN THE ICT.                          */
*/*                                                                  */
*/*          - SET FLAGS, ICTEREQS AND ICTEREQB, IN THE REQUISITE    */
*/*            LIST ENTRIES FOR REQUISITES WHICH ARE SATISFIED AND   */
*/*            BYPASSED RESPECTIVELY.                                */
*/*                                                                  */
*/*          - SET TR1RETRN FOR REQUISITE FAILURES AND BYPASSES.     */
*/*                                                                  */
*/*                                                                  */
*/*       THE FOLLOWING ILLUSTRATES THE FLAGS SET AND TESTED IN      */
*/*       THE PTF SECTION ENTRY POINTED TO BY RLISTPTR:              */
*/*                                                                  */
*/*       ICT SECTION ENTRY - ICTPTF(*)                              */
*/*       -----------------                                          */
*/*       ×               ×                                          */
*/*       ×         PRQCK × ALL REQS "CHECKED" FLAG                  */
*/*       ×         PRQOK × ALL REQS "OK" FLAG                       */
*/*       ×         PRQBY × SOME REQS "BYPASSED" FLAG                */
*/*       ×         PTR1  × HMASMTR1 PROCESSING THIS ENTRY           */
*/*       ×               ×                                          */
*/*       ×               ×         ICTENTLS - REQUISITE LIST        */
*/*       × ICTREQLS      × ------> --------------                   */
*/*       × ICTPRELS      ×         × REQ PTF #  ×                   */
*/*       × ICTIRQLS      ×         ×   ICTEREQS × REQ "SATISFIED"   */
*/*       ×               ×         ×   ICTEREQB × REQ "BYPASSED"    */
*/*       ×               ×         ×   ICTEREQI × "MUST BE IN ICT"  */
*/*       ×               ×         ×   ICTEREQT × REQ "ON (A)CDS"   */
*/*       ×               ×         ×------------×                   */
*/*       ×               ×         × REQ PTF #  ×                   */
*/*       ×               ×         ×            ×                   */
*/*       ×_______________×         ×            ×                   */
*/*                                 ×------------×                   */
*/*                                       .                          */
*/*                                       .                          */
*/*                                       .                          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0083
*REQCHK:                                                           0083
*   PROC(RLISTPTR,REQTYPE);                                        0083
REQCHK   STM   @14,@12,@SA00002                                    0083
         MVC   @PC00002(8),0(@01)                                  0083
*   DCL                                                            0084
*     RLISTPTR PTR(31);             /* POINTER TO REQUISITE LIST     */
*   DCL                                                            0085
*     REQTYPE FIXED(8);             /* REQ TYPE (PRE,REQ,IRQ)        */
*                                                                  0086
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCAL VARIABLES                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0086
*   DCL                                                            0086
*     I FIXED(31);                  /* REQ LIST ENTRY INDEX          */
*   DCL                                                            0087
*     LISTENT@ PTR(31);             /* REQ LIST ENTRY ADDRESS        */
*   DCL                                                            0088
*     INDXSTEP FIXED(31);           /* INDEX STEP INCREMENT          */
*                                                                  0088
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP LOOP INDEX INCREMENT BASED UPON THE TYPE OF REQUISITE  */
*   /* LIST ENTRY BEING DEALT WITH                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0089
*   IF REQTYPE=IRQ THEN                                            0089
         L     @15,@PC00002+4                                      0089
         CLI   REQTYPE(@15),3                                      0089
         BNE   @RF00089                                            0089
*     INDXSTEP=2;                   /* INDEX STEP FOR IREQ ENTRIES IS
*                                      2 ... IREQ ENTRIES ARE MADE UP
*                                      OF TWO "GENERAL" ENTRIES      */
         LA    INDXSTEP,2                                          0090
*   ELSE                                                           0091
*     INDXSTEP=1;                   /* INDEX STEP FOR ALL OTHERS IS 1*/
         B     @RC00089                                            0091
@RF00089 LA    INDXSTEP,1                                          0091
*   RESPECIFY                                                      0092
*     ICTENTLS BASED(RLISTPTR);     /* ADDRESS REQUISITE LIST PASSED */
*                                                                  0092
@RC00089 DS    0H                                                  0093
*   /*****************************************************************/
*   /*                                                               */
*   /* GO THRU ALL REQUISITE LIST ENTRIES                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0093
*   DO I=1 BY INDXSTEP WHILE ICTENTND(I)^=ICTENDIX;                0093
         LA    I,1                                                 0093
         B     @DE00093                                            0093
@DL00093 DS    0H                                                  0094
*     LISTENT@=ADDR(ICTENTLS(I));   /* POINT TO REQ LIST ENTRY       */
         LR    @15,I                                               0094
         SLA   @15,3                                               0094
         L     @14,@PC00002                                        0094
         L     @01,RLISTPTR(,@14)                                  0094
         AL    @01,@CF00930                                        0094
         LA    @15,ICTENTLS(@15,@01)                               0094
         ST    @15,LISTENT@                                        0094
*     RESPECIFY                                                    0095
*       ICTENTLS BASED(LISTENT@);   /* ADDRESS "CURRENT" REQ LIST  0095
*                                      ENTRY                         */
*     ICTEREQS(1)=OFF;              /* CLEAR REQ SATISFIED INDICATOR */
*     ICTEREQB(1)=OFF;              /* CLEAR REQ BYPASSED INDICATOR  */
*                                                                  0097
         NI    ICTEREQS(@15),B'11100111'                           0097
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK THE TARGET SYSTEM FOR THIS REQ                        */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0098
*     IF ICTEREQT(1)=ON             /* REQ IS ON TARGET SYSTEM       */
*         &ICTEREQI(1)=OFF          /* ... NOT REQUIRED TO BE IN ICT */
*       THEN                                                       0098
         TM    ICTEREQT(@15),B'00000010'                           0098
         BNO   @RF00098                                            0098
         TM    ICTEREQI(@15),B'00000100'                           0098
         BNZ   @RF00098                                            0098
*       ICTEREQS(1)=ON;             /* SET THE SATISFIED INDICATOR   */
         OI    ICTEREQS(@15),B'00010000'                           0099
*     IF ICTEREQS(1)=OFF            /* REQUISITE NOT SATISFIED       */
*       THEN                                                       0100
*                                                                  0100
@RF00098 L     @01,LISTENT@                                        0100
         TM    ICTEREQS(@01),B'00010000'                           0100
         BNZ   @RF00100                                            0100
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK THE ICT FOR THIS REQ                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0101
*       CALL CKICTREQ(LISTENT@,     /* PTR TO REQ LIST ENTRY         */
*           REQTYPE);               /* REQUISITE TYPE                */
         LA    @15,LISTENT@                                        0101
         ST    @15,@AL00001                                        0101
         L     @15,@PC00002+4                                      0101
         ST    @15,@AL00001+4                                      0101
         LA    @01,@AL00001                                        0101
         BAL   @14,CKICTREQ                                        0101
*     IF ICTEREQS(1)=OFF            /* REQUISITE NOT SATISFIED       */
*       THEN                                                       0102
*                                                                  0102
@RF00100 L     @01,LISTENT@                                        0102
         TM    ICTEREQS(@01),B'00010000'                           0102
         BNZ   @RF00102                                            0102
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK THE ICT FOR SYSMODS WHICH SUPERSEDE THIS REQ        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0103
*       CALL CKICTSUP(LISTENT@,     /* PTR TO REQ LIST ENTRY         */
*           REQTYPE);               /* REQUISITE TYPE                */
         LA    @15,LISTENT@                                        0103
         ST    @15,@AL00001                                        0103
         L     @15,@PC00002+4                                      0103
         ST    @15,@AL00001+4                                      0103
         LA    @01,@AL00001                                        0103
         BAL   @14,CKICTSUP                                        0103
*     IF ICTEREQS(1)=OFF            /* REQUISITE NOT SATISFIED       */
*       THEN                                                       0104
@RF00102 L     @01,LISTENT@                                        0104
         TM    ICTEREQS(@01),B'00010000'                           0104
         BNZ   @RF00104                                            0104
*       DO;                                                        0105
*                                                                  0105
*         /***********************************************************/
*         /*                                                         */
*         /* THIS REQ HAS FAILED ... TRY THE BYPASS OPTIONS          */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0106
*         IF(REQTYPE=PRE&TBLBPPRE=ON)/* PRE AND BYPASS(PRE)          */
*             ×(REQTYPE=REQ&TBLBPREQ=ON)/* REQ AND BYPASS(REQ)       */
*             ×(REQTYPE=IRQ&TBLBPIRQ=ON)/* IREQ AND BYPASS(IREQ)     */
*           THEN                                                   0106
         L     @15,@PC00002+4                                      0106
         CLI   REQTYPE(@15),1                                      0106
         BNE   @GL00002                                            0106
         L     @15,@PC00001                                        0106
         L     @01,TBLXTBL(,@15)                                   0106
         TM    TBLBPPRE(@01),B'01000000'                           0106
         BO    @RT00106                                            0106
@GL00002 L     @15,@PC00002+4                                      0106
         CLI   REQTYPE(@15),2                                      0106
         BNE   @GL00001                                            0106
         L     @15,@PC00001                                        0106
         L     @01,TBLXTBL(,@15)                                   0106
         TM    TBLBPREQ(@01),B'00100000'                           0106
         BO    @RT00106                                            0106
@GL00001 L     @15,@PC00002+4                                      0106
         CLI   REQTYPE(@15),3                                      0106
         BNE   @RF00106                                            0106
         L     @15,@PC00001                                        0106
         L     @01,TBLXTBL(,@15)                                   0106
         TM    TBLBPIRQ(@01),B'00010000'                           0106
         BNO   @RF00106                                            0106
@RT00106 DS    0H                                                  0107
*           DO;                                                    0107
*                                                                  0107
*             /*******************************************************/
*             /*                                                     */
*             /* THE FAILURE IS BYPASSED                             */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0108
*             ICTEREQB(1)=ON;       /* SET "BYPASSED" FLAG IN LIST   */
*             ICTEREQS(1)=ON;       /* SET "SATISFIED" FLAG IN LIST  */
         L     @01,LISTENT@                                        0109
         OI    ICTEREQB(@01),B'00011000'                           0109
*             TR1RETRN=MAX(TR1RETRN,BYPASSED);/* SET RETURN PARM     */
         L     @15,@PC00001+8                                      0110
         SLR   @14,@14                                             0110
         IC    @14,TR1RETRN(,@15)                                  0110
         LA    @09,4                                               0110
         CR    @14,@09                                             0110
         BNL   *+6
         LR    @14,@09                                             0110
         STC   @14,TR1RETRN(,@15)                                  0110
*           END;                                                   0111
*         ELSE                                                     0112
*                                                                  0112
*           /*********************************************************/
*           /*                                                       */
*           /* THE FAILURE IS NOT BYPASSED                           */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0112
*           TR1RETRN=MAX(TR1RETRN,FAILED);/* SET RETURN PARM         */
         B     @RC00106                                            0112
@RF00106 L     @15,@PC00001+8                                      0112
         SLR   @14,@14                                             0112
         IC    @14,TR1RETRN(,@15)                                  0112
         LA    @09,8                                               0112
         CR    @14,@09                                             0112
         BNL   *+6
         LR    @14,@09                                             0112
         STC   @14,TR1RETRN(,@15)                                  0112
*       END;                        /* END OF BYPASS OPTION CHECK    */
@RC00106 DS    0H                                                  0114
*   END;                            /* END OF LOOP THRU ALL        0114
*                                      REQUISITES IN CURRENT REQ LIST*/
*                                                                  0114
@RF00104 ALR   I,INDXSTEP                                          0114
@DE00093 LR    @15,I                                               0114
         SLA   @15,3                                               0114
         L     @14,@PC00002                                        0114
         L     @01,RLISTPTR(,@14)                                  0114
         AL    @01,@CF00930                                        0114
         L     @15,ICTENTND(@15,@01)                               0114
         CL    @15,ICTENDIX                                        0114
         BNE   @DL00093                                            0114
*   /*****************************************************************/
*   /*                                                               */
*   /* ALL REQS IN THE LIST HAVE BEEN CHECKED                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0115
*   RETURN;                                                        0115
@EL00002 DS    0H                                                  0115
@EF00002 DS    0H                                                  0115
@ER00002 LM    @14,@12,@SA00002                                    0115
         BR    @14                                                 0115
*   END REQCHK;                                                    0116
*                                                                  0117
*/********************************************************************/
*/*                                                                  */
*/*      CKICTREQ - CHECK ICT TO DETERMINE WHETHER THE CURRENT       */
*/*                 REQUISITE IS SATISFIED.                          */
*/*                                                                  */
*/*                                                                  */
*/*        NOTE - THE REQUISITE SATISFIED FLAG, ICTEREQS, IS "OFF"   */
*/*               ON ENTRY TO THIS PROCEDURE.                        */
*/*                                                                  */
*/*             - THE REQUISITE SATISFIED FLAG, ICTEREQS, WILL BE    */
*/*               SET ON IF THE REQUISITE CAN BE FOUND (AND CHECKED) */
*/*               IN THE ICT.                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0117
*CKICTREQ:                                                         0117
*   PROC(LEPTR1,REQTYPE1);                                         0117
CKICTREQ STM   @14,@12,@SA00003                                    0117
         MVC   @PC00003(8),0(@01)                                  0117
*   DCL                                                            0118
*     LEPTR1 PTR(31);               /* PTR TO REQ LIST ENTRY FOR   0118
*                                      REQUISITE TO BE CHECKED       */
*   DCL                                                            0119
*     REQTYPE1 FIXED(8);            /* REQUISITE TYPE (PRE, REQ,IRQ) */
*   DCL                                                            0120
*     REQPTR PTR(31);               /* PTR TO ICT PTF SECTION ENTRY
*                                      FOR REQ (IF FOUND IN ICT)     */
*   RESPECIFY                                                      0121
*     ICTENTLS BASED(LEPTR1);       /* ADDRESS LIST ENTRY PASSED     */
*   RESPECIFY                                                      0122
*     ICTPTF BASED(PTFPTR);         /* ADDRESS PTF PASSED TO TR1     */
*   CALL CHKICT(ICTENTNM(1),REQPTR);/* LOOK FOR REQ IN ICT           */
         L     @15,@PC00003                                        0123
         L     @01,LEPTR1(,@15)                                    0123
         ST    @01,@AL00001                                        0123
         LA    @15,REQPTR                                          0123
         ST    @15,@AL00001+4                                      0123
         LA    @01,@AL00001                                        0123
         BAL   @14,CHKICT                                          0123
*   IF REQPTR^=0                    /* REQUISITE FOUND IN ICT        */
*     THEN                                                         0124
         L     @15,REQPTR                                          0124
         LTR   @15,@15                                             0124
         BZ    @RF00124                                            0124
*     DO;                                                          0125
*                                                                  0125
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK REQUISITE FOUND IN ICT                              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0126
*       IF REQPTR->ICTPTR1(1)=ON    /* TR1 RECURSION FLAG            */
*         THEN                                                     0126
         TM    ICTPTR1(@15),B'01000000'                            0126
         BNO   @RF00126                                            0126
*         DO;                                                      0127
*                                                                  0127
*           /*********************************************************/
*           /*                                                       */
*           /* THIS REQUISITE IS IN THE REQUISITE CHAIN CURRENTLY    */
*           /* BEING CHECKED BY ANOTHER INVOCATION OF HMASMTR1       */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0128
*           IF REQTYPE1=PRE         /* PREREQ-TYPE REQUISITE ?       */
*             THEN                                                 0128
         L     @15,@PC00003+4                                      0128
         CLI   REQTYPE1(@15),1                                     0128
         BNE   @RF00128                                            0128
*             ICTEREQS(1)=ON;       /* YES ... SET "SATISFIED" IN REQ
*                                      LIST ENTRY EVEN THOUGH THIS IS
*                                      A POTENTIAL PREREQ CHAIN ERROR
*                                      SITUATION                     */
         L     @15,@PC00003                                        0129
         L     @01,LEPTR1(,@15)                                    0129
         OI    ICTEREQS(@01),B'00010000'                           0129
*           ELSE                                                   0130
*             ICTEREQS(1)=ON;       /* NO ... SET "SATISFIED" IN REQ
*                                      LIST ENTRY                    */
         B     @RC00128                                            0130
@RF00128 L     @15,@PC00003                                        0130
         L     @01,LEPTR1(,@15)                                    0130
         OI    ICTEREQS(@01),B'00010000'                           0130
*         END;                                                     0131
*       ELSE                                                       0132
*         DO;                                                      0132
*                                                                  0132
         B     @RC00126                                            0132
@RF00126 DS    0H                                                  0133
*           /*********************************************************/
*           /*                                                       */
*           /* THIS REQUISITE IS NOT IN THE REQUISITE CHAIN CURRENTLY*/
*           /* BEING CHECKED ... CHECK IT                            */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0133
*           IF REQPTR->ICTPRQCK(1)=ON/* REQUISITE HAS BEEN CHECKED 0133
*                                      ALREADY                       */
*             THEN                                                 0133
         L     @15,REQPTR                                          0133
         TM    ICTPRQCK(@15),B'00001000'                           0133
         BNO   @RF00133                                            0133
*             DO;                                                  0134
*                                                                  0134
*               /*****************************************************/
*               /*                                                   */
*               /* REQUISITE HAS ALREADY BEEN CHECKED                */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0135
*               IF REQPTR->ICTPRQOK(1)=ON/* CHECKED "OK"             */
*                 THEN                                             0135
         TM    ICTPRQOK(@15),B'00000100'                           0135
         BNO   @RF00135                                            0135
*                 ICTEREQS(1)=ON;   /* SET REQ SATISFIED FLAG        */
         L     @15,@PC00003                                        0136
         L     @01,LEPTR1(,@15)                                    0136
