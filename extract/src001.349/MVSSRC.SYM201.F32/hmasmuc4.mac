         TITLE 'HMASMUC4 - SMP SCDS UCLIN PROCESSOR'
HMASMUC4 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'HMASMUC4  78.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  LA    @10,4095(,@12)                                      0001
         USING @PSTART,@12                                         0001
         USING @PSTART+4095,@10                                    0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(8),0(@01)                                  0001
*                                                                  0176
*   /*****************************************************************/
*   /*                                                               */
*   /* HMASMUC4 - SMP UPDATE CONTROL LANGUAGE HANDLER.               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0176
*   IOPPTR=CCAIOPTR;                /* SET ADDRESSABILITY FOR I/O  0176
*                                      RTNE                          */
         L     IOPPTR,CCAIOPTR(,CCAPTR)                            0176
*   SCPPTR=CRPSCPAD;                /* PICK UP SCP ADDR              */
         L     @08,@PC00001+4                                      0177
         MVC   SCPPTR(4),CRPSCPAD(@08)                             0177
*   UCLRTNCD=ZERO;                  /* INIT RETURN CODE              */
         MVI   UCLRTNCD,X'00'                                      0178
*   UCLSWTBL=''B;                   /* CLEAR CNTL SW                 */
*                                                                  0179
         MVI   UCLSWTBL,X'00'                                      0179
*   /*****************************************************************/
*   /*                                                               */
*   /* SET MGP FOR ALL MSG TO GO TO PRINTER AND LOG                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0180
*   HMASMMGP=HMASMMGP&&HMASMMGP;    /* CLEAR MGP FIRST               */
         XC    HMASMMGP(32),HMASMMGP                               0180
*   MGPPRINT=ON;                    /* MSG TO PRINTER                */
*   MGPHLDS=ON;                     /* MSG TO LOG                    */
         OI    MGPPRINT,B'10010000'                                0182
*                                                                  0183
*   /*****************************************************************/
*   /*                                                               */
*   /* PROGRAM INITIALIZATION - CLEAR CONTROL SWITCHES AND SET DATA  */
*   /* AREAS TO BLANK CHARACTERS                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0183
*   DO WHILE(                       /* CONTINUE LOOP WHILE           */
*         UCLRTNCD<STOPSMP          /* NO CRITICAL ERRS              */
*         &UCLTERM=OFF              /* NO TERMINAL ERRS              */
*         &CRPEOFEN=OFF             /* AND NOT END OF FILE           */
*         );                        /*                               */
         B     @DE00183                                            0183
@DL00183 DS    0H                                                  0184
*     IF UCLRTNCD=STOPFUNC          /* STOP UCLIN PROCESSING         */
*         ×UCLFLUSH=ON              /* OR IN FLUSH MODE              */
*       THEN                        /* YES                           */
         CLI   UCLRTNCD,12                                         0184
         BE    @RT00184                                            0184
         L     @08,@PC00001                                        0184
         TM    UCLFLUSH(@08),B'00000010'                           0184
         BNO   @RF00184                                            0184
@RT00184 DS    0H                                                  0185
*       DO;                         /*                               */
*         SCPNORT=ON;               /* DONT CALL ROUTS               */
         L     @08,SCPPTR                                          0186
         OI    SCPNORT(@08),B'00000100'                            0186
*         UCLFLUSH=ON;              /* SET FLUSH BIT                 */
         L     @08,@PC00001                                        0187
         OI    UCLFLUSH(@08),B'00000010'                           0187
*       END;                        /*                               */
*     ELSE                          /*                               */
*       SCPNORT=OFF;                /* CALL ROUTS                    */
         B     @RC00184                                            0189
@RF00184 L     @08,SCPPTR                                          0189
         NI    SCPNORT(@08),B'11111011'                            0189
*     UCLSWTBL=''B;                 /* CLEAR THE CONTROL SWITCHES    */
@RC00184 MVI   UCLSWTBL,X'00'                                      0190
*     IOPNAME=BLANK8;               /*                               */
*                                                                  0191
         MVC   IOPNAME(8,IOPPTR),BLANK8                            0191
*     /***************************************************************/
*     /*                                                             */
*     /* READ A CONTROL STATEMENT FROM THE INPUT DATA SET            */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0192
*     CALL HMASMCRD(HMASMCRP);      /* READ/PRINT CC                 */
         L     @08,@PC00001+4                                      0192
         ST    @08,@AL00001                                        0192
         L     @15,@CV01389                                        0192
         LA    @01,@AL00001                                        0192
         BALR  @14,@15                                             0192
*     IF CRPRETRN>CRPEOF            /* IF RC GREATER THAN            */
*       THEN                        /* EOF THEN                      */
         L     @08,@PC00001+4                                      0193
         CLI   CRPRETRN(@08),4                                     0193
         BNH   @RF00193                                            0193
*       UCLRTNCD=MAX(UCLRTNCD,CRPRETRN);/* SAVE RC                   */
*                                                                  0194
         SLR   @15,@15                                             0194
         IC    @15,UCLRTNCD                                        0194
         SLR   @14,@14                                             0194
         IC    @14,CRPRETRN(,@08)                                  0194
         CR    @15,@14                                             0194
         BNL   *+6
         LR    @15,@14                                             0194
         STC   @15,UCLRTNCD                                        0194
*     /***************************************************************/
*     /*                                                             */
*     /* IF A RECORD WAS SUCCESSFULLY READ THEN SCAN IT              */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0195
*     IF CRPRETRN=CRPGOOD           /* IF RECORD READ OK             */
*       THEN                        /*                               */
@RF00193 L     @08,@PC00001+4                                      0195
         CLI   CRPRETRN(@08),0                                     0195
         BNE   @RF00195                                            0195
*       DO;                         /*                               */
*         SCPSRCH=ADSCNTBL;         /* PLACE THE START OF THE SCAN 0197
*                                      TABLES INTO THE SCAN PARM LIST*/
         L     @08,SCPPTR                                          0197
         MVC   SCPSRCH(4,@08),ADSCNTBL                             0197
*         CALL HMASMSCN(HMASMSCP);  /* CALL THE SCAN ROUTINE TO SCAN
*                                      THE CARD WE JUST READ IN      */
         ST    @08,@AL00001                                        0198
         L     @15,@CV01391                                        0198
         LA    @01,@AL00001                                        0198
         BALR  @14,@15                                             0198
*                                                                  0199
*/********************************************************************/
*/*                                                                  */
*/*    SCAN RETURN CODES - SYNTAX ERROR & STATUS INDICATORS -        */
*/*                                                                  */
*/*    SCPRET   -  THE RETURN CODE FROM THE SCAN ROUTINE - IF NON-   */
*/*                ZERO, THE SCAN ROUTINE HAS DETECTED A SYNTAX      */
*/*                ERROR                                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0199
*         IF CRPEOFEN=ON            /* EOF ON INPUT?                 */
*           THEN                    /* YES                           */
         L     @08,@PC00001+4                                      0199
         TM    CRPEOFEN(@08),B'10000000'                           0199
         BNO   @RF00199                                            0199
*           CALL UCL258I;           /* WRITE MSG                     */
         BAL   @14,UCL258I                                         0200
*         ELSE                      /*                               */
*           DO;                     /*                               */
         B     @RC00199                                            0201
@RF00199 DS    0H                                                  0202
*             IF SCPRETRN>STOPPTF   /* IF I/O TYPE ERR               */
*               THEN                                               0202
         L     @08,SCPPTR                                          0202
         CLI   SCPRETRN(@08),8                                     0202
         BNH   @RF00202                                            0202
*               UCLRTNCD=MAX(UCLRTNCD,SCPRETRN);/* PASS ALONG RC     */
         SLR   @15,@15                                             0203
         IC    @15,UCLRTNCD                                        0203
         SLR   @14,@14                                             0203
         IC    @14,SCPRETRN(,@08)                                  0203
         CR    @15,@14                                             0203
         BNL   *+6
         LR    @15,@14                                             0203
         STC   @15,UCLRTNCD                                        0203
*             IF DELERR=ON          /*                               */
*               THEN                /*                               */
@RF00202 TM    DELERR,B'00000100'                                  0204
         BNO   @RF00204                                            0204
*               DO;                 /*                               */
*                 CALL UCL253I;     /* PUT OUT ENTRY INCOMPLETE      */
         BAL   @14,UCL253I                                         0206
*                 CALL FLUSH;       /* FLUSH REST OF CARD            */
         BAL   @14,FLUSH                                           0207
*               END;                                               0208
*             ELSE                  /*                               */
*               IF SCPRETRN^=SCPGOOD/* IF SCAN NOT OK                */
*                 THEN              /*                               */
         B     @RC00204                                            0209
@RF00204 L     @08,SCPPTR                                          0209
         CLI   SCPRETRN(@08),0                                     0209
         BE    @RF00209                                            0209
*                 DO;               /*                               */
*                   CALL UCL203I;   /* PUT OUT SYNTAX ERR            */
         BAL   @14,UCL203I                                         0211
*                   CALL FLUSH;     /* FLUSH REST OF CARD            */
         BAL   @14,FLUSH                                           0212
*                 END;              /*                               */
*           END;                    /*                               */
@RF00209 DS    0H                                                  0214
@RC00204 DS    0H                                                  0215
*                                                                  0215
*         /***********************************************************/
*         /*                                                         */
*         /* IF NOT END OF UCL AND NOT FLUSH MODE CALL UCLSTOW.      */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0215
*         IF UCLTERM=OFF            /* IF ENDUCL NOT FOUND           */
*             &UCLFLUSH=OFF         /* AND NOT IN FLUSH MODE         */
*           THEN                    /*                               */
@RC00199 TM    UCLTERM,B'00010000'                                 0215
         BNZ   @RF00215                                            0215
         L     @08,@PC00001                                        0215
         TM    UCLFLUSH(@08),B'00000010'                           0215
         BNZ   @RF00215                                            0215
*           DO;                     /*                               */
*             IF ERR1SW=ON          /*                               */
*               THEN                /*                               */
         TM    ERR1SW,B'10000000'                                  0217
         BNO   @RF00217                                            0217
*               UCLRTNCD=MAX(UCLRTNCD,STOPPTF);/*                    */
         SLR   @08,@08                                             0218
         IC    @08,UCLRTNCD                                        0218
         LA    @15,8                                               0218
         CR    @08,@15                                             0218
         BNL   *+6
         LR    @08,@15                                             0218
         STC   @08,UCLRTNCD                                        0218
*             ELSE                  /*                               */
*               DO;                 /*                               */
         B     @RC00217                                            0219
@RF00217 DS    0H                                                  0220
*                 CALL UCLSTOW;     /* STOW DELETE ENTRYS            */
         BAL   @14,UCLSTOW                                         0220
*                 RFY                                              0221
*                   RTNCODE RSTD;   /*                               */
*                 IF RTNCODE^=ZERO  /*                               */
*                   THEN            /*                               */
         LTR   RTNCODE,RTNCODE                                     0222
         BZ    @RF00222                                            0222
*                   ERR1SW=ON;      /*                               */
         OI    ERR1SW,B'10000000'                                  0223
*                 UCLRTNCD=MAX(UCLRTNCD,RTNCODE);/* SAVE RC          */
@RF00222 SLR   @14,@14                                             0224
         IC    @14,UCLRTNCD                                        0224
         CR    @14,RTNCODE                                         0224
         BNL   *+6
         LR    @14,RTNCODE                                         0224
         STC   @14,UCLRTNCD                                        0224
*                 RFY               /*                               */
*                   RTNCODE UNRSTD;                                0225
*               END;                /*                               */
*                                                                  0226
*             /*******************************************************/
*             /*                                                     */
*             /* UPDATE COMPLETE - ISSUE MESSAGE TELLING USER THE    */
*             /* SCDS ENTRY WAS UPDATED SUCCESSFULLY THEN CONTINUE   */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0227
*             IF ERR1SW=OFF         /* IF ENTRY UPDATED              */
*               THEN                /* ISSUE SUCCESS                 */
@RC00217 TM    ERR1SW,B'10000000'                                  0227
         BNZ   @RF00227                                            0227
*               CALL UCL255I;       /* MSG                           */
         BAL   @14,UCL255I                                         0228
*             ELSE                  /* ELSE ISSUE TERMINATE          */
*               CALL UCL256I;       /* MSG                           */
         B     @RC00227                                            0229
@RF00227 BAL   @14,UCL256I                                         0229
*           END;                    /*                               */
@RC00227 DS    0H                                                  0231
*         PRLFLGS=ZERO;             /*                               */
@RF00215 L     @08,IOPBUFAD(,IOPPTR)                               0231
         MVI   PRLFLGS(@08),X'00'                                  0231
*         PRLBLANK=ON;              /*                               */
         OI    PRLBLANK(@08),B'00100000'                           0232
*         IOPDSID=IOPPRINT;         /*                               */
         MVI   IOPDSID(IOPPTR),X'0B'                               0233
*         IOPFUNCT=IOPWRITE;        /*                               */
         MVI   IOPFUNCT(IOPPTR),X'51'                              0234
*         CALL HMASMIO(HMASMIOP);   /* WRITE BLANK LINE              */
         ST    IOPPTR,@AL00001                                     0235
         L     @15,@CV01390                                        0235
         LA    @01,@AL00001                                        0235
         BALR  @14,@15                                             0235
*         IF IOPRETRN=IOPGOOD       /*                               */
*           THEN                    /*                               */
         CLI   IOPRETRN(IOPPTR),0                                  0236
         BNE   @RF00236                                            0236
*           CALL HMASMIO(HMASMIOP); /* TWO LINES                     */
         ST    IOPPTR,@AL00001                                     0237
         L     @15,@CV01390                                        0237
         LA    @01,@AL00001                                        0237
         BALR  @14,@15                                             0237
*         UCLRTNCD=MAX(UCLRTNCD,IOPRETRN);/*                         */
@RF00236 SLR   @08,@08                                             0238
         IC    @08,UCLRTNCD                                        0238
         SLR   @15,@15                                             0238
         IC    @15,IOPRETRN(,IOPPTR)                               0238
         CR    @08,@15                                             0238
         BNL   *+6
         LR    @08,@15                                             0238
         STC   @08,UCLRTNCD                                        0238
*       END;                        /* END RC RCD AND SCANND         */
*   END;                            /* END READ/SCAN LOOP            */
@RF00195 DS    0H                                                  0240
@DE00183 CLI   UCLRTNCD,16                                         0240
         BNL   @DC00183                                            0240
         TM    UCLTERM,B'00010000'                                 0240
         BNZ   @DC00183                                            0240
         L     @08,@PC00001+4                                      0240
         TM    CRPEOFEN(@08),B'10000000'                           0240
         BZ    @DL00183                                            0240
@DC00183 DS    0H                                                  0241
*                                                                  0241
*   /*****************************************************************/
*   /*                                                               */
*   /* IF LOOP ENDED DUE TO I/O ERROR FROM READ ROUTINE THEN ISSUE   */
*   /* MESSAGE NOW                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0241
*   IF CRPRETRN>CRPEOF              /* IF GREATER THEN EOF           */
*     THEN                          /*                               */
         L     @08,@PC00001+4                                      0241
         CLI   CRPRETRN(@08),4                                     0241
         BNH   @RF00241                                            0241
*     CALL UCL256I;                 /* UPDATE NOT COMPLETE           */
*                                                                  0242
         BAL   @14,UCL256I                                         0242
*   /*****************************************************************/
*   /*                                                               */
*   /* UCL UPDATE COMPLETED SUCCESSFULLY RETURN TO THE CALLER        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0243
*   RETURN CODE(UCLRTNCD);          /* RETURN TO CALLER WITH HIGHEST
*                                      RETURN CODE ENCOUNTERED       */
@RF00241 SLR   @15,@15                                             0243
         IC    @15,UCLRTNCD                                        0243
         L     @13,4(,@13)                                         0243
         L     @14,12(,@13)                                        0243
         LM    @00,@12,20(@13)                                     0243
         BR    @14                                                 0243
         EJECT
*                                                                  0244
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE IS ENTERED WHEN THE ENTRY TYPE KEYWORD IS ENCOUNT*/
*   /* ERED ON THE UCLIN STATEMENT. IT SETS UP THE IOP SO THAT IF THE*/
*   /* ENTRY IS NOT LOCATED THEN THE IOP WILL BE INITIALIZED TO NULL */
*   /* VALUES.                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0244
*RXXX00:                            /*                               */
*   PROCEDURE;                      /*                               */
*                                                                  0244
RXXX00   STM   @14,@12,@SA00002                                    0244
*   /*****************************************************************/
*   /*                                                               */
*   /* PERFORM A LOCATE ON THE SCDS ENTRY NOW THAT WE KNOW THE FULL  */
*   /* NAME.                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0245
*RXXXNAM:                                                          0245
*   ENTRY;                          /*                               */
         B     @EC00245                                            0245
RXXXNAM  STM   @14,@12,@SA00002                                    0245
@EC00245 DS    0H                                                  0246
*   IOPTYPE=IOPSMD;                 /* INDICATE SYSMOD ENTRY         */
         MVI   IOPTYPE(IOPPTR),X'0B'                               0246
*   IOPNAME=SCPSTRNG(1:SCPPMLN);    /*                               */
         MVI   IOPNAME+1(IOPPTR),C' '                              0247
         MVC   IOPNAME+2(6,IOPPTR),IOPNAME+1(IOPPTR)               0247
         L     @08,SCPPTR                                          0247
         LH    @15,SCPPMLN(,@08)                                   0247
         BCTR  @15,0                                               0247
         L     @08,SCPCHAR(,@08)                                   0247
         EX    @15,@SM01504                                        0247
*   PTFNO=IOPNAME;                  /* 7 CHAR ALPHA NUMERIC          */
         MVC   PTFNO(7),IOPNAME(IOPPTR)                            0248
*   IOPDSID=IOPSCDS;                /* SET DSID FOR SCDS             */
         MVI   IOPDSID(IOPPTR),X'21'                               0249
*   IOPFUNCT=IOPLOC;                /* PREPARE FOR LOCATE OPERATION  */
         MVI   IOPFUNCT(IOPPTR),X'03'                              0250
*   CALL HMASMIO(HMASMIOP);         /* PERFORM THE LOCATE            */
*                                                                  0251
         ST    IOPPTR,@AL00001                                     0251
         L     @15,@CV01390                                        0251
         LA    @01,@AL00001                                        0251
         BALR  @14,@15                                             0251
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK AT THIS POINT FOR DELETE ERRORS.                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0252
*   IF IOPRETRN=IOPNTFND            /* IF DEL AND NOT FOUND          */
*     THEN                          /* THE CHECK IS MADE FOR THE   0252
*                                      ERROR CONDITION               */
         CLI   IOPRETRN(IOPPTR),4                                  0252
         BNE   @RF00252                                            0252
*     DO;                           /* WRONG COMBINATION WAS FOUND   */
*       DELERR=ON;                  /* SET SW                        */
         OI    DELERR,B'00000100'                                  0254
*       SCPNORT=ON;                 /* DONT CALL ROUTS               */
         L     @08,SCPPTR                                          0255
         OI    SCPNORT(@08),B'00000100'                            0255
*     END;                                                         0256
*   IF IOPRETRN>IOPNTFND            /* IF SERIOUS ERROR IN LOC       */
*     THEN                          /* YES - ERR                     */
@RF00252 CLI   IOPRETRN(IOPPTR),4                                  0257
         BNH   @RF00257                                            0257
*     DO;                           /*                               */
*       SCPRET=MAX(SCPRET,IOPRETRN);/*                               */
         L     @08,SCPPTR                                          0259
         SLR   @15,@15                                             0259
         IC    @15,SCPRET(,@08)                                    0259
         SLR   @14,@14                                             0259
         IC    @14,IOPRETRN(,IOPPTR)                               0259
         CR    @15,@14                                             0259
         BNL   *+6
         LR    @15,@14                                             0259
         STC   @15,SCPRET(,@08)                                    0259
*       IF IOPRETRN=IOPIOERR        /* CHECK FOR AN I/O ERROR ON THE
*                                      LOCATE OPERATION              */
*         THEN                      /* YES - THERE WAS AN ERROR      */
         CLI   IOPRETRN(IOPPTR),16                                 0260
         BNE   @RF00260                                            0260
*         CALL UCL256I;             /* ISSUE THE MESSAGE             */
         BAL   @14,UCL256I                                         0261
*     END;                          /*                               */
*   RETURN;                         /* RETURN TO HMASMSCN            */
@EL00002 DS    0H                                                  0263
@EF00002 DS    0H                                                  0263
@ER00002 LM    @14,@12,@SA00002                                    0263
         BR    @14                                                 0263
*   END RXXX00;                     /*                               */
         B     @EL00002                                            0264
*                                                                  0265
*   /*****************************************************************/
*   /*                                                               */
*   /* A SYNTAX ERROR WAS DETECTED BY A SCAN EXIT ROUTINE - INDICATE */
*   /* THE ERROR TO THE UCL CONTROL TABLE AND STOP THE SCAN          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0265
*RXTRADTA:                                                         0265
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0265
RXTRADTA DS    0H                                                  0266
*   SCPRETRN=SCPSTOP;               /* STOP SCAN AND RETURN TO CALLER*/
         L     @08,SCPPTR                                          0266
         MVI   SCPRETRN(@08),X'08'                                 0266
*   RETURN;                                                        0267
@EL00003 DS    0H                                                  0267
@EF00003 DS    0H                                                  0267
@ER00003 BR    @14                                                 0267
*   END RXTRADTA;                                                  0268
*                                                                  0268
*   /*****************************************************************/
*   /*                                                               */
*   /* THE TERMINATING UCL CONTROL STATEMENT WAS FOUND BY THE SCAN   */
*   /* ROUTINE. THE CONTROL SWITCH INDICATING SUCCESSFUL COMPLETION  */
*   /* OF THE UPDATE IS SET AND CONTROL IS RETURNED TO THE SCAN      */
*   /* ROUTINE TO ALLOW IT TO FINISH                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0269
*RENDUCL:                                                          0269
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                          */
RENDUCL  DS    0H                                                  0270
*   UCLTERM=ON;                     /* SET ALL OK INDICATOR          */
         OI    UCLTERM,B'00010000'                                 0270
*   RETURN;                         /* SKIP PAST REG LOAD            */
@EL00004 DS    0H                                                  0271
@EF00004 DS    0H                                                  0271
@ER00004 BR    @14                                                 0271
*   END RENDUCL;                    /*                               */
*                                                                  0273
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN THE CONTROL STATEMENT LOOKING FOR A PERIOD (.)           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0273
*FLUSH:                                                            0273
*   PROCEDURE;                      /*                               */
FLUSH    STM   @14,@12,@SA00005                                    0273
*   SCPSRCH=ADFLSHTB;               /* POINT AT THE FLUSH TABLE      */
         L     @08,SCPPTR                                          0274
         MVC   SCPSRCH(4,@08),ADFLSHTB                             0274
*   SCPCHAR=ADDR(CRPBUFFR);         /* POINT TO SAVED RCD            */
         L     @15,@PC00001+4                                      0275
         LA    @15,CRPBUFFR(,@15)                                  0275
         ST    @15,SCPCHAR(,@08)                                   0275
*   CALL HMASMSCN(HMASMSCP);        /* SCAN FOR ENDING .             */
         ST    @08,@AL00001                                        0276
         L     @15,@CV01391                                        0276
         LA    @01,@AL00001                                        0276
         BALR  @14,@15                                             0276
*   ERR1SW=ON;                      /*                               */
         OI    ERR1SW,B'10000000'                                  0277
*   RETURN CODE(0);                 /* RETURN                        */
         SLR   @15,@15                                             0278
         L     @14,@SA00005                                        0278
         LM    @00,@12,@SA00005+8                                  0278
         BR    @14                                                 0278
*   END FLUSH;                      /*                               */
*                                                                  0279
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN EXIT FOR THE FLUSH ROUTINE                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0280
*RFLUSHCH:                                                         0280
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                          */
RFLUSHCH DS    0H                                                  0281
*   IF SCPPMLN=ZERO                 /* TEST FOR ZERO PASS BACK       */
*     THEN                                                         0281
         L     @08,SCPPTR                                          0281
         ICM   @15,3,SCPPMLN(@08)                                  0281
