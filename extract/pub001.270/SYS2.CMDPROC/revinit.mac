PROC 0
/*   CLIST TO ALLOCATE THE REVIEW/RFE PROFILE DATA SET FOR MVS/370 ONLY
SET PROFDSN = &SYSUID..REVPROF
IF &SYSDSN('&PROFDSN') = OK THEN ALLOC F(REVPROF) DA('&PROFDSN')
ELSE DO
CONTROL NOMSG
FREE ATTR(PROFATTR)
CONTROL MSG
ATTR PROFATTR RECFM(F B) LRECL(80) BLKSIZE(2960)
ALLOC F(REVPROF) DA('&PROFDSN') NEW USING(PROFATTR) DIR(10) SP(2 2) TR
FREE ATTR(PROFATTR)
END
