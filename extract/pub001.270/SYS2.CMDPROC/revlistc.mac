PROC 1 DSNAME USERCAT
CONTROL NOPROMPT
/*   COLLECT LISTCAT OUTPUT FOR FULLSCREEN DISPLAY BY RFE.   */
/*   LINE LIMIT OF 300 IS ARBITRARY - INCREASE IF REQUIRED.  */
SET SYSOUTTRAP = 300
LISTCAT ENTRIES('&DSNAME') ALL
IF &USERCAT = USERCAT THEN +
LISTCAT ENTRIES('&DSNAME') ALL CATALOG('&DSNAME')
SET SYSOUTTRAP = 0
SET CATLINES = &SYSOUTLINE
SET N = 0
LOOP: SET N = &N + 1
IF &N > &CATLINES THEN END
SET CATMSG = &&SYSOUTLINE&N
REVCATCH &STR(&CATMSG)
GOTO LOOP
