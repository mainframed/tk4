        PROC 1 DATASET VOLUME()
        CONTROL NOMSG
        FREE FILE(X)
        CONTROL MSG
        IF &LENGTH(&VOLUME) NE 0 THEN +
          ALLOC F(X) DA(&DATASET) UNIT(SYSDA) VOL(&VOLUME) SHR
        IF &LENGTH(&VOLUME) EQ 0 THEN ALLOC FILE(X) DA(&DATASET) SHR
        DDUX
        FREE FILE(X)
        EXIT
