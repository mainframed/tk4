//E0115DMS JOB EDPCSBS1,'SASD',MSGCLASS=X,CLASS=1,NOTIFY=ED115DM
//*
//SAS     EXEC SAS,OPTIONS=NODATE
//DATLIB  DD DSN=SYS1.MAN1,DISP=SHR
//SYSIN   DD *
TITLE SMF 10;
  DATA;
  RETAIN V;
  INFILE DATLIB JFCB=JF MISSOVER;
  IF _N_ = 1 THEN DO;
                  INPUT @;
                  IF INDEX(JF,'SYS1.MAN') ^=0 THEN V=4; ELSE V=0;
                  END;
  IF V=4 THEN
      INPUT    SMFXXLEN PIB2. SMFXXSEG PIB2. @ ;
  INPUT    SMFXXFLG PIB1. SMFXXRTY PIB1.
                       @;
  IF SMFXXRTY = 212 THEN DO;
                        *PUT '212 DETECTED - SKIPPING';
                         DELETE;
                         END;
  INPUT    TIMEOUT SMFSTAMP8.
      SMFXXSID $CHAR4. @;
  IF SMFXXRTY = 10;
  INPUT SMF10JBN $CHAR8. SMF10RST PIB4. SMF10RSD PD4.
        SMF10JIF $CHAR8.
          @ ;
  INPUT SMF10LN PIB2. @ ;
  N = (SMF10LN -2)/ 4;
  PUT SMFXXRTY TIMEOUT SMFXXSID SMF10JBN SMF10RST SMF10RSD SMF10JIF
                               SMF10LN
            ;
  IF N = 0 THEN RETURN;
  DO I = 0 TO (N -1) ;
   INPUT SMF10AD1 PIB1. SMF10AD2 PIB1. SMF10AD3 PIB1. SMF10AD4 PIB1.
   @ ;
   PUT SMF10AD1 SMF10AD2 SMF10AD3 SMF10AD4 HEX2.;
  END;
