//E0115DMS JOB EDPCSBS1,'SASD',MSGCLASS=X,CLASS=1,NOTIFY=ED115DM
//*
//SAS     EXEC SAS,OPTIONS=NODATE
//DATLIB  DD DSN=SYS1.MAN1,DISP=SHR
//SYSIN   DD *
TITLE SMF 9;
  DATA;
  RETAIN V;
  INFILE DATLIB JFCB=JF MISSOVER;
  IF _N_ = 1 THEN DO;
                  INPUT @;
                  IF INDEX(JF,'SYS1.MAN') ^=0 THEN V=4; ELSE V=0;
                  END;
  IF V=4 THEN
      INPUT    SMFXXLEN PIB2. SMFXXSEG PIB2. @ ;
  INPUT    SMFXXFLG PIB1. SMFXXRTY PIB1.
                       @;
  IF SMFXXRTY = 212 THEN DO;
                        *PUT '212 DETECTED - SKIPPING';
                         DELETE;
                         END;
  INPUT    TIMEOUT SMFSTAMP8.
      SMFXXSID $CHAR4. @;
  IF SMFXXRTY = 9;
  INPUT SMF9LENN PIB2. @ ;
  N = (SMF9LENN -2)/ 4;
  PUT SMFXXRTY TIMEOUT DATETIME. SMFXXSID @18 SMF9LENN @25 N
            ;
  IF N = 0 THEN RETURN;
  DO I = 0 TO (N -1) ;
   INPUT SMF9VAD1 PIB1. SMF9VAD2 PIB1. SMF9VAD3 PIB1. SMF9VAD4 PIB1.
   @ ;
   PUT SMF9VAD1 SMF9VAD2 SMF9VAD3 SMF9VAD4 HEX2.;
  END;
