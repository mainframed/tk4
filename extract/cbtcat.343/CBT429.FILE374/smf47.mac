//ED115DMA JOB EDPCSBS1,MSGCLASS=X,CLASS=3,NOTIFY=ED115DM
//*
//  EXEC SAS606
//DATLIB DD DSN=EDH1.PROD.SMFDUMPW.HOLD,DISP=SHR
//*ATLIB DD DSN=SYS1.MAN1,DISP=SHR
//SYSIN DD *
DATA SMF47 (KEEP =
       SMF47SBS         SMF47RSV       SMF47LRR
       SMF47EVT         SMF47LN1       SMF47RMT
       SMF47LIN         SMF47PSW
       SMF47LN2         SMF47MSG
       SMF47RVJ         SMF47RVU
                  );
  RETAIN V;
  INFILE DATLIB JFCB=JF;
  IF _N_ = 1 THEN DO;
                  INPUT @;
                  IF INDEX(JF,'SYS1.MAN') ^=0 THEN V=4; ELSE V=0;
                  END;
  IF V=4 THEN
      INPUT    SMFXXLEN PIB2. SMFXXSEG PIB2. @ ;
  INPUT    SMFXXFLG PIB1. SMFXXRTY PIB1.
                       @;
  IF SMFXXRTY = 212 THEN DO;
                        *PUT '212 DETECTED - SKIPPING';
                         DELETE;
                         END;
  INPUT    TIMEOUT SMFSTAMP8.
      SMFXXSID $CHAR4. @;
  IF SMFXXRTY = 47;
 INPUT SMF47SBS PIB2.   SMF47RSV PIB2. SMF47LRR PIB2.
       SMF47EVT PIB2.   SMF47LN1 PIB2. SMF47RMT $CHAR8.
       SMF47LIN $CHAR8. SMF47PSW $CHAR8. @;
 INPUT SMF47LN2 PIB2.   SMF47MSG $CHAR36.
       SMF47RVJ PIB8.   SMF47RVU PIB4.;
RETURN;
PROC PRINT;
FORMAT TIMEOUT DATETIME.;
