//C4SYSDMR JOB EDPCSBS1,'PDS1',MSGCLASS=X,CLASS=0,NOTIFY=ED115DM
//*
//SAS     EXEC EDSAS609,OPTIONS='NODATE LINESIZE=80 HSWORK'
//PDS     DD DSN=ED115DM.PZ,DISP=SHR,DCB=DSORG=PS
//SYSIN   DD *
DATA DIRECTRY;
   INFILE PDS RECFM=F  LRECL=256 BLKSIZE=256; COL=3;
   *ILE OREC;
   INPUT COUNT PIB2. @;  IF COUNT < 14 THEN STOP;
   *UT _INFILE_;
   DO WHILE (COL < COUNT);
      INPUT @COL MEMBER $8. +3 IND PIB1. @;
      COL = COL +12 + 2*MOD(IND,32);
      OUTPUT;
           PUT MEMBER $CHAR8. ;
   END;
RETURN;
PROC SORT;   BY  MEMBER;
PROC PRINT;  VAR MEMBER;
