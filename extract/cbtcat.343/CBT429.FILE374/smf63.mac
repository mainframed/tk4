//E0115DMS JOB EDPCSBS1,'SMF63',MSGCLASS=X,CLASS=L,NOTIFY=ED115DM
//*
//SAS     EXEC EDSAS608,OPTIONS=NODATE
//DATLIB  DD DSN=SYS1.MAN4,DISP=SHR
//SYSIN   DD *
TITLE SMF 63;
  DATA D63 (KEEP =
           TIMEOUT
      SMFXXSID
        SMF63SMI             SMF63JBN               SMF63RST
        SMF63RSD             SMF63UIF               SMF63FDT
        SMF63TYP             SMF63NSZ               SMF63OSZ
        SMF63CNM             SMF63ENM
  );
  RETAIN V;
  INFILE DATLIB JFCB=JF MISSOVER;
  IF _N_ = 1 THEN DO;
                  INPUT @;
                  IF INDEX(JF,'SYS1.MAN') ^=0 THEN V=4; ELSE V=0;
                  END;
  IF V=4 THEN
      INPUT    SMFXXLEN PIB2. SMFXXSEG PIB2. @ ;
  INPUT    SMFXXFLG PIB1. SMFXXRTY PIB1.
                       @;
  IF SMFXXRTY = 212 THEN DO;
                        *PUT '212 DETECTED - SKIPPING';
                         DELETE;
                         END;
  INPUT    TIMEOUT SMFSTAMP8.
      SMFXXSID $CHAR4. @;
  IF SMFXXRTY = 63;
  INPUT SMF63SMI $CHAR2.     SMF63JBN  $CHAR8.      SMF63RST PIB4.
        SMF63RSD PD4.        SMF63UIF  $CHAR8.      SMF63FDT PIB1.
        SMF63TYP PIB1.       SMF63NSZ  PIB2.        SMF63OSZ PIB2.
        SMF63CNM $CHAR44.    SMF63ENM  $CHAR44.
        @ ;
RETURN;
PROC PRINT;
