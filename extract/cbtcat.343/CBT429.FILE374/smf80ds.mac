//C4SYSDMS JOB EDPCSBS1,'RACF',MSGCLASS=X,CLASS=T,NOTIFY=ED115DM
//*
//SAS     EXEC SAS,OPTIONS=NODATE
//*ATLIB  DD DSN=C4H1.PROD.SMFDUMPW,DISP=OLD
//DATLIB  DD DSN=SYS1.MAN1,DISP=SHR
//SYSIN   DD *
TITLE SMF 80;
  DATA _NULL_;
  V=4;    *0 NVSAM, 4 VSAM;
  INFILE DATLIB MISSOVER LENGTH=LEN;
  INPUT @1+V SMFXXFLG PIB1.  SMFXXRTY PIB1.    DATTIM SMFSTAMP8.
        SMFXXSID $CHAR4. @ ;
  IF SMFXXRTY = 80;
  I=15; J=LEN-14;
LOOP:
  IF J >= 80 THEN
  INPUT @I STR $80. @; ELSE
  INPUT @I STR $VARYING80. J @;
  I=I+60;
  J=J-60;
  T=INDEX(STR,'EBDB.');
  IF T ^= 0 THEN DO;
                 PUT DATTIM DATETIME.;
                 PUT _INFILE_;
                 END;
  IF J>0 THEN GOTO LOOP;
DELETE;
