//E0115DMS JOB EDPCSBS1,'SASD',MSGCLASS=X,CLASS=1,NOTIFY=ED115DM
//*
//SAS     EXEC SAS,OPTIONS=NODATE
//DATLIB  DD DSN=SYS1.MAN1,DISP=SHR
//SMF     DD DSN=ED115DM.PROD.SAS(SMF),DISP=SHR
//SYSIN   DD *
TITLE SMF 81;
  DATA;
  %INCLUDE SMF;
  IF SMFXXRTY = 81;
  INPUT     SMF81RDS $CHAR44. SMF81RVL $CHAR6.  SMF81RUN $CHAR3.
            SMF81UDS $CHAR44. SMF81UVL $CHAR6.  SMF81OPT PIB1.
            SMF81OP2 PIB1.    SMF81OP3 PIB1.    SMF81AOP PIB1.
            SMF81A02 PIB1.    SMF81TMO PIB1.    SMF81PIV PIB1.
            SMF81REL PIB2.    SMF81CNT PIB2.    SMF81VER PIB1.
            SMF81QL  $CHAR8.
            @ ;
    PUT     SMF81RDS   @50    SMF81RVL   @60    SMF81RUN ;
    PUT     SMF81UDS   @50    SMF81UVL   @60    SMF81OPT +1
            SMF81OP2   +1     SMF81OP3   +1     SMF81AOP +1
            SMF81A02   +1     SMF81TMO   +1     SMF81PIV +1
            SMF81REL   +1     SMF81CNT   +1     SMF81VER +1
            SMF81QL
              ;
OFF = SMF81REL +1 ;
IF SMF81CNT > 0 THEN DO;
DO I=0 TO (SMF81CNT-1) ;
     INPUT @OFF SMF81DTP PIB1.   SMF81DLN PIB1.  @ ;
     PUT        SMF81DTP         SMF81DLN ;
     OFF = OFF + 2 + SMF81DLN ;
END;
END;
