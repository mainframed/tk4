//C4SYSDM0 JOB EDPCSBS1,'RMF70',MSGCLASS=X,CLASS=L,NOTIFY=ED115DM
//** XA FORMAT
//*
//SAS     EXEC EDSAS607,OPTIONS=NODATE
//DS      DD DISP=OLD,DSN=ED115DM.CPU
//SYSIN   DD *
DATA;
SET DS.CDATA;
C4DATE = DATEPART(TIMEOUT);
RETURN;
PROC SORT OUT=C; BY C4DATE DESCENDING CPUBSYPC;
DATA;
RETAIN N 0;
SET; BY C4DATE;
IF FIRST.C4DATE THEN N=0;
N+1;
IF N> 50 THEN DELETE;
RETURN;
PROC PRINT; BY C4DATE;
FORMAT TIMEOUT DATETIME.;
FORMAT C4DATE  WEEKDATE.;
FORMAT SMF70SER HEX6.   ;
