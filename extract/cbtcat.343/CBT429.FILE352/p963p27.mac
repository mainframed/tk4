1/*----------------------------------------------------------------*/
 /*  OPEN AND CHECK FOR EOF -                                      */
 /*----------------------------------------------------------------*/
     ON ENDFILE(INLOG)   GO TO EOF20;              /* EOF F020     */
     ON ENDFILE(INSCR)   GO TO EOF15;              /* EOF F015     */
     ON ENDFILE(INHIST)  GO TO EOFHIST;            /* EOF F120     */
     OPEN FILE (INLOG) INPUT ,
          FILE (INSCR) INPUT ,
          FILE (NEWSCR) OUTPUT,
          FILE (OLDSCR) OUTPUT,
          FILE (INHIST) INPUT ,
          FILE (OUTHIST) OUTPUT;
 /*----------------------------------------------------------------*/
 /*  READ SCRATCH HEADER - WRITE OLD CHANGE DATE WRITE NEW SCR HEAD*/
 /*----------------------------------------------------------------*/
     READ FILE(INSCR)  INTO (SCRPARM);    /* READ INSCR DATE RECORD*/
     IF SCR_HEAD ^= 'SCRATCH='            /* IS THIS A HEADER CARD */
         THEN GO TO ERRHEAD;              /* NO THEN ABEND         */
     WRITE FILE(OLDSCR) FROM(SCRPARM);  /* WRITE OUTPUT-OLDSCR DATE*/
      SCR_DATE = RUN_DATE            ;  /* CREATE NOW SCR DATE CARD*/
     WRITE FILE(NEWSCR) FROM(SCRPARM);  /* WRITE OUTPUT-NEWSCR DATE*/
1/*----------------------------------------------------------------*/
 /* CLEAR HOLD AND COUND AREA                                      */
 /*----------------------------------------------------------------*/
 CLEAR:
     DSN_HOLD1  =  '  ' ;               /* DO IT AGAIN - START     */
     VOL_HOLD1  =  '  ' ;
     DSN_HOLD2  =  '  ' ;
     VOL_HOLD2  =  '  ' ;
     GDG_CNT    =  0    ;

 /*----------------------------------------------------------------*/
 /* CHECK LOG DATASET FOR OK TO SCRATCH RECORD                     */
 /*----------------------------------------------------------------*/
 LOG:
     IF DSNAME  =  HIGH_VAL20  THEN       /* CHECK FOR EOF         */
              GO  TO  EOF20;

     CALL READLOG  ;                      /* READ DATASET          */

     DO  I  =  44  TO  1  BY - 1;         /*SET UP FOR POINTER     */

     IF  UCAT     =  'C'                  /* IS DS UNCATALOGED     */
          &
         SCRFLAG  =  'S'                  /*  AND  FLAGED FOR SCR  */
          &
         SUBSTR(DSNAME,I - 6,2)  =  '.G'  /*  AND  A GDG           */
          &
         SUBSTR(DSNAME,I,3)     =  'V00'
          &
         DEVICE   =  '3330V   '            /* AND ON MSS           */
              THEN  DO;
                       K  =  I - 5;         /* HOLD POINTER        */
                       GO TO MOVEDSN;       /* GO MOVE DS AND VOL  */
                    END;
         END;

     IF   (UCAT  ^= 'C')  THEN DO;   /* HIST UPDATE*/
                O = INDEX(DSNAME,'FCI1');
                P = INDEX(DSNAME,'FCI2');
                Q = INDEX(DSNAME,'FCI3');
                R = INDEX(DSNAME,'IMSP');
     IF ( R > 0 ) × ( O > 0 ) × ( P > 0 ) × ( Q > 0 )
                        THEN CALL HIST;
                         END;
     IF GDG_CNT  =  0  THEN  GO  TO  LOG;   /* NO SCR OR HOLD LOOP */

     IF  SUBSTR(DSNAME,1,K)  ^=  SUBSTR(DSN_HOLD1,1,K) THEN
                           GO TO CLEAR;     /* NO CAT IN GDG LOOP  */
     IF  SCRFLAG  =  'S'  THEN  GO TO LOG;  /* CAT BUT SCR LOOP    */

      GO TO SCR;                            /* READY GO TO IT      */

1/*----------------------------------------------------------------*/
 /* FOUND RECORD FOR SCRATCH CHECK AND HOLD IT                     */
 /*----------------------------------------------------------------*/
 MOVEDSN:
     IF  GDG_CNT  ^=  0     &   /* CHK SCR DS EQUAL TO HOLD       */
         SUBSTR(DSNAME,1,K) ^=  SUBSTR(DSN_HOLD1,1,K)  THEN
           DO;
            DSN_HOLD1  =  '  ' ;   /* HOLD DS HAS NO CAT GDG       */
            VOL_HOLD1  =  '  ' ;   /* CLEAR IT OUT                 */
            DSN_HOLD2  =  '  ' ;
            VOL_HOLD2  =  '  ' ;
            GDG_CNT    =  0    ;
           END;

     IF  SUBSTR(DSNAME,1,K)  =  SUBSTR(DSN_HOLD3,1,K)  THEN
            GO  TO  LOG;           /* STILL HAVE MORE THAN TWO     */

     DSN_HOLD3  =  '  '  ;                  /* OK UNCAT DS IS OK   */
     GDG_CNT  =  GDG_CNT  +  1  ;  /* FOUND DS READY FOR SCR       */
     IF  GDG_CNT  >  2   THEN                  /* CAN ONLY HAVE TWO*/
         DO;
            DSN_HOLD3  =  DSNAME;          /*CHECK IF NEXT DS EQUAL*/
            GO TO CLEAR;
         END;
     IF  GDG_CNT  =  1
            THEN  DO;
                   DSN_HOLD1  =  DSNAME  ;     /* MOVE TO HOLD 1   */
                   VOL_HOLD1  =  VOLUME  ;
                  END;
            ELSE DO;
                   DSN_HOLD2  =  DSNAME  ;     /* MOVE TO HOLD 2   */
                   VOL_HOLD2  =  VOLUME  ;
                  END;
         GO TO LOG;                            /* GO LOOP TO LOG   */

1/*----------------------------------------------------------------*/
 /* FOUND DSNAME TO SCRATCH NOW MATCH TO IN SCRATCH DATASET        */
 /*----------------------------------------------------------------*/
 SCR:
     IF ERR_CNT  >  0   THEN              /* CHECK FOR READ ERROR  */
              ERR_CNT =  0;
      ELSE   DO;
          IF IS_DSN  ^=  HIGH_VAL15  THEN    /* NOT EOF SCR ?      */
              CALL  READSCR   ;              /* OK READ SCR        */
             END;
     IF  DSN_HOLD1  <  IS_DSN  THEN  GO  TO  WRTERR; /*NO SCR CARD */
     IF  DSN_HOLD1   =  IS_DSN                       /*SCR NOW     */
             &
         VOL_HOLD1   =  IS_VOL
            THEN  DO;
                N_DSN      =   IS_DSN     ;          /* MOVE WRITE */
                N_VOL      =   IS_VOL     ;
                N_FLAG     =   IS_FLAG    ;
                N_BILLTO   =   IS_BILLTO  ;
                N_LASTUSE  =   IS_LASTUSE ;
                N_USER     =   IS_USER    ;
                N_DEVICE   =   IS_DEVICE  ;
                WRITE FILE(NEWSCR) FROM(NEW_REC);
                GO TO NEXT;                          /*CHECK HOLD2 */
               END;
            ELSE  DO;                                /*SCR LATTER  */
                O_DSN      =   IS_DSN     ;
                O_VOL      =   IS_VOL     ;
                O_FLAG     =   IS_FLAG    ;
                O_BILLTO   =   IS_BILLTO  ;
                O_LASTUSE  =   IS_LASTUSE ;
                O_USER     =   IS_USER    ;
                O_DEVICE   =   IS_DEVICE  ;
                WRITE FILE(OLDSCR) FROM(OLD_REC);
                GO  TO  SCR  ;                       /*LOOP        */
               END;

1NEXT:
     IF  GDG_CNT  ^=  2  THEN   GO  TO  CLEAR;     /* HOLD 2 ?     */
          IF IS_DSN  ^=  HIGH_VAL15  THEN    /* NOT EOF SCR ?      */
              CALL  READSCR   ;              /* OK READ SCR        */

 HOLD2:                       /*USE WHEN FIRST READ HAD NO MATCH   */
     IF  DSN_HOLD2  =  IS_DSN                      /* MATCH OK     */
             &
         VOL_HOLD2  =  IS_VOL THEN  DO;
             N_DSN      =   IS_DSN     ;
             N_VOL      =   IS_VOL     ;
             N_FLAG     =   IS_FLAG    ;
             N_BILLTO   =   IS_BILLTO  ;
             N_LASTUSE  =   IS_LASTUSE ;
             N_USER     =   IS_USER    ;
             N_DEVICE   =   IS_DEVICE  ;
            WRITE FILE(NEWSCR) FROM(NEW_REC);      /*WRITE NEW SCR */
            GO  TO  CLEAR  ;                       /* START OVER   */
           END;

 /*----LOG SCR HAS NO MATCH ON IN-SCR CARD PUT TO SYSOUT-----------*/

            PUT FILE(SYSPRINT) EDIT(' DSN=',DSN_HOLD2,' VOL=',
             VOL_HOLD2,' NOT FOUND IN SCRATCH CARD DATASET  F015')
                 (SKIP(2),X(5),A,A(44),A,A(6),A);
            ERR_CNT = ERR_CNT + 1  ;   /*DONT READ F15 NEXT TIME   */
            GO TO CLEAR  ;  /*  BETTER LUCK NEXT TIME - LOOP  */


 /*----------------------------------------------------------------*/
 /* WRITE ERROR AND CHECK FOR SECOND DATASET TO SCRATCH            */
 /*----LOG SCR HAS NO MATCH ON IN-SCR CARD PUT TO SYSOUT-----------*/
 /*----------------------------------------------------------------*/
 WRTERR:

     PUT FILE(SYSPRINT) EDIT(' DSN=',DSN_HOLD1,' VOL=',
       VOL_HOLD1,' NOT FOUND IN SCRATCH CARD DATASET  F015')
           (SKIP(2),X(5),A,A(44),A,A(6),A);
       IF  GDG_CNT  =  2  THEN  GO  TO  HOLD2; /*    HOLD 2 ?  */
       ERR_CNT = ERR_CNT + 1  ;
       GO TO CLEAR  ;                          /* LOOP NO HOLD 2  */

1/*----------------------------------------------------------------*/
 /* READ SCRATCH DATASET CHECK FOR ASCENDING ORDER                 */
 /*----------------------------------------------------------------*/
 READSCR: PROC;
 READ15:

     READ FILE(INSCR) INTO (IN_SCR);      /* READ SCRATCH  RECORD  */

     IF IS_DSN  <  IS_HOLD  THEN          /* CHECK IF ASCENDING    */
              GO  TO  ERRF15;             /* NO GO - ABEND         */

     IS_HOLD  =  IS_DSN;                  /* LOOKS GOOD            */

 END READSCR;
 /*----------------------------------------------------------------*/
 /* READ LOG DATASET CHECK FOR ASCENDING ORDER                     */
 /*----------------------------------------------------------------*/
 READLOG: PROC;
 READ20:

     READ FILE(INLOG) INTO (IN_LOG);      /* READ LOG RECORD       */

     IF DSNAME  <  IL_HOLD  THEN          /* CHECK IF ASCENDING    */
              GO  TO  ERRF20;             /* NO GO - ABEND         */

     IL_HOLD  =  DSNAME;                  /* LOOKS GOOD            */

 END READLOG;
1/*----------------------------------------------------------------*/
 /* END OF FILE INLOG - CHECK EOF INSCR - NO READ REST INSCR       */
 /*----------------------------------------------------------------*/
 EOF15:
     IF DSNAME = HIGH_VAL20 THEN          /* CHECK INLOG EOF       */
           GO TO EOJ;

     IS_DSN     =  HIGH_VAL15 ;           /* MOVE HIGH VALUE TO SCR*/
           GO TO WRTERR ;                 /* WRT ERROR-NO MATCH F20*/

 /*----------------------------------------------------------------*/
 /* END OF FILE INLOG - CHECK EOF INSCR - NO READ REST INSCR       */
 /*----------------------------------------------------------------*/
 EOF20:
     IF IS_DSN = HIGH_VAL15 THEN          /* CHECK INSCR EOF       */
           GO TO EOJ;

     DSN_HOLD1  =  HIGH_VAL20 ;           /* MOVE HIGH VALUE TO LOG*/
     DSNAME     =  HIGH_VAL20 ;
           GO TO SCR   ;              /*GO READ AND WRITE OLDSCR*/

