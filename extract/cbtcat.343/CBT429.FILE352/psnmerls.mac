*          DATA SET PSNMERLS   AT LEVEL 002 AS OF 10/30/80
         MACRO
         PSNMERLS &TYPE=EQU
.*
         GBLC  &PSNGTYP
&PSNGTYP SETC  '&TYPE'            PASS VALUE ON TO OTHERS
.*
BADNUMB  PSNMERMS  'INVALID ERROR MESSAGE GIVEN',                      X
               OPTION=(ERR,SNAP,ABEND),SEV=20
ERRSNAPR PSNMERMS   'SNAP REQUESTED BY CALLER',                        X
               OPTION=(ERR,SNAP),SEV=0
ERRNOTSP PSNMERMS   'FUNCTION IS NOT SUPPORTED',                       X
               OPTION=(ERR,SNAP),SEV=12
ERRNOPEN PSNMERMS   'DDNAME / DID NOT OPEN',                           X
               OPTION=(ERR,ABEND,INSERT),SEV=12
ERRNOOUT PSNMERMS   'NO OUTPUT DD''S ALLOCATED',                       X
               OPTION=(ERR,ABEND),SEV=12
ERRNOTP  PSNMERMS   'MEMBER / NOT PROCESSED',                          X
               OPTION=(ERR,INSERT),SEV=4
ERRMNOTF PSNMERMS   'MEMBER / NOT FOUND',                              X
               OPTION=(ERR,INSERT),SEV=8,POINT=3
ERRIO    PSNMERMS   'I-O ERROR - MEMBER-/ ON DDNAME / DURING /',       X
               OPTION=(ERR,INSERT,SNAP),SEV=8,POINT=5
ERREMPTY PSNMERMS   'PROC HAS NO ENTRIES - EMPTY MEMBER',              X
               OPTION=(ERR),SEV=8,POINT=10
ERRNPR1  PSNMERMS   'PROC STATEMENT NOT FIRST ENTRY IN MEMBER',        X
               OPTION=(ERR),SEV=4,POINT=4
ERRNONAM PSNMERMS   'NO NAME ON THE PROC STATEMENT',                   X
               OPTION=(ERR),SEV=4,POINT=3
ERRNJCL  PSNMERMS   'THIS IS NOT A JCL STATEMENT',                     X
               OPTION=(CALLER,SNAP,ERR),SEV=8,POINT=8
ERRBLANK PSNMERMS   'ENTIRE LINE IS BLANK',                            X
               OPTION=(CALLER,SNAP,ERR),SEV=8,POINT=7
ERRNGT8  PSNMERMS   'NAME ON FIRST ENTRY OVER 8 CHARACTERS',           X
               OPTION=(CALLER,SNAP,ERR),SEV=8,POINT=5
ERR2P1L  PSNMERMS   'MULTIPLE SUBSTITUTIONS ON ONE LINE',              X
               OPTION=(ERR),SEV=4,POINT=2
ERREOF   PSNMERMS   'PREMATURE END DURING / PROCESSING',               X
               OPTION=(SNAP,ERR,INSERT),SEV=8,POINT=4
ERRBAD1  PSNMERMS   'UNIDENTIFIED FIRST CARD IN MEMBER',               X
               OPTION=(SNAP,ERR),SEV=8,POINT=3
ERRISCAN PSNMERMS   'INTERNAL SCAN ERROR AT /',                        X
               OPTION=(ERR,INSERT,SNAP,CALLER),SEV=12,POINT=4
ERRSYMSB PSNMERMS   'NO EQUAL SIGN IN SYMBOLIC SUBSTITUTION SCAN',     X
               OPTION=(ERR),SEV=8,POINT=4
ERRNOTYP PSNMERMS   'TYPE CODE CANNOT BE DETERMINED',                  X
               OPTION=(ERR,SNAP),SEV=8,POINT=5
ERRSTOPM PSNMERMS   'TERMINATED PROCESSING ON THIS MEMBER',            X
               OPTION=(ERR),SEV=8
ERRBADRC PSNMERMS   'INVALID RETURN CODE - INTERNAL ERROR',            X
               OPTION=(ERR,SNAP,CALLER),SEV=12
ERRPNAM  PSNMERMS   'MEMBER NAME / DOESN''T MATCH PROC NAME /',        X
               OPTION=(ERR,INSERT),SEV=4,POINT=4
ERRPKEYL PSNMERMS   'KEYWORD / IN PROC IS TOO LONG',                   X
               OPTION=(SNAP,ERR,INSERT),SEV=4,POINT=5
ERRNOEQL PSNMERMS   '/ HAD NO EQUAL SIGN IN / STATEMENT',              X
               OPTION=(SNAP,ERR,INSERT),SEV=4,POINT=4
ERRNOKEY PSNMERMS   'LINE HAS NO KEYWORD',                             X
               OPTION=(ERR,SNAP),SEV=8,POINT=6
ERRTBLFL PSNMERMS   '/ TABLE IS FULL - NEED LARGER WORKAREA',          X
               OPTION=(SNAP,ERR,INSERT),SEV=8
ERRPOINT PSNMERMS   'MEMBER / HAS ACCUMULATED / ERROR POINTS',         X
               OPTION=(ERR,INSERT),SEV=0,POINT=0
ERRSYLNG PSNMERMS   'SYMBOLIC SUBSTITUTOR / IS TOO LONG',              X
               OPTION=(ERR,INSERT,SNAP),SEV=8,POINT=4
ERRISUBS PSNMERMS   'INTERNAL SUBSTITUTION ERROR AT / ',               X
               OPTION=(ERR,INSERT,SNAP,CALLER),SEV=8
ERRNOSUB PSNMERMS   'WARNING: SYMBOLIC / NOT FOUND IN PROC STATEMENT', X
               OPTION=(ERR,INSERT),SEV=4,POINT=6
ERRREQDF PSNMERMS   '/ IS REQUIRED IN / AND NOT FOUND',                X
               OPTION=(ERR,INSERT),SEV=4,POINT=4
ERRNOCMT PSNMERMS   'COMMENT BLOCK REQUIRED FOR / - NOT FOUND',        X
               OPTION=(ERR,INSERT),SEV=4,POINT=2
ERRBADKY PSNMERMS   'UNRECOGNIZED KEYWORD / IN / STATEMENT',           X
               OPTION=(ERR,INSERT,SNAP),SEV=4,POINT=4
ERRHSYM  PSNMERMS   'NON-STANDARD SYMBOLIC / IN / STATEMENT',          X
               OPTION=(ERR,INSERT),SEV=4,POINT=3
ERRNONST PSNMERMS   '/ VALUE OF / IS NON-STANDARD NAMING',             X
               OPTION=(ERR,INSERT),SEV=4,POINT=2
ERRCOMNS PSNMERMS   'COMMENT PRECEEDING / IS NON-STANDARD',            X
               OPTION=(ERR,INSERT),SEV=4,POINT=1
ERRQUIT  PSNMERMS   'RUN TERMINATED PREMATURELY DUE TO ERROR(S)',      X
               OPTION=(ERR),SEV=8,POINT=0
ERRPARM  PSNMERMS   'INVALID / IN CTL-DATA :/',                        X
               OPTION=(ERR,INSERT),SEV=8,POINT=0                    NTL
ERRVALUE PSNMERMS   '/ IS INVALID VALUE IN /',                         X
               OPTION=(ERR,INSERT),SEV=4,POINT=2
ERRINTSC PSNMERMS   'INTERNAL SCAN SUBLIST ERROR AT /',                X
               OPTION=(ERR,SNAP,INSERT),SEV=8,POINT=0
ERR2SUB  PSNMERMS   'DOUBLE SUBSTRING ERROR AT /',                     X
               OPTION=(ERR,SNAP,INSERT),SEV=8,POINT=0
ERRVOLUM PSNMERMS   'USE VOL, NOT VOLUME AS THE KEYWORD',              X
               OPTION=(ERR),SEV=4,POINT=5
ERRDSNAM PSNMERMS   'USE DSN, NOT DSNAME AS THE KEYWORD',              X
               OPTION=(ERR),SEV=4,POINT=5
ERRSTDFB PSNMERMS   'STANDARDS FORBID / IN /',                         X
               OPTION=(ERR,INSERT),SEV=4,POINT=3
ERRMISAL PSNMERMS   'MISALIGNED KEYWORD / IN COLUMN /',                X
               OPTION=(ERR,INSERT),SEV=4,POINT=2
ERRDSN1  PSNMERMS   'INVALID DATASET NAME :',                          X
               OPTION=(ERR),SEV=4,POINT=4                           NTL
ERRDSN2  PSNMERMS   'DSN=/',                                           X
               OPTION=(ERR,INSERT),SEV=4,POINT=0
ERRSPBK  PSNMERMS   'BLOCKSIZE DOES NOT MATCH SPACE ALLOCATION',       X
               OPTION=(ERR),SEV=4,POINT=5
ERRMSS   PSNMERMS   'NONSTANDARD MASS STORE ALLOCATION',               X
               OPTION=(ERR),SEV=4,POINT=3
ERRLIMIT PSNMERMS   'VALUE FOR / OUTSIDE LIMITS',                      X
               OPTION=(ERR,INSERT),SEV=4,POINT=3
ERRSPSEC PSNMERMS   'SECONDARY ALLOC GT 10% OF PRIMARY FOR /',         X
               OPTION=(ERR,INSERT),SEV=4,POINT=2
ERRTBLSY PSNMERMS   'TABLE SYNCHRONIZATION ERROR IN /',                X
               OPTION=(ERR,INSERT,SNAP),SEV=8,POINT=1
ERRCOMBK PSNMERMS   'ALL COMMENTS MUST BE IN COMMENT BLOCK',           X
               OPTION=(ERR),SEV=4,POINT=2
ERRDSN3  PSNMERMS   'DSN NOT OWNED BY THIS SYSTEM .. CHK ACF2 RULES.', X
               OPTION=(ERR),SEV=4,POINT=0                           NTL
.EXIT    ANOP
         SPACE 2
         MEND
