)ATTR DEFAULT(Ö^_)
 Ö TYPE(TEXT) INTENS(HIGH) PAD(' ') JUST(LEFT)
 ^ TYPE(TEXT) INTENS(LOW)  PAD(' ') JUST(LEFT)
)BODY
^=====Semi-interactiveÖVTOC Zap^ (generates a JCL) ====== &ZJDATE &ZTIME
^ ===>_ZCMD
Ö             &VTOCMSG
^  DSN ====> _VTOCRDSN                                       ^VOLSER =>
^ +2C _Z ^           :Format DSCB identifier   CCHHR of that DSCB : &CCH
^ +2D _DSSN        ^ :Data set serial number
^ +33 _Z   ^         :Volume sequence number
^ +35 _CREDT ^       :Creation date           +38 _EXPDT ^  :Expiration
^ +3B _Z ^           :Extents on volume       +3C _Z ^   :Bytes in last
^ +3E _SYSCD                     ^   :S y s t e m    c o d e:  &SYSCD2
^ +4B _REFD  ^       :Date last referenced
^ +4E  &SMSRES :SMS flags: X'80'=SMS file X'40'=uncatal  X'20'=SDB  X'08
^ +52 _Z ^      :Dsorg 1st byte: X'80'=ISAM X'40'=PS X'20'=DA X'02'=PO X
^ +53 _Z ^      :Dsorg 2nd byte: X'08'=VSAM
^ +54 _Z ^      :Recfm: X'C0'=U  X'80'=F  X'40'=V  X'10'=B  X'08'=S  X'0
^ +55 _Z ^      :Option code
^ +56 _BLKL^    :Blocksize                    +5A _Z   ^  :Record length
^ +5B _Z   ^    :Relative key position
^ +5D _Z ^      :Indicator: X'80'=last volume         X'40'=RACF-defined
^      X'10'=PSWD required  X'04'=PSWD req to write   X'02'=opened not f
^ +5E _SCALO   ^:Secondary alloc
^ +69 _Z                   ^  :First extent description         C C H H-
^ +73 _Z                   ^  :Second and Third extent +7D _EXT3
^          C C H H-C C H H          Lstar : &LSTAR     Trbal : &TRBAL
)INIT
 .CURSOR    =   VTOCRDSN
 .ZVARS     =   '(FMTID,VOLSQ,NOEPV,NOBDB,DSORG1,DSORG2,RECFM,OPTCD, +
   LRECL,RKP,DSIND,EXT1,EXT2)'
)PROC
     VER    ( &VTOCRDSN , NONBLANK)
/*   VER    ( &VTOCVS , NONBLANK)
/*   VER    ( &VTOCRDSN , DSNAME  )
     VER    ( &VTOCVS , NAME      )
     VER    ( &FMTIF    , HEX     )
     VER    ( &DSSN     , HEX     )
     VER    ( &VOLSQ    , HEX     )
     VER    ( &CREDT    , HEX     )
     VER    ( &EXPDT    , HEX     )
     VER    ( &NOEPV    , HEX     )
     VER    ( &NOBDB    , HEX     )
     VER    ( &SYSCD    , HEX     )
     VER    ( &REFD     , HEX     )
     VER    ( &DSORG    , HEX     )
     VER    ( &DSORG    , HEX     )
     VER    ( &RECFM    , HEX     )
     VER    ( &OPTCD    , HEX     )
     VER    ( &BLKL     , HEX     )
     VER    ( &LRECL    , HEX     )
     VER    ( &RKP      , HEX     )
     VER    ( &DSIND    , HEX     )
     VER    ( &SCALO    , HEX     )
     VER    ( &EXT1     , HEX     )
     VER    ( &EXT2     , HEX     )
     VER    ( &EXT3     , HEX     )
)END
