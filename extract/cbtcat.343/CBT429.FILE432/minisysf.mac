//SECUDISK   EXEC  PGM=ADRDSSU
//SYSPRINT   DD  SYSOUT=*
//K7         DD  DSN=SECU.MINISY,VOL=SER=K7MINI,DCB=TRTCH=COMP,
//           DISP=(NEW,KEEP),UNIT=3480,LABEL=(2,NL,EXPDT=98000)
//DASD       DD  UNIT=SYSALLDA,VOL=SER=MINISY,DISP=SHR
//SYSIN      DD  *
  DUMP  FULL  INDD(DASD)  OUTDD(K7)    CANCELERROR               -
  OPT(4)
