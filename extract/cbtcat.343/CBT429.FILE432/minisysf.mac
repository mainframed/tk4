//CARDDD     DD  UNIT=3480,LABEL=(1,NL,EXPDT=98000),DISP=(,KEEP),
//  DCB=(RECFM=F,LRECL=80,BLKSIZE=80),DSN=D,VOL=(,RETAIN,SER=K7MINI)
