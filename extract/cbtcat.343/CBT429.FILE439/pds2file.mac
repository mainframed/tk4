 DCL PDSFILE FILE RECORD KEYED SEQL INPUT ENV(VSAM GENKEY);
 DCL REC_PTR POINTER;
 DCL 1 I_REC BASED(REC_PTR),
       2 RECTYPE CHAR(1) INIT('I'),
       2 DATZEIT CHAR(20) INIT(' ');
 DCL 1 D_REC BASED(REC_PTR),
       2 RECTYPE CHAR(1) INIT('D'),
       2 DNUM PIC'(8)9' INIT(0),
       2 VOLSER CHAR(8) INIT(' '),
       2 DSN CHAR(44) INIT(' ');
 DCL 1 M_REC BASED(REC_PTR),
       2 RECTYPE CHAR(1) INIT('M'),
       2 MEM CHAR(8) INIT(' '),
       2 DNUM PIC'(8)9';

