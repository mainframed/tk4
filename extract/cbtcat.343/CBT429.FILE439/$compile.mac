//COMPILE  JOB (...)
//*
//* COMPILE AND LINK PL/1 - PROGRAMS
//*
//PLICOMPI PROC SYSLBLK=3200,MBR=NOTSPECIFIED
//PLI      EXEC PGM=IEL0AA,REGION=6000K,
//      PARM='NOMACRO,NUMBER,GN,OBJECT,NODECK,CS(60),AG',
//SYSLIB   DD  DSN=PP.PDSX2.PLI,DISP=SHR
//SYSPRINT DD  SYSOUT=*
//SYSLIN   DD  DSN=&&LOADSET,DISP=(MOD,PASS),UNIT=SYSDA,
//             SPACE=(80,(250,100)),DCB=(BLKSIZE=&SYSLBLK)
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=SYSDA,
//             SPACE=(1024,(200,50),,CONTIG,ROUND),DCB=BLKSIZE=1024
//SYSIN    DD  DSN=PP.PDSX2.PLI(&MBR),DISP=SHR
//LKED     EXEC PGM=IEWL,PARM='XREF,LIST,LET',COND=(9,LT,PLI)
//SYSLIB   DD  DSN=SYS1.SISPLOAD,DISP=SHR     <== ISPLINK
//         DD  DSN=PP.PLI.PLIBASE,DISP=SHR
//         DD  DSN=PP.PLI.SIBMBASE,DISP=SHR
//         DD  DSN=PP.PDSX2.LOAD,DISP=SHR <== PDSALLOC/PDSFREE
//SYSLMOD  DD  DSN=PP.PDSX2.LOAD(&MBR),DISP=SHR
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(1024,(200,20)),
//             DCB=BLKSIZE=1024
//SYSPRINT DD  SYSOUT=*
//SYSLIN   DD  DSN=&&LOADSET,DISP=(OLD,DELETE)
//SYSIN    DD  DUMMY
// PEND
//COMP1    EXEC PLICOMPI,MBR=PDSMEM1
//COMP2    EXEC PLICOMPI,MBR=PDSMEM2
//COMP3    EXEC PLICOMPI,MBR=PDSMEM3
//COMP4    EXEC PLICOMPI,MBR=PDSMEM4
