 CREMEMT: PROC; /* ANLEGEN MEMBER-TABELLE */

 /* TABELLE BEENDEN,FALLS NOCH VORHANDEN */

 DO UNTIL(PLIRETV ^= 0);
    CALL ISPLINK('TBEND ','PDSMEMT ');
 END;

 /* TABELLE ANLEGEN */

 CALL ISPLINK('TBCREATE ','PDSMEMT ',' ',
               '(PDSMEMS,PDSMEML,PDSDSNL,PDSDNUM,PDSVOL) ','NOWRITE ');

 END CREMEMT;
