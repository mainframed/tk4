)BODY CMD(ZSPFCMD)
? Options#? Tape #? TSO1#? TSO2#? JES3#? SP#? Help#    &SER
$&ZTIME &ZJDATE          ***** Job Display Facility *****         &SU
$ISPF COMMAND    ==>_ZSPFCMD                                   $SCROLL
$BAT,STC,TSU,ALL ==>_Z  $     PREFIX ==>_Z       $OPTN ==>_Z$   RPTSEC
   ×CPU=&CPU{AFC=&AFC}PAG=&PAG<TPR=&TPR|UIC=&UIC^MB>&MGBÖTSO=&TSO
$  &ASID &T   JOB    &EXCPCNT &JOBSTEP STATUS    MEMORY &SW DP  PG &PCPU
)MODEL
_Z@Z                                    !Z    @Z
)INIT
 &PCPU=%1CPU..STEPCPU
 &ASID=ASID
 &JOBSTEP='JES_INIT'
 &T=T
 &EXCPCNT=EXCP_CNT
 &SW=SWP
 &CPUCOL=GREEN
 &AFCCOL=GREEN
 &PAGCOL=GREEN
 &TPRCOL=GREEN
 &UICCOL=GREEN
 &MGBCOL=GREEN
 .ZVARS='(XCMD,ZPRE,ZJS3,XRPT,PIKA,ACTLINE,STAT,ACTLINB)'
 .CURSOR=XCMD
 &AMT=CSR
 .HELP=ZDFC4
 &ZTDMARK = '                      ***** End of EDWA job list *****
 IF (&XRPT ^= 0)
     &ZTDMARK = '** Use attention key to terminate display **'
 IF (&CPU > 0090)
     &CPUCOL=RED
 IF (&AFC < 0100)
     &AFCCOL=RED
 IF (&PAG > 0050)
     &PAGCOL=RED
 IF (&TPR > 0150)
     &TPRCOL=RED
 IF (&UIC < 0200)
     &UICCOL=RED
 IF (&MGB < 0151)
     &MGBCOL=RED
 IF (&ZJS3 = 'J')
     &ASID=JOB#
 IF (&ZJS3 = 'Q')
     &JOBSTEP=STEPNAME
     &EXCPCNT='PROCEDUR'
     &SW='PIN'
     &T=C
 IF (&XCMD = 'TSU')
     &JOBSTEP=LOGNPROC
 IF (&XCMD = 'STC')
     &JOBSTEP=STEPNAME
)PROC
 IF (&YCMD ^= &Z)
     &XCMD=&YCMD
     &YCMD=&Z
 IF (&YPRE ^= &Z)
     &ZPRE=&YPRE
     &YPRE=&Z
 IF (&YJS3 ^= &Z)
     &ZJS3=&YJS3
     &YJS3=&Z
 &PTCBTEXT = &XCMD
 IF (&XCMD = &Z)
     &XCMD=BAT
 VER(&XCMD,NONBLANK,LIST,BAT,TSU,STC,ALL)
 VER(&ZJS3,LIST,0,1,Q,S,J)
 IF (&XRPT ^= 0)
  VER(&XRPT,NONBLANK,RANGE,2,99)
)END
