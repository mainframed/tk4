)ATTR
_ TYPE(INPUT)
@ TYPE(OUTPUT) INTENS(HIGH) CAPS(ON) JUST(ASIS) COLOR(YELLOW)
! TYPE(OUTPUT) INTENS(HIGH) CAPS(ON) JUST(ASIS) COLOR(GREEN)
$ TYPE(TEXT) INTENS(LOW) CAPS(ON) JUST(LEFT) COLOR(TURQ)
+ TYPE(TEXT) INTENS(LOW) CAPS(ON) JUST(LEFT) COLOR(TURQ)
Ö TYPE(TEXT) INTENS(LOW) CAPS(ON) JUST(LEFT) COLOR(GREEN)
^ TYPE(TEXT) INTENS(LOW) CAPS(ON) JUST(LEFT) COLOR(&MGBCOL)
× TYPE(TEXT) INTENS(LOW) CAPS(ON) JUST(LEFT) COLOR(&CPUCOL)
{ TYPE(TEXT) INTENS(LOW) CAPS(ON) JUST(LEFT) COLOR(&AFCCOL)
} TYPE(TEXT) INTENS(LOW) CAPS(ON) JUST(LEFT) COLOR(&PAGCOL)
< TYPE(TEXT) INTENS(LOW) CAPS(ON) JUST(LEFT) COLOR(&TPRCOL)
| TYPE(TEXT) INTENS(LOW) CAPS(ON) JUST(LEFT) COLOR(&UICCOL)
? TYPE(AB)
# TYPE(NT)
)ABC DESC(Options)
