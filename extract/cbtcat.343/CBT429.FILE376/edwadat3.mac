)BODY CMD(ZSPFCMD)
$&ZTIME &ZJDATE &SU   ********* Job Display Facility *********
$ISPF COMMAND    ==>_ZSPFCMD                                    $SCROLL
$BAT,STC,TSU,ALL ==>_Z  $     PREFIX ==>_Z       $OPTN ==>_Z$    RPTSEC
×   CPU=&CPU{AFC=&AFC}PAG=&PAG<TPR=&TPR|UIC=&UIC ^MB>&MGBÖTSO=&TSO
$  &ASID &T   JOB    &EXCPCNT &JOBSTEP STATUS    MEMORY SWP DP  PG &PCPU
)MODEL
_Z@Z                                    !Z    @Z
)INIT
 &PCPU=%1CPU..STEPCPU
 &ASID=ASID
 &JOBSTEP='JES_INIT'
 &T=T
 &EXCPCNT=EXCP_CNT
 &CPUCOL=GREEN
 &AFCCOL=GREEN
 &PAGCOL=GREEN
 &TPRCOL=GREEN
 &UICCOL=GREEN
 &MGBCOL=GREEN
 .ZVARS='(XCMD,ZPRE,ZJS3,XRPT,PIKA,ACTLINE,STAT,ACTLINB)'
 .CURSOR=XCMD
 &AMT=CSR
 .HELP=ZDFC4
 &ZTDMARK = '                   ********* End of EDWA job list *********
 IF (&XRPT ^= 0)
     &ZTDMARK = '** Use attention key to terminate display **'
 IF (&CPU > 0090)
     &CPUCOL=RED
 IF (&AFC < 0100)
     &AFCCOL=RED
 IF (&PAG > 0050)
     &PAGCOL=RED
 IF (&TPR > 0150)
     &TPRCOL=RED
 IF (&UIC < 0200)
     &UICCOL=RED
 IF (&MGB < 0151)
     &MGBCOL=RED
 IF (&ZJS3 = 'J')
     &ASID=JOB#
 IF (&ZJS3 = 'Q')
     &JOBSTEP=STEPNAME
     &EXCPCNT='PROCEDUR'
     &T=C
 IF (&XCMD = 'TSU')
     &JOBSTEP=LOGNPROC
 IF (&XCMD = 'STC')
     &JOBSTEP=STEPNAME
)PROC
 IF (&YCMD ^= &Z)
     &XCMD=&YCMD
     &YCMD=&Z
 IF (&YPRE ^= &Z)
     &ZPRE=&YPRE
     &YPRE=&Z
 IF (&YJS3 ^= &Z)
     &ZJS3=&YJS3
     &YJS3=&Z
 &PTCBTEXT = &XCMD
 VER(&XCMD,NONBLANK,LIST,BAT,TSU,STC,ALL)
 VER(&ZJS3,LIST,0,Q,S,J)
 VER(&XRPT,NONBLANK,LIST,0,2,3,4,5,6,7,8,9)
)END
