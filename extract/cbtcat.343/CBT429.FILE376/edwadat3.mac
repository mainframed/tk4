)ABCPROC
 VER(&PDCHOICE,LIST,1,2,3,4,5,6,7,8,9)
 IF ( &PDCHOICE=1)
      &YCMD=BAT
 IF ( &PDCHOICE=2)
      &YCMD=STC
 IF ( &PDCHOICE=3)
      &YCMD=TSU
 IF ( &PDCHOICE=4)
      &YCMD=ALL
 IF ( &PDCHOICE=5)
      &YJS3=Q
 IF ( &PDCHOICE=6)
      &YJS3=J
 IF ( &PDCHOICE=7)
      &YPRE=ED
 IF ( &PDCHOICE=8)
      &YPRE=DT
 IF ( &PDCHOICE=9)
      &YPRE=C4
