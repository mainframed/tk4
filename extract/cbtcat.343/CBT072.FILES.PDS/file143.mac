PROC 2 INPUT OUTPUT
DELETE (&OUTPUT XZQFXABD.DATA)
FREE FILE(SYSUT1,SYSUT2,SYSPRINT,SYSIN)
FREE ATTRLIST(VB,VBA)
ALLOC FILE(SYSIN)    DA('NULLFILE')
ALLOC FILE(SYSPRINT) DA('NULLFILE')
ATTRIB  VB   RECFM(V,B)
ATTRIB  VBA  RECFM(V,B,A)
ALLOC FILE(SYSUT1) DA(&INPUT.) SHR
ALLOC FILE(SYSUT2) DA(XZQFXABD.DATA) NEW SPACE(3,1) TRACKS USING(VB)
CALL 'SYS1.LINKLIB(IEBGENER)'
EDIT XZQFXABD.DATA OLD  CAPS
V OFF
TOP
C * 99999 // / ALL
TOP
C * 99999 / / / ALL
SAVE
END
FREE FILE(SYSUT1,SYSUT2)
ALLOC FILE(SYSUT1) DA(XZQFXABD.DATA) OLD
ALLOC FILE(SYSUT2) DA(&OUTPUT.) NEW SPACE(3,1) TRACKS USING(VBA)
CALL 'SYS1.LINKLIB(IEBGENER)'
FREE FILE(SYSUT1,SYSUT2,SYSPRINT,SYSIN)
FREE ATTRLIST(VB,VBA)
DELETE XZQFXABD.DATA
ALLOC FILE(SYSIN) DA(*)
ALLOC FILE(SYSPRINT) DA(*)
