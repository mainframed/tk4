//*
//*   SMP CDS CROSS-REFERENCE LISTING GENERATOR
//*
//SMPXREF PROC OPT=,
//             CDS=CDS,
//             PREFIX='SYS4.SMP'
//*
//*
//SCAN EXEC PGM=SMPXCDS,PARM=&OPT
//*
//SMPCDS  DD DSN=&PREFIX..&CDS,DISP=SHR
//*
//SORTIN DD DSN=&TEMP1,DISP=(NEW,PASS),SPACE=(CYL,(1,1)),
//          UNIT=SYSDA,DCB=(RECFM=FBS,LRECL=22,BLKSIZE=6446,
//          OPTCD=C,NCP=3,BUFNO=4)
//*
//STEPLIB DD DSN=&PREFIX..LINKLIB,DISP=SHR
//*
//*
//SORT EXEC PGM=IGHRCO00,PARM='BALN,SIZE=MAX,MSG=NO,NOLIST'
//*
//SORTIN DD DSN=&TEMP1,DISP=(OLD,DELETE)
//*
//SORTLIB DD DSN=SYS2.SORTLIB,DISP=SHR
//*
//SORTOUT DD DSN=&TEMP2,DISP=(NEW,PASS),SPACE=(CYL,(1,1)),
//           UNIT=SYSDA,DCB=*.SCAN.SORTIN
//*
//SORTWK01 DD DSN=&&SW01,UNIT=SYSDA,SPACE=(CYL,(10),,CONTIG)
//*           SEP=(SORTWK02,SORTWK03,SORTWK04,SORTWK05,SORTWK06)
//*
//SORTWK02 DD DSN=&&SW02,UNIT=SYSDA,SPACE=(CYL,(10),,CONTIG),
//            SEP=SORTWK01
//*
//SORTWK03 DD DSN=&&SW03,UNIT=SYSDA,SPACE=(CYL,(10),,CONTIG),
//            SEP=(SORTWK01,SORTWK02)
//*
//SORTWK04 DD DSN=&&SW04,UNIT=SYSDA,SPACE=(CYL,(10),,CONTIG),
//            SEP=(SORTWK01,SORTWK02,SORTWK03)
//*
//SORTWK05 DD DSN=&&SW05,UNIT=SYSDA,SPACE=(CYL,(10),,CONTIG),
//            SEP=(SORTWK01,SORTWK02,SORTWK03,SORTWK04)
//*
//SORTWK06 DD DSN=&&SW06,UNIT=SYSDA,SPACE=(CYL,(10),,CONTIG),
//            SEP=(SORTWK01,SORTWK02,SORTWK03,SORTWK04,SORTWK05)
//*
//SYSOUT DD DUMMY
//*
//SYSIN DD DSN=&PREFIX..CNTL(SMPXREF),DISP=SHR
//*
//*
//SPEW EXEC PGM=SMPXREF
//*
//SORTOUT DD DSN=&TEMP2,DISP=(OLD,DELETE)
//*
//STEPLIB DD DSN=&PREFIX..LINKLIB,DISP=SHR
//*
//SYSPRINT DD SYSOUT=A,DCB=(RECFM=VB,LRECL=137,BLKSIZE=2000)
//*
