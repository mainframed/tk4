//ASQCCSVC  JOB (X0041,QCC,ASQCC),'BRIAN COOK',
//   MSGCLASS=X,
//   MSGLEVEL=(1,1),CLASS=X,NOTIFY=ASQCC
/*JOBPARM L=99
//*
//JOBLIB   DD  DSN=SYS1.LINKLIB,DISP=SHR,
//         UNIT=SYSDA,VOL=SER=MVSTGT
//*
//ASM     EXEC PGM=IEV90,
//        REGION=2048K,
//        PARM='OBJECT,NODECK,TERM,RENT,XREF(SHORT)'
//SYSLIB   DD  DSN=SYS1.MACLIB,DISP=SHR,
//         UNIT=SYSDA,VOL=SER=MVSTGT
//         DD  DSN=SYS1.AMODGEN,DISP=SHR,
//         UNIT=SYSDA,VOL=SER=MVSDLB
//SYSUT1   DD  SPACE=(CYL,(5,5)),
//             UNIT=SYSDA
//SYSTERM  DD  SYSOUT=*
//SYSPRINT DD  SYSOUT=*
//SYSPUNCH DD  DUMMY
//SYSLIN   DD   DSN=&&OBJSET,UNIT=SYSSQ,SPACE=(80,(200,50)),
//             DISP=(MOD,PASS)
//SYSIN    DD  *
IGC0023E CSECT
         USING *,6
         USING TCB,4
         L     2,TCBJSCB  PICK UP CALLER'S JSCB
         USING IEZJSCB,2
         LTR   0,0           R0 EQ 0 REQUESTS AUTH OFF
         BZ    AUTHOFF
AUTHON   OI    JSCBOPTS,X'01' SET AUTH ON
         BR    14
AUTHOFF  NI    JSCBOPTS,X'FE' SET AUTH OFF
         BR    14
 DC C'IGC0023E_SVC_235_AUTHON_AUTHOFF_BRIAN_COOK_&SYSTIME_&SYSDATE'
         CNOP  0,8
         IKJTCB
         IEZJSCB
         END
/*
//LKED     EXEC PGM=IEWL,PARM=(XREF,LET,LIST,NCAL,RENT,REFR,REUS),
//             COND=(2,LT,ASM),REGION=1024K
//SYSLIN   DD   DSN=&&OBJSET,DISP=(OLD,DELETE)
