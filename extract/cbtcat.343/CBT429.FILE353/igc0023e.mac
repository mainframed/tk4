//SYSLMOD  DD   DSN=SYS1.LPALIB,DISP=SHR,UNIT=3380,VOL=SER=MVSTGT
//SYSUT1   DD   DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(1024,(50,20))
//SYSTERM  DD   SYSOUT=*
//SYSPRINT DD   SYSOUT=*
//SYSIN    DD   *
     NAME IGC0023E(R)
