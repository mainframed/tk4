)INIT

 .CURSOR = A

 &ZCMD = ' '


 &TEST = 'TEST'
 &SIG1 = 'SIGNAL'
 &LOOP1 = 'REMOTE'
 &PWR1 = 'POWER'
 &SIG2 = 'DETECT'
 &LOOP2 = 'LOOPBK'
 &PWR2 = 'ON'


 &A = 'O'
