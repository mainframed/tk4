 &C = 'O'
 &D = 'O'
 &E = ' '

 &NTDATA00 = ' '
 &NTDATA01 = ' '
 &NTDATA02 = ' '
 &NTDATA03 = ' '
 &NTDATA04 = ' '
 &NTDATA05 = ' '
 &NTDATA06 = ' '
 &NTDATA07 = ' '
 &NTDATA08 = ' '
 &NTDATA09 = ' '


)PROC
 IF (&ZCMD ^= &Z) .MSG=ISPZ001
 VER (&A,LIST,O,X,B)
 VER (&B,LIST,O,X,B)
 VER (&C,LIST,O,X,B)
 VER (&D,LIST,O,X,B)
