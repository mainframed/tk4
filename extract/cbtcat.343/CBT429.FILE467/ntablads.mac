 IF (&NTDATA07 ^= ' ')
   &NTDATA06 = 'USER''S COMMENTS ON RUNNING LOCAL TEST:'


 &NTCONTIN = &E

 &NTNXTPNL = 'EXIT    '


 VPUT (NTNXTPNL NTCONTIN) SHARED
