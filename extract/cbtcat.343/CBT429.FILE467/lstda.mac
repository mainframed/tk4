)ATTR
?  TYPE(OUTPUT) INTENS(HIGH) PAD(NULLS) CAPS(ON)
×  TYPE(OUTPUT) INTENS(LOW) PAD(NULLS) CAPS(ON)
Ö  TYPE(INPUT)  INTENS(LOW) PAD(NULLS) CAPS(ON)
_  TYPE(INPUT)  INTENS(HIGH) CAPS(ON)
)BODY
%LIST OF DDNAMES ALLOCATED ---------------------------------------------
%COMMAND ===>_ZCMD                                            %SCROLL ==
+   DDNAME             VOLUME  DSNAME
)MODEL ROWS(&SCAN)
_Z×Z               ?Z ×VOLSER ×Z
)INIT
  .ZVARS = '(LSEL DDNAME MORE DSNAME)'
  &AMT = &ZSCML
  .HELP = LSTHA
  &NOHEAD = &Z
)REINIT
 IF (.MSG ^= &Z)
     &NOHEAD = &Z
)PROC
 &ZSCML = &AMT
 VPUT (ZSCML) PROFILE
 IF (&ZTDSELS ^= 0)
     VER (&LSEL,LIST,S,E,F,B,P,H,D MSG=DTSM001)
     IF (&DSNAME = NULLFILE)
         IF (&LSEL ^= &Z,F)
             &LSEL = &LSEL
             .MSG = DTSM002
     IF (&LSEL = P)
         &NOHEAD = &Z
         IF (&VOLSER = &Z)
             &VOLUME = &Z
         IF (&VOLSER ^= &Z)
             &VOLUME = 'VOLUME(&VOLSER)'
         &ZSEL = 'PGM(PDS) PARM(PDS ''&DSNAME'' &VOLUME ISPMODE)'
     IF (&LSEL = H)
         &NOHEAD = &Z
         &ZSEL = 'CMD(PRINTOFF ''&DSNAME'')'
     IF (&LSEL = D)
         &NOHEAD = &Z
         &ZSEL = 'CMD(DSD ''&DSNAME'')'
     IF (&LSEL = S)
         IF (&VOLSER = &Z)
             &VOLUME = &Z
         IF (&VOLSER ^= &Z)
             &VOLUME = 'VOLUME(&VOLSER)'
         &ZSEL = 'CMD(DSAT ''&DSNAME'' &VOLUME DSONLY SEC &NOHEAD +
                  NOCR RC(PREVIOUS))'
         &NOHEAD = NOHEAD
 &UMSG1 = 'INVALID LINE COMMAND'
 &LMSG1 = 'SEE THE TUTORIAL FOR A LIST OF VALID LINE COMMANDS (ENTER HEL
 &UMSG2 = 'INVALID DATASET'
 &LMSG2 = 'YOU MAY ONLY "FREE" A "DUMMY" DATASET'
)END
