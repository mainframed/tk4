)ATTR
# TYPE(OUTPUT) INTENS(LOW) JUST(RIGHT)
× TYPE(OUTPUT) INTENS(LOW) JUST(ASIS)
)BODY WIDTH(80)
%MEMBER COMPARE LISTING -(DUPLICATES:&DUPESC)---------------------------
%COMMAND ===>_ZCMD                                            %SCROLL ==
+
+Commands:%SCAN:+duplicates   %ALL:+all members   %NONDUPES:+non-duplica
+
+  NAME       SIZE   LIBRARY
)MODEL ROWS(&XYZ)
×DSN     +#ZLCNORC+ ×LIB
)INIT
  .ZVARS = 'ZSCML'         /* SCROLL AMT VARIABLE NAME  */
  IF (&XYZ = &Z) &XYZ = ALL
)PROC
  VER (&ZCMD,LIST,SCAN,ALL,NONDUPES)
  IF (&ZCMD = NONDUPES) &DUPES = 0 &XYZ = SCAN
  IF (&ZCMD ^= NONDUPES) &DUPES = 1
  VPUT (ZSCML) PROFILE
  IF (.MSG = &Z)
    IF (&ZCMD ^= &Z,NONDUPES)
      &XYZ = &ZCMD
)END
