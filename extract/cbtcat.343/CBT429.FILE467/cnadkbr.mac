  ! TYPE(OUTPUT) INTENS(HIGH)
  # TYPE(INPUT) CAPS(ON) INTENS(HIGH) PAD('_')
)BODY EXPAND(@@)
%BROWSE - DDNAME &DDNAME @-@
%COMMAND ===>?ZCMD                                             %SCROLL%=
%
+%S+- Browse Dsn/Member  %L+-Browse entire PDS (Member List)   %P+- PDS
%
%Sel   Datasets allocated to &DDNAME                             %Volume
+---   -----------------------------------------------------------------
)MODEL
+#Z+  !Z                                                         !TABLEV
)INIT
  .ZVARS = '(LSEL TABLEDSN)'
  &AMT = &ZSCML
  &ZTDMARK = '********************** END OF DSNAME LIST FOR &DDNAME +
              ***************************'
  &ZTDMARK = TRUNC (&ZTDMARK,79)
  &ZTDMSG = DTSM040
  &UMSG40 = &Z
  &LMSG40 = &Z
)PROC
 IF (&ZCMD ^= &Z) .MSG=ZCMD001
 IF (&ZTDSELS ^= 0)
    VER (&LSEL,LIST,S,L,P)
    IF (&LSEL = P)
       &TEMP = TRUNC (&TABLEDSN,'(')
       &TRAIL = .TRAIL
       IF (&TRAIL ^= &Z)
          &TEMP = '&TEMP'''
       &PDSCMD = 'PGM(PDS) PARM(PDS &TEMP VOL(&TABLEVOL) MEMLIST :)'
)END
