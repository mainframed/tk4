)ATTR DEFAULT(~`_)
 % TYPE(TEXT) INTENS(HIGH) SKIP(ON)
 + TYPE(TEXT) INTENS(LOW) SKIP(ON)
)BODY
%-------------------------  EDIT - ENTRY PANEL  -------------------&TEST
%SELECT OPTION ===>_ZCMD

%SELECT MEMBER%===>_MEMBER  +(OVERRIDES SPF MEMBER, '*' FOR LIST)
+SPF LIBRARIES:
%A+ PROJECT%===>_PRJA    +%B+%===>_PRJB    +%C+%===>_PRJC    +%D+%===>_P
+   LIBRARY%===>_LIBA    +   %===>_LIBB    +   %===>_LIBC    +   %===>_L
+   TYPE   %===>_TYPA    +   %===>_TYPB    +   %===>_TYPC    +   %===>_T
+   MEMBER %===>_MEMA    +   %===>_MEMB    +   %===>_MEMC    +   %===>_M
%
%E+ PROJECT%===>_PRJE    +%F+%===>_PRJF    +%G+%===>_PRJG    +%H+%===>_P
+   LIBRARY%===>_LIBE    +   %===>_LIBF    +   %===>_LIBG    +   %===>_L
+   TYPE   %===>_TYPE    +   %===>_TYPF    +   %===>_TYPG    +   %===>_T
+   MEMBER %===>_MEME    +   %===>_MEMF    +   %===>_MEMG    +   %===>_M
+OTHER PARTITIONED OR SEQUENTIAL DATASETS:
%I+ DATASET NAME %===>_DSNI
%J+ DATASET NAME %===>_DSNJ
%K+ DATASET NAME %===>_DSNK
%L+ DATASET NAME %===>_DSNL
%M+ DATASET NAME %===>_DSNM
%N+ DATASET NAME %===>_DSNN
%O+ DATASET NAME %===>_DSNO
%P+ DATASET NAME %===>_DSNP
%S+ DATASET NAME %===>_DSNS
)INIT
   IF (&MSG^=&Z)
      .MSG=&MSG
   .CURSOR=ZCMD
   .HELP=EDTH
   IF (&ZCMD=P)
      &ZCMD=&Z
)PROC
   &MSG=&Z
   VER(&ZCMD,LIST,A,B,C,D,E,F,G,H,I,J,K,L,M,N,O,P,S)
   VER(&ZCMD,NONBLANK)
   IF (&ZCMD=A)
      VER(&MEMA,NAME)
      VER(&PRJA,NONBLANK)
      VER(&LIBA,NONBLANK)
      VER(&TYPA,NONBLANK)
   IF (&ZCMD=B)
      VER(&MEMB,NAME)
      VER(&PRJB,NONBLANK)
      VER(&LIBB,NONBLANK)
      VER(&TYPB,NONBLANK)
   IF (&ZCMD=C)
      VER(&MEMC,NAME)
      VER(&PRJC,NONBLANK)
      VER(&LIBC,NONBLANK)
      VER(&TYPC,NONBLANK)
   IF (&ZCMD=D)
      VER(&MEMD,NAME)
      VER(&PRJD,NONBLANK)
      VER(&LIBD,NONBLANK)
      VER(&TYPD,NONBLANK)
   IF (&ZCMD=E)
      VER(&MEME,NAME)
      VER(&PRJE,NONBLANK)
      VER(&LIBE,NONBLANK)
      VER(&TYPE,NONBLANK)
   IF (&ZCMD=F)
      VER(&MEMF,NAME)
      VER(&PRJF,NONBLANK)
      VER(&LIBF,NONBLANK)
      VER(&TYPF,NONBLANK)
   IF (&ZCMD=G)
      VER(&MEMG,NAME)
      VER(&PRJG,NONBLANK)
      VER(&LIBG,NONBLANK)
      VER(&TYPG,NONBLANK)
   IF (&ZCMD=H)
      VER(&MEMH,NAME)
      VER(&PRJH,NONBLANK)
      VER(&LIBH,NONBLANK)
      VER(&TYPH,NONBLANK)
   IF (&ZCMD=I)
      VER(&DSNI,NONBLANK)
   IF (&ZCMD=J)
      VER(&DSNJ,NONBLANK)
   IF (&ZCMD=K)
      VER(&DSNK,NONBLANK)
   IF (&ZCMD=L)
      VER(&DSNL,NONBLANK)
   IF (&ZCMD=M)
      VER(&DSNM,NONBLANK)
   IF (&ZCMD=N)
      VER(&DSNN,NONBLANK)
   IF (&ZCMD=O)
      VER(&DSNO,NONBLANK)
   IF (&ZCMD=P)
      VER(&DSNP,NONBLANK)
   IF (&ZCMD=S)
      VER(&DSNS,NONBLANK)
)END
