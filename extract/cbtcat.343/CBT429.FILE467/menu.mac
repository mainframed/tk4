   FOREGROUND,'PANEL(CNA$FORE)'                  /* OPTION 8.5      */
     JCLCHECK,'PANEL(JCK$)'                      /* OPTION 8.4/5    */
      JOBXREF,'CMD(EX ''TESTP.ASG.SPF2.NEWCLIB(JOBEXREF)'')' /*OPT 8.7 *
