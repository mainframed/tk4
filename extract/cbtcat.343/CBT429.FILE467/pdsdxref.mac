)ATTR DEFAULT(%+_)
)BODY EXPAND(//)
%/-/  CREATE A CROSS-REFRENCE MEMBER LIST  /-/
%COMMAND ===>_ZCMD
%
+Enter the Libraries DSNAMEs below:
+
+    DSNAME #1%===>_DSNAME1
+    DSNAME #2%===>_DSNAME2
+    DSNAME #3%===>_DSNAME3
+    DSNAME #4%===>_DSNAME4
+
%     Entering the library with the GREATER amount of members
      first, will improve the performance of this function.
)INIT
 .CURSOR = DSNAME1

)PROC
 IF (&ZCMD ^= &Z) .MSG = ZCMD001

 VER (&DSNAME1,NB,DSNAME)
 VER (&DSNAME2,NB,DSNAME)
 VER (&DSNAME3,DSNAME)
 VER (&DSNAME4,DSNAME)

 IF (.MSG = &Z)
     IF (&DSNAME1 ^= &Z)
         &TEMP = TRUNC (&DSNAME1,'''')
         &TRAIL = .TRAIL
         IF (&TRAIL = &Z)
             &DSN1 = '&ZUSER..&DSNAME1'
         IF (&TRAIL ^= &Z)
             &DSN1 = TRUNC (&TRAIL,'''')
     IF (&DSNAME2 ^= &Z)
         &TEMP = TRUNC (&DSNAME2,'''')
         &TRAIL = .TRAIL
         IF (&TRAIL = &Z)
             &DSN2 = '&ZUSER..&DSNAME2'
         IF (&TRAIL ^= &Z)
             &DSN2 = TRUNC (&TRAIL,'''')
     IF (&DSNAME3 ^= &Z)
         &TEMP = TRUNC (&DSNAME3,'''')
         &TRAIL = .TRAIL
         IF (&TRAIL = &Z)
             &DSN3 = '&ZUSER..&DSNAME3'
         IF (&TRAIL ^= &Z)
             &DSN3 = TRUNC (&TRAIL,'''')
     IF (&DSNAME4 ^= &Z)
         &TEMP = TRUNC (&DSNAME4,'''')
         &TRAIL = .TRAIL
         IF (&TRAIL = &Z)
             &DSN4 = '&ZUSER..&DSNAME4'
         IF (&TRAIL ^= &Z)
             &DSN4 = TRUNC (&TRAIL,'''')
)END
