)ATTR
      + TYPE(TEXT)  INTENS(LOW)
      % TYPE(TEXT)  INTENS(HIGH)
      _ TYPE(INPUT) INTENS(HIGH)
)BODY EXPAND (//)
%-/-/ INTERACTIVE AMBLIST  -/-/
%COMMAND ===>_ZCMD                                                     +
+
+
+    VALID OPTIONS:
+    1=LISTIDR  2=LISTLOAD XREF  3=LISTLOAD  4=LISTOBJ  5=LISTLPA
+
+    LIST OPTION  ===>_O+
+    LIBRARY NAME ===>_AMBDSN                                      +
+    MEMBER NAME  ===>_AMBMEM  +
+    VOLSER       ===>_AMBVOL+
+
+
+    CHOOSE AN AMBLIST OPTION. YOU MUST ENTER A LIBRARY NAME
+    AND A MEMBER NAME WHEN USING OPTIONS 1 THROUGH 4.
+
+
+
+    PRESS%PF3+TO EXIT+
)INIT
  &ZPRIM = NO
 .CURSOR = O
 &O = &AMBOPT
 IF (&O = &Z )
    &O = 1
)PROC
  VER (&O,NONBLANK)
  VER (&O,LIST,1,2,3,4,5)
  IF  (&O NE 5)
    VER (&AMBDSN,NONBLANK)
    VER (&AMBDSN,DSNAME)
    VER (&AMBMEM,NONBLANK)
    VER (&AMBMEM,NAME)
  &AMBOPT = &O
  VPUT (AMBDSN AMBMEM AMBOPT AMBVOL) PROFILE
)END
