PROC 1 DSN VOL()
/*                           */
/* AUTHOR: MARK ZELDEN       */
/*                           */
CONTROL NOLIST NOPROMPT
ISPEXEC CONTROL ERRORS RETURN
ISPEXEC VGET ZPCFMCN PROFILE
IF &STR(&ZPCFMCN) = &STR(/) THEN +
  SET &CONF = &STR(YES)
ELSE +
  SET &CONF = &STR(NO)
ISPEXEC VIEW DATASET(&DSN) VOLUME(&VOL) CONFIRM(&CONF)
      IF &LASTCC ^= 0 THEN ISPEXEC SETMSG MSG(&ZERRMSG)
END
