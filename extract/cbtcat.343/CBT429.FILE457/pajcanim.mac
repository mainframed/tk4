PROC     0
/*********************************************************************/
/*       EXECUTE ANIM-DEMO                                           */
/*       PRE-REQUISITE : DOCP                                        */
/*********************************************************************/
         CONTROL MAIN NOMSG
         DOCP ANIM 'ANIM' LIB('->.USER.DEMO.DATA') NORC
         SET &RC = &LASTCC
         IF &RC ^= 0 THEN DO
           WRITE => ANIM-DEMO RETURN CODE IS &RC <=
           EXIT CODE(8)
           END
         EXIT CODE(0)
