//PDSPUNCH   EXEC     PGM=PDSPUNCH,REGION=4M
//SYSPUNCH     DD       DSN=-SEQUENTIAL-OUTPUT-,
//             DISP=(,CATLG),UNIT=TEST,SPACE=(CYL,(020,010),RLSE)
//SYSUT1       DD       DISP=SHR,DSN=-PDS-TO-BE-PUNCHED-
//SYSIN        DD       *
//STEPLIB      DD       DISP=SHR,DSN=-YOUR-STEPLIB-
//***
