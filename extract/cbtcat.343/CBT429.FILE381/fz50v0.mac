++ USERMOD (FZ50AV0) /*

                    FIRST UNION BANK USERMOD 50

         THIS USERMOD UPDATES THE JES2 JQE AND THE QSE CONTROL
         BLOCKS TO ADD INSTALLATION FIELDS.  SINCE THESE CONTROL
         BLOCKS ARE USED BY ALMOST EVERY MODULE, ASSEMBLIES OF ALL
         JES2 SOURCE MODULES WILL BE FORCED.
                                                                     */.
++ VER (Z038) FMID(HJE2221) PRE(PXB2221) .
++ MACUPD($JQE)
   ASSEM(HASPX001,HASPX97,HASPX97A,HASPX97C,HASPX97E,HASPX97I,
         HASPX97J,HASPX97N,HASPX97P,HASPX97R,HASPX97S,HASPX97X,
         HASPAM,HASPBLKS,HASPBSC,HASPCKDS,HASPCKPT,HASPCNVT,
         HASPCOMM,HASPCON,HASPDOC,HASPDYN,HASPFMT0,HASPFMT1,
         HASPFMT2,HASPFMT3,HASPFMT4,HASPFMT5,HASPFMT6,HASPFMT7,
         HASPFSSM,HASPFSSP,HASPHOPE,HASPIRA,HASPIRDA,HASPIRMA,
         HASPIRPL,HASPIRRE,HASPJOS,HASPMISC,HASPMSG,HASPNET,
         HASPNUC,HASPPRPU,HASPPSO,HASPRAS,HASPRDR,HASPRTAM,
         HASPSCAN,HASPSERV,HASPSNA,HASPSPOL,HASPSSSM,HASPSTAB,
         HASPSTAM,HASPSXIT,HASPTABS,HASPTERM,HASPTRAK,HASPWARM,
         HASPXEQ) .
./ CHANGE NAME=$JQE,LIST=ALL
JQEXOWNR DS    CL8                 RACF OWNER OF THE JOB (FOR IOF) FUNB
JQEXTHRN DS    CL8                 THREAD NAME                     FUNB
JQEXEXCN DS    CL8                 EXCLUDE NAME                    FUNB
JQEXRLSN DS    CL8                 RELEASE NAME                    FUNB
*                                                                  FUNB
JQEXFLG1 DS    X                   INSTALLATION FLAG BYTE 1        FUNB
JQEXPROD EQU   C'P'                PRODUCTION JOB                  FUNB
JQEXTEST EQU   C'T'                TEST JOB                        FUNB
*                                                                  FUNB
JQEXFLG2 DS    X                   INSTALLATION FLAG BYTE 2        FUNB
JQEXTHRD EQU   X'80'               JOB HAS THREAD NAME             FUNB
JQEXXCLU EQU   X'40'               JOB HAS EXCLUDE NAME            FUNB
JQEXSETP EQU   X'20'               JOB HAS SETUP - NOT WAITING     FUNB
JQEXWSET EQU   X'30'               JOB HAS SETUP - WAITING         FUNB
JQEXUHLD EQU   X'08'               JOB IS WAITING USER HOLD        FUNB
JQEXSMF  EQU   X'04'               SMF TYPE 30 RECORD WAS WRITTEN  FUNB
JQEXENDJ EQU   X'02'               END OF JOB MESSAGE WAS WRITTEN  FUNB
JQEXRLSE EQU   X'01'               JOB HAS RELEASE REQUESTS        FUNB
*                                                                  FUNB
JQEXFLG3 DS    X                   INSTALLATION FLAG BYTE 3        FUNB
JQEXJCLE EQU   X'80'               JOB FAILED WITH JCL ERROR       FUNB
JQEXABND EQU   X'40'               JOB FAILED WITH AN ABEND        FUNB
JQEXOWNJ EQU   X'20'               OWNERID SUPPLIED FROM JOBPARM   FUNB
*                                                                  FUNB
JQEXFLG4 DS    X                   INSTALLATION FLAG BYTE 4        FUNB
*                                                                  FUNB
JQERESRT DS    BL4                 RESOURCE ROUTING FLAGS          FUNB
*                                                                  FUNB
++ MACUPD($QSE)
   ASSEM(HASPX001,HASPX97,HASPX97A,HASPX97C,HASPX97E,HASPX97I,
         HASPX97J,HASPX97N,HASPX97P,HASPX97R,HASPX97S,HASPX97X,
         HASPAM,HASPBLKS,HASPBSC,HASPCKDS,HASPCKPT,HASPCNVT,
         HASPCOMM,HASPCON,HASPDOC,HASPDYN,HASPFMT0,HASPFMT1,
         HASPFMT2,HASPFMT3,HASPFMT4,HASPFMT5,HASPFMT6,HASPFMT7,
         HASPFSSM,HASPFSSP,HASPHOPE,HASPIRA,HASPIRDA,HASPIRMA,
         HASPIRPL,HASPIRRE,HASPJOS,HASPMISC,HASPMSG,HASPNET,
         HASPNUC,HASPPRPU,HASPPSO,HASPRAS,HASPRDR,HASPRTAM,
         HASPSCAN,HASPSERV,HASPSNA,HASPSPOL,HASPSSSM,HASPSTAB,
         HASPSTAM,HASPSXIT,HASPTABS,HASPTERM,HASPTRAK,HASPWARM,
         HASPXEQ) .
./ CHANGE NAME=$QSE,LIST=ALL
*                                                                  FUNB
QSERESTB DS    F                   RESOURCE ROUTING BITS ATTACHED  FUNB
*                                                                  FUNB
