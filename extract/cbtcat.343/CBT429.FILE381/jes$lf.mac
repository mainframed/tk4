         LA    R1,3(,R1)           INCREMENT PAST 'XS='
         MVC   OUTXS,0(R1)         SAVE XS PARAMETER
         CLI   0(R1),C'N'          SEE IF XS=N
         BE    CLFXS1              YES, BRANCH
         CLI   0(R1),C'D'          SEE IF XS=D
         BE    CLFXS1              YES, BRANCH
         CLI   0(R1),C'S'          SEE IF XS=S
         BE    CLFXS1              YES, BRANCH
         B     BADOPRND            NO, ERROR, NOT Y/N
CLFXS1   CLI   1(R1),C' '          END OF SPECIFICATIONS
         BE    CLFXS2              YES, GET OUT
         CLI   1(R1),C','          END OF SPECIFICATIONS
         BNE   BADOPRND            NO, ERROR
CLFXS2   B     CLFNSPEC            GET NEXT SPECIFICATION
.XJCF3   ANOP
