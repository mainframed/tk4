         CLI   0(R1),C','          END OF FORM NAME
         BE    CLFF2               YES, BRANCH
         LA    R1,1(,R1)           INCREMENT TO NEXT CHAR OF FORM NAME
         LA    R15,1(,R15)         ADD 1 TO LENGTH
         B     CLFF1               LOOP
CLFF2    LTR   R15,R15             TEST LENGTH
