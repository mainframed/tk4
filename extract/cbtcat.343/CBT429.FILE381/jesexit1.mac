PRINTR7  DC    CL8'PRINTR7'        CHARACTER CONSTANT 'PRINTR7'
PRINTR8  DC    CL8'PRINTR8'        CHARACTER CONSTANT 'PRINTR8'
PRINTR9  DC    CL8'PRINTR9'        CHARACTER CONSTANT 'PRINTR9'
PRINTR10 DC    CL8'PRINTR10'       CHARACTER CONSTANT 'PRINTR10'
RMT1PR1  DC    CL8'R1.PR1  '       CHARACTER CONSTANT 'R1.PR1  '
BLANKS   DC    CL20' '             STRING OF 20 BLANKS
