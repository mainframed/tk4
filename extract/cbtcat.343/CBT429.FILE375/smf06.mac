//ED065DM1 JOB C4SYS000,MSGCLASS=X,CLASS=J,REGION=4M,NOTIFY=C4SYSDM
//*
//*MAIN PROC=C4
// EXEC EDMXGSAS,OPTIONS='SOURCE'
//*MF    DD DISP=SHR,DSN=C4MS.OSTAR
//SMF    DD DISP=SHR,DSN=C4MS.OSTAR.SAMPLE
//SYSIN DD *
  DATA T06 (KEEP =   C4DATE C4TIME
           SMFXXFLG          SMFNO             TIMEOUT
           SYSTEM            HSRSSI            HSRSTY
           HSRHVR            HSRJBN            HSRRST
      HU06ICHP           HU06TIME
      HU06XIDT           HU06XIDM
      HU06XIDC           HU06XIDB
      HU06REST           HU06RESM
      HU06RESC           HU06REQT
      HU06REQG           HU06REQU
      HU06REQN           HU06REQF
      HU06UPGR           HU06UPGT
      HU06UPGA           HU06NOPB
      HU06NOPD           HU06NOPI
      HU06NOPR           HU06IUPD
      HU06IUPP           HU06U
      HU06WQFL           HU06DLUD
      HU06DLUP           HU06DLUK
      HU06DPCD           HU06DPCP
      HU06DPCK           HU06LERR
      HU06U2             HU06IERR
      HU06LRUN           HU06LCPU
      HU06HTCB           HU06BLEN
      HU06BNUM           HU06BMAX
      HU06BCUR           HU06BGET
      HU06BFRE
  )
  ;
  RETAIN N 0;
  INFILE SMF MISSOVER;
  N+1;
  *F N > 1000 THEN STOP;
  INPUT @1 SMFXXFLG PIB1.    SMFNO    PIB1.    TIMEOUT SMFSTAMP8.
           SYSTEM $CHAR4.    HSRSSI   $CHAR4.  HSRSTY  PIB2.
           HSRHVR PIB2.      HSRJBN   $CHAR8.  HSRRST  SMFSTAMP8.
@  ;
*;
IF HSRSTY = 06 THEN DO;
INPUT HU06ICHP PIB2.     HU06TIME PIB4.2
      HU06XIDT PIB4.     HU06XIDM PIB4.
      HU06XIDC PIB4.     HU06XIDB PIB4.
      HU06REST PIB4.     HU06RESM PIB4.
      HU06RESC PIB4.     HU06REQT PIB4.
      HU06REQG PIB4.     HU06REQU PIB4.
      HU06REQN PIB4.     HU06REQF PIB4.
      HU06UPGR PIB4.     HU06UPGT PIB4.
      HU06UPGA PIB4.     HU06NOPB PIB4.
      HU06NOPD PIB4.     HU06NOPI PIB4.
      HU06NOPR PIB4.     HU06IUPD PIB4.
      HU06IUPP PIB4.     HU06U    PIB4.
      HU06WQFL PIB4.     HU06DLUD PIB4.
      HU06DLUP PIB4.     HU06DLUK PIB4.
      HU06DPCD PIB4.     HU06DPCP PIB4.
      HU06DPCK PIB4.     HU06LERR PIB4.
      HU06U2   PIB4.     HU06IERR PIB4.
      HU06LRUN PIB4.     HU06LCPU PIB4.
      HU06HTCB PIB4.     HU06BLEN PIB4.
      HU06BNUM PIB4.     HU06BMAX PIB4.
      HU06BCUR PIB4.     HU06BGET PIB4.
      HU06BFRE PIB4.
      @;
      OUTPUT T06;
END; * IF TYPE 06;
RETURN;
PROC PRINT;
FORMAT TIMEOUT DATETIME. ;
FORMAT HSRRST  DATETIME. ;
