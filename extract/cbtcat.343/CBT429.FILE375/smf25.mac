//ED115DM5 JOB C4SYS000,MSGCLASS=X,CLASS=J,REGION=4M,NOTIFY=C4SYSDM
//*
//*MAIN PROC=C4
// EXEC EDMXGSAS,OPTIONS='SOURCE'
//SMF    DD DISP=SHR,DSN=C4MS.OSTAR
//*MF    DD DISP=SHR,DSN=C4MS.OSTAR.SAMPLE
//SYSIN DD *
  DATA T25 (KEEP =
           SMFXXFLG          SMFNO             TIMEOUT
           SYSTEM            HSRSSI            HSRSTY
           HSRHVR            HSRJBN            HSRRST
      HU24ICHP           HU24ITIM
      HU25ICHP           HU25ITIM
      HU25S064           HU25S128
      HU25S256           HU25S512
      HU25S1K            HU25S2K
      HU25S4K            HU25SBIG
      HU25R064           HU25R128
      HU25R256           HU25R512
      HU25R1K            HU25R2K
      HU25R4K            HU25RBIG
  )
  ;
  RETAIN N 0;
  INFILE SMF MISSOVER;
  N+1;
  *F N > 1000 THEN STOP;
  INPUT @1 SMFXXFLG PIB1.    SMFNO    PIB1.    TIMEOUT SMFSTAMP8.
           SYSTEM $CHAR4.    HSRSSI   $CHAR4.  HSRSTY  PIB2.
           HSRHVR PIB2.      HSRJBN   $CHAR8.  HSRRST  SMFSTAMP8.
@  ;
*;
IF HSRSTY = 25 THEN DO;
INPUT HU25ICHP PIB2.     HU25ITIM PIB4.
      HU25S064 PIB4.     HU25S128 PIB4.
      HU25S256 PIB4.     HU25S512 PIB4.
      HU25S1K  PIB4.     HU25S2K  PIB4.
      HU25S4K  PIB4.     HU25SBIG PIB4.
      HU25R064 PIB4.     HU25R128 PIB4.
      HU25R256 PIB4.     HU25R512 PIB4.
      HU25R1K  PIB4.     HU25R2K  PIB4.
      HU25R4K  PIB4.     HU25RBIG PIB4.
      @;
      OUTPUT T25;
END; * IF TYPE 25;
RETURN;
PROC PRINT DATA=T25;
FORMAT TIMEOUT DATETIME. ;
FORMAT HSRRST  DATETIME. ;
