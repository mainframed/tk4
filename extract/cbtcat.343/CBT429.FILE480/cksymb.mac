ISREDIT MACRO
 CONTROL NOMSG NOLIST NOCONLIST NOSYMLIST NOFLUSH NOPROMPT
 CONTROL MSG LIST CONLIST SYMLIST FLUSH PROMPT
 ISREDIT (NLINA) = LINENUM .ZFIRST
 ISREDIT (NLINZ) = LINENUM .ZLAST
 FREE FI(SYSUT1)
 FREE FI(SYSUT2)
 FREE FI(D)
 SET &HLQ = &SYSUID
 SET &MLQ = T&SUBSTR(1:2,&SYSTIME)&SUBSTR(4:5,&SYSTIME)+
             &SUBSTR(7:8,&SYSTIME)
 SET &DSYSUT1 = &HLQ..&MLQ..SYSUT1
 SET &DSYSUT2 = &HLQ..&MLQ..SYSUT2
 ATTR D LRECL(80) BLKSIZE(8000) RECFM(F B) DSORG(PS)
 ALLOC FI(SYSUT1) SPACE(1,1) TRACKS CATALOG DA('&DSYSUT1') -
      UNIT(SYSALLDA) USING(D)
 ALLOC FI(SYSUT2) SPACE(1,1) TRACKS CATALOG DA('&DSYSUT2') -
      UNIT(SYSALLDA) USING(D)
 OPENFILE SYSUT1 OUTPUT
 DO WHILE &NLINA <= &NLINZ
   ISREDIT (LINA) = LINE &NLINA
   SET &SYSUT1 = &NRSTR(&LINA)
   PUTFILE SYSUT1
   SET &NLINA = &NLINA + 1
 END
 CLOSFILE SYSUT1
 CALL 'USXXBCR.CBT.LOADLIB(CKSYMBOL)'
 FREE FI(SYSUT1)
 FREE FI(SYSUT2)
 FREE FI(D)
 SUB '&DSYSUT2'
EXIT CODE(0)
