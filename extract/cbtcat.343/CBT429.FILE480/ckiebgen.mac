CKIEBGEN CKINICIO
         PRINT NOGEN
         OPEN  (SYSUT1,(INPUT))
         OPEN  (SYSUT2,(OUTPUT))
LEER     GET   SYSUT1,AREA
         PUT   SYSUT2,AREA
         B     LEER
FIN      CLOSE SYSUT1
         CLOSE SYSUT2
         CKFINAL RET=0
SYSUT1   DCB   DDNAME=SYSUT1,MACRF=GM,EODAD=FIN,DSORG=PS
SYSUT2   DCB   DDNAME=SYSUT2,MACRF=PM,DSORG=PS
AREA     DS    CL32760
         END   CKIEBGEN
