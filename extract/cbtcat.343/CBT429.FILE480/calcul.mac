/* REXX */
"ISPEXEC LIBDEF ISPPLIB DATASET ID('USXXBCR.CBT.ALL')"
msgstat = MSG('off')
DD = 0
RC2 = 0
ADDRESS ISPEXEC 'ADDPOP ROW(14) COLUMN(1)'
SYNTAX:
  RC2 = RC
  RC = 0
DO WHILE RC = 0
 signal on SYNTAX
 IF RC2 ^= 35 then interpret "RR = "DD""
 RC2 = 0
 address ISPEXEC 'DISPLAY PANEL(CALCULP)'
END
"ISPEXEC LIBDEF ISPPLIB"
EXIT 0
