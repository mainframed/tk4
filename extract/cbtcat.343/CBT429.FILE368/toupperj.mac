//OPRJAW0O JOB (1543TSM),'TOUPPERJ',CLASS=Z,MSGCLASS=R,
//   NOTIFY=&SYSUID        ALL JOBS NEED ONE
//TSOR00NP PROC EXEC=,
//            REGSIZE='32M',
//            SOUT='*',
//            TSIN=DUMMY
//***************************************************
//* EVEN THOUGH WE HAVE THE VENDOR TOOL STARTOOL    *
//* I AM SURE MOST OF THE BELOW CAN BE DONE WITH    *
//* PDS85. THE FIRST  CONTROL CARDS ALLOW US        *
//* TO RUN THE TOUPPERC EDIT MACRO AGAINST A MACLIB *
//* THAT USES MIXED CASE. XPEDITER AT THE 6.5 WITH  *
//* CSS 7.6 LEVEL DOES NOT YET SUPPORT MIXED CASE   *
//* THE OTHERS ARE GOOD FOR EXAMPLES                *
//***************************************************
//TSOR00NP EXEC PGM=IKJEFT1A,
//            DYNAMNBR=50,
//            REGION=&REGSIZE,
//            PARM='&EXEC'
//SYSHELP  DD DSN=SYS1.HELP,
//            DISP=SHR
//         DD DSN=SYSP.HELP,
//            DISP=SHR
//SYSPROC  DD DSN=AGL10.PP1000.CLIST,
//            DISP=SHR
//SYSEXEC  DD DSN=AGL10.PP1000.REXX,
//            DISP=SHR
//SYSTSPRT DD SYSOUT=&SOUT
//SYSTSIN  DD &TSIN
// PEND
//STARTOOL EXEC TSOR00NP
//SYSTSIN  DD  *
  STARTOOL 'OPRJAW0.IRX.MACLIB.UC'
  FIXPDS RESET NOCHECK
  CHANGE 'OPRJAW0.IRX.MACLIB'
  SUBLIST :
  COPY * 'OPRJAW0.IRX.MACLIB.UC' SHR
  CHANGE 'OPRJAW0.IRX.MACLIB.UC'
  SUBLIST :
  EDIT * MACRO(TOUPPERC)
  CONDEND
//
  PDSTOOLS 'OPRJAW0.PGM.LOADLIB'
  IF : RMODEANY THEN(SUBLIST)
  COPY * 'OPRJAW0.PDS.TEST2' NEW
  CONDEND
//
PDSTOOLS 'MMA20.PP1000.DB2UTIL'
SUBLIST :
REPLACE * /MMAU20/MMAP20/ WRITE
//
  PDSTOOLS 'SYSP.LINKLIB'
  ATTRIB X RESIZE
  COMPRESS
  CONDEND
//
 COPY A* 'OPRJAW0.PDS.TEST2' NEW
 CONDEND
 COPY T* 'OPRJAW0.PDS.TEST3' NEW
 END
 //
 PDSTOOLS 'OPRJAW0.RSVP'
 COPY  'OPRJAW0.PDS(TEMP)' REPLACE
 END
//
PDSTOOLS 'OPRJAW0.PGM.LOADLIB'
SUBLIST :
IF * CREATED(1999/01/01:1999/06/08) THEN(ATTRIB)
//
  PDSTOOLS 'SYS4.PP1PRD.PROCLIB'
  IF MMA*  THEN(SUBLIST)
  COPY * 'OPRJAW0.PMM.MEMBERS' NEW
  CHANGE 'OPRJAW0.PMM.MEMBERS'
  SUBLIST :
  FIND * 'LOA.LIB' THEN(SUBLIST)
  FIND * 'PGM='
