)ATTR
 _ TYPE(INPUT)  INTENS(HIGH) CAPS(ON)
 # TYPE(OUTPUT) INTENS(HIGH) SKIP(OFF)
 @ TYPE(OUTPUT) INTENS(LOW)  SKIP(ON)
)BODY WIDTH(80)
%---------------------------- D A S D   S P A C E ----------------------
%COMMAND ===>_ZCMD                                            %SCROLL ==
%
+   NAME%===>_NAME    +
+SORT BY%===>_Z+  (%1+= VOLSER;%2+= DEV NUM;%3+= TOTAL FREE;%4+= MAX CON
%
%                      DEV    IX     TOTAL FREE    EXT    MAX CONTIG
%             VOLSER   NUM   VTOC    CYLS  TRKS    NUM    CYLS  TRKS
+=======================================================================
)MODEL
&MODLINE
)INIT

 .CURSOR = NAME
 .ZVARS  = 'SORT'
 &S      = ' '

 &OLDNAME = &NAME
 &OLDSORT = &SORT

)REINIT

 REFRESH *

 IF (&S = ' ')
   .CURSOR = NAME
 &S      = ' '

)PROC

 VER (&S,LIST,B,S,MSG=FDS002)
 VER (&SORT,LIST,1,2,3,4,MSG=FDS002)

 &NAME = TRUNC(&NAME,' ')
 &NAME = '&NAME..'
 &NAME = TRUNC(&NAME,'*')
 &XTRA = .TRAIL
 IF (&XTRA = &Z)
   &NAME = TRUNC(&NAME,'.')
 IF (&XTRA ^= &Z)
   &NAME = '&NAME.*'


 IF (&NAME = &Z)
   &NAME = '&OLDNAME'

 IF (&SORT = &Z)
   &SORT = '&OLDSORT'

)END
