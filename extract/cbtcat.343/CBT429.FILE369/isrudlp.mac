%-------------------------- DATA SET LIST UTILITY ----------------------
%OPTION  ===>_ZCMD
%
%  blank+- Display data set list%*         %P+ - Print data set list
%  V    +- Display VTOC information only   %PV+- Print VTOC information
%
+Enter one or both of the parameters below:
+  DSNAME LEVEL %===>_DSNLVL                                      +
+  VOLUME       %===>_ZDLVOL+
+
+SPECIFY THE FOLLOWING, IF DISPLAYING A LIST OF DATA SETS:
+  DISPLAY FORMAT OPTION %===>_ZUFOP+  (QUICK, SHORT, or LONG)
+  CONFIRM DELETE REQUEST%===>_ZUPCD+  (YES or NO)
+
%*+The following line commands will be available when the list is displa
+
%    B+- Browse data set                %C+- Catalog data set
%    E+- Edit data set                  %U+- Uncatalog data set
%    D+- Delete entire data set         %P+- Print entire data set
%    R+- Rename entire data set         %X+- Print index listing
%    I+- Data set information           %M+- Display member list
%    S+- Data set information (short)   %Z+- Compress data set
)INIT
  .HELP = ISR34000
  &ZCMD = &ZUOPT
  &ZUFOP = TRANS (&FMOPT Q,'QUICK' S,'SHORT' L,'LONG' *,*)
  &ZUPCD = TRANS (&ZUCDR Y,'YES' N,'NO' *,*)
  .CURSOR = DSNLVL              /*  cjp  */
  IF (&DSNLVL = &Z)
    IF (&ZDLVOL = &Z)
      &DSNLVL = &ZPREFIX        /*  cjp  */
      &ZCMD   = &Z
      &ZUFOP  = QUICK
      &ZUPCD  = YES
  &COMPGM = 'IEBCOPY '

  IF (&PRCMOD = FDSPACE)                         /* PSYWAK */
    IF (&ZDLVOL ^= ' ')                          /* PSYWAK */
      IF (&SELECT = ' ')                         /* PSYWAK */
        .RESP = END                              /* PSYWAK */
    &ZDLVOL = &VOLUME                            /* PSYWAK */
    &DSNLVL = &Z                                 /* PSYWAK */
    &ZUFOP  = SHORT                              /* PSYWAK */
    &ZUPCD  = YES                                /* PSYWAK */
    &ZCMD   = '&ZDEL.SORT SIZE'                  /* PSYWAK */

)PROC
  IF (&ZCMD ^= ' ')
    VER (&ZCMD,LIST,P,V,PV)
  &ZUOPT = TRUNC (&ZCMD,2)
  IF (&ZUOPT = V,PV)
    VER (&ZDLVOL,NONBLANK)
    IF (&DSNLVL ^= ' ')
       .MSG = ISRU188
       .CURSOR = DSNLVL

  IF (&ZUOPT = ' ')
    IF (&ZDLVOL = ' ')
      VER (&DSNLVL,NONBLANK,MSG=ISRU187)
  IF (&ZUOPT = 'P ')
    IF (&ZDLVOL = ' ')
      VER (&DSNLVL,NONBLANK,MSG=ISRU187)

  &ZUCDR = TRUNC (&ZUPCD,1)
  VER (&ZUCDR,NB,LIST,Y,N)

  &FMOPT = TRUNC (&ZUFOP,1)
  VER (&FMOPT,NB,LIST,S,L,Q)

  IF (&FMOPT = S)
     &UDLPANEL = 'ISRUDSL1'
  IF (&FMOPT = L)
     &UDLPANEL = 'ISRUDSLX'
  IF (&FMOPT = Q)
     &UDLPANEL = 'ISRUDSL0'

  IF (&ZDLVOL = &Z )
     &ZUP2HDRC = 'BEGINNING WITH'
     &ZUP2HDRV = &DSNLVL

  IF (&ZDLVOL ^= &Z )
     &ZUP2HDRC = 'ON VOLUME'
     &ZUP2HDRV = &ZDLVOL
  &S1 = 'RENAME  '
  &S2 = 'DELETE  '
  &S3 = 'CATALOG '
  &S4 = 'UNCAT   '
  &S5 = 'PRINTED '
  &S6 = 'PRINTX  '
  &S7 = 'INFO-I  '
  &S8 = 'MEMBLIST'
  &S9 = 'COMPRESS'
  &S10 = 'INFO-S  '
  &S11 = 'BROWSE  '
  &S12 = 'EDIT    '
  &UPRTC = 'PPRINTED'
  &URENC = 'RRENAMED'
  &UDELC = 'DDELETED'
  VPUT (S1 S2 S3 S4 S5 S6 S7 S8 S9 S10 S11 S12 UPRTC URENC UDELC) ASIS
)END
