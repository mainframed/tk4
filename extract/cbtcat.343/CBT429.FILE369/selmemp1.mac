)ATTR
 _ TYPE(INPUT)  INTENS(HIGH) CAPS(ON)
 @ TYPE(OUTPUT) INTENS(LOW)
)BODY WIDTH(80)
%SELECT MEMBER --- &DSNAME ---------------------------------------------
%COMMAND ===>_ZCMD                                            %SCROLL ==
+   NAME                VV.MM   CREATED  LAST MODIFIED   SIZE  INIT   MO
+=======================================================================
)MODEL
_S@MEMBER              @STATS
)INIT
 .HELP = SELMEMH0
 .ZVARS = 'ZSCML'
 &S = ' '
 &ZCMD = ' '
)REINIT
 &ZCMD = ' '
 IF (.MSG=' ')
   &S = ' '
   REFRESH(S)
)PROC
 VER (&S,LIST,S,MSG=ISRM007)
 VPUT (ZSCML) PROFILE
 &ZCMD = ' '
)END
