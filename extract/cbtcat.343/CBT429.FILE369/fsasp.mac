)ATTR
 $ TYPE(TEXT) SKIP(ON) INTENS(LOW)
)BODY
%-------------------- SAS: STATISTICAL ANALYSIS SYSTEM --&SM------------
%OPTION  ===>_ZCMD
%                                                             %USERID -
%   E  +EDIT               - Edit a SAS program               %TIME   -
%   R  +RUN                - Run a SAS program                %DATE   -
%  1,2 +BROWSE OUTPUT      - 1 = LOG, 2 = LIST
%   A  +ALLOCATE           - Allocate an OS dataset
%   F  +FREE               - Free an OS dataset
%   I  +INTERACTIVE        - Invoke SAS interactive mode (%ENDSAS;+to EX
%   P  +PRINT              - Print LOG, LIST, or SAS program
%
+      SAS PROGRAM DSN ===>_SASPGM
$          SAS OPTIONS ===>_SASOPT
$
+    PRINT DESTINATION ===>_SASPDEST$( LOCAL, APS1, APS2, APS3, RMT1, ET
+            PRINT LOG ===>_Z$ LIST ===>_Z$ PGM ===>_Z$  ( enter "Y" to
%
+DDN ===>_SASDD1  $DSN ===>_SASDSN1                              $DISP =
+DDN ===>_SASDD2  $DSN ===>_SASDSN2                              $DISP =
+DDN ===>_SASDD3  $DSN ===>_SASDSN3                              $DISP =
%
+Enter%END+command to terminate SAS dialogs.
)INIT
 &ZPRIM = YES
 .ZVARS = '(SASP1 SASP2 SASP3 SASDISP1 SASDISP2 SASDISP3)'
  .HELP    = ISR00003
 .CURSOR = ZCMD
  &ZPRIM   = YES        /* ALWAYS A PRIMARY OPTION MENU      */
  &ZHTOP   = ISR00003   /* TUTORIAL TABLE OF CONTENTS        */
  &ZHINDEX = ISR91000   /* TUTORIAL INDEX - 1ST PAGE         */
  VPUT (ZHTOP,ZHINDEX) PROFILE
  IF (&ERRMSG ^= ' ')
   .MSG = &ERRMSG
  &ERRMSG = ' '
  &SM = '---'
  IF (&ERRSM ^= ' ')
    &SM = &ERRSM
)PROC
  &OPT = &ZCMD
  &SEL = TRANS( TRUNC (&OPT,'.')
   C,'CMD(%FSASCMD)'
   E,'CMD(%FSASCMD)'
   R,'CMD(%FSASCMD)'
   1,'CMD(%FSASCMD)'
   2,'CMD(%FSASCMD)'
   A,'CMD(%FSASCMD)'
   F,'CMD(%FSASCMD)'
   I,'CMD(%FSASCMD)'
   P,'CMD(%FSASCMD)'
   X,EXIT
   ' ',' '
   *, '?' )
&ERRSM = ' '
&TYPE = TRUNC (&OPT,'.')
IF (&TYPE =C E )
  VER (&SASPGM,NONBLANK)
  VER (&SASPGM,DSNAME)
IF (&TYPE =R )
  VER (&SASPGM,NONBLANK)
  VER (&SASPGM,DSNAME)
IF (&TYPE =A )
  IF (&SASDD3 = ' ')
    IF (&SASDD2 = ' ')
      VER(&SASDD1,NONBLANK)
  IF (&SASDD1 ^= ' ')
    VER(&SASDD1,NONBLANK)
    VER(&SASDD1,NAME)
    VER(&SASDSN1,NONBLANK)
    IF (&SASDSN1 ^= '*')
      VER(&SASDSN1,DSNAME)
    &DISP = TRUNC(&SASDISP1,1)                     /* PSYWAK          *
    VER(&DISP LIST N O S M)                        /* PSYWAK          *
    &SASDISP1 = TRANS(&DISP,                       /* PSYWAK          *
                       N,'NEW'                     /* PSYWAK          *
                       O,'OLD'                     /* PSYWAK          *
                       S,'SHR'                     /* PSYWAK          *
                       M,'MOD'                     /* PSYWAK          *
                       *,'SHR')                    /* PSYWAK          *
  IF (&SASDD2 ^= ' ')
    VER(&SASDD2,NONBLANK)
    VER(&SASDD2,NAME)
    VER(&SASDSN2,NONBLANK)
    IF (&SASDSN2 ^= '*')
      VER(&SASDSN2,DSNAME)
    &DISP = TRUNC(&SASDISP2,1)                   /* PSYWAK          */
    VER(&DISP LIST N O S M)                      /* PSYWAK          */
    &SASDISP2 = TRANS(&DISP,                     /* PSYWAK          */
                       N,'NEW'                   /* PSYWAK          */
                       O,'OLD'                   /* PSYWAK          */
                       S,'SHR'                   /* PSYWAK          */
                       M,'MOD'                   /* PSYWAK          */
                       *,'SHR')                  /* PSYWAK          */
  IF (&SASDD3 ^= ' ')
    VER(&SASDD3,NONBLANK)
    VER(&SASDD3,NAME)
    VER(&SASDSN3,NONBLANK)
    IF (&SASDSN3 ^= '*')
      VER(&SASDSN3,DSNAME)
    &DISP = TRUNC(&SASDISP3,1)                   /* PSYWAK          */
    VER(&DISP LIST N O S M)                      /* PSYWAK          */
    &SASDISP3 = TRANS(&DISP,                     /* PSYWAK          */
                       N,'NEW'                   /* PSYWAK          */
                       O,'OLD'                   /* PSYWAK          */
                       S,'SHR'                   /* PSYWAK          */
                       M,'MOD'                   /* PSYWAK          */
                       *,'SHR')                  /* PSYWAK          */
IF (&TYPE =F )
  VER(&SASDD1,NONBLANK)
  VER(&SASDD1,NAME)
  IF (&SASDD2 ^= ' ')
    VER(&SASDD2,NONBLANK)
    VER(&SASDD2,NAME)
  IF (&SASDD3 ^= ' ')
    VER(&SASDD3,NONBLANK)
    VER(&SASDD3,NAME)
IF (&TYPE =P )
  IF  (&SASPDEST = ' ') &SASPDEST=LOCAL
  VER (&SASPDEST,NAME)
  IF (&SASP3 = ' ')
    IF (&SASP2 = ' ')
      VER(&SASP1,NONBLANK)
  IF (&SASP1 ^= ' ')
    VER(&SASP1,LIST,Y)
  IF (&SASP2 ^= ' ')
    VER(&SASP2,LIST,Y)
  IF (&SASP3 ^= ' ')
    VER(&SASP3,LIST,Y)
)END
