)ATTR
 % TYPE(TEXT) INTENS(HIGH) SKIP(ON)
 + TYPE(TEXT) INTENS(LOW) SKIP(ON)
 ! TYPE(TEXT) COLOR(PINK) SKIP(ON)
 ` TYPE(TEXT) COLOR(YELLOW) SKIP(ON)
 × TYPE(TEXT) COLOR(BLUE) HILITE(REVERSE) SKIP(ON)
 Ö TYPE(TEXT) COLOR(TURQ) HILITE(REVERSE) SKIP(ON)
 ^ TYPE(TEXT) COLOR(WHITE) HILITE(USCORE) SKIP(ON)
 @ TYPE(OUTPUT) COLOR(RED) SKIP(ON)
 # TYPE(OUTPUT) COLOR(GREEN) JUST(ASIS) CAPS(ON) SKIP(ON)
)BODY
Ö                       % EDIT PROFILES TABLE ANALYSIS Ö
`Command ===>_ZCMD
+
+
!USERID :@JHEDPRID+    !APPLID :@JHEDPRAP+
×
×  +
×  +^KEYS :`(variables names)+
×  +    #PMK01
×  +    #PMK02
×  +    #PMK03
×  +^NAMES :`(variables names)+
×  +    #PMN01
×  +    #PMN02
×  +    #PMN03
×  +    #PMN04
×  +^SAVE :`(variables names)+
×  +    #PMS01
×  +    #PMS02
×  +    #PMS03
×  +
×
+
)INIT
  .HELP = PAJTVP03
  &ZCMD = &Z
)PROC
  &PCM = TRANS(TRUNC(&ZCMD,' ') PL,PLIST PLS,PLIST PLST,PLIST *,*)
  IF (&PCM ^= &Z)
    VER (&PCM,LIST,PLIST,MSG=PAJV000V)
    &ZTRAIL = .TRAIL
    &PCL = &Z
    &PDS = &Z
    IF (&ZTRAIL ^= &Z)
      &PDS = TRUNC(&ZTRAIL,' ')
      &PCL = .TRAIL
      IF (&PDS = 'LOCAL')
        &PDS = &Z
      IF (&PDS ^= 'LOCAL')
        &D = TRUNC(&PDS,1)
        IF (&PDS ^= &D)
          VER (&D,LIST,U,R,MSG=PAJV000U)
          &D = .TRAIL
          VER (&D,NB,NUM,MSG=PAJV000U)
        IF (&PDS = &D)
          &PDS = &PCL
          &PCL = &D
          IF (&PDS ^= &Z)
            IF (&PDS = 'LOCAL')
              &PDS = &Z
            IF (&PDS ^= 'LOCAL')
              &D = TRUNC(&PDS,1)
              VER (&D,LIST,U,R,MSG=PAJV000U)
              &D = .TRAIL
              VER (&D,NB,NUM,MSG=PAJV000U)
)END
