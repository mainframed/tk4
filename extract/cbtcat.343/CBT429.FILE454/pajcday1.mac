PROC     0
         ISPEXEC VGET (TSFX)
         ISPEXEC TBOPEN TWKD&TSFX NOWRITE
         SET &CC = &LASTCC
         IF &CC = 0 THEN GOTO DISPL
         IF &CC = 8 THEN DO
             ISPEXEC DISPLAY MSG(PAJD000E)
             EXIT
             END
         ISPEXEC DISPLAY MSG(PAJD000F)
         EXIT
         END
DISPL:   ISPEXEC TBDISPL TWKD&TSFX PANEL(PAJDAYS1)
         ISPEXEC TBEND TWKD&TSFX
