)ATTR
 # TYPE(INPUT) INTENS(LOW) CAPS(ON)
 @ TYPE(OUTPUT) INTENS(LOW) SKIP(ON)
)BODY
%-------------------- Additional local Edit macros Updates -------------
%COMMAND ===>_ZCMD                                            %SCROLL ==
+
+S Mac.name  Macro function (hit%&HPF+to get explanation of various func
+- --------- -----------------------------------------------------------
)MODEL CLEAR(A)
#A@Z        @Z
)INIT
  .HELP = PAJTEMU
  &HPF = PFK(HELP)
  IF (&HPF = &Z)
    &HPF = 'HELP'
  &VARS = '(MACNAME MACDESC)'
  IF (&SCRL = &Z)
    &SCRL = PAGE
  &ZTDMARK = '============================ Bottom of macros list +
              ============================'
  &ZCMD = &Z
)PROC
  IF (&ZTDSELS ^= 0000)
    &EMOPTN = TRUNC(&A,1)
)END
