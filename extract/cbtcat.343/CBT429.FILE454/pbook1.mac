)ATTR
# TYPE(INPUT) INTENS(LOW) CAPS(ON)
@ TYPE(OUTPUT) INTENS(LOW) SKIP(ON)
+ TYPE(TEXT) INTENS(LOW) SKIP(ON)
)BODY EXPAND(//)
%-/-/- IBM Manuals List -/-/-
%COMMAND ===>_ZCMD                                            %SCROLL ==
+
+Current SORT order :&CBSRTF
%-/-/- Manuals list -/-/-
%Act  Key-1    Key-2    Manual no.  Title
%                                   Date         Room          TNL no.
%--- -------- -------- ------------ ----------------------------------
)MODEL CLEAR(A)
#A  @CBKW1   @CBKW2   @CBMNO       @CBTMN                              @
+                                  @CBDTE  +    @CBROOM  +      @CBNW0+
)INIT
  .HELP = PBOOKT1
  &ZHTOP = PBOOKT1
  .AUTOSEL = NO
  IF (&CRP ^= &Z)
    .CSRROW = &CRP
  IF (&AMT = &Z)
    &AMT = PAGE
  &ZTDMARK = '=========================== Bottom of Manuals list +
              ============================'
  &CBCURM = TRANS(&ZMONTH
            01,January 02,February 03,March 04,April 05,May 06,June
            07,July 08,August 09,September 10,October 11,November 12,Dec
  &CBCURD = '&ZDAY &CBCURM 19&ZYEAR'
  VPUT (CBCURD) SHARED
)REINIT
  IF (.MSG ^= &Z)
    .ATTR(.CURSOR) = 'COLOR(RED)'
    .AUTOSEL = YES
    .CSRROW = &CRP
)PROC
  &CMD = TRANS(TRUNC(&ZCMD,' ')
         I,INPUT INP,INPUT T,TNL CAN,CANCEL F,FIND C,CONT CONTINUE,CONT
         L,LIST LST,LIST LF,FULL *,*)
  VER (&CMD,LIST,INPUT,TNL,CANCEL,FIND,CONT,LIST,FULL,SORT,SAVE,MSG=MBKS
  IF (&CMD = 'FIND')
    &ZTRAIL = .TRAIL
  IF (&CMD = 'SORT')
    &ZTRAIL = .TRAIL
    &SFCB1 = TRANS(TRUNC(&ZTRAIL,' ')
             'KEY-1',K1 'KEY-1(A)',K1 'K1(A)',K1 'KEY-1(D)',K1I 'K1(D)',
             'KEY-2',K2 'KEY-2(A)',K2 'K2(A)',K2 'KEY-2(D)',K2I 'K2(D)',
             MANUAL,M MAN,M 'MANUAL(A)',M 'MAN(A)',M 'M(A)',M 'MANUAL(D)
             'MAN(D)',MI 'M(D)',MI
             LOCATION,L LOC,L 'LOCATION(A)',L 'LOC(A)',L 'L(A)',L
             'LOCATION(D)',LI 'LOC(D)',LI 'L(D)',LI
             DATE,D 'DATE(A)',D 'D(A)',D 'DATE(D)',DI 'D(D)',DI
             ROOM,R 'ROOM(A)',R 'R(A)',R 'ROOM(D)',RI 'R(D)',RI *,*)
    VER (&SFCB1,LIST,K1,K2,M,L,D,R,K1I,K2I,MI,LI,DI,RI MSG=MBKS003S)
    &ZTRAIL = .TRAIL
    &SFCB2 = TRANS(TRUNC(&ZTRAIL,' ')
             'KEY-1',K1 'KEY-1(A)',K1 'K1(A)',K1 'KEY-1(D)',K1I 'K1(D)',
             'KEY-2',K2 'KEY-2(A)',K2 'K2(A)',K2 'KEY-2(D)',K2I 'K2(D)',
             MANUAL,M MAN,M 'MANUAL(A)',M 'MAN(A)',M 'M(A)',M 'MANUAL(D)
             'MAN(D)',MI 'M(D)',MI
             LOCATION,L LOC,L 'LOCATION(A)',L 'LOC(A)',L 'L(A)',L
             'LOCATION(D)',LI 'LOC(D)',LI 'L(D)',LI
             DATE,D 'DATE(A)',D 'D(A)',D 'DATE(D)',DI 'D(D)',DI
             ROOM,R 'ROOM(A)',R 'R(A)',R 'ROOM(D)',RI 'R(D)',RI *,*)
    VER (&SFCB2,LIST,K1,K2,M,L,D,R,K1I,K2I,MI,LI,DI,RI MSG=MBKS003S)
    &ZTRAIL = .TRAIL
    &SFCB3 = TRANS(TRUNC(&ZTRAIL,' ')
             'KEY-1',K1 'KEY-1(A)',K1 'K1(A)',K1 'KEY-1(D)',K1I 'K1(D)',
             'KEY-2',K2 'KEY-2(A)',K2 'K2(A)',K2 'KEY-2(D)',K2I 'K2(D)',
             MANUAL,M MAN,M 'MANUAL(A)',M 'MAN(A)',M 'M(A)',M 'MANUAL(D)
             'MAN(D)',MI 'M(D)',MI
             LOCATION,L LOC,L 'LOCATION(A)',L 'LOC(A)',L 'L(A)',L
             'LOCATION(D)',LI 'LOC(D)',LI 'L(D)',LI
             DATE,D 'DATE(A)',D 'D(A)',D 'DATE(D)',DI 'D(D)',DI
             ROOM,R 'ROOM(A)',R 'R(A)',R 'ROOM(D)',RI 'R(D)',RI *,*)
    VER (&SFCB3,LIST,K1,K2,M,L,D,R,K1I,K2I,MI,LI,DI,RI MSG=MBKS003S)
    &ZTRAIL = .TRAIL
    &SFCB4 = TRANS(TRUNC(&ZTRAIL,' ')
             'KEY-1',K1 'KEY-1(A)',K1 'K1(A)',K1 'KEY-1(D)',K1I 'K1(D)',
             'KEY-2',K2 'KEY-2(A)',K2 'K2(A)',K2 'KEY-2(D)',K2I 'K2(D)',
             MANUAL,M MAN,M 'MANUAL(A)',M 'MAN(A)',M 'M(A)',M 'MANUAL(D)
             'MAN(D)',MI 'M(D)',MI
             LOCATION,L LOC,L 'LOCATION(A)',L 'LOC(A)',L 'L(A)',L
             'LOCATION(D)',LI 'LOC(D)',LI 'L(D)',LI
             DATE,D 'DATE(A)',D 'D(A)',D 'DATE(D)',DI 'D(D)',DI
             ROOM,R 'ROOM(A)',R 'R(A)',R 'ROOM(D)',RI 'R(D)',RI *,*)
    VER (&SFCB4,LIST,K1,K2,M,L,D,R,K1I,K2I,MI,LI,DI,RI MSG=MBKS003S)
    &ZTRAIL = .TRAIL
    &SFCB5 = TRANS(TRUNC(&ZTRAIL,' ')
             'KEY-1',K1 'KEY-1(A)',K1 'K1(A)',K1 'KEY-1(D)',K1I 'K1(D)',
             'KEY-2',K2 'KEY-2(A)',K2 'K2(A)',K2 'KEY-2(D)',K2I 'K2(D)',
             MANUAL,M MAN,M 'MANUAL(A)',M 'MAN(A)',M 'M(A)',M 'MANUAL(D)
             'MAN(D)',MI 'M(D)',MI
             LOCATION,L LOC,L 'LOCATION(A)',L 'LOC(A)',L 'L(A)',L
             'LOCATION(D)',LI 'LOC(D)',LI 'L(D)',LI
             DATE,D 'DATE(A)',D 'D(A)',D 'DATE(D)',DI 'D(D)',DI
             ROOM,R 'ROOM(A)',R 'R(A)',R 'ROOM(D)',RI 'R(D)',RI *,*)
    VER (&SFCB5,LIST,K1,K2,M,L,D,R,K1I,K2I,MI,LI,DI,RI MSG=MBKS003S)
    &ZTRAIL = .TRAIL
    &SFCB6 = TRANS(TRUNC(&ZTRAIL,' ')
             'KEY-1',K1 'KEY-1(A)',K1 'K1(A)',K1 'KEY-1(D)',K1I 'K1(D)',
             'KEY-2',K2 'KEY-2(A)',K2 'K2(A)',K2 'KEY-2(D)',K2I 'K2(D)',
             MANUAL,M MAN,M 'MANUAL(A)',M 'MAN(A)',M 'M(A)',M 'MANUAL(D)
             'MAN(D)',MI 'M(D)',MI
             LOCATION,L LOC,L 'LOCATION(A)',L 'LOC(A)',L 'L(A)',L
             'LOCATION(D)',LI 'LOC(D)',LI 'L(D)',LI
             DATE,D 'DATE(A)',D 'D(A)',D 'DATE(D)',DI 'D(D)',DI
             ROOM,R 'ROOM(A)',R 'R(A)',R 'ROOM(D)',RI 'R(D)',RI *,*)
    VER (&SFCB6,LIST,K1,K2,M,L,D,R,K1I,K2I,MI,LI,DI,RI MSG=MBKS003S)
    &ZTRAIL = .TRAIL
  IF (&CMD = 'INPUT')
    &CBIN = .TRAIL
    IF (&CBIN = &Z)
      &CBIN = 1
    VER (&CBIN,NB,NUM)
  IF (&CMD = 'TNL')
    &CBIN = .TRAIL
    IF (&CBIN = &Z)
      &CBIN = 1
    VER (&CBIN,NB,NUM)
  IF (&CMD = 'LIST','FULL')
    &ZTRAIL = .TRAIL
    &CBPCLAS = ' '
    &CBPDEST = ' '
    IF (&ZTRAIL ^= &Z)
      &CBPDEST = TRUNC(&ZTRAIL,' ')
      &CBPCLAS = .TRAIL
      IF (&CBPDEST = 'LOCAL')
        &CBPDEST = ' '
      IF (&CBPDEST ^= 'LOCAL')
        &D = TRUNC(&CBPDEST,1)
        IF (&CBPDEST ^= &D)
          VER (&D,LIST,U,R,MSG=MBKS003D)
          &D = .TRAIL
          VER (&D,NB,NUM,MSG=MBKS003D)
        IF (&CBPDEST = &D)
          &CBPDEST = &CBPCLAS
          &CBPCLAS = &D
          IF (&CBPDEST ^= &Z)
            IF (&CBPDEST = 'LOCAL')
              &CBPDEST = ' '
            IF (&CBPDEST ^= 'LOCAL')
              &D = TRUNC(&CBPDEST,1)
              VER (&D,LIST,U,R,MSG=MBKS003D)
              &D = .TRAIL
              VER (&D,NB,NUM,MSG=MBKS003D)
  IF (&ZTDSELS ^= 0000)
    &ACT = TRUNC(&A,1)
    VER (&ACT,LIST,D,R,S,T,U,MSG=MBKS003L)
    &N = .TRAIL
    IF (&N = &Z)
      &N = 1
    VER (&N,NB,NUM)
)END
