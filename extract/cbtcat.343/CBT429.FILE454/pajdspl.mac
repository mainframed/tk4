)ATTR
 _ TYPE(INPUT)  INTENS(HIGH) CAPS(ON)
 @ TYPE(OUTPUT) INTENS(LOW)
)BODY WIDTH(80)
%DSPRT --- Select member : &PDSN ---------------------------------------
%Command ===>_ZCMD                                            %Scroll ==
+   Name                VV.MM   Created  Last modified  Size  Init   Mod
+=======================================================================
)MODEL
_S@MEMBER              @STATS
)INIT
 .HELP = PAJTDSPL
 .ZVARS = 'ZSCML'
 &S = ' '
 &ZCMD = ' '
 &ZTDMARK = '============================ Bottom of names list +
             ============================='
)REINIT
 IF (.MSG = &Z)
   &S = ' '
   REFRESH(S)
)PROC
 IF (&ZCMD ^= &Z)
   &TBCMD = TRANS (TRUNC (&ZCMD,' ')
            D,D DIR,D DIRECTORY,D
            L,L LOC,L LOCATE,L *,*)
   VER (&TBCMD,LIST,D,L,MSG=PAJX000A)
   &TBMNM = .TRAIL
 IF (&ZCMD = &Z)
   &TBCMD = ' '
   &TBMNM = ' '
 VPUT (ZSCML) PROFILE
)END
