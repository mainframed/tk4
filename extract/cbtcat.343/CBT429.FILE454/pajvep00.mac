)ATTR
 \ TYPE(INPUT) INTENS(HIGH) CAPS(ON) HILITE(USCORE)
 % TYPE(TEXT) INTENS(HIGH) SKIP(ON)
 + TYPE(TEXT) INTENS(LOW) SKIP(ON)
 { TYPE(TEXT) COLOR(GREEN) SKIP(ON)
 ! TYPE(TEXT) COLOR(PINK) SKIP(ON)
 | TYPE(TEXT) COLOR(TURQ) SKIP(ON)
 ` TYPE(TEXT) COLOR(YELLOW) SKIP(ON)
 @ TYPE(TEXT) HILITE(REVERSE) SKIP(ON)
 × TYPE(TEXT) COLOR(BLUE) HILITE(REVERSE) SKIP(ON)
 Ö TYPE(TEXT) COLOR(TURQ) HILITE(REVERSE) SKIP(ON)
 # TYPE(OUTPUT) COLOR(YELLOW) JUST(ASIS) CAPS(OFF) SKIP(ON)
)BODY
Ö                           % VIEW EDIT PROFILES Ö
`Command ===>_ZCMD
+
+
×
×  +
×  +           {Enter!USERID%===>\Z      +
×  +                             |(project)+
×  +     {to access data-set%===>\Z                                   +
×  +                             |(group.type)+
×  +             {and!APPLID%===>\Z   {to access`...EDIT{profile,+
×  +                             |(member)+
×  +                             {or`APPLID|command allows the selection
×  +
×
+
+
+                                                          @
+                                                          @  + #PMIND
+                                                          @
+
)INIT
  .HELP = PAJTVP00
  .ZVARS = '(JHEDPRID JHEDPRDS JHEDPRAP)'
  .CURSOR = JHEDPRID
  &ZCMD = &Z
  IF (&PMT = DLT)
     &PMIND = ' DELETE  '
    .ATTRCHAR(@) = 'COLOR(RED)'
  IF (&PMT = SLM)
     &PMIND = 'S-APPLID '
    .ATTRCHAR(@) = 'COLOR(WHITE)'
  IF (&PMT = TBL)
     &PMIND = ' A-Table '
    .ATTRCHAR(@) = 'COLOR(TURQ)'
  IF (&PMT = ON)
     &PMIND = 'Trace ON '
    .ATTRCHAR(@) = 'COLOR(PINK)'
  IF (&PMT = OFF)
     &PMIND = '(display)'
    .ATTRCHAR(@) = 'COLOR(GREEN)'
  IF (&JHEDPRID = &Z)
    &JHEDPRID = &ZUSER
  IF (&JHEDPRDS = &Z)
    IF (&DSNME ^= &Z)
      &PMC = TRUNC(&DSNME,'.')
      &JHEDPRDS = .TRAIL
  IF (&JHEDPRAP = &Z)
    &JHEDPRAP = &ZAPPLID
)REINIT
  IF (.CURSOR = &Z)
    .CURSOR = JHEDPRID
)PROC
  &PCM = TRANS(TRUNC(&ZCMD,' ') A,APPLID AP,APPLID APPL,APPLID D,DELETE
         DE,DELETE DEL,DELETE T,TRACE TA,TABLE TB,TABLE TR,TRACE TAB,TAB
  IF (&PCM ^= &Z)
    VER (&PCM,LIST,APPLID,DELETE,TABLE,TRACE,MSG=PAJV000W)
    &ZTRAIL = .TRAIL
    IF (&PCM = APPLID)
      &PMT = SLM
    IF (&PCM = DELETE)
      &PMT = DLT
    IF (&PCM = TABLE)
      &PMT = TBL
    IF (&PCM = TRACE)
      IF (&ZTRAIL = &Z)
        &PMT = OFF
      IF (&ZTRAIL ^= &Z)
        &PMT = TRUNC(&ZTRAIL,' ')
        IF (&PMT ^= ON)
          &PMT = OFF
  IF (&PCM = &Z)
    VER (&JHEDPRID,NB,NAME)
    VER (&JHEDPRDS,NB)
    IF (&PMT ^= SLM)
      VER (&JHEDPRAP,NB,NAME)
)END
