)ATTR
@ TYPE(INPUT) INTENS(LOW) CAPS(OFF)
# TYPE(INPUT) INTENS(LOW) CAPS(ON)
+ TYPE(TEXT) INTENS(LOW) SKIP(ON)
% TYPE(TEXT) INTENS(HIGH) SKIP(ON)
)BODY EXPAND(//)
%-/-/- IBM Manuals List : FIND process -/-/-
%COMMAND ===>_ZCMD
+
%Specify your FIND argument for search below :
+
+  Manual no.  %===>#CBF1%-#CBF2%-#NW+(IBM format GGGG - NNNN - S or SS)
+      or name %===>#CBFN        +    (or up to 12 characters name)
+  Title       %===>@CBFT                              +(up to 34 charac
+  Key-1       %===>#CBFK1   +        (up to 8 characters keyword-1 name
+  Key-2       %===>#CBFK2   +        (up to 8 characters keyword-2 name
+  Location    %===>#CBV +            (sequence, up to 4 numerics, forma
+              %===>#CBW+             (number, up to 3 numerics, format
+  Room        %===>#CBFR    +        (up to 8 characters room name)
+  Date        %===>#CBFD   +         (DDMMMYY, where MMM is JAN, FEB, .
+  TNL number  %===>#CBFX%-#CBFY+     (IBM format GGGG - NNNN)
+
%Note :+only one field argument is accepted by the FIND command.
+
+Hit%ENTER+to execute the FIND process
+Hit%&EPF+to cancel this operation and return to Manuals list panel
+Hit%&HPF+to get explanation about Manuals List facility
)INIT
  &HPF = PFK(HELP)
  IF (&HPF = &Z)
    &HPF = 'HELP'
  &EPF = PFK(END)
  IF (&EPF = &Z)
    &EPF = 'END'
  &D = TRUNC(&EPF,2)
  &DT = .TRAIL
  &D = TRUNC(&DT,1)
  IF (&D = '0')
    &DT = .TRAIL
    &EPF = 'PF&DT'
  &ZCMD = &Z
  .CURSOR = CBF1
  &CBF1 = &Z
  &CBF2 = &Z
  &NW = &Z
  &CBFN = &Z
  &CBFT = &Z
  &CBFK1 = &Z
  &CBFK2 = &Z
  &CBV = &Z
  &CBW = &Z
  &CBFR = &Z
  &CBFD = &Z
  &CBFDS = &Z
  &CBFX = &Z
  &CBFY = &Z
)PROC
  &CBSW = NO
  IF (&CBF1 ^= &Z)
    IF (&CBFN ^= &Z)
      .MSG = MBKS006A
    IF (&CBFT ^= &Z)
      .MSG = MBKS006E
    IF (&CBFK1 ^= &Z)
      .MSG = MBKS006E
    IF (&CBFK2 ^= &Z)
      .MSG = MBKS006E
    IF (&CBV ^= &Z)
      .MSG = MBKS006E
    IF (&CBW ^= &Z)
      .MSG = MBKS006E
    IF (&CBFR ^= &Z)
      .MSG = MBKS006E
    IF (&CBFD ^= &Z)
      .MSG = MBKS006E
    IF (&CBFX ^= &Z)
      .MSG = MBKS006E
    IF (&CBFY ^= &Z)
      .MSG = MBKS006E
    VER (&CBF1,PICT,CCNN)
      IF (&CBF2 ^= &Z)
        VER (&CBF2,PICT,NNNN)
      IF (&NW ^= &Z)
        VER (&NW,NB,NUM)
        IF (&CBF2 = &Z)
          .MSG = MBKS006B
    &CBSW = YES
  IF (&CBF1 = &Z)
    IF (&CBF2 ^= &Z)
      .MSG = MBKS006B
    IF (&NW ^= &Z)
      .MSG = MBKS006B
  IF (&CBFN ^= &Z)
    IF (&CBFT ^= &Z)
      .MSG = MBKS006E
    IF (&CBFK1 ^= &Z)
      .MSG = MBKS006E
    IF (&CBFK2 ^= &Z)
      .MSG = MBKS006E
    IF (&CBV ^= &Z)
      .MSG = MBKS006E
    IF (&CBW ^= &Z)
      .MSG = MBKS006E
    IF (&CBFR ^= &Z)
      .MSG = MBKS006E
    IF (&CBFD ^= &Z)
      .MSG = MBKS006E
    IF (&CBFX ^= &Z)
      .MSG = MBKS006E
    IF (&CBFY ^= &Z)
      .MSG = MBKS006E
    &CBSW = YES
  IF (&CBFT ^= &Z)
    IF (&CBFK1 ^= &Z)
      .MSG = MBKS006E
    IF (&CBFK2 ^= &Z)
      .MSG = MBKS006E
    IF (&CBV ^= &Z)
      .MSG = MBKS006E
    IF (&CBW ^= &Z)
      .MSG = MBKS006E
    IF (&CBFR ^= &Z)
      .MSG = MBKS006E
    IF (&CBFD ^= &Z)
      .MSG = MBKS006E
    IF (&CBFX ^= &Z)
      .MSG = MBKS006E
    IF (&CBFY ^= &Z)
      .MSG = MBKS006E
    &CBSW = YES
  IF (&CBFK1 ^= &Z)
    IF (&CBFK2 ^= &Z)
      .MSG = MBKS006E
    IF (&CBV ^= &Z)
      .MSG = MBKS006E
    IF (&CBW ^= &Z)
      .MSG = MBKS006E
    IF (&CBFR ^= &Z)
      .MSG = MBKS006E
    IF (&CBFD ^= &Z)
      .MSG = MBKS006E
    IF (&CBFX ^= &Z)
      .MSG = MBKS006E
    IF (&CBFY ^= &Z)
      .MSG = MBKS006E
    &CBSW = YES
  IF (&CBFK2 ^= &Z)
    IF (&CBV ^= &Z)
      .MSG = MBKS006E
    IF (&CBW ^= &Z)
      .MSG = MBKS006E
    IF (&CBFR ^= &Z)
      .MSG = MBKS006E
    IF (&CBFD ^= &Z)
      .MSG = MBKS006E
    IF (&CBFX ^= &Z)
      .MSG = MBKS006E
    IF (&CBFY ^= &Z)
      .MSG = MBKS006E
    &CBSW = YES
  IF (&CBV ^= &Z)
    IF (&CBFR ^= &Z)
      .MSG = MBKS006E
    IF (&CBFD ^= &Z)
      .MSG = MBKS006E
    IF (&CBFX ^= &Z)
      .MSG = MBKS006E
    IF (&CBFY ^= &Z)
      .MSG = MBKS006E
    VER (&CBV,NB,NUM)
    VER (&CBW,NB,NUM)
    &CBSW = YES
  IF (&CBFR ^= &Z)
    IF (&CBFD ^= &Z)
      .MSG = MBKS006E
    IF (&CBFX ^= &Z)
      .MSG = MBKS006E
    IF (&CBFY ^= &Z)
      .MSG = MBKS006E
    &CBSW = YES
  IF (&CBFD ^= &Z)
    IF (&CBFX ^= &Z)
      .MSG = MBKS006E
    IF (&CBFY ^= &Z)
      .MSG = MBKS006E
    &DD = TRUNC(&CBFD,2)
    VER (&DD,NB,NUM,MSG=MBKS006D)
    &ZTRAIL = .TRAIL
    &MM = TRANS(TRUNC(&ZTRAIL,3)
                JAN,01 FEB,02 MAR,03 APR,04 MAY,05 JUN,06
                JUL,07 AUG,08 SEP,09 OCT,10 NOV,11 DEC,12
                MSG=MBKS006D)
    &YY = .TRAIL
    VER (&YY,NB,NUM,MSG=MBKS006D)
    &CBFDS = '&YY.&MM.&DD'
    &CBSW = YES
  IF (&CBFX ^= &Z)
    VER (&CBFX,PICT,CCNN)
    IF (&CBFY ^= &Z)
      VER (&CBFY,PICT,NNNN)
    &CBSW = YES
  IF (&CBSW = NO)
    .CURSOR = CBF1
    .MSG = MBKS006C
)END
