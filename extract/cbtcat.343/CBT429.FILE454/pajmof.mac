%------------------* MISCELLANEOUS OPERATORS FACILITIES *---------------
%SELECT OPTION ===>_MOFN
+                                                           +USERID   -%
%   B +BRODSCAN  - Analyze Broadcast TSO data-set.          +PREFIX   -%
+                - Browse  :_MOFB    +  (YES/NO)            +TODAY    -%
+                                                           +JULIAN   -%
%   U +BRODUNLD  - Broadcast User'id Messages Unload.       +TIME     -%
+                - USERID  :_USID    +  (User'id name)
+
%   T +Test AUTO - Input DS:_LDDS
+                - Member  :_LDMB    +  (starting member name)
+
)INIT
  .HELP = PAJTMOF
  IF (&MOFN = ' ')
    .CURSOR = LDMB
    &MOFN = T
    &MOFB = TRANS (&MOFB ' ',YES *,*)
    &LDDS = ...OPERATOR.LIBRARY...
    &USID = ' '
  IF (&MOFN = T)
    .CURSOR = LDMB
  IF (&MOFN = U)
    .CURSOR = USID
  IF (&MOFN ^= T)
    IF (&MOFN ^= U)
      .CURSOR = MOFN
)REINIT
  IF (&MOFN = T)
    .CURSOR = LDMB
  IF (&MOFN = U)
    .CURSOR = USID
  IF (&MOFN ^= T)
    IF (&MOFN ^= U)
      .CURSOR = MOFN
)PROC
  IF (&MOFN = B)
    &MOFB = TRUNC (&MOFB,1)
    &MOFB = TRANS (&MOFB N,NO Y,YES *,*)
    VER (&MOFB,NONBLANK)  VER (&MOFB,LIST,NO,YES)
  IF (&MOFN = T)
    VER (&LDDS,NONBLANK)
    VER (&LDMB,NONBLANK)
  IF (&MOFN = U)
    VER (&USID,NONBLANK)  VER (&USID,NAME)
  &SEL = TRANS (TRUNC (&MOFN,' ')
               B,'CMD(%PAJCBRDS) '
               U,'CMD(%PAJCBUMU) '
               T,'CMD(%PAJCAUTO) '
             ' ',' '
               *,'?' )
)END
