)ATTR
 + TYPE(TEXT) SKIP(ON) INTENS(LOW)
)BODY
%-----------------------------  GRS / ENQ DISPLAY  ---------------------
%Command ===>_ZCMD

              (leave all fields blanks to get ENQ-contention)

%Enter/Verify parameters below :
+
          Job name%===>_JOB     +
      Major Q-name%===>_MAJOR   +
      Minor Q-name%===>_MINOR                                       +

%or set specific request for :
+
     Data-set name%===>_VQDSN                                       +
 or User-ID in TSO%===>_VQTSO  +
         or in SPF%===>_VQSPF  +


)INIT
  .HELP = PAJTVQ1
  .CURSOR = JOB
  &ZCMD = &Z
  &VQDSN = &Z
  &VQTSO = &Z
  &VQSPF = &Z
)PROC
  IF (&ZCMD ^= ' ')  .MSG = ISPZ001
  IF (&VQDSN ^= ' ')  VER(&VQDSN,DSNAME)
    &MAJOR = SYSDSN
    &MINOR = &VQDSN
    &VQDSN = &Z
    &VQTSO = &Z
    &VQSPF = &Z
  IF (&VQTSO ^= ' ')  VER(&VQTSO,NAME)
    &MAJOR = SYSIKJUA
    &MINOR = &VQTSO
    &VQTSO = &Z
    &VQSPF = &Z
    &JOB = &Z
  IF (&VQSPF ^= ' ')  VER(&VQSPF,NAME)
    &MAJOR = SPFUSER
    &MINOR = &VQSPF
    &VQSPF = &Z
    &JOB = &Z
  IF (&JOB ^= ' ')  VER(&JOB,NAME)
  IF (&MAJOR ^= ' ')  VER(&MAJOR,NAME)
  IF (&MINOR ^= ' ')  VER(&MINOR,DSNAME)
)END
