%ANALYZE ----------------  VSM and RSM analysis jobs  ------------------
%COMMAND ===>_ZCMD
+
%Enter/verify parameters below:
+
+   Job selection :_IVR+(VSM/RSM)
+
+   Request(s) for :
+       %VSM -  SQA :_IV1+(NO/YES)            or%RSM -  VSM :_IV6+(NO/YE
+       %       CSA :_IV2+(NO/YES)              %      PFTE :_IV7+(NO/YE
+       %      LSQA :_IV3+(NO/YES)              %      ESTE :_IV8+(NO/YE
+       %       PVT :_IV4+(NO/YES)
+       %        AE :_IV5+(NO/YES)
+       %VSM+default = SQA : YES                %RSM+default = PFTE : YE
+       %VSM+special test :_IVT     +
+                           (ALL/GET/hhhhhhhh)
+
%   LIST+DD-name :_IVKL    +
%   SNAP+wanted :_IVKT+(NO/YES, then%SNAP+DD-name :_IVKS    +
%   DUMP+wanted :_IVKD+(NO/YES)
+
)INIT
  .HELP = PAJTIVK1
  .CURSOR = IVR
  &IVR = &Z
  &IV1 = TRANS (&IV1 ' ',NO *,*)
  &IV2 = TRANS (&IV2 ' ',NO *,*)
  &IV3 = TRANS (&IV3 ' ',NO *,*)
  &IV4 = TRANS (&IV4 ' ',NO *,*)
  &IV5 = TRANS (&IV5 ' ',NO *,*)
  &IVT = &Z
  &IV6 = TRANS (&IV6 ' ',NO *,*)
  &IV7 = TRANS (&IV7 ' ',NO *,*)
  &IV8 = TRANS (&IV8 ' ',NO *,*)
  &IVKL = TRANS (&IVKL ' ',SYSPRINT *,*)
  &IVKT = TRANS (&IVKT ' ',NO *,*)
  &IVKS = TRANS (&IVKS ' ',SYSSNAP *,*)
  &IVKD = TRANS (&IVKD ' ',NO *,*)
)PROC
  VER (&IVR,NB,LIST,VSM,RSM)
  &IV1 = TRUNC (&IV1,1)
  &IV1 = TRANS (&IV1 N,NO Y,YES ' ',NO *,*)
  VER (&IV1,NB,LIST,NO,YES)
  &IV2 = TRUNC (&IV2,1)
  &IV2 = TRANS (&IV2 N,NO Y,YES ' ',NO *,*)
  VER (&IV2,NB,LIST,NO,YES)
  &IV3 = TRUNC (&IV3,1)
  &IV3 = TRANS (&IV3 N,NO Y,YES ' ',NO *,*)
  VER (&IV3,NB,LIST,NO,YES)
  &IV4 = TRUNC (&IV4,1)
  &IV4 = TRANS (&IV4 N,NO Y,YES ' ',NO *,*)
  VER (&IV4,NB,LIST,NO,YES)
  &IV5 = TRUNC (&IV5,1)
  &IV5 = TRANS (&IV5 N,NO Y,YES ' ',NO *,*)
  VER (&IV5,NB,LIST,NO,YES)
  &IV6 = TRUNC (&IV6,1)
  &IV6 = TRANS (&IV6 N,NO Y,YES ' ',NO *,*)
  VER (&IV6,NB,LIST,NO,YES)
  &IV7 = TRUNC (&IV7,1)
  &IV7 = TRANS (&IV7 N,NO Y,YES ' ',NO *,*)
  VER (&IV7,NB,LIST,NO,YES)
  &IV8 = TRUNC (&IV8,1)
  &IV8 = TRANS (&IV8 N,NO Y,YES ' ',NO *,*)
  VER (&IV8,NB,LIST,NO,YES)
  &IVKT = TRUNC (&IVKT,1)
  &IVKT = TRANS (&IVKT N,NO Y,YES ' ',NO *,*)
  VER (&IVKT,NB,LIST,NO,YES)
  &IVKD = TRUNC (&IVKD,1)
  &IVKD = TRANS (&IVKD N,NO Y,YES ' ',NO *,*)
  VER (&IVKD,NB,LIST,NO,YES)
  &IVKL = TRANS (&IVKL ' ',SYSPRINT *,*)
  VER (&IVKL,NB,NAME)
  &IVKS = TRANS (&IVKS ' ',SYSSNAP *,*)
  IF (&IVKT = NO)
    &IVKS = &Z
  IF (&IVKT = YES)
    VER (&IVKS,NB,NAME)
  &IVKP = &Z
  IF (&IVR = VSM)
    &IVKW = 1
    IF (&IV1 = YES)
      &IVKP = SQA
    IF (&IV2 = YES)
      IF (&IVKP ^= &Z)
        &IVKP = '&IVKP,CSA'
      IF (&IVKP = &Z)
        &IVKP = CSA
    IF (&IV3 = YES)
      IF (&IVKP ^= &Z)
        &IVKP = '&IVKP,LSQA'
      IF (&IVKP = &Z)
        &IVKP = LSQA
    IF (&IV4 = YES)
      IF (&IVKP ^= &Z)
        &IVKP = '&IVKP,PVT'
      IF (&IVKP = &Z)
        &IVKP = PVT
    IF (&IV5 = YES)
      IF (&IVKP ^= &Z)
        &IVKP = '&IVKP,AE'
      IF (&IVKP = &Z)
        &IVKP = AE
    IF (&IVT ^= &Z)
      IF (&IVT ^= ALL,GET)
        VER (&IVT,PICT,XXXXXXXX)
      IF (&IVKP ^= &Z)
        &IVKP = '&IVKP,T=&IVT'
      IF (&IVKP = &Z)
        &IVKP = 'T=&IVT'
  IF (&IVR = RSM)
    &IVKW = 2
    IF (&IV6 = YES)
      &IVKP = VSM
    IF (&IV7 = YES)
      IF (&IVKP ^= &Z)
        &IVKP = '&IVKP,PFTE'
      IF (&IVKP = &Z)
        &IVKP = PFTE
    IF (&IV8 = YES)
      IF (&IVKP ^= &Z)
        &IVKP = '&IVKP,ESTE'
      IF (&IVKP = &Z)
        &IVKP = ESTE
  IF (&IVKL ^= SYSPRINT)
    IF (&IVKP ^= &Z)
      &IVKP = '&IVKP,L=&IVKL'
    IF (&IVKP = &Z)
      &IVKP = 'L=&IVKL'
  IF (&IVKS ^= &Z)
    IF (&IVKS ^= SYSSNAP)
      IF (&IVKP ^= &Z)
        &IVKP = '&IVKP,S=&IVKS'
      IF (&IVKP = &Z)
        &IVKP = 'S=&IVKS'
  &ZSEL = TRANS (&IVKW
                1,'CMD(%PAJCIVKA) '
                2,'CMD(%PAJCIVKB) '
              ' ',' '
                *,'?' )
)END
