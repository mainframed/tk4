                0,'PANEL(ISPOPTA)'
                1,'PGM(ISRBRO)'
                2,'PGM(ISREDIT)'
                3,'PANEL(ISRUTIL)'
                4,'PANEL(ISRFPA)'
                5,'PGM(ISRJB1) PARM(ISRJPA) NOCHECK'
                6,'PGM(ISRYXDR) NOCHECK'
                7,'PANEL(ISRLPRIM)'
                C,'PGM(ISPTUTOR) PARM(ISR00005)'
                P,'PANEL(NEW@MENU)'
                T,'PGM(ISPTUTOR) PARM(ISR00000)'
               2V,'CMD(%EDVIO)'
               SH,'CMD(%PAJCSHWM)'
               SF,'CMD(%PAJCSSF)'
              ' ',' '
                X,'EXIT'
                *,'?' )
)END
