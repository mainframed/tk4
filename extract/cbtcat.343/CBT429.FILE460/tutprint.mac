START:   CONTROL NOMSG
         ISPEXEC VGET     (REMOT)       SHARED
         FREE  F(LISTING CTABLE LSTDCB)
         ATTR  LSTDCB     LRECL(137) BLKSIZE(141) RECFM(V B A)
         SET &DEST = &STR( )
         IF &REMOT ^= &STR( ) THEN SET &DEST = &STR(DEST(&REMOT))
         DELETE TUTPRINT.LIST
         ALLOC F(LISTING) SYSOUT(X) &DEST USING(LSTDCB) HOLD
         ALLOC F(CTABLE)  SYSOUT(X) &DEST USING(LSTDCB) HOLD
         ISPEXEC SELECT   PGM(TUTPRNT2) PARM(ISASIZE(10K))
         ISPEXEC CONTROL  DISPLAY       REFRESH
         FREE  F(CTABLE LISTING LSTDCB)
FINISH:  EXIT
