    VOL(W$Z003) DSN('YOURHLQ.ZIL.HELP')
 RECEIVE INDS('YOURHLQ.FILE476.PDS(LISP)')
    VOL(W$Z003) DSN('YOURHLQ.ZIL.LISP')
 RECEIVE INDS('YOURHLQ.FILE476.PDS(MACLIB)')
    VOL(W$Z003) DSN('YOURHLQ.ZIL.MACLIB')
 RECEIVE INDS('YOURHLQ.FILE476.PDS(MSG)')
    VOL(W$Z003) DSN('YOURHLQ.ZIL.MSG')
 RECEIVE INDS('YOURHLQ.FILE476.PDS(OBJ)')
    VOL(W$Z003) DSN('YOURHLQ.ZIL.OBJ')
