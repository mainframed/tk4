                     3278CF,3277KN,3278KN,3290A,ASCII)     /* #SPF418 */
 &ZCHARLM = TRANS(&ZTERM
                  3277  ,  ISP3277
                  3277A ,  ISP3277A
                  3278  ,  ISP3278
                  3278A ,  ISP3278A
                  ASCII ,  ISPX7171                        /* #SPF418 */
                  3290A ,  ISP3278A
                  3278T ,  ISP3278T
                  3278CF,  ISP3278C
                  3277KN,  ISP3277K
                  3278KN,  ISP3278K)

 VER (&ZKEYS NB LIST 12,24)


 VER (&ZPADC NB,MSG=ISPP190)
 IF (&ZDEL = &ZPADC) .MSG = ISPO003
