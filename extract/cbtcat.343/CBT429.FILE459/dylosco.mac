*------- DYLON : PRODUCTS SELECTION OPERANDS
*                MANDATORY : MUST START WITH LABEL "OPX0" AND THEN USE
*                =========== LABELS FROM "OPX1" UP TO "OPX9999"
         SPACE 1
OPX0    @CHKO  NOISPF,NOSPF,NOMATCH=OPX1
        @IF    KEYWORD=(SPF,U),ELSE=OPDUPL        CHECK UNSPECIFIED
        @SETKW SPF,N,GOTO=OPNEXT                  SET NO OPTION
OPX1    @CHKO  ISPF,SPF,NOMATCH=OPX2
        @IF    KEYWORD=(SPF,U),ELSE=OPDUPL        CHECK UNSPECIFIED
        @SETKW SPF,S,GOTO=OPNEXT                  SET OPTION
OPX2    @CHKO  NOINFOSYS,NOIFS,NOIS,NOMATCH=OPX3
        @IF    KEYWORD=(IFS,U),ELSE=OPDUPL        CHECK UNSPECIFIED
        @SETKW IFS,N,GOTO=OPNEXT                  SET NO OPTION
OPX3    @CHKO  INFOSYS,IFS,IS,NOMATCH=OPX4
        @IF    KEYWORD=(IFS,U),ELSE=OPDUPL        CHECK UNSPECIFIED
        @SETKW IFS,S,GOTO=OPNEXT                  SET OPTION
OPX4    @CHKO  NOSMP,NOMATCH=OPX5
        @IF    KEYWORD=(SMP,U),ELSE=OPDUPL        CHECK UNSPECIFIED
        @SETKW SMP,N,GOTO=OPNEXT                  SET NO OPTION
OPX5    @CHKO  SMP,NOMATCH=OPX6
        @IF    KEYWORD=(SMP,U),ELSE=OPDUPL        CHECK UNSPECIFIED
        @SETKW SMP,S,GOTO=OPNEXT                  SET OPTION
OPX6    @CHKO  NOADABAS,NOADA,NOMATCH=OPX7
        @IF    KEYWORD=(ADA,U),ELSE=OPDUPL        CHECK UNSPECIFIED
        @SETKW ADA,N,GOTO=OPNEXT                  SET NO OPTION
OPX7    @CHKO  ADABAS,ADA,NOMATCH=OPX8
        @IF    KEYWORD=(ADA,U),ELSE=OPDUPL        CHECK UNSPECIFIED
        @SETKW ADA,S,GOTO=OPNEXT                  SET OPTION
OPX8    @CHKO  NOMFTS,NOMFT,NOMF,NOMATCH=OPX9
        @IF    KEYWORD=(MFT,U),ELSE=OPDUPL        CHECK UNSPECIFIED
        @SETKW MFT,N,GOTO=OPNEXT                  SET NO OPTION
OPX9    @CHKO  MFTS,MFT,MF,NOMATCH=OPEND
        @IF    KEYWORD=(MFT,U),ELSE=OPDUPL        CHECK UNSPECIFIED
        @SETKW MFT,S,GOTO=OPNEXT                  SET OPTION
         SPACE 1
*------- DYLOFF : SPECIAL DATA-SET'S STATUS OPERAND
*                MANDATORY : MUST START WITH LABEL "OPY0" AND THEN USE
*                =========== LABELS FROM "OPY1" UP TO "OPY9999"
         SPACE 1
OPY0    @CHKO  NOSYS,NS,NOMATCH=OPY1
        @IF    KEYWORD=(SYS,U),ELSE=OPDUPL        CHECK UNSPECIFIED
        @SETKW SYS,N,GOTO=OPNEXT                  SET NO OPTION
OPY1    @CHKO  SYS,S,NOMATCH=OPEND
        @IF    KEYWORD=(SYS,U),ELSE=OPDUPL        CHECK UNSPECIFIED
        @SETKW SYS,S                              SET OPTION
        @SETI  TDSI,SYS,GOTO=OPNEXT     SET TO BE DE-ALLOCATED
         SPACE 1
*------- DYLON/DYLOFF : SPECIAL OPERANDS (WIZARD)
*                MANDATORY : MUST START WITH LABEL "OPZ0" AND THEN USE
*                =========== LABELS FROM "OPZ1" UP TO "OPZ9999"
         SPACE 1
OPZ0    @CHKO  SETIFM,NOMATCH=OPZ1
        @SETI  XDSI,IFM,GOTO=OPNEXT     SAY INFO MANAGER
OPZ1    @CHKO  SETDMS,NOMATCH=OPEND
        @SETI  XDSI,DMS,GOTO=OPNEXT     SAY DMS MANAGER
