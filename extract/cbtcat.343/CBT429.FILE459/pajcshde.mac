         ISREDIT MACRO
         ISPEXEC VGET (UUNB1 UUNB2) SHARED
         ISREDIT (CHKLL) = DATA_WIDTH
         SET CHKTT = (&CHKLL / 8) * 8
         IF &CHKTT NE &CHKLL THEN ISREDIT CANCEL
         ISREDIT !CRY D,&UUNB2
         IF &LASTCC NE 0 THEN ISREDIT CANCEL
         ISREDIT !CRY D,&UUNB1
         IF &LASTCC NE 0 THEN ISREDIT CANCEL
         ISREDIT RESET
         ISREDIT EDMODE
