)INIT
  .HELP = PAJTOPSH
  &ZCMD = &Z
  &UUAN1 = &UUVB1
  &UUAN2 = &UUVB2
  &UUAN3 = &UUVB3
  &UUAN4 = &UUVB4
  &UUAN5 = &UUVB5
  &UUAN6 = &UUVB6
  &UUAN7 = &UUVB7
  &UUAN8 = &UUVB8
  &UUAN9 = &UUVB9
  &UUAN10 = &UUVB10
)PROC
  IF (&ZCMD ^= &Z)
    &UUCMD = TRANS(TRUNC(&ZCMD,' ')
             C,C CAN,C CANCEL,C
             E,E END,E
             T,T TEST,T *,*)
    VER (&UUCMD,LIST,C,E,T,MSG=PAJT900S)
  IF (&ZCMD = &Z)
    &UUCMD = &Z
    VER (&UUTLL,NB)
    VER (&UUCN,NB,NAME)
    IF (&UUAN1 ^= &Z)
      VER (&UUAN1,NAME)
    IF (&UUAN2 ^= &Z)
      VER (&UUAN2,NAME)
    IF (&UUAN3 ^= &Z)
      VER (&UUAN3,NAME)
    IF (&UUAN4 ^= &Z)
      VER (&UUAN4,NAME)
    IF (&UUAN5 ^= &Z)
      VER (&UUAN5,NAME)
    IF (&UUAN6 ^= &Z)
      VER (&UUAN6,NAME)
    IF (&UUAN7 ^= &Z)
      VER (&UUAN7,NAME)
    IF (&UUAN8 ^= &Z)
      VER (&UUAN8,NAME)
    IF (&UUAN9 ^= &Z)
      VER (&UUAN9,NAME)
    IF (&UUAN10 ^= &Z)
      VER (&UUAN10,NAME)
  VPUT (UUCMD UUTLL UUCN UUAN1 UUAN2 UUAN3 UUAN4) SHARED
  VPUT (UUAN5 UUAN6 UUAN7 UUAN8 UUAN9 UUAN10) SHARED
