//SHADOW   JOB (........),'INSTALL - SHADOW -',
//             MSGLEVEL=(1,1),MSGCLASS=A,REGION=1M,TIME=1
/*JOBPARM L=5
//* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
//*            INSTALL "SHADOW/SHADUP" ISPF APPLICATION               *
//* PRE-REQUISITE PRODUCTS : IOFSPF                                   *
//* PRE-REQUISITE MODULES INSTALLATION :                              *
//*                CRY       EDMODE    EUSID      EUSUB    INTRDR     *
//*                R050A90                                            *
//* NOTES : 1. ADJUSTMENTS MUST BE DONE BEFORE YOU RUN THIS JOB (SCAN *
//*            ALL "->" OCCURENCES AND SET RIGHT DATA-SETS NAMES).    *
//*         2. VERIFY "SHDATA" DD-CARD DEFINITIONS (STEP "ALL").      *
//*         3. IN THE STEPS BELOW THE FOLLOWING UNIT NAMES ARE        *
//*            ASSUMED : "SYSDA" FOR DISK UNIT                        *
//*                      "VIO" FOR VIRTUAL UNIT                       *
//*         4. CHECK MEMBERS "PAJCSHDU" AND "PAJSSHDU".               *
//* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
//ALL     EXEC PGM=IEFBR14
//SHDATA    DD UNIT=DISK,VOLUME=SER=......,SPACE=(TRK,(5,2,2)),
//             DSN=->.SHADOW.DATA,DISP=(NEW,CATLG,DELETE),
//             DCB=(RECFM=FB,BLKSIZE=3120,LRECL=80)
//ASS     EXEC PGM=IEV90,PARM='XREF(SHORT),NODECK,OBJECT'
//SYSLIB    DD DSN=->.SOURCE.FILE0,DISP=SHR
//          DD DSN=SYS1.MACLIB,DISP=SHR
//SYSUT1    DD UNIT=VIO,DSN=&&SYSUT1,SPACE=(CYL,(10,5))
//SYSLIN    DD DSN=->.SHADOW.DATA(PAJSHDW),DISP=OLD
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD DSN=->.SOURCE.FILE5(PAJSHDW),DISP=SHR
//ASR     EXEC PGM=IEV90,PARM='XREF(SHORT),NODECK,OBJECT'
//SYSLIB    DD DSN=->.SOURCE.FILE0,DISP=SHR
//          DD DSN=SYS1.MACLIB,DISP=SHR
//SYSUT1    DD UNIT=VIO,DSN=&&SYSUT1,SPACE=(CYL,(10,5))
//SYSLIN    DD DSN=->.SHADOW.DATA(R050A90),DISP=OLD
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD DSN=->.SOURCE.FILE0(R050A90),DISP=SHR
//ASM     EXEC PGM=IEV90,PARM='XREF(SHORT),NODECK,OBJECT'
//SYSLIB    DD DSN=->.SOURCE.FILE0,DISP=SHR
//          DD DSN=SYS1.MACLIB,DISP=SHR
//SYSUT1    DD UNIT=VIO,DSN=&&SYSUT1,SPACE=(CYL,(10,5))
//SYSLIN    DD UNIT=SYSDA,SPACE=(CYL,(1,1)),DSN=&LOADSET,
//             DCB=(BLKSIZE=800),DISP=(,PASS,DELETE)
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD DSN=->.SOURCE.FILE5(KMX),DISP=SHR
//LNK     EXEC PAJILKI
//SYSIN     DD *
  INCLUDE SYSISP(ISPLINK)
  INCLUDE SYSADD(R050A90)
  ENTRY   KMX
  NAME    KMX(R)
/*
//* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
//* EXECUTE THE STEP BELOW WITH PROCEDURE :                           *
//*       "PAJISPF" IF YOUR TARGET ->.ISPF.CLISTS DATA-SET            *
//*                                IS RECFM=FB AND LRECL=80, ELSE     *
//*       "PAJISPV" IF YOUR TARGET ->.ISPF.CLISTS DATA-SET            *
//*                                IS RECFM=VB AND LRECL=255.         *
//* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
//CPYC    EXEC PAJISPF,DSS='->.SOURCE.FILE5',
//             DST='->.ISPF.CLISTS'
//CM.SYSIN  DD *
  COPY   OUTDD=TARGET,INDD=SOURCE
  SELECT MEMBER=((PAJCKMX,KMX))
  SELECT MEMBER=(PAJCSHDE,PAJCSHDU,PAJCSHDW,PAJCSHEN)
/*
//CPYP    EXEC PAJISPF,DSS='->.SOURCE.FILE5',
//             DST='->.ISPF.PANELS'
//CM.SYSIN  DD *
  COPY   OUTDD=TARGET,INDD=SOURCE
  SELECT MEMBER=(PAJSHDWH,PAJSHDWI,PAJSHDWQ,PAJSHDWS,PAJSHDWT,PAJSHDWU)
/*
//CPYS    EXEC PAJISPF,DSS='->.SOURCE.FILE5',
//             DST='->.ISPF.SKELETONS'
//CM.SYSIN  DD *
  COPY   OUTDD=TARGET,INDD=SOURCE
  SELECT MEMBER=(PAJSSHDU)
/*
//CPYM    EXEC PAJISPF,DSS='->.SOURCE.FILE5',
//             DST='->.ISPF.MESSAGES'
//CM.SYSIN  DD *
  COPY   OUTDD=TARGET,INDD=SOURCE
  SELECT MEMBER=(PAJU99)
/*
//CPYD    EXEC PAJISPF,DSS='->.SOURCE.FILE5',
//             DST='->.SHADOW.DATA'
//CM.SYSIN  DD *
  COPY   OUTDD=TARGET,INDD=SOURCE
  SELECT MEMBER=(PAJSHDU)
/*
