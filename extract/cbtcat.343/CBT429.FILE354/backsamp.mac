//BACKDSNS EXEC PGM=BACKDSNS,REGION=4000K,COND=(0,NE)
//STEPLIB  DD DSN=HALL.LINKLIB,DISP=SHR   <=== CHANGE
//REPORT   DD SYSOUT=*               FOR AUDIT TRAIL
//SYSPRINT DD SYSOUT=*               FOR PDSFAST/COPY
//RESTORE  DD DSN=IMSX.DBT.RESTORE.JCL(DBTMMYY),DISP=SHR  <= CHANGE
//*ESTORE  DD DSN=IMSX.DBT.RESTORE.JCL(DBTMMYY),
//*           UNIT=DISK,SPACE=(TRK,(1,1,3)),
//*           DCB=(RECFM=FB,LRECL=200,BLKSIZE=4000),
//*           DISP=(,CATLG)
//*SYSABEND DD SYSOUT=*
//*DEBUG    DD SYSOUT=*
//SYSUT3 DD SPACE=(CYL,20),UNIT=SYSDA
//SYSUT4 DD SPACE=(CYL,20),UNIT=SYSDA
//SYSUT2   DD DSN=IMSX.DBT.BACKUP.JAN0998,      <=== CHANGE
//            UNIT=(TAPE,,DEFER),VOL=(,RETAIN),
//            DATACLAS=NOTMM,
//            DISP=(,KEEP),LABEL=(1,SL,EXPDT=99365)
//SYSIN    DD *
