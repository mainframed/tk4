PROC 0 SYSLIB(IMSX.IMF.LOAD) SYSPRINT(HALL.OUTLIST) +
       TEST(NO) MBR(DISASM) +
       SYSIN(HALL.DISASM.SOURCE(SYSIN))
/*     SYSIN(*)          /* TO PROMPT FOR SYSIN...
     CONTROL   LIST   CONLIST NOFLUSH
     IF &TEST = NO THEN GOTO NOTEST
     SET &SYSLIB = HALL.LINKLIB
     SET &SYSIN = RH87944.DISASM.SOURCE(SYSIN)

NOTEST:FREE F(A1,A2,SYSPUNCH)
     ALLOC DA('&SYSLIB') F(SYSLIB) SHR REUSE
     ATTR A2 BLKSIZE(19019)
     ALLOC DA('&SYSPRINT') F(SYSPRINT) SHR REUSE USING(A2)
     ATTR A1 BLKSIZE(80)
     ALLOC DA('NULLFILE') F(SYSPUNCH) REUSE USING(A1)
/*   ALLOC DA(SYSPUNCH) F(SYSPUNCH) NEW  REUSE USING(A1) SP(1,1) TR  DEL
     ALLOC DA(&SYSIN) F(SYSIN) SHR REUSE
     TSOEXEC TEST 'HALL.LOAD(&MBR)' 'FLTPT,SUPVR'
     EQ AA +0
     AT AA+C60
     AT AA+EAA
     AT AA+10E6
