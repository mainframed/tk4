//TSYS210C JOB (6277,TSO,0000),'OKON',MSGCLASS=X,
//         CLASS=4,NOTIFY=TSYS210
//*
//*        THIS JOB WILL PERFORM THE INSTALLATION OF THE SOUT SYSTEM
//*
//*        INSURE THAT THE REQUIRED CHANGES HAVE BEEN MADE TO THE
//*        SOUTSYS.ASM DATASET MEMBERS PRIOR TO RUNNING THIS JOB
//*
//*        ALSO, UPDATE THE FOREGROUND  MENU PANEL TO
//*        ALLOW EXECUTION OF THESE ROUTINES.
//*        (IE: ADD OPTION "S" TO CMN@F00)
//*
//*       VERIFY THE ASM.SYSLIB MACLIBS
//*
//*       VERIFY THE LNKLIB PARM  LIBRARY
//*
//*       VERIFY THE LKED.SYSLIB  LIBRARYS
//*       (NEEDS LIBRARY WHERE "ISPLINK" EXISTS)
//*
//*
//ASMSOUT  PROC I=,O=,LEPARM='MAP',LNKLIB='TSYS210.SOUTSYS.LOAD'
//ASM      EXEC PGM=IEV90,      <=== ASM H
//          PARM='TERM,XREF(SHORT),DECK,NOLIST'
//*ASM      EXEC PGM=IFOX00,
//SYSTERM  DD   SYSOUT=*
//SYSPRINT DD   SYSOUT=*
//SYSLIB   DD   DISP=SHR,DSN=SYS1.MACLIB,
//         DCB=(LRECL=80,RECFM=FB,BLKSIZE=6160)
//         DD   DISP=SHR,DSN=SYS2.MACLIB
//         DD   DISP=SHR,DSN=SYS1.AMODGEN
//*        DD   DISP=SHR,DSN=ISP.USER.MACLIB
//         DD   DISP=SHR,DSN=TSYS210.SOUTSYS.ASM
//SYSUT1   DD   SPACE=(CYL,(2,3)),UNIT=SYSDA
//SYSUT2   DD   SPACE=(CYL,(2,3)),UNIT=SYSDA
//SYSUT3   DD   SPACE=(CYL,(2,3)),UNIT=SYSDA
//SYSPUNCH DD   SPACE=(CYL,(2,3)),UNIT=SYSDA,DISP=(,PASS),DSN=&&OBJ,
//             DCB=(LRECL=80,RECFM=FB,BLKSIZE=3200)
//SYSIN    DD   DISP=SHR,DSN=TSYS210.SOUTSYS.ASM(&I)
//LKED     EXEC PGM=IEWL,PARM='LIST,&LEPARM'
//SYSPRINT DD   SYSOUT=*
//SYSUT1   DD   SPACE=(CYL,(2,3)),UNIT=SYSDA
//*SYSLIB  DD   DISP=SHR,DSN=SYS1.AOS31,UNIT=SYSDA,VOL=SER=ALTRES
//SYSLIB   DD   DISP=SHR,DSN=SYS1.LPALIB   <=== WHERE ISPLINK EXISTS
//SYSLMOD  DD   DISP=SHR,DSN=&LNKLIB(&O)
//SYSLIN   DD   DISP=(OLD,DELETE),DSN=&&OBJ
//         DD   DDNAME=SYSIN
//         PEND
//*
//*        ASSEMBLE AND LINK THE SERVICE ROUTINES TO LINKLIST
//*
//CMNFS00  EXEC ASMSOUT,I=CMNFS00@,O=CMNFS00
//SOUTCALL EXEC ASMSOUT,I=SOUTCALL,O=SOUTCALL,LEPARM='RENT,AC=1'
//SOUTCLN  EXEC ASMSOUT,I=SOUTCLN,O=SOUTCLN
//SOUTCPR  EXEC ASMSOUT,I=SOUTCPR,O=SOUTCPR
//SOUTDATE EXEC ASMSOUT,I=SOUTDATE,O=SOUTDATE
//SOUTPRT  EXEC ASMSOUT,I=SOUTPRT,O=SOUTPRT,LEPARM='AC=1'
//SOUTSEP  EXEC ASMSOUT,I=SOUTSEP,O=SOUTSEP,LEPARM='RENT,AC=1'
//SOUTSUB  EXEC ASMSOUT,I=SOUTSUB,O=SOUTSUB,LEPARM='AC=1'
//SOUTWTR  EXEC ASMSOUT,I=SOUTWTR,O=SOUTWTR,LEPARM='AC=1'
//SOUTXIT  EXEC ASMSOUT,I=SOUTXIT,O=SOUTXIT
//LOCINDEX EXEC ASMSOUT,I=LOCINDEX,O=LOCINDEX
//*
//* ASM/LKED THE CONVERSION PROGRAM IF NEEDED
//*
//*SOUTCVT EXEC ASMSOUT,I=SOUTCVT,O=SOUTCVT
//*
//*
//*        COPY THE STARTABLE PROCS TO 'SYS1.PROCLIB'
//*        AND THE SPF REQUIREMENTS TO THE REQUIRED DATASETS.
//*
//COPY     EXEC PGM=IEBCOPY
//*
//*   IF YOU ARE CONVERTING TO THE NEW SYSTEM, YOU MAY WANT TO
//*   PLACE THE MEMBERS IN A NEW LIBRARY OR ELSE SPECIFY THE
//*   "R"- REPLACE FOR MEMBERS IN YOUR EXISTING LIBRARIES
//*
//SYSPRINT DD   DUMMY
//PROCLIB  DD   DISP=SHR,DSN=SYS1.PROCLIB
//ISPPLIB  DD   DISP=SHR,DSN=SYS2.ISPPLIB
//ISPSLIB  DD   DISP=SHR,DSN=SYS2.ISPSLIB
//SOUTSYS  DD   DISP=SHR,DSN=TSYS210.SOUTSYS.ASM
 COPY I=SOUTSYS,O=PROCLIB
 S M=(DISKWTR,SAVEWTR,CLEANWTR,FLUSH)
 COPY I=SOUTSYS,O=ISPPLIB
 S M=(CMNFS00,CMNFS00A,CMNFS00B,CMNFS00C,CMNFS00D,CMNFS00E)
 S M=(CMNFS01,CMNFS02,CMNFS03)
 COPY I=SOUTSYS,O=ISPSLIB
 S M=((CMNFSKEL,CMNFS00))
//*
//*   RUN THE FORMAT JOB (MEMBER "FORMAT") TO CREATE AND FORMAT
//*   THE ACTIVE FILE
//*
//*
//*   RUN THE JOURNAL INIT JOB (MEMBER "INITJRNL")
//*   TO CREATE AND INITIALIZE THE VSAM JOURNAL.
//*
