//TSUID00I JOB   ACCT#,SOUTSYS,
//             CLASS=F,MSGCLASS=W,NOTIFY=TSUID00
//*
//*        THIS JOB WILL PERFORM AN ASSEMBLY AND LINK ONLY
//*
//*
//ASMSOUT  PROC I=,O=,LEPARM='MAP',LNKLIB='SYSL.LINKLIB'
//*ASM      EXEC PGM=IEV90,      <=== ASM H
//ASM      EXEC PGM=IFOX00,
//             PARM='TERM,XREF(SHORT),DECK,NOLIST'
//SYSTERM  DD   SYSOUT=*
//SYSPRINT DD   SYSOUT=*
//SYSLIB   DD   DISP=SHR,DSN=SYS1.MACLIB
//         DD   DISP=SHR,DSN=SYS2.MACLIB
//         DD   DISP=SHR,DSN=SYS1.AMODGEN
//         DD   DISP=SHR,DSN=ISP.USER.MACLIB
//SYSUT1   DD   SPACE=(CYL,(2,3)),UNIT=VIO
//SYSUT2   DD   SPACE=(CYL,(2,3)),UNIT=VIO
//SYSUT3   DD   SPACE=(CYL,(2,3)),UNIT=VIO
//SYSPUNCH DD   SPACE=(CYL,(2,3)),UNIT=VIO,DISP=(,PASS),DSN=&&OBJ,
//             DCB=(LRECL=80,RECFM=FB,BLKSIZE=3200)
//SYSIN    DD   DISP=SHR,DSN=TSUID00.SOUTSYS.ASM(&I)
//LKED     EXEC PGM=IEWL,PARM='LIST,&LEPARM'
//SYSPRINT DD   SYSOUT=*
//SYSUT1   DD   SPACE=(CYL,(2,3)),UNIT=VIO
//SYSLIB   DD   DISP=SHR,DSN=SYS1.AOS31
//         DD   DISP=SHR,DSN=SYS1.LINKLIB <== WHERE ISPLINK EXISTS!
//SYSLMOD  DD   DISP=SHR,DSN=&LNKLIB(&O)
//SYSLIN   DD   DISP=(OLD,DELETE),DSN=&&OBJ
//         DD   DDNAME=SYSIN
//         PEND
//*
//*        ASSEMBLE AND LINK THE SERVICE ROUTINES TO LINKLIST
//*
//CMNFS00  EXEC ASMSOUT,I=CMNFS00@,O=CMNFS00,LNKLIB='ISP.USER.ISPLOAD'
//SOUTCALL EXEC ASMSOUT,I=SOUTCALL,O=SOUTCALL,LEPARM='RENT,AC=1'
//SOUTCLN  EXEC ASMSOUT,I=SOUTCLN,O=SOUTCLN
//SOUTCPR  EXEC ASMSOUT,I=SOUTCPR,O=SOUTCPR
//SOUTDATE EXEC ASMSOUT,I=SOUTDATE,O=SOUTDATE
//SOUTPRT  EXEC ASMSOUT,I=SOUTPRT,O=SOUTPRT,LEPARM='AC=1'
//SOUTSEP  EXEC ASMSOUT,I=SOUTSEP,O=SOUTSEP,LEPARM='RENT,AC=1'
//SOUTSUB  EXEC ASMSOUT,I=SOUTSUB,O=SOUTSUB,LEPARM='AC=1'
//SOUTWTR  EXEC ASMSOUT,I=SOUTWTR,O=SOUTWTR,LEPARM='AC=1'
//SOUTXIT  EXEC ASMSOUT,I=SOUTXIT,O=SOUTXIT
//LOCINDEX EXEC ASMSOUT,I=LOCINDEX,O=LOCINDEX
//*
//* ASM/LKED THE CONVERSION PROGRAM IF NEEDED
//*
//*SOUTCVT EXEC ASMSOUT,I=SOUTCVT,O=SOUTCVT
//*
