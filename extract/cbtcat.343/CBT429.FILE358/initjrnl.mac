//TSYS200C JOB (6277,TSO,0000),'INIT SOUT JRNL',MSGCLASS=X,
//         CLASS=3,NOTIFY=TSYS200
//*
//*****************************************************************
//*
//* BEFORE RUNNING:
//*   1) CHANGE JOURNAL PREFIX (LEAVE "JOURNAL" AS SUFFIX)
//*      (USED IN 5 PLACES)
//*   2) CHANGE VOLUME
//*   3) CHANGE NUMBER OF RECORDS
//*
//*****************************************************************
//*
//* UNCOMMENT THE FOLLOWING AND CHANGE ACCORDINGLY IF YOU WANT
//* TO POSITION JOURNAL ON A PACK
//*
//*         EXEC PGM=IEFBR14
//*DD1      DD   DSN=&&TEMP1,DISP=(,PASS),
//*         UNIT=SYSDA,SPACE=(TRK,(6255)),VOL=SER=PVT863
//*
//IDCAMS    EXEC PGM=IDCAMS,REGION=4096K
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD *
 /* DELETE (SYS2.OPSWTR.JOURNAL) PURGE CLUSTER */
 /* APPROX 11 3380 CYLINDERS PER 100,000 RECORDS    */
 /*                                                 */
 /* SHARE OPTIONS CHANGED FROM 2,4 TO 2,3           */
 /* BECAUSE IT WILL NOT UPDATE HIGH RBA IN DISP=SHR */
 /* WE MAINTAIN DATA INTEGRITY WITH LOCK DATASET    */
 /* AND ONLY 1 UPDATE JOB                           */
 /*                                                 */
 DEL               PSY.ESA.SOUT.JOURNAL
 DEF CLUSTER (NAME(PSY.ESA.SOUT.JOURNAL)  -
     INDEXED -
     VOL(JOYLIB) -
     CYL(15,4) -
     SHR(2 3) -
     KEYS(22 0) -
     RECSZ(42 42) -
     FSPC(20 20) -
     SPEED -
     IMBED ) -
    DATA -
     (NAME(PSY.SOUT.ESA.JOURNAL.DATA) CISZ(4096)) -
    INDEX -
     (NAME(PSY.SOUT.ESA.JOURNAL.INDEX) )
//*
//IEBDG     EXEC PGM=IEBDG
//SYSPRINT  DD SYSOUT=*
//DD1   DD DSN=&&TEMP2,DISP=(,PASS),
//         UNIT=SYSDA,SPACE=(42,(10),RLSE),
//         DCB=(RECFM=F,LRECL=42,BLKSIZE=42)
//SYSIN     DD *
 DSD OUTPUT=(DD1)
 FD  NAME=F1,LENGTH=8,PICTURE=8,'*JRNINIT'
 FD  NAME=F2,LENGTH=5,PICTURE=5,P'99999'
 FD  NAME=F3,LENGTH=4,PICTURE=4,P'9999'
 FD  NAME=F4,LENGTH=9,PICTURE=9,'00000JOBX'
 FD  NAME=F5,LENGTH=4,PICTURE=4,B'0000'
 FD  NAME=F6,LENGTH=4,PICTURE=4,P'0000'
 FD  NAME=F7,LENGTH=2,PICTURE=2,' A'
 FD  NAME=F8,LENGTH=6,PICTURE=6,P'999999'
 CREATE QUANTITY=1,FILL=X'40',NAME=(F1,F2,F3,F4,F5,F6,F7,F8)
 END
//*
//IDCAMS    EXEC PGM=IDCAMS,REGION=4096K
//SYSPRINT  DD SYSOUT=*
//DD1   DD DSN=&&TEMP2,DISP=(OLD,DELETE)
//SYSIN     DD *
 REPRO INFILE(DD1) ODS(PSY.ESA.SOUT.JOURNAL)
//*
