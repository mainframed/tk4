ISREDIT MACRO (TYPE)
        IF &STR(&TYPE) = HELP THEN +
            DO
            ISPEXEC SELECT PGM(ISPTUTOR) PARM(#CONVISP)
            EXIT
            END

  ISREDIT CHANGE "_OPT" "_ZCMD" ALL
  ISREDIT CHANGE "&&&&OPT" "&&&&ZCMD" ALL
  ISREDIT CHANGE "&&&&SEL" "&&&&ZSEL" ALL
  ISPEXEC CONTROL DISPLAY REFRESH
EXIT CODE(0)
