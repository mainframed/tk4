ISREDIT MACRO (TYPE)
        IF &STR(&TYPE) = HELP THEN +
            DO
            ISPEXEC SELECT PGM(ISPTUTOR) PARM(#CORNER)
            EXIT
            END

ISREDIT CHANGE ALL { X'AC'    /* UPPER LEFT CORNER  */
ISREDIT CHANGE ALL } X'BC'    /* UPPER RIGHT CORNER */
ISREDIT CHANGE ALL Ö X'AB'    /* LOWER LEFT CORNER  */
ISREDIT CHANGE ALL ! X'BB'    /* LOWER RIGHT CORNER */
ISREDIT CHANGE ALL < X'EB'    /* LEFT "T"           */
ISREDIT CHANGE ALL > X'EC'    /* RIGHT "T"          */
ISREDIT CHANGE ALL ~ X'CC'    /* ASCENDING "T"      */
ISREDIT CHANGE ALL ` X'CB'    /* DESCENDING "T"     */
ISREDIT CHANGE ALL × X'FA'    /* VERTICAL BAR       */
ISREDIT CHANGE ALL _ X'BF'    /* HORIZONTAL BAR     */
ISREDIT CHANGE ALL + X'8F'    /* CROSS              */
