.GBLSET  ANOP  ,
&I       SETA  &I+1
         AIF   ('&P' EQ '&ZZAIDTX(&I)').MATCH
         AIF   (&I LT 34).GBLSET
         MNOTE 8,'XDEFAID: KEY NAME &P NOT DEFINED'
&KEY     SETC  '0'
         AGO   .BADKEY
.MATCH   ANOP  ,
&KEY     SETC  'X'''.'&ZZAIDHX(&I)'.''''
.BADKEY  AIF   (K'&T LT 3).TEXTQUO
         AIF   ('&T'(1,1) NE '''').TEXTQUO
&TEXT    SETC  '&T '
         AGO   .TEXTCOM
.TEXTQUO ANOP  ,
&TEXT    SETC  ''''.'&T'.''''
.TEXTCOM ANOP  ,
&NM      DC    AL1(&KEY,&F),CL6&TEXT
         MEND  ,
