         MACRO
        INSETS &LIST
         GBLB  &INLINE(50)
         LCLC  &INNAME(20)
         LCLA  &INNUMB(20)
         LCLA  &I,&J,&K,&L,&MAX
&MAX     SETA  20
&INNAME(1) SETC 'SPINIT'
&INNUMB(1) SETA 2
&INNAME(2) SETC 'SPLINE'
&INNUMB(2) SETA 3
&INNAME(3) SETC 'SPLINEUP'
&INNUMB(3) SETA 3
&INNAME(4) SETC 'INHEX'
&INNUMB(4) SETA 5
&INNAME(5) SETC 'CVH'
&INNUMB(5) SETA 6
&INNAME(6) SETC 'CNVD'
&INNUMB(6) SETA 6
&INNAME(7) SETC 'CNVR'
&INNUMB(7) SETA 7
&INNAME(8) SETC 'CNVX'
&INNUMB(8) SETA 7
&INNAME(9) SETC 'TRTAB'
&INNUMB(9) SETA 9
&INNAME(10) SETC 'RANDOM'
&INNUMB(10) SETA 10
&INNAME(11) SETC 'SPMOVE'
&INNUMB(11) SETA 3
&INNAME(12) SETC 'SPFITS'
&INNUMB(12) SETA 3
&INNAME(13) SETC 'INDEC'
&INNUMB(13) SETA 11
.*
&J       SETA  N'&SYSLIST
.LOOP    AIF   (&I GE &J).MEND
&I       SETA  &I+1
         AIF   ('&SYSLIST(&I)' EQ '').LOOP
&K       SETA  1
.SEARCH  AIF   ('&SYSLIST(&I)' EQ '&INNAME(&K)').FIND
&K       SETA  &K+1
         AIF   (&K LE &MAX).SEARCH
         MNOTE 4,'REQUEST ''&SYSLIST(&I)'' INVALID'
         AGO   .LOOP
.*
.FIND    ANOP
&L       SETA  &INNUMB(&K)
&INLINE(&L) SETB 1
         AGO   .LOOP
.MEND    MEND
