         MACRO ,
&NM      SCITEM &OP,&ITEM    FORMAT SINGLE FD ITEM          NEW GP02268
&NM      MACPARM R1,&OP,NULL=SKIP  LOAD FDW PARM AREA
         AIF   ('&ITEM' EQ '').NOLAD
         MACPARM R15,&ITEM   LOAD ITEM ADDRESS
         MACPARM R15,0(,R1),OP=ST   SET ITEM ADDRESS INTO FDWFDA
.NOLAD   MACPARM R15,EXWASCRN,OP=L  LOAD SCREEN SERVICE ROUTINE
         L     R15,72(,R15)  LOAD ADDRESS OF ROUTINE
         BASR  R14,R15       CALL IT                            GP03275
         MEND  ,
