         MACRO ,                                        ADDED ON 81201
&NM      MAPSCRWK &PREFIX=SW,&WIDTH=    PRINTER WORK AREA MAPPING
         LCLC  &DS
         LCLC  &P            SHORT PREFIX
&P       SETC  '&PREFIX'
&DS      SETC  '&P'.'WORK'
         AIF   ('&NM' EQ '').NONAME
&DS      SETC  '&NM'
.NONAME  ANOP  ,
&DS      DSECT ,             MAPPING OF PRINT DCB AND WORK AREA
&P.LINK   DS   F             LINK TO NEXT GETMAINED AREA
&P.ID     DS   C'SCR-'       ID OF PRINT FILE 'N'
&P.SPLEN  DS   F             SUBPOOL/LENGTH OF THIS AREA
&P.TCB    DS   A             ADDRESS OF OWNING TCB
&P.DCB@   DS   X'BF',AL3(SWDCB)  DCB POINTER
&P.PU@    DS   A             ADDRESS OF USER'S OPEN WORK AREA
&P.TRAN   DC   F'0'          ADDRESS OF TRANSLATE TABLE OR 0
&P.DDNAM  DS   CL8           DDNAME
&P.@BCC DC    A(0)           ADDRESS OF BUFFER PREFIX
&P.@OBUF DS    A(&P.OBUF)    OUTPUT BUFFER START
&P.@IBUF DS    A(&P.IBUF)    INPUT BUFFER ADDRESS
&P.WUMASK DC   XL4'00'       DEVICE CAPABILITY MASKS
&P.WUDFLT DC   XL4'0'        DEFAULT FD OPTIONS
&P.LSTWRK DS   0XL14         WORK AREA FOR EACH FD ENTRY
&P.LSTLEN DS   A             FIELD LENGTH
&P.LSTLTX DS   A             ITEM LENGTH
&P.LSTADD DS   A             ITEM ADDRESS
&P.LSTDAT DS   F             DATA TYPE
&P.PREOPT DS   F             PRIOR OPTIONS
&P.CUROPT DS   F             CURRENT PROCESSING OPTIONS
&P.LSTEDT DS   X             EDIT OPTIONS
&P.LSTCC  DS   X
&P.BUFNXT DS   A             ADDRESS OF NEXT BYTE
&P.SCRCUR DS   H             BYTES USED IN CURRENT LINE
&P.LSTDO  DC    X'00'        CONTROL FLAG
&P.FUN    DC    X'00'        CALLER'S REQUEST
&P.FLIST  EQU   X'80'          BUILD BUFFER FROM FD LIST
&P.FANAL  EQU   X'40'          ANALYZE USER'S RESPONSE
&P.FMOVE  EQU   X'10'          MOVE ERROR-FREE INPUT TO MEMORY
&P.FLOOP  EQU   X'04'          LOOP FDS AND TAKE USER EXIT       87173
&P.FITEM  EQU   X'01'          SCITEM CALL                      GP03011
&P.OPT9   DC    X'00'  1/2   FD OPTION BYTES; 0 IF NONE
&P.OPT7   DC    X'00'  2/2   SECOND OPTION BYTE; DFLT IF MISSING
&P.LSTXEQ DC    A(0)         ADDRESS OF EXEC FD
&P.LSTXEN DC    A(0)         END ADDRESS OF EXEC RANGE
         DC    (4*2)A(0)     PUSH-DOWN STACK
&P.TRNLOW DC    A(TNTRTAB)   TRANSLATE TABLE
         SPACE 1                                                 87173
&P.XEXIT  DC    A(0,0,&P.XPARM)  R15-R1 TO USER (EXIT,LIST,PARM)
&P.XPARM  DC    A(&P.XSAVE,0,0,0) SAVE/FD/FIW/FDW
&P.XSAVE  DC    18F'0'       SAVE AREA FOR USER'S USE            87173
&P.XMYSV  DC    12F'0'       MY SAVE AREA                        87173
&P.FLAG   DS   X             PROCESSING FLAG
&P.FVTAM  EQU  X'80' =PUFVTAM   THIS IS VTAM, NOT BTAM
&P.FAKE   EQU  X'40'         IN-STORAGE ACCESS METHOD            83275
&P.FTSU   EQU  X'20'         OUTPUT ROUTED TO TSO USER
&P.FHEAD  EQU  X'10'         TITLE PRINTED ON THIS PAGE
&P.SETAT  EQU  X'08'           USE SET ATTRIB, NOT SFE
&P.FPXLST EQU  X'04' =PUPGXLST   EXIT LIST PRESENT               84169
&P.FUPAGE EQU  X'02' =PUPUPAGE   USER MAINTAINS PAGE #S          84169
&P.FGPAGE EQU  X'01' =PUPGPAGE   USER PAGE # FEED-BACK OPTION
&P.FILL   DC   AL1(0)        UNPRINTABLE CHARACTER REPLACEMENT
&P.DCB    DCB  DDNAME=ANY,DSORG=PS,MACRF=PM,EROPT=ACC,                 *
               RECFM=VBSA,LRECL=137,EXLST=SWEXLIST
&P.EXLIST DC   0A(0),X'87',AL3(0),A(0,0) +JFCB+@DCBEXIT
&P.TOPPG  DC   F'0'          EDIT LEN/ADDRESS FOR PAGE NUMBER
&P.TOPPS  DC   F'0'             DITTO FOR SUB-PAGE
&P.TOPDT  DC   F'0'             DITTO FOR DATE
&P.TOPTM  DC   F'0'             AND TIME
&P.BOTPG  DC   F'0'          EDIT LEN/ADDRESS FOR PAGE NUMBER
&P.BOTPS  DC   F'0'             DITTO FOR SUB-PAGE
&P.BOTDT  DC   F'0'             DITTO FOR DATE
&P.BOTTM  DC   F'0'             AND TIME
&P.CURLN  DC   H'-1'
&P.MAXLN  DC   H'24'         MAXIMUM LINES PER PAGE (INCL. TITLES)
&P.WIDTH  DC   H'80'         DATA WIDTH FOR TRUNCATION
&P.BUFSIZ DC   Y(&P.IBUF-&P.OBUF)  BUFFER SIZE
&P.SCRSIZ DC   Y(24*80)      SCREEN SIZE
&P.TIT#   DC   H'0'          NUMBER OF TITLE LINES
&P.FOOT#  DC   H'0'          NUMBER OF FOOTER LINES
&P.PAGE   DC   H'0'          CURRENT PAGE NUMBER
&P.SPAGE  DC   H'0'          PAGE SINCE LAST TITLE CHANGE
&P.QUEREP DC    XL128'0'     WSF QUERY REPLY DATA                87315
&P.BINDS  DC    XL128'0'     BIND DATA                           87315
&P.PRSZ  DC    2X'0'         PRIMARY SIZE                        87315
&P.ALSZ  DC    2X'00'        ALTERNATE SIZE                      87315
&P.ATTR  DC    XL4'0'        ATTRIBUTE STRING                    87315
&P.VCON   DC   F'0'          V-FORMAT RECORD HEADER
&P.RECCC  DC   C' '          CARRIAGE CONTROL
         AIF   ('&WIDTH' NE '').HAVEW
&P.REC    EQU  *             DATA RECORD
         AGO   .MEND         TRUNCATE THE MAPPING
.HAVEW   ANOP  ,
&P.REC    DC   CL(&WIDTH)' '   TEXT RECORD
         DS    0F
&P.SIZE  EQU   *-&DS         LENGTH OF FIXED AREA
&P.TIT1   DC   14CL((&WIDTH+5+3)/4*4)' '   14 TITLE/FOOTER RECORDS
&P.CBUF  DS    16X           BUFFER CONTROL BYTES (VAR. LENGTH)
&P.OBUF  DS    (27+13)CL132' '  ROOM FOR COLOR OUTPUT
&P.IBUF  DS    27CL132' '    ROOM FOR FULL INPUT
&P.BUFND  EQU  *               END OF BUFFERS
.MEND    MEND  ,
