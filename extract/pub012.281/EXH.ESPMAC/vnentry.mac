         MACRO
&NM      VNENTRY &LIST       ENTRY POINT FOR XCTLS
         LCLA  &I,&J
&J       SETA  N'&SYSLIST
&NM      ONSPIE BUG99,X'7F'  TRAP PROGRAM CHECK INTERRUPTS
         MVI   SQSPICA+5,X'FF'  REALLY TRAP ALL
&NM      LH    R15,BROFF
         LH    R15,VNENTRY(R15)
         B     0(R15,R12)    BRANCH
VNENTRY  EQU   *
.LOOP    AIF   (&I GE &J).MEND
&I       SETA  &I+1
         DC    AL2(L&SYSLIST(&I)-&SYSECT)
         AGO   .LOOP
.MEND    MEND  ,
