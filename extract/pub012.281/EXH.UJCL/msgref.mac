//             UNIT=2314,VOL=SER=IMPORT,
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=3120)
 SORT FIELDS=(9,1,BI,D,1,8,BI,A,13,19,BI,A),SIZE=E9000
 RECORD TYPE=F,LENGTH=(80)
 END
