         MACRO
&TAG     SCANL &TREE,&MORE=,&DONE=
.*A000000                                                        Y02147
.*A000000                                                        Y02147
.**********************************************************************
LJW&SYSNDX EQU 1
         USING IGARPTH,LJW&SYSNDX
         AIF   (K'&TREE EQ K'&TREE(1)+2).LW1
&TAG     L     1,&TREE
.LW0     L     15,=AL4(IGASCANL)
         BALR  14,15
         AGO   .FIN
.LW1     ANOP
&TAG     LR    1,&TREE
         AGO   .LW0
.FIN     ANOP
         AIF   (K'&MORE NE 0).LW3
         AIF   (K'&DONE EQ 0).END
         LTR   15,15
         BC    8,&DONE
         AGO   .END
.LW3     AIF   (K'&DONE NE 0).LW4
         LTR   15,15
         BC    7,&MORE
         AGO   .END
.LW4     LTR   15,15
         BC    7,&MORE
         BC    8,&DONE
.END     ANOP
         DROP  LJW&SYSNDX
         RPTDSECT
         MEND
