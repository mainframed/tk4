         STRING '0   ACR00-03: ',(SDWAARE0,,X),2X,(SDWAARE1,,X),2X,    X
               (SDWAARE2,,X),2X,(SDWAARE3,,X),INTO=LINE121      GP03014
         BAL   R14,SPACE1          <== PRINT LINE               GP03014
         STRING '   ACR04-07: ',(SDWAARE4,,X),2X,(SDWAARE5,,X),2X,     X
