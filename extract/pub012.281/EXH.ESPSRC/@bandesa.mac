         BAL   R14,SPACE1          <== PRINT LINE
         STRING '   GPR08-11: ',(SDWAGR08,,X),2X,(SDWAGR09,,X),2X,     X
               (SDWAGR10,,X),2X,(SDWAGR11,,X),INTO=LINE
         BAL   R14,SPACE1          <== PRINT LINE
         STRING '   GPR12-15: ',(SDWAGR12,,X),2X,(SDWAGR13,,X),2X,     X
               (SDWAGR14,,X),2X,(SDWAGR15,,X),INTO=LINE
         BAL   R14,SPACE1          <== PRINT LINE
