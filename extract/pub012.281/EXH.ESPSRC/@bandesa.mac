               (00(R1),2,X),1X,(02(R1),2,X),1X,(04(R1),2,X),1X,        X
               (06(R1),2,X),1X,(08(R1),2,X),1X,(10(R1),2,X),1X
         BAL   R14,SPACE1          <== PRINT LINE
PSW25    STRING '0   GPR00-03: ',(SDWASR00,,X),2X,(SDWASR01,,X),2X,    X
               (SDWASR02,,X),2X,(SDWASR03,,X),INTO=LINE121
         BAL   R14,SPACE1          <== PRINT LINE
