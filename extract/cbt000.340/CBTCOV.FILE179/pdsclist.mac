//PDSCLIST PROC PGMLIB='FPC.PGMLIB',VOL='REF=*.PDSVOL',FORMS=A,
//             SORTCYL=5
//* PRINT LIST OF CSECTS WITHIN EACH MEMBER OF PDS ON LSTMEMBR
//* PRINT LIST OF MEMBERS WHICH USE EACH CSECT ON LSTCSECT
//* REV0 11/30/73 DON HIGGINS
//PDSCLIST EXEC PGM=GU024900
//STEPLIB   DD DSN=&PGMLIB,DISP=SHR
//          DD DSN=FPC.SORTLIB,DISP=SHR
//SORTLIB   DD DSN=FPC.SORTLIB,DISP=SHR
//SYSUDUMP  DD SYSOUT=A
//SYSOUT    DD SYSOUT=A
//LSTMEMBR  DD SYSOUT=&FORMS
//LSTCSECT  DD SYSOUT=&FORMS
//PDSVOL    DD DSN=&PDSLIB,DISP=SHR
//PDSLIB    DD DSN=&PDSLIB,DISP=SHR,UNIT=DISK,VOL=&VOL
//PDSMEM    DD DSN=&PDSLIB,DISP=SHR,UNIT=DISK,VOL=&VOL
//NAMEIN    DD DSN=&&NAME,DISP=(NEW,DELETE),
// DCB=BLKSIZE=1600,
// UNIT=DISK,SPACE=(CYL,&SORTCYL)
//NAMEOUT   DD DSN=&&NAME,DISP=(OLD,DELETE),
// DCB=(RECFM=FB,LRECL=16,BLKSIZE=1600),
// UNIT=DISK,VOL=REF=*.NAMEIN
//NAMEWK01  DD UNIT=DISK,SPACE=(CYL,&SORTCYL,,CONTIG)
//NAMEWK02  DD UNIT=DISK,SPACE=(CYL,&SORTCYL,,CONTIG)
//NAMEWK03  DD UNIT=DISK,SPACE=(CYL,&SORTCYL,,CONTIG)
