//FORTCL  PROC NAME=MAIN,LIBRIN='FPC.TESTLIB',PGMLIB='FPC.PGMLIB',
//             OPTION=NOLIST,LSIZE='(118K,24K)',
//             LINECNT=60,OBJLIST=NOLIST,OBJPROG=LOAD,PUNCH=NODECK,
//             SORCODE='EBCDIC,NOSOURCE',
//             STORMAP=MAP,XREF=MAP,
//             NCAL=,LIBRARY='FPC.TESTLIB',MEMBER=TEMPNAME
//FORT     EXEC PGM=PAN#1
//PANDD1   DD DSN=FPC.WORK,DISP=SHR
//PANDD2   DD DSN=&&TEMP,UNIT=DISK,SPACE=(80,(2000,500),RLSE),
//             DISP=(,PASS),DCB=(RECFM=FB,BLKSIZE=4000,LRECL=80)
//PANDD3   DD DUMMY
//SYSPRINT DD SYSOUT=A
//SYSPUNCH DD SYSOUT=P
//TRANFORT EXEC PGM=GU118000,PARM='&OPTION'
//STEPLIB   DD DSN=&PGMLIB,DISP=SHR
//SYSUDUMP  DD SYSOUT=A
//SYSPRINT  DD SYSOUT=A
//SYSUT1    DD DSN=&&TEMP,DISP=(OLD,DELETE)
//SYSUT2    DD DSN=&&FORT,DISP=(NEW,PASS),
// DCB=(RECFM=FB,LRECL=80,BLKSIZE=4000),
// UNIT=DISK,SPACE=(80,(2000,500),RLSE)
//COMP     EXEC PGM=IGIFORT,
//             PARM=('NAME=&NAME,LINECNT=&LINECNT,&OBJLIST,&OBJPROG',
//             '&PUNCH,&SORCODE,&STORMAP')
//SYSLIN   DD DSN=&&LOADSET,DISP=(MOD,PASS),UNIT=DISK,
//             SPACE=(CYL,(5,5,1),RLSE),DCB=BLKSIZE=3200
//SYSPRINT DD SYSOUT=A
//SYSTERM  DD SYSOUT=A
//SYSPUNCH DD SYSOUT=P
//SYSUDUMP DD SYSOUT=A
//SYSIN     DD DSN=&&FORT,DISP=(OLD,DELETE)
//LKED     EXEC PGM=GU007200,
//             PARM='SIZE=&LSIZE,LIST,LET,&XREF,&NCAL',
//             COND=(4,LT,COMP)
//PGMMOD   DD DSN=&&PGMMOD,UNIT=DISK,SPACE=(80,(10,5),RLSE),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=3200),DISP=(,PASS)
//PGMDATE  DD DSN=FPC.PARMLIB(JPGU0030),DISP=SHR
//SYSLMOD  DD DSN=&LIBRARY(&MEMBER),DISP=SHR
//LIBRIN   DD DSN=&LIBRIN,DISP=SHR
//SYSLIB   DD DSN=FORT.G1.FORTLIB,DISP=SHR
//         DD DSN=FPC.SUBLIB,DISP=SHR
//SYSLIN   DD DSN=&&LOADSET,DISP=(OLD,DELETE)
//         DD DSN=&&PGMMOD,UNIT=DISK,DISP=(OLD,DELETE),VOL=REF=*.PGMMOD
//         DD DDNAME=SYSIN
//SYSUT1   DD UNIT=DISK,SPACE=(CYL,(2,2),RLSE)
//SYSPRINT DD SYSOUT=A,SPACE=(CYL,(1,1),RLSE),DCB=BLKSIZE=605
//SYSUDUMP DD SYSOUT=A,SPACE=(CYL,(5,5),RLSE)
