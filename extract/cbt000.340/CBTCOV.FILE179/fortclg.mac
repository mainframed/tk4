//FORTCLG PROC NAME=MAIN,
//             PGMLIB='FPC.PGMLIB',OPTION=LIST,
//             SORCODE='EBCDIC,NOSOURCE',
//             OBJLIST=NOLIST,OBJPROG=LOAD,PUNCH=NODECK,
//             STORMAP=MAP,XREF=MAP,LSIZE='(118K,24K)'
//* REV # 1 ON 5/24/76 BY V.P. DIXON.
//*         ADDED SYMBOLIC PARAMETER LSIZE FOR LINKAGE EDITOR
//*         PROCESSING; CHANGED SYSUT2 BLKSIZE TO 4000 AND
//*         SYSLIN BLKSIZE TO 3200 FOR PERFORMANCE;
//*         AND ADDED RLSE PARAMETER TO ALL DISK
//*         DATA SETS TO REDUCE DISK USAGE.
//FORT    EXEC PGM=GU118000,PARM='&OPTION'
//STEPLIB   DD DSN=&PGMLIB,DISP=SHR
//SYSUDUMP  DD SYSOUT=A
//SYSPRINT  DD SYSOUT=A
//SYSUT1    DD DDNAME=SYSIN
//SYSUT2    DD DSN=&&FORT,DISP=(NEW,PASS),
// DCB=(RECFM=FB,LRECL=80,BLKSIZE=4000),
// UNIT=DISK,SPACE=(80,(2000,500),RLSE)
//COMP    EXEC PGM=IGIFORT,
//             PARM=('NAME=&NAME,&OBJLIST,&OBJPROG,&PUNCH,&SORCODE',
//             '&STORMAP')
//SYSPRINT DD SYSOUT=A,SPACE=(CYL,(10,5),RLSE),
//             DCB=(RECFM=FBA,BLKSIZE=720)
//SYSTERM  DD SYSOUT=A
//SYSPUNCH DD SYSOUT=P,SPACE=(CYL,(5,5),RLSE),
//             DCB=(RECFM=FB,BLKSIZE=4000)
//SYSLIN   DD DSN=&&LOADSET,DISP=(MOD,PASS),UNIT=DISK,
//             SPACE=(CYL,(5,5),RLSE),DCB=BLKSIZE=3200
//SYSIN     DD DSN=&&FORT,DISP=(OLD,DELETE)
//LKED     EXEC PGM=IEWL,
//             PARM='SIZE=&LSIZE,&XREF,LIST,LET,OVLY',
//             COND=(4,LT,FORT)
//SYSLMOD  DD DSN=&&GOSET(MAIN),DISP=(,PASS),UNIT=DISK,
//             SPACE=(CYL,(1,1,1))
//SYSLIB   DD DSN=FORT.G1.FORTLIB,DISP=SHR
//         DD DSN=FPC.SUBLIB,DISP=SHR
//SYSLIN   DD DSN=&&LOADSET,DISP=(OLD,DELETE,DELETE)
//         DD DDNAME=SYSIN
//SYSPRINT DD SYSOUT=A,SPACE=(CYL,(1,1),RLSE),DCB=BLKSIZE=605
//SYSUT1   DD UNIT=DISK,SPACE=(CYL,(2,1),RLSE)
//GO       EXEC PGM=*.LKED.SYSLMOD,COND=((4,LT,FORT),(4,LT,LKED))
//FT05F001 DD DDNAME=SYSIN
//FT06F001 DD SYSOUT=A,SPACE=(CYL,(20,5),RLSE)
//FT07F001 DD SYSOUT=(P,,5081),SPACE=(CYL,(5,5),RLSE)
