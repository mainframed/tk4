 APPENDIX IV                                   Sample PL/1 Display Modul



          /*    PL/1 SAMPLE DISPLAY                          */
           DISPLAY: PROCEDURE OPTIONS(MAIN,REENTRANT);
             %INCLUDE (PLIDCMS);
             %INCLUDE (IOCB);
             %INCLUDE (#STACKCB);
             DECLARE STRING CHAR(79) INIT((79)' ');
             DECLARE TITL_1 CHAR(79) INIT(
             'D C M S    PL/1   SAMPLE DISPLAY  ');
             IMAGE(1) = TITL_1 ;
             IMAGE(2) = STRING ;
             DO I = 3 TO 22 ;
                PUT STRING(IMAGE(I)) EDIT('  LINE # ',I) (A,X(4),F(4) ;
             END;
             %TRMIO ;
             IF IOCBAID = AIDPF9 THEN GOTO RESTART;
             IF IOCBAID ^= AIDPF12 THEN GOTO SCREENS;
           RETURN;
           END;














