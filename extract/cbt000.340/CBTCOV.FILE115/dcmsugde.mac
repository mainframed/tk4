                IOCB=(YES),BASE=(register),REGS=(#baseregs),
                     (no )                       (1)       
                 --      --   --         --     --       --

                 __         __         __               __
                IREG=(iocbregister),KEEP=(parmareaname),
                                         (PARMADDR)    
                 --           --       --               --


                 __         __         __            __
                TEMP=(workregister),WORK=(dsectname),
                     (2)                 (DCMSWORK) 
                 --           --       --            --

                 __         __
                PARM=(parmregister)
                     (2)           
                 --           --









