         MACRO
         RAFIOARG &ARGS
         GBLA  &RAFIOPR,&RAFIORG
         LCLC  &PARMS(30)
         LCLA  &BITS(30),&CNT1,&CNT2
&PARMS(1) SETC 'A1'               AFTER
&PARMS(2) SETC 'A2'               ALL
&PARMS(3) SETC 'R1'               RETRIEVE RECORD ON POINT
&PARMS(4) SETC 'R2'               RGET RECORD(S)
&PARMS(5) SETC 'B1'               BEFORE
&PARMS(6) SETC 'XK'
&PARMS(7) SETC 'XK2'
&PARMS(8) SETC 'NR1'
&PARMS(9) SETC 'NXK'
&PARMS(10) SETC 'S'
&PARMS(11) SETC 'M'
&PARMS(12) SETC 'IS'
&PARMS(13) SETC 'O'
&PARMS(14) SETC 'HIEQ'
&PARMS(15) SETC 'LAST'
&PARMS(16) SETC 'FIRST'
&PARMS(17) SETC 'KEY'
&PARMS(18) SETC 'PREVIOUS'
&PARMS(19) SETC 'NEXT'
&PARMS(20) SETC 'CREATE'
&PARMS(21) SETC 'RELEASE'
&PARMS(22) SETC 'POINT'
&PARMS(23) SETC 'ENDUP'
&PARMS(24) SETC 'INSERT'
&PARMS(25) SETC 'DELETE'
&PARMS(26) SETC 'REPLACE'
&PARMS(27) SETC 'RGET'
&PARMS(28) SETC 'SKIP'
&PARMS(29) SETC 'GBL'
&PARMS(30) SETC 'XGBL'
&BITS(1) SETA  0
&BITS(2) SETA  0
&BITS(3) SETA  4
&BITS(4) SETA  4+2
&BITS(5) SETA  1
&BITS(6) SETA  2
&BITS(7) SETA  2
&BITS(8) SETA  0
&BITS(9) SETA  0
&BITS(10) SETA  0
&BITS(11) SETA  32
&BITS(12) SETA  0
&BITS(13) SETA  1
&BITS(14) SETA  64
&BITS(15) SETA  128+64
&BITS(16) SETA  128
&BITS(17) SETA  2
&BITS(18) SETA  0-64
&BITS(19) SETA  64
&BITS(20) SETA  256*0
&BITS(21) SETA  256*1
&BITS(22) SETA  256*2
&BITS(23) SETA  256*3
&BITS(24) SETA  256*4
&BITS(25) SETA  256*5
&BITS(26) SETA  256*6
&BITS(27) SETA  256*7
&BITS(28) SETA  256*8
&BITS(29) SETA 16
&BITS(30) SETA 16+8
&RAFIORG SETA  0
&RAFIOPR SETA  0
&CNT1    SETA  N'&ARGS
.GO1     ANOP
&CNT2    SETA  30
.GO2     ANOP
         AIF   ('&ARGS(&CNT1)' EQ '&PARMS(&CNT2)').GO4
&CNT2    SETA  &CNT2-1
         AIF   (&CNT2   NE   0).GO2
.GO3     ANOP
&CNT1    SETA  &CNT1-1
         AIF   (&CNT1   NE   0).GO1
         MEXIT
.GO4     ANOP
&RAFIOPR SETA  &RAFIOPR+&BITS(&CNT2)
         AGO   .GO3
         MEND
