         MACRO
         EXCCWDS &TYPE=DSECT
         AIF   ('&TYPE' EQ 'DSECT').GO
IHKEXC   CSECT                    CHANNEL PROGRAM EXECUTOR
         AGO   .GO2
.GO      ANOP
EXCCWDS  DSECT ,                  DEFINITION OF EXCCW MODULE LINKAGE
MASTREQ  EQU   *
ONEARG   EQU   *
SIMPLE   EQU   *
NDXREQ   EQU   *
NDXINTEX EQU   *
CHKNDXEX EQU   *
LOCNDXEX EQU   *
DELNDXEX EQU   *
RELESEX  EQU   *
OLDNDX8  EQU   *
OLNDXCCW EQU   *
OLDNDXEX EQU   *
MAST1EX  EQU   *
SRCHRQ   EQU   *
SRCHPTRQ EQU   *
SRCHXKRQ EQU   *
SRCHEX   EQU   *
NDUPRQ   EQU   *
NDUPSQRQ EQU   *
NDUPMDRQ EQU   *
NDUPEX   EQU   *
FRSTMVRQ EQU   *
FRSTMDRQ EQU   *
FRSTMVEX EQU   *
NEWINSRQ EQU   *
NEWINSEX EQU   *
OLDINSRQ EQU   *
OLDINSEX EQU   *
INSRQ    EQU   *
INSEX    EQU   *
RLSTUPRQ EQU   *
RLSTUPEX EQU   *
RLSTINRQ EQU   *
RLSTINEX EQU   *
NDXRQ    EQU   *
NDXSEQRQ EQU   *
NDXPNTRQ EQU   *
NDXINBRQ EQU   *
NDXRQ1   EQU   *
NDXUEX2  EQU   *
RDXKRQ   EQU   *
RDXKEX   EQU   *
GETRQ    EQU   *
GETEX    EQU   *
REPEX    EQU   *
REPRQ    EQU   *
SKIPRQ   EQU   *
.GO2     ANOP
EX       EQU   * .                ORIGIN OF LINKAGE AND REQUESTOR TABLE
GENMASBQ EQU   *-EX .             GENERATE MASTER INDEX (REQUESTOR).
         DC    Y(MASTREQ-EX) .
         DK    9 .                     NINE CCW'S
         DK    3 .                     AND THREE SEARCH ARGUMENTS
GENMASBX EQU   *-EX .             GENERATE MASTER INDEX (EXECUTOR).
         DC    Y(ONEARG-EX) .
         DC    Y(SIMPLE-EX)
         DC    Y(GENMASCW) .
NDXINTBQ EQU   *-EX .             FORMAT INDEX RECORD TRACK
         DC    Y(NDXREQ-EX) .          (REQUESTOR).
         DK    10 .                    TEN CCW'S
         DK    2 .                     TWO SERACH ARGUMENTS
NDXINTBX EQU   *-EX .             INDEX RECORD FORMAT (EXECUTOR).
         DC    Y(ONEARG-EX) .
         DC    Y(SIMPLE-EX) .
         DC    Y(NDXINTCW) .
CHKNDXBQ EQU   *-EX .             CHECKPOINT ADDRESS OF INDEX RECORD
DELNDXBQ EQU   *-EX .             RESET MASTER INDEX ENTRY (REQUESTOR).
         DC    Y(MASTREQ-EX) .         (REQUESTOR).
         DK    6 .                     SIX CCW'S
         DK    1 .                     AND ONE SEARCH ARGUMENT
CHKNDXBX EQU   *-EX .             CHECKPOINT INDEX RECORD ADDRESS
         DC    Y(ONEARG-EX) .
         DC    Y(CHKNDXEX-EX) .        IN MASTER INDEX (EXECUTOR).
         DC    Y(CHKNDXCW) .
LOCNDXBQ EQU   *-EX .             LOCATE INDEX TRACK THROUGH MASTER
MAST1BQ  EQU   *-EX .             LOC 1ST DATA REC. THRU MASTER REQ.
         DC    Y(MASTREQ-EX) .
         DK    3 .                     THREE CCW'S
         DK    1 .                     AND ONE SEARCH ARGUMENT
LOCNDXBX EQU   *-EX .             LOCATE INDEX TRACK THROUGH MASTER
         DC    Y(ONEARG-EX) .
         DC    Y(LOCNDXEX-EX) .        INDEX (EXECUTOR).
         DC    Y(LOCNDXCW) .
DELNDXBX EQU   *-EX .             DELETE MASTER INDEX ENTRY
         DC    Y(ONEARG-EX) .
         DC    Y(DELNDXEX-EX) .        (EXECUTOR).
         DC    Y(DELNDXCW) .
RELESBQ  EQU   *-EX .             RELEASE ALL DATA TRACKS
         DC    Y(NDXRQ1-EX) .          (REQUESTOR).
RELESBX  EQU   *-EX .             RELEASE ALL DATA TRACKS
         DC    Y(ONEARG-EX) .
         DC    Y(RELESEX-EX) .         IN FILE (EXECUTOR).
         DC    Y(NDXUCW) .
OLDNDXBQ EQU   *-EX .             OLD INDEX REQUESTOR ROUTINE
         DC    Y(NDXREQ-EX) .
         DK    (OLDNDX8-OLNDXCCW)/8 .       NUMBER OF CCW'S
         DK    4 .                FOUR SEARCH ARGUMENTS
OLDNDXBX EQU   *-EX .             OLD INDEX EXECUTOR ROUTINE
         DC    Y(ONEARG-EX) .
         DC    Y(OLDNDXEX-EX) .
         DC    Y(OLDXCCWS) .
MAST1BX  EQU   *-EX .             LOCATE FIRST DATA RECORD THROUGH
         DC    Y(ONEARG-EX) .
         DC    Y(MAST1EX-EX) .         MASTER INDEX EXECUTOR
         DC    Y(MAST1CW) .
SRCHBQ   EQU   *-EX .             POSITION WITHIN NEW SEGMENT REQUESTOR
         DC    Y(SRCHRQ-EX) .
SRCHPTBQ EQU   *-EX .             POSITION WITHIN NEXT SEQUENTIAL
         DC    Y(SRCHPTRQ-EX) .        SEGMENT REQUESTOR
SRCHXKBQ EQU   *-EX .             READ RECORD CHAIN AND POSSIBLY XKEY
         DC    Y(SRCHXKRQ-EX) .        REQUESTOR
SRCHBX   EQU   *-EX .             POSITION WITHIN NEW SEGMENT EXECUTOR
SRCHPTBX EQU   *-EX .             POSITION WITHIN NEXT SEQUENTIAL
SRCHXKBX EQU   *-EX .
         DC    Y(SRCHEX-EX) .
NDUPBQ   EQU   *-EX .             UPDATE PRESENT CHAIN RECORD REQUESTOR
         DC    Y(NDUPRQ-EX) .
NDUPSQBQ EQU   *-EX .             UPDATE CHAIN RECORD AND PROCEDE TO
         DC    Y(NDUPSQRQ-EX) .        NEXT SEGMENT REQUESTOR
NDUPMDBQ EQU   *-EX .             UNCONDITIONAL UPDATE OF CHAIN RECORD
         DC    Y(NDUPMDRQ-EX) .        REQUESTOR
NDUPBX   EQU   *-EX .             UPDATE PRESENT CHAIN RECORD EXECUTOR
NDUPSQBX EQU   *-EX .             UPDATE CHAIN RECORD AND PROCEDE TO
NDUPMDBX EQU   *-EX .             UNCONDITIONAL UPDAE OF CHAIN RECORD
         DC    Y(NDUPEX-EX) .
FRSTMVBQ EQU   *-EX .             CONDITIONAL UPDATE OF END OF SEGMENT
         DC    Y(FRSTMVRQ-EX) .        POINTER REQUESTOR
FRSTMDBQ EQU   *-EX .             UNCONDITIONAL UPDATE OF END OF
         DC    Y(FRSTMDRQ-EX) .        SEGMENT POINTER REQUESTOR
FRSTMVBX EQU   *-EX .             UPDATE EOS OF PREVIOUS SEGMENT
FRSTMDBX EQU   *-EX .                  OR MASTER INDEX
         DC    Y(FRSTMVEX-EX) .        EXECUTOR
NEWINSBQ EQU   *-EX .             FORMAT DATA TRACK AND INSERT ONE OR
         DC    Y(NEWINSRQ-EX) .        MORE RECORDS REQUESTOR
NEWINSBX EQU   *-EX .             FORMAT DATA TRACK AND INSERT ONE OR
         DC    Y(NEWINSEX-EX) .        MORE RECORDS EXECUTOR
OLDINSBQ EQU   *-EX .             INSERT SINGLE RECORD AND GENERATE
         DC    Y(OLDINSRQ-EX) .        NEW EOS IN TRACK CHAIN REQUESTOR
OLDINSBX EQU   *-EX .             INSERT RECORD AND GENERATE NEW EOS
         DC    Y(OLDINSEX-EX) .        ENTRY IN TRACK CHAIN EXECUTOR
INSBQ    EQU   *-EX .             INSERT SINGLE DATA RECORD REQUESTOR
         DC    Y(INSRQ-EX) .
INSBX    EQU   *-EX .             INSERT RECORD(S) (REQUESTOR)
         DC    Y(INSEX-EX) .
RLSTUPBQ EQU   *-EX .             LOCATE TRACK LIST ENTRY FOR PRESENT
         DC    Y(RLSTUPRQ-EX) .        TRACK REQUESTOR
RLSTUPBX EQU   *-EX .             LOCATE TRACK LIST ENTRY FOR PRESENT
RLSTINBX EQU   *-EX .                  TRACK OR LOCATE HOLE IN FILE
         DC    Y(RLSTUPEX-EX) .
RLSTINBQ EQU   *-EX .             LOCATE TRACK LIST ENTRY AND SCAN FOR
         DC    Y(RLSTINRQ-EX) .        HOLE REQUESTOR
NDXBQ    EQU   *-EX .             UPDATE AND SCAN INDEX RECORD
         DC    Y(NDXRQ-EX) .      REQUESTOR
NDXSEQBQ EQU   *-EX .             UPDATE INDEX RECORD AND PROCEDE TO
         DC    Y(NDXSEQRQ-EX) .        NEXT SEGMENT REQUESTOR
NDXPNTBQ EQU   *-EX .             POINT INDEX UPDATE REQUESTOR
         DC    Y(NDXPNTRQ-EX) .
NDXINBBQ EQU   *-EX .             BSR INDEX UPDATE REQUESTOR
         DC    Y(NDXINBRQ-EX) .
NDXBX    EQU   *-EX .             UPDATE AND SCAN INDEX RECORD
NDXSEQBX EQU   *-EX .             UPDATE INDEX RECORD AND PROCEDE TO
NDXPNTBX EQU   *-EX .             POINT INDEX UPDATE EXECUTOR
NDXINBBX EQU   *-EX .             BSR INDEX UPDATE EXECUTOR
         DC    Y(ONEARG-EX) .
         DC    Y(NDXUEX2-EX) .
         DC    Y(NDXUCW) .
RDXKBQ   EQU   *-EX .             READ NEXT KEY REQUESTOR
         DC    Y(RDXKRQ-EX) .
RDXKBX   EQU   *-EX .             READ NEXT KEY EXECUTOR
         DC    Y(RDXKEX-EX) .
GETBQ    EQU   *-EX .             DATA RECORD RETRIEVAL (REQUESTOR)
         DC    Y(GETRQ-EX) .
GETBX    EQU   *-EX .             DATA RECORD RETRIEVAL (EXECUTOR)
         DC    Y(GETEX-EX) .
REPBX    EQU   *-EX .             REPLACE DATA RECORD (EXECUTOR)
         DC    Y(REPEX-EX) .
REPBQ    EQU   *-EX .             REPLACE RECORD (REQUESTOR)
         DC    Y(REPRQ-EX) .
SKIPBQ   EQU   *-EX .             SKIP RECORD
         DC    Y(SKIPRQ-EX) .
         MEND
