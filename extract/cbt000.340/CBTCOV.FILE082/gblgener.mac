        MACRO
&NAME   GBLGENER &C=,&D=,&U1=,&U=,&MIN1=21,&MIN2=51,&TSO=YES
.*  &C = NUMBER OF CONTROL UNITS SUPPORTED
.* &D = NUMBER OF DEVICES SUPPORTED
.* &U1 = NUMBER OF ADDRESSES FOR UNIT NAMES
.* &U = NUMBER OF UNIT NAMES
.* &MIN1 = NUMBER OF LINKLIB USER ROUTINES+1
.* &MIN2 = NUMBER OF SVCLIB USER ROUTINES+1
.* &TSO = TSO SWITCH - 'NO' EXCLUDES TSO GLOBALS
         REPRO
 GBLA &SGTESTA(2),&HEX,&SGCTRLA(2),&SGFORTA(8),&SGSUPRA(2)
 PUNCH ' GBLA &&SGUNNMA(&U1),&&SGCNTRA(10),&&SGDADRA(&D),&&SGCBLEA(2)'
 PUNCH ' GBLA &&SGCPRGA(23),&&SGOPCHA,&&SGDASDA,&&SGACTR'        S21167
