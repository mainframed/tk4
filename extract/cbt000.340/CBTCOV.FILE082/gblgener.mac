         REPRO
 GBLB &SGSCSDB(31),&SGSCMSB(31)
 PUNCH ' GBLB &&SGDUMMY(&D),&&SGDEVB(1),&&SGTYPEB(96)'
.* * * TEST FOR TSO - SGMINPAK WON'T CONTAIN TSO GLOBALS -- GBLB'S  *
.*             THERE ARE NO B-GLOBALS UNIQUE TO TSO
 PUNCH ' GBLC &&SGGJCC(11),&&SGLDRC(3)'
 PUNCH ' GBLC &&SGRESMC(11),&&SGUNNMC(&U),&&SGCTRLC(66)'
         REPRO
