         MACRO
         IEFPRMSG
         DS    0F            ALIGNMENT
ISHEAD   WTO   'IEF249I FOLLOWING P/R AND RSV VOLUMES ARE MOUNTED',    X
               MF=L,ROUTCDE=(2,4),DESC=(4)
ISMSG    WTO   '       ON       (   -   )',MF=L,ROUTCDE=(2,4),DESC=(4)
ISSER    EQU   ISMSG+4
ISUNIT   EQU   ISMSG+14
ISMNT    EQU   ISMSG+21
ISUSE    EQU   ISMSG+25
MSGHEAD  WTO   'IEF250I FOLLOWING MAY BE MOUNTED',MF=L,ROUTCDE=(2,4),  X
               DESC=(4)
VOLMSG   WTO   '       ON A     ',MF=L,ROUTCDE=(2,4),DESC=(4)
VOLSER   EQU   VOLMSG+4
DEVICE   EQU   VOLMSG+16
REPLYMSG WTOR  'IEF250D REPLY DEVICE NAME OR GO',,100,,                X
               DESC=(2),ROUTCDE=(1,4),MF=L
WDGO     EQU   REPLYMSG+41
VERMSG2  WTO   'IEF268I       FOR        (       )',MF=L,              X
               ROUTCDE=(2,4),DESC=(4)
VMSGNUM  EQU   VERMSG2+4
VERDEV2  EQU   VERMSG2+12
VERSER2  EQU   VERMSG2+22
VERMNT   EQU   VERMSG2+30
VERUSE   EQU   VERMSG2+34
REPLYERR WTOR 'IEF269A       ADDRESS INVALID OR OFFLINE. REPEAT REPLY',X
               ,100,,ROUTCDE=(1,4),DESC=(2),MF=L
ERRADD   EQU   REPLYERR+20
FINDERR  WTOR  'IEF270D I/O ERROR IN PRESRES READ. REPLY GO OR RE-IPL',X
               ,4,,MF=L,ROUTCDE=(1,4,10),DESC=(2)
RDERR    WTO   'IEF271I VOLUME NOT CONSIDERED IN SYSTEM',MF=L,         X
               ROUTCDE=(2,4),DESC=(4)
         MEND                                                      XMCS
