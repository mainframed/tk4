 /********************************************************************/
* /*                                                                 */
* /*  MAPPING MACRO FOR THE SMP ICT (IN CORE CDS )                   */
* /* THIS MACRO IS USED BY THE LINKEDIT, COPY AND SUPERZAP           */
* /* INVOKING ROUTINES TO CONTROL ACCESS TO SYSTEM LIBRARIES.        */
* /*                                                                 */
* /* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
* /*  THE ICT CONTAINS THREE VARIABLE LENGTH SECTIONS- THE PTF       */
* /*   SECTION, THE MODULE SECTION AND THE LOAD MODULE SECTION. EACH */
* /*   SECTION IS BASED ON ITS ASSOCIATED POINTER IN THE COMMON      */
* /*   AREA (HMASMCCA).                                              */
 /*                                                                  */
* /* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*  /* ICTIXPF, ICTIXMF AND ICTIXLF HAVE BEEN BASED ON ITS ASSOCIATED */
*  /* CHAIN POINTER WITH A SUBSCRIPT OF 1. THIS SUBSCRIPT MAY HAVE TO*/
*  /* BE CHANGED TO SOME INDEX VARIABLE DEPENDING UPON USAGE. THIS   */
* /* MAY BE ACCOMPLISHED THROUGH USE OF THE RESPECIFY CLAUSE.        */
 /********************************************************************/
* DECLARE
* 1 HMASMICT BASED(CCAICT),   /*  SMP ICT */
* 2 ICTCORE,
* 4 ICTSPLEN BIN(32), /* SUBPOOL AND LENGTH OF GOTTEN CORE */
* 6 ICTSP BIN(8),  /* SUBPOOL */
* 6 ICTLEN BIN(24)BDY(BYTE),  /* LENGTH OF GOTTEN CORE */
* 1 ICTPTF(*)BASED(CCAICPTF),  /*  PTF SECTION OF ICT */
* 2 ICTPTFS CHAR(7),   /* PTF OR MODIFICATION NO. */
* 4 ICTPMID CHAR(2),  /* TWO BYTE MODIFICATION IDENTIFIER  */
 6 ICTPEND BIN(16)BDY(BYTE),      /*USED TO INDICATE END OF PTF
                                    SECTION OF THE ICT. */
* 4 ICTPNO CHAR(5),  /* PTF OR MODIFICATION NUMBER  */
* 2 ICTPFLG1 BIT(8),  /* PTF CDS FLAGS  */
* 6 ICTAPP BIT(1),  /* PTF APPLIED  */
* 6 ICTACC BIT(1),  /* PTF ACCEPTED  */
 6 ICTFREC BIT(1),    /* THIS PTF FORCE RECEIVED */
* 6 ICTDUMMP BIT(1),  /* DUMMY PTF FROM SUPERCEEDS */
* 6 ICTREGEN BIT(1),  /* PTF DEFAULT APPLIED VIA
                         SYSTEM REGEN                        @VS05296*/
* 6 * BIT(3),  /* RESERVED  */
* 2 ICTPIFLG BIT(8),  /*  PTF SECTION INTERNAL FLAGS  */
 6 * BIT(1),    /* RESERVED */
 6 ICTPMAC BIT(1),  /*  PTF CONTAINS MACROS */
* 6 ICTZAP BIT(1),  /* PTF CONTAINS A SUPERZAP  */
 6 ICTPROCS BIT(1),    /* THIS PTF BEING PROCESSED */
 6 ICTFORCE BIT(1),          /*  THIS PTF BEING FORCED */
* 6 ICTPNOGO BIT(1),  /*  PTF SECTION NOGO FLAG  */
* 6 ICTPLNK BIT(1),  /* DO NOT LINK THIS PTF - LIB=NONE */
* 6 ICTPCPL BIT(1),  /* PTF COMPLETE  */
* 2 ICTPCHN PTR(24)BDY(BYTE);  /* CHAIN POINTER TO INDEX EXTENSION */
* DECLARE
 1 ICTIXPF(*)BDY(HWORD)BASED(ICTPCHN(1)), /* INDICES INTO EXTENSION */
 2 ICTIXP PTR(16)BDY(HWORD);       /*  INDEX ENTRIES FOR PTF SECTION */
* /* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
* /*                                                                 */
* /*   MODULE SECTION OF ICT                                         */
* /*                                                                 */
* /* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
* DECLARE
* 1 ICTMOD(*)BASED(CCAICMOD),  /*  MODULE SECTION OF ICT */
* 2 ICTMNAME CHAR(8),   /*   MODULE NAME  */
 4 ICTMEND BIN(16)BDY(BYTE),  /* USED TO INDICATE END OF MODULE SECT.*/
* 2 ICTMFLG1 BIT(8),   /* CDS MODULE FLAGS  */
* 4 ICTLIBLK BIT(1),   /*  CRLIB LOADMODULE   */
* 4 ICTLIBTX BIT(1),  /*  CRLIB TEXT - MUST LINK */
* 4 ICTMALIS BIT(1),   /*  MODULE HAS ALIAS ENTRIES   */
 4 ICTMXPND BIT(1), /*MODULE IS TO BE EXPANDED               @OY04018*/
  4 ICTMPRMS BIT(1),   /*MODULE LINK PARMS OBTAINED          @VS08700*/
* 4 * BIT(3),    /* RESERVED */
* 2 ICTMIFLG BIT(8),   /* MODULE SECTION INTERNAL FLAGS  */
* 4 ICTMPROC BIT(1),   /* MODULE IN PROCESS */
* 4 ICTMMAC BIT(1),   /*  MODULE IS MACRO ASSEMBLY */
* 4 ICTMZAP BIT(1),   /* THIS MODULE CONTAINS A SUPERZAP */
* 4 ICTNOM BIT(1),  /* MODULE NOT FOUND  */
* 4 ICTMIS BIT(1),  /* MODIFICATION MISMATCH  */
* 4 ICTMNOGO BIT(1),  /*  MODULE SECTION NOGO SWITCH  */
 4 ICTCRLIB BIT(1),  /*  CRLIB IS PRESENT */
 4 ICTMCPL BIT(1),   /* COMPLETION BIT */
 2 ICTMLEPR BIT(8), /* LINK EDIT PARMS FOR THIS MODULE */
 2 ICTMID CHAR(2),   /*   MODIFICATION IDENTIFIER */
 2 ICTFMLIB CHAR(8),  /*   FROMLIB */
* 2 ICTPPTR PTR(16)BDY(BYTE),  /* POINTER TO PTF ENTRY  */
* 2 ICTMCHN PTR(24)BDY(BYTE);  /* CHAIN POINTER TO INDEX EXTENSION */
* DECLARE
 1 ICTIXMF(*)BDY(HWORD)BASED(ICTMCHN(1)), /* INDICES INTO LOAD MODULE*/
 2 ICTIXM PTR(16)BDY(HWORD);    /* INDEX ENTRIES MODULE SECTION */
 /********************************************************************/
* /*                                                                 */
* /*   LOAD MODULE SECTION OF ICT.                                   */
* /*                                                                 */
* /* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
* DECLARE
* 1 ICTLMOD(*)BASED(CCAICLMD),  /*  LOAD MODULE SECTION OF ICT */
* 2 ICTLMNAM CHAR(8),  /* LOAD MODULE NAME  */
 4 ICTLEND BIN(16)BDY(BYTE), /* USED TO INDICATE END OF LOAD MOD. */
* 2 ICTLFLG1 BIT(8), /* FIRST BYTE CDS LOAD MOD. FLAGS */
* 4 ICTAPF BIT(1),      /* APF PARM, AC=1                    @VS08700*/
* 4 ICTRENT BIT(1),  /* RE-ENTRANT  */
* 4 ICTREUS BIT(1),  /*  REUSABLE  */
* 4 ICTSCTR BIT(1),  /* SCATTER LOAD  */
* 4 ICTOVLY BIT(1),  /* OVERLAY STRUCTURE  */
* 4 ICTREFR BIT(1),  /* REFRESHABLE  */
* 4 ICTDC  BIT(1),  /* DOWNWARD COMPATABLE  */
* 4 ICTNE BIT(1),  /* NOT EDITABLE  */
* 2 ICTLFLG2 BIT(8),  /* SECOND BYTE CDS LOAD MOD FLAGS */
* 4 ICTCOPY BIT(1),  /* COPIED AT SYSGEN  */
* 4 ICTLINK BIT(1),  /* LINKEDIT PARMS OBTAINED  */
* 4 * BIT(6),  /* RESERVED  */
* 2 ICTLIFLG BIT(8),  /* LOAD MOD SECTION INTERNAL FLAGS */
* 4 ICTTGIND BIT(2),   /*  TARGET LIBRARY INDICATOR BITS */
     /* THESE BITS ARE SET BY TABLE BUILD IF A TARGET EXISTS */
* 6 ICTTIND1 BIT(1),   /*  TARGET LIBRARY 1 INDICATOR */
* 6 ICTTIND2 BIT(1),  /*  TARGET LIBRARY 2 INDICATOR */
* 4 ICTINCLD BIT(1),               /* LOAD MODULE INCLUDED (LKI)   */
* 4 ICTLXPND BIT(1), /* LMOD HAS MOD TO BE EXPANDED          @OY04018*/
* 4 ICTLALIS BIT(1),   /*  MODULE CONTAINS ALIAS ENTRIES */
* 4 ICTLPROC BIT(1),  /*  LOAD MODULE IN PROCESS  */
* 4 ICTLNOGO BIT(1),  /* LOAD MODULE NOGO FLAG */
* 4 ICTLCPL BIT(1),  /* LOAD MODULE COMPLETE  */
* 2 ICTTGLIB CHAR(16),     /* TARGET LIBRARIES  */
* 4 ICTTG1 CHAR(8),   /*  TARGET LIBRARY # 1 . */
* 4 ICTTG2 CHAR(8),  /* TARGET LIBRARY # 2 OR BLANKS */
* 2 ICTLTTR BIN(24)BDY(BYTE), /* TTR OF EXTENDED DIRECTORY  */
* 2 ICTLCHN PTR(24)BDY(BYTE);  /* CHAIN POINTER INDEX EXTENSION */
* DECLARE
 1 ICTIXLF(*)BDY(HWORD)BASED(ICTLCHN(1)), /* INDICES TO MODULES */
 2 ICTIXL PTR(16)BDY(HWORD);  /* INDEX ENTRIES LOAD MOD SECTION */
