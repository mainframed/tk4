         MACRO
&MACNAME GETJSCB  &SYSTEM=MVT
         AIF   ('&SYSTEM' EQ 'MVT').GETMVT
         AIF   ('&SYSTEM' EQ 'MFT').GETMFT
         MNOTE ,'ONLY MFT OR MVT VALID AS SYSTEM PARAMETERS'
         MEXIT
.GETMFT  ANOP
&MACNAME GETMAIN R,LV=JSCBMFTL,SP=255
         XC    0(JSCBMFTL,1),0(1)      CLEAR GOTTEN CORE
         AGO   .SETADDR
.GETMVT  ANOP
&MACNAME GETMAIN R,LV=JSCBMVTL,SP=253
         XC    0(JSCBMVTL,1),0(1)      CLEAR GOTTEN CORE
.SETADDR ANOP
         LA    15,JSCBDISP -  LOAD DISPLACEMENT OF FIRST BYTE IN JSCB
         SR    1,15  -        COMPUTE ADDRESS TO BE STORED IN TCB
         MEXIT
         MEND
