//$A1238TS JOB 'DYCK L    SH28205000*001A0390000     015  9991007000',
//     'LIONEL DYCK',REGION=1500K,
//     TIME=5,MSGLEVEL=1,MSGCLASS=T,NOTIFY=$A1238
//*MAIN SYSTEM=ANY,ORG=WCC1.RMY24
//*********************************************************************
//* THIS IS THE JCL STREAM NEEDED TO ASSEMBLE AND LINK ALL THE LOAD   *
//* MODULES THAT COMPRISE THE TSSO SUBSYSTEM. THIS PROC IS SET TO     *
//* ASSEMBLE USING ASSEMBLER H. IT IS NOT POSSIBLE TO ASSEMBLE TSSO   *
//* AS DISTRIBUTED USING ASSEMBLER F BECAUSE IT DOES NOT SUPPORT THE  *
//* BATCH OPTION. TO ASSEMBLE USING ASSEMBLER F, THE LINKAGE EDITOR   *
//* CONTROL STATEMENTS THAT EXIST AT THE END OF EACH SOURCE MODULE    *
//* MUST BE MOVED, AND LINKAGE EDITOR CONTROL CARDS MUST BE ADDED TO  *
//* PROPERLY ASSEMBLE TSSO.                                           *
//* NOTE: THE EXPECTED CONDITION CODE FOR ALL STEPS IS ZERO, EXCEPT   *
//* FOR THE ASSEMBLY OF THE REPLYLOA MODULE, WHICH WILL YIELD A COND  *
//* CODE OF FOUR. THIS IS NOT A CONCERN.                              *
//*                                                                   *
//*********************************************************************
//ASMTSSO PROC DSNPRFX='$A1238.TSSO43',MEMBER=,RENT=NORENT
//ASM    EXEC  PGM=IEV90,REGION=3500K,
//    PARM=(LOAD,NODECK,'XREF(SHORT)','MSGLEVEL=0',TERM,BATCH,&RENT)
//SYSUT1   DD DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(CYL,(20,20))
//SYSUT2   DD DSN=&&SYSUT2,UNIT=SYSDA,SPACE=(CYL,(20,20))
//SYSUT3   DD DSN=&&SYSUT3,UNIT=SYSDA,SPACE=(CYL,(20,20))
//SYSPUNCH DD DUMMY,DCB=BLKSIZE=80
//SYSLIN   DD DSN=&&LOADSET,DISP=(MOD,PASS,DELETE),UNIT=SYSDA,
//            SPACE=(3520,(40,40),,,ROUND),DCB=BLKSIZE=400
//SYSLIB   DD DSN=&DSNPRFX..ASM,DISP=SHR
//         DD DSN=AMVSXA.MACLIB,DISP=SHR
//         DD DSN=AMVSXA.AMODGEN,DISP=SHR
//SYSPRINT DD DUMMY
//SYSIN    DD DSN=&DSNPRFX..ASM(&MEMBER),DISP=SHR
//SYSTERM  DD SYSOUT=*
//LKED     EXEC PGM=IEWL,REGION=2000K,COND=(4,LT,ASM),
//         PARM=(XREF,LIST,MAP,LET,NCAL,&RENT)
//SYSPRINT DD SYSOUT=*
//SYSUT1   DD DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(CYL,(20,20))
//SYSLIN   DD DSN=&&LOADSET,DISP=(OLD,DELETE)
//         DD DDNAME=SYSIN
//SYSLMOD  DD DSN=&DSNPRFX..LOAD,DISP=SHR
//SYSIN DD DUMMY
// PEND
//*********************************************************************
//* THE FIRST TWO MODULES ARE SUBROUTINES REQUIRED BY THE REST OF THE *
//* MODULES.                                                          *
//*********************************************************************
//JCECVT   EXEC ASMTSSO,MEMBER=JCECVT,RENT=RENT
//TSSOPARS EXEC ASMTSSO,MEMBER=TSSOPARS,RENT=RENT
//*********************************************************************
//* THE NEXT GROUP OF MODULES HANDLE TSSO INITILIZATION.              *
//*********************************************************************
//TSSO     EXEC ASMTSSO,MEMBER=TSSO,RENT=RENT
//TSSOINIT EXEC ASMTSSO,MEMBER=TSSOINIT,RENT=RENT
//TSSOINI1 EXEC ASMTSSO,MEMBER=TSSOINI1,RENT=RENT
//TSSOINI2 EXEC ASMTSSO,MEMBER=TSSOINI2,RENT=RENT
//TSSOINI3 EXEC ASMTSSO,MEMBER=TSSOINI3,RENT=RENT
//TSSOINI4 EXEC ASMTSSO,MEMBER=TSSOINI4,RENT=RENT
//TSSOINI5 EXEC ASMTSSO,MEMBER=TSSOINI5,RENT=RENT
//TSSOGSSC EXEC ASMTSSO,MEMBER=TSSOGSSC,RENT=RENT
//*
//*********************************************************************
//* THE NEXT GROUP OF MODULES ARE TSSO MAINLINE CODE AND SUBS         *
//*********************************************************************
//TSSOMAIN EXEC ASMTSSO,MEMBER=TSSOMAIN,RENT=RENT
//TSSOWTO  EXEC ASMTSSO,MEMBER=TSSOWTO,RENT=RENT
//TSSOSS09 EXEC ASMTSSO,MEMBER=TSSOSS09,RENT=RENT
//TSSOSS10 EXEC ASMTSSO,MEMBER=TSSOSS10,RENT=RENT
//TSSOTERM EXEC ASMTSSO,MEMBER=TSSOTERM,RENT=RENT
//TSSOESTA EXEC ASMTSSO,MEMBER=TSSOESTA,RENT=RENT
//TSSODOCM EXEC ASMTSSO,MEMBER=TSSODOCM,RENT=RENT
//TSSOCCMD EXEC ASMTSSO,MEMBER=TSSOCCMD,RENT=RENT
//TSSLRACF EXEC ASMTSSO,MEMBER=TSSLRACF,RENT=RENT
//*
//*********************************************************************
//* THE NEXT GROUP OF MODULES MAKE UP THE TSO AND NCCF VERSION OF     *
//* THE OSCMD FACILITY.                                               *
//*********************************************************************
//TSSOPREC EXEC ASMTSSO,MEMBER=TSSOPREC,RENT=RENT
//TSSOPCMD EXEC ASMTSSO,MEMBER=TSSOPCMD,RENT=RENT
//OSCMDATH EXEC ASMTSSO,MEMBER=OSCMDATH,RENT=RENT
//OPCMDMSG EXEC ASMTSSO,MEMBER=OPCMDMSG,RENT=RENT
//OSCMD    EXEC ASMTSSO,MEMBER=OSCMD,RENT=RENT
//NCFOSATH EXEC ASMTSSO,MEMBER=NCFOSATH,RENT=RENT
//NCFOSCMD EXEC ASMTSSO,MEMBER=NCFOSCMD,RENT=RENT
//*
//*********************************************************************
//* THE NEXT GROUP OF MODULES MAKE UP THE TSSO/OPF OSWAIT FACILITY.   *
//* THE OSCMD FACILITY.                                               *
//*********************************************************************
//OSWAITRC EXEC ASMTSSO,MEMBER=OSWAITRC,RENT=RENT
//OSWAIT   EXEC ASMTSSO,MEMBER=OSWAIT,RENT=RENT
//*********************************************************************
//* THE NEXT GROUP OF MODULES ARE THE TSSO/OPF OS FAMILY OF COMMANDS  *
//* THE OSCMD FACILITY.                                               *
//*********************************************************************
//OSASK    EXEC ASMTSSO,MEMBER=OSASK,RENT=RENT
//OSPAUSE  EXEC ASMTSSO,MEMBER=OSPAUSE,RENT=RENT
//OSWTO    EXEC ASMTSSO,MEMBER=OSWTO,RENT=RENT
//*
//*********************************************************************
//* THE NEXT MODULE IS THE TSSO CROSS MEMORY SERVICES ROUTINE.        *
//*********************************************************************
//TSSOGAPF EXEC ASMTSSO,MEMBER=TSSOGAPF,RENT=RENT
//*
//*********************************************************************
//* THE NEXT MODULES ARE THE COMMANDS TO CONTROL TSSO/AOF.            *
//*********************************************************************
//DISPAOF  EXEC ASMTSSO,MEMBER=DISPAOF,RENT=NORENT
//ALTAOF   EXEC ASMTSSO,MEMBER=ALTAOF,RENT=NORENT
//*
//*********************************************************************
//* THE NEXT MODULES MAKE UP THE TSSO REPLY COMMAND PROCESSOR.        *
//*********************************************************************
//REPLYLOA EXEC ASMTSSO,MEMBER=REPLYLOA,RENT=RENT
//REPLY    EXEC ASMTSSO,MEMBER=REPLY,RENT=NORENT
//*
//*********************************************************************
//* THE NEXT MODULES ARE UTILITY COMMANDS THAT WORK WELL UNDER TSSO.  *
//*********************************************************************
//CACHE    EXEC ASMTSSO,MEMBER=CACHE,RENT=NORENT
//CPCMD    EXEC ASMTSSO,MEMBER=CPCMD,RENT=NORENT
//SPMON    EXEC ASMTSSO,MEMBER=SPMON,RENT=NORENT
//VALLOC   EXEC ASMTSSO,MEMBER=VALLOC,RENT=NORENT
//VOLSER   EXEC ASMTSSO,MEMBER=VOLSER,RENT=NORENT
//*
//*********************************************************************
//* THE NEXT MODULES WERE WRITTEN BY BILL GODFREY...                  *
//*********************************************************************
//EF       EXEC ASMTSSO,MEMBER=EF,RENT=RENT
//MULT     EXEC ASMTSSO,MEMBER=MULT,RENT=RENT
