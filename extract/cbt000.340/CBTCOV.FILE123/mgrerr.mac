1MGRERR:          /* PROCESS ANY ERROR FOUND BY MANAGER              */
   PROC(ENTITY,MGRRC);
0DCL  ENTITY  CHAR(*) VARYING;
 DCL  MGRRC   FIXED BIN(31,0);
 /*  DCL (RACF_DONE,ON) BIT(1) REQUIRED IN MAINLINE ROUTINE          */
0RACF_DONE=ON;   /* SET NO MORE RACF PROCESSING                      */
 IF MGRRC>4095 THEN            /* IF RC>4096 => RC=36+4096*R0(REASON)*/
    PUT FILE(SYSPRINT)         /* SO PUT OUT SPECIAL MESSAGE WITH    */
        EDIT('MGRRC=36 REASON CODE',MGRRC/4096, /* REASON CODE       */
             ' FOR ENTITY=',ENTITY)
            (A,F(3),A,A);
               ELSE            /* OTHERWISE IF                       */
    IF MGRRC^=12 THEN       /* RETURN CODE 12 => PROFILE NOT FOUND:OK*/
       DO;
          PUT FILE(SYSPRINT) EDIT('RACF MANAGER ERROR',
              MGRRC,' ENTITY=',ENTITY) (A,F(4),A,A);
       END;
 CALL PLIRETC(0);  /*RESET RETURN CODE SO STEP PROCESSING CODE^=MGRRC*/
 RETURN;
 END MGRERR;
