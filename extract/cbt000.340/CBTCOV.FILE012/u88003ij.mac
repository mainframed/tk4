//DEFINE  EXEC PGM=IDCAMS
//SYSPRINT DD  SYSOUT=*
//*
//* AFTER ALLOCATION THE FOLLOWING RACF CHANGES NEED TO BE DONE
//* PE 'SYS2.ARCHIVE.TRACE' ID(USERIDX) ACC(UPDATE)
//* ALD 'SYS2.ARCHIVE.TRACE.KEY.PATH' UACC(READ)
//* ALD 'SYS2.ARCHIVE.TRACE.DSN.PATH' UACC(READ)
//*
//SYSIN DD *
  DEFINE CLUSTER -
  ( NAME(SYS2.ARCHIVE.TRACE) -
      CYLINDERS(10,1) -
      VOLUMES(SYSMCT) -
      NONINDEXED -
      SHAREOPTIONS(2,3) ) -
   DATA -
      (NAME(SYS2.ARCHIVE.TRACE.DATA) -
      RECORDSIZE(321,321) )

  DEFINE ALTERNATEINDEX -
  ( NAME(SYS2.ARCHIVE.TRACE.KEY) -
      RELATE(SYS2.ARCHIVE.TRACE) -
      TRACKS(5,5) -
      VOLUMES(SYSMCT) -
      KEYS(15,0) -
      NONUNIQUEKEY -
      UPGRADE -
      SHAREOPTIONS(2,3) ) -
   DATA -
      (NAME(SYS2.ARCHIVE.TRACE.KEY.DATA)) -
   INDEX -
      (NAME(SYS2.ARCHIVE.TRACE.KEY.INDEX))

  DEFINE ALTERNATEINDEX -
  ( NAME(SYS2.ARCHIVE.TRACE.DSN) -
      RELATE(SYS2.ARCHIVE.TRACE) -
      TRACKS(5,5) -
      VOLUMES(SYSMCT) -
      KEYS(44,15) -
      NONUNIQUEKEY -
      UPGRADE -
      SHAREOPTIONS(2,3) ) -
   DATA -
      (NAME(SYS2.ARCHIVE.TRACE.DSN.DATA)) -
   INDEX -
      (NAME(SYS2.ARCHIVE.TRACE.DSN.INDEX))

  DEFINE PATH -
  ( NAME(SYS2.ARCHIVE.TRACE.DSN.PATH) -
      PATHENTRY(SYS2.ARCHIVE.TRACE.DSN) )

  DEFINE PATH -
  ( NAME(SYS2.ARCHIVE.TRACE.KEY.PATH) -
      PATHENTRY(SYS2.ARCHIVE.TRACE.KEY) )
/*
//LIST EXEC PGM=IDCAMS
//SYSPRINT DD SYSOUT=*
//SYSIN DD *
  LISTCAT ENTRIES(SYS2.ARCHIVE.TRACE) ALL
  LISTCAT ENTRIES(SYS2.ARCHIVE.TRACE.KEY) ALL
  LISTCAT ENTRIES(SYS2.ARCHIVE.TRACE.DSN) ALL
  LISTCAT ENTRIES(SYS2.ARCHIVE.TRACE.DSN.PATH) ALL
  LISTCAT ENTRIES(SYS2.ARCHIVE.TRACE.KEY.PATH) ALL
/*
