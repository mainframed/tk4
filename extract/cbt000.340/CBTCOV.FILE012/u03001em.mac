.STEP14  AIF   (T'&RDONLY EQ 'O').STEP15
*        RDONLY=    NOT APPLICABLE
.STEP15  AIF   (T'&SEPASMB EQ 'O').STEP16
*        SEPASMB=   USER MUST CODE THE DCB AS A CSECT IF REQUIRED
.STEP16  AIF   (T'&RECFORM EQ 'O').STEP17
*        RECFORM=   ...,RECFM=(...)
.STEP17  AIF   (T'&RECSIZE EQ 'O').STEP18
*        RECSIZE=   ...,LRECL=NN
.STEP18  AIF   (T'&TYPEFLE EQ 'O').STEP19
*        TYPEFLE=   ...,DEVD=RD/PC,MACRF=(G/P..)
