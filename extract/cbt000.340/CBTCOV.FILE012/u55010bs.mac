*      IDENTIFICATION DIVISION.
*      PROGRAM-ID. SSU46DMP.
SSU46DMP ENTER ENT=AUTODUMP
         SR    3,3
         LR    12,13
         LA    12,0(0,12)
         SH    12,=H'24'
         ST    3,0(0,12)
         LINK  EP=COMMAND,PARAM=(DUMP)
WAIT     LA    3,40
TIME     STIMER WAIT,DINTVL=SECS30
         TM    0(12),X'40'
         BO    GETEND
         BCT   3,TIME
         LINK  EP=COMMAND,PARAM=(RELDUMP)
         B     WAIT
GETEND   LINK  EP=COMMAND,PARAM=(CANEND)
         LEAVE
         LTORG
DUMP     DC    H'19',C'S RDRD,NAME=DUMPMON'
RELDUMP  DC    H'29',C'A DUMPMON/S RDRD,NAME=DUMPMON'
CANEND   DC    H'29',C'C DUMPMON/S RDRD,NAME=DUMPEND'
SECS30   DS    0D
         DC    CL8'00003000'
         END
