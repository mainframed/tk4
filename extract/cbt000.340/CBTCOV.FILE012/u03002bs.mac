         MVC   BL+13(3),0(R2)
         MVC   PG+6(30),0(R3)
         MVC   PG+59(33),0(R4)
         LA    R2,3(R2)
         LA    R3,30(R3)
         LA    R4,33(R4)
         ST    R6,SAVR6XXX         STORE R6 BEFORE BALR TO PRINT
         L     R6,SAVR6            RESTORE R6 BEFORE BALR TO PRINT
         BALR  R8,R9               PRINT IT
         L     R6,SAVR6XXX         RESTORE R6 AFTER  BALR TO PRINT
         BCT   R6,MOVLOP1
         L     R6,SAVR6            RESTORE R6 BEFORE BALR TO PRINT
         MVC   BL+1(131),BL        CLEAR PRINT AREA
         MVI   CC,W3               SPACE 3 AFTER PRINT
         BALR  R8,R9               PRINT IT
         MVC   PG+5(11),=C'UNIT RECORD'
         MVC   PG+17(19),SYMBOLPA
         MVC   PG+58(13),=C'TAPE AND DISK'
         MVC   PG+72(19),SYMBOLPA
         MVI   CC,W2               SPACE 2 AFTER PRINT
         BALR  R8,R9               PRINT IT
         LA    R2,UNTRCD
         LA    R3,TPNDSK
         LA    R4,6
         MVC   BL+1(131),BL
         MVI   CC,W1
MOVLOP2  MVC   PG+6(30),0(R2)
         MVC   PG+59(31),0(R3)
         LA    R2,30(R2)
         LA    R3,31(R3)
         BALR  R8,R9               PRINT IT
         BCT   R4,MOVLOP2
         MVC   BL+1(131),BL        CLEAR PRINT AREA
         MVI   CC,W3               SPACE 3 AFTER PRINT
         BALR  R8,R9               PRINT IT
         MVC   PG+5(84),SAMPSTRT
         MVI   CC,W2
         BALR  R8,R9               PRINT
         MVC   BL+1(131),BL
         MVC   PG+5(7),=C'COMMAND'
         MVC   PG+58(11),=C'EXPLANATION'
         BALR  R8,R9               PRINT
         LA    R4,11
         LA    R2,SAMPCOMM
         LA    R3,COMEXPL
         MVC   BL+1(131),BL
         MVI   CC,W1
MOVLOP3  MVC   PG+6(44),0(R2)
         MVC   PG+59(41),0(R3)
         LA    R2,44(R2)
         LA    R3,41(R3)
         BALR  R8,R9               PRINT
         BCT   R4,MOVLOP3
         MVC   BL+1(131),BL
         MVI   CC,W1
         BALR  R8,R9               PRINT
         MVI   CC,W2
         MVC   BL+8(5),=C'NOTE.'
         BALR  R8,R9               PRINT
         MVC   BL+1(131),BL        CLEAR PRINT AREA
         MVI   CC,W1               SPACE 1 AFTER PRINT
         MVC   BL+8(121),CCINOTE
         BALR  R8,R9               PRINT
         MVC   BL+8(121),TCTLNOTE
         BALR  R8,R9               PRINT
         MVC   BL+1(131),BL        CLEAR PRINT AREA
         MVC   BL+8(68),DSKNOTE
         BALR  R8,R9               PRINT
         MVC   BL+1(131),BL        CLEAR PRINT AREA
         MVC   BL+8(50),XXXNOTE
         BALR  R8,R9               PRINT
         MVC   BL+1(131),BL        CLEAR PRINT AREA
         MVI   CC,WC1              PRINT & SKIP TO 1
