***********************************************************************
**********        PAGE 4 - 'CONSOLE OPERATION'              ***********
***********************************************************************
         SPACE 1
         MVI   PG,C' '
         MVC   PG+1,PG             CLEAR PRINT AREA
         MVI   CC,W3               SET TO PRINT & TRIPLE SPACE
         MVC   BL+7(19),=C'* * * * * * * * * *'
         MVC   BL+104(19),BL+7
         MVC   BL+51(17),=C'OS/DITTO CONSOLE '
         MVC   BL+68(9),=C'OPERATION'
         BALR  R8,R9               PRINT PAGE 2 HDG. - CONSOLE OPER..
         MVC   BL+1(131),BL        CLEAR PRINT AREA
         MVC   BL+7(115),OPERATOR  OPER. PRINT COMMENT LINE 1
         MVI   CC,W1               SPACE 1 AFTER PRINT
         BALR  R8,R9               PRINT IT
         MVC   BL+7(116),OPERATOR+116   2ND LINE OF OPER. COMMENT
         BALR  R8,R9               PRINT IT
         MVC   BL+7(116),TE7       SET UP WTOR NOTE
         MVI   CC,W2
         BALR  R8,R9               PRINT IT
         MVC   BL+1(131),BL        CLEAR PRINT AREA
         LA    R2,TBL1+3           POINTER TO FUNCTION DESCNS.
         LA    R3,NTBL1            # OF PRINT LINES
         LA    R4,STRING
         MVI   CC,W1
         MVC   PG+62(22),REQUR
         B     OPLOOP1
OPERLOOP MVI   CC,W1               SET TO PRINT & SINGLE SPACE
         MVC   PG+54(37),PG+53     CLEAR PRINT AREA
         LA    R1,PG+54
SAVCORE  CLI   0(R4),C' '          END OF LITERAL ?
         BE    BUMPBL              YES
         MVC   0(1,R1),0(R4)       MOVE DATA
         LA    R1,1(R1)            BUMP PRINT AREA
         LA    R4,1(R4)            BUMP PRINT LITERAL POINTER
         B     SAVCORE
BUMPBL   LA    R4,1(R4)            BUMP PAST BLANK
OPLOOP1  MVC   PG+1(49),0(R2)      DESCNS. TO PRINT AREA
         LA    R2,52(R2)           BUMP TO NEXT DESCN.
         BALR  R8,R9               PRINT LINE
         BCT   R3,OPERLOOP
         MVC   PG+1(95),PG         CLEAR PRINT AREA
         BALR  R8,R9               SINGLE SPACE
         LA    R2,M6+1             ADDRESS OF NBLKS=N..N
         LA    R3,DESCN1           ADDRESS OF NBLKS DESCN
         LA    R4,6                # OF PRINT LINES
         MVI   BL+66,C'*'
OPLOOP   MVI   CC,W1               SET TO SINGLE SPACE
         MVC   BL+07(14),00(R2)    LEFT PARAMETER TO PRINT AREA
         MVC   BL+27(33),00(R3)    LEFT DESCRIPTION TO PRINT AREA
         MVC   BL+74(14),15(R2)    RIGHT PARAMETER TO PRINT AREA
         MVC   BL+94(33),33(R3)    RIGHT DESCRIPTION TO PRINT AREA
         LA    R2,30(R2)           BUMP PARAMETER POINTER
         LA    R3,66(R3)           BUMP DESCRIPTION POINTER
         BALR  R8,R9               PRINT IT
         BCT   R4,OPLOOP
         MVC   BL+01(130),BL       CLEAR PRINT AREA
         MVI   CC,WC1              PRINT & SKIP TO 1
         BALR  R8,R9
         EJECT
***********************************************************************
**********        PAGE 5 - 'TAPE DENSITY & MODE VALUES'     ***********
***********************************************************************
         SPACE 1
         MVI   CC,W3               SET TO PRNT & TRIPLE SPACE
         MVC   PG+22(38),TAPHDG    'TAPE DENSITY...' HDG.
         MVC   PG+0(19),=C'* * * * * * * * * *'
         MVC   PG+62(19),PG
         BALR  R8,R9               PRINT IT - LINE 1
         MVC   PG,HDG              COL. HDGS. - 'TRACK DENSITY...'
         BALR  R8,R9               PRINT THE COLUMN HDGS.
         MVI   CC,W1               SET TO PRNT & SINGLE SPACE
         MVC   PG+1,PG             CLEAR PRNT AREA
         LA    R2,TBL2             POINTER TO PRINT LITERALS
         LA    R3,NTBL2            NUMBER OF LITERALS
MOVTBL2  MVC   DEN,0(R2)           DENSITY VALUE TO PRINT AREA
         MVC   PAR,3(R2)           PARITY VALUE TO PRINT AREA
         MVC   TRN,7(R2)           TRANSLATE VALUE TO PRINT AREA
         MVC   DCT,10(R2)          DATA CONVERT VALUE TO PRINT AREA
         MVC   MMS,13(R2)          MODE VALUE TO PRINT AREA
         MVC   TRK,15(R2)          TRACK VALUE TO PRINT AREA
         BALR  R8,R9               PRINT THE VALUES
         LA    R2,16(R2)           BUMP TO NEXT LITERAL
         BCT   R3,MOVTBL2          LOOP & PRINT ALL 7 TRACK
         MVC   PG+1,PG             CLEAR PRINT AREA
         BALR  R8,R9               PRINT BLANK LINE
         MVI   TRK,C'9'            9 TO TRACK
         MVC   DEN-1(4),=C' 800'   800 TO DENSITY
         MVC   PAR(14),=C'SINGLE DENSITY'
         MVC   MMS,=C'C0'          MODE VALUE TO PRINT AREA
         BALR  R8,R9               PRINT 9-800-SINGLE DENSITY
         MVC   DEN-1(2),=C'16'     1600 TO DENSITY
         BALR  R8,R9               PRINT 9-1600-SINGLE DENSITY
         MVC   DEN-1(3),=C'625'    6250 BPI
         MVI   MMS,C'D'            D0 MODE SET
         MVI   CC,W2               PRT & DOUBLE SPACE
         BALR  R8,R9               PRINT
         MVC   DEN-1(4),=C' 800'   800 TO DENSITY
         MVC   PAR(6),=C'DUAL  '   OVERLAY SINGLE WITH DUAL
         MVI   CC,W1               SET TO PRINT & SINGLE SPACE
         MVC   MMS,=C'C8'              C8
         BALR  R8,R9               PRINT 9-800-DUAL DENSITY
         MVC   DEN-1(2),=C'16'     1600 TO DENSITY
         MVI   MMS+1,C'0'          MODE TO C0
         BALR  R8,R9               PRINT 9-1600-DUAL DENSITY
         MVC   DEN-1(3),=C'625'    6250 BPI
         MVI   MMS,C'D'            D0
         MVI   CC,W3               PRT & TRIPLE SPACE
         BALR  R8,R9               PRINT
         MVC   PG+1,PG             CLEAR PRINT AREA
         MVC   TRK(63),DEFAULT     'NOTE. MM...' TO PRINT AREA
         BALR  R8,R9               PRINT NOTE & TRIPLE SPACE
         MVI   CC,W2               SET TO PRINT & DOUBLE SPACE
         MVC   PG(80),TE1          TAPE ERROR COMMENT LINE 1
         BALR  R8,R9               PRINT IT
         MVC   PG(80),TE2          TAPE ERROR COMMENT LINE 2
         BALR  R8,R9               PRINT IT
         MVC   PG(80),TE3          TAPE ERROR COMMENT LINE 3
         BALR  R8,R9               PRINT IT
         MVC   PG(80),TE4          TAPE ERROR COMMENT LINE 4
         BALR  R8,R9               PRINT IT
         MVC   PG(80),TE5          TAPE ERROR COMMENT LINE 5
         BALR  R8,R9               PRINT IT
         MVC   BL+1(131),BL        CLEAR PRINT AREA
         MVC   PG(28),TE6          TAPE ERROR COMMENT - LINE 6
         BALR  R8,R9               PRINT IT
         L     RA,RETURN
         BR    RA                  WE ARE DONE - RETURN TO DITTO
RETURN   DS    1F
SAVR6    DS    F                   SAVE R6 DURING PROCEDURE PAGE LOOP
SAVR6XXX DS    F                   SAVE R6 BEFORE BR TO PRINT
BUMP     DC    H'4096'             BASE REGISTER BUMP
PARAM    B     X1                  VECTOR TABLE
         B     X2                  ENTRY INTO THIS TABLE
         B     X3                  IS BASED ON AN INDEX
         B     X4                  VALUE OBTAINED FROM
         B     X5                  TBL4.
         B     X6                  EXIT IS TO A SPECIFIC
         B     X7                  MOVE STATEMENT FOR
         B     X8                  THE REQUIRED PARAMETER
         B     X9
         B     XA
         B     XB
         B     XC
X1       LR    R8,R0               NO PARAMETER REQUIRED
