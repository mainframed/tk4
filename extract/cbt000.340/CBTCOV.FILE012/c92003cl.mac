%--------------------------  DEFINE A VSAM CLUSTER  --------------------
+ENTER/VERIFY PARAMETERS BELOW:
+
+CLUSTER PARMETERS:
+   NAME                 %===>_VSNAME
+   MODEL                %===>_VSMOD
+   CLUSTER TYPE         %===>_VSTYPE    +
+   BUFFER SPACE         %===>_VSBUFSP   +
+   UNIQUE/SUBALLOC      %===>_VSATYP  +   SHARE OPTIONS        %===>_VS
+
+DATA COMPONENT PARAMETERS:
+   CONTROL INTERVAL SIZE%===>_VSDCISZ+
+   ALLOCATION UNITS     %===>_VSDALLOC +
+   PRIMARY QUANTITY     %===>_VSDPRIM    +SECONDARY QUANTITY   %===>_VS
+   CI FREESPACE         %===>_VSCIFSP+    CA FREESPACE         %===>_VS
+   AVERAGE RECORD LENGTH%===>_VSALRL+     MAXIMUM RECORD LENGTH%===>_VS
+   KEY LENGTH           %===>_VSKEYLEN+   KEY OFFSET           %===>_VS
+   VOLUMES              %===>_VSDVOL

+INDEX COMPONENT PARAMETERS:
+   IMBED/NOIMBED        %===>_VSIMBED+    REPLICATE/NOREPL     %===>_VS
+   VOLUMES              %===>_VSIVOL

)INIT
 .HELP = V00T51
)PROC
VER (&VSNAME,NONBLANK)
VER (&VSNAME,DSNAME)
VER (&VSMOD,DSNAME)
&VSTYPE = TRANS(TRUNC(&VSTYPE,1) I,INDEXED *,&VSTYPE)
&VSTYPE = TRANS(TRUNC(&VSTYPE,2) NO,NONINDEXED NU,NUMBERED *,&VSTYPE)
VER (&VSTYPE,LIST,INDEXED,NONINDEXED,NUMBERED)
VER (&VSBUFSP,NUM)
&VSATYP = TRANS(TRUNC(&VSATYP,1) U,UNIQUE S,SUBALLOC *,&VSATYP)
VER (&VSATYP,LIST,UNIQUE,SUBALLOC)
VER (&VSSHR,LIST,1,2,3,4,'1 3','1 4','2 3','2 4','3 3','3 4','4 3'
    '4 4')
VER (&VSDCISZ,NUM)
VER (&VSDCISZ,RANGE,512,32768)
IF  (&VSMOD = &Z)
     VER (&VSDALLOC,NONBLANK)
&VSDALLOC = TRANS(TRUNC(&VSDALLOC,1) R,RECORDS T,TRACKS C,CYLINDERS
     *,&VSDALLOC)
VER (&VSDALLOC,LIST,RECORDS,TRACKS,CYLINDERS)
IF  (&VSDALLOC ^= &Z)
    VER (&VSDPRIM,NONBLANK)
VER (&VSDPRIM,NUM)
VER (&VSDPRIM,RANGE,1,999999999999)
VER (&VSDSEC,NUM)
VER (&VSCIFSP,NUM)
VER (&VSCIFSP,RANGE,0,100)
VER (&VSCAFSP,NUM)
VER (&VSCAFSP,RANGE,0,100)
IF  (&VSMLRL ^= &Z)
    VER (&VSALRL,NONBLANK)
IF  (&VSMOD = &Z)
    VER (&VSALRL,NONBLANK)
VER (&VSALRL,NUM)
VER (&VSALRL,RANGE,1,32761)
VER (&VSMLRL,NUM)
VER (&VSMLRL,RANGE,1,32761)
IF  (&VSMOD = &Z)
    IF  (&VSTYPE = INDEXED,&Z)
        VER (&VSKEYLEN,NONBLANK)
IF  (&VSKEYOFF ^= &Z)
    VER (&VSKEYLEN,NONBLANK)
VER (&VSKEYLEN,NUM)
VER (&VSKEYLEN,RANGE,1,255)
VER (&VSKEYOFF,NUM)
IF  (&VSMOD = &Z)
    VER (&VSDVOL,NONBLANK)
&VSIMBED = TRANS(TRUNC(&VSIMBED,1) I,IMBED N,NOIMBED *,&VSIMBED)
VER (&VSIMBED,LIST,IMBED,NOIMBED)
&VSREPL = TRANS(TRUNC(&VSREPL,1) R,REPLICATE N,NOREPL *,&VSREPL)
VER (&VSREPL,LIST,REPLICATE,NOREPL)
/*
&WPRE = TRANS(&ZPREFIX &Z,&Z *,'&ZPREFIX..')
&DSN = TRANS(TRUNC(&VSNAME,1) '''',&VSNAME &Z,&Z
     *,'''&WPRE&VSNAME''')
&DSNMOD = TRANS(TRUNC(&VSMOD,1) '''',&VSMOD &Z,&Z
     *,'''&WPRE&VSMOD''')
&MODEL = TRANS(&VSMOD &Z,&Z *,'MODEL(&DSNMOD)')
&BUFSP = TRANS(&VSBUFSP &Z,&Z *,'BUFFERSPACE(&VSBUFSP)')
&ALLOCTYP = &VSATYP
/*IF  (&VSMOD = &Z)
/*    &ALLOCTYP = TRANS(&VSATYP &Z,UNIQUE *,&VSATYP)
&SHROPT = TRANS(&VSSHR &Z,&Z *,'SHAREOPTIONS(&VSSHR)')
&DCISZ = TRANS(&VSDCISZ &Z,&Z *,'CISZ(&VSDCISZ)')
&DALLOC = TRANS(&VSDALLOC &Z,&Z *,'&VSDALLOC(&VSDPRIM &VSDSEC)')
&FSP = 'FREESPACE(&VSCIFSP &VSCAFSP)'
IF  (&VSCIFSP = &Z)
    &FSP = TRANS(&VSCAFSP &Z,&Z *,'FREESPACE(0 &VSCAFSP)')
&RECSIZE = 'RECORDSIZE(&VSALRL &VSMLRL)'
IF  (&VSMLRL = &Z)
    &RECSIZE = TRANS(&VSALRL &Z,&Z *,'RECORDSIZE(&VSALRL &VSALRL)')

&KEYS = 'KEYS(&VSKEYLEN &VSKEYOFF)'
IF  (&VSKEYOFF = &Z)
    &KEYS = TRANS(&VSKEYLEN &Z,&Z *,'KEYS(&VSKEYLEN 0)')
IF  (&VSTYPE = NUMBERED,NONINDEXED)
    &KEYS = &Z
&DVOL = TRANS(&VSDVOL &Z,&Z *,'VOLUMES(&VSDVOL)')
&IVOL = &DVOL
&IVOL = TRANS(&VSIVOL &Z,&IVOL *,'VOLUMES(&VSIVOL)')
)END
