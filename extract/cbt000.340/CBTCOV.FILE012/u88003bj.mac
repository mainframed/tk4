//ARCHRET  PROC PREFIX=SYS2
//*
//* RUN ARCHIVE SYSTEM TO RETURN FILES VERSION 2
//*
//BACKUPQ EXEC PGM=IEBGENER
//SYSPRINT DD DUMMY
//SYSIN   DD DUMMY
//SYSUT1  DD DSN=&PREFIX..ARCHIVE.QUEUE,DISP=SHR
//SYSUT2  DD DSN=&PREFIX..ARCHIVE.QUEUE.BACKUPR,DISP=OLD
//*
//WTOQ EXEC PGM=WTO,COND=(0,EQ,BACKUPQ),
//     PARM='UNABLE TO MAKE BACKUP COPY OF QUEUE FILE'
//SYSPRINT DD SYSOUT=*
//SYSIN    DD DUMMY
//*
//RETURN   EXEC PGM=RARRETN,COND=(4,LT)
//QUEUE    DD DSN=&PREFIX..ARCHIVE.QUEUE,DISP=SHR CONTROLLED BY ENQ/DEQ
//TRACE    DD DSN=SYS2.ARCHIVE.TRACE.DSN.PATH,DISP=SHR
//SYSPRINT DD SYSOUT=*
//FDRJCL  DD SYSOUT=(A,INTRDR)
