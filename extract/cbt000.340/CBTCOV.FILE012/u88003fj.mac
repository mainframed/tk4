//QUEUEBLD EXEC PGM=IEBDG
//SYSPRINT DD SYSOUT=*
//*MAIN CLASS=HOTJOB
//QUEUE    DD DSN=SYS2.ARCHIVE.QUEUE,DISP-SHR
//* QUEUE  DD DSN=SYS2.ARCHIVE.QUEUE,DISP=(NEW,CATLG),
//*           DCB=(RECFM=F,BLKSIZE=4064,LRECL=4064),
//*            SPACE=(CYL,1),VOL=SER=SYSMCT,UNIT=3350
//* NOTE THE ACTUAL RECORD SIZE IS 127 BUT THIS FORMAT IS USED TO STOP
//* USERS EDITING THE QUEUE WITH TSO OR SPF EDIT
//*
//* SET UP A DUMMY RECORD IN THE QUEUE MARKED AS SCRATCHED AND LAST ONE
//*     ALSO SET BLKEND AND QUEEND FLAGS AT END OF FIRST BLOCK
//SYSIN    DD *
 DSD OUTPUT=(QUEUE)
 FD NAME=DSN,LENGTH=44,STARTLOC=1,FILL=' ',
    PICTURE=20,'<SYS2.ARCHIVE.QUEUE>'
 FD NAME=VOLSER,LENGTH=6,STARTLOC=45,FILL='V'
 FD NAME=USERID,LENGTH=7,STARTLOC=51,FILL='U'
 FD NAME=DATE,LENGTH=6,STARTLOC=58,FILL='D'
 FD NAME=TAPE,LENGTH=6,STARTLOC=64,FILL='T'
 FD NAME=TLBL,LENGTH=4,STARTLOC=70,FILL='L'
 FD NAME=KEY,LENGTH=8,STARTLOC=74,FILL='K'
 FD NAME=NEWDSN,LENGTH=44,STARTLOC=82,FILL='N'
 FD NAME=ACTION,LENGTH=1,STARTLOC=126,FILL=X'F2'
 FD NAME=END,LENGTH=1,STARTLOC=127,FILL=X'08'
 FD NAME=ENDQUE,LENGTH=1,STARTLOC=4064,FILL=X'90'
 CREATE QUANTITY=1,FILL=X'00',
   NAME=(DSN,VOLSER,USERID,DATE,TAPE,TLBL,KEY,NEWDSN,ACTION,END,ENDQUE)
/*
