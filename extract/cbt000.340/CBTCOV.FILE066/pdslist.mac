//PDSLIST  EXEC  PGM=PDSLIST,REGION=128K,PARM=''
//*    PARM=''      -> NORMAL LIST OF COMPLETE PDS
//*    PARM=NOLIST  -> PRINT DIRECTORY ONLY
//*    PARM=SCAN    -> PRINT FIRST PAGE OF EACH MEMBER
//*    PARM=EJECT   -> START EACH MEMBER ON A NEW PAGE
//*    PARM=PUNCH   -> EACH MEMBER PUNCHED AS A SERIES OF CONCATENATED
//*                    SEQUENTIAL DATASETS WITH NO MEMBER ID CARDS
//SYSPRINT DD SYSOUT=A,DCB=BLKSIZE=3500
//SYSUT1   DD DSN=HMD.MVSUG.CNTL,DISP=SHR
//SYSUT2   DD SYSOUT=B
