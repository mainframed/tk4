//COMP        EXEC ASMFC,
//  PARM.ASM='OBJ,NODECK,TERM,STMT,NUM,BUF(MAX)'
//ASM.SYSLIB  DD
//            DD DSN=SYS1.AMODGEN,DISP=SHR
//            DD DSN=HMD.MVSUG.MACLIB,DISP=SHR
//ASM.SYSTERM DD SYSOUT=A
//ASM.SYSGO   DD DSN=HMD.MVSUG.OBJ(VSAMCAT),DISP=SHR
//*
//LINK        EXEC LKED,
//  PARM.LKED='MAP,LIST,XREF,NCAL,LET'
//LKED.SYSLMOD   DD   DSN=HMD.MVSUG.LOAD,DISP=SHR
//LKED.OBJLIB    DD   DSN=HMD.MVSUG.OBJ,DISP=SHR
//LKED.SYSIN     DD   *
 INCLUDE OBJLIB(VSAMCAT)
 ENTRY VSAMCAT
 SETCODE AC(1)      ** FOR READING CATALOG LIKE DATASET **
 NAME  VSAMCAT(R)
 //*
 //* SAMPLE EXECUTION JCL FOLLOWS:
 //*
//PNSACAT JOB (M,AJNJ,S610C021),04542HMD,CLASS=B,NOTIFY=PNSA,
//  MSGCLASS=Q,TYPRUN=HOLD
/*ROUTE  PRINT LOCAL
//*ACCT        HOWARD DEAN,SSU
/*JOBPARM L=999,S=*
//S1       EXEC PGM=VSAMCAT,PARM='PASSWORD'   ** CATALOG MSTR PW    **
//STEPLIB  DD   DSN=HMD.MVSUG.LOAD,DISP=SRH   ** APF AUTHORIZED LIB **
//STEPCAT  DD   DSN=CATALOG.VPS5002,DISP=SHR  ** CATALOG NAME       **
//USERCAT  DD   DSN=CATALOG.VPS5002,DISP=SHR  ** CATALOG NAME       **
//SYSPRINT DD   SYSOUT=*
//CATSTAT  DD   DSN=HMD.STATS.DATA,DISP=(,PASS,DELETE),
//  UNIT=SYSDA,SPACE=(CYL,(10)),DCB=(BLKSIZE=6160,LRECL=80,RECFM=FB)
//DEFINE   DD   DSN=HMD.DEFINE.DATA,DISP=(,PASS,DELETE),
//  UNIT=SYSDA,SPACE=(CYL,(10)),DCB=(BLKSIZE=6160,LRECL=80,RECFM=FB)
//VTOC     DD   UNIT=SYSDA,VOL=SER=PS5002,DISP=SHR
//*                          ** ABOVE IS CATALOG VOLUME SERIAL **
//*
//IEFBR14  EXEC PGM=IEFBR14
//*******************************************************************
//* THIS STEP IS NEEDED SO THAT THE OUTPUT DATASETS DO NOT GET      *
//* CATALOGED IN THE 'STEPCAT' ABOVE. CATALOG MANAGEMENT WILL       *
//* BYPASS CVOL PROCESSING IF 'STEPCAT' IS USED SO IF YOUR HIGH     *
//* LEVEL INDEX SHOULD NOT BE CATALOGED IN THE CATALOG YOU ARE      *
//* PUNCHING, THEN USE THIS STEP, OTHERWISE CHANGE 'DISP' ABOVE     *
//* TO CATALOG AND DELETE THIS STEP.                                *
//*******************************************************************
//DD1      DD   DSN=*.S1.CATSTAT,DISP=(OLD,CATLG)
//DD2      DD   DSN=*.S1.DEFINE,DISP=(OLD,CATLG)
