//IDMFASM  PROC  M=
//*-------------------------------------------------------------------*
//* I D M F - I N T E R A C T I V E   D A T A   M A N A G E M E N T   *
//*                                                                   *
//*   THIS PROCEDURE ASSEMBLES THE MODULES COMPRISING THE IDMF        *
//*   FACILITY OF SPF. THIS FACILITY USES SPF TABLE HANDLING ROUTINES *
//*   TO PROCESS SELECTED FILES.                                      *
//*                                                                   *
//*-------------------------------------------------------------------*
//ASM      EXEC PGM=IFOX00,REGION=1024K,
//    PARM='LOAD,NODECK,TERM,STMT,NUM,RENT,BUF(MAX)'
//SYSPRINT DD   SYSOUT=*
//SYSTERM  DD   SYSOUT=*
//MACREF   DD   DUMMY
//SYSLIB   DD   DSNAME=HMD.MVSUG.MACLIB,DISP=SHR
//         DD   DSNAME=SYS1.MACLIB,DISP=SHR
//         DD   DSNAME=SYS1.AMODGEN,DISP=SHR
//SYSPUNCH DD   DUMMY
//SYSGO    DD   DSN=HMD.MVSUG.OBJ(IDM&M),DISP=OLD
//SYSUT1   DD   DSNAME=&SYSUT1,UNIT=SYSDA,SPACE=(1700,(400,100))
//SYSUT2   DD   DSNAME=&SYSUT2,UNIT=SYSDA,SPACE=(1700,(400,50))
//SYSUT3   DD   DSNAME=&SYSUT3,UNIT=SYSDA,SPACE=(1700,(400,50))
//SYSIN    DD   DSNAME=HMD.MVSUG.SOURCE(IDM&M),DISP=SHR
//   PEND
//*
//LKED     PROC
//LKED     EXEC PGM=IEWL,PARM=(XREF,LIST,MAP,LET,RENT),REGION=1024K
//SYSPRINT DD   SYSOUT=*
//SYSLIN   DD   DDNAME=SYSIN
//OBJLIB   DD   DSNAME=HMD.MVSUG.OBJ,DISP=SHR
//SYSLMOD  DD   DSN=HMD.MVSUG.LOAD,DISP=SHR
//SYSLIB   DD   DSN=HMD.MVSUG.LOAD,DISP=SHR
//SYSUT1   DD   DSNAME=&LK-UT1,UNIT=SYSDA,SPACE=(1024,(100,200))
//   PEND
//UXIT     EXEC IDMFASM,M=UXIT   MUST COME FIRST - USER EXIT TABLE
//LINK     EXEC LKED
//LKED.SYSIN    DD  *
 INCLUDE OBJLIB(IDMUXIT)
 ALIAS IDMXP
 ALIAS IDMXB
 ALIAS IDMXS
 NAME  IDMUXIT(R)
//MAIN     EXEC IDMFASM,M=MAIN            MAIN ROUTINE
//ALLOC    EXEC IDMFASM,M=ALLOC           ALLOCATION ROUTINE
//LOCIX    EXEC IDMFASM,M=LOCIX           LOCINDEX SUBROUTINE
//BOZD     EXEC IDMFASM,M=BOZD            OZD BROWSE INTERFACE
//SOZD     EXEC IDMFASM,M=SOZD            OZD EDIT   INTERFACE
//DELET    EXEC IDMFASM,M=DELET           DELETE ROUTINE
//DSAT     EXEC IDMFASM,M=DSAT            DSAT SIMULATOR
//PRINT    EXEC IDMFASM,M=PRINT           PRINTOFF INTERFACE
//LINK2    EXEC LKED
//LKED.SYSIN    DD  *
 INCLUDE OBJLIB(IDMMAIN)       MAINLINE ROUTINE
 NAME    IDMMAIN(R)
 INCLUDE OBJLIB(IDMALLOC)      ALLOCATION ROUTINE
 NAME    IDMALLOC(R)
 INCLUDE OBJLIB(IDMLOCIX)      LOCINDEX ROUTINE
 NAME    IDMLOCIX(R)
 INCLUDE OBJLIB(IDMBOZD)       OZD BROWSE INTERFACE
 NAME    IDMBOZD(R)
 INCLUDE OBJLIB(IDMSOZD)       OZD EDIT    ROUTINE
 NAME    IDMSOZD(R)
 INCLUDE OBJLIB(IDMDELET)      DELETE ROUTINE
 NAME    IDMDELET(R)
 INCLUDE OBJLIB(IDMDSAT)       DSAT   ROUTINE
 NAME    IDMDSAT(R)
 INCLUDE OBJLIB(IDMPRINT)      PRINT  ROUTINE
 NAME    IDMPRINT(R)
