//ASM  PROC  MOD=DUMMY
//ASMX  EXEC PGM=IFOX00,
//  PARM='OBJ,NODECK,TERM,STMT,NUM,RENT,BUF(MAX)',REGION=2048K
//SYSLIB   DD  DSN=SYS1.MACLIB,DISP=SHR
//         DD  DSN=HMD.MVSUG.MACLIB,DISP=SHR
//         DD  DSN=SYS1.AMODGEN,DISP=SHR
//         DD  DSN=SYS1.HASPSRC,DISP=SHR,UNIT=DISK,VOL=SER=ITSTC2
//         DD  DSN=SYSTC2.ACFMAC,DISP=SHR,UNIT=DISK,VOL=SER=ITSTC2
//* ABOVE REQUIRED IF YOU HAVE ACF2 SECURITY PACKAGE INSTALLED
//SYSUT1   DD  UNIT=SYSDA,SPACE=(CYL,(3,1)),DSN=&SYSUT1
//SYSUT2   DD  UNIT=SYSDA,SPACE=(CYL,(3,1)),DSN=&SYSUT2
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,(3,1)),DSN=&SYSUT3
//SYSPUNCH DD  DUMMY
//SYSPRINT DD  SYSOUT=A
//SYSTERM  DD  SYSOUT=A
//SYSGO    DD  DISP=OLD,DSN=HMD.MVSUG.OBJ(&MOD)
//SYSIN    DD  DISP=SHR,DSN=HMD.MVSUG.SOURCE(&MOD)
//ASM  PEND
//*
//LINK  PROC
//LKED  EXEC   PGM=IEWL,
//    PARM='MAP,LIST,XREF,LET,RENT',REGION=512K
//SYSLIN   DD  DDNAME=SYSIN
//SYSLMOD  DD  DSN=HMD.MVSUG.LOAD,DISP=SHR
//OBJLIB   DD  DSN=HMD.MVSUG.OBJ,DISP=SHR
//SYSUT1   DD  UNIT=SYSDA,SPACE=(CYL,(3,1)),DSN=&SYSUT1
//SYSPRINT DD  SYSOUT=A
//LINK  PEND
//*
//ASM1 EXEC ASM,MOD=STATUS1
//ASM2 EXEC ASM,MOD=STATUS2
//ASM3 EXEC ASM,MOD=STATUS3
//ASM4 EXEC ASM,MOD=STATUS4
//ASM5 EXEC ASM,MOD=STATUS5
//LINK EXEC LINK
//LKED.SYSIN DD *
  INCLUDE OBJLIB(STATUS1)
  INCLUDE OBJLIB(STATUS2)
  INCLUDE OBJLIB(STATUS3)
  INCLUDE OBJLIB(STATUS4)
  INCLUDE OBJLIB(STATUS5)
  ENTRY STATUS
  ALIAS IGC0020H     BE SURE TO ADD SVC-208 TO YOUR SVCTABLE MACRO
  ALIAS FINDASCB     USEFUL AS A STAND-ALONE SUBROUTINE
  ALIAS ST           AVOID EXTRA KEYSTROKES
  NAME STATUS(R)
/*
//*
//* COPY THE STATUS MODULE AND ALL ALIASES TO
//* SYS1.LPALIB AND IPL WITH CLPA TO INSTALL.
//* BE SURE TO ADD SVC 208 TO YOUR SVCTABLE MACRO
//* SVC 208 IS CALLED ONLY FROM STATUS1 SO CHANGE
//* THAT SOURCE IF THE SVC NUMBER CHANGES.
//*
//* BE SURE TO CHANGE CSECT NAME OF IGC0020H AND
//* THE ABOVE ALIAS STATEMENT IF YOU CHANGE THE SVC
//* NUMBER.
//*
//* CAN BE RENAMED TO JSTATUS (ALIAS JS) IF
//* YOU DON'T WANT TO REPLACE IBM'S STATUS TSO COMMAND
//*
