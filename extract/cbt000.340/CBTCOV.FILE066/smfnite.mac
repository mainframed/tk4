//SMFNITE  JOB (7730,SYST,6751),'SMF NIGHTLY JOB    ',NOTIFY=S6751MH,
//  REGION=1024K,MSGCLASS=A,CLASS=A,USER=S6751MH
//STEP1    EXEC PGM=SORT,REGION=1024K
//SYSPRINT DD   SYSOUT=*
//SYSUDUMP DD   SYSOUT=*
//SYSOUT DD SYSOUT=*
//SORTWK01 DD   UNIT=SYSDA,SPACE=(CYL,(20,10))
//SORTWK02 DD   UNIT=SYSDA,SPACE=(CYL,(20,10))
//SORTWK03 DD   UNIT=SYSDA,SPACE=(CYL,(20,10))
//SORTWK04 DD   UNIT=SYSDA,SPACE=(CYL,(20,10))
//SORTIN   DD   DSN=SMFDATA.TEMP.CPUB,DISP=SHR
//         DD   DSN=SMFDATA.TEMP.CPUC,DISP=SHR
//SORTOUT DD    DSN=&&SMF1,UNIT=3380,SPACE=(CYL,(45,20)),
//  VOL=SER=PUB702,DISP=(,PASS)
//SYSIN     DD *
  SORT   FIELDS=(11,4,BI,A,7,4,BI,A)
/*
//STEP2   EXEC PGM=SMFKOPY,PARM='/SMFDATA.NITE.BACKUP'
//STEPLIB DD DSN=SMDC.SYSTEMS.LOADLIB,DISP=SHR
//   DD DSN=SMDC.PLIBASE,DISP=SHR       NEED PLI RUN TIME LIBRARY
//SYSPRINT DD SYSOUT=*
//SYSUDUMP DD SYSOUT=*
//INDS DD DSN=&&SMF1,DISP=(OLD,PASS)
//***************************************************************
//*                                                             *
//* THIS STEP WILL DYNAMICALLY ALLOCATE A NUMBER OF DATASETS    *
//* INCLUDING TWO WHICH WILL BE ON TAPE.                        *
//*                                                             *
//* YOU WILL BE ASKED TO MOUNT TWO TAPES.                       *
//* DO NOT THINK THAT THIS JOB DOES NOT USE TAPE DRIVES BECAUSE *
//* YOU CANNOT SEE ANY IN THE JCL !!!!!!!                       *
//*                                                             *
//* I HATE OLD FASHIONED JCL 'COS I'M A VM BIGOT                *
//*                                                             *
//* THIS STEP ONLY NEEDS A SYSPRINT !!!!! (AND STEPLIBS)        *
//*                                                             *
//***************************************************************
//STEP3 EXEC PGM=SMFPG2,PARM='/SMFDATA.NITE.BACKUP'
//STEPLIB DD DSN=SMDC.SYSTEMS.LOADLIB,DISP=SHR
//   DD DSN=SMDC.PLIBASE,DISP=SHR       NEED PLI RUN TIME LIBRARY
//SYSPRINT DD SYSOUT=A,DCB=(LRECL=133,BLKSIZE=1330,RECFM=FBA)
//***************************************************************
//*
//*       BUILDS A DAILY PDB FROM THE DUMPED SMF RECORDS.
//*       APPENDS A DAILY PDB TO THE CURRENT GENERATION OF PDB GDG.
//*       PRINTS DAILY REPORTS.          A. S. 08/05/85.
//*
//STEP4    EXEC PGM=SAS,PARM='SORT=4',REGION=2048K
//STEPLIB   DD DSN=SMDC.SAS.LIBRARY,DISP=SHR
//          DD DSN=SYS1.PLITASK,DISP=SHR  ***** DSNAME?
//          DD DSN=SYS1.PLILIB,DISP=SHR  ***** DSNAME?
//          DD DSN=SYS1.SORTLIB,DISP=SHR     ***** DSNAME?
//WORK      DD UNIT=3380,SPACE=(6160,(10000,1000),,,ROUND)
//FT11F001  DD SYSOUT=*,DCB=(BLKSIZE=141,LRECL=137,RECFM=VBA) LOG
//FT12F001  DD SYSOUT=*,DCB=(BLKSIZE=141,LRECL=137,RECFM=VBA) PRINT
//FT13F001  DD SYSOUT=*,DCB=(RECFM=F,BLKSIZE=80)              PUNCH
//FT15F001  DD UNIT=SYSDA,SPACE=(400,(100,300)),         PARMCARDS
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=400,BUFNO=1)
//SORTWK01  DD UNIT=DISK,SPACE=(CYL,(8,1))
//SORTWK02  DD UNIT=DISK,SPACE=(CYL,(8,1))
//SORTWK03  DD UNIT=DISK,SPACE=(CYL,(8,1))
//SYSOUT    DD SYSOUT=*
//SASLIB    DD DSN=OSS03AS.SAS.FORMATS,DISP=SHR
//PDBEXTR   DD DSN=SMFDATA.PDBEXTR(0),DISP=OLD
//TITLE1    DD UNIT=DISK,SPACE=(TRK,1)
//TITLE3    DD UNIT=DISK,SPACE=(TRK,1)
//DATES     DD UNIT=DISK,SPACE=(TRK,1)
//RPTTYPE   DD UNIT=DISK,SPACE=(TRK,1)
//SMF       DD   DISP=SHR,DSN=SMFDATA.NITE.BACKUP
//SYSIN     DD *
//          DD DSN=OSS03AS.SAS.REPORTS(PDBEXTR),DISP=SHR
//          DD DSN=OSS03AS.SAS.REPORTS(PDBLDATE),DISP=SHR
//          DD DSN=OSS03AS.SAS.REPORTS(BYHOUR),DISP=SHR
//          DD DSN=OSS03AS.SAS.REPORTS(BRKDOWN),DISP=SHR
//          DD DSN=OSS03AS.SAS.REPORTS(SUMCLASS),DISP=SHR
//          DD DSN=OSS03AS.SAS.REPORTS(SUMPRINT),DISP=SHR
//          DD DSN=OSS03AS.SAS.REPORTS(RANKING),DISP=SHR
//          DD DSN=OSS03AS.SAS.REPORTS(TSOEXPTS),DISP=SHR
//          DD DSN=OSS03AS.SAS.REPORTS(CHPEXPTS),DISP=SHR
//          DD DSN=OSS03AS.SAS.REPORTS(DEVEXPTS),DISP=SHR
//STEP5 EXEC PGM=IEFBR14
//DD1 DD DSN=SMFDATA.NITE.BACKUP,DISP=(OLD,DELETE)
