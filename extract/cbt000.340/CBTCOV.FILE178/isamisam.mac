*          DATA SET GU021500   AT LEVEL 001 AS OF 05/01/73
ISAMISAM SUBENTRY
*              AUTHOR.   DON HIGGINS.
*              DATE.     04/26/73.
*              REMARKS.  COPY ISAM FILE TO ISAM FILE.
*              FILES.  ISAMIN - INPUT/ISAMOUT - OUTPUT
         OPEN  (ISAMIN,(INPUT),ISAMOUT,(OUTPUT))
LOOP     TAG
         GET   ISAMIN
         LR    R0,R1
         PUT   ISAMOUT
         B     LOOP
EOF      TAG
         CLOSE (ISAMIN,,ISAMOUT)
         SUBEXIT
         EQUAL
ISAMIN   DCB   DSORG=IS,MACRF=GL,DDNAME=ISAMIN,EODAD=EOF
ISAMOUT  DCB   DSORG=IS,MACRF=PM,DDNAME=ISAMOUT
         END   ISAMISAM
