C          DATA SET GU723000   AT LEVEL 005 AS OF 08/08/75
C     PGM-ID.  GU723000.
C     AUTHOR.  DON HIGGINS.
C     REMARKS. TEST DRIVER FOR SWF SUBROUTINE (GU722000).
C
      IMPLICIT INTEGER(A-Z)
      DEFINE FILE 11(10,20,U,IX11)
      INTEGER R11(2),B11(20)
      DATA LR11/2/,LB11/20/
C
C     STORE I AND I*I IN SWF FILE FOR I=1,100,2
C
      WRITE(6,1)
    1 FORMAT('1START CREATE OF SWF FILE')
      CALL SWF(4,11,NB11,NR11,B11,LB11,R11,LR11,IEOF)
      DO  I=1,100,2
          R11(1)=I
          R11(2)=I*I
          CALL SWF(2,11,NB11,NR11,B11,LB11,R11,LR11,IEOF)
          WRITE(6,11)NB11,NR11
      OD
      CALL SWF(3,11,NB11,NR11,B11,LB11,R11,LR11,IEOF)
      WRITE(6,11)NB11,NR11
C
C     READ BACK RECORDS FROM SWF FILE AND LIST THEM
C
      WRITE(6,2)
    2 FORMAT('1START READ BACK FROM SWF FILE')
      CALL SWF(4,11,NB11,NR11,B11,LB11,R11,LR11,IEOF)
   10 CALL SWF(1,11,NB11,NR11,B11,LB11,R11,LR11,IEOF)
      IF(IEOF.EQ.1)GO TO 20
      WRITE(6,11)R11
   11 FORMAT(1X,2I5)
      GO TO 10
   20 CONTINUE
      WRITE(6,3)
    3 FORMAT('0END OF SWF TEST')
      STOP
      END
