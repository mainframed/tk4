//SYSUT3 DD    UNIT=(SYSDA,SEP=(SYSUT1,SYSUT2)),SPACE=(CYL,(16,2))
//SYSPUNCH DD  DSNAME=&LOADSET,DISP=(MOD,PASS),UNIT=SYSDA,             X
//             SPACE=(TRK,(50,10))
//SYSIN DD DDNAME=SYSIN,DCB=BLKSIZE=80
