//SYSLIN   DD  DSNAME=&LOADSET,DISP=(OLD,DELETE)
//         DD  DDNAME=SYSIN
//SYSLMOD  DD  DSNAME=&GODATA(RUN),DISP=(NEW,PASS),UNIT=SYSDA,         X
//             SPACE=(1024,(50,20,1))
//SYSLIB   DD  DSNAME=SYS1.COBLIB,DISP=SHR
//SYSUT1   DD  UNIT=(SYSDA,SEP=(SYSLIN,SYSLMOD)),SPACE=(1024,(50,20))
