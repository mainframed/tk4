         MVC   2(4,R2),=C'CMS,'         YES, SAY SO                U057
         LA    R2,5(,R2)                POSITION PAST MSG          U057
SVC06    TM    6(R1),X'20'              OPT LOCK?                  U057
         BZ    SVC07                    NO                         U057
         MVC   2(4,R2),=C'OPT,'         YES, SAY SO                U057
         LA    R2,5(,R2)                POSITION PAST MSG          U057
SVC07    TM    6(R1),X'10'              SALLOC LOCK?               U057
         BZ    SVC08                    NO                         U057
         MVC   2(7,R2),=C'SALLOC,'      YES, SAY SO                U057
