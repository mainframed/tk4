DATA DATES (KEEP = DATE HOUR MONTH)
     DAYS  (KEEP = DATE HOUR MONTH);
SET  RMFDD.RMFDATA;
FORMAT DATE YYMMDD8.;
 MONTH = MONTH(DATE);
 OUTPUT DAYS;
 M2 = MONTH(  TODAY( )  ) ;
      IF M2 = 1 THEN  X = -11 ;
                ELSE  X =  1  ;
 M1 = M2 - X ;
 IF (MONTH = M1)  OR (MONTH = M2) THEN OUTPUT DATES;
PROC SORT DATA= DATES;     BY MONTH DATE;
PROC MEANS DATA= DATES N NOPRINT; BY MONTH DATE;
     OUTPUT OUT=DREPORT N=COUNT;
PROC PRINT DATA= DREPORT;
PROC SORT DATA= DAYS;     BY MONTH DATE;
PROC MEANS DATA= DAYS N NOPRINT; BY MONTH DATE;
     OUTPUT OUT=ZREPORT N=COUNT;
PROC SORT DATA= ZREPORT;   BY MONTH ;
PROC MEANS DATA=ZREPORT N NOPRINT; BY MONTH; OUTPUT OUT=MREPORT N=COUNT;
PROC PRINT DATA= MREPORT;
