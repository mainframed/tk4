BLK8X8   TITLE '8X8 BLOCK LETTER ROUTINE                              '
*---------------------------------------------------------------------*
*                                                                     *
*   PROGRAM NAME:  BLK8X8                                             *
*                                                                     *
*   PURPOSE:       PRODUCE 8X8 BLOCK LETTERS (CREATED FROM IEFSD095   *
*                  PROGRAM THAT PRODUCES 12 X 12 BLOCK LETTERS)       *
*                                                                     *
*   AUTHOR:        ???                                                *
*                                                                     *
*   DATE:          JUNE 03, 1982                                      *
*                                                                     *
*   INSTALLATION:  TITLE INSURANCE AND TRUST                          *
*                                                                     *
*   UPDATES:                                                          *
*      06/03/82  JPG1 MAKE IT PRODUCE 8X8 BLOCK LETTERS               *
*                                                                     *
*---------------------------------------------------------------------*
         TITLE 'MACROS FOR PATTERNS'
         MACRO
&NAME    BLANKS &NUMBER
         LCLA  &COUNT
&NAME    DC    4BL2'0'                                             JPG1
.LOOP    AIF   (&COUNT+1 GE &NUMBER).END
&COUNT   SETA  &COUNT+1
         DC    4BL2'0'                                             JPG1
         AGO   .LOOP
.END     MEND
         SPACE 2
         MACRO
&NAME    SPECIAL &CHARS
         LCLA  &COUNT
         LCLC  &CHAR
         AIF   (K'&CHARS GT 2).THREE
&NAME    DC    3BL1'0'                                             JPG1
         DC    CL2'&CHARS '
         DC    3BL1'0'                                             JPG1
         AGO   .END
.THREE   ANOP
&COUNT   SETA  6-K'&CHARS/2
&NAME    DC    BL1'0'                                              JPG1
&COUNT   SETA  1
.LOOP    ANOP
&CHAR    SETC  '&CHARS'(&COUNT,1)
         DC    CL2'&CHAR '
&COUNT   SETA  &COUNT+1
         AIF   (&COUNT LE K'&CHARS).LOOP
&COUNT   SETA  3-(K'&CHARS-1)/2
         DC    BL1'0'                                              JPG1
.END     MEND
         TITLE 'ENTRIES, REGISTERS AND DSECTS'
*
*        DESCRIPTION OF PARAMETER LIST
*
PARMS    DSECT
         USING PARMS,PARMREG
@LETTERS DS    A                        SOURCE STRING
@LEVEL   DS    A                        WHICH LINE OF BLOCK
@OUTPUT  DS    A                        WORK AREA
@FLEN    DS    A                        LENGTH FOR BLK8X8
*
DVADDR   EQU   8                        CONTAINS ADDRESS OF DV
*
*        DESCRIPTION OF STRING DOPE VECTOR
*
DV       DSECT
DVSTRING DS    A                       ADDRESS OF STRING
DVMAX    DS    H                       MAXIMUM LENGTH OF STRING
DVLENGTH DS    H                       PRESENT LENGTH OF STRING
*
         SPACE
PARMREG  EQU   1                       CONTAINS ADRESS OF PARMS
PATTERN  EQU   2                        USED FOR DECODING PATTERN
LETLEN   EQU   3                        LENGTH OF SOURCE LETTERS
*                                       LETLEN-1 USED DURING SETUP
*                                         IT MUST BE A DOUBLE REG
LTRADDR  EQU   4                        ADDR OF SOURCE LETTERS
LEVEL    EQU   5                       CONTAINS LEVEL OF LETTER PATTERN
FUNCADDR EQU   6                       ADDRESS OUTPUT AREA
*              LTRADDR THROUGH FUNCADDR+1 USED IN A LOAD MULTIPLE
ONE      EQU   8                        CONTAINS F'1' DURING LOOPS
CHAR     EQU   9                        CONTAINS CHARACTER TO BE STORED
BASEREG  EQU   11
SAVEREG  EQU   13
RETREG   EQU   14
LINKREG  EQU   15
         TITLE 'PROLOGUES AND SETUP ROUTINES'
BLK8X8   CSECT
         SAVE  (14,12),T,BLK8X8_&SYSDATE_&SYSTIME
         LR    BASEREG,LINKREG
         USING BLK8X8,BASEREG
         LM    LTRADDR,FUNCADDR+1,@LETTERS
         L     LEVEL,0(LEVEL)
         L     LETLEN,0(FUNCADDR+1)
         LA    RETREG,POSLOOP
         CH    LETLEN,=H'18'
         BNH   SETUP
         LH    LETLEN,=H'18'
SETUP    BCTR  LEVEL,0                  OFFSET IN TABLE
         LTR   LEVEL,LEVEL              ONE BYTE PER ENTRY
         BM    RETURN                   INVALID?
         CH    LEVEL,=H'16'             ALSO MUST BE LESS THEN 2*(9-1)
         BNL   RETURN
         LA    CHAR,C' '                BLANK IF GOING TO NEGLOOP
         A     LEVEL,=A(PATTERNS)       GET ADDRESS IN TABLE
         LA    ONE,1
         BR    RETREG                   LOOP ADDRESS ALREADY IN RETREG
         SPACE 2
FILL     MVC   1(0,FUNCADDR),0(FUNCADDR)
         TITLE 'OUTER LOOPS'
NEGLOOP  SR    PARMREG,PARMREG
         IC    PARMREG,0(0,LTRADDR)     GET NEXT LETTER
         B     DECODE
         SPACE 2
POSLOOP  SR    PARMREG,PARMREG
         IC    PARMREG,0(0,LTRADDR)     GET NEXT LETTER
         IC    CHAR,TRANSLT(PARMREG)    HOW DO WE STORE IT?
         TITLE 'MAIN DECODING LOOP'
DECODE   MH    PARMREG,=H'8'            OFFSET IN TABLE
         AR    LTRADDR,ONE              INCREMENT FOR NEXT TIME
         AR    PARMREG,LEVEL            ADD ON TABLE ADDRESS
         SR    PATTERN,PATTERN          CLEAR FOR INSERT           JPG1
         IC    PATTERN,0(PARMREG)       GET THE BIT PATTERN        JPG1
         LTR   PATTERN,PATTERN          IS THE HIGH ONE ON?
         BL    SPECHARS                 YES, IT MUST BE TWO CHARS
         LR    PARMREG,FUNCADDR         ADDRESS OF THESE TWELVE
         SLL   PATTERN,24               IN LEFT OF REG
         SR    PARMREG,ONE              FIRST THING IN LOOP IS ADD
MAINLOOP AR    PARMREG,ONE              POINT TO NEXT PLACE
         ALR   PATTERN,PATTERN          SHIFT LEFT AND TEST
         BC    4,MAINLOOP               MORE TO GO BUT NOT THIS ONE
         BZ    NEXTLET                  ALL ZERO,FORGET IT
         STC   CHAR,0(0,PARMREG)        STORE IT HERE
         BC    1,MAINLOOP               MORE TO GO
NEXTLET  LA    FUNCADDR,10(FUNCADDR)
         BCTR  LETLEN,RETREG
         B     RETURN
SPECHARS MVC   4(4,FUNCADDR),=CL4' '    CLEAR IT OUT
         MVC   5(2,FUNCADDR),0(PARMREG) MOVE THEM IN
         B     NEXTLET
         TITLE 'RETURN'
RETURN   SR    15,15                   ZERO RETURN CODE
         RETURN (14,12),T,RC=(15)
         TITLE 'CONSTANTS AND STORAGE AREAS'
         DS    0F
TRANSLT  DC    CL256' '
         ORG   TRANSLT+X'49'
         DC    C'*$.<(+×&&'            CENT DOESN'T PRINT
         ORG   TRANSLT+X'59'
         DC    C'*×$*);^-/'            EXCLAMATION DOESN'T PRINT
         ORG   TRANSLT+X'69'
         DC    C'* ,%_>?*'
         ORG   TRANSLT+C':'
         DC    C':#@''=',X'7F'
         ORG   TRANSLT+X'80'
         DC    C'*ABCDEFGHI****'
         ORG   TRANSLT+X'91'
         DC    C'JKLMNOPQR**  *'
         ORG   TRANSLT+X'A0'
         DC    C'**STUVWXYZ**'
         ORG   TRANSLT+X'B0'
         DC    C'0123456789'
         ORG   TRANSLT+C'A'-1
         DC    C'*ABCDEFGHI  * *'
         ORG   TRANSLT+C'J'-1
         DC    C'*JKLMNOPQR'
         ORG   TRANSLT+C'S'-2
         DC    C'/*STUVWXYZ  *'
         ORG   TRANSLT+C'0'
         DC    C'0123456789'
         ORG   TRANSLT+256
         LTORG
