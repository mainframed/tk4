//TS00043  JOB (V0002,11,,2000D,,2627),'R. CRUZ',CLASS=S,
//             NOTIFY=SB03039,MSGCLASS=A
/*JOBPARM ROOM=SOFT
//TS00043 EXEC SMPEPROC
//STAGELIB  DD DSN=SYS1.MVS38.STAGELIB,UNIT=3380,DISP=SHR,
//             VOL=SER=TARG38
//SYSIN     DD *
 SET BOUNDARY (GLOBAL)                     .
     RECEIVE SELECT (TS00043) SYSMODS LIST .
 SET BOUNDARY (TARG38)                     .
     APPLY   SELECT (TS00043)              .
     LIST    SYSMOD (TS00043)              .
 SET BOUNDARY (DLIB38)                     .
     ACCEPT  SELECT (TS00043)              .
     LIST    SYSMOD (TS00043)              .
/*
//SMPPTFIN  DD DATA,DLM='$$'
++ USERMOD (TS00043) .
++ VER (Z038) FMID (JDQ1110)                                      /*

   DESCRIPTION:  INSTALL FRONT-END IEBENQR FOR MODULES IEBCOPY,
                 IEBGENER, AND IEBUPDTE.

                                                                  */ .
++ JCLIN .
//TS00043 EXEC PGM=IEWL,PARM='LIST,MAP,NCAL,LET'
//AOSU0     DD DSN=SYS1.AOSU0,UNIT=3380,DISP=SHR,VOL=SER=DLIB38
//SYSLMOD   DD DSN=SYS1.LINKLIB,UNIT=3380,DISP=SHR,VOL=SER=TARG38
//SYSLIN    DD *
 INCLUDE AOSU0(IEBENQR)
 INCLUDE SYSLMOD(IEBCOPY)
    ORDER IEBDSMCA(P)
    ORDER IEBDSCPY
    ENTRY IEBENQR
    SETCODE AC(1)
    NAME IEBCOPY(R)
 INCLUDE SYSLMOD(IEBGENER)
 INCLUDE AOSU0(IEBENQR)
    ENTRY IEBENQR
    NAME IEBGENER(R)
 INCLUDE SYSLMOD(IEBUPDTE)
 INCLUDE AOSU0(IEBENQR)
   ENTRY IEBENQR
   NAME IEBUPDTE(R)
/*
++ MOD (IEBENQR ) DISTLIB (AOSU0) LKLIB (STAGELIB) .
$$
