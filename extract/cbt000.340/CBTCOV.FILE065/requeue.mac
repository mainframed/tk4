REQUEUE  ENTER ,
         REGISTER
         USING AFMAP,R4      MAP THE "ACTFIL" PARAMETER
         LA    R4,AFPARM     POINT TO "ACTFIL" PARM AREA
         LOAD  EP=ACTFIL     LOAD "ACTFIL"
         LR    R15,R0        SAVE ENTRY POINT ADDRESS
         CALL  (15),((R4))   CALL "ACTFIL"
         CLC   AFJES2NO(3),=C'JOB'  IF THIS IS NOT A BATCH JOB
         BNE   REQUEUEX                THEN LEAVE NOW
         MVC   COMMAND(L'$E),$E   MOVE IN REQUEUE COMMAND
         MVC   COMMAND+3(4),AFJES2NO+4   MOVE IN JES2 NUMBER
         BAL   R14,ISSUE     ISSUE THE COMMAND
         MVC   COMMAND(L'$H),$H   MOVE IN HOLD COMMAND
         MVC   COMMAND+3(4),AFJES2NO+4   MOVE IN JES2 NUMBER
         BAL   R14,ISSUE     ISSUE THE COMMAND
         MVC   COMMAND(L'$C),$C   MOVE IN CANCEL COMMAND
         MVC   COMMAND+3(4),AFJES2NO+4   MOVE IN JES2 NUMBER
         BAL   R14,ISSUE     ISSUE THE COMMAND
REQUEUEX LEAVE ,
         TITLE 'ISSUE AN OPERATOR COMMAND'
ISSUE    BEGIN ,
         MODESET KEY=ZERO,MODE=SUP
         LA    R1,CMD        POINT TO COMMAND
         LA    R0,0          INDICATE MASTER CONSOLE
         SVC   34            SEND THE COMMAND
         MODESET KEY=NZERO,MODE=PROB
ISSUEX   GOBACK ,
         EJECT
* CONSTANTS
$E       DC    C'$EJNNNN'
$H       DC    C'$HJNNNN'
$C       DC    C'$CJNNNN'
CMD      DC    H'44',H'0'
COMMAND  DC    CL80' '
AFPARM   DC    CL80' '
         EJECT
         ACTFIL
         END
