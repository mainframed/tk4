WIZARD   TITLE 'DCMS SRM DATA DISPLAY ROUTINE'
WIZARD   DCMSTART R,LV=4096,STAE=YES,IOCB=YES
         L     R3,CVTPTR
         USING CVT,R3
         L     R11,CVTOPCTP
         DROP  R3
         USING RMCNS,R11
*                                     DO STATIC DISPLAY ITEMS  LINE 1
         LA    R5,STATLINE
         MVC   0(79,R5),BLANKS
         L     R2,RMCTWMST
         LA    R2,WMSTID-WMST(R2)      SET
         MVC   0(L'IPSMSG,R5),IPSMSG      CURRENT
         MVC   L'IPSMSG(2,R5),0(R2)              IPS
*                                     DO STATIC DISPLAY ITEMS  LINE 2
         LA    R5,79(R5)
         MVC   0(79,R5),BLANKS
         MVC   0(79,R5),ERVTITL
         L     R2,RMPTCPU
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   5(3,R5),DECWORK4+1
         L     R2,RMPTIOC
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   13(3,R5),DECWORK4+1
         L     R2,RMPTERV
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   21(4,R5),DECWORK4
*                                     DO STATIC DISPLAY ITEMS  LINE 3
         LA    R5,79(R5)
         MVC   0(79,R5),BLANKS
         MVC   0(79,R5),RFCTITL
         L     R10,RMCTWMST
         USING WMST,R10
*        L     R2,WMSTIPC
*        ST    R2,DECWORK1
*        BAL   R14,@DECIMAL
*        MVC   06(02,R5),DECWORK4+1
*        MVC   09(01,R5),DECWORK4+3
         MVC   07(4,R5),WMSTIPC
         MVC   18(4,R5),WMSTIPI
         MVC   29(4,R5),WMSTIPM
*                                                          LINE 4
         LA    R5,79(R5)
         MVC   0(79,R5),BLANKS
*
         LA    R5,79(R5)
         MVC   0(79,R5),BLANKS
*                                                          LINE 5
         TM    @MODE,X'0F'
         BO    RUNSRMS
DOFLASH  MVI   @MODE,X'7F'
         B     RUNSRMS
STARTMD  MVI   @MODE,X'0F'
         B     RUNSRMS
RUNSRMS  EQU   *
         XC    IOCBWAIT(4),IOCBWAIT
         LA    R0,SCREEN
         LA    R1,DOMAINZ
         BAL   R14,@CLEAR
         LA    R5,LINE17           COPY
         LA    R2,5                    STATIC
         LA    R6,STATLINE                   LINES
STATLOOP EQU   *
         MVC   0(79,R5),0(R6)      ONTO
         LA    R5,79(R5)               CURRENT
         LA    R6,79(R6)                      SCREEN
         BCT   R2,STATLOOP
*
**
*
         LH    R7,RMCTDMNC
         L     R6,RMCTDMDT
         LA    R5,DOMAINZ
         MVC   0(79,R5),PAGETITL
         ST    R7,DECWORK1
         BAL   R14,@DECIMAL
         MVC   26(3,R5),DECWORK4+1
         LA    R4,13               ***LIMIT OF DOMAINS DISPLAYED
         CR    R4,R7
         BNL   DODOMAIN
         LR    R7,R4
DODOMAIN EQU   *
         LA    R5,79(R5)
         MVC   0(79,R5),DMNTITL
DMNLOOP  EQU   *
         LA    R5,79(R5)
         BAL   R15,DMNLINE
         LA    R6,DMDTLEN(R6)
         BCT   R7,DMNLOOP
         LA    R5,79(R5)
         MVI   0(R5),C' '
         MVC   1(79,R5),0(R5)
         LA    R4,1(R4)
         ST    R4,DMLINE
         LA    R5,DOMAINZ+158
         LA    R6,REASONS
         LA    R3,11
         LA    R4,RMCASRC
SWAPLOOP EQU   *
         L     R2,0(R4)
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   48(22,R5),0(R6)
         MVC   72(4,R5),DECWORK4
         LA    R6,22(R6)
         LA    R4,4(R4)
         LA    R5,79(R5)
         BCT   R3,SWAPLOOP
         SH    R5,=H'79'        BACKUP OVER LAMONT CRANSTON
         MVC   48(22,R5),0(R6)
         LH    R2,RMCAINUS
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   72(4,R5),DECWORK4
         LA    R5,79(R5)
         LA    R6,22(R6)
         MVC   48(22,R5),0(R6)
         LH    R2,RMCACIUS
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   72(4,R5),DECWORK4
         LA    R5,079(R5)
         MVC   48(28,R5),RCTTITL
         LA    R5,79(R5)
         MVC   48(28,R5),RCDTITL
         LA    R5,79(R5)
         XC    @LOW(16),@LOW
         SR    R2,R2
RCTLOOP  EQU   *
         LH    R2,RCCUICTL
         ST    R2,@LOW
         LH    R2,RCCUICTH
         ST    R2,@HI
         LH    R2,RCVUICA
         ST    R2,@AV
         L     R2,RCVUICC
         ST    R2,@CT
         BAL   R15,XLINE
         MVC   48(8,R5),=CL8'U I C'
*
*
*
*
*
         LA    R5,79(R5)
         XC    @LOW(16),@LOW
         LH    R2,RCCCPUTL
         SRL   R2,4
         ST    R2,@LOW
         LH    R2,RCCCPUTH
         SRL   R2,4
         ST    R2,@HI
*
**
***
**
*
         LH    R2,RCVCPUA
         SRL   R2,4
         ST    R2,@AV
         L     R2,RCVCPUC
         ST    R2,@CT
         BAL   R15,XLINE
         MVC   48(8,R5),=CL8'C P U'
         LA    R5,79(R5)
         XC    @LOW(16),@LOW
         LH    R2,RCCASMTL
         ST    R2,@LOW
         LH    R2,RCCASMTH
         ST    R2,@HI
         LH    R2,RCVASMQA
         ST    R2,@AV
         L     R2,RCVASMQ
         ST    R2,@CT
         BAL   R15,XLINE
         MVC   48(8,R5),=CL8'A S M Q'
         LA    R5,79(R5)
         XC    @LOW(16),@LOW
         LH    R2,RCCPTRTL
         ST    R2,@LOW
         LH    R2,RCCPTRTH
         ST    R2,@HI
         LH    R2,RCVPTR
         ST    R2,@AV
         L     R2,RCVBPTCT
         ST    R2,@CT
         BAL   R15,XLINE
         MVC   48(8,R5),=CL8'P A G E'
         LA    R5,79(R5)
         LH    R2,RCCTOTUT
         ST    R2,@LOW
         LH    R2,RCCLCHUT
         ST    R2,@HI
         LH    R2,RCCLCHRR
         ST    R2,@AV
         LH    R2,RCVTOTDF
         ST    R2,@CT
         BAL   R15,XLINE
         MVC   48(8,R5),=CL8'L C H '
*
*        MVC   0(29,R5),ERVTITL
*        L     R2,RMPTCPU
*        ST    R2,DECWORK1
*        BAL   R14,@DECIMAL
*        MVC   4(3,R5),DECWORK4+1
*        L     R2,RMPTIOC
*        ST    R2,DECWORK1
*        BAL   R14,@DECIMAL
*        MVC   12(3,R5),DECWORK4+1
*        L     R2,RMPTERV
*        ST    R2,DECWORK1
*        BAL   R14,@DECIMAL
*        MVC   20(4,R5),DECWORK4
         CLI   @MODE,X'7F' .       IS IT FLASH MODE?
         BNE   SRMPUT1 .           IF NOT, GO TO R/W
SRMPUT0  EQU   *
         MVI   @MODE,X'7F'
         OI    IOCBFLGD,IOCBDYON
         OI    IOCBFLGC,IOCBCCLR+IOCBCRAO+IOCBCFRA
         TRMIO IOCB,WRITE,FORMAT=SCREEN,IMAGE=DOMAINZ
         PFKEY DEF=NO,ATTN=SRMEXIT,ENTER=RUNSRMS,                      X
               K9=RUNSRMS,K12=SRMEXIT,K3=STARTMD
         B     RUNSRMS
SRMPUT1  EQU   *
         MVI   @MODE,X'0F'
         NI    IOCBFLGD,255-IOCBDYON
         OI    IOCBFLGC,IOCBCCLR+IOCBCRAO+IOCBCFRA
         TRMIO IOCB,FORMAT=SCREEN,IMAGE=DOMAINZ
         PFKEY DEF=DCMSOVER,ATTN=SRMEXIT,ENTER=RUNSRMS,                X
               K9=DCMSOVER,K12=SRMEXIT,K3=DOFLASH
         EJECT
DMNLINE  EQU   *
         USING DMDT,R6
         SR    R2,R2
         IC    R2,DMDTNO
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   0(3,R5),DECWORK4+1
         IC    R2,DMDTLO
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   4(3,R5),DECWORK4+1
         IC    R2,DMDTHI
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   08(3,R5),DECWORK4+1
         IC    R2,DMDTWT
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   12(3,R5),DECWORK4+1
         LH    R2,DMDTMPLT
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   16(3,R5),DECWORK4+1
         LH    R2,DMDTRUA
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   20(3,R5),DECWORK4+1
         LH    R2,DMDTGOOU
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   24(3,R5),DECWORK4+1
         LH    R2,DMDTCMPL
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   28(3,R5),DECWORK4+1
         LH    R2,DMDTOUTU
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   32(3,R5),DECWORK4+1
         LH    R2,DMDTINCU
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   36(3,R5),DECWORK4+1
         MVC   DECWORK4(4),=CL4'   0'
         IC    R1,DMDTWT        CURRENT WEIGHT
         MH    R1,DMDTRUA        TIMES
         SR    R0,R0             READY USER AVG
         LH    R2,DMDTMPLT       DIVIDED BY
         LTR   R2,R2
         BZ    CCINODR
         DR    R0,R2             MPL TARGET
         ST    R1,DECWORK1       EQUALS
         BAL   R14,@DECIMAL      CONTENTION INDEX
CCINODR  MVC   40(3,R5),DECWORK4+1
         MVI   45(R5),C'×'
         BR    R15
         EJECT
XLINE    EQU   *
         L     R2,@LOW
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   56(4,R5),DECWORK4
         L     R2,@HI
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   61(4,R5),DECWORK4
         L     R2,@AV
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   66(4,R5),DECWORK4
         L     R2,@CT
         ST    R2,DECWORK1
         BAL   R14,@DECIMAL
         MVC   72(4,R5),DECWORK4
         BR    R15
         EJECT
IPSMSG   DC    C' CURRENT IPS =  '
INITINT  DC  F'500'      .      FIVE SECOND INTERVAL
SCREEN   SFMT
DOMAINZ  FIELD 79
LINE2    FIELD 79
LINE3    FIELD 79
LINE4    FIELD 79
LINE5    FIELD 79
LINE6    FIELD 79
LINE7    FIELD 79
LINE8    FIELD 79
LINE9    FIELD 79
LINE10   FIELD 79
LINE11   FIELD 79
LINE12   FIELD 79
LINE13   FIELD 79
LINE14   FIELD 79
LINE15   FIELD 79
LINE16   FIELD 79
LINE17   FIELD 79
LINE18   FIELD 79
LINE19   FIELD 79
LINE20   FIELD 79
LINE21   FIELD 79
LINE22   FIELD 79
LINE23   FIELD 79,INTEN=LO,CURSOR=AFTER
         SFEND
PAGETITL DS    0CL79
         DC    CL40'DOMAIN TABLE (DMDT)       NNN DOMAINS   '
         DC    CL40'     ×  SWAP ANALYSIS DATA    (RMCA)    '
DMNTITL  DS    0CL79
         DC    CL40'DMN MIN MAX WT MPLT RUA GOU CMP OUT INC '
         DC    CL40'CCI  ×  SWAP REASONS          COUNTS  '
ERVTITL  DS    0CL79
         DC    CL79' CPU=NNN I/O=NNN ENQ=NNN   '
RFCTITL  DS    0CL79
         DC    CL79' CPU = NN.N I/O = NN.N MSO = NN.N '
RCDTITL  DS    0CL30
         DC    CL30'VARIABLE LOW HIGH AVG  COUNT  '
RCTTITL  DS    0CL30
         DC    CL30'RESOURCE CONTROL TABLE (RCT) '
REASONS  DS    0C
         DC    CL22'TERMINAL INPUT    (01)'
         DC    CL22'TERMINAL OUTPUT   (02)'
         DC    CL22'LONG WAIT         (03)'
         DC    CL22'AUX STG SHORTAGE  (04)'
         DC    CL22'REAL STG SHORTAGE (05)'
         DC    CL22'DETECTED WAIT     (06)'
         DC    CL22'REQUESTED         (07)'
         DC    CL22'CAP: ENQ EXCHANGE (08)'
         DC    CL22'CAP: WLM EXCHANGE (09)'
         DC    CL22'CAP: UNILATERAL   (0A)'
         DC    CL22'ONLY THE SHADOW KNOWS '
         DC    CL22'IN CORE USERS         '
         DC    CL22'OUTREADY USERS        '
SRMEXIT  DCMSTOP RC=(15)
         DSGEN  SCREEN
DMLINE   DS    F .
@LOW      DC   F'0'
@HI       DC   F'0'
@AV       DC   F'0'
@CT       DC   F'0'
@MODE    DS   X .
STATLINE DS    5CL79
         EJECT
         CVT   DSECT=YES
         EJECT
RMCNS    RMCNS
         EJECT
DMDT     IRADMDT
         EJECT
WAMT     IRAWAMT
         EJECT
WMST     IRAWMST
         END
