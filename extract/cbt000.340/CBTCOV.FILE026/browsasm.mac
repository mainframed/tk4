%---------------------- Browse Assembly Listing ------------------------
%COMMAND ===>_ZCMD
+
+    Module Name          %===>_MEMBER  +
+
+    Prefix               %===>_BPREFIX          + blank = TSO Prefix
+
+
+
+
)INIT
 .CURSOR = MEMBER
 &BPREFIX = TRANS (&BPREFIX ' ',AJES3L *,*)
)PROC
    VER (&MEMBER,NAME)
    IF (&BPREFIX = &Z)
        &BPREFIX = &ZPREFIX
    VER (&BPREFIX,DSNAME)
 &SEL =  'CMD(%BROWSASM)'
)END
