)ATTR
 \ TYPE(OUTPUT) INTENS(LOW)
 ! TYPE(INPUT) PAD('_') CAPS(ON)
)BODY EXPAND(||) WIDTH(&WIDTH)
%|-|  SYS1.DUMPnn Data Sets  |-|
%Command ===>_ZCMD| |%Scroll ===>_Z   +
%| |&NODENAME MVS/&IPCOS
+Primary commands: SHORT; LONG; ORDER DATETIME×CPU×TITLE; REFRESH
+Line Commands:    S = Select; I = Info; M = Migrate; E = Empty
+|-|
%  Date     Time  CPU ID OS   Title
)MODEL
!Z\DATE    \TIME +\Z \ID\Z   \TITLE
&MODEL2
)INIT

 .ZVARS = '(IPCSCROL SELECT CPU DUMPOS)'
 .HELP  = TPCSDUMP

 &SELECT = ''

)PROC

 &VERB     = TRUNC(&ZCMD,' ')
 &OPER     = .TRAIL
 IF  (&VERB ^= DEBUG,NODEBUG)
     &VERB     = TRANS(TRUNC(&VERB,1) S,SHORT L,LONG O,ORDER R,REFRESH *
 IF  (&VERB = ORDER)
     &OPER     = TRANS(TRUNC(&OPER,1) D,DATETIME C,CPU T,TITLE '',DATETI

 VER (&VERB,LIST,SHORT,LONG,ORDER,REFRESH,DEBUG,NODEBUG)
 IF  (&VERB = ORDER)
     VER (&OPER,LIST,DATETIME,CPU,TITLE)
 VER (&SELECT,LIST,S,I,M,E)
 IF  (&ZTDSELS ^= 0000,0001)  VER (&SELECT,LIST,I,M,E,MSG=IPC004)

 VPUT (CPU DATE DATETIME DEVTYPE DSNAME DUMPOS FILE IBMPMR ID JOB NODE O
       PRDUMPT QUAL SERIAL SOURCE SPR SRABD SRRC SRCID SRLM SRCSECT SRFR
       SREGPSW SRPGM SRLVL SRSC SRRRL TIME TITLE TITLE2 TYPE VOLUME)
 VPUT (IPCSCROL) PROFILE

)END
