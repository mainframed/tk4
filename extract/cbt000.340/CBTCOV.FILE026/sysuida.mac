)ATTR DEFAULT(%+_)
       %   TYPE(TEXT) INTENS(HIGH)
       +   TYPE(TEXT) INTENS(LOW) SKIP(ON)
       _   TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT)
)BODY EXPAND(||)
%-|-|- User Logon ID Dialog -|-|-
%COMMAND ===>_ZCMD
+
+Select the ACF2 LogonID Data Base to Search:
+
+
+
+
%         LogonIDs  ==>_Z   + (&SYSUIDT)
%
%         Center    ==>_Z   + (&SYSUIDC)
+
+
+
+
+
+
+
+
+
+
+
)INIT
 .ZVARS = '(UIDTYPE,UIDCTR)'
 .CURSOR = UIDTYPE
 &SYSUIDD = 'mm/dd/yy'
 &SYSUIDT = 'TSO'
 &SYSUIDC = '&$RCENTER'
 /* User Logon Data Bases are current as of &SYSUIDD. */
 /* Now Clists SYSUID and SYSUID use TBSTATS to set UIDDATE */
 IF (&UIDCTR = &Z) &UIDCTR = &$RCENTER
 IF (&UIDTYPE= &Z) &UIDTYPE= TSO
 IF (&$RCENTER = SWCC,SWC2)
   &SYSUIDD = '08/21/87'
   &SYSUIDC = 'SWCC or SWC2'
   &SYSUIDT = 'TSO or ALL'
 IF (&$RCENTER = CGAD)
   &SYSUIDD = '12/18/86'
   &SYSUIDC = 'CGAD, SWCC, or SWC2'
   &SYSUIDT = 'TSO or ALL'
 IF (&$RCENTER = CRCS,MCC)
   &SYSUIDD = '06/16/87'
   &SYSUIDC = 'CRCS or MCC'
   &SYSUIDT = 'TSO or ALL'
 IF (&$RCENTER = WCC1,WCC2)
   &SYSUIDD = 'mm/dd/yy'
   &SYSUIDC = 'WCC1 or WCC2'
   &SYSUIDT = 'TSO'
)PROC
 IF (&$RCENTER = SWCC,SWC2)
   VER (&UIDTYPE,NB,LIST,TSO,ALL)
   VER (&UIDCTR,NB,LIST,SWCC,SWC2)
 IF (&$RCENTER = CGAD)
   VER (&UIDTYPE,NB,LIST,TSO,ALL)
   VER (&UIDCTR,NB,LIST,SWCC,SWC2,CGAD)
 IF (&$RCENTER = CRCS,MCC)
   VER (&UIDTYPE,NB,LIST,TSO,ALL)
   VER (&UIDCTR,NB,LIST,CRCS,MCC)
 IF (&$RCENTER = WCC1,WCC2)
   VER (&UIDTYPE,NB,LIST,TSO)
   VER (&UIDCTR,NB,LIST,WCC1,WCC2)
)END
