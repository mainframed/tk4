)ATTR
  \ TYPE(OUTPUT) INTENS(LOW) PAD(' ') JUST(LEFT)
  ! TYPE(INPUT) INTENS(HIGH) PAD('_') CAPS(ON)

)BODY EXPAND(||) WIDTH(&WIDTH)
%|-|  IPCS - Migrated Dumps  |-|
%Command ===>_ZCMD| |%Scroll ===>_Z   +
%| |&NODENAME MVS/&IPCOS
+Primary commands: SHORT; LONG; REFRESH; DATE date; TYPE XA×370; OWNER o
+Line commands:    S = Select; I = Info; D = Delete
+|-|
%  Date     Time  CPU ID OS   Title
)MODEL ROWS(SCAN)
!Z\DATE    \TIME +\Z \ID\Z   \TITLE
&MODEL2
)INIT

 .ZVARS = '(IPCSCROL SEL CPU DUMPOS)'
 .HELP  = TPCLOG

 &SEL      = ''
 &TYPE  = DUMP

)PROC

 &VERB = TRANS(TRUNC(&ZCMD,' ')
               S,SHORT L,LONG R,REFRESH D,DATE T,TYPE O,OWNER *,*)
 &OPER = .TRAIL
 IF  (&VERB     = TYPE)    &OPER  = TRANS(TRUNC(&OPER,1) X,XA 3,370 *,*)

 VER (&VERB,LIST,SHORT,LONG,REFRESH,DATE,TYPE,OWNER,MAIN,DEBUG,NODEBUG)
 IF  (&VERB     = TYPE)       VER (&OPER,NONBLANK,LIST,XA,370)
 VER (&SEL,LIST,S,D,I)
 IF  (&ZTDSELS ^= 0000,0001)  VER (&SEL ,LIST,D,I,MSG=IPC004)

 &CCMD = '&VERB &OPER'

 VPUT (CPU DATE DATETIME DEVTYPE DSNAME DUMPOS FILE IBMPMR ID JOB NODE O
       QUAL SERIAL SOURCE SPR TIME TITLE TITLE2 TYPE VOLUME)
 VPUT (IPCSCROL) PROFILE

)END
