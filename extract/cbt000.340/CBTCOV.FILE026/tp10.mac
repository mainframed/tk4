)ATTR DEFAULT(%+_)
       ×   TYPE(INPUT) INTENS(HIGH) CAPS(OFF) JUST(LEFT)
)BODY
+------------% Rockwell ISC Technical Proposal Entry Dialog +-----------
%Command ===>_ZCMD
+
+Enter Project Cost Catagory:
+
+    $0 - $50,000         %===>_Z+
+    $50,001 - $99,999    %===>_Z+
+    $100,000 - $999,999  %===>_Z+
+    More than $1,000,000 %===>_Z+
+
+    Enter Y to select cost catagory
+
+  Continue%===>_CONT+ Y or N
)INIT
 &CONT = &Z
 .ZVARS = '(TP10C1 TP10C2 TP10C3 TP10C4)'
)PROC
 IF (&TP10C1^= &Z)
    VER (&TP10C2,LIST,&Z)
    VER (&TP10C3,LIST,&Z)
    VER (&TP10C4,LIST,&Z)
 IF (&TP10C2^= &Z)
    VER (&TP10C1,LIST,&Z)
    VER (&TP10C3,LIST,&Z)
    VER (&TP10C4,LIST,&Z)
 IF (&TP10C3^= &Z)
    VER (&TP10C1,LIST,&Z)
    VER (&TP10C2,LIST,&Z)
    VER (&TP10C4,LIST,&Z)
 IF (&TP10C4^= &Z)
    VER (&TP10C1,LIST,&Z)
    VER (&TP10C2,LIST,&Z)
    VER (&TP10C3,LIST,&Z)
 VER (&CONT,NONBLANK,LIST,Y,N)
)END
