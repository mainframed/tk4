%---------------------  AMDPRDMP - Print Dump EDIT SVC  ----------------
%Command ===>_ZCMD
%
+                           GTF SVC Trace Records
+
+Specify SVC numbers in decimal (0-255) - svcnum,svcnum,...:
+     %===>_GTFSVC1
+     %===>_GTFSVC2
+     %===>_GTFSVC3
+     %===>_GTFSVC4
+     %===>_GTFSVC5
+     %===>_GTFSVC6
+     %===>_GTFSVC7
+     %===>_GTFSVC8
+     %===>_GTFSVC9
+     %===>_GTFSVC10
+     %===>_GTFSVC11
+     %===>_GTFSVC12
+     %===>_GTFSVC13
+     %===>_GTFSVC14
+     %===>_GTFSVC15
+     %===>_GTFSVC16
)INIT
 .CURSOR = GTFSVC1
 &GTFSVC1 = ''
 &GTFSVC2 = ''
 &GTFSVC3 = ''
 &GTFSVC4 = ''
 &GTFSVC5 = ''
 &GTFSVC6 = ''
 &GTFSVC7 = ''
 &GTFSVC8 = ''
 &GTFSVC9 = ''
 &GTFSVC10 = ''
 &GTFSVC11 = ''
 &GTFSVC12 = ''
 &GTFSVC13 = ''
 &GTFSVC14 = ''
 &GTFSVC15 = ''
 &GTFSVC16 = ''
)PROC
 VER (&GTFSVC1,NONBLANK)
 IF  (&GTFSVC3 ^= '')  VER (&GTFSVC2,NONBLANK)
 IF  (&GTFSVC4 ^= '')  VER (&GTFSVC3,NONBLANK)
 IF  (&GTFSVC5 ^= '')  VER (&GTFSVC4,NONBLANK)
 IF  (&GTFSVC6 ^= '')  VER (&GTFSVC5,NONBLANK)
 IF  (&GTFSVC7 ^= '')  VER (&GTFSVC6,NONBLANK)
 IF  (&GTFSVC8 ^= '')  VER (&GTFSVC7,NONBLANK)
 IF  (&GTFSVC9 ^= '')  VER (&GTFSVC8,NONBLANK)
 IF  (&GTFSVC10 ^= '')  VER (&GTFSVC9,NONBLANK)
 IF  (&GTFSVC11 ^= '')  VER (&GTFSVC10,NONBLANK)
 IF  (&GTFSVC12 ^= '')  VER (&GTFSVC11,NONBLANK)
 IF  (&GTFSVC13 ^= '')  VER (&GTFSVC12,NONBLANK)
 IF  (&GTFSVC14 ^= '')  VER (&GTFSVC13,NONBLANK)
 IF  (&GTFSVC15 ^= '')  VER (&GTFSVC14,NONBLANK)
 IF  (&GTFSVC16 ^= '')  VER (&GTFSVC15,NONBLANK)
 VPUT (GTFSVC1  GTFSVC2  GTFSVC3  GTFSVC4  GTFSVC5  GTFSVC6  GTFSVC7  GT
       GTFSVC9  GTFSVC10 GTFSVC11 GTFSVC12 GTFSVC13 GTFSVC14 GTFSVC15 GT
)END
