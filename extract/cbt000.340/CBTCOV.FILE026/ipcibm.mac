%------------------------- IPCS Command Entry --------------------------
%COMMAND ===>_CMD

%----------------------- IPCS Commands+and Abbreviations%---------------
%ADDDSN,  +AD      ×%DROPMAP, +DROPM   ×%FINDMOD, +FMOD    ×%MODPROB, +M
%ADDPROB, +AP      ×%DROPSYM, +DROPS   ×%FINDUCB, +FINDU   ×%NOTE,    +N
%ASCBEXIT,+ASCBX   ×%DSPL3270,+DS      ×%INTEGER  +        ×%OPEN     +
%ASMCHECK,+ASMK    ×%ENQCHECK,+ENQK    ×%IOSCHECK,+IOSK    ×%RUNCHAIN,+R
%CLOSE    +        ×%EQUATE,  +EQU, EQ ×%LIST,    +L       ×%SCAN     +
%COMCHECK,+COMK    ×%EVALDEF  +        ×%LISTDSN, +LD      ×%SETDEF,  +S
%COMPARE, +COMP    ×%EVALDUMP +        ×%LISTDUMP,+LDMP    ×%STATUS,  +S
%COPYDUMP +        ×%EVALMAP  +        ×%LISTMAP, +LMAP    ×%SUMMARY, +S
%DELDSN,  +DD      ×%EVALSYM  +        ×%LISTPROB,+LP      ×%TCBEXIT, +T
%DELPROB, +DP      ×%EVALUATE,+EVAL    ×%LISTSYM, +LSYM    ×%TSO      +
%DROPDUMP,+DROPD   ×%FIND,    +F       ×%MODDSN,  +MD      ×%VERBEXIT,+V
+
+
+Note: quote marks must be doubled if option BROWSE or PRINT is in effec
)PROC

 VER (&CMD,NONBLANK)

 &ZSEL ='PGM(BLSGSCMD) PARM(&PREFIX &CMD &SUFFIX)'

)END
