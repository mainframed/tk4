%EDIT --- DDNAME &DDNAME @-@
%COMMAND ===>?ZCMD                                             %SCROLL%=
%
+Current Profile%===>?CNAFPROF+  Macro%===>?CNAFMACR+
%
+%S+- Edit Dataset/Member  %L+-Edit entire PDS (Member List)  %P+- PDS c
%
%Sel   Datasets allocated to &DDNAME                             %Volume
+---   -----------------------------------------------------------------
)MODEL
+#Z+  !Z                                                         !TABLEV
)INIT
  .ZVARS = '(LSEL TABLEDSN)'
  &AMT = &ZSCML
  &ZTDMARK = '********************** END OF DSNAME LIST FOR &DDNAME +
              ***************************'
  &ZTDMARK = TRUNC (&ZTDMARK,79)
  &ZTDMSG = DTSM040
  &UMSG40 = &Z
  &LMSG40 = &Z
)PROC
 IF (&ZCMD ^= &Z) .MSG=ZCMD001
 IF (&ZTDSELS ^= 0)
    VER (&LSEL,LIST,S,L,P)
    IF (&LSEL = P)
       &TEMP = TRUNC (&TABLEDSN,'(')
       &TRAIL = .TRAIL
       IF (&TRAIL ^= &Z)
          &TEMP = '&TEMP'''
       &PDSCMD = 'PGM(PDS) PARM(PDS &TEMP VOL(&TABLEVOL) MEMLIST :)'
)END
