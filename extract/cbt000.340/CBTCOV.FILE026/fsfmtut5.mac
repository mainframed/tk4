)ATTR DEFAULT(×+_)
 ` TYPE(TEXT)   INTENS(HIGH) COLOR(YELLOW)
 ~ TYPE(TEXT)   INTENS(HIGH) COLOR(RED) HILITE(BLINK)
 | TYPE(TEXT)   INTENS(HIGH)
)BODY
|TUTORIAL ---------- Full-Screen FIND MEMBER in System Library ---------
|OPTION  ===>_ZCMD
+
`      /** **  FSFMUSER clist: {Continued}  DataSet Selection  ** **/
+{Continued IF stmt:}
+ELSE IF &&STR(&&PARM) = &&STR(LPA/LINK) THEN DO /* FSFMLPA allocates LP
+  %FSFMLPA  CENTER(&&CENTER) MVS(&&MVS) CPU(&&CPU) ALLOCDD(&&ALLOCDD.)
+  %FSFMLINK CENTER(&&CENTER) MVS(&&MVS) CPU(&&CPU) ALLOCDD(USERTMP1)
+  $C CONCAT (&&ALLOCDD. USERTMP1)
+  SET &&ALLOCATE = DONE
+END
+
+ELSE IF &&STR(&&PARM) = &&STR(CPU DATA) THEN DO
+  IF     &&CPU = W THEN SET &&DS = &&STR('SYS1.X3090.DATA')
+  ELSE   &&CPU = Q THEN SET &&DS = &&STR('SYS1.X3033.DATA')
+END
+
+ELSE IF &&STR(&&PARM) = &&STR(MVS LINK) THEN DO
+  IF     &&MVS = XA  THEN SET &&DS = &&STR('SYS1.XA','SYS1.LINKLIB')
+  ELSE   &&MVS = 370 THEN SET &&DS = &&STR('SYS1.LINKLIB')
+END
`/* Now complete as on the first screen of sample FSFMUSER, ie ALLOC FI.
`ALSO see FSFMLPA, FSFMLINK, FSFMPROC, FSFMDRDS clists in ATSO.SYS.CLIST
+
)PROC
 .RESP = END
)END
