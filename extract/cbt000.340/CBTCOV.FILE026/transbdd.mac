+--------------------% Transmit Datasets Using MVS/BDT +----------------
%Command ===>_ZCMD
+
+  Enter Dataset Names to be Processed:   (no quotes)
+
%===>_BDTDSN0
%===>_BDTDSN1
%===>_BDTDSN2
%===>_BDTDSN3
%===>_BDTDSN4
%===>_BDTDSN5
%===>_BDTDSN6
%===>_BDTDSN7
%===>_BDTDSN8
%===>_BDTDSN9
%===>_BDTDSNA
%===>_BDTDSNB
%===>_BDTDSNC
%===>_BDTDSND
)INIT
 .CURSOR = BDTDSN0
)PROC
 VER (&BDTDSN0,DSNAME)
 VER (&BDTDSN1,DSNAME)
 VER (&BDTDSN2,DSNAME)
 VER (&BDTDSN3,DSNAME)
 VER (&BDTDSN4,DSNAME)
 VER (&BDTDSN5,DSNAME)
 VER (&BDTDSN6,DSNAME)
 VER (&BDTDSN7,DSNAME)
 VER (&BDTDSN8,DSNAME)
 VER (&BDTDSN9,DSNAME)
 VER (&BDTDSNA,DSNAME)
 VER (&BDTDSNB,DSNAME)
 VER (&BDTDSNC,DSNAME)
 VER (&BDTDSND,DSNAME)
)END
