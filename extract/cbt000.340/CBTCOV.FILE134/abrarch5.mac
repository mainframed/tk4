)ATTR
  +  TYPE(TEXT) INTENS(LOW) SKIP(ON)
  %  TYPE(TEXT) INTENS(HIGH) SKIP(ON)
  _  TYPE(INPUT) INTENS(HIGH)
)BODY
%---------------  DELETE DATASETS FROM THE ARCHIVE  --------------------
% COMMAND ===>_ZCMD
%
+   DATA SET NAME  %===>_ADSN                                        +
%
+   DATA SET GROUP %===>_ADSG                                        +
%
)INIT
  .HELP = TABRARC7
)PROC
  IF (&ADSN = ' ')
    VER (&ADSG,NONBLANK,MSG=ABR002F)
  IF (&ADSN ^= ' ')
    VER (&ADSG,LIST,' ',MSG=ABR002G)
)END
