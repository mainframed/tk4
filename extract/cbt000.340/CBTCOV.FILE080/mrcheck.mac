         MACRO
&NAME    MRCHECK &OPFL
         GBLA  &IEDQZE(64)
         GBLB  &IEDQZA(64)
         LCLB  &NOGO
.* VERSION DATED JUNE 28,1973
         SPACE
         AIF   (NOT &IEDQZA(9)).SCHK
         MNOTE 12,'***  IHB312 MUST FOLLOW STARTMH MACRO WITH TSOMH=NO'
         AGO   .NOMORE
.SCHK    AIF   (&IEDQZA(1) OR &IEDQZA(5)).TCHK
         MNOTE 12,'*** IHB070  SEQUENCE ERROR-MUST FOLLOW INMSG IF SPEC*
               IFIED'
&NOGO    SETB  1
.TCHK    AIF   ('&OPFL' EQ '').VCON
.OPERR   MNOTE 12,'***  IHB300  OPTION FIELD OPERAND INVALID AS SPECIFI*
               ED'
&NOGO    SETB  1
.VCON    AIF   (&NOGO).NOMORE
&IEDQZA(45)  SETB  1
         IEDQVCON  (43),(IEDQB7)
         DC    AL1(&IEDQZE(43)+1),AL1(4),AL1(0),AL1(0)
.NOMORE  SPACE 2
         MEND
