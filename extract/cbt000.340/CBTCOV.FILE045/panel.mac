_JCL1
_JCL2
_JCL3
_JCL4
_JCL5
_JCL6
_JCL7
_JCL8
@/-/
)INIT
 .ATTRCHAR('@') = 'TYPE(TEXT) COLOR(YELLOW) INTENS(LOW)'
 .ATTRCHAR('_') = 'TYPE(INPUT) COLOR(RED) INTENS(LOW)'
 IF (&TYPE = 'TEXT')
    .ATTRCHAR('_') = 'TYPE(TEXT) COLOR(BLUE) INTENS(LOW)'
 IF (&TYPE = 'INPUT')
    .ATTRCHAR('_') = 'TYPE(INPUT) COLOR(RED) INTENS(HIGH)'
