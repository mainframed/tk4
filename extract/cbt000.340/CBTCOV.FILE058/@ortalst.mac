//SORTOUT  DD  DSN=TST2SSG.A.DATA(HELDOFF),DISP=SHR
//SORTWK01 DD  UNIT=SYSDA,SPACE=(CYL,(1))
//SYSIN    DD  *
   SORT FIELDS=(29,8,CH,A,7,7,CH,A)
   RECORD TYPE=F,LENGTH=80
   END
