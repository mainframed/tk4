          BLGPARMS RPANLDS=(RPANEL2,RPANEL1),                  X
                WPANLDS=WPANELS,                               X
                DICTDS=DICTDS,                                 X
                RFTDS=RFTDS,                                   X
                IREPORT=(INST,10)
