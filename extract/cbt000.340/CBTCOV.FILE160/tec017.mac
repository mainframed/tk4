C      BIORHYTHMS PROGRAM
       INTEGER DAY1,YEAR1,YEAR2,SPACE/'    '/,IPLUS/'   +'/
       INTEGER KM(31),GRAPH(31,31)/961*'    '/,DASH/'----'/
     X,PLUS/'   +'/,MINUS/'   -'/,ZERO/'   0'/,PP/'   P'/
     X,SS/'   S'/,II/'   I'/,INUM(31),PDASH/'  P '/
       DIMENSION ID(12),IJ(12),IM(12),NAME(12)
C      ID HAS # OF DAYS IN EACH MONTH
       DATA ID/31,28,31,30,31,30,31,31,30,31,30,31/
C      NAME HAS NAMES OF THE MONTHS
       DATA NAME/'JAN,','FEB,','MAR,','APR,','MAY,','JUN,'
     X,'JUL,','AUG,','SEP,','OCT,','NOV,','DEC,'/
       IJ(1)=0
       IM(12)=0
       DO 5 I=2,12
C      COMPUTE # OF DAYS IN MONTHS OF YEAR1 AND YEAR2
C      IM HAS REMAINING DAYS OF YEAR1, IJ HAS DAYS IN YEAR2
       T=13-I
       IM(T)=IM(T+1)+ID(T+1)
    5  IJ(I)=IJ(I-1)+ID(I-1)
C      READ BIRTHDAY, BIORHYTHM CHART MONTH AND YEAR
   10  READ(5,*,END=99) MONTH1,DAY1,YEAR1,MONTH2,YEAR2
       IYEAR=0
       J=YEAR1+1
       K=YEAR2-1
       DO 50 I=J,K
C      ADD UP DAYS IN BETWEEN YEARS GIVEN, CHECK FOR LEAP YEARS
       IYEAR=IYEAR+365
       IF((I/4)*4.EQ.I) IYEAR=IYEAR+1
   50  CONTINUE
C      # OF DAYS IN GIVEN MONTH OF YEAR1
       IDAY=ID(MONTH1)-DAY1+1
C      # OF DAYS IN REMAINING MONTHS OF YEAR1
       IMONTH=IM(MONTH1)
       IF(MONTH2.GT.1) GO TO 60
C     CHECK FOR LEAP YEAR
       IF((YEAR1/4)*4.EQ.YEAR1) IMONTH=IMONTH+1
   60  WRITE(6,320) NAME(MONTH1),DAY1,YEAR1,NAME(MONTH2),YEAR2
C      # OF DAYS IN YEAR2
       JMONTH=IJ(MONTH2)+1
       IF(MONTH2.LE.2) GO TO 80
C      CHECK FOR LEAP YEAR
   70  IF((YEAR2/4)*4.EQ.YEAR2) JMONTH=JMONTH+1
C      TOTAL # OF DAYS
   80  ITOTAL=IDAY+IMONTH+IYEAR+JMONTH
C      FIND CURRENT POSITION OF EACH CYCLE
       L=ITOTAL-(ITOTAL/23)*23
       M=ITOTAL-(ITOTAL/28)*28
       N=ITOTAL-(ITOTAL/33)*33
       IM2=ID(MONTH2)
       K1=20
       K2=20
       K3=20
       DO 3 I=1,IM2
C      DOES THE FOLLOWING FOR EACH DAY IN THE MONTH
C      SIN FUNCTION GIVES THE CURVES FOR EACH CYCLE
C      M'S FALL BETWEEN 0 AND +15 OR -15
C      COMPUTE PHYSICAL---FASTEST CYCLE
       M1=15.1*SIN(.27318197*(I+L-1))
C      COMPUTE EMOTIONAL---MIDDLE CYCLE
       M2=15.1*SIN(.22439948*(I+M-1))
C      COMPUTE INTELLECTUAL---SLOWEST CYCLE
       M3=15.1*SIN(.19039955*(I+N-1))
C      J'S FALL BETWEEN 1 AND 31
       J1 =16-M1
       J2=16-M2
       J3=16-M3
       INUM(I)=0
C      CHECK FOR ZERO DAYS
       IF(J1.EQ.16) INUM(I)=INUM(I)+1
       IF(J2.EQ.16) INUM(I)=INUM(I)+2
       IF(J3.EQ.16) INUM(I)=INUM(I)+4
       IF(J1+K1.EQ.32) INUM(I)=INUM(I)+1
       IF(J3+K3.EQ.32) INUM(I)=INUM(I)+4
       K1=J1
       K2=J2
       K3=J3
       DO 1 J=1,IM2
C      MAKE ZERO LINE
       KM(J)=J
    1  GRAPH(16,J)=DASH
C      FIND POSITION ON CHART FOR 3 RHYTHMS
       GRAPH(J1,I)=PP
       GRAPH(J2,I)=SS
    3  GRAPH(J3,I)=II
       PLUS=IPLUS
       DO 11 I=1,IM2
C      WRITE OUT CHART
       IF(I.EQ.16) PLUS=ZERO
       IF(I.GT.16) PLUS=MINUS
       WRITE(6,500) PLUS,(GRAPH(I,J),J=1,IM2)
       DO 11 J=1,IM2
C      INITIALIZE CHART BACK TO BLANKS
   11  GRAPH(I,J)=SPACE
C      WRITE OUT DAYS OF MONTH
       WRITE(6,510) SPACE,(KM(I),I=1,IM2)
       DO 28 I=1,IM2
       IN0=INUM(I)
C      WRITE OUT ZERO DAYS
       GO TO (21,22,23,24,25,26,27),IN0
       GO TO 28
   21  WRITE(6,521) NAME(MONTH2),I
       GO TO 28
   22  WRITE(6,522) NAME(MONTH2),I
       GO TO 28
   23  WRITE(6,523) NAME(MONTH2),I
       GO TO 28
   24  WRITE(6,524) NAME(MONTH2),I
       GO TO 28
   25  WRITE(6,525) NAME(MONTH2),I
       GO TO 28
   26  WRITE(6,526) NAME(MONTH2),I
       GO TO 28
   27  WRITE(6,527) NAME(MONTH2),I
   28  CONTINUE
C      WRITE OUT EXPLANATION
       WRITE(6,999)
C      START AGAIN
       GO TO 10
  320  FORMAT('1DATE OF BIRTH: ',A4,1X,I2,',',I4//
     X' BIORHYTHM CHART FOR ',A4,1X,I4//)
  500  FORMAT(32A4)
  510  FORMAT(32I4)
  521  FORMAT('0  *WARNING*   ',A4,I3,' IS A ZERO DAY FOR THE ',
     X'PHYSICAL CYCLE.')
  522  FORMAT('0  *WARNING*   ',A4,I3,' IS A ZERO DAY FOR THE ',
     X'SENSITIVITY CYCLE.')
  523  FORMAT('0 **WARNING**  ',A4,I3,' IS A DOUBLE ZERO DAY ',
     X'FOR THE PHYSICAL AND SENSITIVITY CYCLES.')
  524  FORMAT('0  *WARNING*   ',A4,I3,' IS A ZERO DAY FOR THE ',
     X'INTELLECTUAL CYCLE.')
  525  FORMAT('0 **WARNING**  ',A4,I3,' IS A DOUBLE ZERO DAY ',
     X'FOR THE PHYSICAL AND INTELLECTUAL CYCLES.')
  526  FORMAT('0 **WARNING**  ',A4,I3,' IS A DOUBLE ZERO DAY ',
     X'FOR THE SENSITIVITY AND INTELLECTUAL CYCLES.')
  527  FORMAT('0***WARNING*** ',A4,I3,' IS A TRIPLE ZERO DAY ',
     X'FOR THE PHYSICAL, SENSITIVITY, AND INTELLECTUAL CYCLES.')
  999  FORMAT('0  *EXPLANATION:  3 IMPORTANT BIOLOGICAL',
     X ' RHYTHMS BEGIN THE MOMENT OF BIRTH AND RUN PUNCTUALLY',
     X ' EVER AFTER. THERE ARE PHYSICAL(23-DAY),'/'    EMOTIONAL',
     X '(28-DAY), AND INTELLECTUAL(33-DAY) RHYTHMS. THE ',
     X 'FIRST HALF OF EACH CYCLE SIGNALS STRENGTH, THE ',
     X 'SECOND HALF WEAKNESS'/'    OR A PERIOD OF RECHARGING.',
     X ' FROM HIGH GEAR TO LOW, OR LOW TO HIGH. PEOPLE ARE'/'   ',
     X ' MOST UNSTABLE, EASILY DISTRACTED, AND MORE PRONE TO ',
     X 'ERROR ON CRITICAL DAYS.')
   99  STOP
       END
