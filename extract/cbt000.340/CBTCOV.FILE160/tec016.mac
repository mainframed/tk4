 PROC 0 NAME(LIST)
  IF (&NAME. EQ STARTREK) GO TO LABEL 1111
  IF (&NAME. EQ REX) GO TO LABEL 1111
  IF (&NAME. EQ CHESS) GO TO LABEL 1111
  IF (&NAME. EQ STREKV2) GO TO LABEL 1111
  IF (&NAME. EQ NOVATREK) GO TO LABEL 1113
  IF (&NAME. EQ QUBIC) GO TO LABEL 1110
  IF (&NAME. EQ ELIZA) GO TO LABEL 2220
  IF (&NAME. EQ FOOTBALL)GO TO LABEL 2221
  IF (&NAME. EQ BLAKJAK) GO TO LABEL 2222
  IF (&NAME. EQ COFFEE) GO TO LABEL 2222
  IF (&NAME. EQ ECONOMY) GO TO LABEL 2222
  IF (&NAME. EQ HORSES) GO TO LABEL 2222
  IF (&NAME. EQ PINO) GO TO LABEL 2222
  IF (&NAME. EQ TARGET) GO TO LABEL 2222
  IF (&NAME. EQ THIMK) GO TO LABEL 2222
  IF (&NAME. EQ LIST) GO TO LABEL 3334
  GO TO LABEL 3333
 LABEL 1113
  ASK 'DO YOU WANT ALLOCATE A NEW SAVEGAME.DATA DATASET?'
  SYSRC (EQ 4) GO TO LABEL 1114
  FREE F(FT21F001)
  ALLOC F(FT21F001) DA(SAVEGAME.DATA) NEW SPACE(2 2) TRACKS
  GO TO LABEL 1111
 LABEL 1114
  ASK 'DO YOU WANT TO ALLOCATE SAVEGAME.DATA TO RETRIEVE GAME?'
  SYSRC (EQ 4) GO TO LABEL 1111
  FREE F(FT21F001)
  ALLOC F(FT21F001) DA(SAVEGAME.DATA)
  GO TO LABEL 1111
 LABEL 1110
  FREE ATTR(DF) F(FT32F001)
  ATTR DF INPUT
  ALLOC F(FT32F001) DA('TSO.PGMLIB(QUBDATA)') USING(DF)
  FREE ATTR(DF)
 LABEL 1111
  FREE F(FT05F001 FT06F001)
  ALLOC F(FT05F001) DA(*)
  ALLOC F(FT06F001) DA(*)
  CALL 'TSO.GAMELIB(&NAME.)'
  FREE F(FT05F001 FT06F001 FT32F001 FT21F001)
  FREE DA('TSO.GAMELIB' 'TSO.PGMLIB' SAVEGAME.DATA)
  GO TO LABEL 9999
 LABEL 2220
  FREE F(SYSPUNCH FT01F001 FT05F001 SYSIN SYSPRINT)
  ALLOC F(SYSPUNCH) DA(*)
  ALLOC F(FT01F001) DA(*)
  ALLOC F(FT05F001) DA('TSO.PGMLIB(ELIZAINP)')
  ALLOC F(SYSIN) DA(*)
  ALLOC F(SYSPRINT) DA(*)
  CALL 'TSO.GAMELIB(ELIZA)' 'R=32K'
  FREE F(SYSIN SYSPRINT FT01F001 SYSPUNCH FT05F001)
  FREE DA('TSO.GAMELIB' 'TSO.PGMLIB')
  GO TO LABEL 9999
 LABEL 2221
  FREE F(SYSIN)
  ALLOC F(SYSIN) DA(*)
 LABEL 2222
  FREE F(SYSPRINT)
  ALLOC F(SYSPRINT) DA(*)
  CALL 'TSO.GAMELIB(&NAME.)'
  FREE F(SYSIN SYSPRINT)
  FREE DA('TSO.GAMELIB' 'TSO.PGMLIB')
  GO TO LABEL 9999
 LABEL 3333
  TPRINT '&NAME. DOES NOT EXIST PRESENTLY AS A GAME'
 LABEL 3334
  TPRINT 'THE GAMES WE HAVE ARE : '
  TPRINT ' '
  TPRINT '   BLAKJAK   -   BLACK JACK                            '
  TPRINT '   CHESS     -   CHESS                                 '
  TPRINT '   COFFEE    -   # GUESSING, WHO BUYS COFFEE           '
  TPRINT '   ECONOMY   -   BUILD YOUR OWN ECONOMIC EMPIRE        '
  TPRINT '   ELIZA     -   COMPUTER PSYCHIATRIST                 '
  TPRINT '   FOOTBALL  -   FOOTBALL                              '
  TPRINT '   HORSES    -   SIMULATE HORSE RACING                 '
  TPRINT '   NOVATREK  -   STARTREK WITH NEW SAVE GAME AND TS OPTIONS '
  TPRINT '   PINO      -   PINOCHLE                              '
  TPRINT '   QUBIC     -   3D 4X4X4X4 TIC-TAC-TOE                '
  TPRINT '   REX       -   3D 3X3X3 TIC-TAC TOE                  '
  TPRINT '   STARTREK  -   COMMAND ENTERPRISE AGAIST KLINGONS    '
  TPRINT '   STREKV2   -   ANOTHER TYPE OF STARTREK              '
  TPRINT '   TARGET    -   ARTILLERY PRACTICE                    '
  TPRINT '   THIMK     -   NIM                                   '
  TPRINT ' '
  TPRINT 'GET ANY ONE OF THE GAMES BY "GAME NAME(ABOVE GAME NAME)"'
 LABEL 9999
 PEND
