)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
)BODY EXPAND(\\)
+-\-\-ÖPDS View log Command Selection+-\-\-
%OPTION  ===>_ZCMD
+
+
+Enter operands for View log
%===>_LOPER                                                            +
+
+Optional:
%   n      +- switch to on of 9 log tables (value 1 to 9)
%   RESET  +- Clear table before building a new table.
%   NODISPL+- after switching or resetting the table do not display it
           +  but return to previous function table display
)INIT
  IF (&ZCMD ^= &Z)
        &TR=TRUNC(&ZCMD,' ')
        &OPERS=.TRAIL
        &ZCMD = &Z
  &LOPER  = '&OPERS'
  .CURSOR = LOPER
 .HELP = P84PNK@
  IF(&LOPER  = &Z) &LOPER=&P84#VI1
  IF(&ZCMD ^= &Z)
    &X=TRUNC(&ZCMD,' ')
    &LOPER = .TRAIL
    &ZCMD = &Z
)PROC
 &P84#VI1=&LOPER
 &PDSZSEL = 'View   &LOPER'
)END
