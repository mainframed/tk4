)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
)BODY EXPAND(\\)
+-\-\-ÖPDS+Utility Selection Panel+-\-\-
%OPTION  ===>_ZCMD
+
%  Make selection of entry panel
+
    %Z+Invoke the Interactive ZAP Program

)INIT
)PROC
 &PDSVOLK = &Z
 &PDSUNIT = &Z
 IF (&PDSVOL ^= &Z)
    &PDSVOLK= 'VOL(&PDSVOL)'
    &PDSUNIT= 'UNIT(SYSALLDS)'
 &PDSZSEL=TRANS(TRUNC(&ZCMD,'.')
             Z,'CMD(ZAP$ ''&PDSDSN'' &PDSVOLK)'
            ZA,'CMD(ZAP$ ''&PDSDSN'' &PDSVOLK)'
           ZAP,'CMD(ZAP$ ''&PDSDSN'' &PDSVOLK)'
           ' ',' '
             *,'?')
 &ZTRAIL=.TRAIL
)END
