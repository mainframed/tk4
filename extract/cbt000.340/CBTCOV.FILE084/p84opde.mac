)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
)BODY EXPAND(\\)
+-\-\-ÖO.DEL    DELETE Subcommand+-\-\-
%OPTION  ===>|ZCMD
%
+Enter the member group name and any operands below for%DELETE:
%===>|LOPER

+
+Operands:%memgroup -+ (e.g., *; start:end; start:; first*pat; range*; p
+         % ALIAS   -+ To also delete associated members
+         % NOALIAS -+ To delete only the identified member
+
+Defaults:  memgroup, NOALIAS
)INIT
  &LOPER  = '&OPERS'
  .CURSOR = LOPER
  .HELP   = P84OPHH
  IF(&LOPER  = &Z) &LOPER=&P84#DE1
)PROC
 &P84#DE1=&LOPER
 &PDSZSEL='Delete  &LOPER'
)END
