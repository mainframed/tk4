)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
)BODY EXPAND(\\)
+-\-\-ÖO.PA PATTERN Subcommand+-\-\-
%OPTION ===>|ZCMD
+
+Enter any operands for%PATTERN:+
%===>|LOPER

+
+Operands: %  part1 part2+   (partial member names)
+
+Alternates:% part1
%             memstart:memend
%             memstart:
%             :memend
%             :
%             start*pat
%             start*
%             part1/part2
%             part1/
%             /part1
+
+
+Defaults:    If no specification, the operands last used for PATTERN
)INIT

  &LOPER = '&OPERS'
  .CURSOR = LOPER
  .HELP   = P84OPHH
  IF(&LOPER  = &Z) &LOPER=&P84#PA1
)PROC
 &P84#PA1=&LOPER
  &PDSZSEL='Pattern  &LOPER'
)END
