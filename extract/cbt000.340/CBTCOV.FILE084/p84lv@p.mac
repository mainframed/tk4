)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
)BODY EXPAND(\\)
+-\-\-ÖPDS O Subcommand Selection+-\-\-
%OPTION  ===>_ZCMD
+
 Choose one of the following:
+
    %LISTV+-LISTV command option prompt panel
    %1 +- Function selection list
    %2 +- Display or Table processing options (SORT, F, X)
    %3 +- Features and Short Hand commands
    %4 +- Set default selection list
+
)INIT
 .HELP = P84LA@PH
)PROC
 &PDSZSEL=TRANS(TRUNC(&ZCMD,'.')
         LISTV,'PANEL(P84LVLTV)'
            LV,'PANEL(P84LVLTV)'
             1,'PANEL(P84FNFN)'
             2,'PANEL(P84LVTB)'
             3,'PANEL(P84OPFE)'
             4,'PANEL(P84SET@)'
           ' ',' '
             *,'?')
 &ZTRAIL=.TRAIL
 &OPERS=.TRAIL
)END
