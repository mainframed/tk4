)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
)BODY EXPAND(\\)
+-\-\-Öo.vtoc  VTOC line command+-\-\-
%OPTION  ===>|ZCMD
%
+Enter any operands below for%VTOC  &PDSVVOL
%===>|LOPER

+Operands:+  AND1/OR1('KEYWORD' 'OPER' 'VALUE')  AND2/OR2(...  AND3/OR3(
             BREAK('BREAK-CHARS')
             CAT
             CHARS('CHARS-PER-LINE')
             CONTAINING('DSNAME-STRING')
             DSNLEN('LENGTH')
             ENDING('DSNAME-END')
             LEVEL('DSNAME-START')
             LIMIT('KEYWORD' 'OPER' 'VALUE')
             LINES('LINES-PER-PAGE')
             NOHEADING/HEADING('TEXT')
             NOPRINT/PRINT('PRINT-OP' ('PRINT-ITEM-LIST'))
             NOSORT/SORT('SORT-FIELDS')
             TRK/CYL/KB/MB
+
+Defaults:   SORT, PRINT, TRK
)INIT
  &LOPER  = '&OPERS'
  .CURSOR = LOPER
  .HELP   = P84LOHH
  IF(&LOPER  = &Z) &LOPER=&P84#VTO
  &LOPER=&P84#VTO
)PROC
 &P84#VTO=&LOPER
 &PDSZSEL='vtoc &PDSVVOL &LOPER'
)END
