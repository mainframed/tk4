)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    ÷ TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
)BODY EXPAND(\\)
+-\-\-÷PDS O.2    Subcommand Selection+-\-\-
%OPTION  ===>_ZCMD
+
 Choose one of the following:
    %1 +- Suspend ISPF (revert to line mode)
    %4 +- SUBLIST = (set a sublist equal to the members in the member li
    %5 +- Compare members in table for matching aliases
    %6 +- Memlist command for all members (equivalent to  MEMLIST : )
    %* +- Memlist command for group       (equivalent to  MEMLIST * )
    %7 +- Output copy of table to print or a data set
    %8 +- View the log
    %9 +- Swap to different table display
    %F +- Find data in log or member table
    %X +- Clear a portion of the log or member table
+
    %LA      +- Display allocation table
    %LV      +- Display volume table
    %LISTA   +- List allocation prompt
    %LISTV   +- List volume prompt
    %MEMLIST +- Display member list
    %SORT    +- Sort member list
    %FUNCTION+- Display function status and selection panel
    %QUIT    +- Terminate÷PDS+
)INIT
 .HELP = P84OP@PH
)PROC
 &PDSZSEL=TRUNC(&ZCMD,'.')
 IF (&PDSZSEL='*')
    &PDSZSEL=&ZCMD
 IF (&PDSZSEL^='*')
     &PDSZSEL=TRANS(&PDSZSEL
          LISTA,'PANEL(P84LALTA)'
          LISTV,'PANEL(P84LVLTV)'
              F,'PANEL(P84OPFT&PDSTCODE)'
              S,'PANEL(P84OPSO&PDSTCODE)'
           SORT,'PANEL(P84OPSO&PDSTCODE)'
             FU,'FUNCTION'
           FUNC,'FUNCTION'
       FUNCTION,'FUNCTION'
        MEMLIST,'&ZCMD'
             QU,'QUIT'
            QUI,'QUIT'
           QUIT,'QUIT'
           MEML,'&ZCMD'
              M,'MEMLIST'
              V,'VIEW'
             VI,'VIEW'
            VIE,'VIEW'
           VIEW,'VIEW'
              1,'1'
              4,'&ZCMD'
              5,'&ZCMD'
              6,'&ZCMD'
             65,'&ZCMD'
              7,'&ZCMD'
              8,'&ZCMD'
              9,'&ZCMD'
             ML,'&ZCMD'
             LA,'&ZCMD'
             LV,'&ZCMD'
              X,'PANEL(P84XP2)'
           ' ',' '
             *,'?')
 &ZTRAIL=.TRAIL
 &OPERS=.TRAIL
)END
