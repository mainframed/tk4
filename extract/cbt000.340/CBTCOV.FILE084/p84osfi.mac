)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
)BODY EXPAND(\\)
+-\-\-ÖO.F    FIND Subcommand+-\-\-
%OPTION  ===>|ZCMD
+Enter the search string and any operands for%FIND:
%===>|LOPER

+Operands:% 'string'  NUM / SNUM / NONUM / LBLOCK / LDUMP / BLOCK / DUMP
%                     CAPS/ASIS      FIRST/NOFIRST
%                     MAXIN(num)     MAXLEN(num)      MAXOUT(num)
%                     SKIPCOL(num)   SKIPREC(num)     MAXFIND(num)
+
+Defaults:  If string is null, previous string;
+           NUM or previous FIND/LIST/REPLACE format
)INIT
  &LOPER  = '&OPERS'
  .CURSOR = LOPER
  .HELP   = P84OPHH
  IF(&LOPER  = &Z) &LOPER=&P84#FI2
)PROC
 &P84#FI2=&LOPER
 &PDSZSEL='Find  &LOPER'
)END
