      SUBROUTINE GETIN (WORD1,WORD1X,WORD2,WORD2X,NULLOK)
C
C  GET A COMMAND FROM THE ADVENTURER.
C
C  WORD1 IS SET TO THE FIRST FIVE CHARACTERS OF THE FIRST WORD AND
C  WORD1X IS SET TO THE SECOND FIVE.  WORD2 AND WORD2X ARE USED IN
C  AN ANALAGOUS FASHION FOR THE SECOND WORD.  IF THERE IS NO SECOND
C  WORD, WORD2 IS SET TO ZERO.
C
C  IF NULLOK IS .TRUE. AND A BLANK LINE IS SUPPLIED, WORD1 IS SET TO
C  ZERO; OTHERWISE, THE USER MUST TYPE A NON-BLANK RESPONSE.
C
      IMPLICIT INTEGER (A-Z)
      LOGICAL NULLOK,BLKLIN
      COMMON /IOSCOM/ TTYI,TTYO,BLKLIN,DBFI,DBINIT,DBSAVE
      COMMON /INPCOM/ WRD1SZ,WRD1A1(12),DLM1A1,
     -                WRD2SZ,WRD2A1(12),DLM2A1,
     -                WRD3SZ,WRD3A1(12),DLM3A1,
     -                WRD4SZ,WRD4A1(12),DLM4A1
      DIMENSION LINE(80)
      DATA BLNK/' '/
C
      DO 10 I=1,12
      WRD1A1(I)=BLNK
      WRD2A1(I)=BLNK
      WRD3A1(I)=BLNK
      WRD4A1(I)=BLNK
10    CONTINUE
      DLM1A1=BLNK
      DLM2A1=BLNK
      DLM3A1=BLNK
      DLM4A1=BLNK
      WRD1SZ=0
      WRD2SZ=0
      WRD3SZ=0
      WRD4SZ=0
      WORD1=0
      WORD1X=0
      WORD2=0
      WORD2X=0
      IF(BLKLIN) WRITE(TTYO,11)
11    FORMAT (1X)
20    READ(TTYI,21) LINE
21    FORMAT (80A1)
C
C  CHECK FOR A NULL RESPONSE
C
      DO 30 IFRST=1,80
      IF (LINE(IFRST).NE.BLNK) GO TO 40
30    CONTINUE
      IF (.NOT. NULLOK) GO TO 20
      RETURN
40    CALL CVLTUC (LINE,80)
      CALL GET12 (LINE,IFRST,80,WRD1SZ,WRD1A1,DLM1A1)
      WORD1 = CODE2(WRD1A1(1))
      WORD1X = CODE2(WRD1A1(6))
      CALL GET12 (LINE,IFRST,80,WRD2SZ,WRD2A1,DLM2A1)
      WORD2 = CODE2(WRD2A1(1))
      WORD2X = CODE2(WRD2A1(6))
      CALL GET12 (LINE,IFRST,80,WRD3SZ,WRD3A1,DLM3A1)
      CALL GET12 (LINE,IFRST,80,WRD4SZ,WRD4A1,DLM4A1)
      RETURN
      END
