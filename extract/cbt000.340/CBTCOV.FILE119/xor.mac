      INTEGER FUNCTION XOR(A,B)
C
C  RETURNS THE LOGICAL EXCLUSIVE OR OF ITS INTEGER ARGUMENTS.
C
      IMPLICIT INTEGER(A-Z)
      LOGICAL AL,BL,CL,NAL,NBL
      EQUIVALENCE (AL,A1),(BL,B1),(CL,C1),(NAL,NA1),(NBL,NB1)
C
      A1=A
      B1=B
      NA1=-A1-1
      NB1=-B1-1
      CL=(AL.AND.NBL).OR.(NAL.AND.BL)
      XOR=C1
      RETURN
      END
