      SUBROUTINE DATIME(D,T)
C
C  RETURN THE CURRENT DATE AND TIME.
C
C  D IS SET TO THE JULIAN DATE
C  T IS SET TO THE NUMBER OF MINUTES PAST MIDNIGHT.
C
C AN ASSEMBLER SUBROUTINE IS USED TO OBTAIN THE JULIAN DATE AND
C TIME IN HUNDRETHS OF SECONDS.
C
      IMPLICIT INTEGER(A-Z)
C
      CALL GETDTM(DATE,TIME)
      D=DATE
      T=TIME/6000
      RETURN
      END
