      SUBROUTINE POOF
C
C  AS PART OF DATABASE INITIALIZATION, WE CALL POOF TO SET UP SOME DUMMY
C  PRIME-TIME SPECS, MAGIC WORDS, ETC.
C
      IMPLICIT INTEGER(A-Z)
      DIMENSION HNAME(20)
      COMMON /WIZCOM/ WKDAY,WKEND,HOLID,HBEGIN,HEND,HNAME,
     1SHORT,MAGIC,MAGNM,LATNCY,SAVED,SAVET,SETUP
C
      WKDAY=130944
C  ABOVE CONSTANT SETS PRIME-TIME ON WEEKDAYS AS 08:00 - 17:00
      WKEND=0
      HOLID=0
      HBEGIN=0
      HEND=-1
      SHORT=30
      MAGIC=CODE1('DWARF')
      MAGNM=11111
      LATNCY=90
      RETURN
      END
