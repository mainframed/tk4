PROC 0 DEBUG
GLOBAL DSN LVL VOLSER
CONTROL NOFLUSH END(QUAZ)
ERROR GOTO RESTART
   IF &DEBUG NE DEBUG THEN CONTROL NOLIST MSG PROMPT
   ELSE DO
      CONTROL LIST PROMPT CONLIST SYMLIST MSG
      READ &JUNK
   QUAZ
START: +
   CLRSCRN
WRITE                     INTERACTIVE PROBLEM CONTROL SYSTEM
WRITE                              IPCS CLIST MENU
WRITE
WRITE
WRITE         1 = FINDASID               BR = BROWSE IPCSBRWS
WRITE         2 = FINDFAIL              TSO = TSO COMMAND
WRITE         3 = FINDJOB                 X = RETURN TO PREVIOUS MENU
WRITE         4 = FINDMOD
WRITE         5 = LISTASID
WRITE         6 = LISTREGS
WRITE         7 = LISTSU
WRITE         8 = SETDEF
WRITE         9 = SLIPDUMP
WRITE        10 = WHERE
WRITE
WRITE
WRITENR SELECT OPTION ===>
READ &ANS
   CLRSCRN
IF &ANS = THEN GOTO START
IF &ANS = X  THEN GOTO ENDIT
IF &ANS = END  THEN GOTO ENDIT
IF &ANS = 1  THEN GOTO FINDASID
IF &ANS = 2  THEN GOTO FINDFAIL
IF &ANS = 3  THEN GOTO FINDJOB
IF &ANS = 4  THEN GOTO FINDMOD
IF &ANS = 5  THEN GOTO LISTASID
IF &ANS = 6  THEN GOTO LISTREGS
IF &ANS = 7  THEN GOTO LISTSU
IF &ANS = 8  THEN GOTO SETDEF0
IF &ANS = 9  THEN GOTO SLIPDUMP
IF &ANS = 10 THEN GOTO WHERE0
IF &ANS = BR THEN GOTO BROWSE
IF &ANS = IS THEN ISPF 2
IF &ANS = TSO THEN GOTO TSOCMD
GOTO RESNONO
BROWSE: +
   WRITENR ENTERING BROWSE OF IPCSBRWS.LIST
   ISPF CMD(%GENPRINT DSN(IPCSBRWS.LIST) DEOPT(D))
   GOTO RESTART
ENDIT: +
   EXIT
FINDASID: +
   %FINDASID
   GOTO RESTART
FINDFAIL: +
   %FINDFAIL
   GOTO RESTART
FINDJOB: +
   %FINDJOB
   GOTO RESTART
FINDMOD: +
   %FINDMOD
   GOTO RESTART
LISTASID: +
   %LISTASID
   GOTO RESTART
LISTREGS: +
   %LISTREGS
   GOTO RESTART
LISTSU: +
   %LISTSU
   GOTO RESTART
RESNONO: +
     WRITE &ANS PROBABLY A NON-SUPPORT FUNCTION TRY AGAIN
RESTART: +
   WRITE PRESS ENTER TO CONTINUE
   WRITENR ***
   READ &REPLY
   GOTO START
SETDEF0: +
   SETDEF
   WRITE
   WRITE ENTER SETDEF KEYWORDS TO CHANGE DEFAULTS OR ENTER TO RETURN   -
TO MENU
   WRITE
   WRITENR SETDEF:
      READ &S1 &S2 &S3 &S4 &S5 &S6 &S7 &S8
      IF &S1=  THEN GOTO START
      ELSE %SETDEF0 &S1 &S2 &S3 &S4 &S5 &S6 &S7 &S8
   CLRSCRN
   GOTO SETDEF0
SLIPDUMP: +
   %SLIPDUMP
   GOTO RESTART
TSOCMD: +
   WRITENR ENTER TSOCMD:
   READ &T0 &T1 &T2 &T3 &T4 &T5 &T6 &T7
   &T0 &T1 &T2 &T3 &T4 &T5 &T6 &T7
   GOTO START
WHERE0: +
   WRITE         1 = WHERE
   WRITE         2 = WHEREC
   WRITE         3 = WHEREM
   WRITE         4 = WHEREL
   WRITE         5 = WHEREP
   WRITE         6 = WHERES
   WRITE
   WRITE         X = RETURN TO PREVIOUS MENU
   WRITENR SELECT OPTION ===>
   READ &ANS
      CLRSCRN
   IF &ANS = THEN GOTO WHERE0
   IF &ANS = X  THEN GOTO START
   IF &ANS = 1  THEN GOTO WHERE
   IF &ANS = 2  THEN GOTO WHEREC
   IF &ANS = 3  THEN GOTO WHEREM
   IF &ANS = 4  THEN GOTO WHEREL
   IF &ANS = 5  THEN GOTO WHEREP
   IF &ANS = 6  THEN GOTO WHERES
      GOTO RESTART
   WHERE: +
      %WHERE
      GOTO RESTART
   WHEREC: +
      %WHEREC
      GOTO RESTART
   WHEREM: +
      %WHEREM
      GOTO RESTART
   WHEREL: +
      %WHEREL
      GOTO RESTART
   WHEREP: +
      %WHEREP
      GOTO RESTART
   WHERES: +
      %WHERES
      GOTO RESTART
