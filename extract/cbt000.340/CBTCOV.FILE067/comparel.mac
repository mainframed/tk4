PROC 0 DEBUG
IF &DEBUG = THEN CONTROL NOMSG
            ELSE CONTROL MSG LIST CONLIST SYMLIST PROMPT NOFLUSH
ISPEXEC VGET (DSNO DSNN MEMO MEMN REPORT SNAP )
IF &DEBUG = THEN CONTROL NOMSG
FREE F(WTO I1 I2 SYSPRINT SNAP )
IF &REPORT = T THEN DO
                    ALLOC F(SYSPRINT) DS(*)
                    IF &SNAP = Y THEN ALLOC F(SNAP) DS(*)
                    END
               ELSE DO
                    DELETE COMPAREL.REPORT
                    IF &SNAP = Y THEN ALLOC F(SNAP)                    -
DS(COMPAREL.REPORT) NEW SPA(30,30) T
                                 ELSE ALLOC F(SYSPRINT)                -
DS(COMPAREL.REPORT) NEW SPA(10,10) T
                    END
CONTROL MSG PROMPT
ALLOC F(I1) DS(&DSNO) SHR
ALLOC F(I2) DS(&DSNN) SHR
CONTROL MSG NOPROMPT
ALLOC F(WTO) DS(*)
IF &MEMO = THEN DO
                $ COMPAREL
                END
           ELSE DO
                $ COMPAREL '&MEMO,&MEMN'
                END
IF &REPORT = B THEN DO
                    %GENPRINT DSN(COMPAREL.REPORT)
                    END
CONTROL NOMSG NOPROMPT
FREE F(WTO I1 I2 SYSPRINT SNAP )
