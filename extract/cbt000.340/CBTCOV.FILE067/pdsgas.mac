PROC 0 DEBUG
IF &DEBUG = DEBUG THEN CONTROL LIST MSG NOFLUSH PROMPT SYMLIST CONLIST
                  ELSE CONTROL NOLIST NOMSG NOFLUSH NOPROMPT NOSYMLIST -
NOCONLIST
FREE F(SYSLIB SYSPRINT)
ISPEXEC VGET (GASDSN LOPT)
IF &LOPT = T THEN ALLOC F(SYSPRINT) DS(*)
             ELSE DO
                  DEL PDSGAS.REPORT
                  ALLOC F(SYSPRINT) DS(PDSGAS.REPORT) NEW SPA(1,1) T
                  END
ALLOC F(SYSLIB) DS(&GASDSN) SHR
$P PDSGAS 'ADD'
IF &LOPT = B THEN %GENPRINT DSN(PDSGAS.REPORT) DEOPT(D)
FREE F(SYSLIB SYSPRINT)
