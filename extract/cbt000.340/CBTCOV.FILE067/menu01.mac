PROC 0 DEBUG
GLOBAL DSN LVL VOLSER
CONTROL NOFLUSH END(QUAZ)
ERROR GOTO RESTART
   IF &DEBUG NE DEBUG THEN CONTROL NOLIST MSG PROMPT
   ELSE DO
      CONTROL LIST PROMPT CONLIST SYMLIST MSG
      READ &JUNK
   QUAZ
START: +
CLRSCRN
WRITE                     INTERACTIVE PROBLEM CONTROL SYSTEM
WRITE                              VERBEXIT COMMANDS
WRITE
WRITE
WRITE         1 = ASMDATA                BR = BROWSE IPCSBRWS
WRITE         2 = CVT                     V = VERBEXIT
WRITE         3 = GRSTRACE                X = RETURN TO PREVIOUS MENU
WRITE         4 = LOGDATA
WRITE         5 = MTRACE
WRITE         6 = QCB
WRITE         7 = RSM
WRITE         8 = SRM
WRITE         9 = TRACE
WRITE        10 = VTAMAP
WRITE        11 = JES3
WRITE        12 = SYMDUMP
WRITE
WRITENR SELECT OPTION ===>
READ &ANS
   CLRSCRN
IF &ANS = THEN GOTO START
IF &ANS = X  THEN GOTO ENDIT
IF &ANS = END  THEN GOTO ENDIT
IF &ANS = 1  THEN GOTO ASMDATA
IF &ANS = 2  THEN GOTO CVT
IF &ANS = 3  THEN GOTO GRSTRACE
IF &ANS = 4  THEN GOTO LOGDATA
IF &ANS = 5  THEN GOTO MTRACE
IF &ANS = 6  THEN GOTO QCB
IF &ANS = 7  THEN GOTO RSM
IF &ANS = 8  THEN GOTO SRM
IF &ANS = 9  THEN GOTO TRACE
IF &ANS = 10 THEN GOTO VTAMAP
IF &ANS = 11 THEN GOTO JES3
IF &ANS = 12 THEN GOTO SYMDUMP
IF &ANS = BR THEN GOTO BROWSE
IF &ANS = IS THEN ISPF 2
IF &ANS = V  THEN GOTO VBEXIT
GOTO RESNONO
ASMDATA: +
   %VRBXIT ASMDATA ILRFMT00
   GOTO RESTART
BROWSE: +
   WRITENR ENTERING BROWSE OF IPCSBRWS.LIST
   ISPF CMD(%GENPRINT DSN(IPCSBRWS.LIST) DEOPT(D))
   GOTO RESTART
CVT: +
   %VRBXIT CVT AMDPRCVT
   GOTO RESTART
ENDIT: +
   EXIT
GRSTRACE: +
   %VRBXIT GRSTRACE ISGDPDMP
   GOTO RESTART
JES3: +
   %JES3MNU
   GOTO RESTART
LOGDATA: +
   %VRBXIT LOGDATA IFCERFMT
   GOTO RESTART
MTRACE: +
   %VRBXIT MTRACE AMDPRMTR
   GOTO RESTART
QCB: +
   %VRBXIT QCB AMDPRQCB
   GOTO RESTART
RSM: +
   %VRBXIT RSM AMDPRRSM
   GOTO RESTART
SRM: +
   %VRBXIT SRM IRAPRSRM
   GOTO RESTART
SYMDUMP: +
   %SYMDUMP
   GOTO RESTART
RESNONO: +
     WRITE &ANS PROBABLY A NON-SUPPORT FUNCTION TRY AGAIN
RESTART: +
   WRITE PRESS ENTER TO CONTINUE
   WRITENR ***
   READ &JUNK
   GOTO START
TRACE: +
   %VRBXIT TRACE AMDPRTRC
   GOTO START
VTAMAP: +
   %VRBXIT VTAMAP ISTRAFD1
   GOTO START
VBEXIT: +
   %VEXIT
   GOTO RESTART
