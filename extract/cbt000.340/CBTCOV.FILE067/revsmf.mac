PROC 0
CONTROL NOMSG
ISPEXEC CONTROL SPLIT ENABLE
ISPEXEC DISPLAY PANEL(REVSMF)
   FREE FI(DOG)
ISPEXEC CONTROL DISPLAY LINE
IF &DSN= -
   THEN DO
        END
   ELSE DO
   ALLOC FI(DOG) DA('CBSI.HELP.LOADLIB') SHR
        IF &VOL= -
        THEN DO
ISPEXEC CONTROL DISPLAY LINE
        CMD REVIEW:DOG '&DSN'
        FREE FI(DOG)
        END
  ELSE DO
ISPEXEC CONTROL DISPLAY LINE
        CMD REVIEW:DOG '&DSN' VOL(&VOL)
        FREE FI(DOG)
        END
        END
