PROC 0
ISPEXEC CONTROL SPLIT ENABLE
ISPEXEC DISPLAY PANEL(RLSE)
IF &DSNAME= -
   THEN DO
   END
ELSE DO
        IF &VOLUME= -
        THEN DO
                RLSE &DSNAME
             END
        ELSE DO
               RLSE &DSNAME VOL(&VOLUME)
             END
     ISPEXEC CONTROL DISPLAY REFRESH
END
