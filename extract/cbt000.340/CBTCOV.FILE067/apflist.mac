PROC 0 REPORT
          CONTROL NOMSG
FREE F(SYSPRINT)
     DEL APFLIST.REPORT
          CONTROL MSG
ISPEXEC VGET (REPORT)
          IF &REPORT = T THEN GOTO ALLOCT
          ALLOC F(SYSPRINT) DS(APFLIST.REPORT) NEW SPA(10,10) TR
          GOTO DOIT
ALLOCT:   +
          ALLOC F(SYSPRINT) DS(*)
DOIT:     +
$P APFLIST 'B'
          CONTROL NOMSG
FREE F(SYSPRINT)
          CONTROL MSG
IF &REPORT = T THEN EXIT
%GENPRINT DSN(APFLIST.REPORT) DEOPT(D)
