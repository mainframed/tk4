PROC 0
CONTROL NOMSG
FREE F(IN MACOUT)
DEL FINDMAC.REPORT
CONTROL MSG
    ISPEXEC VGET (MACIN FRDISP)
    ALLOC F(IN) DS(&MACIN) SHR
    IF &FRDISP = T THEN ALLOC F(MACOUT) DS(*)
       ELSE ALLOC F(MACOUT) DS(FINDMAC.REPORT) NEW SPA(1,1) TR         -
BLOCK(6160)
    $P FINDMAC
FREE F(IN MACOUT)
    %GENPRINT DSN(FINDMAC.REPORT)
