PROC 0
ISPEXEC CONTROL SPLIT ENABLE
ISPEXEC DISPLAY PANEL(ALIAS)
IF &DSN= -
   THEN DO
        END
   ELSE DO
        IF &VOL= -
        THEN DO
             ISPEXEC CONTROL DISPLAY LINE
             UTILEXEC PDSAA &DSN ALIAS &MEM &ALI
             END
        ELSE DO
             ISPEXEC CONTROL DISPLAY LINE
             UTILEXEC PDSAA &DSN VOL(&VOL) ALIAS &MEM &ALI
             END
        END
