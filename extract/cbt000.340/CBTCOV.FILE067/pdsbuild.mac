DEL '&SYSUID..GARBAGE.PDS'
FREE FI(SYSIN)
FREE FI(SYSPRINT)
FREE FI(SYSUT1)
FREE FI(SYSUT2)
FREE FI(X)
ALLOC FI(SYSIN) DA('&SYSUID..GARBAGE.SEQ')  SHR
ALLOC FI(X) DA('&SYSUID..GARBAGE.PDS') VOL(PUB707) SPACE (30 5) CYL -
  DIR(500) LRECL(80) BLKSIZE(3120) RECFM (F B)
FREE FI(X)
ALLOC FI(SYSUT1) DA('&SYSUID..GARBAGE.PDS') SHR
ALLOC FI(SYSUT2) DA('&SYSUID..GARBAGE.PDS') SHR
ALLOC FI(SYSPRINT) DA(*)
$ IEBUPDTE
FREE FI(SYSIN)
FREE FI(SYSPRINT)
FREE FI(SYSUT1)
FREE FI(SYSUT2)
WRITE MEMBERS RESTORED : ENTERING ISPF EDIT TO REVIEW DATASET
DEL '&SYSUID..GARBAGE.SEQ'
ISPEXEC EDIT DATASET('&SYSUID..GARBAGE.PDS')
