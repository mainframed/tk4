PROC 0
ISPEXEC CONTROL SPLIT ENABLE
ISPEXEC DISPLAY PANEL(HIST)
IF &DSN= -
   THEN DO
        END
   ELSE DO
        IF &VOL= -
        THEN DO
   ISPEXEC CONTROL DISPLAY LINE
   UTILEXEC  PDSAA &DSN HIST &MEM
             END
        ELSE DO
   ISPEXEC CONTROL DISPLAY LINE
   UTILEXEC  PDSAA &DSN VOL(&VOL) HIST &MEM
             END
        END
