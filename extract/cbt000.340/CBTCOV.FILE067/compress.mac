PROC 0
ISPEXEC CONTROL SPLIT ENABLE
ISPEXEC DISPLAY PANEL(COMPRESS)
IF &DSN= -
   THEN DO
        END
   ELSE DO
        IF &VOL= -
        THEN DO
    ISPEXEC CONTROL DISPLAY LINE
    TSOEXEC  PDSAA &DSN COMPRESS
             END
        ELSE DO
    ISPEXEC CONTROL DISPLAY LINE
    TSOEXEC  PDSAA &DSN VOL(&VOL) COMPRESS
             END
        END
