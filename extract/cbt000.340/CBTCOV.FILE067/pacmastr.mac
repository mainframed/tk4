PROC 0
CONTROL NOMSG NOLIST
ISPEXEC VGET (MEMBER RPT)
IF &RPT = T THEN GOTO TUT
IF &RPT = 6 THEN GOTO WAIV
ISPEXEC BROWSE DATASET('CBSDDC.PACMASTR.RPTS(&MEMBER)')
ISPEXEC DISPLAY PANEL(PACM02)
ISPEXEC VGET (ZTEMPF PGE1 PGE2 MEMBER JC MC COPIES JOBNAME SKEL)
ISPEXEC VGET (SPRDLDSN SPRJODSN SPRMSGCL SPRCHGNO SPRACCNT)
ISPEXEC VGET (SPRLOC SPRCSTNO SUBJOB)
IF &SUBJOB ^= Y THEN GOTO DONE
ISPEXEC FTOPEN TEMP
ISPEXEC FTINCL &SKEL
ISPEXEC FTCLOSE
ISPEXEC CONTROL DISPLAY LINE
    CONTROL MSG
    SUBMIT '&ZTEMPF'
    IF &LASTCC > 0 THEN WRITE *** SUBMIT FAILED ***
    CONTROL NOMSG
DONE: EXIT QUIT
WAIV: PRINTOFF 'CBSDDC.PACMASTR.WAIVFORM'
  EXIT QUIT
TUT: ISPEXEC SELECT PANEL(PACMHELP)
