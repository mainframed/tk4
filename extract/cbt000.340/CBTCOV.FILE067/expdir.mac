PROC 0
ISPEXEC CONTROL SPLIT ENABLE
ISPEXEC DISPLAY PANEL(EXPDIR)
IF &DSN= -
   THEN DO
        END
   ELSE DO
        IF &VOL= -
        THEN DO
     ISPEXEC CONTROL DISPLAY LINE
     UTILEXEC PDSAA &DSN FIXPDS EXPAND (&NUM)
             END
        ELSE DO
     ISPEXEC CONTROL DISPLAY LINE
     UTILEXEC PDSAA &DSN VOL(&VOL) FIXPDS EXPAND (&NUM)
             END
        END
