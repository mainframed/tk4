PROC 0
CONTROL NOMSG
FREE FI(SYSPRINT)
FREE FI(SYSLIB)
ISPEXEC CONTROL SPLIT ENABLE
ISPEXEC DISPLAY PANEL(GAS)
IF &DSN= -
   THEN DO
        END
   ELSE DO
        IF &VOL= -
        THEN DO
     ISPEXEC CONTROL DISPLAY LINE
     ALLOC FI(SYSLIB) DA(&DSN) SHR
     ALLOC FI(SYSPRINT) DA(*)
     K
     $ PDSGAS
     FREE FI(SYSPRINT)
     FREE FI(SYSLIB)
             END
        ELSE DO
     ISPEXEC CONTROL DISPLAY LINE
     ALLOC FI(SYSLIB) DA(&DSN) VOL(&VOL) SHR
     ALLOC FI(SYSPRINT) DA(*)
     K
     $ PDSGAS
     FREE FI(SYSPRINT)
     FREE FI(SYSLIB)
             END
        END
