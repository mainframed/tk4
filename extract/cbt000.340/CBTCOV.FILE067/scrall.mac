PROC 1 DSNAME
CONTROL NOMSG NOFLUSH
/* SCRATCHES ALL OCCURANCES OF DSNAME FROM STORAGE VOLUMES
/* HOWARD M. DEAN  12/12/81
FREE  FILE(SYSPRINT SYSIN)
ALLOC FILE(SYSPRINT) DA(*)
ALLOC FILE(SYSIN) SPACE(1) TRACK UNIT(SYSDA)
OPENFILE SYSIN OUTPUT
SET &SYSIN=&STR(SCRATCH  DSN=&DSNAME,ALL )
PUTFILE  SYSIN
CLOSFILE SYSIN
CALL 'HMD.MVSUG.LOAD(FUS)' ',NOPRINT'
FREE  FILE(SYSIN SYSPRINT)
ALLOC FILE(SYSIN) DA(*)
ALLOC FILE(SYSPRINT) DA(*)
WRITE ** SCRATCH PROCESSING FOR &DSNAME COMPLETE **
EXIT QUIT
