PROC 0
ISPEXEC CONTROL SPLIT ENABLE
ISPEXEC DISPLAY PANEL(ATTRIB)
IF &DSN= -
   THEN DO
        END
   ELSE DO
        IF &VOL= -
        THEN DO
    ISPEXEC CONTROL DISPLAY LINE
    UTILEXEC PDSAA &DSN ATTRIB &MEM
             END
        ELSE DO
    ISPEXEC CONTROL DISPLAY LINE
    UTILEXEC PDSAA &DSN VOL(&VOL) ATTRIB &MEM
             END
        END
