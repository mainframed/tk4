PROC 0 DEBUG
IF &DEBUG = DEBUG THEN CONTROL LIST CONLIST SYMLIST PROMPT NOFLUSH MSG
                  ELSE CONTROL NOMSG
FREE F(ELEMIN ELEMOUT CNTL WORK)
CONTROL MSG
ISPEXEC VGET (PARM CNTLDSN CPYSRC MEMBER MODID SELOPT PROJECT LIBRARY  -
TYPE )
ALLOC: +
ALLOC F(WORK) UNIT(SYSVIO) SPA(2,2) TR
IF &PARM = F THEN GOTO ACNTL
IF &PARM = N THEN GOTO ACNTL
IF &PARM = X THEN GOTO ACNTL
 ALLOC F(ELEMIN) DS(&CPYSRC) SHR
ALLOC F(ELEMOUT) DS('&PROJECT..&LIBRARY..&TYPE') SHR
ACNTL: ALLOC F(CNTL) DS(&CNTLDSN) SHR
IF &PARM = F THEN GOTO CALL
IF &PARM = N THEN GOTO CALL
IF &PARM = X THEN GOTO CALL
WRITE COPYING ELEMENT &MEMBER FROM &CPYSRC TO &PROJECT..&LIBRARY..&TYPE
CALL: $P COPYCNTL   'CODE=&PARM,MEMBER=&MEMBER,MODID=&MODID'
CONTROL NOMSG
FREE F(ELEMIN ELEMOUT CNTL WORK)
