 &PASTE = TRANS(TRUNC(&PASTE,1) C,COPY M,MOVE P,PROMPT)
 VER (&PASTE,NONBLANK,LIST,COPY,MOVE,PROMPT)
 VER (&AT,NUM) VER (&AT,RANGE,1,255)
 VER (&LPASTE,NUM) VER (&LPASTE,RANGE,1,255)
