//IMS      JOB MSGLEVEL=(1,1)
//IMSYS38 PROC PTR=WBK,                                                *
//             DATUM='DATUM FALSCH       ',
//             ZEIT=030,
//             DRUCK='DUMMY,',                                         *
//             IMS=IMSVS,                                              *
//             OUT=A
//STEP1  EXEC  PGM=DFSILTA0,                                           *
//             PARM='ST=ALL,OUT=NOLOG'
//STEPLIB   DD DSN=&IMS..RESLIB,DISP=SHR
//HEADING   DD SYSOUT=&OUT.
//PRINTER   DD &DRUCK.SYSOUT=&OUT.
//REPORT    DD DSN=&&REPORT,DISP=(NEW,PASS),                           *
//             UNIT=SYSDA,SPACE=(CYL,(15,5))
//LOGIN    DD  DSN=&PTR..IMSYS.IMSLG010,DISP=(OLD,KEEP),               *
//             DCB=(OPTCD=B),                                          *
//             UNIT=TAPE,VOL=SER=(&BANDNR.)
//STEP2   EXEC PGM=SORT
//STEPLIB   DD DSN=SYS1.LINKLIB,DISP=SHR
//SORTLIB   DD DSN=SYS1.SORTLIB,DISP=SHR
//SYSOUT    DD SYSOUT=&OUT.
//SORTIN    DD DSN=&&REPORT,DISP=(OLD,PASS)
//SORTOUT   DD DSN=BBK.IMSYS.RPORT010,DISP=(NEW,PASS),                 *
//             DCB=(RECFM=FB,BLKSIZE=1463,LRECL=133),                  *
//             SPACE=(CYL,(15,5)),UNIT=SYSDA
//SORTWK01  DD UNIT=SYSDA,SPACE=(CYL,(15,5))
//SORTWK02  DD UNIT=SYSDA,SPACE=(CYL,(15,5))
//SORTWK03  DD UNIT=SYSDA,SPACE=(CYL,(15,5))
//STEP3   EXEC PGM=O40198,                                             *
//             PARM='&DATUM.'
//STEPLIB   DD DSN=SYS1.LINKLIB,DISP=SHR
//EBAS1     DD DSN=BBK.IMSYS.RPORT010,DISP=(OLD,DELETE),               *
//             UNIT=SYSDA
//ADRS1     DD SYSOUT=&OUT.,DCB=(RECFM=FBM,BLKSIZE=1463,LRECL=133)
//SYSUDUMP  DD SYSOUT=D,DCB=(RECFM=FBA,BLKSIZE=2420,LRECL=121)
//STEP4   EXEC PGM=SORT
//STEPLIB   DD DSN=SYS1.LINKLIB,DISP=SHR
//SORTLIB   DD DSN=SYS1.SORTLIB,DISP=SHR
//SYSOUT    DD SYSOUT=&OUT.
//SORTIN    DD DSN=&&REPORT,DISP=(OLD,DELETE)
//SORTOUT   DD DSN=BBK.IMSYS.RPORT011,DISP=(NEW,PASS),                 *
//             DCB=(RECFM=FB,BLKSIZE=1463,LRECL=133),                  *
//             SPACE=(CYL,(15,5)),UNIT=SYSDA
//SORTWK01  DD UNIT=SYSDA,SPACE=(CYL,(15,5))
//SORTWK02  DD UNIT=SYSDA,SPACE=(CYL,(15,5))
//SORTWK03  DD UNIT=SYSDA,SPACE=(CYL,(15,5))
//STEP5   EXEC PGM=O40199,                                             *
//             PARM='&DATUM.&ZEIT.'
//STEPLIB   DD DSN=SYS1.LINKLIB,DISP=SHR
//EBAS1     DD DSN=BBK.IMSYS.RPORT011,DISP=(OLD,DELETE),               *
//             UNIT=SYSDA
//ADRS1     DD SYSOUT=&OUT.,DCB=(RECFM=FBM,BLKSIZE=1463,LRECL=133)
//SYSUDUMP  DD SYSOUT=D,DCB=(RECFM=FBA,BLKSIZE=2420,LRECL=121)
//             PEND



//IMSYS38 EXEC PROC=IMSYS38,
//             ZEIT=015,
//             DATUM='28.02.77 MITTAGS   ',
//             BANDNR=400101
//STEP1.TITLE DD *
PROGRAMM      IMS-ANTWORTZEITEN IM DC-BETRIEB
/*
//STEP2.SYSIN DD *
  SORT FIELDS=(35,8,CH,A),SIZE=E10000
/*
//STEP4.SYSIN DD *
  SORT FIELDS=(46,10,CH,A),SIZE=E10000
/*
//
