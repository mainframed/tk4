     WHEN('OUCB    ')   BASE = OUCB_PTR;
     WHEN('OUXB    ')   BASE = OUXB_PTR;
     WHEN('PSCB    ')   BASE = PSCB_PTR;
     WHEN('QCBMAJ  ')   BASE = QCBMAJ_FPTR;
     WHEN('RCVT    ')   BASE = RCVT_PTR;
     WHEN('RLGB    ')   BASE = RLGB_PTR;
