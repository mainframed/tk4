 ROUTINE:  PROC(NAME,REGF,REG0,REG1) OPTIONS(REENTRANT) REORDER;

 /********************************************************************/
 /*                                                                  */
 /*   PROC TO EXEC SPECIFIC SVC OR SYSTEM ROUTINE FROM PL/I.         */
 /*                                                                  */
 /*   DCL ROUTINE   EXT ENTRY(CHAR(8),PTR,PTR,PTR);                  */
 /*                                                                  */
 /*   CALL ROUTINE('SVC13',REGF,REG0,REG1);                          */
 /*                                                                  */
 /********************************************************************/

   DCL  NAME                CHAR(8),
        (REGF,REG0,REG1)    PTR;

   DCL  (BALR,SVC)          EXT ENTRY OPTIONS(ASM,INTER),
        (BIN,I)             BIN(31)FIXED,
        REG                 PTR,
        PIC                 PIC'ZZZZ9',
        (NULL,SUBSTR,UNSPEC)BUILTIN;

   DCL  PSA_PTR        PTR;
   DCL  1 PSA                           BASED (PSA_PTR),    /* NUC 0 */
          2 DUM1         CHAR( 16),
          2 CVT_PTR      PTR,
          2 DUM2         CHAR(492),
          2 HEAD         CHAR(  4),
          2 DUM3         CHAR( 24),
          2 TCB_OLD_PTR  PTR,
          2 DUM4         CHAR( 4),
          2 ASCB_OLD_PTR PTR;
   DCL  1 CVT                           BASED (CVT_PTR),    /* NUC 0 */
          2 DUM1         CHAR( 36),
          2 IECILK1_PTR  PTR,
          2 IECILK2_PTR  PTR,
          2 DUM2         CHAR(104),
          2 MSER_PTR     PTR,
          2 DUM3         CHAR(144),
          2 JESCT_PTR    PTR,
          2 DUM4         CHAR(256),
          2 ASVT_PTR     PTR,
          2 DUM5         CHAR( 80),
          2 QCBMAJ_FPTR  PTR,
          2 QCBMAJ_LPTR  PTR,
          2 DUM6         CHAR(344),
          2 RCVT_PTR     PTR;
   DCL  1 JESCT                         BASED (JESCT_PTR),  /* NUC 0 */
          2 DUM1         CHAR( 4),
          2 JESUCB_PTR   PTR,
          2 SWA_LOC_PTR  PTR,
          2 SWA_MOV_PTR  PTR,
          2 RESQM_PTR    PTR,
          2 IEFSSREQ_PTR PTR,
          2 SSCVT_PTR    PTR,
          2 PRI_SS_NAME  CHAR( 4),
          2 JESALLOC_ADDR PTR,
          2 JESUNALC_ADDR PTR,
          2 JESCATL_ADDR PTR,
          2 JES_NUM_UCB  BIN(31)FIXED,
          2 JESSASTA_ADDR PTR;
   DCL  1 SSCVT                         BASED (SSCVT_PTR),  /* 241 0 */
          2 HEAD         CHAR( 4),
          2 SSCVTNEXT    PTR,
          2 SUBNAME      CHAR( 4),
          2 BITS         BIT(32),
          2 SSVT_PTR     PTR,
          2 USE          CHAR( 4);

 /********************************************************************/

   PSA_PTR = NULL;

   ON CONVERSION  GOTO ERROR;

   IF SUBSTR(NAME,1,3)='SVC'  THEN DO;
     IF SUBSTR(NAME,4,5)=' '  THEN GOTO ERROR;
     DO I=8 TO 4 BY -1 WHILE(SUBSTR(NAME,I,1)=' ');
       END;
     PIC = SUBSTR(NAME,4,I-3);
     BIN = PIC;
     CALL SVC(BIN,REGF,REG0,REG1);
     END;

   ELSE DO;
     UNSPEC(REG) = '0'B;
     SELECT(NAME);
       WHEN('IEFJSREQ')  REGF = IEFSSREQ_PTR;
       WHEN('POINT'   )  DO;
         UNSPEC(I) = UNSPEC(REGF);
         I = I + 4;
         UNSPEC(REG) = UNSPEC(I);
         END;
       OTHER             ;
       END;
     IF (UNSPEC(REG)='0'B)  THEN REG = REGF;
     CALL BALR(REG,REGF,REG0,REG1);
     END;
   RETURN;

 ERROR:
   REGF = NULL;
   RETURN;

   END ROUTINE;
