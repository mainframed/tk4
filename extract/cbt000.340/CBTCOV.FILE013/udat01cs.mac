./ ADD NAME=RLOG
PROC 0
SEND 'LOG020I TOOMUCH LOG-USERS AKTIVE - TRY LATER ' USER(*)
END
./ ADD NAME=SLOG
PROC 1 NAME Q(TSL)
DEL LOGDATA.TEXT
FREE F(TSLOGIN,TSLOGOUT)
ALLOC F(TSLOGIN) DA('&Q..LOG#&NAME.') SHR
ALLOC F(TSLOGOUT) DA(LOGDATA.TEXT) NEW SP(5 5) TR
CALL 'SYS1.TSOLOAD(SPTSO02)'
FREE F(TSLOGIN,TSLOGOUT)
DEL '&Q..LOG#&NAME.'
SEND 'LOG017I EDIT-DATASET ''LOGDATA.TEXT'' CREATED' USER(*)
EDIT LOGDATA TEXT
VERIFY
TOP
L *
BOTTOM
END
END
./ ADD NAME=TLOG
PROC 0
SEND 'LOG021I DUMP/TIME-BIT SWITCHED ' USER(*)
END
./ ADD NAME=ULOG
PROC 0
SEND 'LOG010I LOGON/LOGOFF IGNORED - HARDCOPY-LOG IN PROCESS' USER(*)
SEND 'LOG011I ENTER ''LOG DEL'',''LOG EDIT'' OR ''LOG PRINT'' TO STOP H-
ARDCOPY-LOG' USER(*)
END
./ ADD NAME=VLOG
PROC 1 NAME Q(TSL)
SEND 'LOG013I LOG WILL BE WRITTEN TO &Q..LOG#&NAME AGAIN' USER(*)
END
./ ADD NAME=WLOG
PROC 1 DEL Q(TSL)
DEL '&Q..LOG#&DEL.'
SEND 'LOG012I &Q..LOG#&DEL DELETED' USER(*)
END
./ ADD NAME=XLOG
PROC 1 EIN V(VOLSER) Q(TSL)
ALLOC DA('&Q..LOG#&EIN.') F(SYSTSLOG) NEW SP(5,5) TR REL VOL(&V.)
FREE F(SYSTSLOG)
SEND 'LOG008I LOG WILL BE WRITTEN TO &Q..LOG#&EIN.' USER(*)
END
./ ADD NAME=YLOG
PROC 0
SEND 'LOG014I LOG-WRITING STOPPED' USER(*)
END
./ ADD NAME=ZLOG
PROC 1 PRINT CLASS(T) Q(TSL) DCLASS(7)
FREE F(SYSIN,SYSPRINT,SYSUT1,SYSUT2)
ALLOC F(SYSIN) DUMMY
ALLOC F(SYSPRINT) SYSOUT SP(1,1) TR
ALLOC F(SYSUT1) DA('&Q..LOG#&PRINT.') SHR
ALLOC F(SYSUT2) SYSOUT SP(10,15) TR
SEND 'LOG009I LOGDATA WILL BE MOVED TO SYSOUT=&CLASS.' USER(*)
CALL 'SYS1.LINKLIB(IEBGENER)'
FREE F(SYSIN,SYSUT1) DA('SYS1.LINKLIB')
FREE F(SYSUT2) SYSOUT(&CLASS.)
FREE F(SYSPRINT) SYSOUT(&DCLASS.)
ALLOC F(SYSIN) DA(*)
ALLOC F(SYSPRINT) DA(*)
DEL '&Q..LOG#&PRINT.'
END
