         SOOPT=SOOPT+OOPT;
         GOTO DRUCK;
 SUMME:  IBLNR=IBLNR+1;
         BLNR=IBLNR;
         VON=ZDATE;
         BIS=ZDATEND;
         WRITE FILE(LIST) FROM(KOPF1);
         WRITE FILE(LIST) FROM(KOPF2);
         WRITE FILE(LIST) FROM(KOPFLEER);
         WRITE FILE(LIST) FROM(KOPFSUM);
         WRITE FILE(LIST) FROM(KOPFLEER);
         WRITE FILE(LIST) FROM(KOPF21);
         WRITE FILE(LIST) FROM(KOPFLEER);
         WRITE FILE(LIST) FROM(KOPF22);
         WRITE FILE(LIST) FROM(KOPFLEER);
         WRITE FILE(LIST) FROM(KOPFSUMZ1);
         WRITE FILE(LIST) FROM(KOPFSUMZ2);
         WRITE FILE(LIST) FROM(KOPFLEER);
         WRITE FILE(LIST) FROM(KOPFSTERN1);
         SOELAPS3=MOD(SUMWERT,60);
         SMWERT=SUMWERT/60;
         SOELAPS2=MOD(SMWERT,60);
         SOELAPS1=SMWERT/60;
         WRITE FILE(LIST) FROM(SUMZEIL);
         GOTO EOF1;
 KOPF:   IBLNR=IBLNR+1;
         BLNR=IBLNR;
         WRITE FILE(LIST) FROM(KOPF1);
         WRITE FILE(LIST) FROM(KOPF2);
         WRITE FILE(LIST) FROM(KOPFLEER);
         WRITE FILE(LIST) FROM(KOPF21);
         WRITE FILE(LIST) FROM(KOPFLEER);
         WRITE FILE(LIST) FROM(KOPF3);
         WRITE FILE(LIST) FROM(KOPF4);
         WRITE FILE(LIST) FROM (KOPFLEER);
         WRITE FILE(LIST) FROM(KOPF5);
         WRITE FILE(LIST) FROM(KOPFLEER);
         WRITE FILE(LIST) FROM(KOPFSTERN1);
         IZZ=10;
         GOTO AUSGAB;
 DRUCK:  IF ANF THEN DO;
                        IF EINZEL THEN GOTO AUSGAB1;
                        ANF='0'B;
                        GOTO KOPF;
                     END;
         IF IZZ>65 THEN GOTO KOPF;
 AUSGAB: IF EINZEL THEN GOTO AUSGAB1;
         WRITE FILE(LIST) FROM(OUTPUT);
         IZZ=IZZ+1;
 AUSGAB1: OUTPUT=LOESCHSA;
         IF D THEN GOTO LES1;
         ELSE IF E THEN GOTO LES2;
         ELSE IF F THEN GOTO LES3;
                   ELSE GOTO LESDIS;
 EOF:    GOTO SUMME;
 EOF1:    IF NOCH THEN DO;
                          CLOSE FILE(DISKIN) ENV(REREAD),FILE(LIST);
                          OPEN FILE(DISKIN),FILE(LIST);
                          OUTPUT=LOESCHSA;
                          CARDINP=LCARDINP;
                          SUMZEIL=LSUMZEIL;
                          SUMWERT=0;
                          CART='1'B;
                          SSS='0'B;
                          SB='0';
                          SG='0'B;
                          E='0'B;
                          D='0'B;
                          F='0'B;
                          ANF='1'B;
                          IBLNR=0;
                          IZZ=0;
                          ZDATE=0;
                          ZDATEND=0;
                          EINZEL='0'B;
                          SACHC=' ';
                          GRC=' ';
                          BAC=' ';
                          GOTO LESKART;
                       END;
                  ELSE CLOSE FILE(DISKIN),FILE(LIST);
         END SMFLSTS;
