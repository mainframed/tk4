//SPLREPRO PROC RGN=512K,MBR=NOMBR,PXREF=NOXREF,PCOMP=NOCOMP,LN=60,
//             PSUBS=NOSUBS,PDIAG=NODIAG,COMPR=NOCOMPRESS,
//             COMPR2=COMPRESS
//SECUR   EXEC PGM=HES11MDV,PARM=(&MBR),REGION=&RGN
//STEPLIB  DD  DSN=MDV.P.LINKLIB,DISP=SHR
//STEPCAT  DD  DSN=UCAT.MDV01,DISP=SHR
//MBDAT    DD  DSN=&&TEMP1,UNIT=3330,DISP=(,PASS),SPACE=(TRK,1),
//             DCB=BLKSIZE=10
//UPD     EXEC PGM=HES01MDV,TIME=9,REGION=&RGN,PARM='NR'
//STEPCAT  DD  DSN=UCAT.MDV01,DISP=SHR
//STEPLIB  DD  DSN=MDV.P.LINKLIB,DISP=SHR
//CATA     DD  DSN=B001C211.N02.SPLIGH,DISP=SHR,
//             AMP='RECFM=F,OPTCD=IL,BUFNI=4'
//KORR     DD  DSN=B001C211.N01.SPLGH,DISP=SHR,
//             AMP='RECFM=F,OPTCD=IL,BUFNI=4'
//LOG      DD  DSN=B001C211.N01.SPLLOGGH,DISP=SHR
//TAPE     DD  DUMMY,LABEL=(,NL),VOL=SER=000000,
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=800,DEN=1,TRTCH=ET)
//PUNCH    DD  SYSOUT=B,DCB=BLKSIZE=80
//TEMP     DD  DSN=&&TEMPO,UNIT=3330,DISP=(,PASS),SPACE=(CYL,(2,1)),
//             DCB=(LRECL=80,BLKSIZE=800,RECFM=FB)
//SAVETY   DD  DSN=&&TEMP1,DISP=(OLD,DELETE)
//SYSPRINT DD  SYSOUT=A
//S264A   EXEC PGM=DFSUPAA0,TIME=10,REGION=&RGN,COND=(264,NE,UPD),
//             PARM='&PXREF,&PCOMP,&PSUBS,&PDIAG,LINECNT=&LN,&COMPR'
//SYSIN    DD  DSN=&&TEMPO,DISP=(OLD,DELETE)
//SYSLIB   DD  DSN=B001C211.N07.PREPROGH,DISP=SHR
//REFIN    DD  DSN=IMSVS.REFERAL,DISP=OLD
//REFOUT   DD  DSN=IMSVS.REFERAL,DISP=OLD
//REFRD    DD  DSN=IMSVS.REFERAL,DISP=OLD
//SYSTEXT  DD  DSN=&&TXTPASS,UNIT=SYSDA,SPACE=(CYL,(1,1)),
//             DCB=BLKSIZE=800
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,(1,1))
//DUMMY    DD  DSN=IMSVS.PROCLIB(REFCPY),DISP=SHR
//UTPRINT  DD  SYSOUT=A
//SYSPRINT DD  SYSOUT=A
//SEQBLKS  DD  DSN=&&BLKS,DISP=(,PASS),UNIT=SYSDA,SPACE=(CYL,(1,1))
//S264B   EXEC PGM=DFSUNUB0,REGION=&RGN,PARM='&COMPR2',
//             COND=((264,NE,UPD),(8,LT,S264A))
//SEQBLKS  DD  DSN=&&BLKS,DISP=(OLD,DELETE)
//UTPRINT  DD  SYSOUT=A
//FORMAT   DD  DSN=IMSVS.FORMAT,DISP=OLD
//SYSPRINT DD  SYSOUT=A
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,(1,1))
//DUMMY    DD  DSN=IMSVS.PROCLIB(FMTCPY),DISP=SHR


