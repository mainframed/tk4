//TCAMCHK PROC P='C0010005P00DO010'
//STEP1   EXEC PGM=AMACPT,                    AMACPT IS A NAME FROM PPT
//             DPRTY=(12,15),
//             REGION=12K,ADDRSPC=REAL,
//             TIME=1,PARM=&P
//*
//*      DIESE PROCEDURE LISTET FELDER AUS DER TCAM-AVT
//*      DIE PROCEDURE IST WIE FOLGT ZU STARTEN:
//*      S TCAMCHK,P='C0010005P00DO010'  DEFAULT PARM-FELD
//*
//*      DEFINITION DES PARMFELDES:
//*                BYTE 0    = C WTO AUF CONSOLE UEBER FREIE BUFFER
//*                              IM TCAM-BUFFER-POOL UND
//*                              LISTE DER AVT-FELDER JE INTERVAL.
//*                          = L NUR LISTE DER AVT-FELDER.
//*
//*                BYTE 1-5  = 00001-99999 ZAEHLER UEBER ANZAHL DER
//*                                        AVT-SNAPSHOTS.
//*
//*                BYTE 6+7  = 01-59 INTERVAL IN SEC. JE SNAPSHOT.
//*
//*                BYTE 8-15 = 1-8 STELLIGER JOB ODER STC-NAME
//*                                DER TASK, DIE DAS TCAM-MCP
//*                                BEINHALTET.
//*
//*      A C H T U N G :
//*
//*      CHARACTER IM PARMFELD MUESSEN AUF DER CONSOLE IN
//*      GROSS-BUCHSTABEN EINGEGEBEN WERDEN.
//*
//*      DAS PARMFELD WIRD  N I C H T  AUF RICHTIGKEIT GEPRUEFT.
//*
//*      WENN TCAMCHK VOR ABLAUF DER SPEZIFIZIERTEN WERTE ABGEBROCHEN
//*      WERDEN SOLL, IST :   P TCAMCHK  EINZUGEBEN.
//*
//STEPLIB  DD  DSN=TCAM.LOADT,DISP=SHR
//SYSUDUMP DD  SYSOUT=A
//PRINT    DD  SYSOUT=A,DCB=(RECFM=FBA,LRECL=81,BLKSIZE=972,BUFNO=2)
