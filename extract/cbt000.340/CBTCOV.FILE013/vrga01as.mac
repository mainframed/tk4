         BE    SNAP
*          SNAP DCB=SNAPDCB,STORAGE=(FELD,FEND)
X00      EQU   *
         TM    JLETYPE,X'10'
         BO    A04
A01      NOP   A02
         CLI   QUELIST+1,X'40'
         BE    *+8
         BAL   R3,PRINT
         MVI   XMODE,X'01'
         LA    R9,QUELIST+1
         MVI   VS,X'13'
         OI    A01+1,X'F0'
A02      EQU   *
         CLI   XMODE,X'00'
         BE    *+10
         MVC   QUEN(5),=C'SOUTQ'
         B     A03
A04      EQU   *
         CLI   XMODE,X'00'
         BE    *+10
         MVC   QUEN(4),=C'JOBQ'
A03      EQU   *
         CLI   QUELIST+1,X'40'
         BNE   A05
         MVC   QUEN(17),=C'INPUT QUEUE EMPTY'
         BAL   R3,PRINT
         XI    XMODE,X'01'
         B     A02
A05      EQU   *
         MVC   QCLASS,JLECL
         TM    JLESTAT,X'80'
         BZ    *+14
         MVC   QSTAT(4),=C'HOLD'
         B     *+10
         MVC   QSTAT(5),=CL5' '
         MVC   QJOBN(8),JLENAM
         LA    R8,15
         MVN   WORT+3(1),JLETYPE
         S     R8,WORT
         CVD   R8,DWORT
         MVC   QPRTY,=X'40202020'
         ED    QPRTY,DWORT+6
         CLC   JLEQID,=H'0'
         BNE   *+14
         MVC   QUSER(7),=C'CENTRAL'
         B     *+10
         MVC   QUSER,=C'TR      '
         XI    XMODE,X'01'
         CLI   XMODE,X'01'
         BE    P00
         LA    R9,34(R9)
         B     SNAP
P00      EQU   *
         LA    R9,QUELIST+1
P01      EQU   *
         BAL   R3,PRINT
         B     SNAP
A1       EQU   *
