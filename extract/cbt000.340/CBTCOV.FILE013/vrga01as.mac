QUEUE    CSECT
         RETTE (10,11)
* ANZEIGEN DER LAUFENDEN PARTITION UND DER INPUT-OUTPUT QUEUE
* FUER CICS-, VSPC- UND RTAM-ANWENDERIN VS1
* IN CICS MUSS EIN CICS-STEUERPR. DIESES PROGRAMM ALS UNTERPROGRAMM
* AUFRUFEN. EIN PL1-STEUERPR. LIEGT BEI.
* FUER VSPC WIRD DIESES PROGRAMM ALS BATCH-JOB AUSGEFUERT UND MIT
* DEM LOAD-OUTPUT-BEFEHL DIE AUSGABE AM BILDSCHIRM ANGEZEIGT
* RTAM-ANWENDER MUESSEN DIE AUSGABE DEM TERMINAL ENTSPRECHEND
* MODIFIZIEREN
* DIE ANZUZEIGENDE PARTITION MUSS EBENFALLS MODIFIZIERT WERDEN
*
         L     2,16   CVT
         L     1,0(2)  CVTP  TCB
         L     1,4(1)  CURRENT TCB
         IC    0,=X'01'
         SVC   243   KEY=0
         CLM   15,1,=X'10'   WAS JOB  AUTHORIZED
         BE    WTO
         CLM   15,1,=X'00'
         BNE   WAITEXB
WTO      EQU   *
         MODESET KEY=ZERO
         OPEN  (LISTE,(OUTPUT))
         MVI   QUELIST,X'40'
         MVC   QUELIST+1(L'QUELIST-1),QUELIST
         MVI   VS,X'0B'
ANF1     EQU   *
         L     R0,0
         L     R5,16(R0)     ADR CVT
         LA    R5,244(R5)         POINTER TCB P0
         L     R6,0(R5)
         L     R5,0(R6)      TCB  PARTITION P0
TCB      EQU   *
         LA    R6,40(R6)   TCB PARTITION P10
         L     R5,0(R6)      TCB P5 LADEN
         L     R8,164(R5)    ADR TCBTCT
         L     R7,124(R5)
         MVC   M3+14(8),64(R7)
         CLC   64(6,R7),INIT
         BNE   P0
         LA    R2,3
         LA    R4,M
         B     P2
P0       EQU   *
         C      R8,=F'0'
         BE    P1
         L     R9,28(R8)     ADR JMR
         L     R2,32(R9)
         C     R2,=F'0'
         BE    *+14
         MVC   WK1,32(R9)
         B     *+10
         MVC   WK1,56(R8)
         L     R3,WK1        JOB INITATION-ZEIT
         BAL   R4,TIMECNVR
         LA    R2,TAB
P3       CLC   0(3,R2),20(R9)
         BNE   *+14
         MVC   M8+14(17),3(R2)
         B     P4
         LA    R2,20(R2)
         CLC   0(3,R2),=C'XXX'
         BNE   P3
         MVC   M8+14(17),=C'                '
         MVC   M8+14(3),20(R9)
P4       EQU   *
         MVC   OUTINIT1,OUTINIT
         MVC   WK1,8(R9)
         L     R3,WK1
         BAL   R4,TIMECNVR
         ZAP   WK1,=PL1'0'
         MVC   MONTAB,=PL2'31,28,31,30,31,30,31,31,30,31,30,31'
         ZAP   MONFLD,=PL1'1'
         MVO   WK1,12(2,R9)
         UNPK  YEAR,WK1+2(2)
         DP    WK1,=PL1'4'
         CP    WK1+3(1),=PL1'0'
         BNZ   NOLAP
         ZAP   MONTAB+2(2),=PL2'29'
NOLAP    ZAP   WK1,14(2,R9)
         LA    R4,MONTAB
DATLOOP  SP    WK1,0(2,4)
         BNH   HIT
         AP    MONFLD,=PL1'1'
         LA    4,2(4)
         B     DATLOOP
HIT      AP    WK1,0(2,4)
         UNPK  MON,MONFLD
         UNPK  DAY,WK1+2(2)
         XC    DATUM,=X'0030910030910030'
P1       EQU   *
         L     R7,12(R5)     ADR TIOT IM TCB
         MVC   M3+37(8),8(R7)
         MVC   M4+14(8),OUTINIT1
         MVC   M4+37(8),OUTINIT
         MVC   M5+14(8),DATUM
         ZAP   WK1,=PL1'0'
         MVO   WK1,44(1,R9)
         OI    WK1+3,X'0F'
         UNPK  M5+37(2),WK1+3(1)
         LA    R4,M1
         LA    R2,7
P2       EQU   *
UEB      EQU   *
         MVC   QUELIST+1(47),0(R4)
         BAL   R3,PRINT
         LA    R4,47(R4)
         BCT   R2,UEB
         MVI   TCB+3,X'24'
         MVC   M0+23(2),=C'9 '
         MVC   M2+29(2),=C'9 '
         MVC   INIT,=C'INIT9 '
END3     NOP   END2
         OI    END3+1,X'F0'
         B     ANF1
TIMECNVR EQU   *
         SR    R2,R2
         AH    R3,=H'50'
         D     R2,=F'100'
         SR       R2,R2               GET HOURS
         D     R2,=F'3600'
         CVD   R3,PAKT
         UNPK  HOUR,PAKT
         LR    R3,R2         GET MINUTES
         SR    R2,R2
         D     2,=F'60'
         CVD   R3,PAKT
         UNPK     MINUTE,PAKT
         CVD   R2,PAKT
         UNPK  SECOND,PAKT
         OC    TIME(8),=8X'F0'
         MVC   OUTINIT(2),HOUR
         MVI   OUTINIT+2,C'.'
         MVC   OUTINIT+3(2),MINUTE
         MVI      OUTINIT+5,C'.'
         MVC   OUTINIT+6(2),SECOND
         BR    R4
INIT     DC    C'INIT10'
