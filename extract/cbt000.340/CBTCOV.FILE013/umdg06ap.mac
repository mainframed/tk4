          /*                                                         */
          /*        SOURCE PROGRAM LIBRARY INDEX MBR-INSERT          */
          /*                                                         */
          /***********************************************************/
-SPLIMI:
          PROC OPTIONS (MAIN);
-         DCL  WORK      CHAR (80);
-         DCL WORK1      CHAR (160);
-         DCL  SCHL      CHAR (16);
-         DCL  DOW  CHAR (1)  INIT ('*');
-         DCL  DCA  CHAR (1)  INIT ('*');
-         DCL  XPAGE     PIC'999' INIT (0);
-         DCL  XDATE     CHAR (8);
-         DCL  DATE      BUILTIN;
-         DCL  ONCODE    BUILTIN;
-         DCL  P1   POINTER;
-         DCL  P2   POINTER;
-         DCL  CATA FILE RECORD KEYED ENV (INDEXED);
-         DCL 1  STRUIN  BASED (P1),
               2 DOLLAR  CHAR  (3),
               2 SPLINA  CHAR  (8),
               2 TESTMB  CHAR  (10),
               2 PRDUMB  CHAR  (10),
               2 TLMBR   CHAR  (10);
-         DCL 1  STRUOUT BASED (P2),
               2 FREI1   CHAR  (93),
               2 TMBR    CHAR  (10),
               2 PMBR    CHAR  (10),
               2 LMBR    CHAR  (10),
               2 KZPR    CHAR (01),
               2 FREI2   CHAR (20),
               2 ISCHL   CHAR (16);
          ON ENDFILE (SYSIN) BEGIN;
                             DOW = ' ';
                             END;
          ON KEY (CATA) BEGIN;
                        IF ONCODE = 52 THEN DCA = ' ';
                        END;
          ON ENDPAGE (SYSPRINT) BEGIN;
                                CALL UEB;
                                END;
          OPEN FILE (SYSPRINT) STREAM LINESIZE(132);
          XDATE = SUBSTR (DATE,5,2) ×× '/' ××
                  SUBSTR (DATE,3,2) ×× '/' ××
                  SUBSTR (DATE,1,2);
1ANF:     CALL UEB;
          P1 = ADDR (WORK);
          P2 = ADDR (WORK1);
          OPEN FILE (CATA) DIRECT UPDATE;
          GET EDIT (WORK) (A(80));
          DO WHILE (DOW = '*'); /* DO WHILE */
          MERK = 0;
          CALL GUELT;
          IF MERK = 1 THEN DO; /* DO 1 */
                      END; /* END DO 1 */
            ELSE DO; /* ELSE 1 */
            STRUOUT = '';
            TMBR = TESTMB;
            PMBR = PRDUMB;
            LMBR = TLMBR;
            KZPR = 'J';
            ISCHL= '00000000' ×× SPLINA;
            SCHL = ISCHL;
            WRITE FILE (CATA) FROM (WORK1) KEYFROM (SCHL);
            IF DCA = ' ' THEN DO; /* DO 2 */
                         READ FILE (CATA) INTO (WORK1) KEY (SCHL);
                         TMBR = TESTMB;
                         PMBR = PRDUMB;
                         LMBR = TLMBR;
                         KZPR = 'J';
                         DCA = '*';
                         REWRITE FILE (CATA) FROM (WORK1) KEY (SCHL);
                         CALL AUSDRU;
                         END; /* END DO 2 */
              ELSE DO; /* ELSE 2 */
              CALL AUSDRU;
              END; /* END ELSE 2 */
            END; /* END ELSE 1 */
          GET EDIT (WORK) (A(80));
          END; /* END DO WHILE */
1         /***********************************************************/
          /*                                                         */
          /*             UP UEB UEBERSCHRIFT                         */
          /*                                                         */
          /***********************************************************/
-UEB:
          PROC;
          PUT FILE (SYSPRINT) PAGE;
          XPAGE = XPAGE +1;
          PUT FILE (SYSPRINT) SKIP (3) EDIT
          ('  SOURCE PROGRAM LIBRARY','DATE  ',XDATE,'PAGE ',XPAGE)
          (A,X(83),A,A,X(3),A,A);
          PUT FILE (SYSPRINT) SKIP (4) EDIT
          ('L I B R A R Y   I N D E X   I N S E R T') (X(40),A);
          PUT FILE (SYSPRINT) SKIP (4) EDIT
          ('PROGRAMMNAME','TESTMBR','PRODUKTIONSMBR','TESTMBR')
          (X(3),A,X(10),A,X(10),A,X(10),A);
          PUT FILE (SYSPRINT) SKIP (2);
          END UEB;
          /*****              END UEB                            *****/
1         /***********************************************************/
          /*                                                         */
          /*             UP AUSDRU                                   */
          /*                                                         */
          /***********************************************************/
-AUSDRU:
          PROC;
          PUT FILE (SYSPRINT) SKIP;
          PUT FILE (SYSPRINT) EDIT
          (SPLINA,TMBR,PMBR,LMBR) (X(6),A,X(11),A,X(13),A,X(10),A);
          END AUSDRU;
          /*****              END UP AUSDRU                      *****/
 GUELT:
          PROC;
          END;
          CLOSE FILE (CATA);
          PUT FILE (SYSPRINT) SKIP (3) EDIT
          ('*****  ENDE PROGRAMM  *****') (X(45),A);
          END SPLIMI;
          /*****              END HP SPLIMI                      *****/
