 PLUPRO:    PROC (PARM);
 ON ERROR CALL PLIDUMP ('TFBHS','PLUPRO');
          DCL  PARM CHAR (3000) VAR;
         DCL 1 STR BASED (POINT),
                 2   UETEXT   CHAR (30),
                 2   MDRZEI   FIXED (1),          /* ANZAHL DER MEHR-*/
                                                  /* FACHDRUCKZEILEN */
                 2   TABMDR (9),                  /* TABELLE UEBER   */
                                                  /* INHALT DER MEHR-*/
                                                  /* FACHDRUCKZEILEN */
                   3 ABKUER CHAR (1),             /* Z.B. C,P,I,O,R  */
                   3 TEXT   CHAR (20),            /* Z.B. CPU,PAGE IN*/
                   3 MAXIMW FIXED (15,1),         /* 100 %           */
                 2   ANZPART  FIXED (3),          /* IN-OUT-DATEIEN  */
                 2   TABPART (99)  CHAR (8);      /* P01 - P99       */
          POINT = ADDR (PARM);
         HI = 0;
         DO L = 1 TO MDRZEI;
         IF MAXIMW (L) > HI THEN HI = MAXIMW (L);
         END;
         MAXIMW (MDRZEI + 1) = HI;
         DCL Z FIXED (1);
  DCL DATTAB (9) CHAR (1) INIT('1',
  '2',
  '3',
  '4',
  '5',
  '6',
  '7',
  '8',
  '9');
          /*   EINGABEBEREICH      */
          DCL  1    EBER BASED (P),
                 2  KZ     CHAR (1),
                 2  TEXTE  CHAR (31),
                 2  DAT    PIC '99999',
                 2  ZAHL   (9) FIXED  (15,1);
         DCL HI FIXED (15);
          /*   DRUCKSTRUKTUREN     */
          DCL  DRUZEI CHAR (133);
          DCL  1    UE1  BASED (P1),
                 2  AZ1  CHAR (1),      /* ASA - ZEICHEN            */
                 2  T11  CHAR (74),     /* VON UETEXT               */
                 2  T11A   CHAR (2),
                 2  T11B   CHAR (3),
                 2  T12  CHAR (20),     /* VON TEXT                 */
                 2  T13  PIC 'ZZZ.ZZZ.ZZZ.ZZ9,V9',
                 2  T14  CHAR (16);               /*  = 100 %       */
          P1 = ADDR (DRUZEI);
          DCL 1   UE2  BASED (P1),
                2 AZ2  CHAR (1),
                2 PART CHAR (8),
                2 T21  CHAR (09),
                2 TAG  PIC  '99.999',   /* VON  DAT  */
                2 T22  CHAR (8),
                2 T23  CHAR (1),        /* I  */
                2 T24  CHAR (99),
                2 EINS CHAR (1);        /* 1 */
           DCL 1   UE3  BASED (P1),
                 2 AZ3  CHAR (1),
                 2 T31  CHAR (31),
                 2 T32  CHAR (1),       /* I  */
                 2 TZAHL1(10) PIC  '(9)Z9';
          DCL 1   UE4  BASED (P1),
                2 AZ4    CHAR (1),
                2 T41    CHAR (31),
                2 T42    CHAR (1),      /* I  */
                2 T43    CHAR (10),     /*1        0*/
                2 TZAHL2 (9)  PIC  '(9)Z9';   /*  9 MAL 0 */
          DCL 1   LFDZEI BASED (P1),
                2 AZ5    CHAR (1),
                2 T51    CHAR (31),     /* VON TEXTE  */
                2 T53    CHAR (1),      /* I */
                2 STERN  (100) CHAR (1);
          DCL PLWERT FIXED (15,2);
          DCL PLOTT  FIXED BIN (15);
          DCL INDAT (9) FILE;
  INDAT (1) = D1;
  INDAT (2) = D2;
  INDAT (3) = D3;
  INDAT (4) = D4;
  INDAT (5) = D5;
  INDAT (6) = D6;
  INDAT (7) = D7;
  INDAT (8) = D8;
  INDAT (9) = D9;
         PUT DATA (ANZPART,MDRZEI);
          DO I = 1 TO ANZPART;
             OPEN FILE (INDAT(I))       TITLE ('D' ×× DATTAB(I))
         RECORD INPUT;
          END;
          DCL OUTDAT (9) FILE;
 DCL (OUT1,OUT2,OUT3,OUT4,OUT5,OUT6,OUT7,OUT8,OUT9) FILE;
  OUTDAT (1) = OUT1;
  OUTDAT (2) = OUT2;
  OUTDAT (3) = OUT3;
  OUTDAT (4) = OUT4;
  OUTDAT (5) = OUT5;
  OUTDAT (6) = OUT6;
  OUTDAT (7) = OUT7;
  OUTDAT (8) = OUT8;
  OUTDAT (9) = OUT9;
          DO I = 1 TO ANZPART;
             OPEN FILE (OUTDAT(I))       TITLE ('OUT' ×× DATTAB(I))
         RECORD OUTPUT;
          END;
          DCL SCH (10) BIT (8);
          DO I = 1 TO 9;
             SCH (I) = '';
          END;
          SCH (10) = (8) '1'B;
-        ON ENDFILE (D1) BEGIN;
          CLOSE FILE (D1) ;
         SCH(I) = SCH(10); K = K + 1; GO TO M2; END;
-        ON ENDFILE (D2) BEGIN;
          CLOSE FILE (D2) ;
         SCH(I) = SCH(10); K = K + 1; GO TO M2; END;
-        ON ENDFILE (D3) BEGIN;
          CLOSE FILE (D3) ;
         SCH(I) = SCH(10); K = K + 1; GO TO M2; END;
-        ON ENDFILE (D4) BEGIN;
          CLOSE FILE (D4) ;
         SCH(I) = SCH(10); K = K + 1; GO TO M2; END;
-        ON ENDFILE (D5) BEGIN;
          CLOSE FILE (D5) ;
         SCH(I) = SCH(10); K = K + 1; GO TO M2; END;
-        ON ENDFILE (D6) BEGIN;
          CLOSE FILE (D6) ;
         SCH(I) = SCH(10); K = K + 1; GO TO M2; END;
-        ON ENDFILE (D7) BEGIN;
          CLOSE FILE (D7) ;
         SCH(I) = SCH(10); K = K + 1; GO TO M2; END;
-        ON ENDFILE (D8) BEGIN;
          CLOSE FILE (D8) ;
         SCH(I) = SCH(10); K = K + 1; GO TO M2; END;
-        ON ENDFILE (D9) BEGIN;
          CLOSE FILE (D9) ;
         SCH(I) = SCH(10); K = K + 1; GO TO M2; END;
-         DCL  ZZ (9)  FIXED (3);
          DO  I = 1 TO  9;
              ZZ (I) = 99;
          END;
          DCL ZWDAT (9) PIC '99999';
          DO I = 1 TO 9;
             ZWDAT (2) = '';
          END;
 EOFPR:   DO I = 1 TO ANZPART;
             IF SCH (I) ^=  SCH (10) THEN GOTO M1;
          END;
          GOTO CL;
 M1:      K = I;
 M2:      DO I = K TO ANZPART;
             IF SCH (I) = SCH (10) THEN DO;
                K = K + 1;
                GOTO M2;
             END;
             READ FILE  (INDAT(I)) SET (P);
             IF ZZ(I) > 60 × EBER.DAT ^= ZWDAT (I) THEN DO;
                ZZ(I) = 0;
                ZWDAT (I) = EBER.DAT;
                DO L = 1 TO MDRZEI;
                   IF L = 1 THEN DO;
                      AZ1 = '1';
                      T11 = UETEXT;
                      GOTO MFDR1;
                   END;
                   AZ1 = ' ';
                   T11 = ' ';
 MFDR1:            T11A =  TABMDR.ABKUER (L);
                   T11B = '=';
                   T12  =  TABMDR.TEXT (L);
                   T13  =  TABMDR.MAXIMW (L);
                   T14  = ' = 100 %';
                   WRITE FILE (OUTDAT(I)) FROM (DRUZEI);
                   ZZ(I) = ZZ(I) + 1;
                END;
                AZ2 = '0';
                PART = TABPART (I);
                T21 = ' ';
                     TAG = EBER.DAT;
                     T22 = ' ';
                     T23 = 'I';
                     T24 = ' ';
                     EINS = '1';
                     WRITE FILE (OUTDAT(I)) FROM (DRUZEI);
                     ZZ (I) = ZZ (I) + 2;
                     AZ3 = ' ';              /* VORSCHUB 1 ZEILE */
                     T31 = ' ';
                     T32 = 'I';
                     DO L = 1 TO 9;
                       TZAHL1(L) =  L;
                     END;               TZAHL1 (10) = 0;
                     WRITE FILE (OUTDAT(I)) FROM (DRUZEI);
                     ZZ (I) = ZZ (I) + 1;
                     AZ4 = ' ';         /* VORSCHUB 1 ZEILE    */
                     T41 = ' ';
                     T42 = 'I';
                     T43 = '1        0';
          T53 = 'I';
                     DO L = 1 TO 9;
                       TZAHL2 (L) = 0;
                     END;
                     WRITE FILE (OUTDAT(I)) FROM (DRUZEI);
                     ZZ (I) = ZZ (I) + 2;
         DRUZEI = (133)' ';
                     WRITE FILE (OUTDAT(I)) FROM (DRUZEI);
                 END;
           /*     AUFBEREITUNG DER LAUFENDEN DRUCKZEILE              */
             IF KZ ^= ' ' THEN DO;           /* ZUSATZTEXTZEILE      */
                   AZ5 = ' ';                /* VORSCHUB 1 ZEILE     */
                   T51 = TEXTE;
                   T53 = 'I';
                   STERN = '';
                   WRITE FILE (OUTDAT(I)) FROM (DRUZEI);
                   ZZ(I) = ZZ(I) + 1;
                   K = K + 1;
                   GOTO  M3;
               END;
          /* VERARBEITUNG PLOTTZEILE                            */
         HI = 0;
               DO L = 1 TO MDRZEI + 1;  STERN = '';
         IF L = MDRZEI + 1 THEN DO;
               DO L1 = 1 TO HI;
                  STERN(L1)  = '_';
               END;
                  GO TO WRITE;
         END;
         IF L = 1 THEN DO; AZ5 = ''; T51 = TEXTE; END;
                  ELSE DO; AZ5 = '+'; T51 = ''; END;
               IF EBER.ZAHL(L) =0 THEN GO TO WRITE;
         IF EBER.ZAHL(L) > MAXIMW(L) THEN EBER.ZAHL(L) = MAXIMW(L);
               PLWERT = MULTIPLY(ZAHL(L),100,15,2);
               PLWERT = DIVIDE(PLWERT,MAXIMW(L),15,2);
               PLOTT = PLWERT + 0.5;
         IF PLOTT > HI THEN HI = PLOTT;
         IF PLOTT = 0 THEN PLOTT = 1;
              STERN (PLOTT) = ABKUER (L);
0WRITE:
         T53 = 'I';
               WRITE FILE (OUTDAT(I)) FROM (DRUZEI);
         IF AZ5 = '+' THEN Z = 0;
                      ELSE Z = 1;
               ZZ(I) = ZZ(I) + Z;
               END;
 M3:
         END;
                GOTO EOFPR;
 CL:      END;
