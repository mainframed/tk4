               2 FIL5 CHAR(55),
               2 #_TRKS_OFL BIT(16),
               2 OFLW_RCDS BIT(16),
               2 FULL_CYLOFL BIT(16),
               2 FIL6 CHAR(3),
               2 PTR CHAR(5);
         DCL STRING3 CHAR(140 ) STATIC;
 DCL     1 DSCB5 BASED (P003),
              2 FIL1 CHAR(4),
               2 EXTS(8),
                  3 TT BIT(16),
                   3 CYLS BIT(16),
                   3 TRKS BIT(8),
               2 TYP CHAR(1),
