 (SUBSCRIPTRANGE,SIZE):
 MAPVTOC: PROC OPTIONS(MAIN);
  ON SUBSCRIPTRANGE
 BEGIN;
  WRITE FILE(PRINT)FROM(FEME);
  DCL 1 FEME,
        2 MCHAR BIT(8) INIT('00001001'B),
        2 FEMA CHAR(22) INIT('*** SUBSCRIPTRANGE ***');
  PUT FILE(OUT) DATA(J); DCL OUT FILE PRINT;
 END;
 ON ZERODIVIDE BEGIN;
   WRITE FILE(PRINT) FROM(FEMS);
   DCL 1 FEMS,
      2 MCHAR BIT(8) INIT('00001001'B),
      2 FEMA CHAR(18) INIT('** ZERODIVIDE ***');
 END;
         ON ERROR BEGIN,.
            DISPLAY('***RZSYS10*** A B B R U C H'),.
            CALL PLIDUMP('HBS'),.
         END,.
 ON SIZE BEGIN;
   WRITE FILE(PRINT)FROM(FEMS);
   DCL 1 FEMS,
      2 MCHAR BIT(8) INIT('00001001'B),
      2 FEMA CHAR(18) INIT('*** SIZE ERROR ***');
 END;
 DCL     VDSCB1         EXTERNAL ENTRY,.
 DCL     VDSCB2         EXTERNAL ENTRY,.
 DCL     VDSCB3         EXTERNAL ENTRY,.
         DCL PRINT FILE RECORD OUTPUT;
         DCL 1 HEAD1 STATIC,
               2 CC CHAR(1) INIT('1'),
               2 FILL CHAR(64) INIT('                         LISTING O
 F VOLUME TABLE OF CONTENTS ON '),
                2 VOL_SER CHAR(6),
                2 FILL2 CHAR(5) INIT(' '),
               2 DATE,
                    3 MO PIC'Z9',
                   3 SS CHAR(1) INIT('/'),
                    3 DA PIC'Z9',
                   3 S2 CHAR(1) INIT('/'),
                    3 YR PIC'99',
                2 FILL3 CHAR(10) INIT('     TIME '),
               2 TIME PIC'Z9.99.99',
               2 FILL4 CHAR(19) INIT('              PAGE '),
               2 PAGE PIC'ZZ9' INIT(1);
         DCL 1 HEAD2 CHAR(90) INIT('                DATA SET NAME
           ORG.     EXTENTS       ALLOCATION      DATES') STATIC;
         DCL 1 LINE1 STATIC,
               2 CC CHAR(1) INIT('0'),
                2 NAME CHAR(44),
               2 FIL1 CHAR(1) INIT(' '),
                2 ORG CHAR(3),
               2 FIL2 CHAR(1) INIT(' '),
               2 EXTENT CHAR(19),
               2 FIL3 CHAR(4) INIT(' BY '),
               2 ALLOC_TYPE CHAR(10),
               2 FIL4 CHAR(3) INIT(' C-'),
                2 C_DATE_MO PIC'Z9',
         2 SLASH CHAR(1) INIT('/'),
               2 C_DATE_DAY PIC'Z9',
         2 SLSH CHAR(1) INIT('/'),
         2 C_DATE_YR PIC'99',
 2 FIL5 CHAR(4) INIT(' RF-'),
                 2 RECFM(4) CHAR(1),
                 2 FIL6 CHAR(10) INIT('  BLKSIZE-'),
                 2 BLKSI PIC'ZZZZ9',
                 2 FIL7 CHAR(9) INIT('   LRECL-'),
                 2 LRECL PIC'ZZZZ9';
         DCL 1 LINE2 STATIC,
                2 CC CHAR(7) INIT(' '),
                2 DATA CHAR(36),
               2 FIL1 CHAR(7) INIT(' '),
               2 EXTENT CHAR(19),
               2 FIL2 CHAR(09) INIT(' SEC.QTY '),
                2 SEC_QTY PIC'ZZZ9',
               2 FIL3 CHAR(4) INIT('  E-'),
               2 E_DATE_MO PIC'Z9',
               2 SLASH CHAR(1) INIT('/'),
               2 E_DATE_DAY PIC'Z9',
               2 SLSH CHAR(1) INIT('/'),
               2 E_DATE_YR PIC'99',
               2 FIL4 CHAR(2) INIT(' '),
               2 VARIES CHAR(35);
         DCL 1 LINE3 STATIC,
               2 FIL1 CHAR(50) INIT(' '),
               2 EXTENT CHAR(19),
               2 FIL3 CHAR(27) INIT(' '),
               2 VARYS CHAR(35);
         DCL 1 KEY_DATA DEF LINE2.VARIES,
                                       2 WD1 CHAR(7),
               2 KEYL PIC'ZZ9',
               2 WD2 CHAR(6),
               2 RKP PIC'ZZZ9',
               2 WD3 CHAR(8),
               2 OPTCD CHAR(7);
         DCL 1 SUMMARY_LINE STATIC,
               2 CC CHAR(1),
               2 FIL1 CHAR(10) INIT(' '),
               2 INFO CHAR(45);
         DCL 1 DSCB_LINE DEF SUMMARY_LINE.INFO,
               2 FIL1 CHAR(10),
               2 #DSCB PIC'ZZZ9',
               2 FIL2 CHAR(31);
         DCL TOT CHAR(4) DEF INFO;
         DCL 1 EXT_LIN DEF INFO,
               2 CNT PIC'Z9.B',
               2 CYLS PIC 'ZZ9',
               2 FIL1 CHAR(6),
               2 TRKS PIC'Z,ZZ9',
               2 FIL2 CHAR(7);
 DCL     1 DSCB1 BASED (P001),
                2 NAME CHAR(44),
                2 FIL1 CHAR(9),
               2 CYR BIT(8),
               2 CDD BIT(16),
               2 EYR BIT(8),
               2 EDD BIT(16),
               2 #EXT BIT(8),
                2 FIL2 CHAR(22),
                2 DSORG BIT(8),
                2 FIL3 CHAR(1),
                2 RECFORM BIT(8),
                2 FIL4 CHAR(1),
               2 BLKSZ BIT(16),
               2 LREC BIT(16),
               2 KEYL BIT(8),
               2 RKP BIT(16),
                2 DSIND BIT(8),
                2 ALPARM BIT(8),
               2 ALLQTY BIT(24),
               2 LBTT BIT(16),
               2 LBR BIT(8),
               2 LEFT BIT(16),
               2 FIL5 CHAR(2),
               2 EXTENT(3) LIKE EXTENTS1,
                2 DSCBPTR CHAR (5);
 DCL     1 DSCB3 BASED (P001),
                 2 FILL CHAR(4),
                 2 EXTENTS1(4:7),
                   3 IND BIT(8),
                   3 SEQ BIT(8),
                   3 CCL BIT(16),
                   3 HHL BIT(16),
                   3 CCH BIT(16),
                   3 HHH BIT(16),
                 2 TYPE CHAR(1),
               2 EXTENTS2(8:16) LIKE DSCB3.EXTENTS1,
                 2 FIL2 CHAR(5);
 DCL     1 DSCB2 BASED (P002),
               2 FILZ CHAR(44),
               2 TYPE CHAR(1),
