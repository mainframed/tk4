*                       AUSDRUCKEN.
*
JCLXREFP INIT  BASIS=(2,11)
*
         L     R1,0(R1)
         LH    R3,0(R1)
         BCTR  R3,R0
         STC   R3,MVCPARM+1
MVCPARM  MVC   PARM,2(R1)
*
         OPEN  (SYSUT1,,SYSPRINT,OUTPUT)
         CALL  UZED10A,(DATUM)
LESEN    EQU   *
         GET   SYSUT1
         LA    R1,44(R1)
         MVC   XLINE,0(R1)
*
         CLC   XDSN(2),=C'* '
         BE    LESEN
         CLC   XDSN(7),=C'SYSOUT '
         BE    LESEN
         CLC   XDSN(6),=C'DUMMY '
         BE    LESEN
         CLC   XDSN(5),=C'DATA '
         BE    LESEN
         CLC   XDSN(2),=C'  '
         BE    LESEN
         CLC   XDSN(1),=X'50'
         BE    LESEN
*
************************************
******** ZEILE AUFBEREITEN *********
************************************
*
         CLC   XGEN(1),=C' '
         BNE   GOGEN1
         CLC   ZWDSNAME,XDSN
         BNE   MOVEDSN1
         MVI   PDSNAME,C' '
         MVC   PDSNAME+1(43),PDSNAME
         B     MOVEZW
MOVEDSN1 EQU   *
         MVC   PDSNAME,XDSN
         B     MOVEZW
*
GOGEN1   EQU   *
         CLC   ZWDSNAME(30),XDSN
         BNE   MOVEDSN2
         MVI   PDSNAME,C' '
         MVC   PDSNAME+1(43),PDSNAME
         B     MOVEGEN1
MOVEDSN2 EQU   *
         MVC   PDSNAME,XDSN
MOVEGEN1 MVI   PDSNAME+34,C'('
         MVC   COUNT,MASKE
         ED    COUNT,XGEN+2
         MVC   PDSNAME+35(1),XGEN
         MVC   PDSNAME+36(4),COUNT
         MVI   PDSNAME+40,C')'
*
MOVEZW   MVC   ZWDSNAME,XDSN
*
*
GODISP   EQU   *
         LA    R3,XDISP
         LA    R4,3
         LA    R5,PDISP1
         LA    R6,DISPTAB
         LA    R7,10
         B     DISPL02
DISPL01  EQU   *
         LA    R5,5(R5)
         LA    R3,1(R3)
         LA    R6,DISPTAB
         LA    R7,10
DISPL02  EQU   *
         CLC   0(1,R3),0(R6)
         BE    DISPL03
         LA    R6,6(R6)
         BCT   R7,DISPL02
DISPL03  MVC   0(5,R5),1(R6)            DISPOSITION
         BCT   R4,DISPL01
*
*
         MVC   PUNIT,=6CL1' '
         LA    R3,UNITTAB
         LA    R4,9
GOUNIT   EQU   *
         CLC   XUNIT,0(R3)
         BE    MOVEUNIT
         LA    R3,7(R3)
         BCT   R4,GOUNIT
         B     GOSERIAL
MOVEUNIT EQU   *
         MVC   PUNIT,1(R3)              UNIT
*
*
GOSERIAL EQU   *
         CLI   XSER,C'('
         BNE   MOVESER
         MVC   PSER(6),=C'* ? * '
         B     MOVENAME
MOVESER  MVC   PSER,XSER                SERIAL
*
*
MOVENAME MVC   PMEMBER,XMEMB            MEMBER-NAME
         MVC   PJOBNAME,XJOBNM          JOB-NAME
         MVC   PSTEPNM,XSTEPNM          STEP-NAME
         MVC   COUNT,MASKE
         ED    COUNT,XSTEPCNT
         MVC   PSTEPCNT,COUNT+1         STEP-COUNT
         MVC   PPGMMBR,XPGMMBR          PGM-NAME  /  MBR-NAME
         MVC   PPROC,XPROC              PROCEDURE
*
*
         EJECT
PUTEN    EQU   *
         CP    ZZ,=P'0'
         BE    PUTTITEL
         CP    ZZ,=P'58'
         BH    PUTTITEL
         B     PUTLINE
PUTTITEL EQU   *
         ZAP   ZZ,=P'0'
         AP    SZ,=P'1'
         MVC   SEITE,MASKE
         ED    SEITE,SZ
         MVI   TCC,X'8B'
         PUT   SYSPRINT,TITEL
         MVI   TCC,X'09'
         PUT   SYSPRINT,TITEL
         PUT   SYSPRINT,BLANK
         PUT   SYSPRINT,BLANK
         PUT   SYSPRINT,TLINE
         PUT   SYSPRINT,BLANK
PUTLINE  EQU   *
         PUT   SYSPRINT,PLINE
         AP    ZZ,=P'1'
         B     LESEN
*
ENDLESEN EQU   *
         CLOSE (SYSUT1,,SYSPRINT)
ENDE2    L     R13,S0001+4
         L     R15,RETCODE
         RETURN (14,12),RC=(15)
         EJECT
SYSUT1   DCB   DDNAME=SYSUT1,DSORG=PS,MACRF=GL,EODAD=ENDLESEN
SYSPRINT DCB   DDNAME=SYSPRINT,DSORG=PS,MACRF=PM,                      *
               RECFM=FBM,LRECL=133,BLKSIZE=133
*
XLINE    DS  0CL110
XLCC     DS  CL1
XMEMB    DC  CL8' '
XJOBNM   DC  CL8' '
XSTEPNM  DC  CL8' '
XSTEPCNT DC  PL2'0'
*
XDDNM    DC  CL8' '
XDSN     DC  CL44' '
XGEN     DS  0CL4
         DC  CL1' '
         DC  PL3'0'
XDISP    DC  CL3' '
XUNIT    DC  CL1' '
XSER     DC  CL6' '
*
XPROC    DC  CL8' '
XPGMMBR  DC  CL8' '
XEMPTY   DC  CL2' '
***********************************************
