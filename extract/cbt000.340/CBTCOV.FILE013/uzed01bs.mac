PLINE    DS    0CL133
PLCC     DC    XL1'09'
PDSNAME  DS    CL44
PDISP1   DS    CL5
PDISP2   DS    CL5
PDISP3   DS    CL5
         DC    CL3' '
PUNIT    DS    CL6
         DC    CL1' '
PSER     DS    CL6
         DC    CL1' '
PMEMBER  DS    CL8
         DC    CL1' '
PJOBNAME DS    CL8
         DC    CL1' '
PSTEPNM  DS    CL8' '
         DC    CL2' ('
PSTEPCNT DS    CL3
         DC    CL2') '
         DC    CL1' '
PPGMMBR  DS    CL8
