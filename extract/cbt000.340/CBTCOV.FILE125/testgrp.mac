TESTGRP  $PROLOG
         LA    R1,=C'AUTH'
         SVC   231
         ICHEINTY LOCATE,TYPE='GRP',ENTRY=GRPLN,WKAREA=WK1,ACTIONS=ACT1
CHECK    DC    X'0700'
         $EPILOG
GRPLN    DC    X'04'
         DC    CL8'TS00'
ACT1     ICHEACTN MF=L,FIELD=ACL,TESTS=(TST1)
TST1     ICHETEST MF=L,FIELD=USERID,FLDATA=(8,ID),COND=EQ
ID       DC    CL8'TS52'
WK1      DC    A(256)
         DS    CL256
         END
