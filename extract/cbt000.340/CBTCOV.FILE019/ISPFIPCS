00010001PROC 0 DEBUG ID(&SYSUID.) NEW VOLUME() NOSPF MODE()
00020000 
00030000IF &DEBUG NE DEBUG THEN CONTROL NOLIST MSG PROMPT
00040000ELSE +
00050000   CONTROL LIST PROMPT CONLIST SYMLIST MSG
00060000 
00070002IF &MODE ^= THEN SET &M = &STR(MODE(&MODE))
00080002IF &VOLUME ^= THEN SET &V = &STR(VOLUME(&VOLUME))
00090002 
00100002%IPCS &DEBUG ID(&ID) &NEW &V &NOSPF &M
