00050003PROC 0 INDEX() OPT()
00070003SET DISP = NEW
00080007IF &SYSDSN('&SYSUID..SYSUCATP.&INDEX..LIST') = OK THEN SET DISP = OLD
00120006ALLOC FILE(SYSUCATP) DA('&SYSUID..SYSUCATP.&INDEX..LIST') +
00121006      &DISP. CAT REUSE +
00130002      RECFM(V B A) LRECL(255) BLKSIZE(3120) +
00140002      SPACE(1 1) CYL
