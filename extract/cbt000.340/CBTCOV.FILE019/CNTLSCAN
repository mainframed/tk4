00000103PROC 0
00000208   CONTROL MSG
00000300ISPEXEC VGET (CNTLDSN MEMBER)
00000404IF &MEMBER = THEN  ISPEXEC BROWSE DATASET(&CNTLDSN)
00000500             ELSE  GOTO USEMEM
00000600        EXIT
00000700USEMEM:    +
00000807   SET &DSNWORK = &CNTLDSN
00000907     ISPEXEC VPUT DSNWORK
00001009       ISPEXEC SELECT PGM(FIXUPDSN) PARM(DSNWORK)
00001107           ISPEXEC VGET DSNWORK
00002007               ISPEXEC BROWSE DATASET('&DSNWORK(&MEMBER)')
