00010000PROC 0 DEBUG
00020000IF &DEBUG = DEBUG THEN CONTROL LIST CONLIST PROMPT NOFLUSH SYMLIST MSG
00030000                  ELSE CONTROL MSG PROMPT
00200002ISPEXEC SELECT PGM(RJOBCARD)
00380000         ISPEXEC  FTOPEN   TEMP
00390000         ISPEXEC  FTINCL   DFREST
00400000         ISPEXEC  FTCLOSE
00410000         ISPEXEC  VGET     ZTEMPF
00420000         IF &DEBUG = DEBUG THEN LIST   '&ZTEMPF'
00430001                           ELSE %GENSUB '&ZTEMPF'
