00010000PROC     0        DEBUG
00020003 
00030000IF       &DEBUG = DEBUG +
00040000THEN     CONTROL   CONLIST NOFLUSH   LIST   MSG   PROMPT   SYMLIST
00050000ELSE     CONTROL NOCONLIST NOFLUSH NOLIST NOMSG   PROMPT NOSYMLIST
00060003 
00070000FREE     ATTRLIST(FBA VB) +
00080001         FILE(JES3JOB JES3LIB JES3MRCH SPOOL00 SPOOL01 SPOOL02 SPOOL03 SPOOL04 SPOOL05 SPOOL06 SPOOL07 +
00090000              SPOOL08 SPOOL09 SPOOL10 SPOOL11 SPOOL12 SPOOL13 SPOOL14 SPOOL15)
00100003 
