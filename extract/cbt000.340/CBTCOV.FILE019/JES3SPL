00120003 
00130000ISPEXEC  DISPLAY  PANEL(JES3SPL)
00140000SET      &JES3SPL = &LASTCC
00150003 
00160000IF       &JES3SPL ^= 0 +
00170000THEN     EXIT
00180000ATTRIB   FBA RECFM(F B A) LRECL(80) BLKSIZE(6160)
00190000ATTRIB   VB  RECFM(V B) LRECL(1896) BLKSIZE(6137)
00200002ALLOCATE FILE(JES3JOB) DATASET(JES3JOB.OUTLIST) NEW SPACE(1 2) CYLINDERS USING(VB)
00210000IF       &LASTCC ^= 0 +
00220002THEN     ALLOCATE FILE(JES3JOB) DATASET(JES3JOB.OUTLIST) OLD
00230003 
00240002ALLOCATE FILE(JES3MRCH) DATASET(JES3MRCH.OUTLIST) NEW SPACE(1) TRACK USING(FBA)
00250000IF       &LASTCC ^= 0 +
00260002THEN     ALLOCATE FILE(JES3MRCH) DATASET(JES3MRCH.OUTLIST) OLD
00270003 
