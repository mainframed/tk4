PROC 0  DEBUG
CONTROL NOMSG
IF &DEBUG = DEBUG THEN CONTROL LIST CONLIST MSG
FREE F(HELP LIB LCL3270)
CONTROL MSG
ALLOC F(LIB)    DA('APERFORM.RESOLVE.PARMXA') SHR
ALLOC F(HELP)   DA('APERFORM.RESOLVE.PARMXA') SHR
ALLOC F(LCL3270) DUMMY
@P RESOLVE
ISPEXEC CONTROL DISPLAY REFRESH
FREE F(HELP LIB LCL3270)
EXIT CODE(0)
END
