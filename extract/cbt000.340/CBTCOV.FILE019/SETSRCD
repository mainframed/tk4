00010016PROC 0 EXIST NOM
00020016   IF &EXIST = EXIST THEN GOTO RECALL
00030016   ISPEXEC VGET (PROJECT LIBRARY TYPE ASMDSN OBJDSN SYSLIB1 SYSLIB2 SYSLIB3 SYSLIB4 SYSLIB5 SYSLIB6 LCNT -
00040016                 INCR ANUMDEF SMPSRC SMPMAC ADEL XPROJ XLIB XTYPE CNTLDSN FMID SYSLIBS SYSLIBM SYSLIBO DISTLIBS -
00050016                 DISTLIBM DISTLIBO SLOAD LDSN1 LDSN2 LDSN3 LDSN4 LDSN5 SDSN1 SDSN2 SDSN3 PDSN ADSN -
00060017                 RIDSN TRACKDSN APPLNAME)
00070016   ISPEXEC VPUT (PROJECT LIBRARY TYPE ASMDSN OBJDSN SYSLIB1 SYSLIB2 SYSLIB3 SYSLIB4 SYSLIB5 SYSLIB6 LCNT -
00080016                 INCR ANUMDEF SMPSRC SMPMAC ADEL XPROJ XLIB XTYPE CNTLDSN FMID SYSLIBS SYSLIBM SYSLIBO DISTLIBS -
00090016                 DISTLIBM DISTLIBO SLOAD LDSN1 LDSN2 LDSN3 LDSN4 LDSN5 SDSN1 SDSN2 SDSN3 PDSN ADSN -
00100017                 RIDSN TRACKDSN APPLNAME) PROFILE
00110016   GOTO EXIT
00120016RECALL: +
00121016 ISPEXEC VGET ZAPPLID
00122016 IF &ZAPPLID ^= THEN GOTO EXIT
00130016   ISPEXEC VGET (PROJECT LIBRARY TYPE ASMDSN OBJDSN SYSLIB1 SYSLIB2 SYSLIB3 SYSLIB4 SYSLIB5 SYSLIB6 LCNT -
00140016                 INCR ANUMDEF SMPSRC SMPMAC ADEL XPROJ XLIB XTYPE CNTLDSN FMID SYSLIBS SYSLIBM SYSLIBO DISTLIBS -
00150016                 DISTLIBM DISTLIBO SLOAD LDSN1 LDSN2 LDSN3 LDSN4 LDSN5 SDSN1 SDSN2 SDSN3 PDSN ADSN -
00160017                 RIDSN TRACKDSN APPLNAME) PROFILE
00190016   ISPEXEC VPUT (PROJECT LIBRARY TYPE ASMDSN OBJDSN SYSLIB1 SYSLIB2 SYSLIB3 SYSLIB4 SYSLIB5 SYSLIB6 LCNT -
00200016                 INCR ANUMDEF SMPSRC SMPMAC ADEL XPROJ XLIB XTYPE CNTLDSN FMID SYSLIBS SYSLIBM SYSLIBO DISTLIBS -
00210016                 DISTLIBM DISTLIBO SLOAD LDSN1 LDSN2 LDSN3 LDSN4 LDSN5 SDSN1 SDSN2 SDSN3 PDSN ADSN -
00220017                 RIDSN TRACKDSN APPLNAME) SHARED
00230016EXIT: +
00240016   IF &NOM = NOM THEN EXIT
00250016   WRITE SOURCE VARIABLES INITIALIZED
