00010004PROC 2 PDSMEM PDSDSN
00020006/* PDS 8.2 INTERFACE TO USER %COBCL  CLIST BY M.THEYS ROCKWELL SWCC */
00030004CONTROL NOFLUSH NOMSG
00040004%COBCL &PDSMEM &PDSDSN
00050004SET RC = &LASTCC
00060005IF &RC = 12 THEN SET FOUND = &STR(CLIST MISSING?)
00070004SET PDSMSG2 = &STR(UT2: **> "%COBCL &PDSMEM &PDSDSN". &FOUND RC=&RC)
00080004ISPEXEC SETMSG MSG(PDSM001A)
00090004EXIT CODE(&STR(&RC))
