00010000PROC 1 OPT DEBUG
00020000IF &DEBUG = DEBUG THEN CONTROL LIST CONLIST SYMLIST NOFLUSH PROMPT MSG
00030000                  ELSE CONTROL NOLIST NOCONLIST NOSYMLIST FLUSH NOPROMPT NOMSG
00040000 
00050000IF &OPT = C THEN DO
00060001                 ISPEXEC BROWSE DATASET('JSERV.SHIFT.STATUS')
00070000                 END
00080000 
00090000IF &OPT = H THEN DO
00100001                 ISPEXEC BROWSE DATASET('JSERV.MONTHLY.STATUS')
00110000                 END
