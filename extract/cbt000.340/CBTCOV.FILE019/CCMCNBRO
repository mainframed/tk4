00010003PROC 0 DEBUG
00020003IF &DEBUG = DEBUG THEN CONTROL LIST CONLIST SYMLIST NOFLUSH PROMPT MSG
00030003                  ELSE CONTROL NOLIST NOCONLIST NOSYMLIST FLUSH NOPROMPT NOMSG
00040004ISPEXEC VGET CCMCNDSN
00050004ISPEXEC VPUT CCMCNDSN PROFILE
00051006CONTROL MSG
00060005%GENPRINT DSN(''&CCMCNDSN'') NODEL
00070004SET &EDSN = &CCMCNDSN
00080001ISPEXEC VPUT EDSN
00090001ISPEXEC SELECT PANEL(EMPTY)
