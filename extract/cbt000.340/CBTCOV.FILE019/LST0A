00010000PROC 0 PARM()
00020001 
00030001ISPEXEC VGET (LST0APRM) PROFILE
00040001 
00050003IF &STR(&PARM) = FULL AND &STR(&LST0APRM) =     THEN DO
00060003  SET &LST0APRM = FULL
00070003  ISPEXEC VPUT (LST0APRM) PROFILE
00080003  SET &LSTAPARM = &LST0APRM
00090001END
00100004ELSE SET &LSTAPARM = &STR(&LST0APRM &PARM)
00110001 
00120001ISPEXEC SELECT PGM(LST0A) PARM(&STR(&LSTAPARM))
