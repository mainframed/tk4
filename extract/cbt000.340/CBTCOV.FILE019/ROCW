00010000ISPEXEC CONTROL ERRORS RETURN
00020000SET P = 1
00030002DO WHILE(&P <= 31)
00040000  ISPEXEC CONTROL DISPLAY LOCK
00050000  ISPEXEC DISPLAY PANEL(ROCW&P)
00060000  SET P = &P + 1
00070000END
00080001SET K = 1
00090001DO WHILE(&K <= 3)
00100001  ISPEXEC CONTROL DISPLAY LOCK
00110002  ISPEXEC DISPLAY PANEL(ROCW31)
00111001  ISPEXEC CONTROL DISPLAY LOCK
00112001  ISPEXEC DISPLAY PANEL(ROCW1)
00120001  SET K = &K + 1
00130001END
