00010000PROC 0
00020000CONTROL NOMSG
00030000   QSPF
00040000   IF &LASTCC = 4 THEN ISPEXEC  BROWSE DATASET('ATSO.SPFNEWS')
00050000                  ELSE DO
00060000                          SETSCRN
00070000                          REVIEW 'ATSO.SPFNEWS'
00080000                          SETSCRN
00090000                       END
