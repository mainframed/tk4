00011000PROC 0 EXIST NOM
00020000  IF &EXIST = EXIST THEN GOTO RECALL
00030000  ISPEXEC VGET (SUBSYS CDSDSN CSIZONE PTSDSN PTSUNIT PTSVOL EXITOPT ENQDSN -
00040000                LPREFIX MSVGP SPRIM SSEC CCMCDSN)
00050000  ISPEXEC VPUT (SUBSYS CDSDSN CSIZONE PTSDSN PTSUNIT PTSVOL EXITOPT ENQDSN -
00060000                LPREFIX MSVGP SPRIM SSEC CCMCDSN) PROFILE
00070000  GOTO EXIT
00080000RECALL:  +
00081000  ISPEXEC VGET ZAPPLID
00082000  IF &ZAPPLID ^= THEN GOTO EXIT
00090000  ISPEXEC VGET (SUBSYS CDSDSN CSIZONE PTSDSN PTSUNIT PTSVOL EXITOPT ENQDSN -
00100000                LPREFIX MSVGP SPRIM SSEC CCMCDSN) PROFILE
00110000  ISPEXEC VPUT (SUBSYS CDSDSN CSIZONE PTSDSN PTSUNIT PTSVOL EXITOPT ENQDSN -
00120000                LPREFIX MSVGP SPRIM SSEC CCMCDSN) SHARED
00130000EXIT:  +
00130100  IF &NOM = NOM THEN EXIT
00131000  WRITE SMP VARIABLES INITIALIZED
