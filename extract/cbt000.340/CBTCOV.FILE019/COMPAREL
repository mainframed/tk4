00260000                END
00270001IF &REPORT = B THEN DO
00280006                    %GENPRINT DSN(COMPAREL.REPORT)
00290000                    END
00300009CONTROL NOMSG NOPROMPT
00310007FREE F(WTO I1 I2 SYSPRINT SNAP )
