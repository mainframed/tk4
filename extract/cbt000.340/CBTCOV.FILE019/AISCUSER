00010005  PROC 0 QUICK
00020000  CONTROL NOFLUSH NOMSG
00030002  SHOWTSO NAME FDS DEPT GROUP
00040006  $C SAFECOPY FROMDSNAME(*) TODATASET('AISC.AOST.USERS') NONOTIFY MOD
00050002  &STR(&SYSDATE &SYSTIME BY &SUBSTR(1:7,&SYSUID       ) &NAME   &FDS &DEPT &GROUP)
00060000  END
00070005  /*  FREE DA('AISC.AOST.USERS') */
