00010003PROC 1 JOB DEBUG
00020000IF &DEBUG = DEBUG THEN CONTROL LIST CONLIST SYMLIST NOFLUSH PROMPT MSG
00030000                  ELSE CONTROL NOLIST NOCONLIST NOSYMLIST FLUSH NOPROMPT NOMSG
00040000 
00050002ISPEXEC DISPLAY PANEL(SDF3REQ)
00060002IF &LASTCC ^= 0 THEN EXIT
00070002 
00080002IF &NEWCLASS ^= THEN SET &A = &STR(NEWCLASS(&NEWCLASS))
00090002IF &NEWDEST  ^= THEN SET &B = &STR(DEST(&NEWDEST))
00100002 
00110002OUT &JOB &A &B
