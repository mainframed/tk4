00018750/*                                                                 */
00018760/* CLEAN WAY TO LOGOFF. KEEPS PREFIX THE WAY IT SHOULD BE.         */
00018770/*                                                                 */
00018780IF &SYSPREF NE &SYSUID THEN +
00018790DO
00018800  WRITE PREFIX RESET FROM &SYSPREF TO &SYSUID
00018810  PROF PREF(&SYSUID)
00018820END
00018830PROFILE NOMSGID
00018840TIME
00018850PROFILE MSGID
