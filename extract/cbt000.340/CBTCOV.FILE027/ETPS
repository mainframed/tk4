00010000PROC 2 TERM ADDR OPT(Y)
00010102CONTROL LIST CONLIST SYMLIST
00011002 /*
00012002 /*  VALID TERM VALUES ARE:
00013002 /*  2 = 3278 MODEL 2
00014002 /*  A = 3278 MODEL 2A
00015002 /*  3 = 3278 MODEL 3B
00016002 /*  4 = 3278 MODEL 4
00017002 /*
00020000IF &OPT = Y THEN +
00030002OSCMD S TSSOSRV,SUB=MSTR,CLIST='%ETPS &TERM &ADDR OPT(X)'
00040000IF &OPT = X THEN +
00050000@P ETPS 'ETPS&TERM&ADDR'
