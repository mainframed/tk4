00010000PROC 1 WTRNAME DEBUG
00020000 
00030000IF &DEBUG = DEBUG THEN CONTROL LIST CONLIST SYMLIST
00040000 
00050001SHOWTSO SYSID
00060000 
00070001SET CPUMODE = &SUBSTR(1:1,&SYSID )  /* EXTRACT CPU PROCESSING MODE */
00080000 
00090000IF &CPUMODE = 9 THEN EXIT           /* IF CLASSIFIED OR DOD THEN EXIT */
00100000 
00110000IF SAS = &SUBSTR(1:3,&WTRNAME  ) THEN EXIT
00120000 
00130000OSCMD P &WTRNAME
