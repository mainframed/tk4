//TEC3166P JOB (T369055000),'BEN/PLOUGH',CLASS=Z,MSGCLASS=X,
//             NOTIFY=TEC3166,RESTART=REST2
/*JOBPARM ROOM=03,S=G081
//REST1    EXEC PGM=IEBCOPY
//*
//*        COPY MY DATASETS
//*
//SYSPRINT DD SYSOUT=*
//DISK1    DD DSN=TEC3166.CHANGES,UNIT=TEMP,DISP=(,CATLG),
//            SPACE=(CYL,(20,5,200))    19CYL 126DIR 766MEM
//DISK2    DD DSN=TEC3166.TSLOAD,UNIT=TEMP,DISP=(,CATLG),
//            SPACE=(CYL,(10,5,90))     10CYL  53DIR 332MEM
//TAPE1    DD DSN=SYS1.IPO.CHANGES,
//            UNIT=TAPE,VOL=(,RETAIN,,SER=BENTAP),
//            DISP=OLD,LABEL=(2,NL,EXPDT=98000)
//TAPE2    DD DSN=SYS1.TSLOAD,
//            UNIT=TAPE,VOL=(,RETAIN,,SER=BENTAP),
//            DISP=OLD,LABEL=(4,NL,EXPDT=98000)
//SYSIN    DD  *
 COPY I=TAPE1,O=DISK1
 COPY I=TAPE2,O=DISK2
//REST2    EXEC PGM=IEBCOPY
//*
//*        COPY PLOUGH'S ADVENT
//*
//SYSPRINT DD SYSOUT=*
//DISK1    DD DISP=OLD,DSN=SHARE.PLOUGH.FILE1
//TAPE1    DD DSN=ADVENT.TAPE,
//            UNIT=TAPE,VOL=(,RETAIN,,SER=ADVENT),
//            DISP=OLD,LABEL=(1,SL,EXPDT=98000)
//SYSIN    DD  *
 COPY I=TAPE1,O=DISK1
//TAPESCAN PROC T=TAPEIN,CLASS='*',TMS=98000,LBL=1,BLP=BLP,
//  NAME=TAPESCAN
//*
//*  THE PROCEDURE PARAMETERS ARE EXPLAINED BELOW (NONE ARE REQUIRED):
//*  T      =  THE VOLUME SERIAL OF THE INPUT TAPE. DEFAULT IS TAPEIN.
//*  CLASS  =  THE PRINT OUTPUT CLASS. DEFAULT IS '*'.
//*  TMS    =  USED FOR TMS. DEFAULT IS 98000.
//*  LBL    =  STARTING DATASET LOCATION FOR SCAN. DEFAULT IS 1.
//*  BLP    =  DEFAULT IS BLP. SPECIFY SL WHEN USING THE COPY OPTION
//*            ON A STANDARD LABEL TAPE.
//*
//*  WHEN USING THE COPY OPTION AN OUTPUT DD CARD MUST BE PROVIDED.
//* EXAMPLE:
//*  //   EXEC  TAPESCAN,PARM=(NOLIST,COPY,MAXEOV2,SKIPEOV1),T=T9999
//*  //OUTPUT DD DSN=LOSTDATA,DISP=(,KEEP),UNIT=TAPE,VOL=SER=T8888
//* JOB TO COPY DATA BETWEEN FIRST EOV AND SECOND EOV.
//*
//SCAN    EXEC PGM=TAPESCAN
//STEPLIB  DD  DSN=SHARE.FILE050.LOAD,DISP=SHR
//SYSPRINT DD  SYSOUT=&CLASS
//INPUT    DD  DSN=&NAME,DISP=OLD,UNIT=TAPE,VOL=SER=&T,
//             LABEL=(&LBL,&BLP,EXPDT=&TMS)
//        PEND
//COPY1   EXEC TAPESCAN,PARM.SCAN='NOLIST,COPY',T=ADVENT
//OUTPUT   DD  DSN=TAPE.COPY1,DISP=(,KEEP),UNIT=TAPE,VOL=SER=COPY1,
//             LABEL=(1,BLP,EXPDT=98000)
