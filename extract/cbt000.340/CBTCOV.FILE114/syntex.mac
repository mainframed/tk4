//TEC3166S JOB (T369057000),'BEN/SYNTEX',CLASS=Z,MSGCLASS=X,
//             NOTIFY=TEC3166
/*JOBPARM R=103
/*SETUP SYNTEX - TAPE SHOULD BE AT TAPE CONSOLE
//*
//*        COPY SYNTEX MODS
//*
//KILL    EXEC SAS
//FILE1    DD  DSN=SHARE.SYNTEX.FILE1,DISP=OLD
//FILE2    DD  DSN=SHARE.SYNTEX.FILE2,DISP=OLD
//FILE3    DD  DSN=SHARE.SYNTEX.FILE3,DISP=OLD
//SYSIN    DD  *
PROC PDS DDNAME=FILE1 NOLIST KILL;
PROC PDS DDNAME=FILE2 NOLIST KILL;
PROC PDS DDNAME=FILE3 NOLIST KILL;
//IEBCOPY EXEC PGM=IEBCOPY
//SYSPRINT DD  SYSOUT=*
//IN1      DD  DSN=FILE1,UNIT=TAPE,VOL=SER=SYNTEX,DISP=(,PASS),
//             LABEL=(1,NL,EXPDT=98000)
//IN2      DD  DSN=FILE2,UNIT=TAPE,VOL=SER=SYNTEX,DISP=(,PASS),
//             LABEL=(2,NL,EXPDT=98000)
//IN3      DD  DSN=FILE3,UNIT=TAPE,VOL=SER=SYNTEX,DISP=(,KEEP),
//             LABEL=(3,NL,EXPDT=98000)
//OUT1     DD  DSN=SHARE.SYNTEX.FILE1,DISP=OLD
//OUT2     DD  DSN=SHARE.SYNTEX.FILE2,DISP=OLD
//OUT3     DD  DSN=SHARE.SYNTEX.FILE3,DISP=OLD
//SYSIN    DD  *
 C I=IN1,O=OUT1
 C I=IN2,O=OUT2
 C I=IN3,O=OUT3
