PROC 0 PREFIX(&SYSPREF) CONFIRM(Y) DFORM(L) DELAY(20) MENU TRACE
/************ A HEADER DESCRIBING THIS CLIST IS AT THE BOTTOM *******
  CONTROL NOMSG  NOFLUSH
  ISPEXEC VPUT (DELAY TRACE) SHARED
  SET CONTR = RETURN
    IF &TRACE = TRACE THEN DO
      CONTROL MSG,LIST,CONLIST,SYMLIST
      SET CONTR = CANCEL
    END
    ISPEXEC CONTROL ERRORS &CONTR

    SET CONT = 1
    DO WHILE (&CONT = 1)
      IF &MENU = MENU THEN DO
        ISPEXEC VGET (PREFIX CONFIRM DFORM) ASIS
        ISPEXEC DISPLAY PANEL(DS@FLSTM)
        IF &LASTCC ^= 0 THEN  +
           SET CONT = 0
        ELSE  +
           SET CONT = 1
      END
      ELSE  +
        ISPEXEC VPUT (PREFIX DFORM CONFIRM) SHARED
      IF &CONT = 1 THEN DO
        ISPEXEC VPUT (PREFIX) SHARED
        ISPEXEC SELECT PGM(DS@FLSTC)
        IF &MENU = THEN  +
          SET CONT = 0
      END
    END
  END
EXIT:    +
  EXIT
