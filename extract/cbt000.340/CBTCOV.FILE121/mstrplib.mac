./ ADD  NAME=RESCUE
%------------------ &YGROUP PRIMARY OPTION MENU ------------------------
%SELECT OPTION ===>_OPT     +
%
%   0 +SPF PARMS  - SPECIFY TERMINAL AND SPF PARAMETERS     +USERID   -
%   1 +BROWSE     - DISPLAY SOURCE DATA OR OUTPUT LISTINGS  +PREFIX   -
%   2 +EDIT       - CREATE OR CHANGE SOURCE DATA            +DATE     -
%   3 +UTILITIES  - PERFORM SPF UTILITY FUNCTIONS           +TIME     -
%   4 +FOREGROUND - INTERACTIVE PROCESSING                  +TERMINAL -
%   5 +BACKGROUND - BATCH JOB SUBMISSION                    +PF KEYS  -
%  &Y1+&Y2
%  &Y4+&Y5
%   E +EXTENSIONS - &YGROUP EXTENSIONS TO SPF
%   T +TUTORIAL   - DISPLAY INFORMATION ABOUT SPF
%   X +EXIT       - TERMINATE SPF USING LIST/LOG DEFAULTS
%
+PRESS%END KEY+TO TERMINATE SPF+
)INIT
/*     FOR A DESCRIPTION OF OPTIONS, SEE COMMENTS BELOW              */
  .HELP = TTUTOR
  &ZHTOP = TTUTOR     /* TUTORIAL TABLE OF CONTENTS */
  &ZHINDEX = TINDEX   /* TUTORIAL INDEX - 1ST PAGE  */
  /*******************************************************************/
  /*                                                                 */
  /*    THE FOLLOWING CODE TAILORS THE PRIMARY OPTION MENU BY        */
  /*    GROUP.  CERTAIN FUNCTIONS (E.G. OPTIONS 6 AND 7) ARE         */
  /*    ALLOWED FOR SOME USERS AND NOT OTHERS.                       */
  /*                                                                 */
  /*    ADDITIONALLY, THE VARIABLE NAMES &YCLASS AND &YGROUP         */
  /*    ARE DERIVED FROM THE 2ND-4TH CHARACTERS OF THE               */
  /*    LOGON PROC IN ORDER TO DETERMINE AUTHORITY.                  */
  /*                                                                 */
  /*    NOTE: ANY VARIABLE SET IN THIS MENU, (OR ANY MENU), BECOMES  */
  /*          PART OF THE 'SHARED VARIABLE POOL'. THE 'Y' VARAIBLES  */
  /*          ARE USED GLOBALLY THROUGHOUT THE MENUS.                */
  /*                                                                 */
  /*******************************************************************/
  &YCLASS = PDA             /* SET DEFAULTS IN CASE INVALID PROC NAME*/
  &YGROUP = SPF             /* SET DEFAULTS IN CASE INVALID PROC NAME*/
  &Y1 = ' 6'                /* ALLOW THE USE OF OPTION-6             */
  &Y2 = 'COMMAND    - ENTER TSO COMMAND OR CLIST'
  &Y3 = 'ISPTSO'            /* ALLOW THE USE OF OPTION-6             */
  &Y4 = ' 7'                /* ALLOW THE MENU TESTING FACILITY       */
  &Y5 = 'SUPPORT    - TEST DIALOG OR CONVERT MENU/MESSAGE FORMATS'
  &Y6 = 'ISPQTAC'           /* USER, ALLOW THE MENU TESTING FACILITY */

  &X = TRUNC(&ZLOGON,1)     /* ISOLATE THE 1ST CHAR FOR VALIDATION   */
  IF (&X = '@')             /* IS THIS USER FOLLOWING OUR RULES?     */

    &YCLASS = TRUNC(&ZLOGON,4) /* ISOLATE THE 1ST 4 CHARS OF PROC    */
                            /* TO ISOLATE CLASS OF USER              */
    &YCLASS = TRANS(&YCLASS @PDA,PDA @PD1,PD1 @PD2,PD2 @PD3,PD3 *,*)
    &YCLASS = TRANS(&YCLASS @SEC,SEC @TPS,TPS @CIC,CIC @IMS,IMS *,*)
    &YCLASS = TRANS(&YCLASS @MVS,MVS @AUD,AUD @OPR,OPR *,*)
    /* ************************  ADDITIONAL GROUPS MAY BE ADDED HERE */

    &YGROUP = TRUNC(&YCLASS,2) /* ISOLATE THE 1ST 2 CHARS OF CLASS   */
                            /* TO DEVELOP THE GROUP NAME FOR MENUS   */
    &YGROUP = TRANS(&YGROUP PD,PROGRAM-DEVELOPMENT *,*)
    &YGROUP = TRANS(&YGROUP SE,SECURITY *,*)
    &YGROUP = TRANS(&YGROUP TP,TP-SYSTEM-PROGRAMMER *,*)
    &YGROUP = TRANS(&YGROUP CI,CICS-SYSTEM-PROGRAMMER *,*)
    &YGROUP = TRANS(&YGROUP IM,IMS-SYSTEM-PROGRAMMER *,*)
    &YGROUP = TRANS(&YGROUP MV,MVS-SYSTEM-PROGRAMMER *,*)
    &YGROUP = TRANS(&YGROUP AU,AUDITORS *,*)
    &YGROUP = TRANS(&YGROUP OP,OPERATIONS *,*)
    /* ************************  ADDITIONAL GROUPS MAY BE ADDED HERE */


    /* THIS SECTION ALLOWS/DISALLOWS CERTAIN FUNCTIONS BY GROUP      */

      &YEXT = &YCLASS       /* SET DEFAULT PREFIX FOR EXTENSIONS     */
      &YFOR = CMN           /* SET DEFAULT PREFIX FOR FORGROUND      */
      &YBAT = CMN           /* SET DEFAULT PREFIX FOR BACKGROUND     */

      IF (&YCLASS = PDA)    /* IF THIS IS A PROGRAM DEVEL USER       */
        &YEXT = CMN         /* USE TO PROGRAM DEVEL EXTENSION        */
        &Y1 = ' '           /* DISALLOW THE USE OF OPTION-6          */
        &Y2 = ' '           /* DISALLOW THE USE OF OPTION-6          */
        &Y3 = ' '           /* DISALLOW THE USE OF OPTION-6          */
        &Y3 = ' '           /* DISALLOW THE USE OF OPTION-7          */
        &Y4 = ' '           /* DISALLOW THE USE OF OPTION-7          */
        &Y5 = ' '           /* DISALLOW THE USE OF OPTION-7          */


)PROC
  &SEL = TRANS( TRUNC (&OPT,'.')
                0,'PANEL(ISPOPT)'
                1,'PGM(ISPBRO)'
                2,'PGM(ISPEDIT)'
                3,'PANEL(ISPUTIL)'
                4,'PANEL(&YFOR.@F00)'
                5,'PANEL(&YBAT.@B00)'
                6,'PGM(&Y3)'
                7,'PANEL(&Y6)'
                E,'PANEL(&YEXT.@E00)'
                T,'PGM(ISPTUTOR) PARM(T)'
                ' ',' '
                X,'EXIT'
                *,'?' )
)END
