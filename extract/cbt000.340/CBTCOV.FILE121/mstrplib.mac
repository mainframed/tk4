+ SOURCE NAME%===>_IMSEMM6 + IF BLANK, THEN MEMBER NAME IS USED

+ DISPLAY    %===>_IMSEMM8 + (NO/EDIT/SPF) {IMSEMM18         +
+ NEW    NAME%===>_IMSEMM9 + (IF SPF REQUEST) IF BLANK, THEN MEMBER NAME
+ MACRO  GEN %===>_IMSEMM10+ (YES/NO){IMSEMM19         +
+ MACRO NAME %===>_IMSEMM11+ IF BLANK, THEN MEMBER NAME SUFFIXED WITH A

+ FORMAT GEN %===>_IMSEMM13+ (YES/NO){IMSEMM20         +

+
%MFS OUTPUT OPTIONS:+ MACRO  GEN  :%===>_IMSEMM15+ (LISTID)
+                     FORMAT GEN  :%===>_IMSEMM16+ (LISTID)
+                      %(ONLY IF OUTPUT IS TO BE SAVED)
)INIT
  &IMSEMM5 = TRANS (&IMSEMM5 Y,YES  N,NO *,*)
  &IMSEMM8 = TRANS (&IMSEMM8 S,SPF  N,NO  E,EDIT *,*)
  &IMSEMM10 = TRANS (&IMSEMM10 Y,YES  N,NO  *,*)
  &IMSEMM13 = TRANS (&IMSEMM13 Y,YES  N,NO  *,*)
  &IMSEMM6  = ' '
  &IMSEMM9 = ' '
  &IMSEMM11 = ' '
  &IMSEMM15 = ' '
  &IMSEMM16 = ' '
)PROC
  &IMSEMM7  = 'IMSVS.MFS.DATA'
  &IMSEMM12 = 'IMSVS.MFS.MACROS'
  &IMSEMM14 = 'IMSVS.TFORMAT'
  VER (&IMSEMM1,NONBLANK)
  VER (&IMSEMM2,NONBLANK)
  VER (&IMSEMM3,NONBLANK)
  VER (&IMSEMM4,NONBLANK)
  IF  (&IMSEMM6 = ' ')
      &IMSEMM6 = &IMSEMM4
  IF  (&IMSEMM9 ^= ' ')
      IF  (&IMSEMM11 = ' ')
          &IMSEMM11 = TRUNC (&IMSEMM9,' ')
          &IMSEMM11 = '&IMSEMM11.M'
  IF  (&IMSEMM11 = ' ')
      &IMSEMM11 = TRUNC (&IMSEMM4,' ')
      &IMSEMM11 = '&IMSEMM11.M'
  IF  (&IMSEMM9 = ' ')
      &IMSEMM9 = &IMSEMM4
  &IMSEMM5 = TRUNC (&IMSEMM5,1)
  &IMSEMM5 = TRANS (&IMSEMM5 Y,Y  N,N  *,N)
  &IMSEMM8 = TRUNC (&IMSEMM8,1)
  VER (&IMSEMM8,LIST,N,E,S)
  &IMSEMM10 = TRUNC (&IMSEMM10,1)
  &IMSEMM10 = TRANS (&IMSEMM10 Y,Y N,N *,N)
  &IMSEMM13 = TRUNC (&IMSEMM13,1)
  &IMSEMM13 = TRANS (&IMSEMM13 Y,Y N,N *,N)
  &IMSEMM17 = TRANS (&IMSEMM17 *,' ')
  &IMSEMM18 = TRANS (&IMSEMM18 *,' ')
  &IMSEMM19 = TRANS (&IMSEMM19 *,' ')
  &IMSEMM20 = TRANS (&IMSEMM20 *,' ')
  &IMSEMM15 = TRANS (&IMSEMM15 ' ',NULL *,*)
  &IMSEMM16 = TRANS (&IMSEMM16 ' ',NULL *,*)
)END
