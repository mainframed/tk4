//FORTTEST JOB (0496,CO26,0,9),AUSTIN,NOTIFY=AUSTIN
/*ROUTE PRINT REMOTE99
//FORTHCLG PROC RGN=600K
//FORT    EXEC PGM=IFEAAB,REGION=&RGN.,PARM=GOSTMT
//STEPLIB   DD DISP=SHR,DSN=STC1.FORTHX.COMPILER
//SYSLIN    DD DISP=(MOD,PASS),DSN=&&LOADSET,SPACE=(CYL,2),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=800),UNIT=WORK
//SYSTERM   DD SYSOUT=*
//SYSUT1    DD UNIT=WORK,SPACE=(3465,(3,3)),DCB=BLKSIZE=3465
//SYSUT2    DD UNIT=WORK,SPACE=(2048,(10,10))
//SYSPRINT  DD SYSOUT=*
//LKED    EXEC PGM=IEWL,REGION=&RGN,COND=(9,LT,FORT),
//             PARM='LET,LIST,XREF,MAP'
//SYSLIB    DD DISP=SHR,DSN=STC1.FORTHX.RESIDENT
//          DD DISP=SHR,DSN=STC2.SUBRTNS.NCAL
//SYSLIN    DD DISP=(OLD,DELETE),DSN=&&LOADSET
//          DD DDNAME=SYSIN
//SYSLMOD   DD DISP=(,PASS),DSN=&&L(FGGO),
//             SPACE=(CYL,(3,1,1)),UNIT=WORK
//SYSUT1    DD DISP=(,DELETE),SPACE=(CYL,10),UNIT=WORK
//SYSPRINT  DD SYSOUT=*
//GO      EXEC PGM=*.LKED.SYSLMOD,REGION=&RGN,
//             COND=((9,LT,FORT),(9,LT,LKED))
//STEPLIB DD DISP=SHR,DSN=STC1.FORTRAN.TRANSENT
//FT03F001  DD SYSOUT=*,DCB=(RECFM=VBA,LRECL=121,BLKSIZE=6233)
//FT01F001  DD DDNAME=SYSIN
//          PEND
//*
//FORTTEST EXEC FORTHCLG
//*
//FORT.SYSIN DD *
      CALL PLOTS(0,0,0)
      CALL SYMBOL( 0.0, 0.0, .25, 6HCIRCLE,   0.0, 6 )
      CALL SYMBOL( 0.0, 7.0, .25, 6HCIRCLE, -90.0, 6 )
      CALL PLOT(  2.50, 2.50,-3)
      CALL PLOT( -2.50, 0.00, 3)
      CALL PLOT(  2.50, 0.00, 2)
      CALL PLOT(  0.00,-2.50, 3)
      CALL PLOT(  0.00, 2.50, 2)
      CALL PLOT(  2.00, 0.00, 3)
      THETA = 0.0
 1    CONTINUE
      THETA = THETA + 0.001
      X = 2 * COS(THETA)
      Y = 2 * SIN(THETA)
      CALL PLOT( X, Y, 2 )
      IF ( THETA .LT. 6.2832 ) GOTO 1
C
      CALL PLOT( 0.0, 0.0, 999 )
      STOP
      END
//*
//LKED.PLTLIB DD DISP=SHR,DSN=AUSTIN.PAGEPLOT.NCAL
//*
  INCLUDE PLTLIB(I38INIT,I38PAGE,PLTPLOT,PLTSYMB,PLTMSG)
//*
//GO.IBM3800 DD SYSOUT=(A,,1030),CHARS=PLOT,FCB=12,
//           DCB=BLKSIZE=151
//SYSUDUMP DD SYSOUT=*
