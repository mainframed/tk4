//SASTEST JOB (0496,BX99,0,20,,1030),AUSTIN,NOTIFY=AUSTIN
//*
//SAS     PROC RELEASE='STC2.SAS',
//*********************************************************************
//* NAME:      SAS                                                    *
//*            EXECUTE STATISTICAL ANALYSIS SYSTEM                    *
//* END                                                               *
//*********************************************************************
// CYL=20,                     CYLINDERS FOR WORK FILE
// DBMS='STC2.STUB',           FOR RAPID/SAS INTERFACE
// INCL='STC2.STUB',           SAS SOURCE LIBRARY FOR %INCLUDE INCL(?);
// MAINT='STC2.STUB',          DSN ROOT OF MAINTENANCE LIBRARIES
// OPTIONS=,                   OPTIONS FOR SAS
// RGN=512K,                   REGION FOR PROGRAM TO EXECUTE
// ROOT='STC2.STUB',           DSN ROOT OF USER LIBRARIES
// SORTLIB='STC1.SYNCSORT',    LOAD LIBRARY FOR SORT UTILITY
// TRNSNT='STC1.PLIO'          PLI TRANSIENT LIBRARY
//*********************************************************************
//* STEP: SAS - EXECUTE SAS UTILITY                                   *
//*********************************************************************
//SAS     EXEC PGM=SASLOAD,REGION=&RGN.,
//             PARM='0,STCNEWS,SASLPA%&OPTIONS.'
//LIBRARY   DD UNIT=WORK,SPACE=(CYL,(1,1,10))          FOR PROC FORMAT
//STEPLIB   DD DSN=&ROOT..LOAD,DISP=SHR
//          DD DSN=&MAINT..LOAD,DISP=SHR
//          DD DSN=&RELEASE..LOAD,DISP=SHR
//          DD DSN=*.LIBRARY,DISP=(OLD,PASS),VOL=REF=*.LIBRARY
//          DD DSN=&DBMS..LOAD,DISP=SHR
//          DD DSN=&SORTLIB,DISP=SHR
//          DD DSN=&TRNSNT..TRANSENT,DISP=SHR
//INCL      DD DSN=&INCL..INCL,DISP=SHR
//          DD DSN=&MAINT..INCL,DISP=SHR
//          DD DSN=&RELEASE..INCL,DISP=SHR
//SAMPLE    DD DSN=&RELEASE..SAMPLE.CNTL,DISP=SHR         235 EXAMPLES
//NEWSIN    DD DSN=&RELEASE..HELP(SASNEWS),DISP=SHR
//NEWSOUT   DD SYSOUT=*
//FT06F001  DD SYSOUT=*,DCB=(RECFM=FA,LRECL=133)          FORT MESSAGES
//FT11F001  DD SYSOUT=*,DCB=(RECFM=VA,LRECL=137)          LOG
//FT12F001  DD SYSOUT=*,DCB=(RECFM=VA,LRECL=137)          PRINT
//FT13F001  DD SYSOUT=B                                   PUNCH
//FT15F001  DD UNIT=WORK,SPACE=(CYL,(1,1)),               PARMCARDS
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=6160)
//WORK      DD UNIT=WORK,SPACE=(CYL,(&CYL,&CYL))          SAS WORK
//* THE FOLLOWING DATASETS ARE USED BY SAS/GRAPH
//* TO USE DEVICES CODE %INCLUDE INCL(DEV); IN YOUR SAS PROGRAM
//FT31F001  DD SYSOUT=(A,,9400),CHARS=POH1,               DEV=STCPOH1
//             DCB=(RECFM=FA,LRECL=153)
//FT32F001  DD SYSOUT=(A,,9400),CHARS=POH2,               DEV=STCPOH2
//             DCB=(RECFM=FA,LRECL=153)
//IBM3800   DD SYSOUT=(A,,9400),CHARS=PLOT,               DEV=STC3800
//             DCB=(RECFM=FA,LRECL=151),FCB=12
//XROX9700  DD SYSOUT=X,DCB=(RECFM=FA,LRECL=151)          DEV=STC9700
//MAP       DD DSN=&RELEASE..MAP,DISP=SHR                 PROC GMAP
//************ END OF PROCEDURE: SAS **********************************
//     PEND
//*
//LKED    PROC RGN=150K
//**********************************************************
//* FUNCTION:  TO EXECUTE LINKAGE EDITOR.                  *
//**********************************************************
//*********************** PARAMETERS ***********************
//* RGN=       OPTIONAL.                                   *
//*            REGION SIZE FOR STEP.                       *
//*            DEFAULTS TO RGN=150K                        *
//**********************************************************
//LKED    EXEC PGM=IEWL,REGION=&RGN,PARM='LIST,LET'
//SYSLIB    DD DISP=SHR,DSN=STC1.COBOL.RESIDENT
//          DD DISP=SHR,DSN=STC1.FORTHX.RESIDENT
//          DD DISP=SHR,DSN=STC1.PLIO.RESIDENT
//          DD DISP=SHR,DSN=STC2.SUBRTNS.NCAL
//SYSLIN    DD DDNAME=SYSIN
//SYSUT1    DD DISP=(,DELETE),SPACE=(CYL,10),UNIT=WORK
//SYSLMOD   DD DISP=(,PASS),DSN=&&L(LFGO),
//             SPACE=(CYL,(3,1,1)),UNIT=WORK
//SYSPRINT  DD SYSOUT=*
//     PEND
//*
//DRIVERS EXEC LKED,PARM='LIST,LET,MAP'
//*
//SYSLMOD DD DISP=SHR,DSN=SRS1.SAS.PAGEPLOT.SAS.LOAD
//*
//PAGEPLOT  DD DISP=SHR,DSN=SRS1.SAS.PAGEPLOT.NCAL
//SASLIB    DD DISP=SHR,DSN=STC2.SAS.NCAL
//*
//SYSIN     DD *
       INCLUDE PAGEPLOT(PLTC1,PLTF1,PLTG1,I38INIT,PLTPLOT,PLTSYMB)
       INCLUDE PAGEPLOT(PLTPEN,PLTMSG,I38PAGE)
       CHANGE  LINKABLE(STC3800)
       INCLUDE SASLIB(LINKABLE)
       ENTRY   STC3800
       NAME    STC3800(R)
       INCLUDE PAGEPLOT(PLTC1,PLTF1,PLTG1,X97INIT,PLTPLOT,PLTSYMB)
       INCLUDE PAGEPLOT(PLTPEN,PLTMSG,X97PAGE)
       CHANGE  LINKABLE(STC9700L)
       INCLUDE SASLIB(LINKABLE)
       ENTRY   STC9700L
       NAME    STC9700L(R)
       INCLUDE PAGEPLOT(PLTC1,PLTF1,PLTG1,Y97INIT,PLTPLOT,PLTSYMB)
       INCLUDE PAGEPLOT(PLTPEN,PLTMSG,X97PAGE)
       CHANGE  LINKABLE(STC9700P)
       INCLUDE SASLIB(LINKABLE)
       ENTRY   STC9700P
       NAME    STC9700P(R)
//*
//STC3800 EXEC SAS,ROOT='SRS1.SAS.PAGEPLOT.SAS'
//*
GOPTIONS NOAUTOCOPY AUTOFEED NOAXES BAUD=0 NOBORDER CBACK='' CBY=BLACK
         NOCELL CHARACTERS CHARTYPE=0 NOCIRCLEARC COLORS=(BLACK)
         CPATTERN=BLACK CSYMBOL=BLACK CTITLE=BLACK NODASH FBY=NONE
         NOFILL FTITLE=NONE GSIZE=0 HBY=1 NOGOPT10 NOGOPT11 NOGOPT12
         NOGOPT13 NOGOPT14 NOGOPT15 INTERPOL='' PENMOUNTS=0 NOPIEFILL
         NOPOLYGONFILL NOROTATE SPEED=0 NOSYMBOL NOTERMINAL UNIT=0

         DEVICE=STC3800 HPOS=100 HSIZE=11 VPOS=50 VSIZE=8.5;

 /* THIS DRIVER MAY BE USED ANY NUMBER OF TIMES WITHIN ANY JOB STEP
  *
  * MAXIMUM VALUES FOR HSIZE & VSIZE ARE 11 INCHES & 8.5 INCHES.
  *
  * OPTIMAL VALUES FOR HPOS & VPOS ARE 50 & 25, 100 & 50 OR 150 & 75;
  */
 PROC GTESTIT;
//*
//STC9700L EXEC SAS,ROOT='SRS1.SAS.PAGEPLOT.SAS'
//*
GOPTIONS NOAUTOCOPY AUTOFEED NOAXES BAUD=0 NOBORDER CBACK='' CBY=BLACK
         NOCELL CHARACTERS CHARTYPE=0 NOCIRCLEARC COLORS=(BLACK)
         CPATTERN=BLACK CSYMBOL=BLACK CTITLE=BLACK NODASH
         FBY=NONE NOFILL FTITLE=NONE GSIZE=0 HBY=1 NOGOPT10 NOGOPT11
         NOGOPT12 NOGOPT13 NOGOPT14 NOGOPT15 INTERPOL='' PENMOUNTS=0
         NOPIEFILL NOPOLYGONFILL NOROTATE SPEED=0 NOSYMBOL NOTERMINAL
         UNIT=0

         DEVICE=STC9700L HPOS=95 HSIZE=10.5 VPOS=50 VSIZE=8.5;

 /* ONLY ONE OF EITHER STC9700L OR STC9700P MAY BE USED WITHIN ANY JOB
  * AND IT MUST BE USED ONLY ONCE WITHIN THAT JOB.
  *
  * MAXIMUM VALUES FOR HSIZE & VSIZE ARE 10.7 & 8.5 INCHES.
  *
  * OPTIMAL VALUES FOR HPOS & VPOS ARE: 95 & 50.
  *
  * WHERE MORE THAN ONE PROC IS TO BE USED WITH ONE XEROX 9700 DEVICE
  * DRIVER, THE USER MAY DEFER GENERATION OF THE XROX9700 SYSOUT FILE
  * BY USING THE GOUT= FEATURE OF SAS AND PROC GREPLAY AS FOLLOWS:
  *
  *      //SYSIN DD *
  *      %INCLUDE INCL(STC9700L);
  *
  *         (* USER CODE SUCH AS DATA AND SAS PROC STEPS *)
  *
  *      PROC GPLOT GOUT=WORK.PLOT1;                   (* FOR EXAMPLE *)
  *         (* USER CODE FOR PROC GPLOT *)
  *
  *      PROC G3D GOUT=WORK.PLOT2;                     (* FOR EXAMPLE *)
  *         (* USER CODE FOR PROC G3D *)
  *         (* ETC *)
  *
  *      DATA WORK.COMBINED;    (* STEP TO COLLECT ALL PLOTS TOGETHER *)
  *                SET WORK.PLOT1 WORK.PLOT2 (* ETC *)   ;
  *                IF COLOR = -1 THEN PICT + 1;
  *                PICTURE = PICT;
  *                DROP PICT;
  *      RUN;
  *
  *      PROC GREPLAY DATA=WORK.COMBINED;
  *      RUN;
  *      /*
  */
 PROC GTESTIT;
