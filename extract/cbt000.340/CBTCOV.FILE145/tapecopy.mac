//TAPECOPY JOB (ACCOUNT-DATA),PROGRAMMER-NAME
//*
//S1  EXEC  PGM=IEBGENER ---------- PRINT THE INSTALLATION INSTRUCTIONS
//SYSPRINT  DD  SYSOUT=*
//SYSUT1  DD  DISP=OLD,DSN=PAGEPLOT.INSTALL,
//            LABEL=1,UNIT=TAPE,VOL=SER=??????,
//            DCB=DEN=3
//SYSUT2  DD  SYSOUT=*
//SYSIN   DD  DUMMY
//*
//S2  EXEC  PGM=IEBCOPY, ----- COPY THE SAMPLE OUTPUT AND DOCUMENTATION
//      REGION=512K,PARM='SIZE=256K'
//SYSPRINT  DD  SYSOUT=*
//SYSUT3  DD  SPACE=(CYL,(2,2)),UNIT=SYSDA
//SYSUT4  DD  SPACE=(CYL,(2,2)),UNIT=SYSDA
//TAPEDD  DD  DISP=OLD,DSN=PAGEPLOT.PRINT,
//            LABEL=2,UNIT=TAPE,VOL=REF=*.S1.SYSUT1,
//            DCB=DEN=3
//DISKDD  DD  DISP=(NEW,CATLG,DELETE),
//            DSN=ANY.VALID.DSNAME
//            UNIT=SYSDA,                               VOL=SER=??????,
//            SPACE=(6233,(50,10,10),RLSE),
//            DCB=(DSORG=PO,RECFM=VBA,LRECL=255,BLKSIZE=6233)
//SYSIN  DD *
 COPY  I=TAPEDD,O=DISKDD
/*
//*
//S3  EXEC  PGM=IEBUPDTE, ------ COPY THE SOURCE, MACROS AND SAMPLE JCL
//      PARM=NEW
//SYSPRINT  DD  SYSOUT=*                                     (OR DUMMY)
//SYSUT2  DD  DISP=(NEW,CATLG,DELETE),
//            DSN=ANY.VALID.DSNAME,
//            UNIT=SYSDA,                               VOL=SER=??????,
//            SPACE=(6160,(200,50,10)),
//            DCB=(DSORG=PO,RECFM=FB,LRECL=80,BLKSIZE=6160),
//SYSIN DD  DISP=OLD,DSN=PAGEPLOT.CARDS,
//          LABEL=3,UNIT=TAPE,VOL=REF=*.S1.SYSUT1,
//          DCB=DEN=3
//*
//* -- COPY THE LOAD MODULES AND PRINTER CHARACTER FONTS
//*
//S4  EXEC  PGM=IEBCOPY, ------------COPY LOAD MODULES AND PRINTER FONTS
//      REGION=512K,PARM='SIZE=256K'
//SYSPRINT  DD  SYSOUT=*
//SYSUT3  DD  SPACE=(CYL,(2,2)),UNIT=SYSDA
//SYSUT4  DD  SPACE=(CYL,(2,2)),UNIT=SYSDA
//TAPEDD  DD  DISP=OLD,DSN=PAGEPLOT.LOAD,
//            LABEL=4,UNIT=TAPE,VOL=REF=*.S1.SYSUT1,
//            DCB=DEN=3
//DISKDD  DD  DISP=(NEW,CATLG,DELETE),
//            DSN=ANY.VALID.DSNAME,
//            UNIT=SYSDA,                                VOL=SER=??????,
//            SPACE=(TRK,(20,10,10))
//SYSIN  DD *
 COPY  I=TAPEDD,0=DISKDD
/*
//
