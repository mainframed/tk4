C
C     DUMMY CALCOMP STYLE SUBROUTINES FOR DEBUGGING THE CALCOMP
C     COMPATIBLE DRIVERS.
C
C     OUTPUT IS A FORTRAN PROGRAM WHICH CAN BE COMPILED & LINKED
C     TO A SET OF CALCOMP ROUTINES
C
      SUBROUTINE PLOT(X,Y,IPEN)
      IF( (IPEN .EQ. 2) .OR. (IPEN .EQ. 3) ) RETURN
      WRITE(14,1000)X,Y,IPEN
1000  FORMAT('      CALL PLOT(',E14.7,',',E14.7,',',I3,')')
      RETURN
      END
C
      SUBROUTINE PLOTS(I1,I2,I3)
      WRITE(14,1000)I1,I2,I3
1000  FORMAT('      CALL PLOTS(', I3, ',', I3, ',', I3, ')' )
      RETURN
      END
C
      SUBROUTINE SYMBOL(X,Y,H,ARRAY,ANGLE,NCHAR)
      LOGICAL*1 ARRAY(NCHAR),ACOPY(40),BLANK/' '/
      DO 1 I=1,40
        IF (I .LE. NCHAR) ACOPY(I) = ARRAY(I)
        IF (I .GT. NCHAR) ACOPY(I) = BLANK
1     CONTINUE
      RETURN
      END
C
      SUBROUTINE NEWPEN(IPEN)
      WRITE(14,1000)IPEN
1000  FORMAT('      CALL NEWPEN(',I5,')')
      RETURN
      END
