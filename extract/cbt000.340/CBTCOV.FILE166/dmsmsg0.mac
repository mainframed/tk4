MSG18A   DC    AL1(MSGCONS),AL3(MSG18)
MSG19A   DC    AL1(MSGCONS),AL3(MSG19)
MSG20A   DC    AL1(MSGCONS),AL3(MSG20)
MSG21A   DC    AL1(MSGCONS),AL3(MSG21)
