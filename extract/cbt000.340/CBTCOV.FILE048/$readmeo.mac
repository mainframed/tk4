                           "CSRROW("rowcrp") AUTOSEL(NO)"
                end
        src = rc
          if src > 4 then do
             "TBEnd" table_name
             Exit 0
             end

        crp = ztdtop
        rowcrp = null

        if row <> null then
           if row > 0 then do
             "TBTop" table_name
             "TBSkip" table_name "NUMBER("row")"
             end

        Usrrfind = null
        "VPUT USRRFIND"

        zcmd_ok = null

        Select
          When words(Zcmd) > 1 then do
               parse value zcmd with o1 o2
               if abbrev("FIND",o1,1) = 1 then call Find_It
               if zcmd_ok <> "ok" then do
                  smsg = "Error"
