                   end
           end

        "Select Cmd(%SOFTPUB PRIME ><" prod_table "SOFTCOPY" debug test_
                   ") Newpool"

        Exit_Risoft:
           if do_pan = "on" then do
              options = ""
              signal display_selection
              end
