.TR37    AIF   (&TB(14)).TR39          NO 'HAN DAKUTEN'
&TB(14)  SETB  1
         AIF   (&PIB(15) AND &PIB(22)).TR34
         AIF   (&PIB(15)).TR38         SKIP 'DC' IF FORM INVALID
         DC    X'4777' .                CONSTANT
         AIF   (&PIB(22)).TR34         NO DISPLAY
