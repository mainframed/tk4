         MACRO
&NAME    RESERVE &PARAM,&SMC=,&RET=,&UCB=,&MF=I
         GBLB  &IHBRES
&IHBRES  SETB  1
         AIF   ('&MF(1)' EQ 'E').RES000
         AIF   ('&UCB' NE '').RES000
         IHBERMAC 157,UCB,&UCB
.RES000  ANOP
         AIF   ('&SMC' EQ '' AND '&RET' EQ '' AND '&UCB' EQ '').RES010
         AIF   ('&SMC' EQ '' AND '&RET' EQ '').RES020
         AIF   ('&RET' EQ '' AND '&UCB' EQ '').RES030
         AIF   ('&SMC' EQ '' AND '&UCB' EQ '').RES040
         AIF   ('&UCB' EQ '').RES050
         AIF   ('&SMC' EQ '').RES060
         AIF   ('&RET' EQ '').RES070
&NAME    ENQ   &PARAM,SMC=&SMC,RET=&RET,UCB=&UCB,MF=&MF
         AGO   .RES080
.RES010  ANOP
&NAME    ENQ   &PARAM,MF=&MF
         AGO   .RES080
.RES020  ANOP
&NAME    ENQ   &PARAM,UCB=&UCB,MF=&MF
         AGO   .RES080
.RES030  ANOP
&NAME    ENQ   &PARAM,SMC=&SMC,MF=&MF
         AGO   .RES080
.RES040  ANOP
&NAME    ENQ   &PARAM,RET=&RET,MF=&MF
         AGO   .RES080
.RES050  ANOP
&NAME    ENQ   &PARAM,SMC=&SMC,RET=&RET,MF=&MF
         AGO   .RES080
.RES060  ANOP
&NAME    ENQ   &PARAM,RET=&RET,UCB=&UCB,MF=&MF
       AGO     .RES080
.RES070  ANOP
&NAME    ENQ   &PARAM,SMC=&SMC,UCB=&UCB,MF=&MF
.RES080  AIF   (NOT(&IHBRES)).RES090
&IHBRES  SETB  0
         SVC   56
.RES090  MEND
