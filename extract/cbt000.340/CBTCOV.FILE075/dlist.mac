         MACRO
&NAME    DLIST &ENTRY
         GBLA  &TSIZE
         GBLC  &SECT
         LCLA  &CNT
         LCLB  &NOGO
         LCLC  &TERM
         AIF   ('&NAME' NE '').OK
         MNOTE 12,'***  IHB001  LISTNAME OPERAND REQ''D-NOT SPECIFIED'
&NOGO    SETB  1
.OK      AIF   ('&ENTRY' NE '').OK1
         MNOTE 12,'***  IHB001  ENTRY OPERAND REQ''D-NOT SPECIFIED'
&NOGO    SETB  1
.OK1     AIF   (&NOGO).END
         AIF   (K'&NAME LE &TSIZE).OK2
         MNOTE 12,'***  IHB002  INVALID LISTNAME OPERAND SPECIFIED'
&NOGO    SETB  1
.OK2     AIF   (&NOGO).END
&SECT    CSECT
&NAME    DC    AL1(IHB&SYSNDX.-&NAME)  LENGTH OF ENTRY
         DC    VL3(IECKDLQT)
         DC    XL5'0'
         DC    CL9'&NAME'
.LOOP    ANOP
&CNT     SETA  &CNT+1
&TERM    SETC  '&ENTRY(&CNT)'
         AIF   (K'&ENTRY(&CNT) LE &TSIZE).OK3
         MNOTE 12,'***  IHB002  INVALID ENTRY OPERAND SPECIFIED &TERM'
         AGO   .CK
.OK3     DC    AL2(&TERM-TERMTBL)
.CK      AIF   (&CNT LT N'&ENTRY).LOOP
         DC    H'0'                    LAST ENTRY
IHB&SYSNDX DS 0F
.END     MEND
