         MACRO
&NAME    WRITE &DECB,&TYPE,&DCB,&AREA,&LENGTH,&KEY,&RCDREF,&MF=I
         GBLB  &IHBWR,&IHBT(2),&IHBS(4)
         LCLB  &ERR(2)
&ERR(1)  SETB  ('&DECB' EQ '')
&ERR(2)  SETB  ('&TYPE' EQ '')
         AIF   (&ERR(2)).E0
&IHBS(1) SETB  (K'&TYPE EQ 1)
&IHBS(2) SETB  (K'&TYPE EQ 2)
&IHBS(3) SETB  (K'&TYPE EQ 3)
&IHBS(4) SETB  (K'&TYPE EQ 4)
&IHBWR   SETB  1
&IHBT(1) SETB  (T'&LENGTH EQ 'N')
&IHBT(2) SETB  (T'&RCDREF EQ 'N')
         AIF   ('&TYPE'(1,1) EQ 'S').BSAMR
         AIF   ('&TYPE'(1,1) EQ 'K').BISAMR
         AIF   ('&TYPE'(1,1) EQ 'D').BDAMR
         AIF   ('&TYPE'(1,1) EQ 'T').BTAMR
         IHBERMAC 63,,&TYPE             TYPE INVALID
         AGO   .E0
.BSAMR   AIF   (&ERR(1)).E0
&NAME    IHBRDWRS &DECB,&TYPE,&DCB,&AREA,&LENGTH,&KEY,&RCDREF,MF=&MF
         MEXIT
.BISAMR  AIF   (&ERR(1)).E0
&NAME    IHBRDWRK &DECB,&TYPE,&DCB,&AREA,&LENGTH,&KEY,&RCDREF,MF=&MF
         MEXIT
.BTAMR   AIF   (&ERR(1)).E0
&NAME    IHBRDWRT &DECB,&TYPE,&DCB,&AREA,&LENGTH,&KEY,&RCDREF,MF=&MF
         MEXIT
.BDAMR   AIF   (&ERR(1)).E0
&NAME    IHBRDWRD &DECB,&TYPE,&DCB,&AREA,&LENGTH,&KEY,&RCDREF,MF=&MF
         MEXIT
.E0      AIF   (NOT &ERR(1)).E1
         IHBERMAC 07                    &DECB MISSING
.E1      AIF   (NOT &ERR(2)).MEND
         IHBERMAC 25                    &TYPE MISSING
.MEND    MEND
