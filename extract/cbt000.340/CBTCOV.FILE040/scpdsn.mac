)ATTR DEFAULT(%+_)
      /*  % TYPE(TEXT) INTENS(HIGH)      defaults displayed for      */
      /*  + TYPE(TEXT) INTENS(LOW)       information only            */
      /*  _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT)             */
)BODY
+           &TITLE                +DSN ENTRY PANEL
%COMMAND ===>_ZCMD
%
%&REQ1
+   PROJECT ===>_PROJECT +
+   LIBRARY ===>_LIBRARY +
+   TYPE    ===>_TYPE    +
+   MEMBER  ===>_MEMBER  +       (Blank for member selection list)
+
+OTHER PARTITIONED OR SEQUENTIAL DATA SET:
+   DATA SET NAME  ===>_DSN1
+   VOLUME SERIAL  ===>_VOL1  +  (If not cataloged)
+
+&REQ2               +==>_ENT2                                       +
+&REQ3               +==>_ENT3                                       +
+&REQ4               +==>_ENT4                                       +
+&REQ5               +==>_ENT5                                       +
+&REQ6               +==>_ENT6                                       +
+
)INIT
 IF (&CLR='R')
   &REQ1=''
   &REQ2=''
   &REQ3=''
   &REQ4=''
   &REQ5=''
   &REQ6=''
   &CLR='D'
 IF (&CLR='D')
   &PROJECT=&ZPREFIX
   &LIBRARY=&LIB0
   &TYPE=&TYP0
   &CLR='C'
 IF (&CLR='C')
   &MEMBER=''
   &DSN1=''
   &VOL1=''
   &ENT2=''
   &ENT3=''
   &ENT4=''
   &ENT5=''
   &ENT6=''
   &CLR=''
 &ENT1=''
)PROC
   IF (&DSN1='')
     VER (&PROJECT,NB,NAME)
     VER (&LIBRARY,NB,NAME)
     VER (&TYPE,NB,NAME)
     VER (&MEMBER,NAME)
     &MEM1=&MEMBER
     &ENT1='&PROJECT..&LIBRARY..&TYPE'
   IF (&DSN1 ^= '')
     VER (&DSN1,DSNAME)
     &ENT1=TRUNC(&DSN1,'(')
     &MEM1=''
     IF (&ENT1 ^= &DSN1) /* MEMBER NAME GIVEN
       &MEM1=.TRAIL
       &MEM1=TRUNC(&MEM1,')')
       &T=TRUNC(&ENT1,1)
       IF (&T='''')
         &ENT1=.TRAIL
       IF (&T^='''')
         &ENT1='&ZPREFIX..&ENT1'
     IF (&MEM1='') /* NO MEMBER NAME GIVEN
       &T=TRUNC(&DSN1,1)
       IF (&T='''')
         &ENT1=.TRAIL
         &ENT1=TRUNC(&ENT1,'''')
       IF (&T^='''')
         &ENT1='&ZPREFIX..&DSN1'
       VER(&MEMBER,NAME)
       &MEM1=&MEMBER
   VER (&MEM1,NAME)
   VER (&VOL1,NAME)
   VER (&ENT1,NB,DSNAME)
)END
)PROC
)END
