)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
)BODY EXPAND(\\)
+-\-\-ÖO.TSOE    TSOEDIT Subcommand+-\-\-
%OPTION  ===>|ZCMD
%
+Enter the member group name and any operands below for%TSOEDIT:
%===>|LOPER

+
+Operands:%memgroup   +   (e.g., *; start:end; start:; first*pat; range*
%          NONUM
%          ASIS
%          OLD/NEW
%          ASM/BASIC/CLIST/CNTL/COBOL/DATA/
%          FORTE/FORTG/FORTGI/FORTH/GOFORT/
%          IPLI/LIST/PLI/PLIF/TEXT/VSBASIC
+
+Defaults: memgroup, edit type based on the data set name
)INIT
  &LOPER  = '&OPERS'
  .CURSOR = LOPER
  .HELP   = P85OPHH
  IF(&LOPER  = &Z) &LOPER=&P85#TE1
)PROC
 &P85#TE1=&LOPER
 &PDSZSEL='Tsoedit  &LOPER'
)END
