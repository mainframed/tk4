)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
)BODY EXPAND(\\)
+-\-\-Öo.del    Delete line command+-\-\-
%OPTION  ===>|ZCMD
%
+Enter any operands below for%DELETE  &PDSMEM
%===>|LOPER

+
+Operands: %ALIAS   - +To also delete associated members
+          %NOALIAS - +To delete only the identified member
+
+Defaults:  NOALIAS
)INIT
  &LOPER  = '&OPERS'
  .CURSOR = LOPER
  .HELP   = P85LOHH
  IF(&LOPER  = &Z) &LOPER=&P85#DE3
)PROC
 &P85#DE3=&LOPER
 &PDSZSEL='Delete  &PDSMEM &LOPER'
)END
