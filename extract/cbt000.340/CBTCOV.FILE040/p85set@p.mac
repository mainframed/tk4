)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    # TYPE(output) INTENS(high) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
)BODY EXPAND(\\)
+-\-\-ÖPDS SET Defaults Selection+-\-\-
%OPTION  ===>_ZCMD
+
+
+Choose one of the following options:
    %SETENV -+Set environment defaults
    %SETLOG -+Set log limits
    %SETML  -+Set MEMLIST defaults
    %SETSEL -+Set SELECT command defaults
    %SETV   -+Set LISTV  command defaults
+
+ This panel can be displayed by the SET command.
+
+
)INIT
 .HELP = P85FN@PH
 .CURSOR=ZCMD
)PROC
    &PDSZSEL=TRANS(TRUNC(&PDSZSEL,'.')
           SETE,'&ZCMD'
          SETEN,'&ZCMD'
         SETENV,'&ZCMD'
           SETS,'&ZCMD'
          SETSE,'&ZCMD'
         SETSEL,'&ZCMD'
           SETL,'&ZCMD'
          SETLO,'&ZCMD'
         SETLOG,'&ZCMD'
           SETM,'&ZCMD'
          SETML,'&ZCMD'
           SETV,'&ZCMD'
            ' ',' '
              *,'?')
 &ZTRAIL=.TRAIL
)END
