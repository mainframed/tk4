DECRSV03 EQU   BIT3,,C'X' -   RESERVED
DECRSV04 EQU   BIT4,,C'X' -   RESERVED
DECRSV05 EQU   BIT5,,C'X' -   RESERVED
DECRSV06 EQU   BIT6,,C'X' -   RESERVED
DECRSV07 EQU   BIT7,,C'X' -   RESERVED
