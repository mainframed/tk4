         MACRO
         $LIBTSO                                                       X
               &CONTROL=ENQ,                                           X
               &CPLINK=YES,                                            X
               &DESC=YES,                                              X
               &FAIRMOD=LINK,                                          X
               &HIST=YES,                                              X
               &INC=NO,                                                X
               &KANA=NO,                                               X
               &LIBAUDT=YES,                                           X
               &LIBMSG=TEMP,                                           X
               &LIBPARM='NRJS,NJTA',                                   X
               &LIBRPGM=AFOLIBR,                                       X
               &LIST=NO,                                               X
               &MASTER=LIBR.MASTER,                                    X
               &MASTEXT=,                                              X
               &MASTHI=LIBR,                                           X
               &MAXMOD=5,                                              X
               &NOLKED=NO,                                             X
               &PDSDIR=10,                                             X
               &PDSPRI=50,                                             X
               &PDSSEC=50,                                             X
               &PGMR=ADD,                                              X
               &PREFIX=NO,                                             X
               &PRTBLK=1330,                                           X
               &PSWD=DGSXL,                                            X
               &TLICD=LIBR.TLICD,                                      X
               &TSOBLK=3120,                                           X
               &TYPE=COBOL,                                            X
               &UPDBLK=3120,                                           X
               &EXITLA=,                                               X
               &EXITLC=,                                               X
               &EXITLD=,                                               X
               &EXITLG=,                                               X
               &EXITLI=,                                               X
               &EXITLL=,                                               X
               &EXITLS=,                                               X
               &EXITLU=,                                               X
               &EXITLX=,                                               X
               &ATYP1=(ASM,ASM,1,*),                                   X
               &ATYP2=(COBOL,COB,7,*),                                 X
               &ATYP3=(PLI,PLI,2,/*),                                  X
               &ATYP4=(PLIF,PLF,2,/*),                                 X
               &ATYP5=(FORT,FOR,1,C),                                  X
               &ATYP6=(FORTGI,FRG,1,C),                                X
               &ATYP7=(FORTH,FRH,1,C),                                 X
               &ATYP8=(GOFORT,GOF,1,C),                                X
               &ATYP9=(TEXT,TXT,1,C),                                  X
               &ATYP10=(DATA,DAT,1,*),                                 X
               &ATYP11=(CLIST,CMD,1,*),                                X
               &ATYP12=(CNTL,JCL,1,//*),                               X
               &ATYP13=(BASIC,BAS,1,*),                                X
               &ATYP14=(VSBASIC,VSB,1,*),                              X
               &ATYP15=(RPG,RPG,1,*),                                  X
               &ATYP16=(GIS,GIS,1,*),                                  X
               &ATYP17=(A0000000,000,1,DUM),                           X
               &ATYP18=(B1111111,111,1,DUM),                           X
               &ATYP19=(C2222222,222,1,DUM),                           X
               &ATYP20=(D3333333,333,1,DUM),                           X
               &ATYP21=(E4444444,444,1,DUM),                           X
               &ATYP22=(F5555555,555,1,DUM),                           X
               &ATYP23=(G6666666,666,1,DUM),                           X
               &ATYP24=(H7777777,777,1,DUM),                           X
               &ATYP25=(I8888888,888,1,DUM),                           X
               &ATYP26=(J9999999,999,1,DUM)
.*--------------------------------------------------------------------*
         LCLA  &PARMLEN
         LCLA  &TLICDLN
         LCLA  &MASTERL
         LCLA  &MASTHIL
.*--------------------------------------------------------------------*
.*  DETERMINE WHETHER TO GENERATE THE REQUIRED LINKAGE EDITOR INPUT   *
.*  TO CREATE COMMAND PROCESSOR LOAD MODULES.                         *
.*                                                                    *
.*       NOLKED=YES              SUPRESS ALL LINK STATEMENTS *REV*    *
.*       CPLINK=YES              DEFAULT: CREATE EVERYTHING.          *
.*       CPLINK=NO               CREATE 'LIBTSOGN' ONLY.              *
.*--------------------------------------------------------------------*
         AIF   ('&NOLKED' EQ 'YES').NOLKED
         AIF   ('&CPLINK' EQ 'NO').NOCPS
         AIF   ('&CPLINK' EQ 'YES').CPLEIP
         MNOTE 12,'CPLINK MUST SPECIFY EITHER YES OR NO.'
.*--------------------------------------------------------------------*
.*      G-E-N-E-R-A-T-E  L-I-N-K-A-G-E  E-D-I-T-O-R  I-N-P-U-T        *
.*--------------------------------------------------------------------*
.*  GENERATE LINKAGE EDITOR I/P TO CREATE FOLLOWING COMMAND PROCESSOR *
.*  LOAD MODULES FOR THE LIB/TSO INTERFACE:                           *
.*                                                                    *
.*       1.  LIBADD---> (ALIAS LA)     COMMAND PROCESSOR              *
.*       2.  LIBCTL---> (ALIAS LC)     "     " "       "              *
.*       3.  LIBDLM---> (ALIAS LD)     "     " "       "              *
.*       4.  LIBGET---> (ALIAS LG)     "     " "       "              *
.*       5.  LIBINDEX-> (ALIAS LI)     "     " "       "              *
.*       6.  LIBLIST--> (ALIAS LL)     "     " "       "              *
.*       7.  LIBSAVE--> (ALIAS LS)     "     " "       "              *
.*       8.  LIBUTI---> (ALIAS LU)     "     " "       "              *
.*       9.  LIBEXP---> (ALIAS LX)     "     " "       "              *
.*      10.  LIBTLICD-> (TLICD FILE'S BATCH UTILITY)                  *
.*--------------------------------------------------------------------*
.CPLEIP  ANOP
         PUNCH ' INCLUDE RELOC(LVS4500)'
         PUNCH ' INCLUDE RELOC(LIBTLICD)'
         PUNCH ' ENTRY LIBTLICD'
         PUNCH ' NAME LIBTLICD(R)'
         PUNCH ' INCLUDE RELOC(LVS4500)'
         PUNCH ' INCLUDE RELOC(LIBRLA00)'
         PUNCH ' ENTRY LIBRLA00'
         PUNCH ' ALIAS LA'
         PUNCH ' NAME LIBADD(R)'
         PUNCH ' INCLUDE RELOC(LVS4500)'
         PUNCH ' INCLUDE RELOC(LIBRLC00)'
         PUNCH ' ENTRY LIBRLC00'
         PUNCH ' ALIAS LC'
         PUNCH ' NAME LIBCTL(R)'
         PUNCH ' INCLUDE RELOC(LVS4500)'
         PUNCH ' INCLUDE RELOC(LIBRLD00)'
         PUNCH ' ENTRY LIBRLD00'
         PUNCH ' ALIAS LD'
         PUNCH ' NAME LIBDLM(R)'
         PUNCH ' INCLUDE RELOC(LVS4500)'
         PUNCH ' INCLUDE RELOC(LIBRLG00)'
         PUNCH ' ENTRY LIBRLG00'
         PUNCH ' ALIAS LG'
         PUNCH ' NAME LIBGET(R)'
         PUNCH ' INCLUDE RELOC(LVS4500)'
         PUNCH ' INCLUDE RELOC(LIBRLI00)'
         PUNCH ' ENTRY LIBRLI00'
         PUNCH ' ALIAS LI'
         PUNCH ' NAME LIBINDEX(R)'
         PUNCH ' INCLUDE RELOC(LVS4500)'
         PUNCH ' INCLUDE RELOC(LIBRLL00)'
         PUNCH ' ENTRY LIBRLL00'
         PUNCH ' ALIAS LL'
         PUNCH ' NAME LIBLIST(R)'
         PUNCH ' INCLUDE RELOC(LVS4500)'
         PUNCH ' INCLUDE RELOC(LIBRLS00)'
         PUNCH ' ENTRY LIBRLS00'
         PUNCH ' ALIAS LS'
         PUNCH ' NAME LIBSAVE(R)'
         PUNCH ' INCLUDE RELOC(LVS4500)'
         PUNCH ' INCLUDE RELOC(LIBRLU00)'
         PUNCH ' ENTRY LIBRLU00'
         PUNCH ' ALIAS LU'
         PUNCH ' NAME LIBUTI(R)'
         PUNCH ' INCLUDE RELOC(LVS4500)'
         PUNCH ' INCLUDE RELOC(LIBRLX00)'
         PUNCH ' ENTRY LIBRLX00'
         PUNCH ' ALIAS LX'
         PUNCH ' NAME LIBEXP(R)'
.NOCPS   ANOP
.*--------------------------------------------------------------------*
.*        B-U-I-L-D  L-O-A-D  M-O-D-U-L-E  'L-I-B-T-S-O-G-N'          *
.*--------------------------------------------------------------------*
.*  THE MAKEUP OF LOAD MODULE 'LIBTSOGN' IS AS FOLLOWS:               *
.*                                                                    *
.*        1- COMMON SUBROUTINES (LVS4500; LIBRTS01-THRU-LIBRTS07).    *
.*        2- COMPARATOR (LVSCOG01,LVSCPRR):  WHEN LIBAUDT=YES ONLY.   *
.*        3- FAIR (FAIROPN;FAIRLOC;FAIRMOD;FAIRREC;FAIRCLS):  WHEN    *
.*           FAIRMOD=LINK ONLY.                                       *
.*        4- USER OPTIONS CSECT - 'LIBTSOGN' <--- ENTRY POINT.        *
.*--------------------------------------------------------------------*
.*  GENERATE LINKAGE EDITOR INPUT FOR LIB/TSO COMMON SUBROUTINES:     *
.*--------------------------------------------------------------------*
         PUNCH ' INCLUDE RELOC(LVS4500)'
         PUNCH ' INCLUDE RELOC(LIBRTS01)'
         PUNCH ' INCLUDE RELOC(LIBRTS02)'
         PUNCH ' INCLUDE RELOC(LIBRTS03)'
         PUNCH ' INCLUDE RELOC(LIBRTS04)'
         PUNCH ' INCLUDE RELOC(LIBRTS05)'
         PUNCH ' INCLUDE RELOC(LIBRTS06)'
         PUNCH ' INCLUDE RELOC(LIBRTS07)'
.*--------------------------------------------------------------------*
.*  GENERATE LINKAGE EDITOR INPUT FOR COMPARATOR IF LIBAUDT=YES:      *
.*                                                                    *
.*        LIBAUDT=YES            DEFAULT: CREATE LINKAGE EDITOR INPUT.*
.*        LIBAUDT=NO             BYPASS COMPARATOR.                   *
.*--------------------------------------------------------------------*
         AIF   ('&LIBAUDT' EQ 'NO').NOAUDT
         PUNCH ' INCLUDE RELOC(LVSCOG01)'
         PUNCH ' INCLUDE RELOC(LVSCPRR)'
.*--------------------------------------------------------------------*
.*  GENERATE LINKAGE EDITOR INPUT FOR FAIR MODULES IF FAIRMOD=LINK:   *
.*                                                                    *
.*       FAIRMOD=LINK            DEFAULT: LINK FAIR MODULES.          *
.*       FAIRMOD=LOAD            FAIR MODULES TO BE LOADED.           *
.*--------------------------------------------------------------------*
.NOAUDT  ANOP
         AIF   ('&FAIRMOD' EQ 'LOAD').PUNCH3
         AIF   ('&FAIRMOD' EQ 'LINK').PUNCH2
         MNOTE 12,'INVALID FAIRMOD OPTION. MUST SPECIFY LINK OR LOAD'
         AGO   .PUNCH3
.PUNCH2  ANOP
         PUNCH ' INCLUDE RELOC(FAIR1000)'
         PUNCH ' INCLUDE RELOC(FAIR5000)'
         PUNCH ' INCLUDE RELOC(FAIR3000)'
         PUNCH ' INCLUDE RELOC(FAIR5100)'
         PUNCH ' INCLUDE RELOC(FAIR3100)'
         PUNCH ' INCLUDE RELOC(FAIR5200)'
         PUNCH ' INCLUDE RELOC(FAIR1100)'
         PUNCH ' INCLUDE RELOC(FAIR1200)'
         PUNCH ' INCLUDE RELOC(FAIR1300)'
         PUNCH ' INCLUDE RELOC(LVS1010)'
         PUNCH ' INCLUDE RELOC(FAIR1400)'
.*--------------------------------------------------------------------*
.*         G-E-N-E-R-A-T-E  U-S-E-R  O-P-T-I-O-N-S  C-S-E-C-T         *
.*--------------------------------------------------------------------*
.*  CREATE USER TAILORED INSTALLATION OPTIONS CSECT WITH ENTRY POINT  *
.*  'LIBTSOGN'.  VARIABLE OPTION VALUES DETERMINE CSECT'S CONTENTS.   *
.*--------------------------------------------------------------------*
.PUNCH3  ANOP
         PUNCH ' ENTRY LIBTSOGN'
.NOLKED  ANOP
LIBTSOGN CSECT
         DS    0F
         DC    H'0'              ***RESERVED***
.*--------------------------------------------------------------------*
.*  SET BLKSIZE TO USE TO CREATE NEW TSO DATA SET IN COMMAND 'LIBGET'.*
.*                                                                    *
.*       TSOBLK=3120             DEFAULT: 3120                        *
.*       TSOBLK=NNNNN            BLKSIZE UP TO 5 DIGITS.              *
.*--------------------------------------------------------------------*
         DC    H'&TSOBLK'        NEW TSO DATA SET BLKSIZE
         DC    H'0'              ***RESERVED***
.*--------------------------------------------------------------------*
.*  SET THE BLKSIZE TO BE USED TO CREATE AN UPDATE FILE TO THE BATCH  *
.*  LIBRARIAN IN COMMANDS LIBADD, LIBSAVE, LIBDLM, LIBINDEX AND LIBEXP*
.*                                                                    *
.*       UPDBLK=3120             DEFAULT: 3120                        *
.*       UPDBLK=NNNNN            BLKSIZE UP TO 5 DIGITS.              *
.*--------------------------------------------------------------------*
         DC    H'&UPDBLK'        UPDATE FILE BLKSIZE
         DC    H'0'              ***RESERVED***
.*--------------------------------------------------------------------*
.*  SET BLKSIZE TO USE TO CREATE BATCH LIBRARIANS SYSPRINT, INDEX AND *
.*  LIST FILES WHEN IN COMMAND LIBADD, LIBSAVE, LIBDLM, LIBINDEX AND  *
.*  LIBEXP.                                                           *
.*                                                                    *
.*       PRTBLK=1330             DEFAULT: 1330                        *
.*       PRTBLK=NNNNN            BLKSIZE UP TO 5 DIGITS.              *
.*--------------------------------------------------------------------*
         DC    H'&PRTBLK'        SYSPRINT, INDEX, LIST BLKSIZE
.*--------------------------------------------------------------------*
.*  SET MAXIMUM NUMBER OF OUTSTANDING MODULES ALLOWED PER USER FOR USE*
.*  BY COMMAND 'LIBGET':                                              *
.*                                                                    *
.*       MAXMOD=5                DEFAULT: 5 OUTSTANDING MODULES.      *
.*       MAXMOD=NN               MAXIMUM NUMBER UP TO 99.             *
.*--------------------------------------------------------------------*
         AIF   (K'&MAXMOD GT 2).MAXMOD1 CHECK IF INVALID LENGTH
         DC    H'&MAXMOD'        MAXIMUM OUTSTANDING MODULES
         AGO   .MAXMOD2
.MAXMOD1 ANOP
         DC    H'0'              INVALID MAXIUM MODULES NUMBER
         MNOTE 12,'INVALID MAXMOD OPTION. NUMBER GREATER THAN 99.'
.MAXMOD2 ANOP
.*--------------------------------------------------------------------*
.*  SET THE BATCH LIBRARIAN EXECUTION PARAMETER AND ITS LENGTH:       *
.*                                                                    *
.*       LIBPARM='NRJS,NJTA'     DEFAULT: 'NJTA,NRJS'                 *
.*       LIBPARM='X,Y,Z'         STRING UP TO 50 CHARACTERS.          *
.*                                                                    *
.*  NOTE: THE PARAMETER MUST BE SPECIFIED IN SINGLE QUOTES.           *
.*--------------------------------------------------------------------*
&PARMLEN SETA  K'&LIBPARM-2
         DC    H'&PARMLEN'       BATCH LIBRARIAN PARM LENGTH
         DC    CL50&LIBPARM      BATCH LIBRARIAN PARAMETER
.*--------------------------------------------------------------------*
.*  SET SPACE TO BE USED WHEN ALLOCATING A NEW PARTITIONED DATA SET IN*
.*  THE COMMAND LIBGET.  SPACE UNIT IS BY BLOCKS.  THE BLKSIZE USED IS*
.*  FROM OPTION TSOBLK.                                               *
.*                                                                    *
.*       PDSPRI=50               DEFAULT: 50 BLOCKS                   *
.*       PDSPRI=NNNNN            PRIMARY SPACE QUANTITY.              *
.*                                                                    *
.*       PDSSEC=200              DEFAULT: 200 BLOCKS                  *
.*       PDSSEC=NNNNN            SECONDARY SPACE QUANTITY.            *
.*                                                                    *
.*       PDSDIR=10               DEFAULT: 10 BLOCKS                   *
.*       PDSDIR=NNNNN            DIRECTORY BLOCK QUANTITY.            *
.*--------------------------------------------------------------------*
         DC    H'&PDSPRI'        PDS PRIMARY SPACE IN BLOCKS
         DC    H'&PDSSEC'        PDS SECONDARY SPACE IN BLOCKS
         DC    H'&PDSDIR'        PDS DIRECTORY BLOCKS
.*--------------------------------------------------------------------*
.*  INDICATE IF THE MODULE UPDATE LISTING IS TO BE WRITTEN TO A DATA  *
.*  SET WHEN IN THE COMMANDS LIBADD, LIBSAVE AND LIBEXP:              *
.*                                                                    *
.*       LIST=NO                 DEFAULT: DATASET NOT REQUIRED.       *
.*       LIST=YES                LIST DATASET REQUIRED.               *
.*--------------------------------------------------------------------*
         AIF   ('&LIST' EQ 'NO').LIST1
         AIF   ('&LIST' EQ 'YES').LIST2
         DC    C' '
         MNOTE 12,'INVALID LIST OPTION. MUST SPECIFY YES OR NO'
         AGO   .LIST3
.LIST1   ANOP
         DC    C'0'              LISTING DATASET NOT REQUIRED
         AGO   .LIST3
.LIST2   ANOP
         DC    C'1'              LISTING DATASET REQUIRED
.LIST3   ANOP
.*--------------------------------------------------------------------*
.*  INDICATE HOW THE BATCH LIBRARIAN UPDATE MESSAGE FILE SHOULD BE    *
.*  ALLOCATED IN COMMANDS LIBADD, LIBSAVE, LIBINDEX, LIBDLM AND LIBEXP*
.*                                                                    *
.*       LIBMSG=PERM             DEFAULT: BATCH MESSAGES TO DATA SET. *
.*       LIBMSG=TERM             BATCH MESSAGES TO TERMINAL.          *
.*       LIBMSG=TEMP             BATCH MESSAGES TO TEMP DATA SET.     *
.*--------------------------------------------------------------------*
         AIF   ('&LIBMSG' EQ 'PERM').LIBMSG1
         AIF   ('&LIBMSG' EQ 'TERM').LIBMSG2
         AIF   ('&LIBMSG' EQ 'TEMP').LIBMSG3
         DC    C' '              INVALID LIBMSG OPTION
         MNOTE 12,'INVALID LIBMSG OPTION. MUST SPECIFY YES, NO OR TEMP'
         AGO   .LIBMSG4
.LIBMSG1 ANOP
         DC    C'0'              BATCH MESSAGES TO TERMINAL
         AGO   .LIBMSG4
.LIBMSG2 ANOP
         DC    C'1'              BATCH MESSAGES TO DATA SET
         AGO   .LIBMSG4
.LIBMSG3 ANOP
         DC    C'2'              BATCH MESSAGES TO TEMP DATA SET
.LIBMSG4 ANOP
.*--------------------------------------------------------------------*
.*  INDICATE IF THE 'RESERVE' MACRO OR 'ENQ' MACRO IS TO BE USED WHEN *
.*  UPDATING THE DIRECTORY OF THE TLICD FILE FOR NEW USERS:           *
.*                                                                    *
.*       CONTROL=ENQ             DEFAULT: ENQ TLICD FILE.             *
.*       CONTROL=RESERVE         RESERVE TLICD FILE.                  *
.*--------------------------------------------------------------------*
         AIF   ('&CONTROL' EQ 'RESERVE').CNTROL1
         AIF   ('&CONTROL' EQ 'ENQ').CNTROL3
         DC    C' '              INVALID CONTROL OPTION
         MNOTE 12,'INVALID CONTROL OPTION. MUST SPECIFY RESERVE OR ENQ'
         AGO   .CNTROL4
.CNTROL1 ANOP
         DC    C'1'              RESERVE TLICD
         AGO   .CNTROL4
.CNTROL3 ANOP
         DC    C'3'              ENQ TLICD
.CNTROL4 ANOP
.*--------------------------------------------------------------------*
.*  INDICATE IF KATAKANI CHARACTERS NEED TO BE LEFT ALONE WHEN ENTERED*
.*  IN THE COMMANDS LIBADD, LIBSAVE AND LIBEXP UNDER OPERANDS HIST    *
.*  AND DESC:                                                         *
.*                                                                    *
.*       KANA=NO                 DEFAULT: CONVERT I/P TO UPPER CASE.  *
.*       KANA=YES                LEAVE INPUT ASIS.                    *
.*--------------------------------------------------------------------*
         AIF   ('&KANA' EQ 'YES').KANA1
         AIF   ('&KANA' EQ 'NO').KANA2
         DC    C' '              INVALID KANA OPTION
         MNOTE 12,'INVALID KANA OPTION. MUST SPECIFY YES OR NO'
         AGO   .KANA3
.KANA1   ANOP
         DC    C'1'              LEAVE INPUT ASIS IN LA/LS/LX
         AGO   .KANA3
.KANA2   ANOP
         DC    C'0'              CONVERT ALL INPUT TO UPPER CASE
.KANA3   ANOP
.*--------------------------------------------------------------------*
.*  INDICATE IF THE USERID OR THE PREFIX NAME IN USER PROFILE IS TO BE*
.*  USED TO PREFIX DATA SET NAMES IN ALL COMMANDS:                    *
.*                                                                    *
.*       PREFIX=NO               DEFAULT: USERID IS ALWAYS USED.      *
.*       PREFIX=YES              IF A PREFIX NAME IS IN THE USER PRO- *
.*                               FILE, PREFIX NAME IS USED; IF NO PRE-*
.*                               FIX NAME, THEN THE USERID IS USED.   *
.*                                                                    *
.*  NOTE: PREFIX IN THE USER PROFILE IS RECOGNIZED ONLY IN MVS SYSTEM.*
.*--------------------------------------------------------------------*
         AIF   ('&PREFIX' EQ 'NO').PREFIX1
         AIF   ('&PREFIX' EQ 'YES').PREFIX2
         DC    C' '              INVALID PREFIX OPTION
         MNOTE 12,'INVALID PREFIX OPTION. MUST SPECIFY YES OR NO'
         AGO   .PREFIX3
.PREFIX1 ANOP
         DC    C'0'              USERID TO PREFIX DATA SET NAME
         AGO   .PREFIX3
.PREFIX2 ANOP
         DC    C'1'              PREFIX THEN USERID AS PREFIX
.PREFIX3 ANOP
.*--------------------------------------------------------------------*
.*  INDICATE IF THE PASSWORD IS REQUIRED IN COMMANDS LIBDLM, LIBGET,  *
.*  LIBSAVE, LIBLIST AND LIBEXP:                                      *
.*                                                                    *
.*       PSWD=DGSXL              DEFAULT: REQUIRED LD,LG,LS,LL,LX.    *
.*       PSWD=NO                 PASSWORD ISN'T REQUIRED.             *
.*       PSWD=D                  REQUIRED FOR LIBDLM ONLY.            *
.*       PSWD=DG                 LIBDLM AND LIBGET.                   *
.*       PSWD=DGSX               LIBDLM, LIBGET, LIBSAVE AND LIBEXP.  *
.*--------------------------------------------------------------------*
         AIF   ('&PSWD' EQ 'NO').PSWD1
         AIF   ('&PSWD' EQ 'D').PSWD2
         AIF   ('&PSWD' EQ 'DG').PSWD3
         AIF   ('&PSWD' EQ 'DGSX').PSWD4
         AIF   ('&PSWD' EQ 'DGSXL').PSWD5
         DC    C' '              INVALID PASSWORD OPTION
         MNOTE 12,'INVALID PASSWORD OPTION. MUST BE NO,D,DG,DGS,DGSL'
         AGO   .PSWD6
.PSWD1   ANOP
         DC    C'0'              PASSWORD ISN'T REQUIRED
         AGO   .PSWD6
.PSWD2   ANOP
         DC    C'1'              PSWD REQUIRED FOR LIBDLM ONLY
         AGO   .PSWD6
.PSWD3   ANOP
         DC    C'2'              PSWD REQUIRED FOR LIBGET,LIBDLM
         AGO   .PSWD6
.PSWD4   ANOP
         DC    C'3'              PSWD REQUIRED IN LD, LG, LS, LX
         AGO   .PSWD6
.PSWD5   ANOP
         DC    C'4'              PSWD REQUIRED IN LD, LG, LS, LL, LX
.PSWD6   ANOP
.*--------------------------------------------------------------------*
.*  INDICATE IF A HISTORY IS REQUIRED IN COMMANDS LIBSAVE AND LIBEXP: *
.*                                                                    *
.*       HIST=YES                DEFAULT: HISTORY REQUIRED.           *
.*       HIST=NO                 HISTORY NOT REQUIRED IN LS AND LX.   *
.*--------------------------------------------------------------------*
         AIF   ('&HIST' EQ 'NO').HIST1
         AIF   ('&HIST' EQ 'YES').HIST2
         DC    C' '              INVALID HISTORY OPTION
         MNOTE 12,'INVALID HISTORY OPTION. MUST SPECIFY YES OR NO'
         AGO   .HIST3
.HIST1   ANOP
         DC    C'0'              HISTORY NOT REQUIRED IN LIBSAVE/LIBEXP
         AGO   .HIST3
.HIST2   ANOP
         DC    C'1'              HISTORY REQUIRED IN LIBSAVE/LIBEXP
.HIST3   ANOP
.*--------------------------------------------------------------------*
.*  INDICATE IF THERE IS A USER DAIR ROUTINE WHEN ALLOCATING THE      *
.*  LIBRARIAN MASTER FILE IN ALL COMMANDS EXCEPT LIBCTL:              *
.*                                                                    *
.*       MASTEXT=                DEFAULT: NULL                        *
.*       MASTEXT=XXXXXXXX        USER EXIT NAME UP TO 8 CHARACTERS.   *
.*--------------------------------------------------------------------*
         AIF   ('&MASTEXT' NE '').MASTEX1
         DC    CL8' '            MASTER DAIR EXIT NOT USED
         AGO   .MASTEX2
.MASTEX1 ANOP
         DC    CL8'&MASTEXT'     MASTER DAIR EXIT NAME
.MASTEX2 ANOP
.*--------------------------------------------------------------------*
.*  SET DEFAULT DATASET TYPE TO BE USED BY COMMANDS LIBGET & LIBADD:  *
.*                                                                    *
.*       TYPE=COBOL              DEFAULT: COBOL                       *
.*       TYPE=XXXXXXXX           1-8 CHARACTER TYPE.                  *
.*                                                                    *
.*  VALIDATE SPECIFIED DEFAULT TYPE FOR THE FOLLOWING:                *
.*                                                                    *
.*       1. SPECIFIED VALUE NOT GREATER THAN 8 CHARACTERS, NOR NULL.  *
.*       2. VALUE'S 1ST BYTE IS EITHER ALPHABETIC OR NATIONAL (@,#,$) *
.*          CHARACTER.                                                *
.*       3. SPECIFIED VALUE MATCHES ONE OF THE TYPES (EITHER DEFAULT  *
.*          OR USER SPECIFIED) USED TO BUILD THE 'TYPE' TABLE.        *
.*--------------------------------------------------------------------*
         AIF   (K'&TYPE EQ 0).TY6
         AIF   (K'&TYPE GT 8).TY6
         AIF   ('&TYPE'(1,1) LT 'A').TY2
         AIF   ('&TYPE'(1,1) GT 'Z').TYERR
         AGO   .TY4
.TY2     AIF   ('&TYPE'(1,1) EQ '@').TY4
         AIF   ('&TYPE'(1,1) EQ '#').TY4
         AIF   ('&TYPE'(1,1) EQ '$').TY4
.TYERR   DC    CL8' '            INVALID DEFAULT TYPE
         MNOTE 12,'SPECIFIED DEFAULT TYPE''S 1ST CHARACTER MUST BE ALPHX
               ABETIC OR NATIONAL CHARACTER.'
         AGO   .TYPEEND
.TY4     AIF   ('&TYPE' EQ '&ATYP1(1)' OR '&TYPE' EQ '&ATYP2(1)').TY8
         AIF   ('&TYPE' EQ '&ATYP3(1)' OR '&TYPE' EQ '&ATYP4(1)').TY8
         AIF   ('&TYPE' EQ '&ATYP5(1)' OR '&TYPE' EQ '&ATYP6(1)').TY8
         AIF   ('&TYPE' EQ '&ATYP7(1)' OR '&TYPE' EQ '&ATYP8(1)').TY8
         AIF   ('&TYPE' EQ '&ATYP9(1)' OR '&TYPE' EQ '&ATYP10(1)').TY8
         AIF   ('&TYPE' EQ '&ATYP11(1)' OR '&TYPE' EQ '&ATYP12(1)').TY8
         AIF   ('&TYPE' EQ '&ATYP13(1)' OR '&TYPE' EQ '&ATYP14(1)').TY8
         AIF   ('&TYPE' EQ '&ATYP15(1)' OR '&TYPE' EQ '&ATYP16(1)').TY8
         AIF   ('&TYPE' EQ '&ATYP17(1)' OR '&TYPE' EQ '&ATYP18(1)').TY8
         AIF   ('&TYPE' EQ '&ATYP19(1)' OR '&TYPE' EQ '&ATYP20(1)').TY8
         AIF   ('&TYPE' EQ '&ATYP21(1)' OR '&TYPE' EQ '&ATYP22(1)').TY8
         AIF   ('&TYPE' EQ '&ATYP23(1)' OR '&TYPE' EQ '&ATYP24(1)').TY8
         AIF   ('&TYPE' EQ '&ATYP25(1)' OR '&TYPE' EQ '&ATYP26(1)').TY8
         AIF   ('&ATYP1'(1,1) NE '(' OR '&ATYP2'(1,1) NE '(').TY8
         AIF   ('&ATYP3'(1,1) NE '(' OR '&ATYP4'(1,1) NE '(').TY8
         AIF   ('&ATYP5'(1,1) NE '(' OR '&ATYP6'(1,1) NE '(').TY8
         AIF   ('&ATYP7'(1,1) NE '(' OR '&ATYP8'(1,1) NE '(').TY8
         AIF   ('&ATYP9'(1,1) NE '(' OR '&ATYP10'(1,1) NE '(').TY8
