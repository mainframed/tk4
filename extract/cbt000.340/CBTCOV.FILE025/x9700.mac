)SEL &JOBCLASS ^= &Z
//*MAIN SYSTEM=ANY,CLASS=&JOBCLASS
)ENDSEL
)SEL &JOBCLASS = &Z
//*MAIN SYSTEM=ANY
)ENDSEL
)SEL &OLDJDE = DD
)SEL &DUPLEX = YES && &HOLES = NO
