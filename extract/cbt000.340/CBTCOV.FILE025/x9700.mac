)ENDSEL
)SEL &DUPLEX = NO  && &HOLES = NO
//*FORMAT PR,DDNAME=OUT,DEST=&X97DEST,FCB=JB10,FORMS=DD104-NH
)ENDSEL
)SEL &DUPLEX = YES && &HOLES = YES
//*FORMAT PR,DDNAME=OUT,DEST=&X97DEST,FCB=JB10,FORMS=DD105
)ENDSEL
)SEL &DUPLEX = NO  && &HOLES = YES
//*FORMAT PR,DDNAME=OUT,DEST=&X97DEST,FCB=JB10,FORMS=DD104
)ENDSEL
)ENDSEL
)SEL &OLDJDE ^= DD
)SEL &DUPLEX = YES && &HOLES = NO
//*FORMAT PR,DDNAME=OUT,DEST=&X97DEST,FCB=JB10,FORMS=DZ$N
)ENDSEL
)SEL &DUPLEX = NO  && &HOLES = NO
//*FORMAT PR,DDNAME=OUT,DEST=&X97DEST,FCB=JB10,FORMS=SZ$N
)ENDSEL
)SEL &DUPLEX = YES && &HOLES = YES
//*FORMAT PR,DDNAME=OUT,DEST=&X97DEST,FCB=JB10,FORMS=DZ$H
)ENDSEL
)SEL &DUPLEX = NO  && &HOLES = YES
//*FORMAT PR,DDNAME=OUT,DEST=&X97DEST,FCB=JB10,FORMS=SZ$H
)ENDSEL
)ENDSEL
//JOBLIB DD DSN=AOST.LOAD,DISP=SHR
//STEP1 EXEC PGM=MINPUT,PARM=03
//IN01 DD *
)ENDSEL
)SEL &FPRINT = YES
1DFM1   JDE=&JDE,DEPT='&FDS&ACCTNO',COPIES=&NUM,;
)ENDSEL
)SEL &FPRINT = YES && &FNAME ^= &Z  && &JDE ^= DD127
1DFM1   FORMS=&FNAME,;
)ENDSEL
)SEL &FPRINT = NO
1DFM1   JDE=&JDE,DEPT='&FDS&ACCTNO',COPIES=&NUM,FORMS=NONE,;
)ENDSEL
)SEL &TOF ^= &Z
 DFM1   TOF=&TOF,;
)ENDSEL
)SEL &BOF ^= &Z
 DFM1   BOF=&BOF,;
)ENDSEL
)SEL   &JDE = DD100  && &TOF = &Z && &BOF = &Z
)SET BOF = 58
 DFM1   BOF=&BOF,;
)ENDSEL
)SEL   &JDE = DD109  && &TOF = &Z && &BOF = &Z
)SET BOF = 60
 DFM1   BOF=&BOF,;
)ENDSEL
)SEL   &JDE = DD124  && &TOF = &Z && &BOF = &Z
)SET BOF = 57
)SET TOF = 3
 DFM1   TOF=&TOF,BOF=&BOF,;
)ENDSEL
)SEL   &JDE = DD116  && &TOF = &Z && &BOF = &Z
)SET TOF = 6
)SET BOF = 72
 DFM1   TOF=&TOF,BOF=&BOF,;
)ENDSEL
)SEL &FORMAT ^= &Z
 DFM1   FORMAT=&FORMAT,;
)ENDSEL
)SET  FNAME = &NAME
 DFM1   DUPLEX=&DUPLEX,COLLATE=&COLLATE,SHIFT=&SHIFT,;
 DFM1   RTEXT=('DELIVER TO:  &NAME    ',01,05,01),;
 DFM1   RTEXT=('       DIV  DEPT  GROUP BLDG  FLR  MAIL',01,07,01),;
)TB 24 29 35 41 47 52
 DFM1   RTEXT=(' !&FDS!&DEPT!&GROUP!&BLDG!&FLR!&MAIL',01,08,01),;
)SEL &TITLE ^= &Z
 DFM1   RTEXT=('** &TITLE **',01,09,01),;
)ENDSEL
 DFM1   RTEXT=('** DATE=&ZDATE TIME=&ZTIME',01,10,01),;
 DFM1   RTEXT=('OPTIONS: JDE=&JDE  DUPLEX=&DUPLEX',01,11,01),;
 DFM1   RTEXT=('         COLLATE=&COLLATE SHIFT=&SHIFT',01,12,01),;
 DFM1   RTEXT=('         FORMS=&FPRINT COPIES=&NUM',01,13,01),;
)SEL &TOF ^= &Z && &BOF = &Z
 DFM1   RTEXT=('         TOF=&TOF       ',01,15,01),;
)ENDSEL
)SEL &BOF ^= &Z && &TOF = &Z
 DFM1   RTEXT=('         BOF=&BOF       ',01,16,01),;
)ENDSEL
)SEL &BOF ^= &Z && &TOF ^= &Z
 DFM1   RTEXT=('         TOF=&TOF BOF=&BOF  ',01,16,01),;
)ENDSEL
)SEL &FORMAT ^= &Z
 DFM1   RTEXT=('         FORMAT=&FORMAT ',01,17,01),;
)ENDSEL
)SEL &HOLES = NO
 DFM1   RTEXT=('*** NO HOLES FORM ***   ',01,18,01),;
)ENDSEL
)SEL &TABLE ^= &Z
)SET CNT = 2
)DOT &TABLE
 DFM1   RTEXT=('DELIVER TO:  &NAME     ',&CNT,06,01),;
 DFM1   RTEXT=('       DIV  DEPT  GROUP BLDG  FLR  MAIL',&CNT,07,01),;
)TB 24 29 35 41 47 52
 DFM1   RTEXT=(' !&FDS!&DEPT!&GROUP!&BLDG!&FLR!&MAIL ',&CNT,08,01),;
)SEL &TITLE ^= &Z
 DFM1   RTEXT=('** &TITLE **',&CNT,10,01),;
 DFM1   RTEXT=('** FROM: &FNAME ** ',&CNT,11,01),;
)ENDSEL
)SET CNT = &CNT + 1
)ENDDOT
)ENDSEL
 DFM1   END;
