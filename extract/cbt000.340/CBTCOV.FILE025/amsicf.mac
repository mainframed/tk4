)SET DD1 = YES
)SET STEPCAT = YES
)IM AMSJCL
)SEL &AMSOPTX = A
 DEFINE USERCATALOG -
        (NAME(&ICFDSN) -
        VOLUMES(&ICFVOL) -
        NONINDEXED -
        &ICFAUNIT     (&ICFPRIM,&ICFSEC) -
        ICFCATALOG -
        FREESPACE     (&ICFFPRIM &ICFFSEC) -
        SHAREOPTIONS  (&ICFSOPT1 &ICFSOPT2) -
        RECORDSIZE    (&ICFR1    &ICFR2))  -
   DATA(    -
        BUFND     (&BUFND) -
        CISZ      (&CISIZ)) -
   INDEX(    -
        BUFND     (&BUFNDI) -
        CISZ      (&CISIZI)) -
)SEL &MCATPSWD = &Z
    CATALOG(&MCAT)
)ENDSEL
)SEL &MCATPSWD ^= &Z
    CATALOG(&MCAT/&MCATPSWD)
)ENDSEL
)SEL &VVDS = YES
   DEFINE CLUSTER -
      (NAME(SYS1.VVDS.V&ICFVOL) -
      VOLUMES(&ICFVOL) -
      NONINDEXED -
      RECATALOG) -
)SEL &MCATPSWD = &Z
    CATALOG(&MCAT)
)ENDSEL
)SEL &MCATPSWD ^= &Z
    CATALOG(&MCAT/&MCATPSWD)
)ENDSEL
)ENDSEL
)ENDSEL
)SEL &AMSOPTX = D
      DELETE  &ICFDSN  -
        USERCATALOG    -
        FILE(DD1)      -
        FORCE
)ENDSEL
