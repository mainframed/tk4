)SET DD1 = YES
)SET STEPCAT = YES
)IM AMSJCL
)SEL &AMSOPTX = A
 DEFINE CLUSTER -
        (NAME(SYS1.VVDS.V&ICFVOL) -
        VOLUMES(&ICFVOL) -
        NONINDEXED -
        &VVDSAUNT(&VVDSPRIM,&VVDSSEC)) -
)SEL &MCATPSWD = &Z
    CATALOG(&MCAT)
)ENDSEL
)SEL &MCATPSWD ^= &Z
    CATALOG(&MCAT/&MCATPSWD)
)ENDSEL
)ENDSEL
)SEL &AMSOPTX = D
 DELETE SYS1.VVDS.V&ICFVOL   -
        NOSCRATCH
        CLUSTER
        FILE(DD1)
)SEL &MCATPSWD = &Z
    CATALOG(&MCAT)
)ENDSEL
)SEL &MCATPSWD ^= &Z
    CATALOG(&MCAT/&MCATPSWD)
)ENDSEL
)ENDSEL
