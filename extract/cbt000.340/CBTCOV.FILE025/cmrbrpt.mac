)TB 40 80
)SET PP = 0
)SET LPP = 0
)SET CNT = 3
)CM  TCX = YES if already initialized
)CM  TCA = TOTAL CHANGES APPROVED
)CM  TCB = TOTAL CHANGES Backed Out
)CM  TCH = TOTAL CHANGES HELD
)CM  TCI = TOTAL CHANGES INITIAL
)CM  TCR = TOTAL CHANGES RESCHEDULED
)CM  TCS = TOTAL CHANGES SCHEDULED
)CM  TCC = TOTAL CHANGES COMPLETED
)CM  TCW = TOTAL CHANGES WITHDRAWN
)CM  CX = yes if any value set
)CM  CA = CHANGES APPROVED
)CM  CB = CHANGES Backed Out
)CM  CH = CHANGES HELD
)CM  CI = CHANGES INITIAL
)CM  CR = CHANGES RESCHEDULED
)CM  CS = CHANGES SCHEDULED
)CM  CC = CHANGES COMPLETED
)CM  CW = CHANGES WITHDRAWN
)SEL &TCX ^= YES
 =UDK=~
)SET TCX = YES
)SET TCA = 0
)SET TCB = 0
)SET TCH = 0
)SET TCI = 0
)SET TCR = 0
)SET TCS = 0
)SET TCW = 0
)SET TCC = 0
)ENDSEL
)SET CX = NO
)SET CA = 0
)SET CB = 0
)SET CH = 0
)SET CI = 0
)SET CR = 0
)SET CS = 0
)SET CW = 0
)SET CC = 0
)CM *************************************************************
)CM *  Generate a CMRB Report                                   *
)CM *************************************************************
)SEL &CHREPORT = CMRB
)DOT @&CHGROUP
)SET S = &CHSTATUS
)SEL &S = Rescheduled
)SET S  = R
)ENDSEL
)SEL &S = Approved × &S = Held × &S = Initial × &S = Scheduled × &S = R
)SEL &CHPROG = &Z × &CHPROG = &CMRBPGMR
)IM CMRBRPTL
)ENDSEL
)SEL &S = Approved
)SET CX = YES
)SET CA = &CA + 1
)SET TCA = &TCA + 1
)ENDSEL
)SEL &S = Initial
)SET CX = YES
)SET CI = &CI + 1
)SET TCI = &TCI + 1
)ENDSEL
)SEL &S = Held
)SET CH = &CH + 1
)SET CX = YES
)SET TCH = &TCH + 1
)ENDSEL
)CM R = Rescheduled
)SEL &S = R
)SET CR = &CR + 1
)SET CX = YES
)SET TCR = &TCR + 1
)ENDSEL
)SEL &S = Scheduled
)SET CS = &CS + 1
)SET CX = YES
)SET TCS = &TCS + 1
)ENDSEL
)ENDSEL
)ENDDOT
)SEL &CX = YES && &CHOPT ^= LOG
1  Change Notice Summary for Group &CHGROUP       &ZMONTH/&ZDAY/&ZYEAR
0  Approved      &CA      !Rescheduled   &CR
0  Held          &CH      !Scheduled     &CS
0  Initial       &CI
)ENDSEL
)ENDSEL
)CM *************************************************************
)CM *----------------- Selection Seperator ---------------------*
)CM *************************************************************
)CM *  Generate a Report of only Initial Status                 *
)CM *************************************************************
)SEL &CHREPORT = INITIAL
)DOT @&CHGROUP
)SEL &CHSTATUS = Initial
)SEL &CHPROG = &Z × &CHPROG = &CMRBPGMR
)IM CMRBRPTL
)SET TCI = &TCI + 1
)SET CI = &CI + 1
)SET CX = YES
)ENDSEL
)ENDSEL
)ENDDOT
)SEL &CX = YES && &CHOPT ^= LOG
1  Change Notice Summary for Group &CHGROUP       &ZMONTH/&ZDAY/&ZYEAR
0  Total Change Notices in Initial Status:    &CI
)ENDSEL
)ENDSEL
)CM *************************************************************
)CM *----------------- Selection Seperator ---------------------*
)CM *************************************************************
)CM *  Generate a Complete Report                               *
)CM *************************************************************
)SEL &CHREPORT = ALL
)DOT @&CHGROUP
)SEL &CHPROG = &Z × &CHPROG = &CMRBPGMR
)IM CMRBRPTL
)SEL &CHSTATUS = Approved
)SET CA = &CA + 1
)SET TCA = &TCA + 1
)SET CX = YES
)ENDSEL
)SEL &CHSTATUS = Initial
)SET CI = &CI + 1
)SET TCI = &TCI + 1
)SET CX = YES
)ENDSEL
)SEL &CHSTATUS = Held
)SET CH = &CH + 1
)SET TCH = &TCH + 1
)SET CX = YES
)ENDSEL
)SEL &CHSTATUS = Rescheduled
)SET CR = &CR + 1
)SET TCR = &TCR + 1
)SET CX = YES
)ENDSEL
)SEL &CHSTATUS = Completed
)SET CC = &CC + 1
)SET TCC = &TCC + 1
)SET CX = YES
)ENDSEL
)SEL &CHSTATUS = Withdrawn
)SET CW = &CW + 1
)SET TCW = &TCW + 1
)SET CX = YES
)ENDSEL
)SEL &CHSTATUS = BackedOut
)SET CB = &CB + 1
)SET TCB = &TCB + 1
)SET CX = YES
)ENDSEL
)SEL &CHSTATUS = Scheduled
)SET CS = &CS + 1
)SET TCS = &TCS + 1
)SET CX = YES
)ENDSEL
)ENDSEL
)ENDDOT
)SEL &CX = YES && &CHOPT ^= LOG
1  Change Notice Summary for Group &CHGROUP       &ZMONTH/&ZDAY/&ZYEAR
0  Approved      &CA    !  Initial       &CI
0  Backed Out    &CB    !  Rescheduled   &CR
0  Completed     &CC    !  Scheduled     &CS
0  Held          &CH    !  Withdrawn     &CW
)ENDSEL
)ENDSEL
)CM *************************************************************
)CM *----------------- Selection Seperator ---------------------*
)CM *************************************************************
)CM *  Generate a Report of only Scheduled Status               *
)CM *************************************************************
)SEL &CHREPORT = SCHEDULED
)DOT @&CHGROUP
)SEL &CHSTATUS = Scheduled
)SEL &CHPROG = &Z × &CHPROG = &CMRBPGMR
)IM CMRBRPTL
)SET TCS = &TCS + 1
)SET CS = &CS + 1
)SET CX = YES
)ENDSEL
)ENDSEL
)ENDDOT
)SEL &CX = YES && &CHOPT ^= LOG
1  Change Notice Summary for Group &CHGROUP       &ZMONTH/&ZDAY/&ZYEAR
0  Total Change Notices in Scheduled Status:    &CS
)ENDSEL
)ENDSEL
