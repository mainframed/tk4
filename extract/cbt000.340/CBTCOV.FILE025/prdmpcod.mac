)SET PARM = &CODADDRB.:&CODADDRE

)SEL &CODASID ^= &Z
)SET PARM = &PARM.,ASID=&CODASID
)ENDSEL

)SEL &CODCTRL = NO
)SET PARM = &PARM.,NOCONTROL
)ENDSEL

)SEL &CODDEC = NO
)SET PARM = &PARM.,NODECIMAL
)ENDSEL

)SEL &CODFLOAT = NO
)SET PARM = &PARM.,NOFLOAT
)ENDSEL

)SEL &CODIO = NO
)SET PARM = &PARM.,NOIO
)ENDSEL

DECODE &PARM
