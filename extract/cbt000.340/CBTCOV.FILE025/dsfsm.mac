&RJCARD1
//  TIME=15,MSGLEVEL=1,REGION=1024K,PRTY=&DFJPRTY,
//  MSGCLASS=&DFMCLASS,NOTIFY=&ZUSER
//*MAIN SYSTEM=ANY<,CLASS=&DFJCLASS×><,ORG=&DFSDEST×>
//DSFSM  EXEC PGM=REBUS
//SYSPRINT    DD  SYSOUT=&DFSCLASS
//SYSIN       DD *
)SEL &FSMOPT ^= FREESPACE
INDYNAM(&FSMVOL)
)SEL &VIRT = YES
MOUNTVIRTUAL
)ENDSEL
)ENDSEL
)CM
)CM ******* START OF REPORT OPTION CARDS ********
)SEL &FSMOPT = REPORT
)SEL &DETAIL = YES
REPORT(DETAIL<&AOP×><,&FOP×>)
)ENDSEL
)SEL &SUMMARY = YES
REPORT(SUMMARY<&AOP×><,&FOP×>)
)ENDSEL
)SEL &CATCHECK = YES
CATCHECK
)ENDSEL
)SEL &GASCHECK = YES
GASCHECK
)ENDSEL
)SEL &SORT = NO
NOSORT
)ENDSEL
)SEL &OPTBLOCK ^= ^Z
OPTBLOCK(&OPTBLOCK)
)ENDSEL
)ENDSEL
)CM ******* END OF REPORT OPTION CARDS ********
)CM
)CM ******* START OF FREESPACE OPTION CARDS ********
)SEL &FSMOPT = FREESPACE
FREESPACE(MOUNT(&MOUNT)<,PREFIX(&PREFIX)×>)
)ENDSEL
)CM ******* END OF FREESPACE OPTION CARDS ********
)CM
)SEL &FSMOPT = RELEASEUNUSED
RELEASEUNUSED(MINR(&MINREL),MINS(&MINSEC)<&AOP×><,&FOP×>)
)ENDSEL
)CM ******* END OF RELEASEUNUSED OPTION CARDS ********
)CM
)CM ******* START OF CYLRELEASE OPTION CARDS *******
)SEL &FSMOPT = CYLRELEASE
CYLRELEASE(MINT(&MINTRK),MINC(&MINCYL)<&AOP×>)
)ENDSEL
)CM ******* END OF CYLRELEASE OPTION CARDS *******
)CM
)CM ******* START OF COMPRESSPDS OPTION CARDS *******
)SEL &FSMOPT = COMPRESSPDS
COMPRESSPDS(MINGAS(&MINGAS),PRINTCLASS(&PRNTCLAS)<,&AOP×><,&FOP×>)
)SEL &RESERVE = NO
NORESERVE
)ENDSEL
)SEL &GASCHECK = YES
GASCHECK
)ENDSEL
)SEL &SAVEDATE = YES
SAVEDATE
)ENDSEL
)CM *******   END OF COMPRESSPDS OPTION CARDS   ********
)ENDSEL
)CM
)CM ******* START OF UNCDELETE OPTION CARDS *******
)SEL &FSMOPT = UNCDELETE
UNCDELETE<(&AOP×><,&FOP×>)
CATCHECK
)SEL &PURGE = YES
PURGE
)ENDSEL
)SEL &ZERODEL = YES
ZERODELETE
)ENDSEL
)CM *******   END OF UNCDELETE OPTION CARDS   ********
)ENDSEL
)CM
)CM ******* START OF EXPDELETE OPTION CARDS *******
