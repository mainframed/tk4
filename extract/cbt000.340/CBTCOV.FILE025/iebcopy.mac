)SEL &COFUNC = COPYMOD
)SEL &REP = YES
 COPYMOD INDD=((COPYIN,R)),OUTDD=COPYOUT,MAXBLK=&MAXBLK
)ENDSEL
)SEL &REP = NO
 COPYMOD INDD=COPYIN,OUTDD=COPYOUT,MAXBLK=&MAXBLK
)ENDSEL
)ENDSEL
)SEL &COFUNC = COPY
)SEL &REP = YES
 COPY INDD=((COPYIN,R)),OUTDD=COPYOUT
)ENDSEL
)SEL &REP = NO
 COPY INDD=COPYIN,OUTDD=COPYOUT
)ENDSEL
)ENDSEL
)SEL &COFUNC = ALTERMOD
 ALTERMOD OUTDD=COPYIN
)ENDSEL
)SEL &IDSM = &Z
)DOT COPYOTBL
 S M=&DIRNAME
)ENDDOT
)ENDSEL
)SEL &IDSM ^= &Z  && &IDSM ^= *
 S M=&IDSM
)ENDSEL
