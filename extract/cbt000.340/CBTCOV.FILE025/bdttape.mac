&RJCARD1
//  TIME=10,MSGLEVEL=1,REGION=1024K,PRTY=7,
//  MSGCLASS=&MSGC,NOTIFY=&ZUSER
//*FORMAT PR,DDNAME=,DEST=&DEST
//GENER EXEC PGM=IEBGENER
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD DUMMY
//SYSUT1    DD DSN=&TMDSN,
//  VOL=SER=(&TAPEVOL1<,&TAPEVOL2×><,&TAPEVOL3×><,&TAPEVOL4×>),
//  UNIT=&TUNIT,
//  LABEL=(,&TPLABTYP),
)SEL &TDEN ^= &Z
//  DCB=(LRECL=&TPLRECL,BLKSIZE=&TPBLKSIZ,RECFM=&TPRECFM,DEN=&TDEN)
)ENDSEL
)SEL &TDEN = &Z
//  DCB=(LRECL=&TPLRECL,BLKSIZE=&TPBLKSIZ,RECFM=&TPRECFM)
)ENDSEL
//SYSUT2 DD DSN=&ZUSER..&TAPEVOL1..DISK,
//          UNIT=&TODUNIT,
)SEL &TOVOLSER ^= &Z
//          VOL=SER=&TOVOLSER,
)ENDSEL
//          DISP=(NEW,CATLG,DELETE),SPACE=(&TPBLKSIZ,&TOTBLK),
//          DCB=(LRECL=&TPLRECL,BLKSIZE=&TPBLKSIZ,RECFM=&TPRECFM)
//BDT   EXEC PGM=BDTBATCH
//SYSPRINT DD SYSOUT=*
//SYSIN DD *
Q JOBNAME(IN&TAPEVOL1) NETID(&TAPEVOL1) NETHOLD(1) MSGCLASS(NOTIFY)
FROM DSN(&ZUSER..&TAPEVOL1..CNTL)
     LOC(&Z$CENTER) DAP(SEQ) OLD DISP(DELETE,KEEP)
TO INTRDR DAP(SEQ) LOC(&TOCEN) BLKSIZE(80)
/EOT
Q JOBNAME(TD&TAPEVOL1) NETID(&TAPEVOL1) NETREL(IN&TAPEVOL1)
  MSGCLASS(NOTIFY) PRIORITY(3)
FROM DSN(&ZUSER..&TAPEVOL1..DISK)
     DAP(SEQ) LOC(&Z$CENTER) OLD DISP(DELETE,KEEP)
TO   DSN(&ZUSER..&TAPEVOL1..DISK)
     DAP(SEQ) LOC(&TOCEN) NEW DISP(CATLG,DELETE)
     BLOCK(&TPBLKSIZ) SPACE(&TOTBLK,10)
     UNIT(&TODUNIT)
)SEL &TOVOLSER ^= &Z
     VOLUME(&TOVOLSER)
)ENDSEL
     BLKSIZE(&TPBLKSIZ) LRECL(&TPLRECL) RECFM(&TPRECFM)
/EOT
