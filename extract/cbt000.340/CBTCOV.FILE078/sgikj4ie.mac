         MACRO
         SGIKJ4IE
         COPY  SGGBLPAK
&SGCTRLC(8) SETC '&SGCTRLC(24)'        SET CMDLIB DEVICE
&SGCTRLC(7) SETC '&SGCTRLC(23)'        SET CMDLIB VOLID
&SGCTRLC(9) SETC 'CMDLIB'              SET SYSLMOD NAME
&SGCTRLC(10) SETC ',RENT'              SET LINKAGE EDITOR ATTRIBUTES
&SGCTRLC(11) SETC ',REFR'
         COPY  SGLEDPK1
 PUNCH '//DCMDLIB DD DSNAME=SYS1.DCMDLIB,DISP=SHR,VOLUME=(,RETAIN)'
         COPY  SGLEDPK2
 PUNCH ' INCLUDE DCMDLIB(IKJLKL01,IKJLKMSG)'
 PUNCH ' ENTRY IKJLKL01'
 PUNCH ' NAME IKJLKL01(R)'
 PUNCH '/*'
         MEND
