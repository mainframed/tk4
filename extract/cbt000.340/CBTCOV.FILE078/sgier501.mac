         MACRO
         SGIER501
         COPY  SGGBLPAK
         PUNCH ' COPY OUTDD=LINKLIB,INDD=SM023'
         PUNCH '           SELECT MEMBER=(IERRCO00,SORT,IERRCB)      '
         PUNCH ' COPY OUTDD=SORTLIB,INDD=SORTLIBA'
 PUNCH ' SELECT MEMBER=(IERABQ,IERABR,IERADL,IERADM,IERAGH)'
 PUNCH ' SELECT MEMBER=(IERAMA,IERAMB,IERAMC,IERDM4,IERAPI)'
 PUNCH ' SELECT MEMBER=(IERRMB,IERRMC,IERRPG,IERAPJ,IERAP1)'
 PUNCH ' SELECT MEMBER=(IERAP2,IERRCT,IERRC6,IERRC7,IERRC8,IERRC9)'
 PUNCH ' SELECT MEMBER=(IERAP3,IERRCV,IERRDL,IERRGA,IERRMA)'
 PUNCH ' SELECT MEMBER=(IERAOV,IERROV,IERAOQ,IERROQ,IERAPC)'
 PUNCH ' SELECT MEMBER=(IERCHK,IERRPF,IERAPH,IERAPG,IERRPC)'
.*  THE ABOVE MODULES ARE ALWAYS SELECTED.  REMAINING MODULES ARE
.*  SELECTED BASED ON BITS SET IN SGSTMGB.  THESE BITS ARE ASSIGNED
.*  THE FOLLOWING MEANINGS
.*    1 = ENTIRE LIBRARY
.*    2 = FIXED LENGTH RECORDS
.*    3 = VARIABLE LENGTH RECORDS
.*    4 = RECORD LENGTH GREATER THAN 256 BYTES
.*    5 = SINGLE CONTROL FIELDS
.*    6 = MULTIPLE CONTROL FIELDS
.*    7 = 2301
.*    8 = 2311
.*    9 = 2400
.*   10 = MERGE ONLY
.*   11 = 2314
.*   12 = SORT MESSAGES ON PRINTER
.*   13 = SORT MESSAGES ON CONSOLE
.*   14 = ALL MESSAGES
.*   15 = CRITICAL MESSAGES
.*   16 = USER MODIFICATIONS
         PUNCH ' COPY OUTDD=SORTLIB,INDD=SM023'
.CAT1    AIF   (&SGSTMGB(1)+&SGSTMGB(9) EQ 0).CAT3
.*  CATEGORY 1 - TAPE (2400)
         PUNCH '           SELECT MEMBER=(IERRGB,IERRGD,IERROI,IERROJ)'
         PUNCH '           SELECT MEMBER=(IERROR,IERROS,IERRPA,IERRPD)'
         PUNCH '           SELECT MEMBER=(IERAGA,IERAGB,IERAGD,IERAGG)'
         PUNCH '           SELECT MEMBER=(IERAOI,IERAOJ,IERAOR,IERAOS)'
         PUNCH '           SELECT MEMBER=(IERAPA,IERAPD,IERAPK)'
         PUNCH '           SELECT MEMBER=(IERAGL,IERRGL,IERAGM,IERRGM)'
         PUNCH '           SELECT MEMBER=(IERAON,IERRON,IERRPM,IERAGN)'
.CAT3    AIF   (&SGSTMGB(1)+&SGSTMGB(7) EQ 0).CAT5
.*  CATEGORY 3 - 2301
         PUNCH '           SELECT MEMBER=(IERAGO,IERRGO,IERAGP,IERRGP)'
         PUNCH '           SELECT MEMBER=(IERAOO,IERROO,IERAO3,IERRO3)'
         PUNCH '           SELECT MEMBER=(IERAPN,IERRPN,IERAPO,IERRPO)'
.CAT5    AIF   (&SGSTMGB(1)+&SGSTMGB(7)+&SGSTMGB(8)+&SGSTMGB(11) EQ 0).X
               CAT6
.*  CATEGORY 5 - 2311 AND/OR 2301 AND/OR 2314
         PUNCH '           SELECT MEMBER=(IERAGK)'
         PUNCH ' SELECT MEMBER=(IERRGC,IERRGE,IERROK,IERROT,IERRPE)'
         PUNCH ' SELECT MEMBER=(IERRPB,IERAGC,IERAGE,IERAOK,IERAOT)'
         PUNCH ' SELECT MEMBER=(IERAPE,IERAPB,IERAGI,IERAGJ)'
.CAT6    AIF   (&SGSTMGB(1)+&SGSTMGB(9)+&SGSTMGB(11) EQ 0).CAT8
.*  CATEGORY 6 - 2400 TAPE AND/OR 2314
         PUNCH '           SELECT MEMBER=(IERAPL)'
.CAT8    AIF   (&SGSTMGB(1)+&SGSTMGB(11) EQ 0).CAT9
.* CATEGORY 8 - 2314
         PUNCH '           SELECT MEMBER=(IER8GB,IER9GB,IER8GC,IER9GC)'
         PUNCH '           SELECT MEMBER=(IER8PA,IER9PA,IER8ON,IER9ON)'
         PUNCH '           SELECT MEMBER=(IER9GN,IER8PM)'
.CAT9    AIF   (&SGSTMGB(1)+&SGSTMGB(2) EQ 0).CAT10
.*  CATEGORY 9 - FIXED LENGTH RECORDS
         PUNCH '           SELECT MEMBER=(IERRBL,IERABL,IERRDH,IERADH)'
         PUNCH '           SELECT MEMBER=(IERAOB,IERROB,IERABS)'
         PUNCH ' SELECT MEMBER=(IERRBB,IERRBG,IERRDB,IERABB,IERABG)'
         PUNCH ' SELECT MEMBER=(IERADB)'
.CAT10   AIF   (&SGSTMGB(1)+&SGSTMGB(4) EQ 0).CAT11
.*  CATEGORY 10- FIXED LENGTH RECORDS GREATER THAN 256 BYTES
         PUNCH '           SELECT MEMBER=(IERRBP,IERABP)'
         PUNCH ' SELECT MEMBER=(IERRBC,IERRBH,IERRDC,IERABC,IERABH)'
         PUNCH ' SELECT MEMBER=(IERADC)'
.CAT11   AIF   (&SGSTMGB(1)+&SGSTMGB(3) EQ 0).CAT12
.*  CATEGORY 11- VARIABLE LENGTH RECORDS
         PUNCH '           SELECT MEMBER=(IERRDI,IERADI,IERRBN,IERABN)'
         PUNCH '           SELECT MEMBER=(IERAOD,IERROD)'
         PUNCH ' SELECT MEMBER=(IERRBE,IERRBF,IERRBI,IERRDG,IERABE)'
         PUNCH ' SELECT MEMBER=(IERABF,IERABI,IERADG)'
         PUNCH ' SELECT MEMBER=(IER8BN,IER9BN)'                  A26453
.CAT12   AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(2) OR (NOT &SGSTMGB(9X
               ) AND NOT &SGSTMGB(11)))).CAT15
.*  CATEGORY 12 - FIXED LENGTH RECORDS WITH 2314, OR 2400 TAPES
         PUNCH '           SELECT MEMBER=(IERAOX,IERROX,IERADP,IERRDP)'
         PUNCH '           SELECT MEMBER=(IERABT,IERRBT,IERABY,IERRBY)'
.CAT15   AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(3) OR (NOT &SGSTMGB(9X
               ) AND NOT &SGSTMGB(11)))).CAT17
.*  CATEGORY 15 - VARIABLE LENGTH RECORDS WITH 2314, OR 2400 TAPE
         PUNCH '           SELECT MEMBER=(IERABA,IERRBA,IERABV,IERRBV)'
         PUNCH '           SELECT MEMBER=(IERAOZ,IERROZ,IERADT,IERRDT)'
.CAT17   AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(4) OR (NOT &SGSTMGB(9X
               ) AND NOT &SGSTMGB(11)))).CAT18
.*  CATEGORY 17 - FIXED LENGTH RECORDS GREATER THAN 256 BYTES, WITH
.*             2314, OR 2400 TAPES
         PUNCH '           SELECT MEMBER=(IERABU,IERRBU,IERABZ,IERRBZ)'
         PUNCH '           SELECT MEMBER=(IERADQ,IERRDQ)'
.CAT18   AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(2) OR NOT &SGSTMGB(16X
               ))).CAT19
.*  CATEGORY 18- FIXED LENGTH RECORDS WITH USER MODIFICATIONS
         PUNCH '           SELECT MEMBER=(IERABM,IERRBM)'
         PUNCH '           SELECT MEMBER=(IERRBJ,IERABJ,IERRDD,IERADD)'
.CAT19   AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(2) OR NOT &SGSTMGB(16X
               ) OR (NOT &SGSTMGB(9) AND NOT &SGSTMGB(11)))).CAT21
.*  CATEGORY 19 - FIXED LENGTH RECORDS WITH USER MODIFICATIONS AND
.*             2314 OR 2400 TAPE.
         PUNCH '           SELECT MEMBER=(IERABW,IERRBW,IERADR,IERRDR)'
.CAT21   AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(3) OR NOT &SGSTMGB(16X
               ))).CAT22
.*  CATEGORY 21- VARIABLE LENGTH RECORDS WITH USER MODIFICATIONS
         PUNCH '           SELECT MEMBER=(IERRBO,IERABO)'
         PUNCH '           SELECT MEMBER=(IERRBK,IERABK,IERRDE,IERADE)'
         PUNCH '           SELECT MEMBER=(IER8BO,IER9BO)'        A26453
.CAT22   AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(3) OR NOT &SGSTMGB(16X
               ) OR (NOT &SGSTMGB(9) AND NOT &SGSTMGB(11)))).CAT24
.*  CATEGORY 22 - VARIABLE LENGTH RECORDS WITH USER MODIFICATIONS AND
.*             2314 OR 2400 TAPES
         PUNCH '           SELECT MEMBER=(IERABX,IERRBX,IERADS,IERRDS)'
.CAT24   AIF   (&SGSTMGB(1)+&SGSTMGB(6) EQ 0).CAT25
.*  CATEGORY 24- MULTIPLE CONTROL FIELDS
         PUNCH '           SELECT MEMBER=(IERAOU,IERROU)'
         PUNCH '           SELECT MEMBER=(IERROP,IERAOP)'
.CAT25   AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(2) OR NOT &SGSTMGB(6)X
               )).CAT26
.*  CATEGORY 25- FIXED LENGTH RECORDS WITH MULTIPLE CONTROL FIELDS
         PUNCH '           SELECT MEMBER=(IERROA,IERAOA)'
.CAT26   AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(2) OR NOT &SGSTMGB(6)X
               OR (NOT &SGSTMGB(9) AND NOT &SGSTMGB(11)))).CAT27
.*  CATEGORY 26 - FIXED LENGTH RECORDS WITH MULITPLE CONTROL FIELDS AND
.*             2314 OR 2400 TAPES
         PUNCH '           SELECT MEMBER=(IERROW,IERAOW)'
.CAT27   AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(3) OR NOT &SGSTMGB(6)X
               )).CAT28
.*  CATEGORY 27- VARIABLE LENGTH RECORDS WITH MULTIPLE CONTROL FIELDS
         PUNCH '           SELECT MEMBER=(IERAOC,IERROC)'
.CAT28   AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(3) OR NOT &SGSTMGB(6)X
               OR (NOT &SGSTMGB(9) AND NOT &SGSTMGB(11)))).SKPVMCF
.*  CATEGORY 28 - VARIABLE LENGTH RECORDS WITH MULTIPLE CONTROL FIELDS
.*             AND 2314 OR 2400 TAPES
         PUNCH '           SELECT MEMBER=(IERAOY,IERROY)'
.SKPVMCF AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(2) OR NOT &SGSTMGB(6)X
                OR NOT &SGSTMGB(9))).SKPFMFT
.*               FIXED LENGTH RECORDS WITH MULTIPLE CONTROL FIELDS AND
.*               2400 TAPE
         PUNCH '           SELECT MEMBER=(IERROE,IERAOE)'
.SKPFMFT AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(2) OR NOT &SGSTMGB(9)X
               )).SKPFSFT
.*               FIXED LENGTH RECORDS WITH 2400 TAPE
         PUNCH '           SELECT MEMBER=(IERROF,IERAOF,IERADX,IERRDX)'
.SKPFSFT AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(3) OR NOT &SGSTMGB(9)X
               )).SKPVSFT
.*               VARIABLE LENGTH RECORDS WITH 2400 TAPE
         PUNCH '           SELECT MEMBER=(IERROH,IERAOH)'
.SKPVSFT AIF   (NOT &SGSTMGB(1) AND (NOT &SGSTMGB(3) OR NOT &SGSTMGB(6)X
                OR NOT &SGSTMGB(9))).SKPVMFT
.*               VARIABLE LENGTH RECORDS WITH MULTIPLE CONTROL FIELDS
.*               AND 2400 TAPE
         PUNCH '           SELECT MEMBER=(IERROG,IERAOG)'
.SKPVMFT AIF   (&SGSTMGB(1)+&SGSTMGB(10) EQ 0).SKPMERG
.*               MERGE ONLY
         PUNCH '           SELECT MEMBER=(IERRDJ,IERRGF)'
         PUNCH '           SELECT MEMBER=(IERAPF,IERADJ,IERAGF)'
         AIF   (&SGSTMGB(1)+&SGSTMGB(3) EQ 0).SKPMERG
         PUNCH '           SELECT MEMBER=(IER8DJ,IER9DJ)'
.SKPMERG AIF   (&SGSTMGB(1)+&SGSTMGB(16) EQ 0).SKPUMOD
.*               USER MODIFICATIONS
         PUNCH '           SELECT MEMBER=(IEREX1,IEREX2,IEREX3)'
.SKPUMOD ANOP
         MEND
