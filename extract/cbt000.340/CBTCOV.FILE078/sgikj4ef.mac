         MACRO
         SGIKJ4EF
.*********************************************************************
.***  THIS MACRO PRODUCES THE CONTROL STATEMENTS NECESSARY TO
.***  LINK-EDIT THE MODULES FOR THE TSO OUTPUT COMMAND.
.*********************************************************************
         COPY SGGBLPAK
&SGCTRLC(7)     SETC  '&SGCTRLC(23)'    CMDVOL
&SGCTRLC(8)    SETC  '&SGCTRLC(24)'    UNIT NAME OF CMDVOL
&SGCTRLC(9)    SETC  'CMDLIB'          LIBRARY NAME
&SGCTRLC(11)   SETC  ',REFR'
&SGCTRLC(12)   SETC  ',RENT'
         COPY SGLEDPK1
  PUNCH '//DCMDLIB DD DISP=SHR,VOLUME=(,RETAIN),DSNAME=SYS1.DCMDLIB'
         COPY SGLEDPK2
 PUNCH '  INCLUDE DCMDLIB(IKJEFF60,IKJEFF62,IKJEFF63)'
 PUNCH '  INCLUDE DCMDLIB(IKJEFF64,IKJEFF68)'
 PUNCH '  ENTRY IKJEFF60'
 PUNCH '  ALIAS OUTPUT'
 PUNCH '  ALIAS OUT'
 PUNCH '  NAME IKJEFF60(R)'
 PUNCH '/*'
&SGCTRLC(7)    SETC  '&SGCTRLC(32)'    LINKLIB VOLUME
&SGCTRLC(8)    SETC  '&SGCTRLC(31)'    LINKLIB UNIT NAME
&SGCTRLC(9)    SETC  'LINKLIB'         LIBRARY NAME
&SGCTRLC(11)   SETC  ',REFR'
&SGCTRLC(12)   SETC  ',RENT'
         COPY SGLEDPK1
 PUNCH '//CI505 DD DISP=SHR,VOLUME=(,RETAIN),DSNAME=SYS1.CI505'
 PUNCH '//CI555 DD DISP=SHR,VOLUME=(,RETAIN),DSNAME=SYS1.CI555'
         COPY SGLEDPK2
 PUNCH '  INCLUDE CI555(IKJEFF61,IKJEFF66)'
 PUNCH '  CHANGE  IEFSD110(IKJEFF61)'
 PUNCH '  INCLUDE  CI505(IEFQAGST,IEFLOCDQ)'
 PUNCH '  CHANGE  IEFSD112(IKJEFF61)'
 PUNCH '  INCLUDE CI505(IEFQDELQ,IEFCNVRT,IEFRDWRT)'
 PUNCH '  ENTRY IKJEFF61'
 PUNCH '  NAME IKJEFF61(R)'
 PUNCH '  INCLUDE SYSPUNCH(IKJEFF65)'
 PUNCH '  ENTRY IKJEFF65'
 PUNCH '  NAME IKJEFF65(R)'
 PUNCH '/*'
         MEND
