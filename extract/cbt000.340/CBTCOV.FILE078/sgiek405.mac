         MACRO
         SGIEK405
.*       THIS MACRO PUNCHES OUT THE JOB STREAM TO LINKEDIT THE UNIT
.*        ASSIGNMENT TABLE AND OPTION TABLE ONTO FORTLIB
         COPY  SGGBLPAK           DEFINE SYSGEN GLOBALS
&SGCTRLC(7)    SETC '&SGCTRLC(22)' SET VOLUME NUMBER FOR FORTLIB
&SGCTRLC(8)    SETC '&SGCTRLC(21)' SET UNIT NUMBER FOR FORTLIB
&SGCTRLC(9)    SETC 'FORTLIB'      DATA SET NAME
&SGCTRLC(10)   SETC ',LET'
         COPY SGLEDPK1             JCL FOR LINKEDIT
         COPY SGLEDPK2
         PUNCH '        INCLUDE     SYSPUNCH(IEK205)  '
         PUNCH '      NAME    IHCUATBL(R)        '
         AIF   (&SGFORLB(1)).END
     PUNCH    '    INCLUDE      SYSPUNCH(IEK206)                  '
         PUNCH '     NAME    IHCUOPT(R)          '
.END     ANOP
         PUNCH '/*'
         MEND
