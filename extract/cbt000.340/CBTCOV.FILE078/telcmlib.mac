         MACRO
          TELCMLIB  &UNIT=,&VOLNO=
         COPY  SGGBLPAK
         AIF   (NOT &SGMENTB(48)).TEL1
    MNOTE 5,'* * * IEITEL101 TELCMLIB MACRO PREVIOUSLY USED'
         AGO   .TEQUIT
.TEL1    ANOP
&SGMENTB(48) SETB  1
         AIF   (N'&UNIT EQ N'&VOLNO).TEL2
    MNOTE 5,'* * * IEITEL102 MISSING OPERAND IN TELCMLIB'
         AGO   .TEQUIT
.TEL2    AIF   ('&UNIT' EQ '').TELRES
         AIF   (K'&UNIT LE 8).TEL3
    MNOTE 5,'* * * IEITEL103 &UNIT IS INVALID TELCMLIB UNIT NAME'
         AGO   .TEQUIT
.TEL3    AIF   (K'&VOLNO LE 6).TEL4
    MNOTE 5,'* * * IEITEL104 &VOLNO IS INVALID VOLUME '
         AGO   .TEQUIT
.TEL4    ANOP
&SGCTRLC(33)  SETC  '&UNIT'
&SGCTRLC(34)  SETC  '&VOLNO'
         MEXIT
.TELRES  MNOTE *,'   TELECOMMUNICATION LIBRARY WILL RESIDE ON SYSRES'
         MEXIT
.TEQUIT  ANOP
&SGQUITB SETB  1
         MEND
