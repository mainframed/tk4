         MACRO
         SGIER201
         COPY SGGBLPAK
         LCLA  &PRNTOPT
         LCLA  &CSIZE
&CSIZE   SETA  &SGSTMGA
         AIF   (&SGSTMGB(1) OR (&SGSTMGB(13) AND &SGSTMGB(14))).CONSALL
         AIF   (&SGSTMGB(14) AND &SGSTMGB(12)).PRNTALL
         AIF   (&SGSTMGB(13) AND &SGSTMGB(15)).CONSCRT
         AIF   (&SGSTMGB(12) AND &SGSTMGB(15)).PRNTCRT
          AGO   .NOMSG
.PRNTALL ANOP
&PRNTOPT SETA  &PRNTOPT+4
.CONSALL ANOP
&PRNTOPT SETA  &PRNTOPT+4
.PRNTCRT ANOP
&PRNTOPT SETA  &PRNTOPT+4
.CONSCRT ANOP
&PRNTOPT SETA  &PRNTOPT+4
.NOMSG   ANOP
&SGCTRLC(6) SETC 'IERAM1'
         COPY  SGASMPAK
         PUNCH ' SGIER001 &CSIZE,&PRNTOPT'
         PUNCH ' END'
         PUNCH '/*'
         MEND
