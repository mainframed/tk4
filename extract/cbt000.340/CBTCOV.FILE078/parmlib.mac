         MACRO
         PARMLIB &UNIT=,&VOLNO=
         COPY  SGGBLPAK
         AIF   (NOT &SGMENTB(55)).PARM1
   MNOTE 5,'* * * IEIPRM101 PARMLIB MACRO PREVIOUSLY SPECIFIED'
         AGO   .PARMQ
.PARM1   ANOP
&SGMENTB(55)   SETB 1
         AIF   (N'&UNIT EQ N'&VOLNO).PARM2
   MNOTE 5,'* * * IEIPRM102 UNIT OR VOLNO VALUE MISSING OR INVALID'
         AGO   .PARMQ
.PARM2   ANOP
          AIF   ('&UNIT' EQ '').M1
         AIF   (K'&UNIT LE 8).PARM3
   MNOTE 5,'* * * IEIPRM103 UNIT VALUE &UNIT INVALID'
         AGO   .PARMQ
.PARM3   AIF   (K'&VOLNO LE 6).PARM4
   MNOTE 5,'* * * IEIPRM104 VOLNO VALUE &VOLNO INVALID'
         AGO   .PARMQ
.PARM4   ANOP
&SGCTRLC(44)   SETC '&UNIT'
&SGCTRLC(45)   SETC '&VOLNO'
   MNOTE *,'      PARMLIB WILL RESIDE ON VOLUME &VOLNO'
         MEXIT
.M1 MNOTE *,'      PARMLIB WILL RESIDE ON SYSRES'
         MEXIT
.PARMQ   ANOP
&SGQUITB SETB  1
         MEND
