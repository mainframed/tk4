         MACRO
         DCMLIB &UNIT=,&VOLNO=
         COPY  SGGBLPAK
         AIF   (NOT &SGMENTB(89)).A
    MNOTE 5,'* * * IEIDCM100 DCMLIB MACRO PREVIOUSLY USED'
         AGO   .QUIT
.A       ANOP
&SGMENTB(89) SETB  1
         AIF   (N'&UNIT EQ N'&VOLNO).B
    MNOTE 5,'* * * IEIDCM200 UNIT VALUE OR VOLNO VALUE MISSING'
         AGO   .QUIT
.B       AIF   ('&UNIT' NE '').C
    MNOTE *,'     DCM LIBRARY WILL RESIDE ON LNKVOL'
         AGO   .MEND
.C       AIF   (K'&UNIT LE 8).D
    MNOTE 5,* * * IEIDCM300 UNIT VALUE &UNIT INVALID'
         AGO   .QUIT
.D       AIF   (K'&VOLNO LE 6).E
    MNOTE 5,'* * * IEIDCM400 VOLNO VALUE &VOLNO INVALID'
         AGO   .QUIT
.E       ANOP
&SGCTRLC(65) SETC '&UNIT'
&SGCTRLC(66) SETC '&VOLNO'
   MNOTE *,'      DCMLIB WILL RESIDE ON VOLUME &VOLNO'
         AGO   .MEND
.QUIT    ANOP
&SGQUITB SETB  1
.MEND    MEND
