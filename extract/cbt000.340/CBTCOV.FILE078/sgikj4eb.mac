         MACRO
         SGIKJ4EB
.*C87600                                                         A45714
.*********************************************************************
.***  THIS MACRO PRODUCES THE JOBSTREAM NECESSARY TO LINK-EDIT
.***  THE TSO MODULES FOR THE EDIT COMMAND.
.*********************************************************************
         COPY SGGBLPAK
&SGCTRLC(7)    SETC  '&SGCTRLC(23)'    CMDVOL
&SGCTRLC(8)    SETC  '&SGCTRLC(24)'    UNIT NAME OF CMDLIB
&SGCTRLC(9)    SETC  'CMDLIB'          LIBRARY NAME
&SGCTRLC(11)   SETC  ',REFR'
&SGCTRLC(12)   SETC  ',RENT'
         COPY SGLEDPK1
 PUNCH '//DCMDLIB DD DISP=SHR,VOLUME=(,RETAIN),DSNAME=SYS1.DCMDLIB'
         COPY SGLEDPK2
 PUNCH '  INCLUDE DCMDLIB(IKJEBEFI,IKJEBESE)'
 PUNCH '  ENTRY IKJEBEFI'
 PUNCH '  NAME IKJEBEFI(R)'
 PUNCH '  INCLUDE DCMDLIB(IKJEBECG,IKJEBESE)'
 PUNCH '  ENTRY IKJEBECG'
 PUNCH '  NAME IKJEBECG(R)'
 PUNCH '  INCLUDE SYSPUNCH(IKJEBEIN)'                            A45714
 PUNCH '  ENTRY IKJEBEIN'
 PUNCH '  ALIAS EDIT'
 PUNCH '  ALIAS IKJEBEIN'
 PUNCH '  NAME E(R)'
 PUNCH '  INCLUDE DCMDLIB(IKJEBEMA,IKJEBEAT,IKJEBEUT,IKJEBEAE)'
 PUNCH '  ENTRY IKJEBEMA'
 PUNCH '  NAME IKJEBEMA(R)'
 PUNCH '  INCLUDE DCMDLIB(IKJEBEEN)'
 PUNCH '  ENTRY IKJEBEEN'
 PUNCH '  ALIAS IKJEBEXT'
 PUNCH '  NAME IKJEBEEN(R)'
 PUNCH '/*'
&SGCTRLC(7)    SETC  '&SGCTRLC(23)'    CMDVOL
&SGCTRLC(8)    SETC  '&SGCTRLC(24)'    UNIT NAME OF CMDLIB
&SGCTRLC(9)    SETC  'CMDLIB'          LIBRARY NAME
&SGCTRLC(10)   SETC  ',OL'
&SGCTRLC(11)   SETC  ',REFR'
&SGCTRLC(12)   SETC  ',RENT'
         COPY SGLEDPK1
 PUNCH '//DCMDLIB DD DISP=SHR,VOLUME=(,RETAIN),DSNAME=SYS1.DCMDLIB'
         COPY SGLEDPK2
 PUNCH '  INCLUDE DCMDLIB(IKJEBEPS)'
 PUNCH '  INCLUDE SYSPUNCH(IKJEBEPD)'
 PUNCH '  ENTRY IKJEBEPS'
 PUNCH '  NAME IKJEBEPS(R)'
 PUNCH '/*'
         MEND
