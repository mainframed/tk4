         MACRO
         SGIKM400
.*                 REL 20 DELETIONS.
.*  300000-420000,540000,680000-700000                           S20046
.*  536000,*  668000,*  704000,*                 DCR526           M2254
.*   680000,*                                                     M0790
.* R20.1  500000  640000  692000                                  M5790
.* R21.6 C 528000,664000,700000                               MCB 49385
         COPY  SGGBLPAK
         AIF   (NOT &SGCKPLB(1)).END
.* TYPE=PL1 SPECIFIED IN CHECKER MACRO
.*  GENERATE JCL TO LINK PL1 SYNTAX CHECKER MODULES TO LINKLIB
&SGCTRLC(7)  SETC  '&SGCTRLC(32)'    VOLUME NUMBER FOR LINKLIB
&SGCTRLC(8)  SETC  '&SGCTRLC(31)'    UNIT NAME FOR LINKLIB
&SGCTRLC(9)  SETC  'LINKLIB'         OUTPUT LIBRARY
&SGCTRLC(6)  SETC  ''                MEMBER NAMES WITH NAME CARDS
&SGCTRLC(11) SETC  ',RENT'            MODULES ARE REENTRANT
         COPY  SGLEDPK1
         PUNCH '//PL552 DD DISP=SHR,VOL=(,RETAIN),DSN=SYS1.PL552'
         COPY  SGLEDPK2
         AIF   (NOT &SGCKPLB(3)).NO20K
.* SIZE=20K SPECIFIED IN CHECKER MACRO OR SIZE DEFAULTED
         PUNCH ' INCLUDE  PL552(IKM001,IKM02,IKM03,IKM11,IKM12)'
         PUNCH ' INCLUDE  PL552(IKM131,IKM132,IKM133,IKM134)'
         PUNCH ' INCLUDE  PL552(IKM135,IKM136)'
         PUNCH ' ENTRY IKM001 '                                   M5790
         PUNCH ' ALIAS  PLICHK20 '
         AIF   (NOT &SGSCHDB(44)).NOTS20                          49385
         PUNCH ' ALIAS PLIFSCAN'
         PUNCH ' ALIAS PLISCAN '
         MNOTE *,' TSO WILL USE THE 20K VERSION OF PL/I SYNTAX CHECKER'
.NOTS20  PUNCH ' NAME  IKM001(R)'
.NO20K   AIF   (NOT &SGCKPLB(4)).NO27K
.* SIZE=27K SPECIFIED IN CHECKER MACRO
         PUNCH ' INCLUDE  PL552(IKM002,IKM02,IKM03,IKM11,IKM12)'
         PUNCH ' INCLUDE  PL552(IKM131,IKM132,IKM133,IKM134)'
         PUNCH ' INCLUDE PL552(IKM135,IKM136,IKM23,IKM22,IKM21)'
         PUNCH ' ENTRY IKM002 '                                   M5790
         PUNCH ' ALIAS  PLICHK27 '
         AIF   (NOT &SGSCHDB(44) OR &SGCKPLB(3)).NOTS27           49385
         PUNCH ' ALIAS PLIFSCAN'
         PUNCH ' ALIAS PLISCAN '
         MNOTE *,' TSO WILL USE THE 27K VERSION OF PL/I SYNTAX CHECKER'
.NOTS27  PUNCH ' NAME  IKM002(R)'
.NO27K   AIF   (NOT &SGCKPLB(2)).NO16K                            M0790
.* SIZE=16K SPECIFIED IN CHECKER MACRO
         PUNCH ' INCLUDE PL552(IKM003,IKM02,IKM03,IKM11,IKM12)'
         PUNCH ' INCLUDE PL552(IKM132,IKM133,IKM134,IKM135)'
         PUNCH ' ENTRY IKM003 '                                   M5790
         PUNCH ' ALIAS PLICHK16'
         AIF   (NOT &SGSCHDB(44)).NOTS16                          49385
         AIF   (&SGCKPLB(3) OR &SGCKPLB(4)).NOTS16                49385
         PUNCH ' ALIAS PLIFSCAN'
         PUNCH ' ALIAS PLISCAN '
         MNOTE *,' TSO WILL USE THE 16K VERSION OF PL/I SYNTAX CHECKER'
.NOTS16  PUNCH ' NAME  IKM003(R)'
.NO16K   ANOP
         PUNCH '/*'
.END     MEND
