          MACRO
          SGIKJ4EG
.********************************************************************
.***  THIS MACRO PRODUCES THE LINKEDIT CONTROL STATEMENTS
.***  NECESSARY TO LINKAGE EDIT THE LINK-EDITABLE MODULES
.***  FOR THE TSO TEST FUNCTION.
.********************************************************************
          COPY SGGBLPAK
&SGCTRLC(7)   SETC  '&SGCTRLC(23)'        CMDVOL
&SGCTRLC(8)  SETC  '&SGCTRLC(24)'        UNIT NAME OF CMDVOL
&SGCTRLC(9)  SETC  'CMDLIB'              LIBRARY NAME
&SGCTRLC(10)   SETC  ',REFR'
&SGCTRLC(11)   SETC  ',RENT'
          COPY SGLEDPK1
 PUNCH '//DCMDLIB DD DISP=SHR,VOLUME=(,RETAIN),DSNAME=SYS1.DCMDLIB'
          COPY SGLEDPK2
 PUNCH '  INCLUDE DCMDLIB(IKJEGCVT,IKJEGATN,IKJEGSTA)'
 PUNCH '  INCLUDE DCMDLIB(IKJEGIO,IKJEGSCD,IKJEGMNL)'
 PUNCH '  ENTRY  IKJEGMNL'
 PUNCH '  NAME   IKJEGMNL(R)'
 PUNCH '  INCLUDE DCMDLIB(IKJEGINT,IKJEGIO,IKJEGSCD)'
 PUNCH '  ENTRY IKJEGINT'
 PUNCH '  NAME TEST(R)'
 PUNCH '/*'
          MEND
