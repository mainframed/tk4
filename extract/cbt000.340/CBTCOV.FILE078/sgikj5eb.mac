          MACRO
          SGIKJ5EB
.*C700000                                                        A42956
.***  THIS MACRO PRODUCES THE COPY STATEMENTS NECESSARY TO
.***  COPY THE MODULES NEEDED FOR THE EDIT COMMAND AND
.***  SUBCOMMANDS IN A TSO SYSTEM.
.********************************************************************
 PUNCH  '  COPY OUTDD=CMDLIB,INDD=DCMDLIB'
 PUNCH  '   SELECT MEMBER=(IKJEBERU,IKJEBEMS,IKJEBEFC,IKJEBESA)'
 PUNCH  '   SELECT MEMBER=(IKJEBECO,IKJEBELT,IKJEBELI,IKJEBEMR)'
 PUNCH  '   SELECT MEMBER=(IKJEBEM5,IKJEBEIM,IKJEBEIP,IKJEBESC)'
 PUNCH  '   SELECT MEMBER=(IKJEBELE,IKJEBESN,IKJEBECI,IKJEBEUI)'
 PUNCH  '   SELECT MEMBER=(IKJEBEUT,IKJEBEEX,IKJEBEAA,IKJEBEIA)'
 PUNCH  '   SELECT MEMBER=(IKJEBEDE,IKJEBEME,IKJEBERE,IKJEBEIS)'
 PUNCH  '   SELECT MEMBER=(IKJEBETA,IKJEBEDO,IKJEBEHE)'          A42956
 PUNCH  '   SELECT MEMBER=(IKJEBEFO,IKJEBETO,IKJEBEBO,IKJEBEUP)'
 PUNCH  '   SELECT MEMBER=(IKJEBEVE,IKJEBEDA,IKJEBEM6,IKJEBEM7)'
 PUNCH  '   SELECT MEMBER=(IKJEBEM1,IKJEBEM2,IKJEBEM3,IKJEBEM4)'
 PUNCH  '   SELECT MEMBER=(IKJEBECH,IKJEBECN,IKJEBERN)'
          MEND
