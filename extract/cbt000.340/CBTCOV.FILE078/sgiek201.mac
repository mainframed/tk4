         MACRO
         SGIEK201
.*       140000-320000                                          LL45297
.*             SGIEK201 IS A FORTRAN H SYSGEN MACRO
         COPY  SGGBLPAK            DEFINE GLOBALS FOR SYSGEN
         LCLA  &OPT,&LINECNT,&STORMAP,&TYPERUN,&PUNCH,&OBJLIST,&SORCODE*
               ,&SORLST,&OBJID,&SOREDIT,&SORXREF
&OPT     SETA  &SGFORTA(1)              OPTIMIZATION LEVEL FOR COMPILER
&LINECNT SETA  &SGFORTA(6)              LINE COUNT FOR COMPILER
.STORMAP AIF   (&SGFORTB(23)).TYPERUN   STORMAP OPTION TEST
&STORMAP SETA  1                  MAP OPTION
.TYPERUN AIF   (&SGFORTB(25)).PUNCH     LOAD OPTION TEST
&TYPERUN SETA  1                  LOAD DEFAULT OPTION
.PUNCH   AIF   (&SGFORTB(19)).OBJLIST   PUNCH OPTION TEST
&PUNCH   SETA  1                  DECK OPTION
.OBJLIST AIF   (NOT &SGFORTB(28)).SORCODE OBJLIST TEST
&OBJLIST SETA  1                  LIST OPTION
.SORCODE AIF   (&SGFORTB(27)).SORLIST   SORCODE OPTION TEST
&SORCODE SETA  1                  BCD OPTION
.SORLIST AIF   (&SGFORTB(21)).OBJCTID
&SORLST  SETA  1
.OBJCTID AIF   (&SGFORTB(29)).SOREDIT
&OBJID   SETA  1
.SOREDIT AIF   (&SGFORTB(44)).SORXREF
&SOREDIT SETA  1
.SORXREF AIF   (NOT &SGFORTB(49)).JCL
&SORXREF SETA  1
.JCL     ANOP
&SGCTRLC(6) SETC 'IEK201'
         COPY  SGASMPAK            PROVIDE JCL
         PUNCH '         SGIEK001   OPTN=&OPT,MAP=&STORMAP,TYPE=&TYPERU*
               N,PNCH=&PUNCH,OBLST=&OBJLIST,CODE=&SORCODE,         *'
         PUNCH '               SRLST=&SORLST,LINCNT=&LINECNT,SZE=&SGFOR*
               TA(5),ID=&OBJID,EDIT=&SOREDIT,XREF=&SORXREF'
         PUNCH '         END'
         PUNCH '/*'
         MEND
