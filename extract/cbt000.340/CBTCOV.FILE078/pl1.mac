         MACRO
 PL1 &DESIGN=,&PUNCH=NODECK,&TYPERUN=LOAD,&SORCODE=EBCDIC,&SIZE=999999,X
               &OBJLIST=NOLIST,&MSGLEV=FLAGW,&OPT=1,&SORLIST=SOURCE,   *
               &CHARSET=CHAR60,&EXTLIST=NOEXTREF,&ATRLIST=NOATR,       X
               &REFLIST=NOXREF,&SORMGIN=(2,72),&LINECNT=50,&STMDIAG=NOSX
               TMT,&CMPTIME=NOMACRO,&COMPILE=COMP,&MACLIST=SOURCE2,&DEL*
               ETE=,&OBJCODE=OBJOUT,&PAGECTL=,&MCPUNCH=NOMACDECK,&DICTYX
               PE=NOEXTDIC,&OPRINT=OP,&LEVCNT=NONEST,&SYNTXLV=SYNCHKT
         COPY  SGGBLPAK
         LCLA  &I,&L,&N,&M,&K
         LCLB  &B,&F,&H
   LCLC  &C(46)
 AIF (NOT &SGMENTB(30)).A1
         AGO   .E1
.A1 AIF  ('&DESIGN' NE 'F').E2
    MNOTE *,'      PL/1 DESIGN LEVEL &DESIGN SPECIFIED'
&SGMENTB(30)  SETB  1
&F   SETB  (&SGPL11B(1))
&SGPL11B(1) SETB ('&DESIGN' EQ 'F')
&I       SETA  0
&L       SETA  0
.A2 AIF ('&PUNCH' NE 'NODECK' AND '&PUNCH' NE 'DECK').E3
&SGPL11B(&I+8) SETB ('&PUNCH' EQ 'DECK')
&SGPL11B(&I+9) SETB ('&PUNCH' EQ 'NODECK')
    MNOTE *,'      PUNCH OPTION FOR OBJECT DECK IS &PUNCH'
.A3 AIF ('&TYPERUN' NE 'NOLOAD' AND '&TYPERUN' NE 'LOAD').E4
&SGPL11B(&I+18) SETB ('&TYPERUN' EQ 'NOLOAD')
&SGPL11B(&I+17) SETB ('&TYPERUN' EQ 'LOAD')
    MNOTE *,'      RUN OPTION FOR OBJECT PROGRAM IS &TYPERUN'
.A4 AIF ('&SORCODE' NE 'EBCDIC' AND '&SORCODE' NE 'BCD').E5
&SGPL11B(&I+4) SETB ('&SORCODE' EQ 'BCD')
&SGPL11B(&I+5) SETB ('&SORCODE' EQ 'EBCDIC')
    MNOTE *,'      SOURCE CODE CHARACTER SET OPTION IS &SORCODE'
.A5  AIF  (T'&SIZE  NE  'N').E6
    AIF  (&SIZE LT 45056 OR &SIZE GT 999999).E6
&SGPL11A(&L+3) SETA &SIZE
    MNOTE *,'      MAIN STORAGE FOR USE BY COMPILER IS &SIZE BYTES'
.A6 AIF ('&OBJLIST' NE 'NOLIST' AND '&OBJLIST' NE 'LIST').E7
&SGPL11B(&I+15) SETB ('&OBJLIST' EQ 'LIST')
&SGPL11B(&I+16) SETB ('&OBJLIST' EQ 'NOLIST')
    MNOTE *,'      LISTING OPTION FOR OBJECT PROGRAM IS &OBJLIST'
.A7 AIF ('&MSGLEV' NE 'FLAGW' AND '&MSGLEV' NE 'FLAGE' AND '&MSGLEV' NEX
                'FLAGS').E8
&SGPL11B(&I+12) SETB ('&MSGLEV' EQ 'FLAGW')
&SGPL11B(&I+13) SETB ('&MSGLEV' EQ 'FLAGE')
&SGPL11B(&I+14) SETB ('&MSGLEV' EQ 'FLAGS')
    MNOTE *,'      COMPILATION ERROR MESSAGE OPTION IS &MSGLEV'
.A8      AIF   ('&OPT' NE '0' AND '&OPT' NE '1' AND '&OPT' NE '2').E9
&SGPL11B(&I+25) SETB ('&OPT' EQ '0')
&SGPL11B(&I+26) SETB ('&OPT' EQ '1')
&SGPL11B(&I+41) SETB ('&OPT' EQ '2')
    MNOTE *,'      OBJECT PROGRAM OPTIMIZATION OPTION IS &OPT'
.A9 AIF ('&SORLIST' NE 'SOURCE' AND '&SORLIST' NE 'NOSOURCE').EA
&SGPL11B(&I+21) SETB ('&SORLIST' EQ 'SOURCE')
&SGPL11B(&I+22) SETB ('&SORLIST' EQ 'NOSOURCE')
    MNOTE *,'      LISTING OPTION FOR SOURCE PROGRAM IS &SORLIST'
.A91 AIF ('&MACLIST' NE 'SOURCE2' AND '&MACLIST' NE 'NOSOURCE2').EA1
&SGPL11B(&I+23) SETB ('&MACLIST' EQ 'SOURCE2')
&SGPL11B(&I+24) SETB ('&MACLIST' EQ 'NOSOURCE2')
    MNOTE *,'      LISTING OPTION FOR MACROS IS &MACLIST'
.A92 AIF ('&OBJCODE' NE 'OBJOUT' AND '&OBJCODE' NE 'OBJIN').EA2
&SGPL11B(&I+27) SETB ('&OBJCODE' EQ 'OBJIN')
&SGPL11B(&I+28) SETB ('&OBJCODE' EQ 'OBJOUT')
    MNOTE *,'      LISTING OPTION FOR OBJCODE IS &OBJCODE'
.AA AIF ('&CHARSET' NE 'CHAR60' AND '&CHARSET' NE 'CHAR48').EB
&SGPL11B(&I+6)  SETB ('&CHARSET' EQ 'CHAR60')
&SGPL11B(&I+7)  SETB ('&CHARSET' EQ 'CHAR48')
    MNOTE *,'      OPTION FOR NUMBER OF CHARACTERS IS &CHARSET'
.AB AIF ('&EXTLIST' NE 'NOEXTREF' AND '&EXTLIST' NE 'EXTREF').EC
&SGPL11B(&I+11) SETB ('&EXTLIST' EQ 'NOEXTREF')
&SGPL11B(&I+10) SETB ('&EXTLIST' EQ 'EXTREF')
    MNOTE *,'      LISTING OPTION FOR EXTERNAL ENTRIES IS &EXTLIST'
.AC AIF ('&ATRLIST' NE 'NOATR' AND '&ATRLIST' NE 'ATR').ED
&SGPL11B(&I+3)  SETB ('&ATRLIST' EQ 'NOATR')
&SGPL11B(&I+2)  SETB ('&ATRLIST' EQ 'ATR')
    MNOTE *,'      LISTING OPTION FOR IDENTIFIERS IS &ATRLIST'
.AD AIF ('&REFLIST' NE 'NOXREF' AND '&REFLIST' NE 'XREF').EE
&SGPL11B(&I+20) SETB ('&REFLIST' EQ 'NOXREF')
&SGPL11B(&I+19) SETB ('&REFLIST' EQ 'XREF')
    MNOTE *,'      LISTING OPTION FOR REFERENCES IS &REFLIST'
.AE AIF (T'&SORMGIN(1) NE 'N' OR T'&SORMGIN(2) NE 'N').EF
  AIF   (&SORMGIN(1) LT 1 OR &SORMGIN(1) GT &SORMGIN(2) OR &SORMGIN(2) *
               GT 100).EF
&SGPL11A(&L+4)  SETA &SORMGIN(1)
&SGPL11A(&L+5)  SETA &SORMGIN(2)
    MNOTE *,'      MARGINS FOR COMPILER SCAN ARE &SORMGIN(1) AND &SORMGX
               IN(2)'
.AF  AIF  (T'&LINECNT NE 'N').EG
   AIF  (&LINECNT LT 10 OR &LINECNT GT 99).EG
&SGPL11A(&L+1)  SETA &LINECNT
    MNOTE *,'      MAXIMUM LINES PER PAGE FOR COMPILER OUTPUT IS &LINECX
               NT'
.AG AIF ('&STMDIAG' NE 'STMT' AND '&STMDIAG' NE 'NOSTMT').EH
&SGPL11B(&I+35) SETB ('&STMDIAG' EQ 'STMT')
&SGPL11B(&I+36) SETB ('&STMDIAG' EQ 'NOSTMT')
    MNOTE *,'      SOURCE PROGRAM MESSAGE OPTION IS &STMDIAG'
.AG1 AIF ('&CMPTIME' NE 'MACRO' AND '&CMPTIME' NE 'NOMACRO').EI
&SGPL11B(&I+37) SETB ('&CMPTIME' EQ 'MACRO')
&SGPL11B(&I+38) SETB ('&CMPTIME' EQ 'NOMACRO')
    MNOTE *,'      COMPILE TIME PREPROCESSOR OPTION IS &CMPTIME'
.AG2 AIF ('&COMPILE' NE 'COMP' AND '&COMPILE' NE 'NOCOMP').EJ
&SGPL11B(&I+44) SETB ('&COMPILE' EQ 'COMP')
&SGPL11B(&I+45) SETB ('&COMPILE' EQ 'NOCOMP')
    MNOTE *,'      COMPILATION PREPROCESSING OPTION IS &COMPILE'
.AG3 AIF (T'&PAGECTL EQ 'O').AG31
 AIF (T'&PAGECTL NE 'N').EK
 AIF (&PAGECTL LT 1 OR &PAGECTL GT 100).EK
 AIF (&PAGECTL GE &SORMGIN(1) AND &PAGECTL LE &SORMGIN(2)).EK
 AIF (&PAGECTL GT &SORMGIN(1) AND &PAGECTL LT &SORMGIN(2)).EK
&SGPL11A(6) SETA &PAGECTL
 MNOTE *,' THE ASA CONTROL CHARACTER WILL BE PLACED IN COLUMN &PAGECTL'
 AGO .AG4
.AG31 MNOTE *,' NO SPACING CONTROL PROVIDED'
&SGPL11A(6) SETA 0
.AG4 AIF ('&MCPUNCH' NE 'MACDECK' AND '&MCPUNCH' NE 'NOMACDECK').EL
&SGPL11B(29) SETB ('&MCPUNCH' EQ 'MACDECK')
&SGPL11B(30) SETB ('&MCPUNCH' EQ 'NOMACDECK')
 MNOTE *,' DEFAULT OPTION FOR GENERATION OF MACRO DECK IS &MCPUNCH'
.AG5 AIF ('&DICTYPE' NE 'EXTDIC' AND '&DICTYPE' NE 'NOEXTDIC').EM
&SGPL11B(31) SETB ('&DICTYPE' EQ 'EXTDIC')
&SGPL11B(32) SETB ('&DICTYPE' EQ 'NOEXTDIC')
 MNOTE *,' TYPE OF DICTIONARY REFERENCING MECHANISM IS &DICTYPE'
.AG6 AIF ('&OPRINT' NE 'OP' AND '&OPRINT' NE 'NOOP').EN
&SGPL11B(33) SETB ('&OPRINT' EQ 'OP')
&SGPL11B(34) SETB ('&OPRINT' EQ 'NOOP')
 MNOTE *,' OPTION FOR WRITING A COMPLETE LIST OF OPTIONS ON SYSPRINT IS*
                &OPRINT'
.AG7 AIF ('&LEVCNT' NE 'NEST' AND '&LEVCNT' NE 'NONEST').EO
&SGPL11B(39) SETB ('&LEVCNT' EQ 'NEST')
&SGPL11B(40) SETB ('&LEVCNT' EQ 'NONEST')
 MNOTE *,' SOURCE LISTING OPTION FOR DEPTH COUNTS OF BLOCK LEVEL AND NE*
               STED DO GROUPS IS &LEVCNT'
.AH      AIF   ('&SYNTXLV' NE 'SYNCHKE' AND '&SYNTXLV' NE 'SYNCHKS' ANDX
               '&SYNTXLV' NE 'SYNCHKT').EP
&SGPL11B(42)  SETB ('&SYNTXLV' EQ 'SYNCHKE')
&SGPL11B(43)  SETB ('&SYNTXLV' EQ 'SYNCHKS')
&SGPL11B(157) SETB ('&SYNTXLV' EQ 'SYNCHKT')
 MNOTE *,' SYNTAX CHECKING OPTION IS &SYNTXLV'
.AH1     AIF   (T'&DELETE EQ 'O').AN
&C(1)    SETC  'ATR'
&C(2)    SETC  'NOATR'
&C(3)    SETC  'BCD'
&C(4)    SETC  'EBCDIC'
&C(5)    SETC  'CHAR60'
&C(6)    SETC  'CHAR48'
&C(7)    SETC  'DECK'
&C(8)    SETC  'NODECK'
&C(9)    SETC  'EXTREF'
&C(10)   SETC  'NOEXTREF'
&C(11)   SETC  'FLAGW'
&C(12)   SETC  'FLAGE'
&C(13)   SETC  'FLAGS'
&C(14)   SETC  'LIST'
&C(15)   SETC  'NOLIST'
&C(16)   SETC  'LOAD'
&C(17)   SETC  'NOLOAD'
&C(18)   SETC  'XREF'
&C(19)   SETC  'NOXREF'
&C(20)   SETC  'SOURCE'
&C(21)   SETC  'NOSOURCE'
&C(22)   SETC  'SOURCE2'
&C(23)   SETC  ''
&C(24)   SETC  'OPT'
&C(25)   SETC  'LINECNT'
&C(26)   SETC  ''
&C(27)   SETC  'SIZE'
&C(28)   SETC  'SORMGIN'
&C(29)   SETC  'DUMP'
&C(30)   SETC  'STMT'
&C(31)   SETC  'NOSTMT'
&C(32)   SETC  'MACRO'
&C(33)   SETC  'NOMACRO'
&C(34)   SETC  'COMP'
&C(35)   SETC  'NOCOMP'
&C(36)   SETC  'OBJIN'
&C(37)   SETC  'OBJOUT'
&C(38) SETC 'PAGECTL'
&C(39) SETC 'MACDECK'
&C(40) SETC ''
&C(41) SETC 'EXTDIC'
&C(42) SETC 'NOEXTDIC'
&C(43) SETC 'OP'
&C(44) SETC 'NOOP'
&C(45) SETC 'NEST'
&C(46) SETC 'NONEST'
&N       SETA  N'&DELETE
&M       SETA  1
.AI      ANOP
&K       SETA  1
         AIF   ('&DELETE(&M)' EQ '').AM
.AJ      AIF   (&K GT 46).AL
         AIF   ('&C(&K)' EQ '&DELETE(&M)').AK
 AIF ('NOSOURCE2' EQ '&DELETE(&M)').AK1
 AIF ('NOMACDECK' EQ '&DELETE(&M)').AK2
 AIF  ('SYNCHKE' EQ '&DELETE(&M)').AK3
 AIF  ('SYNCHKS' EQ '&DELETE(&M)').AK4
 AIF  ('SYNCHKT' EQ '&DELETE(&M)').AK5
&K       SETA  &K+1
         AGO   .AJ
.AK MNOTE *,'      COMPILATION OPTION PARAMETER &DELETE(&M) DELETED'
&SGPL11B(&I+&K+47) SETB 1
         AGO   .AM
.AK1 MNOTE *,'     COMPILATION OPTION PARAMETER &DELETE(&M) DELETED'
&SGPL11B(70) SETB 1
         AGO   .AM
.AK2 MNOTE *,'     COMPILATION OPTION PARAMETER &DELETE(&M) DELETED'
&SGPL11B(87) SETB 1
 AGO .AM
.AK3 MNOTE *,' COMPILATION OPTION  PARAMETER &DELETE(&M) DELETED'
&SGPL11B(158)  SETB 1
         AGO   .AM
.AK4 MNOTE *,' COMPILATION OPTION  PARAMETER &DELETE(&M) DELETED'
&SGPL11B(159)  SETB 1
         AGO   .AM
.AK5 MNOTE *,' COMPILATION OPTION  PARAMETER &DELETE(&M) DELETED'
&SGPL11B(160)  SETB 1
         AGO   .AM
.AL MNOTE 5,'* * * IEIPL1117 DELETE VALUE &DELETE(&M) INVALID'
&B       SETB  1
.AM      ANOP
&M       SETA  &M+1
         AIF   (&M LE &N).AI
         AGO   .AO
.AN   MNOTE *,'      ALL COMPILATION OPTION PARAMETERS MAY BE RESPECIFI*
               ED AT COMPILE TIME'
.AO      AIF   (NOT &B).AP
&SGQUITB SETB  1
.AP      ANOP
&SGPL11B(1) SETB (&SGPL11B(1) OR &F)
.AQ      ANOP
         MEXIT
.E1 MNOTE 5,'* * * IEIPL1100 PL1 MACRO PREVIOUSLY ENTERRED'
&SGQUITB SETB 1
         AGO   .AQ
.E2 MNOTE 5,'* * * IEIPL1101 DESIGN VALUE &DESIGN INVALID'
&B       SETB  1
         AGO   .A2
.E3 MNOTE 5,'* * * IEIPL1102 PUNCH VALUE &PUNCH INVALID'
&B       SETB  1
         AGO   .A3
.E4 MNOTE 5,'* * * IEIPL1103 TYPERUN VALUE &TYPERUN INVALID'
&B       SETB  1
         AGO   .A4
.E5 MNOTE 5,'* * * IEIPL1104 SORCODE VALUE &SORCODE INVALID'
&B       SETB  1
         AGO   .A5
.E6 MNOTE 5,'* * * IEIPL1105 SIZE VALUE &SIZE INVALID'
&B       SETB  1
         AGO   .A6
.E7 MNOTE 5,'* * * IEIPL1106 OBJLIST VALUE &OBJLIST INVALID'
&B       SETB  1
         AGO   .A7
.E8 MNOTE 5,'* * * IEIPL1107 MSGLEV VALUE &MSGLEV INVALID'
&B       SETB  1
         AGO   .A8
.E9 MNOTE 5,'* * * IEIPL1108 OPT VALUE &OPT INVALID'
&B       SETB  1
         AGO   .A9
.EA MNOTE 5,'* * * IEIPL1109 SORLIST VALUE &SORLIST INVALID'
&B       SETB  1
         AGO   .A91
.EA1 MNOTE 5,'* * * IEIPL119 MACLIST VALUE &MACLIST INVALID'
         AGO   .A92
.EA2 MNOTE 5,'* * * IEIPL129 OBJCODE VALUE &OBJCODE INVALID'
&B       SETB  1
         AGO   .AA
.EB MNOTE 5,'* * * IEIPL1110 CHARSET VALUE &CHARSET INVALID'
&B       SETB  1
         AGO   .AB
.EC MNOTE 5,'* * * IEIPL1111 EXTLIST VALUE &EXTLIST INVALID'
&B       SETB  1
         AGO   .AC
.ED MNOTE 5,'* * * IEIPL1112 ATRLIST VALUE &ATRLIST INVALID'
&B       SETB  1
         AGO   .AD
.EE MNOTE 5,'* * * IEIPL1113 REFLIST VALUE &REFLIST INVALID'
&B       SETB  1
         AGO   .AE
.EF MNOTE 5,'* * * IEIPL1114 SORMGIN VALUE &SORMGIN(1) OR &SORMGIN(2) IX
               NVALID'
&B       SETB  1
         AGO   .AF
.EG MNOTE 5,'* * * IEIPL1115 LINECNT VALUE &LINECNT INVALID'
&B       SETB  1
         AGO   .AG
.EH MNOTE 5,'* * * IEIPL1116 STMDIAG VALUE &STMDIAG INVALID'
&B       SETB  1
         AGO   .AG1
.EI MNOTE 5,'* * * IEIPL1117 CMPTIME VALUE &CMPTIME INVALID'
&B       SETB  1
         AGO   .AG2
.EJ MNOTE 5,'* * * IEIPL1118 COMPILE VALUE &COMPILE INVALID'
&B       SETB  1
 AGO .AG3
.EK MNOTE 5,'* * * IEIPL1119 PAGECTL VALUE &PAGECTL INVALID'
&B SETB 1
 AGO .AG4
.EL MNOTE 5,'* * * IEIPL1120 MCPUNCH VALUE &MCPUNCH INVALID'
&B SETB 1
 AGO .AG5
.EM MNOTE 5,'* * * IEIPL1121 DICTYPE VALUE &DICTYPE INVALID'
&B SETB 1
 AGO .AG6
.EN MNOTE 5,'* * * IEIPL1122 OPRINT VALUE &OPRINT INVALID'
&B SETB 1
 AGO .AG7
.EO MNOTE 5,'* * * IEIPL1123 LEVCNT VALUE &LEVCNT INVALID'
&B SETB 1
         AGO   .AH
.EP MNOTE 5,'* * * IEIPL124 SYNTXLV VALUE &SYNTXLV INVALID'
&B       SETB 1
         AGO   .AH1
         MEND
