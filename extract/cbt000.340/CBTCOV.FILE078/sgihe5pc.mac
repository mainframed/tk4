         MACRO
         SGIHE5PC
         COPY SGGBLPAK
.*       THIS MODULE  PRODUCES COPY CARDS FOR THE OPTIONAL MODULES AS
.*       REQUIRED
         PUNCH ' COPY OUTDD=PL1LIB,INDD=LM512'
         AIF   (NOT &SGPL11B(47)).NOCOMPX
         PUNCH ' SELECT MEMBER=(IHEADV0)'
         PUNCH ' SELECT MEMBER=(IHEABU0,IHEABV0,IHEABW0,IHEABZ0)'
         PUNCH ' SELECT MEMBER=(IHEATWN,IHEATWH,IHEATZN,IHEATZH)'
         PUNCH ' SELECT MEMBER=(IHEDIMA,IHEDOMA,IHEDZW0,IHEDZZ0,IHEDVU0/
               ,IHEDVV0)'
         PUNCH ' SELECT MEMBER=(IHELNW0,IHELNZ0,IHEMPU0,IHEMPV0,IHEEXW0/
               ,IHEEXZ0)'
         PUNCH ' SELECT MEMBER=(IHEMZUM,IHEMZUD,IHEMZVM,IHEMZVD)'
         PUNCH ' SELECT MEMBER=(IHEMZW0,IHEMZZ0,IHEPDW0,IHEPDX0,IHEPDZ0/
               )'
         PUNCH ' SELECT MEMBER=(IHEPSW0,IHEPSX0,IHEPSZ0,IHESMX0)'
         PUNCH ' SELECT MEMBER=(IHESNWK,IHESNWC,IHESNWS,IHESNWZ)'
         PUNCH ' SELECT MEMBER=(IHESNZC,IHESNZS,IHESNZK,IHESNZZ)'
         PUNCH ' SELECT MEMBER=(IHESQW0,IHESQZ0,IHESSX0,IHETNWH,IHETNWN/
               )'
         PUNCH ' SELECT MEMBER=(IHETNZH,IHETNZN)'
         PUNCH ' SELECT MEMBER=(IHEXIU0,IHEXIV0,IHEXIW0,IHEXIZ0)'
         PUNCH ' SELECT MEMBER=(IHEXXW0,IHEXXZ0)'
         PUNCH ' SELECT MEMBER=(IHEYGWV,IHEYGWS,IHEYGXV,IHEYGXS,IHEYGZV/
               ,IHEYGZS)'
.NOCOMPX PUNCH ' SELECT MEMBER=(IHEVCSA,IHEVCSB,IHEVCS)'
         AIF   (&SGSUPRB(13) OR &SGSUPRB(14) OR &SGSUPRB(19)).TIME
         PUNCH ' SELECT MEMBER=((IHEMSTA,IHEOSTA))'
         AGO   .NEXTA
.TIME    PUNCH ' SELECT MEMBER=(IHEOSTA) '
.NEXTA   AIF   (&SGSUPRB(14) OR &SGSUPRB(19)).INTTIM
         PUNCH ' SELECT MEMBER=((IHEMSIA,IHEOSIA))'
         AGO   .WAIT
.INTTIM  PUNCH ' SELECT MEMBER=(IHEOSIA)'
.WAIT    AIF   (&SGSUPRB(12)).MULTW
         PUNCH ' SELECT MEMBER=((IHEMSWA,IHEOSWA))'
         AGO   .END
.MULTW   PUNCH ' SELECT MEMBER=(IHEOSWA)'
.END     MEND
