         MACRO
        RESMODS  &PDS=,&MEMBERS=
         COPY  SGGBLPAK
         LCLA  &I,&J
         AIF   (&SGMENTB(37)).M1
&SGMENTB(37) SETB 1
&I       SETA  1
  AIF  (N'&PDS EQ 0).M2
  AIF  (N'&PDS GT 1 OR K'&PDS LE 5 OR K'&PDS GE 14).M3
  AIF  ('&PDS'(1,5) NE 'SYS1.').M3
&SGRESMC(&I)  SETC  '&PDS'(6,K'&PDS-5)
.L2      AIF   (N'&MEMBERS LT 1).N1
  AIF  (N'&MEMBERS GT 10).N2
&J       SETA  N'&MEMBERS
    MNOTE *,'                LISTED MEMBERS OF &PDS ARE RESIDENT'
.L4      AIF   (&I GT &J).L51
         AIF   (K'&MEMBERS(&I) LT 1 OR K'&MEMBERS(&I) GT 8).N3
&SGRESMC(&I+1) SETC '&MEMBERS(&I)'(1,K'&MEMBERS(&I))
    MNOTE *,'         &MEMBERS(&I)'
.L5      ANOP
&I       SETA  &I+1
         AGO   .L4
.L51     ANOP
   MEXIT
.M1 MNOTE 5,'* * * IEIRES101 RESMODS MACRO PREVIOUSLY USED'
&SGQUITB  SETB  1
         MEXIT
.M2 MNOTE 5,'* * * IEIRES102 PDS VALUE NOT SPECIFIED'
&SGQUITB SETB 1
   AGO .L2
.M3 MNOTE 5,'* * * IEIRES103 PDS VALUE &PDS INVALID'
&SGQUITB  SETB 1
  AGO  .L2
.N1 MNOTE 5,'* * * IEIRES104 MEMBERS VALUE NOT SPECIFIED'
&SGQUITB  SETB  1
    MEXIT
.N2 MNOTE 5,'* * * IEIRES105 MAXIMUM NUMBER OF MEMBERS EXCEEDED'
&SGQUITB  SETB  1
&J  SETA  10
   AGO  .L4
.N3 MNOTE 5,'* * * IEIRES106 MEMBERS VALUE &MEMBERS(&I) INVALID'
&SGQUITB  SETB  1
   AGO   .L5
         MEND
