         MACRO
         SGIEW401
         COPY  SGGBLPAK
.* UPDATED 29 DEC 65
         AIF   (&SGLINKB(1) OR &SGOVERB(1)).GENOVLY
         MEXIT
.GENOVLY    ANOP
&SGCTRLC(7) SETC   '&SGCTRLC(32)'  VOLUME NO. FOR LINKLIB.
&SGCTRLC(8) SETC   '&SGCTRLC(31)'
&SGCTRLC(9) SETC   'LINKLIB'
&SGCTRLC(6) SETC   'IEWSZOVR'
&SGCTRLC(10)  SETC  ',RENT'
            AIF    (&SGOVERB(2) OR &SGOVERB(3)).OVR
            AIF     (&SGCPRGB(6)).ASYNCH
         AIF   (&SGCPRGB(8)).ASYNCH
&SGQUITB    SETB   1
    MNOTE 5,'* * * IEIIEW400 ASYNCHRONOUS OVERLAY REQUIRES THE VMS CONT*
               ROL PROGRAM.'
            AGO    .RETURN
.ASYNCH     ANOP
            COPY  SGLEDPK1
            PUNCH  '//CI535 DD DISP=SHR,VOLUME=(,RETAIN),DSNAME=SYS1.CIS
               535  '
            COPY   SGLEDPK2
            PUNCH  ' INCLUDE    CI535(IEWSWOVR) '
            AGO    .PUNCH
.OVR        ANOP
            COPY   SGLEDPK1
            PUNCH  '//CI505 DD DISP=SHR,VOLUME=(,RETAIN),DSNAME=SYS1.CIS
               505  '
            COPY   SGLEDPK2
            AIF    (&SGOVERB(3)).BASWCK
            PUNCH  ' INCLUDE    CI505(IEWSYOVR) '
            AGO    .PUNCH
.ERR         ANOP
&SGQUITB SETB 1
    MNOTE 5,'* * * IEIIEW400 ADVANCED OVERLAY IS NOT SELECTABLE ON E LE*
               VEL (32K) MACHINES.'
             AGO    .RETURN
.BASWCK     ANOP
             AIF    (&SGCPUB(20)).ERR
            PUNCH  ' INCLUDE    CI505(IEWSXOVR) '
.PUNCH      ANOP
            PUNCH     ' NAME   IEWSZOVR(R)  '
            PUNCH  '/*  '
.RETURN  MEND
