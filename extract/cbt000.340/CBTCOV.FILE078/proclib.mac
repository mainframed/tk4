         MACRO
         PROCLIB &UNIT=,&VOLNO=
         COPY  SGGBLPAK
         AIF   (NOT &SGMENTB(44)).PROC1
    MNOTE 5,'* * * IEIPRL101 PROCLIB MACRO PREVIOUSLY USED'
         AGO   .PRQUIT
.PROC1   ANOP
&SGMENTB(44)   SETB  1
         AIF   (N'&UNIT EQ N'&VOLNO).PROC2
    MNOTE 5,'* * * IEIPRL102 UNIT VALUE OR VOLNO VALUE MISSING'
         AGO   .PRQUIT
.PROC2 AIF  ('&UNIT' EQ '').M1
         AIF   (K'&UNIT LE 8).PROC3
    MNOTE 5,'* * * IEIPRL103 UNIT VALUE &UNIT INVALID'
&SGQUITB  SETB  1
.PROC3   AIF   (K'&VOLNO LE 6).PROC4
    MNOTE 5,'* * * IEIPRL104 VOLNO VALUE &VOLNO INVALID'
         AGO   .PRQUIT
.PROC4   ANOP
&SGCTRLC(26)   SETC   '&VOLNO'
&SGCTRLC(25)   SETC   '&UNIT'
         MEXIT
.M1 MNOTE *,'      PROCESS LIBRARY WILL RESIDE ON SYSRES'
         MEXIT
.PRQUIT  ANOP
&SGQUITB SETB  1
         MEND
