          MACRO
         SGIKJ4EH
.********************************************************************
.***  THIS MACRO PRODUCES THE LINKEDIT CONTROL STATEMENTS
.***  FOR THE UTILITY COMMANDS DELETE, RENAME, AND PROTECT.
.********************************************************************
          COPY SGGBLPAK
&SGCTRLC(7)   SETC  '&SGCTRLC(23)'        CMDVOL
&SGCTRLC(8)  SETC  '&SGCTRLC(24)'        UNIT NAME OF CMDVOL
&SGCTRLC(9)  SETC  'CMDLIB'              LIBRARY NAME
&SGCTRLC(11)   SETC  ',REFR'
&SGCTRLC(12)   SETC  ',RENT'
          COPY SGLEDPK1
 PUNCH '//DCMDLIB DD DISP=SHR,VOLUME=(,RETAIN),DSNAME=SYS1.DCMDLIB'
          COPY SGLEDPK2
 PUNCH '  INCLUDE DCMDLIB(IKJEHREN)'
 PUNCH '  ALIAS RENAME'
 PUNCH '  ALIAS REN'
 PUNCH '  NAME IKJEHREN(R)'
 PUNCH '  INCLUDE DCMDLIB(IKJEHDEL)'
 PUNCH '  ALIAS DELETE'
 PUNCH '  ALIAS D'
 PUNCH '  NAME IKJEHDEL(R)'
 PUNCH '  INCLUDE DCMDLIB(IKJEHPRO)'
 PUNCH '  ALIAS PROTECT'
 PUNCH '  ALIAS PROT'
 PUNCH '  NAME IKJEHPRO(R)'
 PUNCH '/*'
          MEND
