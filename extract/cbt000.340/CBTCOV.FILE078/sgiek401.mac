                               XXXXXXXX'
 PUNCH '//             PARM=''OVLY,LET&SGCTRLC(1)&SGCTRLC(2)&SGCTRLC(4)*
               ,SIZE=(250K,50K)'''
 PUNCH '//SYSUT1 DD DISP=OLD,VOLUME=(,RETAIN),DSNAME=SYS1.&SGCTRLC(3)'
 PUNCH '//SYSPRINT DD SPACE=(121,(500,1000),RLSE),                     X
                       X'
 PUNCH '//             DCB=(RECFM=FB,LRECL=121,BLKSIZE=121),           X
                       X'
 PUNCH '//             SYSOUT=A&SGCTRLC(46)&SGCTRLC(47)&SGCTRLC(48)'
