*%BSLMACRO: ;
*        /************************************************************/
*        /*                                                          */
*        /*    THIS MACRO IS USED TO DEFINE THE FIELDS IN THE        */
*        /*    COMMUNICATION AREA USED BY ALL MODULES THAT MAKE      */
*        /*    UP THE EDIT COMMAND PROCESSOR                         */
*        /*                                                          */
*        /************************************************************/
*     DCL 1 IKJEBECA BASED(COMMREG) BDY(DWORD), /* COMMUNICATION
*                                                  AREA              */
*            2 CAPTTMP PTR,            /* ADDRESS OF TMP PARAMETER
*                                         LIST                       */
*            2 * FIXED(31),            /* RESERVED                   */
*            2 CAPTAE PTR,             /* ADDRESS OF IKJEBEAE        */
*            2 CAPTAT PTR,             /* ADDRESS OF IKJEBEAT        */
*            2 CAPTLE PTR,             /* ADDRESS OF IKJEBELE        */
*            2 CAPTMS PTR,             /* ADDRESS OF IKJEBEMS        */
*            2 CAPTUT PTR,             /* ADDRESS OF IKJEBEUT        */
*            2 CAPTMSGM PTR,           /* ADDRESS OF MESSAGE MODULE
*                                         PRESENTLY IN STORAGE       */
*            2 CAPTRTRY PTR,           /* ADDRESS OF STAE RETRY
*                                         ROUTINE                    */
*            2 CAPTPRSD PTR,           /* ADDRESS OF IKJPARS PDL     */
*              3 CAPRSPDL PTR(8),      /* INDICATOR BYTE             */
*                4 CAFREEDL BIT(1),    /* 1 - PDL DOES NOT EXIST
*                                         0 - PDL REQUIRES FREEMAIN  */
*            2 CAPTIBFR PTR,           /* ADDRESS OF INPUT BUFFER    */
*              3 CAOPERND BIT(1),      /* 1 - OPERANDS PRESENT
*                                         0 - NO OPERANDS            */
*            2 CAPTSCMD PTR,           /* ADDRESS OF SUBCOMMAND
*                                         LAST ENTERED               */
*            2 CASCMDLN FIXED(15),     /* LENGTH OF SUBCOMMAND NAME
*                                         LAST ENTERED               */
*            2 * FIXED(15),            /* RESERVED                   */
*            2 CAPTCDCB PTR,           /* ADDRESS OF CURRENT UTILITY
*                                         DCB                        */
*            2 CAPTPDCB PTR,           /* ADDRESS OF NEW UTILITY DCB */
*            2 CAUTILNO FIXED(31),     /* NUMBER OF RECORDS IN
*                                         UTILITY DATA SET           */
*            2 CAPTCORE PTR,           /* ADDRESS OF GETMAIN AREA    */
*            2 CACORELN FIXED(31),     /* LENGTH OF GETMAIN AREA     */
*            2 CAPTCHK PTR,            /* ADDRESS OF SYNTAX CHECKER  */
*                                      /* OR LANGUAGE PROCESSOR      */
*            2 CAPTNBFR PTR,           /* ADDRESS OF SUBCOMMAND A45155
*                                         BUFFER TO BE USED     A45155
*                                         UPON COMPLETION OF    A45155
*                                         CURRENT SUBCOMMAND    A45155*/
*            2 * CHAR(40),             /* RESERVED              A45155*/
*        /************************************************************/
*        /*                                                          */
*        /*    THIS SECTION CONSISTS OF THE CONTROL FLAGS            */
*        /*    AND A BREAK DOWN OF THE BIT SWITCHES                  */
*        /*                                                          */
*        /************************************************************/
*            2 CAATTN FIXED(31) BDY(DWORD), /* ATTENTION ECB         */
*              3 * BIT(1),             /* WAIT BIT                   */
*              3 CAATTNIS BIT(1),      /* COMPLETE BIT               */
*            2 CACFLAG CHAR(28),       /* CONTROL FLAGS              */
*              3 CACFLAG1 CHAR(1),     /* CONTROL FLAG 1             */
*                4 CALNTOVF BIT(1),    /* LINE TO BE VERIFIED,
*                                         1 - YES/ 0 - NO            */
*                4 CAVRFYSW BIT(1),    /* VERIFY SWITCH, 1-ON/0-OFF  */
*                4 CAPROMPT BIT(1),    /* PROMPT SWITCH, 1-ON/0-OFF  */
*                4 CASCANSW BIT(1),    /* SCAN SWITCH, 1-ON/0-OFF    */
*                4 CAINITSC BIT(1),    /* SPECIAL CALL OF SCAN
*                                         1-YES/0-NO                 */
*                4 CAENDSC BIT(1),     /* SCAN CALLED BY END,
*                                         1 - YES / 0 - NO           */
*                4 CACAPS BIT(1),      /* 1 - 'CAPS' / 0 - 'ASIS'    */
*                4 CANONUM BIT(1),     /* 1-'NONUM'/0-'NUM'          */
*              3 CACFLAG2 CHAR(1),     /* CONTROL FLAG 2             */
*                4 CADSMODS BIT(1),    /* DATA SET MODIFIED,
*                                         1 - YES/ 0 - NO            */
*                4 CARECFM BIT(1),     /* 0 - VARIABLE/ 1 - FIXED    */
*                4 CASCANON BIT(1),    /* 1 - 'SCAN'/ 0 - 'NO SCAN'  */
*                4 CAMODMSG BIT(1),    /* 0-MODE MSG NOT TO BE ISSUED
*                                         1-ISSUE EDIT MODE MSG      */
*                4 CASEQCOL BIT(1),    /* SEQUENCE FIELD COLUNM
*                                         NUMBERS ARE NON-STANDARD,
*                                         1-YES/0-NO                 */
*                4 * BIT(3),           /* RESERVED                   */
*              3 CACFLAG3 CHAR(1),     /* CONTROL FLAG  3            */
*                4 CAIMFLG BIT(8),     /* FLAGS USED BY INPUT        */
*                  5 CAIMPT BIT(1),    /* 1 - PROMPT/ 0 - NO PROMPT  */
*                  5 CAIMINS BIT(1),   /* 1-INPUT ENTERED FROM INSERT
*                                         0-NOT ENTERED FROM INSERT  */
*                  5 CAIMSC BIT(1),    /* INPUT ENTERED FROM
*                                         CARRIAGE RETURN, 1-YES/0-NO*/
*                  5 CAIMIR BIT(1),    /* 1 - I-FORM/ 0 - R-FORM     */
*                  5 CAIMCIN BIT(1),   /* 1-INCREMENT SPECIFIED      */
*                                      /* 0-NO INCREMENT SPECIFIED   */
*                  5 CAIMSFPT BIT(1),  /* 1-INPUT WILL PROMPT        */
*                                      /* 0-TCAM WILL PROMPT         */
*                  5 CAIMINPT BIT(1),  /* 1-INPUT HAS WRITTEN   M5144
*                                         LINES, 0 - NO         M5144*/
*                  5 * BIT(1),         /* RESERVED              M5144*/
*              3 CACFLAG4 CHAR(1),     /* CONTROL FLAG 4             */
*                4 CAFINDIS BIT(1),    /* 1-FIND ISSUED              */
*                                      /* 0-FIND NOT ISSUED          */
*                4 CAPTGTBF BIT(1),    /*1-FREE BUFFER AT EXIT FROM
*                                        SUBCOMMAND/0-DO NOT FREE    */
*                4 CATPUTVF BIT(1),    /*1-PRINT VERIFY LINE         */
*                                      /*0-DO NOT PRINT VERIFY LINE  */
*                4 CAABEND BIT(1),     /*1-ABEND IN PROCESS          */
*                                      /*0-ABEND NOT IN PROCESS      */
*                4 CASCRC20 BIT(1),    /*1-SYNTAX CHECKER RECOVERY IN
*                                        PROCESS/0-NOT IN PROCESS    */
*                4 CAINPROC BIT(1),    /*EDIT BEING EXECUTED FROM AN
*                                        IN CORE PROCEDURE,1-YES/0-NO*/
