BADRU    BAL   @14,CHAINBUF                                        0468
*   REG0=BADRSP;                    /* SETUP BAD RET CODE    @Y17XAR5*/
         LA    REG0,28                                             0469
*   OPCLDNME=MODNAME7;              /* SETUP FOR NEXT MOD    @Y17XAR5*/
         MVC   OPCLDNME(8,ROPCAVT),MODNAME7                        0470
