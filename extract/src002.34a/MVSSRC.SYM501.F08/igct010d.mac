*   BC(SEVEN,LASTUNIT);             /* IF SO, CHAIN SAVED BUFFS TO 0591
*                                      THIS UNIT             @Y17XAR5*/
         BC    7,LASTUNIT                                          0591
*   PREVADDR=REG1;                  /* SAVE PREVIOUS BUFF ADDR     0592
*                                                            @Y17XAR5*/
         ST    REG1,PREVADDR(,ROPCAVT)                             0592
*   REG1=REG1->OCUNIT;              /* POINT TO NEXT UNIT    @Y17XAR5*/
         L     REG1,OCUNIT(,REG1)                                  0593
*   GOTO FINDLAST;                  /* FIND LAST UNIT                */
         B     FINDLAST                                            0594
*LASTUNIT:                                                         0595
