         B     XCTL                                                0471
*GOODRESP:                          /*                       @Y17XAR5*/
*   REG1=OCUNIT;                    /* SET UP TO FREE RU     @Y17XAR5*/
GOODRESP L     @10,OPCCOPCE(,ROPCAVT)                              0472
         L     REG1,OCUNIT(,@10)                                   0472
*   CALL FREEBUFF;                  /* FREE RU               @Y17XAR5*/
         L     @15,OPCFREBF(,ROPCAVT)                              0473
         BALR  @14,@15                                             0473
*   OCUNIT=OCPTRSAV;                /* SETUP CHECKPOINT SAVED BUFFERS
