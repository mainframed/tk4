*                                                            @Y17XAR5*/
         L     @10,ELMNT-1(,ROPCAVT)                               0434
         LA    @10,0(,@10)                                         0434
         CLI   WANSCD(@10),7                                       0434
         BNH   @RT00434                                            0434
         CLI   WANSCD(@10),11                                      0434
         BNH   @RF00434                                            0434
@RT00434 DS    0H                                                  0435
*         PARAM->WLINK=ELMNT->WLINK;/* TAKE CIB OFF CHAIN      S05331*/
         L     @10,ELMNT-1(,ROPCAVT)                               0435
         MVC   WLINK(3,PARAM),WLINK(@10)                           0435
