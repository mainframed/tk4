*     REG14 RSTD;                   /*                       @Y17XAR5*/
GETUNIT  DS    0H                                                  0381
*   SAVERET1=REG14;                 /* SAVE RETURN REG       @Y17XAR5*/
         L     @15,OPCWORK(,ROPCAVT)                               0381
         ST    REG14,SAVERET1(,@15)                                0381
*   RFY                                                            0382
*     REG14 UNRSTD;                 /*                       @Y17XAR5*/
*   TNTNDX=WTNTNDX;                 /* SAVE TRM NAME TABLE INDEX     */
         LH    @10,WTNTNDX(,PARAM)                                 0383
         STH   @10,TNTNDX(,@15)                                    0383
*   IF TNTNDX=ZERO THEN                                            0384
         LTR   @10,@10                                             0384
         BNZ   @RF00384                                            0384
*     DO;                           /*                       @Y17XARW*/
*       RTRM=WTO-LENGTH(IEDNTRM);   /* SETUP TTE             @Y17XARW*/
         L     @10,WTO(,PARAM)                                     0386
         SL    @10,@CF01542                                        0386
         ST    @10,RTRM(,ROPCAVT)                                  0386
*       TNTNDX=OCTRMTBL;            /* SETUP TNT             @Y17XARW*/
         L     @10,OPCCOPCE(,ROPCAVT)                              0387
         LH    @10,OCTRMTBL(,@10)                                  0387
         N     @10,@CF01908                                        0387
         STH   @10,TNTNDX(,@15)                                    0387
*     END;                          /*                       @Y17XARW*/
*   ELSE                                                           0389
*     DO;                           /*                       @Y17XARW*/
         B     @RC00384                                            0389
@RF00384 DS    0H                                                  0390
