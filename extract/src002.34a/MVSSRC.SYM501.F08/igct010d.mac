         MVI   PRUTH(@09),X'0C'                                    0409
*   PRUTTCIN=TNTNDX;                /* SET UP TNT            @Y17XAR5*/
         L     @09,OPCWORK(,ROPCAVT)                               0410
         LH    @06,TNTNDX(,@09)                                    0410
         STH   @06,PRUTTCIN(,RPRU)                                 0410
*   PRUDATCT=LENGTH(IEDBDU);        /* SET UP DATA COUNT     @Y17XAR5*/
         MVC   PRUDATCT(2,RPRU),@CH00089                           0411
*   PRUTIC=INVALTIC;                /* SET UP INVALID TIC    @Y17XAR5*/
         MVC   PRUTIC(4,RPRU),@CB01756                             0412
*   BDUHEADR='1'B;                  /* TURN ON HDR FLAG      @Y17XAR5*/
         OI    BDUHEADR+12(RPRU),B'01000000'                       0413
*   OCWTG=MODT0;                    /* SET UP RETURN TO T0   @Y17XAR5*/
         MVC   OCWTG(2,@10),MODT0                                  0414
*   OCACTREQ='1'B;                  /* YES- INDICATE IN OPCE @Y17XAR5*/
         OI    OCACTREQ(@10),B'00001000'                           0415
*   OCPTRSAV=INVALTIC;              /* INDICATE NO SAVED BUFFERS YET
*                                                            @Y17XAR5*/
         MVC   OCPTRSAV(4,@10),@CF01756                            0416
*   REG14=SAVERET1;                 /* RESTORE RETURN REG    @Y17XAR5*/
         L     REG14,SAVERET1(,@09)                                0417
*   RETURN;                                                        0418
@EL00001 DS    0H                                                  0418
@EF00001 DS    0H                                                  0418
@ER00001 BR    @14                                                 0418
*ERREXIT:                           /* YES                   @Y17XAR5*/
*   OCUNIT=INVALTIC;                /* SET UP INVALID TIC    @Y17XAR5*/
ERREXIT  L     @10,OPCCOPCE(,ROPCAVT)                              0419
