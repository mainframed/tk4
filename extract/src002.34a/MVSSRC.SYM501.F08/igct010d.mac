*   PREVADDR=OPCCOPCE;              /* POINT TO OPCE         @Y17XAR5*/
         L     @15,OPCCOPCE(,ROPCAVT)                              0588
         ST    @15,PREVADDR(,ROPCAVT)                              0588
*   REG1=OCUNIT;                    /* SETUP TO FIND LAST UNIT     0589
*                                                            @Y17XAR5*/
         L     REG1,OCUNIT(,@15)                                   0589
