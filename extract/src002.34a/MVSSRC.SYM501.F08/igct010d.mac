         L     @10,OPCWORK(,ROPCAVT)                               0390
         LH    REG1,TNTNDX(,@10)                                   0390
*       CALL IEDQTNT;               /* GET TTE               @YM07744*/
         L     @15,AVTRNMPT(,AVT)                                  0391
         BALR  @14,@15                                             0391
*       RTRM=REG1-LENGTH(IEDNTRM);  /* BACKUP TERM ADDR      @Y17XAR5*/
         LR    @10,REG1                                            0392
         SL    @10,@CF01542                                        0392
         ST    @10,RTRM(,ROPCAVT)                                  0392
