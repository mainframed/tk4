         L     REG1,OPCCOPCE(,ROPCAVT)                             0446
*   CALL FREEBUFF;                  /* FREE OPCE             @Y17XAR5*/
         L     @15,OPCFREBF(,ROPCAVT)                              0447
         BALR  @14,@15                                             0447
