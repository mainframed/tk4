         L     @10,OPCCOPCE(,ROPCAVT)                              0474
         L     @09,OCPTRSAV(,@10)                                  0474
         ST    @09,OCUNIT(,@10)                                    0474
*   REG1=OCPTRSAV;                  /* ANY SAVED BUFFERS?    @Y17XARW*/
         LR    REG1,@09                                            0475
