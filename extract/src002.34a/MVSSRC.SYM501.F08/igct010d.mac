         L     @10,OPCCOPCE(,ROPCAVT)                              0393
         L     @09,OPCWORK(,ROPCAVT)                               0393
         LH    @09,TNTNDX(,@09)                                    0393
         STH   @09,OCTRMTBL(,@10)                                  0393
*     END;                          /*                       @Y17XARW*/
*   CALL CHK3705;                   /* CHECK 3705 STATUS     @Y17XAR5*/
@RC00384 BAL   @14,CHK3705                                         0395
*   IF ERRFLG='1'B THEN                                            0396
         TM    ERRFLG(ROPCAVT),B'10000000'                         0396
         BO    @RT00396                                            0396
*     GOTO ERREXIT;                 /* PROCESS ERROR         @Y17XAR5*/
*   PARAM->WLINK=SAVTRQCH;          /* LINK THIS CIB ...       S05331*/
         L     @10,AVTSAVTP(,AVT)                                  0398
         MVC   WLINK(3,PARAM),SAVTRQCH+1(@10)                      0398
*   SAVTRQCH=PARAM;                 /* ... AT TOP OF CHAIN     S05331*/
         ST    PARAM,SAVTRQCH(,@10)                                0399
*   PARAM->WRTCD=0;                 /* CLEAR THE RETURN CODE @ZA02648*/
         MVI   WRTCD(PARAM),X'00'                                  0400
*   RFY                                                            0401
*     REG15 RSTD;                   /*                       @Y17XARW*/
*   REG15=OPCGETBF;                 /* ADDRESS OF GETBUFF RTN      0402
