*BRANCH:                            /*                       @ZM46809*/
*   RWORK=ELMNT->WENTCD;            /* GET ANSWER CODE       @Y17XAR5*/
BRANCH   L     @10,ELMNT-1(,ROPCAVT)                               0440
         SLR   RWORK,RWORK                                         0440
         IC    RWORK,WENTCD(,@10)                                  0440
*   SRL(RWORK,2);                   /* SHIFT OUT FLAGS       @Y17XAR5*/
         SRL   RWORK,2                                             0441
*   SLL(RWORK,2);                   /* RESTORE CODE VALUE    @Y17XAR5*/
         SLL   RWORK,2                                             0442
*   GEN;                                                           0443
         B     BRANCHA(RWORK)      /* BRANCH TO PROPER ROUTINE       */
BRANCHA  EQU   *
         B     ATCOPY              /* TCOPY RESPONSE ROUTINE         */
         B     AICOPY              /* ICOPY RESPONSE ROUTINE         */
         B     ATCHNG1             /* TCHNG RESPONSE 1 ROUTINE       */
         B     ATCHNG2             /* TCHNG RESPONSE 2 ROUTINE       */
         B     AICHNG1             /* ICHNG RESPONSE 1 ROUTINE       */
         B     AICHNG2             /* ICHNG RESPONSE 2 ROUTINE       */
*   RFY                                                            0444
*     RWORK UNRSTD;                 /*                       @Y17XAR5*/
