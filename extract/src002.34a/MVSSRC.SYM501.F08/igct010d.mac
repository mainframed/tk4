*     THEN                                                         0424
ANSWER   CLI   WANSCD(PARAM),7                                     0424
         BNH   @RF00424                                            0424
         CLI   WANSCD(PARAM),11                                    0424
         BH    @RF00424                                            0424
*     DO;                           /* TCHNG DO NOT          @ZM46809
*                                      DEQUEUE CIB           @ZM46809*/
*       WENTCD=WANSCD;              /* SET ANSWER CODE       @ZM46809*/
         IC    @10,WANSCD(,PARAM)                                  0426
         STC   @10,WENTCD(,PARAM)                                  0426
*       GOTO BRANCH;                /* PROCESS RESPONSE      @ZM46809*/
         B     BRANCH                                              0427
*     END;                                                         0428
*   IF(WANSCD>TCHNG1A&              /* IS THIS FIRST         @Y17XAR5*/
