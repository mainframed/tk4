*       WANSCD=ICOPY×               /* IS THIS ICOPY         @Y17XAR5*/
*       WANSCD=TCOPY                /* IS THIS TCOPY         @Y17XAR5*/
*     THEN                                                         0429
@RF00424 CLI   WANSCD(PARAM),7                                     0429
         BNH   @GL00004                                            0429
         CLI   WANSCD(PARAM),11                                    0429
         BNH   @RT00429                                            0429
@GL00004 CLI   WANSCD(PARAM),12                                    0429
         BE    @RT00429                                            0429
         CLI   WANSCD(PARAM),4                                     0429
         BE    @RT00429                                            0429
         CLI   WANSCD(PARAM),0                                     0429
         BNE   @RF00429                                            0429
@RT00429 DS    0H                                                  0430
*     DO;                                                          0430
*       CALL CIBLOOP;               /* SEARCH FOR CIB        @Y17XAR5*/
         BAL   @14,CIBLOOP                                         0431
*       IF NOCIB='1'B THEN                                         0432
         TM    NOCIB(ROPCAVT),B'01000000'                          0432
         BO    @RT00432                                            0432
