*       REG1->OCELEM=ZERO;          /* DETACH OPCES          @Y17XAR5*/
         SLR   @10,@10                                             0454
         ST    @10,OCELEM(,REG1)                                   0454
*       REG1->OCATTACH='0'B;        /* INDICATE NO ATTACHED OPCES  0455
*                                                            @Y17XAR5*/
         NI    OCATTACH(REG1),B'11111101'                          0455
*     END;                          /*                       @Y17XAR5*/
*   CALL FREEBUFF;                  /* FREE START/STOP OPCE  @Y17XAR5*/
@RF00451 L     @15,OPCFREBF(,ROPCAVT)                              0457
         BALR  @14,@15                                             0457
*   CALL CIBLOOP;                   /* SEARCH FOR CIB        @Y17XAR5*/
         BAL   @14,CIBLOOP                                         0458
*   IF NOCIB='1'B THEN                                             0459
         TM    NOCIB(ROPCAVT),B'01000000'                          0459
