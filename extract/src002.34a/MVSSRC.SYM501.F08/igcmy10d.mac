*                                                                  0298
*PARSE:                                                            0298
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0298
PARSE    DS    0H                                                  0299
*   DCL                                                            0299
*     RLNMAXL FIXED(8) CONSTANT(3); /* THE MAX. LENGTH OF RLN FIELD
*                                      ALLOWED                       */
*   DCL                                                            0300
*     PAR14SAV PTR(31);             /* SAVE AREA FOR REG 14          */
*   DCL                                                            0301
*     IOBFLG BIT(1);                /* FLAG INDICATING IOB           */
*   DCL                                                            0302
*     BRIND FIXED(8) GEND;          /* BRANCH INDICATOR              */
*   GEN DATA DEFS(BRIND);                                          0303
*   RFY                                                            0304
*     RRET RSTD;                                                   0304
*   PAR14SAV=RRET;                  /* SAVE REG 14                   */
         ST    RRET,PAR14SAV                                       0305
*   RFY                                                            0306
*     RRET UNRSTD;                                                 0306
*   IOBFLG=OFF;                     /* INIT.                 @YM04608*/
         NI    IOBFLG,B'01111111'                                  0307
*                                                                  0308
*   /*****************************************************************/
*   /*                                                               */
*   /* ON ENTRANCE TO MY, THE RESOURCE NAME HAS BEEN PLACED IN       */
*   /* OPCDOUBL AND THE WORK AREA SCAN PTR. POINTS BEYOND THE FIELD  */
*   /* CONTAINING THE RESOURCE (IN THE CASE OF 'GROUPNAME,RLN', THE  */
*   /* POINTER POINTS TO THE BEGINNING OF THE RLN FIELD)             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0308
*   IF ADDR(OPCDOUBL)->CHARNUM>=NUMLIMIT THEN/* 1ST CHAR. A NUM?     */
         CLI   CHARNUM(ROPCAVT),240                                0308
         BL    @RF00308                                            0308
*     DO;                                                          0309
*                                                                  0309
*       /*************************************************************/
*       /*                                                           */
*       /* CHANNEL ADDRESS SPECIFIED                                 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0310
*       RPARM=ZERO;                                                0310
         SLR   RPARM,RPARM                                         0310
*       RBRNCH=OPCDCBLK;            /* ADDR. OF DCB LOOKUP ROUTINE   */
         L     RBRNCH,OPCDCBLK(,ROPCAVT)                           0311
*       GEN REFS(RBRNCH) SETS(RRET,RPARM,RZERO,BRIND);             0312
         BALR  RRET,RBRNCH             BRANCH TO DCB LOOK UP ROUTINE
         B     GEN00100                RETURN IS HERE WHEN DCB FOUND
         MVI   BRIND,BADBR             RETURN IS TO HERE WHEN DCB
*                                        NOT FOUND - BRANCH INDICATOR
         B     GEN00200                  SET TO ERROR
GEN00100 MVI   BRIND,GOODBR            SET BRANCH INDICATOR
GEN00200 EQU   *
*       IF BRIND=GOODBNCH THEN                                     0313
         CLI   BRIND,1                                             0313
         BNE   @RF00313                                            0313
*         DO;                       /* IF DCB ADDR. FOUND THEN       */
*           IOBFLG=ON;              /* TURN ON IOB FLAG              */
         OI    IOBFLG,B'10000000'                                  0315
*           DCBPTR=RPARM;           /* SAVE DCB ADDR.                */
         ST    RPARM,DCBPTR                                        0316
*         END;                                                     0317
*       ELSE                                                       0318
*         DO;                                                      0318
         B     @RC00313                                            0318
@RF00313 DS    0H                                                  0319
*           ERRFLG=ON;              /* TURN ON ERROR FLAG            */
         OI    ERRFLG,B'10000000'                                  0319
*           RZERO=SYNMSG;           /* SYNTAX ERROR MSG. NO.         */
         LA    RZERO,18                                            0320
*         END;                                                     0321
*     END;                                                         0322
*   ELSE                                                           0323
*     DO;                                                          0323
*                                                                  0323
         B     @RC00308                                            0323
@RF00308 DS    0H                                                  0324
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK IF OPCDOUBL CONTAINS NAME OF ENTRY IN TNT           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0324
*       RBRNCH=OPCTOFLK;            /* ADDR. OF TNT LOOKUP           */
         L     RBRNCH,OPCTOFLK(,ROPCAVT)                           0324
*       GEN REFS(RBRNCH) SETS(RRET,RPARM,RZERO,BRIND);             0325
         BALR  RRET,RBRNCH             BRANCH TO TNT LOOK UP
         B     GEN00300                RETURN IF NAME FOUND
         MVI   BRIND,BADBR             RETURN IF NAME NOT FOUND
         B     GEN00400                BRANCH AROUND
GEN00300 MVI   BRIND,GOODBR            SET BRANCH INDICATOR AS GOOD
GEN00400 EQU   *
*       IF BRIND=GOODBNCH THEN      /* IF RESOURCE IS SYMBOLIC NAME
*                                      IN TNT                        */
         CLI   BRIND,1                                             0326
         BNE   @RF00326                                            0326
*         DO;                                                      0327
*                                                                  0327
*           /*********************************************************/
*           /*                                                       */
*           /* THE CONTENTS OF OPCDOUBL WAS FOUND IN THE TNT AND THE */
*           /* TTE ADDR IS IN REG 1 (RPARM). CHECK IF TTE IS A VALID */
*           /* ENTRY TYPE. IF NOT SET ERROR INDICATOR AND PLACE MSG #*/
*           /* IN REG 0                                              */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0328
*           RTRM=RPARM;             /* ESTABLISH ADDRESSABILITY TO 0328
*                                      TTE                           */
         LR    RTRM,RPARM                                          0328
*           IF TRMPREF=OFF THEN     /* IF ENTRY IS FOR A 270X        */
         TM    TRMPREF(RTRM),B'00010000'                           0329
         BNZ   @RF00329                                            0329
*             DO;                                                  0330
*               ERRFLG=ON;          /* ERROR INDICATOR SET ON        */
         OI    ERRFLG,B'10000000'                                  0331
*               RZERO=TYPEMSG;      /* 'WRONG TTE TYPE' MSG. NO.     */
         LA    RZERO,173                                           0332
*             END;                                                 0333
*           ELSE                                                   0334
*             IF TRMPROC=ON THEN    /* IF PROCESS ENTRY              */
         B     @RC00329                                            0334
@RF00329 TM    TRMPROC(RTRM),B'00100000'                           0334
         BNO   @RF00334                                            0334
*               DO;                                                0335
*                 ERRFLG=ON;                                       0336
         OI    ERRFLG,B'10000000'                                  0336
*                 RZERO=TYPEMSG;                                   0337
         LA    RZERO,173                                           0337
*               END;                                               0338
*             ELSE                                                 0339
*               IF TRMTYPE=TRMGRP THEN/* IF GROUP ENTRY              */
*                                                                  0339
         B     @RC00334                                            0339
@RF00334 LR    @14,RTRM                                            0339
         AL    @14,@CF01567                                        0339
         CLI   TRMTYPE-11(@14),X'00'                               0339
         BNE   @RF00339                                            0339
*                 /***************************************************/
*                 /*                                                 */
*                 /* GROUP ENTRY SPECIFIED. THE 'GROUP' SUBROUTINE   */
*                 /* CHECKS TO INSURE THAT A VALID RLN IS SPECIFIED  */
*                 /* AND IF SO OBTAINS THE LINE TTE ADDR. WHICH IT   */
*                 /* PLACES IN RTRM. IF THE RLN IS SPECIFIED         */
*                 /* INCORRECTLY ERRFLG IS SET AND RZERO IS SET WITH */
*                 /* THE MESSAGE NUMBER FOR A SYNTAX ERROR.          */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0340
*                 IF WABYTES<=ZERO THEN/* IF AT END OF COMMAND       */
         LH    @14,WABYTES                                         0340
         LTR   @14,@14                                             0340
         BP    @RF00340                                            0340
*                   DO;                                            0341
*                     ERRFLG=ON;                                   0342
         OI    ERRFLG,B'10000000'                                  0342
*                     RZERO=SYNMSG;                                0343
         LA    RZERO,18                                            0343
*                   END;                                           0344
*                 ELSE                                             0345
*                   CALL GROUP;     /* GET RLN                       */
         B     @RC00340                                            0345
@RF00340 BAL   @14,GROUP                                           0345
*               ELSE                                               0346
*                 ;                                                0346
@RF00339 DS    0H                                                  0347
*         END;                                                     0347
*       ELSE                                                       0348
*                                                                  0348
*         /***********************************************************/
*         /*                                                         */
*         /* RESOURCE IN OPCDOUBL MAY BE DDNAME OR CHAN. ADDR. >9.   */
