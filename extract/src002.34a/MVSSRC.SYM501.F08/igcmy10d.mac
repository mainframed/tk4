*   /*                                                               */
*   /* WAPTR POINTS PAST THE FIELD CONTAINING THE DDNAME OR CHANNEL  */
*   /* ADDR. BACK UP TO EQUAL SIGN IN ORDER TO POINT TO THIS FIELD.  */
*   /* THE ROUTINE IN IEDQCA USES THIS FIELD TO RETURN A DCB ADDR.   */
*   /* REGS 5 AND 11 ARE PARAMETERS.                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0505
*   REG11=WAPTR;                    /* COPY POINTER TO WORKAREA      */
         L     REG11,WAPTR                                         0505
*   DO WHILE(REG11->WORKAREA(1:1)^=EQUAL);/* SEARCH FOR EQUAL SIGN   */
         B     @DE00506                                            0506
@DL00506 DS    0H                                                  0507
*     REG11=REG11-1;                /* BACKUP POINTER                */
         BCTR  REG11,0                                             0507
*   END;                                                           0508
@DE00506 CLI   WORKAREA(REG11),C'='                                0508
         BNE   @DL00506                                            0508
*   REG11=REG11+1;                  /* POINT PAST EQUAL SIGN         */
*                                                                  0509
         AL    REG11,@CF00061                                      0509
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP TO CALL OPCSCAN (OPCDCBLK+4)                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0510
*   REG5=OPCWORK;                   /* POINT TO START OF OP.CTL. W.A.*/
         L     REG5,OPCWORK(,ROPCAVT)                              0510
*   OCSCAN=OCDDADDR;                /* INDICATES TO OPCSCAN THAT   0511
*                                      RESOURCE IS DDNAME OR ADDR.   */
         MVI   OCSCAN(ROPCE),X'A0'                                 0511
*   RBRNCH=OPCDCBLK;                /* SET R15 TO DCB LOOKUP ADDR. 0512
*                                      FOR BASE REG IN IEDQCA        */
         L     RBRNCH,OPCDCBLK(,ROPCAVT)                           0512
*   ROPCSCAN=RBRNCH+OPCSCOFF;       /* BRACH REG TO OPCSCAN ADDR.    */
         LA    ROPCSCAN,4                                          0513
         ALR   ROPCSCAN,RBRNCH                                     0513
*   GEN REFS(RBRNCH,ROPCSCAN) SETS(RRET,RPARM,RZERO,BRIND);        0514
         BALR  RRET,ROPCSCAN           LINK TO OPCSCAN ROUTINE
         B     GEN00700                RETURN IS HERE WHEN DCB FOUND
         B     GEN00800                RETURN IS HERE WHEN NOT FOUND
GEN00800 MVI   BRIND,BADBR             RETURN IS HERE WHEN NOT FOUND
*                                      SET BRANCH INDICATOR TO ERROR
         B     GEN00900
GEN00700 MVI   BRIND,GOODBR            SET BRANCH INDIC. TO NO ERROR
GEN00900 EQU   *
*   IF BRIND=GOODBNCH THEN                                         0515
         CLI   BRIND,1                                             0515
         BNE   @RF00515                                            0515
*     DO;                           /* IF GOOD RTN FROM DCB LKUP     */
*       IOBFLG=ON;                  /* TURN ON IOB FLAG              */
         OI    IOBFLG,B'10000000'                                  0517
*       DCBPTR=RPARM;               /* SAVE POINTER TO DCB           */
         ST    RPARM,DCBPTR                                        0518
*     END;                                                         0519
*   ELSE                                                           0520
*     DO;                                                          0520
         B     @RC00515                                            0520
@RF00515 DS    0H                                                  0521
*       ERRFLG=ON;                  /* ERROR IF DCB NOT FOUND        */
         OI    ERRFLG,B'10000000'                                  0521
*       RZERO=SYNMSG;               /* SYNTAX ERROR MSG. NO.         */
         LA    RZERO,18                                            0522
*     END;                                                         0523
*   RFY                                                            0524
*     REG5 UNRSTD;                                                 0524
@RC00515 DS    0H                                                  0525
*   RFY                                                            0525
*     REG11 UNRSTD;                                                0525
*   RFY                                                            0526
*     RRET RSTD;                                                   0526
*   RRET=DDAD14SA;                  /* RESTORE RETURN ADDR.          */
         L     RRET,DDAD14SA                                       0527
*   RFY                                                            0528
*     RRET UNRSTD;                                                 0528
*   END DDNADDR;                                                   0529
@EL00006 DS    0H                                                  0529
@EF00006 DS    0H                                                  0529
@ER00006 BR    @14                                                 0529
@PB00006 DS    0H                                                  0529
*   RFY                                                            0530
*     RRET RSTD;                                                   0530
*   RRET=PAR14SAV;                                                 0531
         L     RRET,PAR14SAV                                       0531
*   RFY                                                            0532
*     RRET UNRSTD;                                                 0532
*   END PARSE;                                                     0533
@EL00002 DS    0H                                                  0533
@EF00002 DS    0H                                                  0533
@ER00002 BR    @14                                                 0533
