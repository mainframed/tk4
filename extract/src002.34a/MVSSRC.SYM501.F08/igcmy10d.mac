         L     @14,OPCWORK(,ROPCAVT)                               0251
         LA    CHARPTR,8                                           0251
         ALR   CHARPTR,@14                                         0251
         LH    @14,CMNDLEN(,@14)                                   0251
         N     @14,@CF01884                                        0251
         ALR   CHARPTR,@14                                         0251
*       CHARBYTE=BLANK;             /* BLANK IT OUT          @YM04610*/
         MVI   CHARBYTE(CHARPTR),X'40'                             0252
*     END;                          /*                       @OZ29999*/
*   FLAGS=''B;                      /* INIT. FLAGS                   */
@RF00249 MVI   FLAGS,X'00'                                         0254
*   IF OPCRSTRT=ON THEN             /* IF RESTART                    */
         TM    OPCRSTRT(ROPCAVT),B'00100000'                       0255
         BNO   @RF00255                                            0255
*     DO;                                                          0256
*                                                                  0256
*       /*************************************************************/
*       /*                                                           */
*       /* SET REMEMBER BIT IN TTE                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0257
*       RPARM=OCTRMTBL;             /* SET PARM REG WITH TTCIN       */
         LH    RPARM,OCTRMTBL(,ROPCE)                              0257
         N     RPARM,@CF01884                                      0257
*       RBRNCH=AVTRNMPT;            /* SET BRANCH REG TO CONVERSION
*                                      ROUTINE                       */
         L     RBRNCH,AVTRNMPT(,RAVT)                              0258
*       CALL SUBRTNE;               /* CALL CONVERSION ROUTINE. NOTE:
*                                      SUBRTNE BASED ON RBRNCH       */
         BALR  @14,@15                                             0259
*       RTRM=RPARM;                 /* COPY TTE ADDR.                */
         LR    RTRM,RPARM                                          0260
*       IF OCSWITCH=OCTOFA THEN     /* IF THIS IS AN ACTIVATE COMMAND*/
         CLI   OCSWITCH(ROPCE),X'22'                               0261
         BNE   @RF00261                                            0261
*         TRMREMB=ON;               /* THEN TURN ON REMEMBER BIT     */
         LR    @07,RTRM                                            0262
         AL    @07,@CF01889                                        0262
         LA    @09,TRMREMB-2(,@07)                                 0262
         LR    @05,@09                                             0262
         LA    @14,3                                               0262
         NR    @05,@14                                             0262
         LCR   @05,@05                                             0262
         ALR   @09,@05                                             0262
         L     @14,0(,@09)                                         0262
@GL00001 LR    @11,@14                                             0262
         O     @11,@CB01885+3(@05)                                 0262
         CS    @14,@11,0(@09)                                      0262
         BNE   @GL00001                                            0262
*       ELSE                                                       0263
*         TRMREMB=OFF;              /* ELSE TURN OFF REMEMBER BIT    */
         B     @RC00261                                            0263
@RF00261 LR    @07,RTRM                                            0263
         AL    @07,@CF01889                                        0263
         LA    @09,TRMREMB-2(,@07)                                 0263
         LR    @05,@09                                             0263
         LA    @14,3                                               0263
         NR    @05,@14                                             0263
         LCR   @05,@05                                             0263
         ALR   @09,@05                                             0263
         L     @14,0(,@09)                                         0263
@GL00002 LR    @11,@14                                             0263
         N     @11,@CB01890+3(@05)                                 0263
         CS    @14,@11,0(@09)                                      0263
         BNE   @GL00002                                            0263
*       OPCMODID=CKPTMOD;           /* SET UP TO XCTL TO TAKE AN   0264
*                                      INCIDENT RECORD               */
@RC00261 MVC   OPCMODID(2,ROPCAVT),@CC01798                        0264
*     END;                                                         0265
*   ELSE                                                           0266
*     DO;                                                          0266
*                                                                  0266
         B     @RC00255                                            0266
@RF00255 DS    0H                                                  0267
*       /*************************************************************/
*       /*                                                           */
*       /* NOTE: REGS 6 AND 7 AS SAVED IN OPCSAVE ARE NOT SET        */
*       /* CORRECTLY ON ENTRANCE TO MY IF CMND. CAME FROM TOTE. SINCE*/
*       /* TOTE DOESN'T SET OPTIONS, THE BYTES LEFT TO SCAN IS SET TO*/
*       /* ZERO.                                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0267
*       IF OCTOTE=ON THEN           /* IF TOTE ISSUED CMND.          */
         TM    OCTOTE(ROPCE),B'01000000'                           0267
         BNO   @RF00267                                            0267
*         WABYTES=ZERO;             /* BYTES LEFT TO SCAN = 0        */
         SLR   @14,@14                                             0268
         STH   @14,WABYTES                                         0268
*       ELSE                                                       0269
*         ;                                                        0269
@RF00267 DS    0H                                                  0270
*       CALL PARSE;                 /* PARSE REMAINDER OF STRING     */
*                                                                  0270
         BAL   @14,PARSE                                           0270
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK ERROR FLAG TO CHECK IF PARSE DONE SUCCESSFULLY      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0271
*       IF ERRFLG=ON THEN                                          0271
         TM    ERRFLG,B'10000000'                                  0271
         BNO   @RF00271                                            0271
*         DO;                                                      0272
*                                                                  0272
*           /*********************************************************/
*           /*                                                       */
