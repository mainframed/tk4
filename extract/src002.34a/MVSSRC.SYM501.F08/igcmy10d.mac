*           /*                                                       */
*           /*********************************************************/
*                                                                  0273
*           ERRFLG=OFF;             /* RESET FOR NEXT PASS THRU    0273
*                                      MODULE                        */
         NI    ERRFLG,B'01111111'                                  0273
*           OCPARM=RZERO;           /* SET COMPLETION CODE FROM MSG #*/
         STH   RZERO,OCPARM(,ROPCE)                                0274
*           IF OCTOTE=ON THEN       /* IF TOTE CMND.                 */
         TM    OCTOTE(ROPCE),B'01000000'                           0275
         BNO   @RF00275                                            0275
*             DO;                                                  0276
*               OPCMODID=OUTWRMOD;  /* SET TO EXIT TO OUTPUT WRITER
*                                      TO POST APPL. OR TOTE         */
         MVC   OPCMODID(2,ROPCAVT),@CC01788                        0277
*               RZERO=TOTEBAD;                                     0278
         LA    RZERO,4                                             0278
*             END;                                                 0279
*           ELSE                                                   0280
*             IF MSGMODSW=ON THEN   /* MSG MOD SW. INDICATES WHICH 0280
*                                      WHICH MSG MOD TO XCTL TO      */
         B     @RC00275                                            0280
@RF00275 TM    MSGMODSW,B'01000000'                                0280
         BNO   @RF00280                                            0280
*               DO;                                                0281
*                 MSGMODSW=OFF;     /* RESET FOR NEXT PASS THRU MOD  */
         NI    MSGMODSW,B'10111111'                                0282
*                 OPCMODID=MSGMOD0H;/* SET TO XCTL TO MSG MOD '0H'   */
         MVC   OPCMODID(2,ROPCAVT),@CC01792                        0283
*               END;                                               0284
*             ELSE                                                 0285
*               OPCMODID=MSGMOD03;  /* SET TO XCTL TO MSG MOD '03'   */
         B     @RC00280                                            0285
@RF00280 MVC   OPCMODID(2,ROPCAVT),@CC01790                        0285
*         END;                                                     0286
*       ELSE                                                       0287
*         DO;                       /*                       @YM09074*/
*                                                                  0287
         B     @RC00271                                            0287
@RF00271 DS    0H                                                  0288
*           /*********************************************************/
*           /*                                                       */
*           /* PARSE WAS SUCCESSFUL. DETERMINE MODULE TO XCTL TO.    */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0288
*           OCALSTAT=''B;           /* CLEAR FLAGS           @YM09074*/
         MVI   OCALSTAT(ROPCE),X'00'                               0288
*           OCWTGSAV=ZERO;          /* CLEAR FIELD FOR F PROCESSORS
*                                                            @OY20474*/
         SLR   @14,@14                                             0289
         STH   @14,OCWTGSAV(,ROPCE)                                0289
*           IF OCSWITCH=OCTOFA THEN /* IF ACTIVATE CMND.             */
         CLI   OCSWITCH(ROPCE),X'22'                               0290
         BNE   @RF00290                                            0290
*             OPCMODID=ACTMOD;      /* THEN SET UP TO BRANCH TO    0291
*                                      ACTIVATE PROCESSOR            */
         MVC   OPCMODID(2,ROPCAVT),@CC01794                        0291
*           ELSE                                                   0292
*             OPCMODID=DACTMOD;     /* ELSE SET UP TO BRANCH TO    0292
*                                      DEACTIVATE PROCESSOR          */
         B     @RC00290                                            0292
@RF00290 MVC   OPCMODID(2,ROPCAVT),@CC01796                        0292
*         END;                      /*                       @YM09074*/
@RC00290 DS    0H                                                  0294
*     END;                                                         0294
@RC00271 DS    0H                                                  0295
*   RPARM=ROPCAVT;                  /* SET PARM. FOR XCTL            */
@RC00255 LR    RPARM,ROPCAVT                                       0295
*   OCRSID=ZERO;                    /* ENSURE STATUS FIELD CLEAR   0296
*                                                            @XM05698*/
         SLR   @14,@14                                             0296
         STH   @14,OCRSID(,ROPCE)                                  0296
*   GEN EXIT REFS(ROPCAVT,OPCXCTL);                                0297
         XCTL  SF=(E,OPCXCTL(ROPCAVT))
*                                                                  0298
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE FINISHES HE PARSING OF THE COMMAND               */
