*         /* PLACED IN DCBADDR AND IOBFLG IS TURNED ON. IF NOT ERRFLG*/
*         /* IS TURNED ON AND THE SYNTAX MESSAGE NO. IS PLACED IN    */
*         /* RZERO.                                                  */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0348
*         CALL DDNADDR;             /* CHECK IF RESOURCE IS DDNAME OR
*                                      CHAN. ADR.>9                  */
         B     @RC00326                                            0348
@RF00326 BAL   @14,DDNADDR                                         0348
*     END;                                                         0349
@RC00326 DS    0H                                                  0350
*   IF ERRFLG=OFF THEN              /* IF ERROR FLAG NOT SET         */
*                                                                  0350
@RC00308 TM    ERRFLG,B'10000000'                                  0350
         BNZ   @RF00350                                            0350
*     /***************************************************************/
*     /*                                                             */
*     /* THE SUBROUTINE OPTSET CHECKS FOR THE PRESENCE OF IDLE,QUICK,*/
*     /* FLUSH, OR FORCE IN THE COMMAND.                             */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0351
*     CALL OPTSET;                  /* SET OPTIONS                   */
         BAL   @14,OPTSET                                          0351
*                                                                  0352
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE IS EXECUTED WHEN THE NAME OF A GROUP ENTRY IS    */
*   /* SPECIFIED. THE WORKAREA IS SCANNED FOR THE RLN AND THIS IS    */
*   /* USED TO FIND THE TTCIN FOR THE LINE                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0352
*GROUP:                                                            0352
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0352
         B     @PB00003                                            0352
GROUP    DS    0H                                                  0353
*   DCL                                                            0353
*     DECRLN CHAR(3);               /* HOLDS EBCDIC DECIMAL        0353
*                                      REPRESENTATION OF RLN         */
*   DCL                                                            0354
*     LASTBYTE PTR(31);             /* PTR. TO LAST BYTE IN CMND     */
*   DCL                                                            0355
*     PACKRLN CHAR(2);              /* HOLDS PACKED DEC. FORM OF RLN */
*   DCL                                                            0356
*     RLN FIXED(15);                /* HOLDS BINARY FORM OF RLN      */
*   DCL                                                            0357
*     COMMA CHAR(1) CONSTANT(',');  /* COMMA                         */
*   DCL                                                            0358
*     NUMERIC BIT(1);               /* INDICATES FIELD ALL NUMERICS  */
*   DCL                                                            0359
*     GRP14SAV PTR(31);             /* SAVE AREA FOR REG 14          */
*   RFY                                                            0360
*     RRET RSTD;                                                   0360
*   GRP14SAV=RRET;                  /* SAVE RETURN ADDR.             */
         ST    RRET,GRP14SAV                                       0361
*   RFY                                                            0362
*     RRET UNRSTD;                                                 0362
*                                                                  0363
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN WORKAREA FOR NEXT DELIMITER (COMMA) OR END OF CMND.      */
*   /* RESULTANT FIELD SHOULD DEFINE THE RLN. WHEN SCANNING,INSURE   */
*   /* EACH CHARACTER IN FIELD ARE EBCIDIC NUMBERS.                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0363
*   NUMERIC=YES;                    /* INITIALIZE                    */
         OI    NUMERIC,B'10000000'                                 0363
*   LASTBYTE=WAPTR+WABYTES-1;       /* SET UPPER LIMIT FOR LOOP AS 0364
*                                      LAST BYTE IN CMND. STRING     */
         L     @14,WAPTR                                           0364
         LH    LASTBYTE,WABYTES                                    0364
         ALR   LASTBYTE,@14                                        0364
         BCTR  LASTBYTE,0                                          0364
*   DO CHARPTR=WAPTR TO LASTBYTE WHILE(CHARBYTE^=COMMA);           0365
         LR    CHARPTR,@14                                         0365
         B     @DE00365                                            0365
@DL00365 CLI   CHARBYTE(CHARPTR),C','                              0365
         BE    @DC00365                                            0365
*     IF CHARNUM<NUMLIMIT THEN                                     0366
         CLI   CHARNUM(CHARPTR),240                                0366
         BNL   @RF00366                                            0366
*       NUMERIC=NO;                 /* CHARACTER IS NOT A NUMBER     */
