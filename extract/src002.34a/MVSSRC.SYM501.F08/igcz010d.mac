                        '
IGCZ010D CSECT ,                                                01S0001
@PROLOG  DS    0H                                               01S0002
         USING *,RBASE                  ESTABLISH A BASE REGISTER
         LR    RBASE,REG15              SETUP BASE REG
IGCZ010D IEDHJN AFTERHJN,HJN            MODULE IDENTIFICATION MACRO
*   ROPCAVT=RPARM;                  /* SET BASE POINTER FOR OP  03S0369
*                                      CONTROL AVT                   */
         LR    ROPCAVT,RPARM                                    03S0369
*   RAVT=OPCAVTPT;                  /* SET THE BASE POINTER FOR THE
*                                      TCAM ADDRESS VECTOR TABLE     */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          03S0370
*   ROPCE=OPCCOPCE;                 /* SET THE BASE POINTER FOR THE
*                                      OPERATOR CONTROL ELEMENT      */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         03S0371
*   RSAVE=ADDR(OPCSAVE);            /* SETUP A SAVEAREA              */
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          03S0372
*   OPCFLAG1=ZERO;                  /* CLEAR CONTROL SWITCHES        */
         MVI   OPCFLAG1(ROPCAVT),X'00'                          03S0373
*   IF OPCFLAG2=ENTRY2 THEN                                     02S0374
         CLC   OPCFLAG2(1,ROPCAVT),ENTRY2                       02S0374
         BNE   @RF00374                                         02S0374
*     DO;                           /* IF THIS IS A RE-ENTRY FROM
*                                      IGCZ110D, PROCESS SYSTEM 02S0375
*                                      CLOSEDOWN                     */
*                                                               03S0376
*       /*************************************************************/
*       /*                                                           */
*       /* PROCESS.REENTRY: THIS SEGMENT CONTINUES PROCESSING SYSTEM */
*       /* CLOSEDOWN AFTER RETURN FROM IGCZ110D. IF A SAT TABLE      */
*       /* EXISTS, 3705 DEACTIVATION IS NECESSARY.THE SAT TABLE IS   */
*       /* USED TO FIND THE TTCIN OF EACH NCP IN THE SYSTEM. A DEACT */
*       /* OPCE IS BUILT FOR EACH NCP FOUND AND THE TTCIN OF EACH NCP*/
*       /* IS STORED IN THE DEACT OPCE. THESE DEACT OPCES ARE POSTED */
*       /* TO THE OP CONTROL QCB AND CONTROL IS PASSED TO IGC0010D.  */
*       /* IF 3705 DEACTIVATION NOT NECESSARY, CONTROL IS PASSED TO  */
*       /* THE CHECKPOINT REOUTINE, IGC0610D, TO PERFORM AN          */
*       /* ENVIRONMENT CHECKPOINT                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                               03S0376
*       RFY                                                     03S0376
*        (RSAT,                                                 03S0376
*         RCTR) RSTD;                                           03S0376
*       OPCFLAG2=ZERO;              /* RESET ENTRY FLAG              */
         MVI   OPCFLAG2(ROPCAVT),X'00'                          03S0377
*       OCPARM=ZERO;                /* SETUP MESSAGE NUMBER  @YM08121*/
         SLR   @09,@09                                          03S0378
         STH   @09,OCPARM(,ROPCE)                               03S0378
*       OCWTG=MOD00;                /* NEXT MOD TO GET CONTROL AFTER
*                                      CHKPOINT              @YM08121*/
         MVC   OCWTG(2,ROPCE),MOD00                             03S0379
*       OPCMODID=MOD06;             /* SETUP ID OF CKPT MOD  @YM08121*/
         MVC   OPCMODID(2,ROPCAVT),MOD06                        03S0380
*       IF AVTVTMCP=ON THEN                                     03S0381
         TM    AVTVTMCP(RAVT),B'10000000'                       03S0381
         BNO   @RF00381                                         03S0381
*         DO;                       /* IF 3705 ENVIRONMENT BUILD
*                                      DEACT OPCES FOR ALL NCPS 03S0382
*                                                            @OY16432*/
*           RSAT=SAVTSAT;           /* POINT TO START OF SAT TABLE   */
         L     @07,AVTSAVTP(,RAVT)                              03S0383
         L     RSAT,SAVTSAT(,@07)                               03S0383
*           RCTR=SAVTSUBA;          /* SETUP NUMBER OF SAT ENTRIES   */
         SLR   RCTR,RCTR                                        03S0384
         IC    RCTR,SAVTSUBA(,@07)                              03S0384
*           OPCECTR=0;              /* CLEAR OPCE COUNTER            */
         STH   @09,OPCECTR(,ROPCE)                              03S0385
*           DO UNTIL(RCTR=0);       /* SEARCH THROUGH ALL OF SAT
*                                      TABLE FOR RVT ENTRIES         */
@DL00386 DS    0H                                               03S0387
*                                                               04S0387
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS.REENTRY.SCANRVT: THIS SEGMENT GETS NCP TNT  */
*             /* FROM RVT, STORES IT IN ORIGINAL OPCE, GETS A BUFFER */
*             /* AND BUILDS A DEACT OPCE WHICH LATER GETS POSTED TO  */
*             /* THE OP CONTROL QCB                                  */
*             /*                                                     */
*             /*******************************************************/
*                                                               04S0387
*             IF SATRNRVT^=ZERO THEN                            04S0387
         L     @09,SATRNRVT(,RSAT)                              04S0387
         LTR   @09,@09                                          04S0387
         BZ    @RF00387                                         04S0387
*               DO;                 /* IF RVT POINTER EXISTS         */
*                 IF RVTINDEX^=SAVTTCPU THEN                    04S0389
         LH    @09,RVTINDEX(,@09)                               04S0389
         L     @07,AVTSAVTP(,RAVT)                              04S0389
         LH    @07,SAVTTCPU(,@07)                               04S0389
         N     @07,@CF02669                                     04S0389
         CR    @09,@07                                          04S0389
         BE    @RF00389                                         04S0389
*                   DO;             /* AND THIS IS NOT A HOST SUBAREA*/
*                     OCTRMTBL=RVTINDEX;/* SAVE NCP TNT              */
         STH   @09,OCTRMTBL(,ROPCE)                             04S0391
*                     RPARM=OCTRMTBL;/* NCP TNT OFFSET       @YM08098*/
         LR    RPARM,@09                                        04S0392
*                     CALL IEDQTNT; /* GET TTE ADDRESS       @YM08098*/
         L     @15,AVTRNMPT(,RAVT)                              04S0393
         BALR  @14,@15                                          04S0393
*                     RTRM=RPARM-LENGTH(IEDNTRM);/* TRM         04S0394
*                                      ADDRESSABILITY        @YM08098*/
         LR    RTRM,RPARM                                       04S0394
         SL    RTRM,@CF02354                                    04S0394
*                     IF TRMNCPCL=OFF THEN/* CLOSEDOWN NOT ALREADY IN
*                                      PROGRESS              @YM08098*/
         TM    TRMNCPCL(RTRM),B'01000000'                       04S0395
         BNZ   @RF00395                                         04S0395
*                       DO;         /* YES                   @YM08098*/
*                         TRMNCPCL=ON;/* SET CLOSEDOWN IN PROGRESS
*                                                            @YM08098*/
         LA    @11,TRMNCPCL(,RTRM)                              04S0397
         LR    @10,@11                                          04S0397
         LA    @09,3                                            04S0397
         NR    @10,@09                                          04S0397
         LCR   @10,@10                                          04S0397
         ALR   @11,@10                                          04S0397
         L     @09,0(,@11)                                      04S0397
@GL00001 LR    @07,@09                                          04S0397
         O     @07,@CB02670+3(@10)                              04S0397
         CS    @09,@07,0(@11)                                   04S0397
         BNE   @GL00001                                         04S0397
*                         IF OCQUCKN=ON THEN/* QUICK CLOSEDOWN  04S0398
*                                                            @YM08098*/
         TM    OCQUCKN(ROPCE),B'10000000'                       04S0398
         BNO   @RF00398                                         04S0398
*                           TRMQUCKN=ON;/* SET QUICK CLOSE INDICATOR
*                                                            @YM08098*/
         LA    @11,TRMQUCKN(,RTRM)                              04S0399
         LR    @10,@11                                          04S0399
         LA    @09,3                                            04S0399
         NR    @10,@09                                          04S0399
         LCR   @10,@10                                          04S0399
         ALR   @11,@10                                          04S0399
         L     @09,0(,@11)                                      04S0399
@GL00002 LR    @07,@09                                          04S0399
         O     @07,@CB02674+3(@10)                              04S0399
         CS    @09,@07,0(@11)                                   04S0399
         BNE   @GL00002                                         04S0399
*                         RPARM=ONE;/* SETUP TO GET ONE BUFFER       */
@RF00398 LA    RPARM,1                                          04S0400
*                         CALL ALLOCBUF;/* GET BUFFER                */
         L     @15,OPCGETBF(,ROPCAVT)                           04S0401
         BALR  @14,@15                                          04S0401
*                         OPCECTR=OPCECTR+ONE;/* INCREMENT OPCE 04S0402
*                                      COUNTER                       */
         LA    @09,1                                            04S0402
         LH    @07,OPCECTR(,ROPCE)                              04S0402
         ALR   @07,@09                                          04S0402
         STH   @07,OPCECTR(,ROPCE)                              04S0402
*                         IF OPCECTR=ONE THEN/* IF THIS IS FIRST OPCE*/
         CR    @07,@09                                          04S0403
         BNE   @RF00403                                         04S0403
*                           OPCWRKSP(1)=RPARM;/* SAVE ADDR OF FIRST
*                                      OPCE                          */
         ST    RPARM,OPCWRKSP(,ROPCAVT)                         04S0404
*                         ELSE                                  04S0405
*                           OPCWRKSP(4)->OCELMPTR=RPARM;/* ATTACH
*                                      OPCE TO LAST OPCE     @YM08446*/
         B     @RC00403                                         04S0405
@RF00403 L     @09,OPCWRKSP+12(,ROPCAVT)                        04S0405
         STCM  RPARM,7,OCELMPTR(@09)                            04S0405
*                         OPCWRKSP(4)=RPARM;/* SAVE OPCE ADDRESS
*                                                            @YM08446*/
@RC00403 ST    RPARM,OPCWRKSP+12(,ROPCAVT)                      04S0406
*                         CALL IPBLDACT;/* BUILD DEACT OPCE          */
         BAL   @14,IPBLDACT                                     04S0407
*                         OPREQCB=ADDR(AVTOPCOB);/* SETUP OP CONTROL
*                                      QCB                           */
         LA    @09,AVTOPCOB(,RAVT)                              04S0408
         STCM  @09,7,OPREQCB(RPARM)                             04S0408
*                         OPREPRI=PRIHIFLG;/* SETUP POST PRIORITY    */
         MVI   OPREPRI(RPARM),X'E9'                             04S0409
*                         OPREFLG1=OPRFC;/* SETUP FLAG VALUE         */
         MVI   OPREFLG1(RPARM),X'09'                            04S0410
*                       END;        /*                       @YM08098*/
*                   END;                                        04S0412
@RF00395 DS    0H                                               04S0413
*               END;                                            04S0413
@RF00389 DS    0H                                               04S0414
*             RSAT=RSAT+LENGTH(IEDSATD);/* POINT TO NEXT ENTRY       */
@RF00387 AL    RSAT,@CF00045                                    03S0414
*             RCTR=RCTR-ONE;        /* DECREMENT NUMBER OF ENTRIES   */
         BCTR  RCTR,0                                           03S0415
*           END;                                                03S0416
@DE00386 SLR   @09,@09                                          03S0416
         CR    RCTR,@09                                         03S0416
         BNE   @DL00386                                         03S0416
*           IF OPCECTR>0 THEN                                   03S0417
         CH    @09,OPCECTR(,ROPCE)                              03S0417
         BNL   @RF00417                                         03S0417
*             DO;                   /* IF ANY DEACTS TO BE DONE 03S0418
*                                                            @YM08121*/
*               OPCAQWD1=OPCWRKSP(1);/* SETUP ADDRESS OF FIRST OPCE  */
         L     @09,OPCWRKSP(,ROPCAVT)                           03S0419
         ST    @09,OPCAQWD1(,ROPCAVT)                           03S0419
*               OPCAQWD2=RPARM;     /* SETUP ADDRESS OF LAST OPCE    */
         ST    RPARM,OPCAQWD2(,ROPCAVT)                         03S0420
*               OPCAQREQ=OPREQ;     /* INDICATE MORE THAN ONE OPCE   */
         MVC   OPCAQREQ(1,ROPCAVT),OPREQ                        03S0421
*               RPARM=ADDR(OPCAQCTL);/* SETUP ADDR OF AQCTL LIST     */
         LA    RPARM,OPCAQCTL(,ROPCAVT)                         03S0422
*               GEN( SVC 102);      /* POST OPCES                    */
          SVC 102
*               OPCMODID=MOD00;     /* SETUP ID OF NEXT MOD          */
         MVC   OPCMODID(2,ROPCAVT),MOD00                        03S0424
*               OCWTG=''B;          /* CLEAR WTG FIELD       @YM08121*/
         XC    OCWTG(2,ROPCE),OCWTG(ROPCE)                      03S0425
*             END;                                              03S0426
*         END;                                                  03S0427
*       ELSE                                                    03S0428
*         CALL IPTSOCK;             /* CHECK FOR TSO IN SYSTEM  03S0428
*                                                            @OZ30501*/
         B     @RC00381                                         03S0428
@RF00381 BAL   @14,IPTSOCK                                      03S0428
*       RFY                                                     03S0429
*        (RSAT,                                                 03S0429
*         RCTR) UNRSTD;                                         03S0429
@RC00381 DS    0H                                               03S0430
*     END;                                                      02S0430
*   ELSE                                                        02S0431
*     IF OCSWITCH=OCDEACTE THEN                                 02S0431
         B     @RC00374                                         02S0431
@RF00374 CLI   OCSWITCH(ROPCE),X'23'                            02S0431
         BNE   @RF00431                                         02S0431
*       DO;                         /* IF THIS IS AN ENTRY FROM 02S0432
*                                      DEACT, PROCESS DEACT RESPONSE */
*                                                               03S0433
*         /***********************************************************/
*         /*                                                         */
*         /* PROCESS.DEACT: THIS SEGMENT DETERMINES IF INITIAL       */
*         /* REQUEST WAS FOR A SYSTEM CLOSEDOWN OR CLOSEDOWN OF ONE  */
*         /* NCP. IF CLOSEDOWN OF ONE NCP, ORIGINAL OPCE IS FREED AND*/
*         /* CONTROL IS PASSED TO CHECKPOINT, ALONG WITH DEACT OPCE. */
*         /* IF REQUEST WAS FOR A SYSTEM CLOSEDOWN, DEACT OPCE IS    */
*         /* FREED. IF THIS IS LAST OPCE TO BE FREED, CONTROL IS     */
*         /* PASSED TO CHECKPOINT. ELSE MORE DEACT OPCES EXPECTED AND*/
*         /* CONTROL PASSED TO IGC0010D                              */
*         /*                                                         */
*         /***********************************************************/
*                                                               03S0433
*         RFY                                                   03S0433
*           RTEMP RSTD;                                         03S0433
*         RPARM=OCPTRSAV;           /* SETUP PTR TO ORIGINAL OPCE    */
         L     RPARM,OCPTRSAV(,ROPCE)                           03S0434
*         IF RPARM->CLOSEONE=ON THEN                            03S0435
         TM    CLOSEONE(RPARM),B'10000000'                      03S0435
         BNO   @RF00435                                         03S0435
*           DO;                     /* IF REQST TO CLOSEDOWN ONE NCP,
*                                      SEND DEACT OPCE TO CHKPOINT   */
*             OCBACKUP=RPARM->OCBACKUP;/* SAVE NCP NAME FROM ORIG
*                                      OPCE                          */
         MVC   OCBACKUP(8,ROPCE),OCBACKUP(RPARM)                03S0437
*             IF RPARM->OCCONSOL=ON THEN                        03S0438
         TM    OCCONSOL(RPARM),B'10000000'                      03S0438
         BNO   @RF00438                                         03S0438
*               DO;                 /* IF CMD FROM CONSOLE           */
*                 OCCONID=RPARM->OCCONID;/* SAVE CONSOLE ID FROM ORIG
*                                      OPCE                          */
         MVC   OCCONID(1,ROPCE),OCCONID(RPARM)                  03S0440
*                 OCCONSOL=ON;      /* INDICATE FROM CONSOLE         */
         OI    OCCONSOL(ROPCE),B'10000000'                      03S0441
*               END;                                            03S0442
*             ELSE                                              03S0443
*               OCDEST=RPARM->OCDEST;/* SAVE OCDEST FROM ORIGINAL
*                                                            @OZ30354*/
         B     @RC00438                                         03S0443
@RF00438 LH    @11,OCDEST(,RPARM)                               03S0443
         N     @11,@CF02669                                     03S0443
         STH   @11,OCDEST(,ROPCE)                               03S0443
*             RPARM->OCUNIT=INVALTIC;/* SETUP INVALID TIC            */
@RC00438 MVC   OCUNIT(4,RPARM),@CF02567                         03S0444
*             CALL FREEBUFF;        /* FREE ORIGINAL OPCE            */
         L     @15,OPCFREBF(,ROPCAVT)                           03S0445
         BALR  @14,@15                                          03S0445
*             OCPTRSAV=ZERO;        /* CLEAR ORIG OPCE PTR           */
         SLR   @11,@11                                          03S0446
         ST    @11,OCPTRSAV(,ROPCE)                             03S0446
*             OCOPTCDE=OCMODIFY;    /* SETUP DEACT OP CODE           */
         MVI   OCOPTCDE(ROPCE),X'44'                            03S0447
*             OCWTG=MOD09;          /* NEXT MOD AFTER CHKPOINT       */
         MVC   OCWTG(2,ROPCE),MOD09                             03S0448
*             OCPARM=RZERO;         /* SETUP MSG NUMBER              */
         STH   RZERO,OCPARM(,ROPCE)                             03S0449
*             OPCMODID=MOD06;       /* SETUP MOD ID OF CHKPOINT      */
         MVC   OPCMODID(2,ROPCAVT),MOD06                        03S0450
*           END;                                                03S0451
*         ELSE                                                  03S0452
*           DO;                                                 03S0452
         B     @RC00435                                         03S0452
@RF00435 DS    0H                                               03S0453
*             IF OCATTACH=ON THEN                               03S0453
         TM    OCATTACH(ROPCE),B'00000010'                      03S0453
         BNO   @RF00453                                         03S0453
*               DO;                 /* IF OTHER DEACT REQUESTS  03S0454
*                                      ATTACHED TO DEACT OPCE, DETACH
*                                      THEM AND ATTACH TO ORIG OPCE  */
*                 RTEMP=RPARM;      /* SETUP PTR TO ORIGINAL OPCE    */
         LR    RTEMP,RPARM                                      03S0455
*                                                               04S0456
*                 /***************************************************/
*                 /*                                                 */
*                 /* PROCESS.DEACT.ATTACH: THIS SEGMENT CHECKS       */
*                 /* ORIGINAL OPCE FOR ATTACHED OPCES. IF THERE ARE  */
*                 /* ANY, A SEARCH IS MADE FOR LAST ATTACHED OPCE OFF*/
*                 /* ORIGINAL OPCE. ALL OPCES ATTACHED TO DEACT OPCE */
*                 /* ARE THEN ATTACHED TO THE LAST OPCE OFF OF       */
*                 /* ORIGINAL OPCE.                                  */
*                 /*                                                 */
*                 /***************************************************/
*                                                               04S0456
*                 DO WHILE(RTEMP->OCATTACH=ON);/* SEARCH FOR LAST    */
         B     @DE00456                                         04S0456
@DL00456 DS    0H                                               04S0457
*                   RTEMP=RTEMP->OCELMPTR;/* GET NEXT ATTACHED OPCE  */
         L     @11,OCELMPTR-1(,RTEMP)                           04S0457
         LA    @11,0(,@11)                                      04S0457
         LR    RTEMP,@11                                        04S0457
*                 END;                                          04S0458
@DE00456 TM    OCATTACH(RTEMP),B'00000010'                      04S0458
         BO    @DL00456                                         04S0458
*                 RTEMP->OCELMPTR=OCELMPTR;/* ATTACH OTHER REQUESTS  */
         MVC   OCELMPTR(3,RTEMP),OCELMPTR(ROPCE)                04S0459
*                 RTEMP->OCATTACH=ON;/* INDICATE OPCE ATTACHED       */
         OI    OCATTACH(RTEMP),B'00000010'                      04S0460
*                 OCATTACH=OFF;     /* RESET ATTACH FLAG IN DEACT
*                                      OPCE                          */
         NI    OCATTACH(ROPCE),B'11111101'                      04S0461
*               END;                                            03S0462
*             RPARM=OCTRMTBL;       /* GET NCP TTCIN         @XM05820*/
@RF00453 LH    RPARM,OCTRMTBL(,ROPCE)                           03S0463
         N     RPARM,@CF02669                                   03S0463
*             CALL IEDQTNT;         /* FIND TTE FOR NCP      @XM05828*/
         L     @15,AVTRNMPT(,RAVT)                              03S0464
         BALR  @14,@15                                          03S0464
*             RPARM=RPARM-LENGTH(IEDNTRM);/* BACK UP TO PREFIX  03S0465
*                                                            @XM05820*/
         SL    RPARM,@CF02354                                   03S0465
*             RPARM->TRMREMB=ON;    /* TURN ACTIVE BIT ON FOR RESTART
*                                                            @XM05820*/
         LA    @05,TRMREMB(,RPARM)                              03S0466
         LR    @07,@05                                          03S0466
         LA    @11,3                                            03S0466
         NR    @07,@11                                          03S0466
         LCR   @07,@07                                          03S0466
         ALR   @05,@07                                          03S0466
         L     @11,0(,@05)                                      03S0466
@GL00003 LR    @09,@11                                          03S0466
         O     @09,@CB02670+3(@07)                              03S0466
         CS    @11,@09,0(@05)                                   03S0466
         BNE   @GL00003                                         03S0466
*             RPARM=ROPCE;          /* SETUP PTR TO DEACT OPCE       */
         LR    RPARM,ROPCE                                      03S0467
*             ROPCE=RPARM->OCPTRSAV;/* SETUP PTR TO ORIGINAL OPCE    */
         L     ROPCE,OCPTRSAV(,RPARM)                           03S0468
*             OPCCOPCE=ROPCE;       /* SETUP OPCE IN OPCAVT          */
         ST    ROPCE,OPCCOPCE(,ROPCAVT)                         03S0469
*             RPARM->OCUNIT=INVALTIC;/* INDICATE LAST UNIT           */
         MVC   OCUNIT(4,RPARM),@CF02567                         03S0470
*             CALL FREEBUFF;        /* FREE BUFFER                   */
         L     @15,OPCFREBF(,ROPCAVT)                           03S0471
         BALR  @14,@15                                          03S0471
*             OPCECTR=OPCECTR-ONE;  /* DECREMENT OPCE COUNT          */
         LH    @11,OPCECTR(,ROPCE)                              03S0472
         BCTR  @11,0                                            03S0472
         STH   @11,OPCECTR(,ROPCE)                              03S0472
*             IF OPCECTR>ZERO THEN  /* IF MORE OPCES LEFT TO PROCESS */
         LTR   @11,@11                                          03S0473
         BNP   @RF00473                                         03S0473
*               OPCMODID=MOD00;     /* SETUP MOD TO GET CONTROL      */
         MVC   OPCMODID(2,ROPCAVT),MOD00                        03S0474
*             ELSE                                              03S0475
*               DO;                 /* SETUP FOR CHKPOINT            */
         B     @RC00473                                         03S0475
@RF00473 DS    0H                                               03S0476
*                 IF OCATTACH=ON THEN                           03S0476
         TM    OCATTACH(ROPCE),B'00000010'                      03S0476
         BNO   @RF00476                                         03S0476
*                   DO;             /* IF ANY OPCES ATTACHED, DELINK
*                                      THEM                          */
*                     OPCNEXT=OCELEM;/* CHAIN OPCES TO OPCAVT        */
         L     @11,OCELEM(,ROPCE)                               03S0478
         ST    @11,OPCNEXT(,ROPCAVT)                            03S0478
*                     OCELEM=ZERO;  /* DETACH OPCES FROM OPCE        */
         SLR   @11,@11                                          03S0479
         ST    @11,OCELEM(,ROPCE)                               03S0479
*                     OCATTACH=OFF; /* TURN ATTACH FLAG OFF          */
         NI    OCATTACH(ROPCE),B'11111101'                      03S0480
*                     OCUNIT=INVALTIC;/* INDICATE LAST UNIT          */
         MVC   OCUNIT(4,ROPCE),@CF02567                         03S0481
*                   END;                                        03S0482
*                 CALL IPTSOCK;     /* CHECK FOR TSO IN SYSTEM  03S0483
*                                                            @OZ30501*/
@RF00476 BAL   @14,IPTSOCK                                      03S0483
*                 OCWTG=MOD00;      /* SETUP NEXT MOD AFTER CHKPOINT */
         MVC   OCWTG(2,ROPCE),MOD00                             03S0484
*                 OCPARM=ZERO;      /* SETUP MSG NUMBER              */
         SLR   @11,@11                                          03S0485
         STH   @11,OCPARM(,ROPCE)                               03S0485
*                 OPCMODID=MOD06;   /* SETUP ID OF CHKPOINT          */
         MVC   OPCMODID(2,ROPCAVT),MOD06                        03S0486
*               END;                                            03S0487
*           END;                                                03S0488
@RC00473 DS    0H                                               03S0489
*         RFY                                                   03S0489
*           RTEMP UNRSTD;                                       03S0489
@RC00435 DS    0H                                               03S0490
*       END;                                                    02S0490
*     ELSE                                                      02S0491
*       DO;                         /* ELSE PROCESS INITIAL ENTRY    */
         B     @RC00431                                         02S0491
@RF00431 DS    0H                                               02S0492
*                                                               03S0492
*         /***********************************************************/
*         /*                                                         */
*         /* PROCESS.FIRSTREQ: THIS SEGMENT WILL CHECK SYNTAX ERRORS */
*         /* OR INVALID OPERANDS. THE TYPE OF CLOSEDOWN REQUEST IS   */
