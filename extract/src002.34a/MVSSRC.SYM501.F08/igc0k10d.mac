         TITLE 'IGC0K10D, OPERATOR CONTROL MESSAGE MODULE              *
                        '
IGC0K10D CSECT ,                                                01S0001
@PROLOG  BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                                   /*
IGC0K10D IEDHJN AFTERHJN,HJN           MODULE IDENTIFICATION MACRO
*   ROPCAVT=RPARM;                  /* ESTABLISH ADDRESSABILITY TO
*                                      THE OPERATOR CONTROL ADDRESS
*                                      VECTOR TABLE                  */
         LR    ROPCAVT,RPARM                                    02S0176
*   RFY                                                         02S0177
*     RPARM UNRSTD;                 /* RELEASE THE REGISTER     02S0177
*                                      RESTRICTED AT ENTRY TO THE
*                                      MODULE THAT CONTAINS THE 02S0177
*                                      ADDRESS OF THE OPERATOR  02S0177
*                                      CONTROL ADDRESS VECTOR TABLE  */
*   REG10=OPCWORK+LENGTH(MSGWORKA); /* SET THE BASE REGISTER TO BE
*                                      USED FOR ADDRESSING AUTOMATIC
*                                      DATA WITH THE ADDRESS OF THE
*                                      SECTION OF THE OPERATOR  02S0178
*                                      CONTROL RESIDENT WORK AREA
*                                      THAT IS AFTER THE SECTION USED
*                                      TO CONSTRUCT MESSAGES         */
         LA    REG10,84                                         02S0178
         AL    REG10,OPCWORK(,ROPCAVT)                          02S0178
*   GEN REFS(REG10) NOSETS;                                     02S0179
*                                   /*
         USING @DATD,REG10             PROVIDE ADDRESSABILITY TO ALL
*                                      AUTOMATIC DATA
*   RSAVE=ADDR(OPCSAVE);            /* SET THE SAVE AREA ADDRESS
*                                      REGISTER WITH THE ADDRESS OF
*                                      THE OPERATOR CONTROL SAVE AREA*/
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          02S0180
*                                                               02S0181
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS: THIS SEGMENT DOES THE PROCESSING THAT IS NECESSARY TO*/
*   /* PERFORM THE FUNCTION OF THE MODULE.                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0181
*                                                               03S0181
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS.INITIAL: THIS SEGMENT DOES THE INITIAL PROCESSING.    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               03S0181
*   RAVT=OPCAVTPT;                  /* SET THE BASE POINTER FOR THE
*                                      TCAM ADDRESS VECTOR TABLE     */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          03S0181
*   ROPCE=OPCCOPCE;                 /* SET THE BASE POINTER FOR THE
*                                      OPERATOR CONTROL ELEMENT      */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         03S0182
*   SEARCH=YES;                     /* INITIALIZE THE "SEARCH" SWITCH
*                                      TO INDICATE THAT A SEARCH IS
*                                      TO BE PERFORMED               */
         OI    SEARCH,B'10000000'                               02S0183
*   DO PTRENTRY=ADDR(TABDATA) BY LENGTH(TABENTRY) WHILE(SEARCH=YES);
         LA    PTRENTRY,TABDATA                                 02S0184
         B     @DE00184                                         02S0184
@DL00184 DS    0H                                               02S0185
*                                   /* INCREMENT THE POINTER TO 02S0185
*                                      ENTRIES IN THE MESSAGE DATA
*                                      TABLE WHILE THE SEARCH IS IN
*                                      PROGRESS                      */
*     IF TABMNUM=REG00              /* MATCHING MESSAGE NUMBERS      */
*       THEN                        /* STOP THE SEARCH AND BUILD THE
*                                      APPROPRIATE MESSAGE           */
         CH    REG00,TABMNUM(,PTRENTRY)                         02S0185
         BNE   @RF00185                                         02S0185
*                                                               03S0186
*       /*************************************************************/
*       /*                                                           */
*       /* PROCESS.BUILDMSG: THIS SEGMENT STOPS THE SEARCH FOR A     */
*       /* MATCHING MESSAGE TABLE ENTRY, BUILDS THE APPROPRIATE      */
*       /* MESSAGE IN THE OPERATOR CONTROL RESIDENT WORK AREA AND    */
*       /* DOES THE PREPARATION THAT IS NECESSARY TO TRANSFER CONTROL*/
*       /* TO THE LAST MODULE IN THE MESSAGE MODULE CHAIN.           */
*       /*                                                           */
*       /*************************************************************/
*                                                               03S0186
*       DO;                                                     03S0186
*         SEARCH=STOP;              /* INDICATE THAT THE SEARCH IS TO
*                                      BE STOPPED                    */
         NI    SEARCH,B'01111111'                               03S0187
*         MSGLNGTH=TABLNGTH;        /* SET THE LENGTH OF THE MESSAGE
*                                      IN THE OPERATOR CONTROL  03S0188
*                                      RESIDENT WORK AREA            */
         L     @11,OPCWORK(,ROPCAVT)                            03S0188
         LH    @01,TABLNGTH(,PTRENTRY)                          03S0188
         STH   @01,MSGLNGTH(,@11)                               03S0188
*         MSGTEXT(1:MSGLNGTH)=MSGSTRNG(1:MSGLNGTH);/* PUT THE MESSAGE
*                                      CHARACTER STRING INTO THE
*                                      MESSAGE TEXT FIELD OF THE WORK
*                                      AREA                          */
         BCTR  @01,0                                            03S0189
         L     @15,TABMPTR(,PTRENTRY)                           03S0189
         EX    @01,@SM01172                                     03S0189
*         IF TABCASE>0              /* ADDITIONAL PROCESSING OF THE
*                                      MESSAGE IS NECESSARY          */
*           THEN                    /* PROCESS THE UNIQUE       03S0190
*                                      REQUIREMENTS OF THE MESSAGE   */
         L     @11,TABCASE(,PTRENTRY)                           03S0190
         LTR   @11,@11                                          03S0190
         BNP   @RF00190                                         03S0190
*                                                               04S0191
*           /*********************************************************/
*           /*                                                       */
*           /* PROCESS.BUILDMSG.CASE: THIS SEGMENT IMPLEMENTS THE    */
*           /* CASE CONTROL STRUCTURE IN ORDER TO SATISFY THE UNIQUE */
*           /* REQUIREMENTS OF THE MESSAGES WITH VARIABLE CHARACTER  */
*           /* FIELDS THAT ARE BUILT IN THE OPERATOR CONTROL RESIDENT*/
*           /* WORK AREA BY THIS MODULE.                             */
*           /*                                                       */
*           /*********************************************************/
*                                                               04S0191
*           DO;                     /* CASE(TABCASE)NAMES(SUBORDIN,AL
*                                      LSUBOR,INVALRLN,ACTIVATE,LDMOD
*                                      ULE,ACTDEACT,CLUSTNME)        */
*             DCL                                               04S0192
*               CL0001@ LABEL BASED(CP0001@(TABCASE)) VALUERANGE(
*                   SUBORDIN,ALLSUBOR,INVALRLN,ACTIVATE,LDMODULE,
*                   ACTDEACT,CLUSTNME);/* BRANCH TARGET              */
*             DCL                                               04S0193
*               CP0001@(7) PTR(31) INIT(ADDR(SUBORDIN),ADDR(ALLSUBOR),
*                   ADDR(INVALRLN),ADDR(ACTIVATE),ADDR(LDMODULE),ADDR(
*                   ACTDEACT),ADDR(CLUSTNME));                  04S0193
*             GOTO CL0001@;         /* BRANCH TO SELECTED CASE       */
         SLA   @11,2                                            04S0194
         L     @01,CP0001@-4(@11)                               04S0194
         BR    @01                                              04S0194
*                                                               05S0195
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS.BUILDMSG.CASE.SUBORDIN: THIS SEGMENT PLACES */
*             /* THE HOLD OR RELEASE COMMAND,THE RESOURCE NAME,AND   */
*             /* THE RELATIVE LINE NUMBER INTO THE REQUIRED FIELDS IN*/
*             /* THE MESSAGE TEXT                                    */
*             /*                                                     */
*             /*******************************************************/
*                                                               05S0195
*SUBORDIN:                                                      05S0195
*             DO;                   /* SUBCASE                       */
SUBORDIN DS    0H                                               05S0196
*               IF OCOPTCDE=OCHOLD  /* IF THE COMMAND OPERATION CODE
*                                      IS A HOLD COMMAND             */
*                 THEN                                          05S0196
         CLI   OCOPTCDE(ROPCE),X'6C'                            05S0196
         BNE   @RF00196                                         05S0196
*                 DO;                                           05S0197
*                   OPTCMD(1:LENGTH(OPTHELD))=OPTHELD;/* SOME   05S0198
*                                      SUBORDINATES WERE HELD        */
         L     @01,OPCWORK(,ROPCAVT)                            05S0198
         MVC   OPTCMD(4,@01),@CC01038                           05S0198
*                 END;                                          05S0199
*               ELSE                                            05S0200
*                 OPTCMD=OPTRELSE;  /* SOME SUBORDINATES WERE   05S0200
*                                      RELEASED                      */
         B     @RC00196                                         05S0200
@RF00196 L     @01,OPCWORK(,ROPCAVT)                            05S0200
         MVC   OPTCMD(8,@01),@CC01040                           05S0200
*               GRPULNME=OCBACKUP;  /* PLACE SUBORDINATE NAME INTO
*                                      MESSAGE TEXT                  */
@RC00196 L     @11,OPCWORK(,ROPCAVT)                            05S0201
         MVC   GRPULNME(8,@11),OCBACKUP(ROPCE)                  05S0201
*               CVD(OCRLN,OPCDOUBL);                            05S0202
         SLR   @01,@01                                          05S0202
         IC    @01,OCRLN(,ROPCE)                                05S0202
         CVD   @01,OPCDOUBL(,ROPCAVT)                           05S0202
*               UNPK(OPCDOUBL,OPCDOUBL);                        05S0203
         UNPK  OPCDOUBL(8,ROPCAVT),OPCDOUBL(8,ROPCAVT)          05S0203
*               OPCDOUBL(8:8)=OPCDOUBL(8:8)×ZONEMASK;           05S0204
         OI    OPCDOUBL+7(ROPCAVT),C'0'                         05S0204
*               IF OCRLN=NORLN THEN /* DO WE WANT RLN        @YM07016*/
         CLI   OCRLN(ROPCE),0                                   05S0205
         BNE   @RF00205                                         05S0205
*                 MSGLNGTH=MSGLNGTH-MFOUR;/* IF NOT SHORTEN MSG 05S0206
*                                                            @YM07016*/
         LH    @01,MSGLNGTH(,@11)                               05S0206
         SL    @01,@CF00327                                     05S0206
         STH   @01,MSGLNGTH(,@11)                               05S0206
*               ELSE                                            05S0207
*                 RLNMBR=OPCDOUBL(6:8);/* PLACE RELATIVE LINE NUMBER
*                                      INTO MESSAGE TEXT             */
         B     @RC00205                                         05S0207
@RF00205 L     @01,OPCWORK(,ROPCAVT)                            05S0207
         MVC   RLNMBR(3,@01),OPCDOUBL+5(ROPCAVT)                05S0207
*               GOTO CE0001@;       /* BRANCH TO END OF CASE         */
         B     CE0001@                                          05S0208
*             END;                  /* END OF SUBCASE                */
*                                                               05S0210
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS.BUILDMSG.CASE.ALLSUBOR: THIS SEGMENT PLACES */
*             /* THE HOLD OR RELEASE COMMAND, THE RESOURCE NAME,AND  */
*             /* THE RELATIVE LINE NUMBER INTO THE REQUIRED FIELDS IN*/
*             /* THE MESSAGE TEXT                                    */
*             /*                                                     */
*             /*******************************************************/
*                                                               05S0210
*ALLSUBOR:                                                      05S0210
*             DO;                   /* SUBCASE                       */
ALLSUBOR DS    0H                                               05S0211
*               IF OCOPTCDE=OCHOLD  /* IF THE COMMAND OPERATION CODE
*                                      IS A HOLD COMMAND             */
*                 THEN                                          05S0211
         CLI   OCOPTCDE(ROPCE),X'6C'                            05S0211
         BNE   @RF00211                                         05S0211
*                 DO;                                           05S0212
*                   OPT(1:LENGTH(OPTHELD))=OPTHELD;/* SUBORDINATES
*                                      WERE HELD                     */
         L     @01,OPCWORK(,ROPCAVT)                            05S0213
         MVC   OPT(4,@01),@CC01038                              05S0213
*                 END;                                          05S0214
*               ELSE                                            05S0215
*                 OPT=OPTRELSE;     /* SUBORDINATES WERE RELEASED    */
         B     @RC00211                                         05S0215
@RF00211 L     @01,OPCWORK(,ROPCAVT)                            05S0215
         MVC   OPT(8,@01),@CC01040                              05S0215
*               GRPLNPU=OCBACKUP;   /* PLACE SUBORDINATE NAME INTO
*                                      MESSAGE TEXT                  */
@RC00211 L     @11,OPCWORK(,ROPCAVT)                            05S0216
         MVC   GRPLNPU(8,@11),OCBACKUP(ROPCE)                   05S0216
*               CVD(OCRLN,OPCDOUBL);                            05S0217
         SLR   @01,@01                                          05S0217
         IC    @01,OCRLN(,ROPCE)                                05S0217
         CVD   @01,OPCDOUBL(,ROPCAVT)                           05S0217
*               UNPK(OPCDOUBL,OPCDOUBL);                        05S0218
         UNPK  OPCDOUBL(8,ROPCAVT),OPCDOUBL(8,ROPCAVT)          05S0218
*               OPCDOUBL(8:8)=OPCDOUBL(8:8)×ZONEMASK;           05S0219
         OI    OPCDOUBL+7(ROPCAVT),C'0'                         05S0219
*               IF OCRLN=NORLN THEN /* DO WE WANT RLN        @YM07016*/
         CLI   OCRLN(ROPCE),0                                   05S0220
         BNE   @RF00220                                         05S0220
*                 MSGLNGTH=MSGLNGTH-MFOUR;/* SHORTEN MSG BY FOUR
*                                                            @YM07016*/
         LH    @01,MSGLNGTH(,@11)                               05S0221
         SL    @01,@CF00327                                     05S0221
         STH   @01,MSGLNGTH(,@11)                               05S0221
*               ELSE                                            05S0222
*                 RLN=OPCDOUBL(6:8);/* PLACE RELATIVE LINE NUMBER
*                                      INTO MESSAGE TEXT             */
         B     @RC00220                                         05S0222
@RF00220 L     @01,OPCWORK(,ROPCAVT)                            05S0222
         MVC   RLN(3,@01),OPCDOUBL+5(ROPCAVT)                   05S0222
*               GOTO CE0001@;       /* BRANCH TO END OF CASE         */
         B     CE0001@                                          05S0223
*             END;                  /* END OF SUBCASE                */
*                                                               05S0225
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS.BUILDMSG.CASE.INVALRLN: THIS SEGMENT PLACES */
*             /* THE RESOURCE NAME AND THE RELATIVE LINE NUMBER INTO */
*             /* THE REQUIRED FIELDS IN THE MESSAGE TEXT             */
*             /*                                                     */
*             /*******************************************************/
*                                                               05S0225
*INVALRLN:                                                      05S0225
*             DO;                   /* SUBCASE                       */
INVALRLN DS    0H                                               05S0226
*               HLDRELSE=OCBACKUP;  /* PLACE GROUP NAME INTO MESSAGE */
         L     @11,OPCWORK(,ROPCAVT)                            05S0226
         MVC   HLDRELSE(8,@11),OCBACKUP(ROPCE)                  05S0226
*               CVD(OCRLN,OPCDOUBL);                            05S0227
         SLR   @01,@01                                          05S0227
         IC    @01,OCRLN(,ROPCE)                                05S0227
         CVD   @01,OPCDOUBL(,ROPCAVT)                           05S0227
*               UNPK(OPCDOUBL,OPCDOUBL);                        05S0228
         UNPK  OPCDOUBL(8,ROPCAVT),OPCDOUBL(8,ROPCAVT)          05S0228
*               OPCDOUBL(8:8)=OPCDOUBL(8:8)×ZONEMASK;           05S0229
         OI    OPCDOUBL+7(ROPCAVT),C'0'                         05S0229
*               RELNMBR=OPCDOUBL(6:8);/* PLACE RELATIVE LINE NUMBER
*                                      INTO MESSAGE                  */
         MVC   RELNMBR(3,@11),OPCDOUBL+5(ROPCAVT)               05S0230
*               GOTO CE0001@;       /* BRANCH TO END OF CASE         */
         B     CE0001@                                          05S0231
*             END;                  /* END OF SUBCASE                */
*                                                               05S0233
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS.BUILDMSG.CASE.ACTIVATE: THIS SEGMENT PLACES */
*             /* THE RESOURCE NAME INTO THE REQUIRED FIELD IN THE    */
*             /* MESSAGE TEXT                                        */
*             /*                                                     */
*             /*******************************************************/
*                                                               05S0233
*ACTIVATE:                                                      05S0233
*             DO;                   /* SUBCASE                       */
ACTIVATE DS    0H                                               05S0234
*               RESNAME=OCBACKUP;   /* PLACE RESOURCE NAME INTO 05S0234
*                                      MESSAGE                       */
         L     @01,OPCWORK(,ROPCAVT)                            05S0234
         MVC   RESNAME(8,@01),OCBACKUP(ROPCE)                   05S0234
*               GOTO CE0001@;       /* BRANCH TO END OF CASE         */
         B     CE0001@                                          05S0235
*             END;                  /* END OF SUBCASE                */
*                                                               05S0237
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS.BUILDMSG.CASE.LDMODULE: THIS SEGMENT PLACES */
*             /* THE RESOURCE NAME AND THE LOAD MODULE NAME INTO THE */
*             /* REQUIRED FIELDS IN THE MESSAGE TEXT                 */
*             /*                                                     */
*             /*******************************************************/
*                                                               05S0237
*LDMODULE:                                                      05S0237
*             DO;                   /* SUBCASE                       */
LDMODULE DS    0H                                               05S0238
*               LOADMOD=OCBACKUP;   /* PLACE NAME INTO MESSAGE       */
         L     @11,OPCWORK(,ROPCAVT)                            05S0238
         MVC   LOADMOD(8,@11),OCBACKUP(ROPCE)                   05S0238
*               LDMODID=OCLDNME;    /* PLACE LOAD MODULE ID INTO
*                                      MESSAGE                       */
         LR    @01,ROPCE                                        05S0239
         AL    @01,@CF01174                                     05S0239
         MVC   LDMODID(8,@11),OCLDNME(@01)                      05S0239
*               GOTO CE0001@;       /* BRANCH TO END OF CASE         */
         B     CE0001@                                          05S0240
*             END;                  /* END OF SUBCASE                */
*                                                               05S0242
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS.BUILDMSG.CASE.ACTDEACT: THIS SEGMENT PLACES */
*             /* THE RESOURCE NAME INTO THE REQUIRED FIELD IN THE    */
*             /* MESSAGE TEXT                                        */
*             /*                                                     */
*             /*******************************************************/
*                                                               05S0242
*ACTDEACT:                                                      05S0242
*             DO;                   /* SUBCASE                       */
ACTDEACT DS    0H                                               05S0243
*               RESOUNME=OCBACKUP;  /* PLACE RESOURCE NAME INTO 05S0243
*                                      MESSAGE                       */
         L     @01,OPCWORK(,ROPCAVT)                            05S0243
         MVC   RESOUNME(8,@01),OCBACKUP(ROPCE)                  05S0243
*               GOTO CE0001@;       /* BRANCH TO END OF CASE         */
         B     CE0001@                                          05S0244
*             END;                  /* END OF SUBCASE                */
*                                                               05S0246
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS.BUILDMSG.CASE.CLUSTNME: THIS SEGMENT PLACES */
*             /* THE RESOURCE NAME INTO THE REQUIRED FIELD IN THE    */
*             /* MESSAGE TEXT                                        */
*             /*                                                     */
*             /*******************************************************/
*                                                               05S0246
*CLUSTNME:                                                      05S0246
*             DO;                   /* SUBCASE                       */
CLUSTNME DS    0H                                               05S0247
*               CLUSTER=OCBACKUP;   /* PLACE CLUSTER NAME INTO  05S0247
*                                      MESSAGE                       */
         L     @01,OPCWORK(,ROPCAVT)                            05S0247
         MVC   CLUSTER(8,@01),OCBACKUP(ROPCE)                   05S0247
*               GOTO CE0001@;       /* BRANCH TO END OF CASE         */
         B     CE0001@                                          05S0248
*             END;                  /* END OF SUBCASE                */
*CE0001@:                                                       04S0250
*           END;                    /* END OF CASE                   */
CE0001@  DS    0H                                               04S0251
*         OPCMODID='07';            /* SET THE MODULE IDENTIFICATION
*                                      CHARACTERS FOR THE NEXTMODULE
*                                      TO BE EXECUTED                */
@RF00190 MVC   OPCMODID(2,ROPCAVT),@CC01168                     03S0251
*       END;                                                    03S0252
*     ELSE                          /* CHECK FOR NO MATCHING TABLE
*                                      ENTRY FOR THE MESSAGE NUMBER  */
*       IF TABMNUM=0                /* LAST TABLE ENTRY REACHED      */
*           ×TABMNUM>REG00          /* THERE IS NO TABLE ENTRY FOR
*                                      THE ARGUMENT MESSAGE NUMBER   */
*         THEN                                                  02S0253
         B     @RC00185                                         02S0253
@RF00185 LH    @11,TABMNUM(,PTRENTRY)                           02S0253
         LTR   @11,@11                                          02S0253
         BZ    @RT00253                                         02S0253
         CR    @11,REG00                                        02S0253
         BNH   @RF00253                                         02S0253
@RT00253 DS    0H                                               02S0254
*         DO;                       /* PREPARE TO TRANSFER CONTROL TO
*                                      THE NEXT MESSAGE MODULE TO
*                                      CONTINUE THE SEARCH           */
*           SEARCH=STOP;            /* INDICATE THAT THE SEARCH IS TO
*                                      BE STOPPED                    */
         NI    SEARCH,B'01111111'                               02S0255
*           OPCMODID='0L';          /* SET MODULE IDENTIFICATION
*                                      CHARACTERS FOR THE NEXT  02S0256
*                                      MESSAGE MODULE IN THE CHAIN   */
         MVC   OPCMODID(2,ROPCAVT),@CC01169                     02S0256
*         END;                                                  02S0257
*   END;                                                        02S0258
@RF00253 DS    0H                                               02S0258
@RC00185 AL    PTRENTRY,@CF00032                                02S0258
@DE00184 TM    SEARCH,B'10000000'                               02S0258
         BO    @DL00184                                         02S0258
*                                                               02S0259
*   /*****************************************************************/
*   /*                                                               */
*   /* EPILOGUE: THIS SEGMENT COMPLETES THE EPILOGUE CODE THAT WAS   */
*   /* ALTERED BY THE MODULE PROCEDURE STATEMENT. THE XCTL ASSEMBLER */
*   /* MACRO INSTRUCTION IS USED BY THIS SEGMENT TO TRANSFER CONTROL */
*   /* TO THE NEXT MODULE THAT IS TO BE EXECUTED.                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0259
*   RFY                                                         02S0259
*     REG01 RSTD;                   /* RESTRICT THE ARGUMENT REGISTER
*                                      TO BE USED TO CONTAIN THE
*                                      ADDRESS OF THE OPERATOR  02S0259
*                                      CONTROL ADDRESS VECTOR TABLE
*                                      FROM USE BY THE COMPILER      */
*   REG01=ROPCAVT;                  /* SET THE ARGUMENT REGISTER IN
*                                      PREPARATION TO TRANSFERRING
*                                      CONTROL TO THE NEXT MODULE TO
*                                      BE EXECUTED                   */
         LR    REG01,ROPCAVT                                    02S0260
*   GEN EXIT REFS(OPCXCTL,ROPCAVT) NOSEQFLOW SETS(REG15);       02S0261
*                                   /*
         XCTL  SF=(E,OPCXCTL(ROPCAVT)) TRANSFER CONTROL MACRO
*   END IGC0K10D                                                01S0262
*                                                               01S0262
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */
*/*%INCLUDE SYSLIB  (TAVTD   )                                       */
*/*%INCLUDE SYSLIB  (TOPCED  )                                       */
*/*%INCLUDE SYSLIB  (TOPCAVTD)                                       */
*/*%INCLUDE SYSUT5  (RFY     )                                       */
*/*%INCLUDE SYSUT5  (DCL     )                                       */
*/*%INCLUDE SYSUT5  (PROLOGUE)                                       */
*/*%INCLUDE SYSUT5  (PROCESS )                                       */
*/*%INCLUDE SYSUT5  (INITIAL )                                       */
*/*%INCLUDE SYSUT5  (BUILDMSG)                                       */
*/*%INCLUDE SYSUT5  (CASE    )                                       */
*/*%INCLUDE SYSUT5  (SUBORDIN)                                       */
*/*%INCLUDE SYSUT5  (ALLSUBOR)                                       */
*/*%INCLUDE SYSUT5  (INVALRLN)                                       */
*/*%INCLUDE SYSUT5  (ACTIVATE)                                       */
*/*%INCLUDE SYSUT5  (LDMODULE)                                       */
*/*%INCLUDE SYSUT5  (ACTDEACT)                                       */
*/*%INCLUDE SYSUT5  (CLUSTNME)                                       */
*/*%INCLUDE SYSUT5  (EPILOGUE)                                       */
*                                                               01S0262
*       ;                                                       01S0262
@DATA    DS    0H
@SM01172 MVC   MSGTEXT(0,@11),MSGSTRNG(@15)
@DATD    DSECT
         DS    0F
IGC0K10D CSECT
         DS    0F
@CF00327 DC    F'4'
@CF00032 DC    F'12'
@CF01174 DC    F'-8'
@DATD    DSECT
         DS    0D
SEARCH   DS    BL1
         ORG   *+1-(*-@DATD)/(*-@DATD) INSURE DSECT DATA
@ENDDATD EQU   *
IGC0K10D CSECT
         DS    0F
@SIZDATD DC    AL1(0)
         DC    AL3(@ENDDATD-@DATD)
         DS    0D
@CC01040 DC    C'RELEASED'
@CC01038 DC    C'HELD'
@CC01168 DC    C'07'
@CC01169 DC    C'0L'
IED521I  DC    CL56'IED521I SOME SUBORDINATES WERE          FOR        C
                ,   '
IED522I  DC    CL54'IED522I ALL SUBORDINATES ARE          FOR         ,C
                  '
IED523I  DC    CL54'IED523I NO SUBORDINATES WERE          FOR         ,C
                  '
IED524I  DC    CL39'IED524I RLN     IS INVALID FOR         '
IED525I  DC    CL45'IED525I COMMAND INVALID FOR RESOURCE         '
IED529I  DC    CL38'IED529I          ACTIVATE UNSUCCESSFUL'
IED530I  DC    CL45'IED530I          PARTIAL ACTIVATION COMPLETED'
