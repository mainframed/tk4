         TITLE 'IGCV810D, STOPLINE FOR 3705 LINES                      *
                        '
IGCV810D CSECT ,                                                01S0001
@PROLOG  DS    0H                                               01S0002
         USING *,RBASE                  ESTABLISH A BASE REGISTER
         LR    RBASE,REG15              SETUP BASE REG
IGCV810D IEDHJN AFTERHJN,HJN            MODULE IDENTIFICATION MACRO
*   ROPCAVT=RPARM;                  /* SET BASE POINTER FOR OP  03S0398
*                                      CONTROL AVT                   */
         LR    ROPCAVT,RPARM                                    03S0398
*   RAVT=OPCAVTPT;                  /* SET THE BASE POINTER FOR THE
*                                      TCAM ADDRESS VECTOR TABLE     */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          03S0399
*   ROPCE=OPCCOPCE;                 /* SET THE BASE POINTER FOR THE
*                                      OPERATOR CONTROL ELEMENT      */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         03S0400
*   OPCFLAG1=ZERO;                  /* CLEAR CONTROL SWITCHES        */
         MVI   OPCFLAG1(ROPCAVT),X'00'                          03S0401
*   IF OCRESP=ON THEN                                           02S0402
         TM    OCRESP(ROPCE),B'00000001'                        02S0402
         BNO   @RF00402                                         02S0402
*     DO;                           /* IF THIS IS AN OPCE WITH A
*                                      NON-SNA RESPONSE, PROCESS THE
*                                      RESPONSE                      */
*                                                               03S0404
*       /*************************************************************/
*       /*                                                           */
*       /* PROCESS.RESPROC: THIS ROUTINE WILL PROCESS A NON-SNA      */
*       /* RESPONSE                                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                               03S0404
*       RFY                                                     03S0404
*        (RWORK5,                                               03S0404
*         RWORK6,                                               03S0404
*         RWORK7,                                               03S0404
*         RWORKA) RSTD;                                         03S0404
*       CALL IPWLSRCH;              /* SEARCH WAITV CHAIN FOR   03S0405
*                                      MATCHING OPCE                 */
         BAL   @14,IPWLSRCH                                     03S0405
*       RZERO->OCLINK=RPARM->OCLINK;/* UNLINK OPCE FROM CHAIN        */
         L     @11,OCLINK(,RPARM)                               03S0406
         LR    @09,RZERO                                        03S0406
         ST    @11,OCLINK(,@09)                                 03S0406
*       RWORK6=RPARM;               /* SAVE ADDRESS OF REAL OPCE     */
         LR    RWORK6,RPARM                                     03S0407
*       RWORKA=0;                   /* CLEAR OUT WORK REGISTER  03S0408
*                                                            @OY19465*/
         SLR   RWORKA,RWORKA                                    03S0408
*       WAITFLG=ON;                 /* TURN WAITFLG ON       @OY19465*/
         OI    WAITFLG(ROPCAVT),B'00100000'                     03S0409
*       RPARM=RZERO->OCLINK;        /* SET RPARM TO NEXT ELEMENT
*                                                            @OY19465*/
         LR    RPARM,@11                                        03S0410
*       DO WHILE(WAITFLG=ON);       /* CONTINUE UNITL WAITFLG IS
*                                      TURNED OFF            @OY19465*/
         B     @DE00411                                         03S0411
@DL00411 DS    0H                                               03S0412
*         IF RPARM=ZERO THEN                                    03S0412
         LTR   RPARM,RPARM                                      03S0412
         BNZ   @RF00412                                         03S0412
*           WAITFLG=OFF;            /* END OF WAITV CHAIN    @OY19465*/
         NI    WAITFLG(ROPCAVT),B'11011111'                     03S0413
*         ELSE                                                  03S0414
*           DO;                     /* CONTINUE SEARCHING    @OY19465*/
         B     @RC00412                                         03S0414
@RF00412 DS    0H                                               03S0415
*             IF OCTRMTBL=RPARM->OCTRMTBL&RPARM->OCDACTIP=OFF THEN
         CLC   OCTRMTBL(2,ROPCE),OCTRMTBL(RPARM)                03S0415
         BNE   @RF00415                                         03S0415
         TM    OCDACTIP(RPARM),B'01000000'                      03S0415
         BNZ   @RF00415                                         03S0415
*               DO;                 /* IF THE TTCIN OF THE ELEMENT IS
*                                      EQUAL TO THAT OF THE CURRENT
*                                      OPCE AND A DEACT HAS NOT BEEN
*                                      SENT OUT FOR THE LINE THEN
*                                      DELINK THE ELEMENT OFF OF
*                                      OPCWAITV AND ATTACH IT TO THE
*                                      CURRENT ELEMENT       @OY19465*/
*                 CALL DLKATT;      /* CALL THE DELINK AND ATTACH
*                                      ROUTINE               @OY19465*/
         BAL   @14,DLKATT                                       03S0417
*                 RPARM=RZERO->OCLINK;/* SET REGISTER RPARM UP FOR
*                                      REST OF SEARCH OF THE CHAIN
*                                                            @OY19465*/
         LR    @11,RZERO                                        03S0418
         L     RPARM,OCLINK(,@11)                               03S0418
*               END;                /*                       @OY19465*/
*             ELSE                                              03S0420
*               DO;                 /* MUST SEARCH CHAIN FOR ANY
*                                      STOPTERMS THAT ARE IN PROGRESS
*                                      ON THIS LINE          @OY19465*/
         B     @RC00415                                         03S0420
@RF00415 DS    0H                                               03S0421
*                 IF RPARM->OCWTG=MODVF&OCBACKUP=RPARM->OCBACKUP THEN
         CLC   OCWTG(2,RPARM),MODVF                             03S0421
         BNE   @RF00421                                         03S0421
         CLC   OCBACKUP(8,ROPCE),OCBACKUP(RPARM)                03S0421
         BNE   @RF00421                                         03S0421
*                   DO;             /* THIS IS A STOPTERM FOR A 03S0422
*                                      TERMINAL ON A LINE IN THIS
*                                      GROUP                 @OY19465*/
*                     WAITFLG=OFF;  /* ONCE WE FIND A STOPTERM FOR A
*                                      TERMINAL IN THIS GROUP, THEN
*                                      WE KNOW THAT THERE WILL BE NO
*                                      MORE OPCE'S ON THIS CHAIN
*                                      WHICH NEED TO BE ATTACHED
*                                                            @OY19465*/
         NI    WAITFLG(ROPCAVT),B'11011111'                     03S0423
*                     RWORK5=RZERO; /* SAVE RZERO            @OY19465*/
         LR    RWORK5,RZERO                                     03S0424
*                     RWORK7=RPARM; /* SAVE RPARM            @OY19465*/
         LR    RWORK7,RPARM                                     03S0425
*                     RPARM=RPARM->OCTRMTBL;/* GET TTCIN OF TERMINAL
*                                                            @OY19465*/
         SLR   @11,@11                                          03S0426
         ICM   @11,3,OCTRMTBL(RPARM)                            03S0426
         LR    RPARM,@11                                        03S0426
*                     CALL IEDQTNT; /* GET TRM ENTRY         @OY19465*/
         L     @15,AVTRNMPT(,RAVT)                              03S0427
         BALR  @14,@15                                          03S0427
*                     RTRM=RPARM;   /* SET UP TERMINAL BASE  @OY19465*/
         LR    RTRM,RPARM                                       03S0428
*                     RPARM=OCTRMTBL;/* RESET RPARM FOR REST OF 03S0429
*                                      SEARCH                @OY19465*/
         SLR   RPARM,RPARM                                      03S0429
         ICM   RPARM,3,OCTRMTBL(ROPCE)                          03S0429
*                     DO WHILE(TRMTYPE^=TRMNSDLC);/* LOOP UNTIL FIND
*                                      LINE                  @OY19465*/
         B     @DE00430                                         03S0430
@DL00430 DS    0H                                               03S0431
*                       CALL IEDQTNT;/* GET TTE              @OY19465*/
         L     @15,AVTRNMPT(,RAVT)                              03S0431
         BALR  @14,@15                                          03S0431
*                       CALL COHORTLK;/* GET NEXT RES        @OY19465*/
         L     @15,OPCHORT(,ROPCAVT)                            03S0432
         BALR  @14,@15                                          03S0432
*                       RTRM=RPARM; /* EST ADDRESSABILITY    @OY19465*/
         LR    RTRM,RPARM                                       03S0433
*                       RPARM=RZERO;/* SET UP INTERFACE TO TNT CODE
*                                                            @OY19465*/
         LR    RPARM,RZERO                                      03S0434
*                     END;          /* END OF LOOP TO FIND LINE 03S0435
*                                                            @OY19465*/
@DE00430 LR    @11,RTRM                                         03S0435
         AL    @11,@CF01505                                     03S0435
         CLI   TRMTYPE-11(@11),X'20'                            03S0435
         BNE   @DL00430                                         03S0435
*                     IF RPARM=OCTRMTBL THEN                    03S0436
         SLR   @11,@11                                          03S0436
         ICM   @11,3,OCTRMTBL(ROPCE)                            03S0436
         CR    RPARM,@11                                        03S0436
         BNE   @RF00436                                         03S0436
*                       DO;         /* TERMINAL IS ON CURRENT LINE
*                                                            @OY19465*/
*                         RZERO=RWORK5;/* RESTORE RZERO      @OY19465*/
         LR    RZERO,RWORK5                                     03S0438
*                         RPARM=RWORK7;/* RESTORE RPARM      @OY19465*/
         LR    RPARM,RWORK7                                     03S0439
*                         CALL DLKATT;/* DELINK THE STOPTERM ELEMENT
*                                      AND ATTACH TO CURRENT ELEMENT
*                                                            @OY19465*/
         BAL   @14,DLKATT                                       03S0440
*                       END;        /*                       @OY19465*/
*                     ELSE                                      03S0442
*                       ;           /*                       @OY19465*/
@RF00436 DS    0H                                               03S0443
*                   END;            /*                       @OY19465*/
*                 ELSE                                          03S0444
*                   DO;             /* THIS ELEMENT ON WAITCHN DOES
*                                      NOT NEED TO BE ATTACHED TO THE
*                                      CURRENT ELEMENT THEREFORE THE
*                                      CHAIN POINTERS WILL BE   03S0444
*                                      INCREMENTED IN ORDER TO  03S0444
*                                      CONTINUE THE SEARCH   @OY19465*/
         B     @RC00421                                         03S0444
@RF00421 DS    0H                                               03S0445
*                     RZERO=RPARM;  /* SAVE CURRENT POINTER  @OY19465*/
         LR    RZERO,RPARM                                      03S0445
*                     RPARM=RPARM->OCLINK;/* NEXT ELEMENT TO BE 03S0446
*                                      COMPARED WITH CURRENT OPCE
*                                                            @OY19465*/
         L     RPARM,OCLINK(,RPARM)                             03S0446
*                   END;                                        03S0447
*               END;                                            03S0448
@RC00421 DS    0H                                               03S0449
*           END;                                                03S0449
@RC00415 DS    0H                                               03S0450
*       END;                                                    03S0450
@RC00412 DS    0H                                               03S0450
@DE00411 TM    WAITFLG(ROPCAVT),B'00100000'                     03S0450
         BO    @DL00411                                         03S0450
*       IF RWORKA^=0 THEN                                       03S0451
         LTR   RWORKA,RWORKA                                    03S0451
         BZ    @RF00451                                         03S0451
*         RWORK6=RWORKA;            /* IF RWORKA IS NOT 0 THEN RWORK6
*                                      MUST BE RESTORED BECAUSE 03S0452
*                                      RWORK6 WAS CHANGED BECAUSE
*                                      MORE THAN ONE ELEMENT ON THE
*                                      WAIT CHAIN MAY BE DELINKED AND
*                                      ATTACHED TO THE CURRENT OPCE
*                                                            @OY19465*/
         LR    RWORK6,RWORKA                                    03S0452
*       RWORK7=OCPARM;              /* SAVE RETURN CODE              */
@RF00451 LH    @11,OCPARM(,ROPCE)                               03S0453
         LR    RWORK7,@11                                       03S0453
*       RWORK6->OCUNIT=INVALTIC;    /* INDICATE OPCE ONLY UNIT TO BE
*                                      FREED                         */
         MVC   OCUNIT(4,RWORK6),@CF01775                        03S0454
*       IF OCPARM^=ONE&RWORK6->OCALLFLG=OFF THEN                03S0455
         CH    @11,@CH00061                                     03S0455
         BE    @RF00455                                         03S0455
         TM    OCALLFLG(RWORK6),B'00000100'                     03S0455
         BNZ   @RF00455                                         03S0455
*         DO;                       /* IF NOT A GOOD RESPONSE AND NOT
*                                      AN 'ALL' REQUEST THEN @YM06145*/
*           IF OCPARM=TWO THEN                                  03S0457
         CH    @11,@CH00065                                     03S0457
         BNE   @RF00457                                         03S0457
*             DO;                   /* IF LINE ALREADY STOPPED THEN  */
*               IF RWORK6->OCTOTE=ON THEN                       03S0459
         TM    OCTOTE(RWORK6),B'01000000'                       03S0459
         BNO   @RF00459                                         03S0459
*                 TRMSTOTE=ON;      /* INDICATE TOTE OWN     @YM06145*/
         LR    @11,RTRM                                         03S0460
         AL    @11,@CF02313                                     03S0460
         OI    TRMSTOTE-3(@11),B'00000001'                      03S0460
*               OPCWRKSP(1)=MSG25;  /* SETUP MSG NUMBER              */
@RF00459 MVC   OPCWRKSP(4,ROPCAVT),@CF02218                     03S0461
*               OPCWRKSP(2)=EIGHT;  /* SETUP RETURN CODE             */
         MVC   OPCWRKSP+4(4,ROPCAVT),@CF00045                   03S0462
*             END;                                              03S0463
*           ELSE                                                03S0464
*             DO;                                               03S0464
         B     @RC00457                                         03S0464
@RF00457 DS    0H                                               03S0465
*               OPCWRKSP(1)=MSG199; /* BAD RESPONSE                  */
         MVC   OPCWRKSP(4,ROPCAVT),@CF02224                     03S0465
*               OPCWRKSP(2)=FOUR;   /* SETUP RETURN CODE             */
         MVC   OPCWRKSP+4(4,ROPCAVT),@CF00256                   03S0466
*               RWORK6->OCUNIT=OCUNIT;/* ATTACH UNIT FROM INPUT OPCE
*                                      TO REAL OPCE                  */
         MVC   OCUNIT(4,RWORK6),OCUNIT(ROPCE)                   03S0467
*               OCUNIT=INVALTIC;    /* INDICATE LAST UNIT    @OY20509*/
         MVC   OCUNIT(4,ROPCE),@CF01775                         03S0468
*             END;                                              03S0469
*         END;                                                  03S0470
@RC00457 DS    0H                                               03S0471
*       RPARM=ROPCE;                /* SETUP INPUT OPCE TO BE FREED  */
@RF00455 LR    RPARM,ROPCE                                      03S0471
*       RFY                                                     03S0472
*         RSAVE RSTD;                                           03S0472
*       RSAVE=ADDR(OPCSAVE);        /* SETUP SAVEAREA                */
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          03S0473
*       CALL FREEBUFF;              /* FREE INPUT OPCE AND ATTACHED
*                                      UNIT                          */
         L     @15,OPCFREBF(,ROPCAVT)                           03S0474
         BALR  @14,@15                                          03S0474
*       RFY                                                     03S0475
*         RSAVE UNRSTD;                                         03S0475
*       ROPCE=RWORK6;               /* SETUP ADDRESS OF REAL OPCE    */
         LR    ROPCE,RWORK6                                     03S0476
*       OPCCOPCE=RWORK6;            /* SAVE OPCE ADDR IN OP CONTROL
*                                      AVT                           */
         ST    RWORK6,OPCCOPCE(,ROPCAVT)                        03S0477
*       RPARM=OCTRMTBL;             /* GET TTCIN OF LINE TTE         */
         SLR   RPARM,RPARM                                      03S0478
         ICM   RPARM,3,OCTRMTBL(ROPCE)                          03S0478
*       CALL IEDQTNT;               /* GET LINE TTE                  */
         L     @15,AVTRNMPT(,RAVT)                              03S0479
         BALR  @14,@15                                          03S0479
*       RTRM=RPARM;                 /* SETUP TERM BASE               */
         LR    RTRM,RPARM                                       03S0480
*       OCWTG=OCWTGB;               /* SETUP WHERE-TO-GO FIELD       */
         MVC   OCWTG(2,ROPCE),OCWTGB(ROPCE)                     03S0481
*       IF RWORK7=ONE THEN                                      03S0482
         CH    RWORK7,@CH00061                                  03S0482
         BNE   @RF00482                                         03S0482
*         DO;                       /* WAS IT A GOOD RESPONSE?       */
*           OPCWRKSP(1)=MSG26;      /* SETUP GOOD RESPONSE           */
         MVC   OPCWRKSP(4,ROPCAVT),@CF02220                     03S0484
*           OPCWRKSP(2)=ZERO;       /* SETUP GOOD RETURN CODE        */
         SLR   @11,@11                                          03S0485
         ST    @11,OPCWRKSP+4(,ROPCAVT)                         03S0485
*           IF OCTOTE=ON THEN                                   03S0486
         TM    OCTOTE(ROPCE),B'01000000'                        03S0486
         BNO   @RF00486                                         03S0486
*             TRMSTOTE=ON;          /* INDICATE TOTE OWNERSHIP       */
         LR    @11,RTRM                                         03S0487
         AL    @11,@CF02313                                     03S0487
         OI    TRMSTOTE-3(@11),B'00000001'                      03S0487
*           CALL IPEXIT;            /* PERFORM NORMAL EXIT PROCESSING*/
@RF00486 BAL   @14,IPEXIT                                       03S0488
*         END;                                                  03S0489
*       ELSE                        /* EITHER A BAD RESPONSE OR LINE
*                                      ALREADY STOPPED               */
*         CALL IPERROR;             /* PERFORM ERROR PROCESSING      */
         B     @RC00482                                         03S0490
@RF00482 BAL   @14,IPERROR                                      03S0490
*       RFY                                                     03S0491
*        (RWORK6,                                               03S0491
*         RWORK7) UNRSTD;                                       03S0491
@RC00482 DS    0H                                               03S0492
*     END;                                                      02S0492
*   ELSE                                                        02S0493
*     IF OCPARM^=ZERO THEN                                      02S0493
         B     @RC00402                                         02S0493
@RF00402 LH    @11,OCPARM(,ROPCE)                               02S0493
         LTR   @11,@11                                          02S0493
         BZ    @RF00493                                         02S0493
*       DO;                         /* IF THIS IS AN OPCE WITH A SNA
*                                      RESPONSE, PROCESS IT          */
*                                                               03S0495
*         /***********************************************************/
*         /*                                                         */
*         /* PROCESS.SNAPROC: THIS SEGMENT PROCESSES SNA RESPONSES   */
*         /*                                                         */
*         /***********************************************************/
*                                                               03S0495
*         OCWTG=OCWTGB;             /* SETUP WHERE-TO-GO FIELD       */
         MVC   OCWTG(2,ROPCE),OCWTGB(ROPCE)                     03S0495
*         OPCWRKSP(1)=OCPARM;       /* SETUP MESSAGE NUMBER          */
         ST    @11,OPCWRKSP(,ROPCAVT)                           03S0496
*         RPARM=OCTRMTBL;           /* SETUP TTCIN OF SNA LINE       */
         SLR   RPARM,RPARM                                      03S0497
         ICM   RPARM,3,OCTRMTBL(ROPCE)                          03S0497
*         CALL IEDQTNT;             /* GET LINE TTE                  */
         L     @15,AVTRNMPT(,RAVT)                              03S0498
         BALR  @14,@15                                          03S0498
*         RTRM=RPARM;               /* SETUP TERM BASE               */
         LR    RTRM,RPARM                                       03S0499
*         IF OCPARM=MSG401 THEN                                 03S0500
         CLC   OCPARM(2,ROPCE),@CH02230                         03S0500
         BNE   @RF00500                                         03S0500
*           DO;                     /* IF SNA LINE DEACTIVATED, 03S0501
*                                      PROCESS NORMALLY              */
*             OPCWRKSP(2)=ZERO;     /* SETUP RETURN CODE             */
         SLR   @11,@11                                          03S0502
         ST    @11,OPCWRKSP+4(,ROPCAVT)                         03S0502
*             CALL IPEXIT;          /* DO NORMAL EXIT PROCESSING     */
         BAL   @14,IPEXIT                                       03S0503
*           END;                                                03S0504
*         ELSE                                                  03S0505
*           DO;                                                 03S0505
         B     @RC00500                                         03S0505
@RF00500 DS    0H                                               03S0506
*             IF OCPARM=MSG539 THEN                             03S0506
         CLC   OCPARM(2,ROPCE),@CH02232                         03S0506
         BNE   @RF00506                                         03S0506
*               OPCWRKSP(2)=EIGHT;  /* LINE ALREADY DEACTIVATED      */
         MVC   OPCWRKSP+4(4,ROPCAVT),@CF00045                   03S0507
*             ELSE                                              03S0508
*               OPCWRKSP(2)=FOUR;   /* ELSE SETUP ERROR RETURN CODE  */
         B     @RC00506                                         03S0508
@RF00506 MVC   OPCWRKSP+4(4,ROPCAVT),@CF00256                   03S0508
*             CALL IPERROR;         /* DO ERROR PROCESSING           */
@RC00506 BAL   @14,IPERROR                                      03S0509
*           END;                                                03S0510
*       END;                                                    02S0511
*     ELSE                                                      02S0512
*       FIRSTREQ=ON;                /* ELSE INDICATE ITS A FIRST
*                                      REQUEST                       */
         B     @RC00493                                         02S0512
@RF00493 OI    FIRSTREQ(ROPCAVT),B'10000000'                    02S0512
*   DO WHILE(FIRSTREQ=ON×MORLINES=ON);/* IF THIS IS A FIRST REQUEST
*                                      OR MORE LINES ARE LEFT TO
*                                      PROCESS, PROCESS STOPLINE
*                                      REQUEST                       */
@RC00493 DS    0H                                               02S0513
@RC00402 B     @DE00513                                         02S0513
@DL00513 DS    0H                                               02S0514
*                                                               03S0514
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS.STPLPROC: THIS SEGMENT PROCESSES STOPLINE REQUESTS  */
*     /*                                                             */
*     /***************************************************************/
*                                                               03S0514
*     MORLINES=OFF;                 /* RESET LINE INDICATOR          */
         NI    MORLINES(ROPCAVT),B'10111111'                    03S0514
*     IF FIRSTREQ=ON THEN                                       03S0515
         TM    FIRSTREQ(ROPCAVT),B'10000000'                    03S0515
         BNO   @RF00515                                         03S0515
*       DO;                         /* IF THIS REQUEST IS FIRST ONE,
*                                      SETUP TO ESTABLISH FIRST LINE */
*         FIRSTREQ=OFF;             /* TURN OFF FIRST REQUEST FLAG   */
         NI    FIRSTREQ(ROPCAVT),B'01111111'                    03S0517
*                                                               04S0518
*         /***********************************************************/
*         /*                                                         */
*         /* PROCESS.STPLPROC.FIRSTLN: THIS SEGMENT ESTABLISHES THE  */
*         /* FIRST LINE TO BE STOPPED                                */
*         /*                                                         */
*         /***********************************************************/
*                                                               04S0518
*         IF OCRENTRD=ON THEN                                   04S0518
         TM    OCRENTRD(ROPCE),B'10000000'                      04S0518
         BNO   @RF00518                                         04S0518
*           DO;                     /* IS THIS A RE-ENTRY?           */
*             OCRENTRD=OFF;         /* RE-SET 'RE-ENTERED' INDICATOR */
         NI    OCRENTRD(ROPCE),B'01111111'                      04S0520
*             OCWTG=OCWTGB;         /* SETUP WHERE-TO-GO FIELD       */
         MVC   OCWTG(2,ROPCE),OCWTGB(ROPCE)                     04S0521
*           END;                                                04S0522
*         RPARM=OCTRMTBL;           /* GET TTCIN OF RESOURCE         */
@RF00518 SLR   RPARM,RPARM                                      04S0523
         ICM   RPARM,3,OCTRMTBL(ROPCE)                          04S0523
*         CALL IEDQTNT;             /* FIND TTE OF RESOURCE          */
         L     @15,AVTRNMPT(,RAVT)                              04S0524
         BALR  @14,@15                                          04S0524
*         RTRM=RPARM;               /* SETUP TERM BASE               */
         LR    RTRM,RPARM                                       04S0525
*         IF TRMTYPE=TRMGRP THEN                                04S0526
         LR    @11,RTRM                                         04S0526
         SH    @11,@CH01234                                     04S0526
         CLI   TRMTYPE(@11),X'00'                               04S0526
         BNE   @RF00526                                         04S0526
*           DO;                     /* IS THIS A GROUP TTE           */
*             IF TRMSUBST^=ZERO THEN                            04S0528
         ICM   @09,3,TRMSUBST(@11)                              04S0528
         BZ    @RF00528                                         04S0528
*               DO;                                             04S0529
*                 OCTRMTBL=TRMSUBST;/* SAVE TTCIN OF LINE            */
         SLR   @09,@09                                          04S0530
         ICM   @09,3,TRMSUBST(@11)                              04S0530
         STH   @09,OCTRMTBL(,ROPCE)                             04S0530
*                 RPARM=OCTRMTBL;   /* GET TTCIN FROM OPCE           */
         LR    RPARM,@09                                        04S0531
*                 CALL IEDQTNT;     /* FIND LINE TTE                 */
         L     @15,AVTRNMPT(,RAVT)                              04S0532
         BALR  @14,@15                                          04S0532
*                 RTRM=RPARM;       /* SETUP LINE TTE ADDRESS        */
         LR    RTRM,RPARM                                       04S0533
*               END;                                            04S0534
*             ELSE                                              04S0535
*               DO;                                             04S0535
         B     @RC00528                                         04S0535
@RF00528 DS    0H                                               04S0536
*                 EXITFLG=ON;       /* PREPARE TO EXIT               */
         OI    EXITFLG(ROPCAVT),B'00010000'                     04S0536
*                 OPCMODID=MOD03;   /* SETUP MOD ID                  */
         MVC   OPCMODID(2,ROPCAVT),MOD03                        04S0537
