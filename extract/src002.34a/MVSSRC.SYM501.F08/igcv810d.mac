*   /* POINTED TO BY RWORK6. IT ALSO MERGES THE ATTACHED CHAINS OF   */
*   /* THE ELEMENTS                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0842
*DLKATT:                                                        02S0842
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                            02S0842
DLKATT   DS    0H                                               02S0843
*   RFY                                                         02S0843
*    (RWORK6,                                                   02S0843
*     RWORK7) RSTD;                                             02S0843
*   RZERO->OCLINK=RPARM->OCLINK;    /* TAKE THE ELEMENT OUT OF THE
*                                      CHAIN                 @OY19465*/
         LR    @11,RZERO                                        02S0844
         MVC   OCLINK(4,@11),OCLINK(RPARM)                      02S0844
*   IF RWORK6->OCATTACH=ON THEN                                 02S0845
         TM    OCATTACH(RWORK6),B'00000010'                     02S0845
         BNO   @RF00845                                         02S0845
*     DO;                           /* FIRST ELEMENT HAS ATTACHED
*                                      CHAIN                 @OY19465*/
*       RWORK5=RWORK6->OCELEM;      /* SAVE THE ATTACHED CHAIN  02S0847
*                                                            @OY19465*/
         L     RWORK5,OCELEM(,RWORK6)                           02S0847
*       RWORK6->OCELEM=RPARM;       /* ATTACH ELEMENT TO FIRST  02S0848
*                                      ELEMENT               @OY19465*/
         ST    RPARM,OCELEM(,RWORK6)                            02S0848
*       IF RPARM->OCATTACH^=ON THEN                             02S0849
         TM    OCATTACH(RPARM),B'00000010'                      02S0849
         BO    @RF00849                                         02S0849
*         DO;                       /* SECOND ELEMENT DOES NOT HAVE
*                                      ATTACHED CHAIN        @OY19465*/
*           RPARM->OCELEM=RWORK5;   /* ATTACH FIRST'S CHAIN TO SECOND
*                                      ELEMENT               @OY19465*/
         ST    RWORK5,OCELEM(,RPARM)                            02S0851
*           RPARM->OCATTACH=ON;     /* TURN ON OCATTACH      @OY19465*/
         OI    OCATTACH(RPARM),B'00000010'                      02S0852
*         END;                                                  02S0853
*       ELSE                                                    02S0854
*         DO;                       /* RPARM AND RWORK6 HAVE ATTACHED
*                                      CHAINS                @OY19465*/
         B     @RC00849                                         02S0854
@RF00849 DS    0H                                               02S0855
*           RWORK7=RPARM;           /* SAVE RPARM            @OY19465*/
         LR    RWORK7,RPARM                                     02S0855
*           DO WHILE(RWORK7->OCATTACH=ON);/* SEARCH FOR END OF CHAIN
*                                                            @OY19465*/
         B     @DE00856                                         02S0856
@DL00856 DS    0H                                               02S0857
*             RWORK7=RWORK7->OCELEM;/* GO DOWN ATTACHED CHAIN   02S0857
*                                                            @OY19465*/
         L     RWORK7,OCELEM(,RWORK7)                           02S0857
*           END;                                                02S0858
@DE00856 TM    OCATTACH(RWORK7),B'00000010'                     02S0858
         BO    @DL00856                                         02S0858
*           RWORK7->OCELEM=RWORK5;  /* ATTACH SECOND CHAIN TO END OF
*                                      FIRST CHAIN           @OY19465*/
         ST    RWORK5,OCELEM(,RWORK7)                           02S0859
*           RWORK7->OCATTACH=ON;    /* TURN ON OCATTACH      @OY19465*/
         OI    OCATTACH(RWORK7),B'00000010'                     02S0860
*         END;                                                  02S0861
*     END;                                                      02S0862
*   ELSE                                                        02S0863
*     DO;                                                       02S0863
         B     @RC00845                                         02S0863
@RF00845 DS    0H                                               02S0864
*       RWORK6->OCATTACH=ON;        /* TURN ON OCATTACH      @OY19465*/
         OI    OCATTACH(RWORK6),B'00000010'                     02S0864
*       RWORK6->OCELEM=RPARM;       /* ATTACH SECOND CHAIN   @OY19465*/
         ST    RPARM,OCELEM(,RWORK6)                            02S0865
*     END;                                                      02S0866
*   RPARM->OCLINK=RPARM->OCLINK&&RPARM->OCLINK;/* OY14092            */
@RC00845 L     @11,OCLINK(,RPARM)                               02S0867
         LR    @09,@11                                          02S0867
         XR    @09,@11                                          02S0867
         ST    @09,OCLINK(,RPARM)                               02S0867
*   IF RPARM->OCWTG=MODVE THEN      /* PARTIALLY DONE OY14092        */
         CLC   OCWTG(2,RPARM),MODVE                             02S0868
         BNE   @RF00868                                         02S0868
*     RPARM->OCWTG=MODV8;           /* RESTART OY14092               */
         MVC   OCWTG(2,RPARM),MODV8                             02S0869
*   ELSE                            /* NOT STOPLINE OY14092          */
*     IF RPARM->OCWTG=MODVF THEN    /* PARTIALLY DONE OY14092        */
         B     @RC00868                                         02S0870
@RF00868 CLC   OCWTG(2,RPARM),MODVF                             02S0870
         BNE   @RF00870                                         02S0870
*       RPARM->OCWTG=MODV9;         /* RESTART OY14092               */
         MVC   OCWTG(2,RPARM),MODV9                             02S0871
*     ELSE                          /* OY14092                       */
*       IF RPARM->OCWTG^=MODVG&(RPARM->OCWTG^=MODV8&RPARM->OCWTG^=MODV9
*           ) THEN                  /* ALL STARTED OY14092           */
         B     @RC00870                                         02S0872
@RF00870 CLC   OCWTG(2,RPARM),MODVG                             02S0872
         BE    @RF00872                                         02S0872
         CLC   OCWTG(2,RPARM),MODV8                             02S0872
         BE    @RF00872                                         02S0872
         CLC   OCWTG(2,RPARM),MODV9                             02S0872
         BE    @RF00872                                         02S0872
*         RPARM->OCWTG=MODV8;       /* RESTART OY14092               */
         MVC   OCWTG(2,RPARM),MODV8                             02S0873
*   IF WAITFLG=ON THEN                                          02S0874
@RF00872 DS    0H                                               02S0874
@RC00870 DS    0H                                               02S0874
@RC00868 TM    WAITFLG(ROPCAVT),B'00100000'                     02S0874
         BNO   @RF00874                                         02S0874
*     DO;                           /* AT THIS POINT WE MUST STILL
*                                      CONTINUE TO SEARCH THE WAITV
*                                      CHAIN. THEREFORE, RWORK6 MUST
*                                      BE SET EQUAL TO RPARM IN ORDER
*                                      TO ATTACH THE NEXT ELEMENT IF
*                                      NECESSARY             @OY19465*/
*       RWORKA=RWORK6;                                          02S0876
         LR    RWORKA,RWORK6                                    02S0876
*       RWORK6=RPARM;                                           02S0877
         LR    RWORK6,RPARM                                     02S0877
*     END;                                                      02S0878
*   END;                                                        02S0879
@EL00008 DS    0H                                               02S0879
@EF00008 DS    0H                                               02S0879
@ER00008 BR    @14                                              02S0879
*   RFY                                                         02S0880
*    (RWORK6,                                                   02S0880
*     RWORK7) UNRSTD;                                           02S0880
*   END IGCV810D                                                01S0881
