* /* START OF SPECIFICATIONS ****
*     PROLOGUE
*
*01*  MINI-MODULE-NAME = ISTYCLUS
*
*01*  FUNCTIONS = TO CREATE A SKELETEL DVT FOR VARY (NON SNA DEVICES)
*                 CONTAINING THE PROCESS MODULES NEEDED TO BE USED
*                 ALONG WITH THE NECESSARY QUALIFIERS SO AS THE PROPER
*                 FUNCTIONS ARE SELECTED AS PER DEVICE TYPE USED
*                NOTE -- SEE FUNCT MACRO FOR QUALIFYING ARGUMENTS
*
*01*  MODULE-SIZE = 64-BYTES
*
*01*  PROCESSOR = ASSEMBLER
*
*01*  SYSTEMS-WHERE-USED = OS/VS1, OS/VS2
*
*01*  HOW-USED = AS-FOLLOWS:
*
*                THE SKELETEL DVT WILL BE SCANNED BY ISTOCCMD AND
*                THE CORRECT PROCESSES RETAINED BY MATCHING FUNCTION
*                QUALIFIERS WITH RESOURCE DEFINITION TABLE ENTRY INFO
*
*01*  INPUT-OUTPUT = NONE
*
*01*  CHANGE-ACTIVITY = NONE
*
**** END OF SPECIFICATIONS ***/
ISTYCLUS STRTMODE , CLUSTER SOLICITOR DVT (VS2)
*A-000000-999999                                                XL03HBS
*
ISTCLOBP DVT
         FUNCT PROC=ISTNMCCO  /*  OUTBOUND ROUTER         */
INOSTCTL DVT
         FUNCT PROC=ISTNMCCA  /*  START CONTROL OUTBOUND  */
INOREDRV DVT
         FUNCT PROC=ISTNMCCR  /*  REDRIVE OUTBOUND        */
INOENCTL DVT
         FUNCT PROC=ISTNMCCZ  /*  END CONTROL OUTBOUND    */
INOPURGE DVT
         FUNCT PROC=ISTDCC80  /*  BASIC CTL LAYER PURGE   */
*
         EJECT
ISTCLIBP DVT
         FUNCT PROC=ISTNMCCI  /*  INBOUND ROUTER          */
INISTCTL DVT
         FUNCT PROC=ISTNMCCB  /*  START CONTROL FEEDBACK  */
INSTCTLF DVT
         FUNCT PROC=ISTNMCCF  /*  START CONTROL FAILURE   */
INSTCTLR DVT
         FUNCT PROC=ISTNMCCT  /*  START CONTROL REDRIVE   */
INIREDRV DVT
         FUNCT PROC=ISTNMCCS  /*  REDRIVE FEEDBACK        */
INIPURGE DVT
         FUNCT PROC=ISTDCC81  /*  BASIC CTL LAYER PURGE   */
*
         EJECT
