         LR    GPR04P,NCSPLPTR                                     0632
*           GPR05P=GPR15P; /*PASS PAB PTR*/                        0633
         LR    GPR05P,GPR15P                                       0633
*           CALL ATCLCPTR->ATCAP37->ISTAPC37;                      0634
         L     @12,ATCLCPTR                                        0634
         L     @15,ATCAP37(,@12)                                   0634
         BALR  @14,@15                                             0634
*           GEN(NOPR  0)NOSETS NOREFS;                             0635
         NOPR  0
