         SLR   GPR00P,GPR00P                                       0630
*           GPR15P=ADDR(OCWVPAB->DYPPAB); /*HOLD PAB PTR*/         0631
         L     GPR15P,OCWVPAB(,OCWPTR)                             0631
         LA    GPR15P,DYPPAB(,GPR15P)                              0631
