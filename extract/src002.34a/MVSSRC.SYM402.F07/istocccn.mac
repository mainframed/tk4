*                                                                  0667
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTRPH)  */                           0667
*/* END OF ATBUILD */                                              0667
*             GEN REFS(GPR01P,GPR02F,GPR06F)SETS(RPHWORK);         0667
        STM GPR02F,GPR06F,RPHWORK(GPR01P)
*             GPR04F=OCWCPOST;          /*LOAD RPH PNTR INTO REG 4*/
         L     GPR04F,OCWCPOST(,OCWPTR)                            0668
