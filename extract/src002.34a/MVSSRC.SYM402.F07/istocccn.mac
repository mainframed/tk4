         L     GPR15P,ATCLCPTR                                     0653
*            END; /*LISATCVT*/                                     0654
*        GPR15P=GPR15P->ATCAP33;                                   0655
         L     GPR15P,ATCAP33(,GPR15P)                             0655
*        DCL FMZL0018 BASED(GPR15P) LABEL VALUERANGE(*);           0656
*        CALL FMZL0018;                                            0657
         BALR  @14,@15                                             0657
*        GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(GPR00P,GPR01P
*GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,GPR09P,GPR10P,GPR11P,
*PR12P,GPR13P,GPR14P,GPR15P);                                      0658
         LM GPR00P,GPR15P,RPHWORK(GPR01P)
