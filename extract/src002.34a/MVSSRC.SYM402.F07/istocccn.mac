*               CS (GPR14P,GPR15P,GPR04P->RPHRESMA);               0690
         CS    GPR14P,@15,RPHRESMA(GPR04P)                         0690
*                                       /*COMPARE REG 14 WITH FLAG */
*                                       /*FIELD--IF WT/EXT BIT ON, */
*                                       /*TURN ON POST FLAG        */
*               BC (4,FMZL0028);        /*IF WT/EXT BIT WAS OFF, LOOP*/
         BC    4,FMZL0028                                          0691
*                                       /*TO TEST CONTENTION FLAG    */
*               BC (8,FMZT0028);        /*IF WT/EXT BIT WAS OFF,     */
         BC    8,FMZT0028                                          0692
*                                       /*BRANCH TO BYPASS CODE GEN  */
*             END;                      /*IF POST FLAG IS ON, FALL*/
*                                       /*THRU BOTTOM OF LOOP     */
*             ELSE;                     /*WT/EXT CONTENTION FLAG OFF*/
@RF00686 DS    0H                                                  0695
*             IF GPR04P->RPHTSKID=GPR01P->RPHTSKID                 0695
