         SLR   @12,@12                                             0670
         LR    GPR03F,@12                                          0670
*           DO; /*MODESET  SAVEKEY(GPR06F) KEYADDR(GPR03F) WORKREG(GPR0
*F)*/         RESPECIFY (GPR02F) RESTRICTED;                       0671
*             GPR05F = GPR02F; /*SAVE REGISTER 2*/                 0673
         LR    GPR05F,GPR02F                                       0673
*             IPK; /*SAVE CURRENT PSW KEY*/                        0674
         IPK                                                       0674
*             GPR06F = GPR02F;                                     0675
         LR    GPR06F,GPR02F                                       0675
*             GPR02F = GPR05F; /*RESTORE REGISTER 2*/              0676
         LR    GPR02F,GPR05F                                       0676
*             SPKA (GPR03F); /*SET PSW KEY*/                       0677
         SPKA  0(GPR03F)                                           0677
*             RESPECIFY (GPR02F) UNRESTRICTED;                     0678
*           END; /*MODESET  SAVEKEY(GPR06F) KEYADDR(GPR03F) WORKREG(GPR
*5F)*/        RFY GPR03F UNRSTD;                                   0679
*             GPR05P=GPR04P->RPHMAJCB+GPR04P->RPHPABOF;            0681
         L     GPR05P,RPHMAJCB-1(,GPR04P)                          0681
         LA    GPR05P,0(,GPR05P)                                   0681
         SLR   @10,@10                                             0681
         IC    @10,RPHPABOF(,GPR04P)                               0681
         ALR   GPR05P,@10                                          0681
*                                       /*LOAD REG 5  WITH ADDR OF*/
*                                       /*MAJOR CONTROL BLOCK PLUS*/
*                                       /*OFFSET OF PAB IN BLOCK  */
*             GPR14P=0;                 /*LOAD 0 INTO REG 14      */
         LR    GPR14P,@12                                          0682
