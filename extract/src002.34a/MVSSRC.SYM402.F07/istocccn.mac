         L     @12,OCWDRDTE(,OCWPTR)                               0625
         L     @12,RPRNSNEA(,@12)                                  0625
         L     @12,RCCVPAB(,@12)                                   0625
         ST    @12,OCWVPAB(,OCWPTR)                                0625
