*        GPR00P = CRALMS04;             /*SET PARAM - LOCK LEVEL FLAG*/
         LA    GPR00P,8                                            0650
*        GPR03P = ADDR(GPR04P->CRAL4PTR);                          0651
         LA    GPR03P,CRAL4PTR(,GPR04P)                            0651
