         SLR   GPR15P,GPR15P                                       0683
         BCTR  GPR15P,0                                            0683
*                                       /*ADDRESS                 */
*             CS (GPR14P,GPR15P,GPR04P->RPHRESMA);                 0684
         CS    GPR14P,@15,RPHRESMA(GPR04P)                         0684
*                                       /*COMPARE RESUME ADDRESS  */
*                                       /*WITH 0 AND,IF EQUAL, PUT*/
*                                       /*DUMMY ADDRESS IN CONTROL*/
*                                       /*BLOCK RESUME ADDRESS    */
*             BC (8,FMZT0028);          /*IF EQUAL, BYPASS REMAINDER*/
         BC    8,FMZT0028                                          0685
*                                       /*OF EXPANSION              */
*FMZL0028:    IF GPR04P->RPHWT='1'B THEN/*WT/EXT CONTENTION FLAG ON */
FMZL0028 TM    RPHWT(GPR04P),B'00001000'                           0686
         BNO   @RF00686                                            0686
*             DO;                       /*IN RPH                    */
*               GPR14P=GPR04P->RPHRESMA×RPHWT1;                    0688
         L     GPR14P,RPHRESMA(,GPR04P)                            0688
         O     GPR14P,@CF01392                                     0688
