         B     @RC01112                                            1114
@RF01112 MVI   OCWEC(OCWPTR),X'01'                                 1114
*          OCWRC = '24'X;              /* SYSTEM ERROR               */
@RC01112 MVI   OCWRC(OCWPTR),X'24'                                 1115
*        END;                                                      1116
*C14SVPTR = C14SVPTR-4;                                            1117
@RC01106 DS    0H                                                  1117
