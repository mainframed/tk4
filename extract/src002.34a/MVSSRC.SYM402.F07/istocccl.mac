*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : FMCBIO                                            */
*/*                                                                  */
*/* FUNCTION : FIND THE FMCB FOR THE DRDTE AND CALL CLIOPROC         */
*/*            TO INITIATE THE PURGE OR CLEAR/UNBIND I/O             */
*/*                                                                  */
*/* INPUT: OCWPTR (R4) - PTR TO OPEN CLOSE WORK AREA                 */
*/*                                                                  */
*/* OUTPUT : NONE, BUT IF ERROR, OCWRC AND OCWEC ARE SET             */
*/*                                                                  */
*/* SUBROUTINES CALLED :                                             */
*/*            CLFMCB - FIND FMCB                                    */
*/*            CLIOPROC - PROCESS I/O REQUEST                        */
*/*                                                                  */
*/* ROUTINES CALLED: NONE                                            */
*/*                                                                  */
*/* MACROS USED: NONE                                                */
*/*                                                                  */
*/********************************************************************/
*OCWCID = RPRCOMID;                    /* ORIGINAL CID BYTES @D32CKJY*/
         L     @10,OCWDRDTE(,OCWPTR)                               1154
         L     @10,RPRCOMID(,@10)                                  1154
         ST    @10,OCWCID(,OCWPTR)                                 1154
*CALL CLFMCB;                          /* LOCATE FMCB        @D32CKJY*/
         BAL   @14,CLFMCB                                          1155
*IF OCWFMCB ^= CZERO                   /* FMCB FOUND, AND    @D32CKJY*/
*   & OCWRC = CZERO                    /* NO ERROR           @D32CKJY*/
*  THEN                                /* YES                @D32CKJY*/
         L     @10,OCWFMCB(,OCWPTR)                                1156
         LTR   @10,@10                                             1156
         BZ    @RF01156                                            1156
         CLI   OCWRC(OCWPTR),0                                     1156
         BNE   @RF01156                                            1156
*    DO;                                                           1157
*      OCWCSCAL = CON;                 /* INDICATE FMCB FOUND@D32CKJY*/
         OI    OCWCSCAL(OCWPTR),B'00000001'                        1158
*      IF FMCCPIP = CON                /* ALREADY PURGED?    @D32CKJY*/
*         & OCWCLDEB = COFF            /* NO ABEND RUNNING   @D32CKJY*/
*        THEN                          /* PURGE ALREADY INIT @D32CKJY*/
         TM    FMCCPIP(@10),B'00001000'                            1159
         BNO   @RF01159                                            1159
         TM    OCWCLDEB(OCWPTR),B'10000000'                        1159
         BNZ   @RF01159                                            1159
*          DO;                                                     1160
*            OCWRC = '1C'X;            /* PURGE IN PROCESS   @D32CKJY*/
         MVI   OCWRC(OCWPTR),X'1C'                                 1161
*            OCWEC = '06'X;            /* CLSDST ALR'DY DONE @D32CKJY*/
         MVI   OCWEC(OCWPTR),X'06'                                 1162
*          END;                                                    1163
*        ELSE                          /* NO PURGE IN PROG   @D32CKJY*/
*          DO;                                                     1164
         B     @RC01159                                            1164
@RF01159 DS    0H                                                  1165
*            /* PREPARE FOR I/O SESSION                      @D32CKJY*/
*              /*SEGMENT(CLIOPRC)REQUEST*/DO;GOTO CLIOPRC;CLIOPRC#:END;
         B     CLIOPRC                                             1166
CLIOPRC# DS    0H                                                  1168
*                /* @D32CKJY*/                                     1168
*            IF OCWRC = CZERO          /* DID ERROR OCCUR?   @D32CKJY*/
*              THEN                    /* NO                 @D32CKJY*/
         CLI   OCWRC(OCWPTR),0                                     1168
         BNE   @RF01168                                            1168
*                DO;                                               1169
*                  /* SINCE VOC/RDT LOCKS WERE RELEASED DURING THE   */
*                  /* I/O SESSION, ANOTHER CLSDST MAY HAVE ENTERED   */
*                  /* AND RELEASED THE FMCB.                 @D32CKJY*/
*                  /* CALL CLFMCB TO ENSURE ITS EXISTENCE    @D32CKJY*/
*                                                                  1170
*                  CALL CLFMCB;        /* FIND FMCB AGAIN    @D32CKJY*/
*                                                                  1170
         BAL   @14,CLFMCB                                          1170
*                  /* IF FMCB NOT FOUND, CLSDST WAS ALREADY DONE,    */
*                  /* UNLESS SYTEM ERROR HAD OCCURED.        @D32CKJY*/
*                                                                  1171
*                  IF OCWFMCB = CZERO  /* NO FMCB FOUND, BUT @D32CKJY*/
*                     & OCWRC = CZERO  /* NO ERROR EITHER    @D32CKJY*/
*                    THEN              /* YES                @D32CKJY*/
         L     @10,OCWFMCB(,OCWPTR)                                1171
         LTR   @10,@10                                             1171
         BNZ   @RF01171                                            1171
         CLI   OCWRC(OCWPTR),0                                     1171
         BNE   @RF01171                                            1171
*                      DO;                                         1172
*                        OCWRC='1C'X;    /* ALREADY PURGED   @D32CKJY*/
         MVI   OCWRC(OCWPTR),X'1C'                                 1173
*                        OCWEC='06'X;    /* ALREADY CLSDST   @D32CKJY*/
         MVI   OCWEC(OCWPTR),X'06'                                 1174
*                      END;                                        1175
*                    ELSE;             /* EITHER FMCB FOUND OR SYS ERR
*                                         (OCWRC ALREADY SET)@D32CKJY*/
@RF01171 DS    0H                                                  1177
*                END;                                              1177
*              ELSE;                   /* ERROR IN I/O SESS  @D32CKJY*/
@RF01168 DS    0H                                                  1179
*          END;                                                    1179
*    END;                                                 /* @D32CKJY*/
*  ELSE;                               /* NO FMCB FOUND      @D32CKJY*/
@RF01156 DS    0H                                                  1182
*IF OCWCLOCK = CON                     /* DAF STILL LOCKED?  @D32CKJY*/
*  THEN                                /* YES                @D32CKJY*/
@RC01156 TM    OCWCLOCK(OCWPTR),B'00010000'                        1182
         BNO   @RF01182                                            1182
*    DO;                                                           1183
*      RFY CPARMRG2 RSTD;                                          1184
*      CPARMRG2 = OCWCID;              /* CID BYTES          @D32CKJY*/
*                                                                  1185
         L     CPARMRG2,OCWCID(,OCWPTR)                            1185
*        DO; /* CIDCTL(FINISH)  */                                 1186
*                                                                  1186
*        RFY (EMCR2,EMCR3) RSTD;                                   1187
*          GEN SETS(RPHWORK)                                       1188
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      1188
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH
*          EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                     1189
         SLR   EMCR3,EMCR3                                         1189
*          CALL ATCLCPTR->ATCCDFIN->ISTSDCCR; /*PROCESS REQUEST*/  1190
*                                                                  1190
         L     @10,ATCLCPTR                                        1190
         L     @15,ATCCDFIN(,@10)                                  1190
         BALR  @14,@15                                             1190
*        RFY (EMCR2,EMCR3) UNRSTD;                                 1191
*        END; /*CIDCTL*/              /* RELEASE DAF LOCK   @D32CKJY*/
*      RFY CPARMRG2 UNRSTD;                                        1193
*      OCWCLOCK = COFF;                /* DAF NOW RELEASED   @D32CKJY*/
         NI    OCWCLOCK(OCWPTR),B'11101111'                        1194
*    END;                                                          1195
*  ELSE;                                /* DAF NOT LOCKED    @D32CKJY*/
@RF01182 DS    0H                                                  1197
*  /*SEGMENT(FMCBIO)FINISH*/DO;GOTO FMCBIO#;FMCBIO@:END;           1197
         B     FMCBIO#                                             1198
*               /* @D32CKJY*/                                      1200
*CLFMCB: PROC OPTIONS(NOSAVE,NOSAVEAREA);                 /* @D32CKJY*/
         B     @PB00005                                            1200
CLFMCB   DS    0H                                                  1201
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME: CLFMCB -- LOCATE FMCB                              */
*/*                                                                  */
*/* FUNCTION: LOCATE THE FMCB FOR THE DEST RDTE IN OCWDRDTE VIA      */
*/*           CIDCTL. FMCB IS LOCATED THROUGH EITHER THE FMCB        */
*/*           CHAIN IF NCB IS FOUND, OR DEB IF APPL RDTE IS FOUND.   */
*/*                                                                  */
*/* INPUT: OCWPTR (R4) - PTR TO ISTOCW                               */
*/*                                                                  */
*/* OUTPUT: IF FMCB FOUND, ITS ADDRESS PLACED IN OCWFMCB             */
*/*                                                                  */
*/* SUBROUTINES: DOALTCID - FIND FMCB THRU DEB, IF NEC.              */
*/*                                                                  */
*/* ROUTINES: NONE                                                   */
*/*                                                                  */
*/* MACROS: CIDCTL                                                   */
*/*                                                                  */
*/********************************************************************/
*C14SAVE = CR14PTR;                    /* SAVE RETURN ADDR   @D32CKJY*/
         LA    @10,624                                             1201
         ALR   @10,OCWPTR                                          1201
         L     @05,C14SVPTR(,@10)                                  1201
         ST    CR14PTR,C14SAVE(,@05)                               1201
*C14SVPTR = C14SVPTR + 4;              /* INCR SAVEAREA PTR  @D32CKJY*/
         AL    @05,@CF00075                                        1202
         ST    @05,C14SVPTR(,@10)                                  1202
*RFY (CPARMRG0,CPARMRG2) RSTD;                            /* @D32CKJY*/
*OCWFMCB = CZERO;                      /* CLEAR PTR TO FMCB  @D32CKJY*/
         SLR   @10,@10                                             1204
         ST    @10,OCWFMCB(,OCWPTR)                                1204
*CPARMRG2 = OCWCID;                    /* CID FROM OCW       @D32CKJY*/
*                                                                  1205
         L     CPARMRG2,OCWCID(,OCWPTR)                            1205
*  DO; /* CIDCTL(FIND) LOCK(XCLUSIVE) */                           1206
*                                                                  1206
*  RFY (EMCR2,EMCR3) RSTD;                                         1207
*    GEN SETS(RPHWORK)                                             1208
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      1208
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH
*    EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                           1209
         SLR   EMCR3,EMCR3                                         1209
*    EMCR3 = '02'X; /*INDICATE EXCLUSIVE LOCK*/                    1210
         LA    EMCR3,2                                             1210
*    CALL ATCLCPTR->ATCCDFND->ISTSDCCF; /*PROCESS REQUEST*/        1211
*                                                                  1211
         L     @10,ATCLCPTR                                        1211
         L     @15,ATCCDFND(,@10)                                  1211
         BALR  @14,@15                                             1211
*  RFY (EMCR2,EMCR3) UNRSTD;                                       1212
*  END; /*CIDCTL*/                          /* @D32CKJY*/          1213
*IF CRETCODE = CZERO                   /* NCB FOUND?         @D32CKJY*/
*  THEN                                /* YES--NCB IS IN R0  @D32CKJY*/
         LTR   CRETCODE,CRETCODE                                   1214
         BNZ   @RF01214                                            1214
*    DO;                                                           1215
*      OCWCLOCK = CON;                 /* DAF LOCK HELD      @D32CKJY*/
         OI    OCWCLOCK(OCWPTR),B'00010000'                        1216
*      RFY (FMCBPTR,NCBPTR) RSTD;                         /* @D32CKJY*/
*      NCBPTR = CPARMRG0;              /* PICK NCB ADDRESS   @D32CKJY*/
         LR    NCBPTR,CPARMRG0                                     1218
*      FMCBPTR = NCBFMCBA;             /* FMCB FROM NCB      @D32CKJY*/
         L     FMCBPTR,NCBFMCBA(,NCBPTR)                           1219
*      /* SCAN THRU CHAIN FOR FMCB WITH SAME DEB AS IN OCW   @D32CKJY*/
*      IF OCWCLDEB ^= '1'B                                 /*@ZA17000*/
*         & RPLREQ ^= '21'X                                /*@ZA17000*/
*         & RPLNIB ^= '1'B                                 /*@ZA17000*/
*        THEN DO;                                          /*@ZA17000*/
         TM    OCWCLDEB(OCWPTR),B'10000000'                        1220
         BO    @RF01220                                            1220
         L     @10,OCWRPL(,OCWPTR)                                 1220
         CLI   RPLREQ(@10),33                                      1220
         BE    @RF01220                                            1220
         TM    RPLNIB(@10),B'00000100'                             1220
         BO    @RF01220                                            1220
*      DO WHILE FMCBPTR ^= CZERO       /* EXIT IF NO FMCB    @D32CKJY*/
*              &(OCWACDEB^=FMCBPTR->FMCDEBA  /* OR SAME FMCB @ZA17000*/
*              × RPLSAF ^= FMCBPTR->FMCSAF);               /*@ZA17000*/
         B     @DE01222                                            1222
@DL01222 DS    0H                                                  1223
*        FMCBPTR = FMCBPTR->FMCFMCBA;  /* PICK UP NEXT FMCB  @D32CKJY*/
         L     FMCBPTR,FMCFMCBA(,FMCBPTR)                          1223
*      END;                            /* LOOP UNTIL EITHER FOUND OR
*                                         NO MORE FMCB       @D32CKJY*/
@DE01222 LTR   FMCBPTR,FMCBPTR                                     1224
         BZ    @DC01222                                            1224
         CLC   OCWACDEB(4,OCWPTR),FMCDEBA(FMCBPTR)                 1224
         BNE   @DL01222                                            1224
         L     @10,OCWRPL(,OCWPTR)                                 1224
         CLC   RPLSAF(2,@10),FMCSAF(FMCBPTR)                       1224
         BNE   @DL01222                                            1224
@DC01222 DS    0H                                                  1225
*      END;                            /* LOOP UNTIL EITHER FOUND OR */
*      ELSE DO;                        /*                    @ZA20812*/
         B     @RC01220                                            1226
@RF01220 DS    0H                                                  1227
*      DO WHILE FMCBPTR ^= CZERO       /* EXIT IF NO FMCB    @ZA20812*/
*              & OCWACDEB^=FMCBPTR->FMCDEBA; /* OR SAME FMCB @ZA20812*/
         B     @DE01227                                            1227
@DL01227 DS    0H                                                  1228
*        FMCBPTR = FMCBPTR->FMCFMCBA;  /* PICK UP NEXT FMCB  @ZA20812*/
         L     FMCBPTR,FMCFMCBA(,FMCBPTR)                          1228
*                                      /* NO MORE FMCB       @ZA20812*/
*      END;                                                /*@ZA20812*/
@DE01227 LTR   FMCBPTR,FMCBPTR                                     1229
         BZ    @DC01227                                            1229
         CLC   OCWACDEB(4,OCWPTR),FMCDEBA(FMCBPTR)                 1229
         BNE   @DL01227                                            1229
@DC01227 DS    0H                                                  1230
*      END;                            /*                    @ZA20812*/
*      OCWFMCB = FMCBPTR;              /* STORE FMCB IN OCW  @D32CKJY*/
@RC01220 ST    FMCBPTR,OCWFMCB(,OCWPTR)                            1231
*      OCWNCB = NCBPTR;                /* STORE NCB IN OCW   @D32CKJY*/
         ST    NCBPTR,OCWNCB(,OCWPTR)                              1232
*      IF FMCBPTR ^= CZERO             /* FMCB FOUND?        @D32CKJY*/
*        THEN                          /* YES                @D32CKJY*/
         LTR   FMCBPTR,FMCBPTR                                     1233
         BZ    @RF01233                                            1233
*          DO;                                                     1234
*            OCWSAF = FMCBPTR->FMCSAF; /* SAF FROM FMCB      @D32CKJY*/
         LH    @10,FMCSAF(,FMCBPTR)                                1235
         N     @10,@CF06204                                        1235
         STH   @10,OCWSAF(,OCWPTR)                                 1235
*            OCWDAF = NCBDAF;          /* DAF FROM NCB       @D32CKJY*/
         LH    @10,NCBDAF(,NCBPTR)                                 1236
         N     @10,@CF06204                                        1236
         STH   @10,OCWDAF(,OCWPTR)                                 1236
*          END;                                                    1237
*        ELSE;                         /* NO FMCB FOUND      @D32CKJY*/
@RF01233 DS    0H                                                  1239
*    END;                                                          1239
*  ELSE                                /* NCB NOT FOUND      @D32CKJY*/
*     /*SEGMENT(DALTCID)REQUEST*/DO;GOTO DALTCID;DALTCID#:END;       /*
*GET FMCB VIA DEB   @D32CKJY*/                                     1240
         B     @RC01214                                            1240
@RF01214 DS    0H                                                  1241
         B     DALTCID                                             1241
DALTCID# DS    0H                                                  1243
*RFY (CPARMRG0,CPARMRG2,FMCBPTR,NCBPTR) UNRSTD;                    1243
@RC01214 DS    0H                                                  1244
*C14SVPTR = C14SVPTR - 4;              /* ADJUST SA PTR      @D32CKJY*/
