*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : DOPASSCK -- CHECK STATUS OF CLSDST PASS   @D32CKJY*/
*/*                                                                  */
*/* FUNCTION : CHECK ALL CONDITIONS OF BOTH SOURCE AND TARGET RDTES  */
*/*            REQUIRED OF THE CONNECTION SERVICES(C.S) TO MINIMIZE  */
*/*            CHANCE OF ERROR FROM C.S. SINCE THE FMCB WILL HAVE    */
*/*            ALREADY BEEN PURGED IF. C.S. FAILS            @D32CKJY*/
*/*                                                                  */
*/* INPUT : OCWPTR (R4) - PTR TO OPEN/CLOSE WORK AREA                */
*/*                OCWDRDTE = ADDRESS OF SOURCE RDTE         @D32CKJY*/
*/*                CSPDEST = ADDRESS OF TARGET RDTE          @D32CKJY*/
*/* OUTPUT : NONE, BUT IF ERROR,                                     */
*/*            OCWRC - MAJOR RETURN CODE                             */
*/*            OCWEC - MINOR RETURN CODE                             */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* ROUTINES CALLED : NONE                                           */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*OCWRC = '08'X;                        /* ASSUME ERROR(TEMP) @D32CKJY*/
         MVI   OCWRC(OCWPTR),X'08'                                 1124
*IF RPRVRYPD = CON                     /* VARY PENDING ON THIS RDTE? */
*  THEN                                /* YES                @D32CKJY*/
         L     @10,OCWDRDTE(,OCWPTR)                               1125
         TM    RPRVRYPD(@10),B'00000010'                           1125
         BNO   @RF01125                                            1125
*    OCWEC = '4A'X;                    /* VARY PENDING ERROR @D32CKJY*/
         MVI   OCWEC(OCWPTR),X'4A'                                 1126
*  ELSE                                /* NO VARY PENDING    @D32CKJY*/
*    IF RAPAPASS ^= CON                /* AUTHORIZED FOR PASS?       */
*      THEN                            /* NO                 @D32CKJY*/
         B     @RC01125                                            1127
@RF01125 L     @10,OCWARDTE(,OCWPTR)                               1127
         TM    RAPAPASS(@10),B'00100000'                           1127
         BO    @RF01127                                            1127
*        OCWEC = '60'X;                /* PASS NOT AUTHORZED @D32CKJY*/
         MVI   OCWEC(OCWPTR),X'60'                                 1128
*      ELSE                            /* PASS AUTHORIZED    @D32CKJY*/
*        IF CSPDEST->RPRACT^=CON       /* TARGET APPL ACTIVE @D32CKJY*/
*          THEN                        /* NO                 @D32CKJY*/
         B     @RC01127                                            1129
@RF01127 L     @10,CSPDEST+224(,OCWPTR)                            1129
         TM    RPRACT(@10),B'10000000'                             1129
         BO    @RF01129                                            1129
*            OCWEC = '4A'X;            /* TARGET IS INACTIVE @D32CKJY*/
         MVI   OCWEC(OCWPTR),X'4A'                                 1130
*          ELSE                        /* TARGET IS ACTIVE   @D32CKJY*/
*            IF CSPDEST->RAPABLGN=CON  /* TARGET APPL BLOCKING     1131
*                                         LOGONS             @D32CKJY*/
*              THEN                    /* YES--CAN'T PASS    @D32CKJY*/
         B     @RC01129                                            1131
@RF01129 L     @10,CSPDEST+224(,OCWPTR)                            1131
         TM    RAPABLGN(@10),B'10000000'                           1131
         BNO   @RF01131                                            1131
*                OCWEC = '55'X;        /* TGT CAN'T ACCEPT   @D32CKJY*/
         MVI   OCWEC(OCWPTR),X'55'                                 1132
*              ELSE                    /* TARGET APPL CLEAR  @D32CKJY*/
*                IF CSPDEST->RAPAQUI=CON /* TGT APPL QUIES'G @D32CKJY*/
*                  THEN                /* YES--CAN'T PASS    @D32CKJY*/
         B     @RC01131                                            1133
@RF01131 L     @10,CSPDEST+224(,OCWPTR)                            1133
         TM    RAPAQUI(@10),B'01000000'                            1133
         BNO   @RF01133                                            1133
*                    OCWEC='55'X;      /* TGT IS QUIESCING   @D32CKJY*/
         MVI   OCWEC(OCWPTR),X'55'                                 1134
*                  ELSE                /* TGT NOT QUIESCING  @D32CKJY*/
*                    /* ACE QUEUE IS EMPTY IF RCPACETR POINTS BACK   */
*                    /* TO ITSELF                            @D32CKJY*/
*                    IF RCPACETR=ADDR(RCPACETR) /* QUE EMPTY @D32DCKJY*
*                      THEN            /* YES--CAN'T PASS    @D32CKJY*/
         B     @RC01133                                            1135
@RF01133 L     @10,OCWDRDTE(,OCWPTR)                               1135
         LA    @05,RCPACETR(,@10)                                  1135
         C     @05,RCPACETR(,@10)                                  1135
         BNE   @RF01135                                            1135
*                        OCWEC='61'X;   /* PASSER NOT OWNER  @D32CKJY*/
         MVI   OCWEC(OCWPTR),X'61'                                 1136
*                      ELSE            /* PASSER OWNS RSRC   @D32CKJY*/
*                        DO;                                       1137
         B     @RC01135                                            1137
@RF01135 DS    0H                                                  1138
*                          RFY ACEPTR RSTD;                        1138
*                          ACEPTR = RCPACETR; /* PICK UP ACE @D32CKJY*/
         L     @10,OCWDRDTE(,OCWPTR)                               1139
         L     ACEPTR,RCPACETR(,@10)                               1139
*                          /* SEARCH FOR LAST ACE IN QUEUE   @D32CKJY*/
*                          DO WHILE ACEOBJQ^=ADDR(RCPACETR);       1140
         B     @DE01140                                            1140
@DL01140 DS    0H                                                  1141
*                            ACEPTR=ACEOBJQ; /* GET NEXT ACE @D32CKJY*/
         L     ACEPTR,ACEOBJQ(,ACEPTR)                             1141
*                          END;                                    1142
@DE01140 L     @10,OCWDRDTE(,OCWPTR)                               1142
         LA    @10,RCPACETR(,@10)                                  1142
         C     @10,ACEOBJQ(,ACEPTR)                                1142
         BNE   @DL01140                                            1142
*                          IF ACESUBJ^=OCWARDTE /* MATCHED?  @D32CKJY*/
*                            THEN               /* NO        @D32CKJY*/
         CLC   ACESUBJ(4,ACEPTR),OCWARDTE(OCWPTR)                  1143
         BE    @RF01143                                            1143
*                              OCWEC='61'X;     /* RSRC NOT OWNED    */
         MVI   OCWEC(OCWPTR),X'61'                                 1144
*                            ELSE           /* NO ERRORS     @D32CKJY*/
*                              OCWRC=CZERO; /* RESET TO ZERO @D32CKJY*/
         B     @RC01143                                            1145
@RF01143 MVI   OCWRC(OCWPTR),X'00'                                 1145
*                        END;                                      1146
@RC01143 DS    0H                                                  1147
*RFY ACEPTR UNRSTD;                                                1147
@RC01135 DS    0H                                                  1147
@RC01133 DS    0H                                                  1147
@RC01131 DS    0H                                                  1147
@RC01129 DS    0H                                                  1147
@RC01127 DS    0H                                                  1147
@RC01125 DS    0H                                                  1148
*  /*SEGMENT(DPASSCK)FINISH*/DO;GOTO DPASSCK#;DPASSCK@:END;        1148
         B     DPASSCK#                                            1149
DPASSCK@ DS    0H                                                  1151
*  /*SEGMENT(FMCBIO)START*/DO;GOTO FMCBIO@;FMCBIO:END;             1151
         B     FMCBIO@                                             1152
FMCBIO   DS    0H                                                  1154
