         L     GPR02P,OCWDRDTE(,OCWPTR)                            0344
         LA    GPR02P,RPRRLOCK(,GPR02P)                            0344
*        GPR04P = GPR01P->RPHCRR;       /*SET PARAM - ISTCRA PTR*/ 0345
         L     GPR04P,RPHCRR(,GPR01P)                              0345
*        GPR00P = CRALMS04;             /*SET PARAM - LOCK LEVEL FLAG*/
         LA    GPR00P,8                                            0346
*        GPR03P = ADDR(GPR04P->CRAL4PTR);                          0347
         LA    GPR03P,CRAL4PTR(,GPR04P)                            0347
*                                       /*SET PARAM - LOCK LEVEL PTR*/
*        GPR15P=ATCLCPTR->ATCAP36;      /*ADDRESS OF LOCK ROUTINE*/
         L     @10,ATCLCPTR                                        0348
         L     GPR15P,ATCAP36(,@10)                                0348
*        DCL FMZL0009 BASED(GPR15P) LABEL VALUERANGE(*);           0349
