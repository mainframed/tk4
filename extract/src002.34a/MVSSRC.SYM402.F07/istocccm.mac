*      /*       12 OR OTHERS = STORAGE CANNOT BE ALLOCATED           */
*      IF CRETCODE =< C8               /* STORAGE ALLOCATED? @X04BHJY*/
*        THEN                          /* YES                @X04BHJY*/
         LA    @14,8                                               0528
         CR    CRETCODE,@14                                        0528
         BH    @RF00528                                            0528
