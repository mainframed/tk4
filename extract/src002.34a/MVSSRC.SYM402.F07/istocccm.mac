*                  DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */   0544
*                    GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/   0545
         L     GPR15P,ATCLCPTR                                     0545
*                  END; /*LISATCVT*/                               0546
*                    RESPECIFY GPR00F RSTD;                        0547
*                    GPR00F=0;     /*SET PARAMETER REGISTER TO 0*/ 0548
         SLR   GPR00F,GPR00F                                       0548
*                    GPR15P=GPR15P->ATCAP31;                       0549
         L     GPR15P,ATCAP31(,GPR15P)                             0549
*                    DCL FMZL0072 BASED(GPR15P) LABEL VALUERANGE(*);
*                    CALL FMZL0072;                                0551
         BALR  @14,@15                                             0551
*                    RESPECIFY (GPR00F,GPR15P) UNRSTD;             0552
*                  END; /***** END OF TPEXIT  *****/               0553
*LHAB0067:;        GPR01P->RPHRESMA = 0;                           0554
LHAB0067 DS    0H                                                  0555
         SLR   @08,@08                                             0555
         ST    @08,RPHRESMA(,GPR01P)                               0555
*                  RFY (GPR00P) RSTD;                              0556
*LHAA0067:;        GEN (LM    GPR00P,GPR15P,RPHWORK(GPR01P))       0557
LHAA0067 DS    0H                                                  0558
*                  REFS(RPHWORK,GPR01P) SETS(GPR00P,GPR15P);       0558
         LM    GPR00P,GPR15P,RPHWORK(GPR01P)
*                  RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;       0559
*                  END; /*****END OF TPWAIT *****/             /* WAIT
*OR COMPLETION        */                                           0560
*              ELSE;                   /* ALLOCATED DONE     @X04BHJY*/
@RF00530 DS    0H                                                  0562
*            OCWLCPBA=RPHSRPRM;        /* SAVE NMLPB PTR     D303013 */
         L     @14,RPHSRPRM(,RPHPTR)                               0562
         ST    @14,OCWLCPBA(,OCWPTR)                               0562
*            LCPTYPE=NMLPBTYP;         /* NMLPB TYPE         D303013 */
         MVI   LCPTYPE(@14),X'27'                                  0563
