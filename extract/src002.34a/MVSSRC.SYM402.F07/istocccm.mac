*/*                                                                  */
*/* ROUTINE NAME : DOENDCTL -- ISSUE END CONTROL                     */
*/*                                                                  */
*/* FUNCTION : FOR A CLUSTER AND A PORT, AN NMLPB INDICATING END     */
*/*            CONTROL REQUEST IS TPQUEUED TO THE CLOP.              */
*/*                                                                  */
*/* INPUT : OCWPTR (R4) - PTR TO OPEN/CLOSE WORK AREA                */
*/*                                                                  */
*/* OUTPUT : NONE                                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED :                                             */
*/*            DOLCPB - OBTAIN NMLPB FOR END CONTROL SESSION         */
*/*                                                                  */
*/* ROUTINES CALLED : NONE                                           */
*/*                                                                  */
*/* MACROS USED :                                                    */
*/*            CIDCTL FIND - LOCK DAF                                */
*/*            CIDCTL FINISH - UNLOCK DAF                            */
*/*            TPQUE NORM - QUEUE NMLPB TO CLOP                      */
*/*                                                                  */
*/********************************************************************/
*RFY CWORKRGA RSTD;                                       /* @D32CKJY*/
*CWORKRGA = CZERO;                     /* CLEAR REG 10       @D32CKJY*/
         SLR   @14,@14                                             0393
         LR    CWORKRGA,@14                                        0393
*IF OCWNCB = CZERO                     /* IS THERE A DNCB            */
*  THEN                                /* NO                 @ZM00768*/
         C     @14,OCWNCB(,OCWPTR)                                 0394
         BNE   @RF00394                                            0394
*    IF RPRNSNEA->RPRENTRY=RPRENTCL    /* IF CLUSTER AND     @ZM00768*/
*        & OCWSTCTL = CON              /* STRT CTL STARTED   @ZM00768*/
*      THEN                                                        0395
         L     @14,OCWDRDTE(,OCWPTR)                               0395
         L     @14,RPRNSNEA(,@14)                                  0395
         CLI   RPRENTRY(@14),X'70'                                 0395
         BNE   @RF00395                                            0395
         TM    OCWSTCTL(OCWPTR),B'00100000'                        0395
         BNO   @RF00395                                            0395
*        DO;                                                       0396
*          OCWCID=RPRNSNEA->RPRCOMID; /* GET CLUSTER DAF D303080 */
         L     @05,RPRCOMID(,@14)                                  0397
         ST    @05,OCWCID(,OCWPTR)                                 0397
*          CWORKRGA=RPRNSNEA; /* GET CLUSTER RDTE FOR END CTRL */  0398
         LR    CWORKRGA,@14                                        0398
*        END;                                              /*@DM08626*/
*     ELSE;                                                /*@DM08626*/
@RF00395 DS    0H                                                  0401
*  ELSE                                /* DNCB IS IN OCW     @D32CKJY*/
*    IF OCWNCB->NCBRDTE->RPRENTRY=RPRENTLN                         0401
*       & OCWNCB->NCBRDTE->RLNPORT=CON /* PORT SOLICITOR?            */
*      THEN                                                        0401
         B     @RC00394                                            0401
@RF00394 L     @14,OCWNCB(,OCWPTR)                                 0401
         L     @14,NCBRDTE(,@14)                                   0401
         CLI   RPRENTRY(@14),X'50'                                 0401
         BNE   @RF00401                                            0401
         TM    RLNPORT(@14),B'00010000'                            0401
         BNO   @RF00401                                            0401
*        DO;                                                       0402
*          OCWCID=OCWNCB->NCBRDTE->RPRCOMID; /*GET PORT DAF D303080*/
         L     @14,RPRCOMID(,@14)                                  0403
         ST    @14,OCWCID(,OCWPTR)                                 0403
*          CWORKRGA=OCWDRDTE; /*GET PORT RDTE FOR ENDCTL     @DM08626*/
         L     CWORKRGA,OCWDRDTE(,OCWPTR)                          0404
*        END;                                              /*@DM08626*/
*      ELSE;                                               /*@DM08626*/
@RF00401 DS    0H                                                  0407
*/* IF DRDTE NOT FOUND, REG 10 WILL STILL BE ZERO            @D32CKJY*/
*IF CWORKRGA = CZERO                   /* DO ENDCTL?         @D32CKJY*/
*  THEN;                               /* NO                 @D32CKJY*/
@RC00394 LTR   CWORKRGA,CWORKRGA                                   0407
         BZ    @RT00407                                            0407
*  ELSE                                /* YES                @DM08626*/
*    DO;                                                           0409
*        /*SEGMENT(DOLCPB)REQUEST*/DO;GOTO DOLCPB;DOLCPB#:END;     /* G
*T NMLPB FOR ENDCTL       */                                       0410
         B     DOLCPB                                              0411
DOLCPB#  DS    0H                                                  0413
*      IF OCWLCPBA = CZERO             /* NMLPB OBTAINED?     D303013*/
*        THEN;                         /* NO                  D303013*/
         L     @14,OCWLCPBA(,OCWPTR)                               0413
         LTR   @14,@14                                             0413
         BZ    @RT00413                                            0413
*        ELSE                          /* YES                 D303013*/
*          DO;                                                     0415
*            RFY (CPARMRG2,CWORKRG9) RSTD;                         0416
*        OCWSAF=ATCVTMID × '0001'X;  /*MAJOR NODE ID D303080*/     0417
         L     @14,OCWACVT(,OCWPTR)                                0417
         LA    @08,1                                               0417
         LH    @14,ATCVTMID(,@14)                                  0417
         N     @14,@CF04211                                        0417
         OR    @08,@14                                             0417
         STH   @08,OCWSAF(,OCWPTR)                                 0417
*        CPARMRG2=OCWCID;  /* CID FOR CIDCTL D303080 */            0418
         L     CPARMRG2,OCWCID(,OCWPTR)                            0418
*        CWORKRG9=CPARMRG2;                 /* SAVE CID */         0419
*                                                                  0419
         LR    CWORKRG9,CPARMRG2                                   0419
*         DO; /* CIDCTL(FIND)  */                                  0420
*                                                                  0420
*         RFY (EMCR2,EMCR3) RSTD;                                  0421
*           GEN SETS(RPHWORK)                                      0422
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      0422
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH
*           EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                    0423
         SLR   EMCR3,EMCR3                                         0423
*           EMCR3 = '01'X; /*INDICATE SHARED LOCK*/                0424
         LA    EMCR3,1                                             0424
*           CALL ATCLCPTR->ATCCDFND->ISTSDCCF; /*PROCESS REQUEST*/ 0425
*                                                                  0425
         L     @08,ATCLCPTR                                        0425
         L     @15,ATCCDFND(,@08)                                  0425
         BALR  @14,@15                                             0425
*         RFY (EMCR2,EMCR3) UNRSTD;                                0426
*         END; /*CIDCTL*/                    /* GET DAF LOCK */    0427
*        IF CRETCODE=CZERO                  /* IF LOCK OBTAINED */ 0428
*          THEN                                                    0428
         LTR   CRETCODE,CRETCODE                                   0428
         BNZ   @RF00428                                            0428
*            DO;                                                   0429
*              IF OCWPGDON = CON       /* FMCB PURGED?       @DC41340*/
*                THEN                  /* YES, CONTINUE      @DC41340*/
         TM    OCWPGDON(OCWPTR),B'00010000'                        0430
         BNO   @RF00430                                            0430
*                  OCWPGDON = COFF;    /* TURN IT OFF        @DC41340*/
         NI    OCWPGDON(OCWPTR),B'11101111'                        0431
*                ELSE                  /* NO, CHK IF PORT    @DC41340*/
*                  IF OCWNCB^=CZERO    /* NCB EXISTS, AND    @DC41340*/
*                     & OCWNCB->NCBRDTE->RPRENTRY=RPRENTLN         0432
*                     & OCWNCB->NCBRDTE->RLNPORT=CON /* PORT @DC41340*/
*                    THEN              /* YES                @DC41340*/
         B     @RC00430                                            0432
@RF00430 L     @14,OCWNCB(,OCWPTR)                                 0432
         LTR   @14,@14                                             0432
         BZ    @RF00432                                            0432
         L     @14,NCBRDTE(,@14)                                   0432
         CLI   RPRENTRY(@14),X'50'                                 0432
         BNE   @RF00432                                            0432
         TM    RLNPORT(@14),B'00010000'                            0432
         BNO   @RF00432                                            0432
*                      NMECRST = CON;  /* NOTIFY ENDCTL TO RESET     */
         L     @14,OCWLCPBA(,OCWPTR)                               0433
         OI    NMECRST(@14),B'00010000'                            0433
*                    ELSE;             /* NOT PORT. SOL.     @DC41340*/
@RF00432 DS    0H                                                  0435
*              NMRDTEA=CWORKRGA;   /* RDTE ADDR FOR ENDCTL   @DM08626*/
@RC00430 L     @14,OCWLCPBA(,OCWPTR)                               0435
         ST    CWORKRGA,NMRDTEA(,@14)                              0435
*              NMFNCTCD=NMFCD04;   /* SPECIFY ENDCTL */            0436
         MVI   NMFNCTCD(@14),X'04'                                 0436
*              LCPCID=CWORKRG9; /* SET UP CID IN LCPB */           0437
         ST    CWORKRG9,LCPCID(,@14)                               0437
*              /* LCPRPHA, ET AL, ALREADY ZEROD OUT BY               */
*              /* DOLCPB SUBRTN.            DA04711, DM13003 @D31JKJY*/
*              LCPNXLCW=ADDR(NMLCW2);  /* LCCW ADDR  DM13003 @D31JKJY*/
         LA    @08,NMLCW2(,@14)                                    0438
         ST    @08,LCPNXLCW(,@14)                                  0438
*              CWORKRGA=CPARMRG2; /* FMCB BASE */                  0439
         LR    CWORKRGA,CPARMRG2                                   0439
*              /* TPQUE TO CLOP */                                 0440
*                                                                  0440
*/*  TPQUE (OCWLCPBA,CWORKRGA->FMCPAB1) NORM */                    0440
*                                                                  0440
*               DO;                                                0440
*               RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;     0441
*               GEN REFS(GPR01P) SETS(RPHWORK)                     0442
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      0442
         STM GPR02P,GPR08P,RPHWORK(GPR01P)
*               GPR00P=0;                                          0443
         SLR   GPR00P,GPR00P                                       0443
*               GPR15P=ADDR(CWORKRGA->FMCPAB1); /*HOLD PAB PTR*/   0444
         LA    GPR15P,FMCPAB1(,CWORKRGA)                           0444
*               GPR04P=OCWLCPBA; /*WORK ELE PTR*/                  0445
         L     GPR04P,OCWLCPBA(,OCWPTR)                            0445
*               GPR05P=GPR15P; /*PASS PAB PTR*/                    0446
         LR    GPR05P,GPR15P                                       0446
*               CALL ATCLCPTR->ATCAP37->ISTAPC37;                  0447
         L     @08,ATCLCPTR                                        0447
         L     @15,ATCAP37(,@08)                                   0447
         BALR  @14,@15                                             0447
*               GEN(NOPR  0)NOSETS NOREFS;                         0448
         NOPR  0
*               RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) UNRSTD;   0449
*               END; /**** END OF TPQUE GENERATION ****/ /* @X04BHJY*/
*              OCWLCPBA=CZERO;  /* CLEAR NMLPB POINTER D303013 */  0451
         SLR   @14,@14                                             0451
         ST    @14,OCWLCPBA(,OCWPTR)                               0451
*              CPARMRG2=CWORKRG9;           /* GET CID */          0452
*                                                                  0452
         LR    CPARMRG2,CWORKRG9                                   0452
*               DO; /* CIDCTL(FINISH)  */                          0453
*                                                                  0453
*               RFY (EMCR2,EMCR3) RSTD;                            0454
*                 GEN SETS(RPHWORK)                                0455
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      0455
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH
*                 EMCR3 = '00'X; /*CLEAR EXTRA BITS*/              0456
         SLR   EMCR3,EMCR3                                         0456
*                 CALL ATCLCPTR->ATCCDFIN->ISTSDCCR; /*PROCESS REQUEST*
*                                                                  0457
         L     @08,ATCLCPTR                                        0457
         L     @15,ATCCDFIN(,@08)                                  0457
         BALR  @14,@15                                             0457
*               RFY (EMCR2,EMCR3) UNRSTD;                          0458
*               END; /*CIDCTL*/            /* RELEASE LOCK */      0459
*            END;                                                  0460
*          ELSE;                                                   0461
@RF00428 DS    0H                                                  0462
*        RFY (CPARMRG2,CWORKRG9,CWORKRGA) RSTD;                    0462
*      END;                      /* D303013 */                     0463
*    END;                                                          0464
@RT00413 DS    0H                                                  0465
*  /*SEGMENT(ENDCTL)FINISH*/DO;GOTO ENDCTL#;ENDCTL@:END;           0465
@RT00407 DS    0H                                                  0466
         B     ENDCTL#                                             0466
ENDCTL@  DS    0H                                                  0468
*               /* @D32CKJY*/                                      0468
*  /*SEGMENT(DOCLOSE)START*/DO;GOTO DOCLOSE@;DOCLOSE:END;          0468
         B     DOCLOSE@                                            0469
DOCLOSE  DS    0H                                                  0471
