         L     GPR03F,FMZA0048                                     0518
*          GPR04F=19;                                              0519
         LA    GPR04F,19                                           0519
*          GPR15P=GPR15P->ATCSMRQ;                                 0520
         L     GPR15P,ATCSMRQ(,GPR15P)                             0520
*          DCL FMZL0048 BASED(GPR15P) LABEL VALUERANGE(*);         0521
*          CALL FMZL0048;                                          0522
*                                                                  0522
         BALR  @14,@15                                             0522
*       RFY (GPR13P) RSTD;                                         0523
*          GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);      0524
*                                                                  0524
           LM GPR02P,GPR13P,RPHWORK(GPR01P)
*       RFY (GPR13P) UNRSTD;                                       0525
