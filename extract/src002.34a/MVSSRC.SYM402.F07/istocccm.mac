*        GPR15P=GPR15P->ATCAP33;                                   0366
         L     GPR15P,ATCAP33(,GPR15P)                             0366
*        DCL FMZL0014 BASED(GPR15P) LABEL VALUERANGE(*);           0367
*        CALL FMZL0014;                                            0368
         BALR  @14,@15                                             0368
*        GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(GPR00P,GPR01P
*GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,GPR09P,GPR10P,GPR11P,
