         MVI   LCPLNGTH(@14),X'64'                                 0564
*          END;                        /* D303013 */               0565
*        ELSE                          /* ALLOCATION FAILED  @X04BHJY*/
*          DO;                                                     0566
         B     @RC00528                                            0566
@RF00528 DS    0H                                                  0567
*            OCWRC='24'X;              /* SYSTEM ERROR CODE  @X04BHJY*/
         MVI   OCWRC(OCWPTR),X'24'                                 0567
*            OCWEC='01'X;              /* STG MGMT FAILURE   @X04BHJY*/
         MVI   OCWEC(OCWPTR),X'01'                                 0568
*          END;                                                    0569
*    END;                                                          0570
*  ELSE                                /* NMPLB ALREADY HERE @X04BHJY*/
