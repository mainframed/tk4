         TITLE 'IGCFG10D - DEACTIVATE RESPONSE PROCESSOR               *
                        '
IGCFG10D CSECT ,                                                01S0001
@PROLOG  BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                                   /*
IGCFG10D IEDHJN AFTERHJN,HJN
*                                                               01S0330
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZATION                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0330
*   ROPCAVT=RPARM;                  /* SET POINTER TO OP. CTL. AVT   */
         LR    ROPCAVT,RPARM                                    01S0330
*   ROPCE=OPCCOPCE;                 /* SET POINTER TO CURRENT OPCE   */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         01S0331
*   RAVT=OPCAVTPT;                  /* SET POINTER TO TCAM AVT       */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          01S0332
*   RSAVE=ADDR(OPCSAVE);            /* SET POINTER TO SAVE AREA      */
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          01S0333
*   RWRKAREA=OPCWORK;               /* OP. CTL. WORKAREA BASE - USED
*                                      FOR TEMPORARY STORAGE         */
*                                                               01S0334
         L     RWRKAREA,OPCWORK(,ROPCAVT)                       01S0334
*   /*****************************************************************/
*   /*                                                               */
*   /* BASE COMPILER GENERATED DSECT (CREATED BY REENTRANT OPTION)   */
*   /* FOR TEMPORARY STORAGE ON WORK AREA BASE.                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0335
*   GEN REFS(RWRKAREA);                                         01S0335
*                        /*
         USING @DATD,RWRKAREA    */
*                                                               01S0336
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS THE MAIN PROCEDURE. THE CURRENT OPCE IS CHECKED TO SEE IF*/
*   /* IT IS A CLEANUP REQUEST ELEMENT OR A SESSION TERM. OPCE.      */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0336
*   FLAGS=''B;                      /* CLEAR THE FLAGS FIELD @YM05682*/
         MVI   FLAGS,X'00'                                      01S0336
*   IF OCSWITCH=CLNSWVAL THEN       /* IF THIS IS REALLY A CLEANUP
*                                      REQ ELMT                      */
         CLI   OCSWITCH(ROPCE),X'00'                            01S0337
         BNE   @RF00337                                         01S0337
*     DO;                                                       01S0338
*                                                               02S0339
*       /*************************************************************/
*       /*                                                           */
*       /* THIS SEGMENT PROCESSES THE CLEANUP REQUEST ELEMENT WHICH  */
*       /* HAS BEEN RETURNED BY RESIDENT CLEANUP.                    */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0339
*       CUROPCE->OCUNIT=ROPCE;      /* SET DEACT OPCE TO POINT TO
*                                      ELMT                          */
         L     @14,CUROPCE(,ROPCE)                              02S0339
         ST    ROPCE,OCUNIT(,@14)                               02S0339
*       RTRM=CURTRM;                /* SET TTE BASE                  */
         L     RTRM,CURTRM(,ROPCE)                              02S0340
*       ROPCE=CUROPCE;              /* DEACT OPCE                    */
         LR    ROPCE,@14                                        02S0341
*       OPCCOPCE=ROPCE;             /* SET DEACT OPCE AS CURRENT OPCE*/
         ST    ROPCE,OPCCOPCE(,ROPCAVT)                         02S0342
*       RPARM=ROPCE;                /* POINT TO CURRENT OPCE         */
         LR    RPARM,ROPCE                                      02S0343
*       RZERO=TOFFINPG;             /* AND SET VALUE INDICATING THAT
*                                      THE IN-PROGRESS               */
         SLR   RZERO,RZERO                                      02S0344
*       CALL PROGRTNE;              /* BITS SHOULD BE TURNED OFF IN
*                                      HIERARCHY                     */
         L     @15,OPCPGCK(,ROPCAVT)                            02S0345
         BALR  @14,@15                                          02S0345
*       IF OCWTG^=CLNOCMOD THEN     /* IF DEACT OPCE WAS NOT FROM OP
*                                      CTL CLEANUP                   */
         CLC   OCWTG(2,ROPCE),CLNOCMOD                          02S0346
         BE    @RF00346                                         02S0346
*         DO;                       /* IT WAS A DEACT NCP FROM  02S0347
*                                      ANOTHER SOURCE                */
*           RPARM=OCUNIT;           /* GET ADDR OF REQUEST ELMT      */
         L     RPARM,OCUNIT(,ROPCE)                             02S0348
*           RPARM->CURLINK=PRUINTIC;/* INSURE INVALID TIC    @YM06502*/
         MVC   CURLINK(4,RPARM),@CF01223                        02S0349
*           CALL FREEBUFF;          /* AND FREE IT                   */
         L     @15,OPCFREBF(,ROPCAVT)                           02S0350
         BALR  @14,@15                                          02S0350
*           OCUNIT=PRUINTIC;        /* SET INVALID TIC IN OPCE       */
         MVC   OCUNIT(4,ROPCE),@CF01223                         02S0351
*         END;                                                  02S0352
*       ELSE                                                    02S0353
*         ;                         /* DEACT WAS FROM OP CTL CLEANUP
*                                      AND ELMT WILL BE USED WHEN
*                                      RETURN TO OP CTL CLEANUP      */
@RF00346 DS    0H                                               02S0354
*       DO;                         /*                       @YM07730*/
*         CALL CMNDTERM;            /* TERMINATE DEACTIVATION AND SET
*                                      TO XCTL TO TO OP CTL CLEANUP  */
         BAL   @14,CMNDTERM                                     02S0355
*         OCELMPRI=PRICULCB;        /* SET PRIORITY FOR IGCC110D TO
*                                      RECOGNIZE RETURN FROM    02S0356
*                                      DEACTIVATE            @YM07730*/
         MVI   OCELMPRI(ROPCE),X'C0'                            02S0356
*       END;                        /*                       @YM07730*/
*     END;                                                      01S0358
*   ELSE                                                        01S0359
*     IF OCSWITCH=OCSESTRM THEN     /* IF THIS IS A TERMINATE SESSION
*                                      RESPONSE OPCE                 */
         B     @RC00337                                         01S0359
@RF00337 CLI   OCSWITCH(ROPCE),X'E6'                            01S0359
         BNE   @RF00359                                         01S0359
*       DO;                                                     01S0360
*         NEXTTCIN=OCTRMTBL;        /* SET PARAMETER FOR REFORMAT
*                                      INDICATING RESOURCE FOR NEW
*                                      OPCE                          */
         LH    NEXTTCIN,OCTRMTBL(,ROPCE)                        01S0361
         N     NEXTTCIN,@CF01464                                01S0361
*         CALL REFORMAT;            /* REFORMAT OPCE INTO DEACTIVATE
*                                      LU OPCE                       */
         BAL   @14,REFORMAT                                     01S0362
*       END;                                                    01S0363
*     ELSE                          /* THIS IS A STOPLINE OR    01S0364
*                                      DEACTIVATE RESPONSE OPCE      */
*       DO;                                                     01S0364
         B     @RC00359                                         01S0364
@RF00359 DS    0H                                               01S0365
*         IF OCSWITCH=OCVAROFF&OCPARM=BADVARY THEN/* IF VARY OFF
*                                      UNSUCCESSFUL          @YM05710*/
         CLI   OCSWITCH(ROPCE),X'D2'                            02S0365
         BNE   @RF00365                                         02S0365
         CLC   OCPARM(2,ROPCE),@CH01432                         02S0365
         BNE   @RF00365                                         02S0365
*           DO;                     /*                       @YM05710*/
*             OCALSTAT=''B;         /* CLEAR ALSTAT          @YM05710*/
         MVI   OCALSTAT(ROPCE),X'00'                            02S0367
*             OCPARM=''B;           /* CLEAR PARM FIELD      @YM05710*/
         SLR   @14,@14                                          02S0368
         STH   @14,OCPARM(,ROPCE)                               02S0368
*             OCWTG=DACRPMOD;       /* SET TO RETURN         @YM05710*/
         MVC   OCWTG(2,ROPCE),DACRPMOD                          02S0369
*             OCRESP=OFF;           /* TURN OFF RESP FLAG    @YM05710*/
         NI    OCRESP(ROPCE),B'11111110'                        02S0370
*             OCACTREQ=ON;          /* REQUEST FROM A/D      @YM05710*/
         OI    OCACTREQ(ROPCE),B'00001000'                      02S0371
*             OPCMODID=VOFFMOD;     /* SET TO XCTL TO STOP LINE MOD
*                                                            @YM05710*/
         MVC   OPCMODID(2,ROPCAVT),VOFFMOD                      02S0372
*           END;                    /*                       @YM05710*/
*         ELSE                      /*                       @YM05710*/
*           IF OCTRMTBL=OCPTRSAV->OCTRMTBL THEN/* IF THIS RESPONSE
*                                      OPCE'S RESOURCE IS THE SAME
*                                      RESOURCE AS IN THE ORIG. 02S0374
*                                      DEACTIVATE CMND.              */
         B     @RC00365                                         02S0374
@RF00365 L     @14,OCPTRSAV(,ROPCE)                             02S0374
         CLC   OCTRMTBL(2,ROPCE),OCTRMTBL(@14)                  02S0374
         BNE   @RF00374                                         02S0374
*             DO;                                               02S0375
*               RPARM=OCTRMTBL;     /* SET TO TTCIN OF RESOURCE      */
         LH    RPARM,OCTRMTBL(,ROPCE)                           02S0376
         N     RPARM,@CF01464                                   02S0376
*               CALL IEDQTNT;       /* AND OBTAIN TTE OF RESOURCE    */
         L     @15,AVTRNMPT(,RAVT)                              02S0377
         BALR  @14,@15                                          02S0377
*               RTRM=RPARM;         /* COPY TTE ADDR                 */
         LR    RTRM,RPARM                                       02S0378
*               IF TRMTYPE=TRMLNCP THEN/* IF ORIG (DEACT) OPCE IS FOR
*                                      THE NCP               @YM07656*/
         LR    @14,RTRM                                         02S0379
         AL    @14,@CF01018                                     02S0379
         CLI   TRMTYPE-11(@14),X'0F'                            02S0379
         BNE   @RF00379                                         02S0379
*                 DO;                                           02S0380
*                                                               02S0380
*                   /*************************************************/
*                   /*                                               */
*                   /* ORIGINAL OPCE WAS FOR A DEACTIVATE OF THE NCP.*/
*                   /* BUILD A CLEANUP REQUEST ELEMENT AND POST IT TO*/
*                   /* RESIDENT CLEANUP. RESIDENT CLEANUP WILL POST  */
*                   /* IT BACK TO AVTOPCOB TO BE ROUTED TO THIS      */
*                   /* MODULE WHEN ITS PROCESSING IS FINISHED.       */
*                   /*                                               */
*                   /*************************************************/
*                                                               02S0381
*                   RPARM=OCUNIT;   /* FREE ATTACHED                 */
         L     RPARM,OCUNIT(,ROPCE)                             02S0381
*                   CALL FREEBUFF;  /* RU                            */
         L     @15,OPCFREBF(,ROPCAVT)                           02S0382
         BALR  @14,@15                                          02S0382
*                   RZERO=1;        /* DECREMENT OPCADGOT BY ONE EVEN
*                                      THOUGH DACTPU OPCE            */
         LA    RZERO,1                                          02S0383
*                   CALL DECRCT;    /* IS NOT FREED SINCE THE REQUEST
*                                      ELEMENT WILL BE BUILT IN IT
*                                      AND BE USED BY CLEANUP        */
         L     @15,OPCDECR(,ROPCAVT)                            02S0384
         BALR  @14,@15                                          02S0384
*                   RPARM=OCPTRSAV; /* SAVE POINTER TO ORIG (DEACT)
*                                      OPCE                          */
         L     RPARM,OCPTRSAV(,ROPCE)                           02S0385
*                   IEDCUREQ=''B;   /* CLEAR OPCE FOR USE AS CLEANUP
*                                      REQUEST ELMT                  */
         XC    IEDCUREQ(44,ROPCE),IEDCUREQ(ROPCE)               02S0386
*                   CURQCB=SAVTCLUP;/* SET QCB ADDR OF POST          */
         L     @14,AVTSAVTP(,RAVT)                              02S0387
         MVC   CURQCB(3,ROPCE),SAVTCLUP+1(@14)                  02S0387
*                   CURPRI=PRICULCB;/* SET POST PRIORITY             */
         MVI   CURPRI(ROPCE),X'C0'                              02S0388
*                   CURLINK=PRUINTIC;/* SET INVALID TIC IN LINK FIELD
*                                                            @YM06502*/
         MVC   CURLINK(4,ROPCE),@CF01223                        02S0389
*                   CURTTCIN=RPARM->OCTRMTBL;/* SET RESOURCE TTCIN
*                                      FROM ORIG OPCE                */
         LH    @14,OCTRMTBL(,RPARM)                             02S0390
         N     @14,@CF01464                                     02S0390
         STH   @14,CURTTCIN(,ROPCE)                             02S0390
*                   CURTRM=RTRM;    /* SET TTE ADDR                  */
         ST    RTRM,CURTRM(,ROPCE)                              02S0391
*                   CUROPCE=RPARM;  /* SET POINTER TO ORIG (DEACT)
*                                      OPCE                          */
         ST    RPARM,CUROPCE(,ROPCE)                            02S0392
*                   CURSWCH=CURCUREQ;/* SET SWITCH VALUE INDICATING
*                                      CLEANUP                       */
         MVI   CURSWCH(ROPCE),X'39'                             02S0393
*                   OPCAQWD1=ROPCE; /* SET ADDR OF ELMT IN FIRST WORD
*                                      OF AQCTL PARM LIST            */
         ST    ROPCE,OPCAQWD1(,ROPCAVT)                         02S0394
*                   OPCAQWD2=ROPCE; /* SET ALSO IN SECOND WORD       */
         ST    ROPCE,OPCAQWD2(,ROPCAVT)                         02S0395
*                   OPCAQREQ=POSTCODE;/* SET CODE INDICATING SINGLE
*                                      ELMT POST                     */
         MVI   OPCAQREQ(ROPCAVT),X'0C'                          02S0396
*                   OPCAQRQ2=POSTEOL;/* SET END OF PARM LIST    02S0397
*                                      INDICATOR                     */
         MVI   OPCAQRQ2(ROPCAVT),X'80'                          02S0397
*                   RPARM=ADDR(OPCAQCTL);/* POINT TO AQCTL PARM LIST */
         LA    RPARM,OPCAQCTL(,ROPCAVT)                         02S0398
*                   SVC(102);       /* POST                          */
         SVC   102                                              02S0399
*                   OPCMODID=TO00;  /* SET TO XCTL TO OP CTL CONTROL
*                                      MODULE                        */
         MVC   OPCMODID(2,ROPCAVT),TO00                         02S0400
*                 END;                                          02S0401
*               ELSE                                            02S0402
*                 DO;                                           02S0402
*                                                               02S0402
         B     @RC00379                                         02S0402
@RF00379 DS    0H                                               02S0403
*                   /*************************************************/
*                   /*                                               */
*                   /* DEACTIVATION IS COMPLETE. CLEANUP AND         */
*                   /* TERMINATE COMMAND PROCESSING                  */
*                   /*                                               */
*                   /*************************************************/
*                                                               02S0403
*                                                               03S0403
*                   /*************************************************/
*                   /*                                               */
*                   /* THIS SEGMENT CLEANS UP AT THE END OF          */
*                   /* DEACTIVATE PROCESSING.                        */
*                   /*                                               */
*                   /*************************************************/
*                                                               03S0403
*                   OPCCOPCE=OCPTRSAV;/* MAKE THE ORIG. OPCE THE
*                                      CURRENT OPCE                  */
         L     @14,OCPTRSAV(,ROPCE)                             03S0403
         ST    @14,OPCCOPCE(,ROPCAVT)                           03S0403
*                   RPARM=ROPCE;    /* POINT TO RSP. OPCE            */
         LR    RPARM,ROPCE                                      03S0404
*                   CALL FREEBUFF;  /* FREE OPCE AND RU (IF ANY)     */
         L     @15,OPCFREBF(,ROPCAVT)                           03S0405
         BALR  @14,@15                                          03S0405
*                   ROPCE=OPCCOPCE; /* POINT TO ORIG. OPCE           */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         03S0406
*                   RZERO=1;        /* DECREMENT OPCADGOT ....       */
         LA    RZERO,1                                          03S0407
*                   CALL DECRCT;    /* .... BY ONE                   */
         L     @15,OPCDECR(,ROPCAVT)                            03S0408
         BALR  @14,@15                                          03S0408
*                   RPARM=ROPCE;    /* POINT TO ORIGINAL OPCE        */
         LR    RPARM,ROPCE                                      03S0409
*                   RZERO=TOFFINPG; /* TURN OFF ....                 */
         SLR   RZERO,RZERO                                      03S0410
*                   CALL PROGRTNE;  /* .... IN-PROG BITS IN TTE'S    */
         L     @15,OPCPGCK(,ROPCAVT)                            03S0411
         BALR  @14,@15                                          03S0411
*                   CALL CMNDTERM;                              02S0412
         BAL   @14,CMNDTERM                                     02S0412
*                 END;                                          02S0413
*             END;                                              02S0414
*           ELSE                                                02S0415
*             DO;                                               02S0415
*                                                               02S0415
         B     @RC00374                                         02S0415
@RF00374 DS    0H                                               02S0416
*               /*****************************************************/
*               /*                                                   */
*               /* DEACTIVATION IS NOT COMPLETE. NEXT CHECK TO SEE IF*/
*               /* THIS IS THE LAST DEACTIVATE FOR A GIVEN           */
*               /* HIERARCHICAL LEVEL.                               */
*               /*                                                   */
*               /*****************************************************/
*                                                               02S0416
*                                                               03S0416
*               /*****************************************************/
*               /*                                                   */
*               /* THIS SEGMENT CHECKS TO SEE IF THE RESOURCE        */
*               /* ASSOCIATED WITH THE RESPONSE OPCE IS THE LAST     */
*               /* RESOURCE TO BE DEACTIVATED ON THIS HIERARCHICAL   */
*               /* LEVEL.                                            */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0416
*               RPARM=OCTRMTBL;     /* GET ADDR OF RESOURCE....      */
         LH    RPARM,OCTRMTBL(,ROPCE)                           03S0416
         N     RPARM,@CF01464                                   03S0416
*               CALL IEDQTNT;       /* AND FIND HIGHER       @YM05670*/
         L     @15,AVTRNMPT(,RAVT)                              03S0417
         BALR  @14,@15                                          03S0417
*               CALL COHORTLK;      /* .... RESOURCE                 */
         L     @15,OPCHORT(,ROPCAVT)                            03S0418
         BALR  @14,@15                                          03S0418
*               NXTHIRSC=RPARM;     /* COPY TTE ADDR.                */
         LR    NXTHIRSC,RPARM                                   03S0419
*               NEXTTCIN=RZERO;     /* COPY TTCIN OF RESOURCE        */
         LR    NEXTTCIN,RZERO                                   03S0420
*               RTRM=NXTHIRSC;      /* SET TTE PTR.                  */
         LR    RTRM,NXTHIRSC                                    03S0421
*               RPARM=TRMSUBST;     /* OBTAIN TTE ADDR. OF FIRST ....*/
         LR    @14,RTRM                                         03S0422
         BCTR  @14,0                                            03S0422
         BCTR  @14,0                                            03S0422
         LH    RPARM,TRMSUBST-12(,@14)                          03S0422
         N     RPARM,@CF01464                                   03S0422
*               CALL IEDQTNT;       /* .... RESOURCE ON LEVEL        */
         L     @15,AVTRNMPT(,RAVT)                              03S0423
         BALR  @14,@15                                          03S0423
*               RTRM=RPARM;         /* SET TTE BASE                  */
         LR    RTRM,RPARM                                       03S0424
*               DO WHILE LOOPFLG=OFF;/* LOOP THRU ALL RESOURCES AT
*                                      HIERARICAL LEVEL UNLESS AN
*                                      ACTIVE RESOURCE IS FOUND 03S0425
*                                                            @YM05682*/
         B     @DE00425                                         03S0425
@DL00425 DS    0H                                               03S0426
*                 IF RTRM=NXTHIRSC THEN/* IF BACK TO HIGHER RESOURCE
*                                                            @YM05682*/
         CR    RTRM,NXTHIRSC                                    03S0426
         BNE   @RF00426                                         03S0426
*                   LOOPFLG=ON;     /* SET TO GET OUT        @YM05682*/
         OI    LOOPFLG,B'10000000'                              03S0427
*                 ELSE                                          03S0428
*                   IF TRMLINE=ON THEN/* ELSE IF NOT HIGHER RESOURCE
*                                      THEN CHECK IF IS LINE @YM05682*/
         B     @RC00426                                         03S0428
@RF00426 TM    TRMLINE(RTRM),B'10000000'                        03S0428
         BNO   @RF00428                                         03S0428
*                     IF TRMRSACT=ON THEN/* AND IS ACTIVE    @YM05682*/
         LR    @14,RTRM                                         03S0429
         AL    @14,@CF01468                                     03S0429
         TM    TRMRSACT-2(@14),B'10000000'                      03S0429
         BNO   @RF00429                                         03S0429
*                       LOOPFLG=ON; /* SET TO GET OUT        @YM05682*/
         OI    LOOPFLG,B'10000000'                              03S0430
*                     ELSE          /* ELSE GET GO TO NEXT   @YM05682*/
*                       DO;         /*                       @YM05682*/
         B     @RC00429                                         03S0431
@RF00429 DS    0H                                               03S0432
*                         RPARM=TRMCOHRT;/* OBTAIN TTE ADDR. OF NEXT
*                                      ....                          */
         LR    @14,RTRM                                         03S0432
         AL    @14,@CF01469                                     03S0432
         LH    RPARM,TRMCOHRT-4(,@14)                           03S0432
         N     RPARM,@CF01464                                   03S0432
*                         CALL IEDQTNT;/* .... RESOURCE ON LEVEL     */
         L     @15,AVTRNMPT(,RAVT)                              03S0433
         BALR  @14,@15                                          03S0433
*                         RTRM=RPARM;/* SET TTE BASE                 */
         LR    RTRM,RPARM                                       03S0434
*                       END;        /*                       @YM05682*/
*                   ELSE            /* ELSE CHECK FOR PU OR LU STILL
*                                      ACTIVE                @YM05682*/
*                     IF TRMSIBPT^=0 THEN/* IF SIB NOT FREED THEN
*                                      ACTIVE                @YM05682*/
         B     @RC00428                                         03S0436
@RF00428 LR    @14,RTRM                                         03S0436
         AL    @14,@CF01470                                     03S0436
         ICM   @11,7,TRMSIBPT-7(@14)                            03S0436
         BZ    @RF00436                                         03S0436
*                       LOOPFLG=ON; /* SET TO GET OUT        @YM05682*/
         OI    LOOPFLG,B'10000000'                              03S0437
*                     ELSE          /*                       @YM05682*/
*                       DO;         /*                       @YM05682*/
         B     @RC00436                                         03S0438
@RF00436 DS    0H                                               03S0439
*                         RPARM=TRMCOHRT;/* OBTAIN TTE ADDR. OF NEXT
*                                      ....                  @YM05682*/
         LR    @14,RTRM                                         03S0439
         AL    @14,@CF01469                                     03S0439
         LH    RPARM,TRMCOHRT-4(,@14)                           03S0439
         N     RPARM,@CF01464                                   03S0439
*                         CALL IEDQTNT;/* .... RESOURCE ON LEVEL
*                                                            @YM05682*/
         L     @15,AVTRNMPT(,RAVT)                              03S0440
         BALR  @14,@15                                          03S0440
*                         RTRM=RPARM;/* SET TTE BASE         @YM05682*/
         LR    RTRM,RPARM                                       03S0441
*                       END;        /*                       @YM05682*/
*               END;                                            03S0443
*                                                               03S0443
@RC00436 DS    0H                                               03S0443
@RC00428 DS    0H                                               03S0443
@RC00426 DS    0H                                               03S0443
@DE00425 TM    LOOPFLG,B'10000000'                              03S0443
         BZ    @DL00425                                         03S0443
*               /*****************************************************/
*               /*                                                   */
*               /* IF NO RESOURCES STILL ACTIVE ON THIS LEVEL        */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0444
*               IF RTRM=NXTHIRSC THEN/* IF TTE PTR. EQUAL NEXT  03S0444
*                                      HIGHEST RESOURCE              */
         CR    RTRM,NXTHIRSC                                    03S0444
         BNE   @RF00444                                         03S0444
*                 DO;                                           03S0445
*                   IF OCSWITCH=OCVAROFF&OCATTACH=ON THEN/* IF  03S0446
*                                      STOPLINE RESPONSE OPCE WITH
*                                      ATTACHED OPCE'S       @OX19284*/
         CLI   OCSWITCH(ROPCE),X'D2'                            03S0446
         BNE   @RF00446                                         03S0446
         TM    OCATTACH(ROPCE),B'00000010'                      03S0446
         BNO   @RF00446                                         03S0446
*                     DO;                                       03S0447
*                                                               03S0447
*                       /*********************************************/
*                       /*                                           */
*                       /* THIS IS A STOPLINE RESPONSE OPCE WITH     */
*                       /* ATTACHED OPCE'S. ATTACH THESE OPCE'S TO   */
*                       /* THE ORIG. OPCE'S ATTACH CHAIN.    @OX19284*/
*                       /*                                           */
*                       /*********************************************/
*                                                               03S0448
*                                                               04S0448
*                       /*********************************************/
*                       /*                                           */
*                       /* THIS SEGMENT MOVES THE ATTACHED OPCE'S    */
*                       /* ASSOCIATED WITH THE STOPLINE RESPONSE OPCE*/
*                       /* AND ATTACHES THEM TO THE ORIG. OPCE'S     */
*                       /* ATTACH CHAIN.                             */
*                       /*                                           */
*                       /*********************************************/
*                                                               04S0448
*                       ROPCE=OCPTRSAV;/* POINT TO ORIGINAL OPCE     */
         L     ROPCE,OCPTRSAV(,ROPCE)                           04S0448
*                       DO WHILE(OCATTACH=ON);/* DO WHILE NOT AT END
*                                      OF ATTACH CHAIN               */
