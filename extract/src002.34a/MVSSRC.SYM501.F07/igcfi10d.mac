         L     @15,AVTRNMPT(,RAVT)                              04S0546
         BALR  @14,@15                                          04S0546
*             RTRM=RPARM;           /* OR                            */
         LR    RTRM,RPARM                                       04S0547
*             IF TRMPREF^=ONTRMDIAL^=ONNATODAF^=RRTRSID THEN/* IF
*                                      NON-DIAL RESOURCE OR ALREADY
*                                      ON ANOTHER NCP THEN CLEAR RVT
