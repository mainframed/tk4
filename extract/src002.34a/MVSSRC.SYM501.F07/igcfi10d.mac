         TITLE 'IGCFI10D - RVT AND HIERARCHY BUILD                     *
                        '
IGCFI10D CSECT ,                                                01S0001
@PROLOG  BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                                   /*
IGCFI10D IEDHJN AFTERHJN,HJN
*                                      ADDRESSABILITY TO NAT ENTRY   */
*   ROPCAVT=RPARM;                  /* SET POINTER TO OP. CTL. AVT   */
         LR    ROPCAVT,RPARM                                    01S0383
*   RAVT=OPCAVTPT;                  /* SET POINTER TO TCAM AVT       */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          01S0384
*   ROPCE=OPCCOPCE;                 /* SET POINTER TO CURRENT OPCE   */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         01S0385
*   RSAVE=ADDR(OPCSAVE);            /* SET POINTER TO SAVE AREA      */
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          01S0386
*   RDATA=OPCWORK;                  /* SET POINTER TO OP.CTL. WORK
*                                      AREA                          */
         L     RDATA,OPCWORK(,ROPCAVT)                          01S0387
*   FLAGS1='00'X;                   /* CLEAR FLAG BYTE 1             */
         MVI   FLAGS1(RDATA),X'00'                              01S0388
*   OCADNCP=ON;                     /* FLAG AS F ACT,NCP             */
         LR    @14,ROPCE                                        01S0389
         AL    @14,@CF02814                                     01S0389
         OI    OCADNCP-2(@14),B'00010000'                       01S0389
*   OPCADGOT=OPCADGOT+1;            /* INCREMENT OPCADGOT            */
         LA    @14,1                                            01S0390
         SLR   @11,@11                                          01S0390
         ICM   @11,3,OPCADGOT(ROPCAVT)                          01S0390
         ALR   @14,@11                                          01S0390
         STH   @14,OPCADGOT(,ROPCAVT)                           01S0390
*                                                               02S0391
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT CREATES AN ENTRY IN THE SUBAREA TABLE FOR THE    */
*   /* CURRENT NCP AND CREATES ENTRIES IN THE NETWORK ADDRESS TABLE  */
*   /* FOR THE NCP AND EACH OF ITS RESOURCES.                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0391
*   RFY                                                         02S0391
*     IEDSATD BASED(RWORK);         /* SUBAREA TABLE ENTRY           */
*   RFY                                                         02S0392
*     RWORK RSTD;                                               02S0392
*   RTRM=NCPTTEA;                   /* SET POINTER TO NCP TTE        */
         L     RTRM,NCPTTEA(,RDATA)                             02S0393
*   DCBPTR=QCBDCBAD;                /* SET POINTER TO DCB            */
         ICM   @14,7,TRMDESTQ(RTRM)                             02S0394
         SLR   @11,@11                                          02S0394
         ICM   @11,7,QCBDCBAD(@14)                              02S0394
         ST    @11,DCBPTR(,RDATA)                               02S0394
*   RWORK=RSCPTR->RRTRSID;          /* LOAD NETWORK ADDR. OF FIRST
*                                      RESOURCE IN RRT TO OBTAIN
*                                                             SUBAREA*/
         L     @14,RSCPTR(,RDATA)                               02S0395
         SLR   RWORK,RWORK                                      02S0395
         ICM   RWORK,3,RRTRSID(@14)                             02S0395
*   ISOSUBA=(8*LENGTH(RRTRSID))-SAVTSUBL;/* OBTAIN NO. OF BITS TO
*                                      SHIFT TO ISOLATE SUBAREA      */
         L     @09,AVTSAVTP(,RAVT)                              02S0396
         LA    @07,16                                           02S0396
         SLR   @06,@06                                          02S0396
         IC    @06,SAVTSUBL(,@09)                               02S0396
         SLR   @07,@06                                          02S0396
         STC   @07,ISOSUBA(,RDATA)                              02S0396
*   SRL(RWORK,ISOSUBA);             /* ISOLATE SUBAREA               */
         SRL   RWORK,0(@07)                                     02S0397
*   RWORK=SAVTSAT+((RWORK-ADJX)*LENGTH(IEDSATD));/* OBTAIN PTR. TO
*                                      SAT ENTRY                     */
         LR    @07,RWORK                                        02S0398
         BCTR  @07,0                                            02S0398
         SLA   @07,3                                            02S0398
         AL    @07,SAVTSAT(,@09)                                02S0398
         LR    RWORK,@07                                        02S0398
*   LCBPTR=DCBIOBAD+DCBEIOBX-LCBPRFSZ;/* SET LCB ADDR. IN ENTRY      */
         AH    @11,@CH01349                                     02S0399
         SLR   @07,@07                                          02S0399
         IC    @07,DCBEIOBX(,@11)                               02S0399
         AL    @07,DCBIOBAD(,@11)                               02S0399
         SH    @07,@CH01853                                     02S0399
         ST    @07,LCBPTR(,RDATA)                               02S0399
*   SATRNLCB=LCBPTR;                /* SAVE LCB IN SAT ENTRY         */
         ST    @07,SATRNLCB(,RWORK)                             02S0400
*   SATRNRVT=RVTPTR;                /* SET RVT ADDR. IN ENTRY        */
         L     @11,RVTPTR(,RDATA)                               02S0401
         ST    @11,SATRNRVT(,RWORK)                             02S0401
*   ENTCTR=ENTRYNUM;                /* SET COUNTER FOR LOOP TO SET UP
*                                      NAT ENTRIES FOR THIS NCP AND
*                                      ITS RESOURCES TO NO. OF RRT
*                                      ENTRIES                       */
         MVC   ENTCTR(4,RDATA),ENTRYNUM(RDATA)                  02S0402
*   ENTADDRR=RSCPTR;                /* POINT TO START OF RESOURCE
*                                      ENTRIES IN RRT                */
         ST    @14,ENTADDRR(,RDATA)                             02S0403
*   ENTADDRV=RVTPTR;                /* POINT TO FIRST ENTRY IN RVT   */
         ST    @11,ENTADDRV(,RDATA)                             02S0404
*   NATODAF=RRTRSID&SAVTZELM;       /* 'AND' RSID WITH SUBAREA MASK
*                                      TO OBTAIN NCP NETWORK ADDR.   */
         LH    @07,RVTINDEX(,@11)                               02S0405
         BCTR  @07,0                                            02S0405
         ALR   @07,@07                                          02S0405
         AL    @07,SAVTNAT(,@09)                                02S0405
         SLR   @06,@06                                          02S0405
         ICM   @06,3,SAVTZELM(@09)                              02S0405
         SLR   @09,@09                                          02S0405
         ICM   @09,3,RRTRSID(@14)                               02S0405
         NR    @06,@09                                          02S0405
         STH   @06,NATODAF(,@07)                                02S0405
*   ENTADDRV=ENTADDRV+LENGTH(RVTINDEX);/* POINT PAST NCP ENTRY       */
         AH    @11,@CH00236                                     02S0406
         ST    @11,ENTADDRV(,RDATA)                             02S0406
*   DO WHILE(ENTCTR^=0);            /* CREATE NAT ENTRIES FOR THE
*                                      RESOURCES OFF THE NCP         */
         B     @DE00407                                         02S0407
@DL00407 DS    0H                                               02S0408
*     IF RVTINDEX^=0 THEN           /* INDICATES RESOURCE CAN BE USED
*                                      BY TCAM                       */
         L     @14,ENTADDRV(,RDATA)                             02S0408
         LH    @14,RVTINDEX(,@14)                               02S0408
         LTR   @14,@14                                          02S0408
         BZ    @RF00408                                         02S0408
*       IF RRTDEVCE=ON&RRTSWTCH=ON&RRTSNA=OFF&RRTCTERM=OFF THEN/*
*                                                            @YM08444*/
         L     @11,ENTADDRR(,RDATA)                             02S0409
         TM    RRTDEVCE(@11),B'01000100'                        02S0409
         BNO   @RF00409                                         02S0409
         TM    RRTSNA(@11),B'01100000'                          02S0409
         BNZ   @RF00409                                         02S0409
*         IF NATODAF=0 THEN         /* IF PRESNA DIAL TERM AND NOT ON
*                                                            @YM08444*/
         L     @09,AVTSAVTP(,RAVT)                              02S0410
         BCTR  @14,0                                            02S0410
         ALR   @14,@14                                          02S0410
         AL    @14,SAVTNAT(,@09)                                02S0410
         ICM   @09,3,NATODAF(@14)                               02S0410
         BNZ   @RF00410                                         02S0410
*           NATODAF=RRTRSID;        /* OTHER NCP THEN SET ITS NAT
*                                      ENTRY                 @YM08444*/
         MVC   NATODAF(2,@14),RRTRSID(@11)                      02S0411
*         ELSE                                                  02S0412
*           ;                       /* ELSE LEAVE IT ALONE   @YM08444*/
@RF00410 DS    0H                                               02S0413
*       ELSE                        /* ELSE SET NAT ENTRY FOR OTHER
*                                      DEVICES               @YM08444*/
*         NATODAF=RRTRSID;          /* RSID = NETWORK ADDR.          */
         B     @RC00409                                         02S0413
@RF00409 L     @14,AVTSAVTP(,RAVT)                              02S0413
         L     @11,ENTADDRV(,RDATA)                             02S0413
         LH    @11,RVTINDEX(,@11)                               02S0413
         BCTR  @11,0                                            02S0413
         ALR   @11,@11                                          02S0413
         AL    @11,SAVTNAT(,@14)                                02S0413
         L     @14,ENTADDRR(,RDATA)                             02S0413
         MVC   NATODAF(2,@11),RRTRSID(@14)                      02S0413
*     ELSE                                                      02S0414
*       ;                                                       02S0414
@RF00408 DS    0H                                               02S0415
*     ENTADDRV=ENTADDRV+LENGTH(RVTINDEX);/* POINT TO NEXT RVT ENTRY  */
@RC00408 LA    @14,2                                            02S0415
         AL    @14,ENTADDRV(,RDATA)                             02S0415
         ST    @14,ENTADDRV(,RDATA)                             02S0415
*     ENTADDRR=ENTADDRR+ENTRYSIZ;   /* POINT TO NEXT RRT ENTRY       */
         L     @14,ENTADDRR(,RDATA)                             02S0416
         AL    @14,ENTRYSIZ(,RDATA)                             02S0416
         ST    @14,ENTADDRR(,RDATA)                             02S0416
*     ENTCTR=ENTCTR-1;              /* DECREMENT LOOP CTR.           */
         L     @14,ENTCTR(,RDATA)                               02S0417
         BCTR  @14,0                                            02S0417
         ST    @14,ENTCTR(,RDATA)                               02S0417
*   END;                                                        02S0418
@DE00407 ICM   @14,15,ENTCTR(RDATA)                             02S0418
         BNZ   @DL00407                                         02S0418
*   RFY                                                         02S0419
*     RWORK UNRSTD;                                             02S0419
*   RFY                                                         02S0420
*     IEDSATD;                                                  02S0420
*                                                               02S0421
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT CONTROLS THE BUILDING OF THE HIERARCHY.          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0421
*   ENTCTR=ENTRYNUM;                /* SET CTR. TO NO. OF RRT ENTRIES*/
         MVC   ENTCTR(4,RDATA),ENTRYNUM(RDATA)                  02S0421
*   BSCTR=PSNANUM;                  /* SET CTR. FOR NO. OF PRE-SNA
*                                      RESOURCES                     */
         MVC   BSCTR(4,RDATA),PSNANUM(RDATA)                    02S0422
*   ENTADDRR=RSCPTR;                /* POINT TO FIRST RRT RESOURCE
*                                      ENTRY                         */
         MVC   ENTADDRR(4,RDATA),RSCPTR(RDATA)                  02S0423
*   ENTADDRV=RVTPTR+LENGTH(RVTINDEX);/* POINT TO SECOND RVT ENTRY    */
         LA    @14,2                                            02S0424
         AL    @14,RVTPTR(,RDATA)                               02S0424
         ST    @14,ENTADDRV(,RDATA)                             02S0424
*                                                               03S0425
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT DOES THE FOLLOWING: 1) SETS RVTINDEX TO 0 FOR    */
*   /* THOSE RESOURCES UNDER A RESOURCE THAT HAD BEEN SET 0 BY A     */
*   /* PREVIOUS SEGMENT. THIS MAINTAINS THE HIERARCHICAL INTEGRITY OF*/
*   /* THE RESOURCES. 2) SETS TRMGP FOR PRE-SNA TERMINALS UNDER A    */
*   /* CLUSTER WHEN RRTGPTRM IS SET IN THE RRT AND THE CLUSTER       */
*   /* RVTINDEX IS NOT 0.                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               03S0425
*/* THIS SEGMENT IS CONTROLLED BY 4 SWITCHES. THEY HAVE THE FOLLOWING*/
*/* MEANING WHEN ON:                                                 */
*/* LINEDEL : INDICATES THAT A LINE'S RVTINDEX IS 0 AND THAT RVTINDEX*/
*/*           SHOULD BE SET TO 0 FOR ALL RESOURCES UNDER THE LINE.   */
*/* PUDEL   : INDICATES THAT A PU'S RVTINDEX IS 0 AND THAT RVTINDEX  */
*/*           SHOULD BE SET TO 0 FOR ALL LU'S UNDER THE PU.          */
*/* CTERMDEL: SET WHEN A PRE-SNA SWITCHED LINE IS ENCOUNTERED.       */
*/*           INDICATES THAT ALL TERMINALS WITHOUT RRTCTERM SET      */
*/*           SHOULD HAVE THEIR RVTINDEX SET TO 0.                   */
*/* GPCLUST : INDICATES A PRE-SNA CLUSTER HAS BEEN ENCOUNTERED AND   */
*/*           THAT ALL TERMINALS UNDERNEATH WHICH HAVE RRTGPTRM SET  */
*/*           SHOULD HAVE TRMGP SET.                                 */
*                                                               03S0425
*   DO WHILE(ENTCTR^=0);            /* LOOP THROUGH RVT AND AND RRT  */
         B     @DE00425                                         03S0425
@DL00425 DS    0H                                               03S0426
*     IF RRTLINE=ON THEN            /* IF RESOURCE IS A LINE         */
         L     @14,ENTADDRR(,RDATA)                             03S0426
         TM    RRTLINE(@14),B'10000000'                         03S0426
         BNO   @RF00426                                         03S0426
*       DO;                                                     03S0427
*         FLAGS2='00'X;             /* RESET ALL SWITCHES FOR NEW
*                                      LINE                          */
         MVI   FLAGS2(RDATA),X'00'                              03S0428
*         IF RVTINDEX^=0 THEN                                   03S0429
         SLR   @11,@11                                          03S0429
         L     @09,ENTADDRV(,RDATA)                             03S0429
         CH    @11,RVTINDEX(,@09)                               03S0429
         BE    @RF00429                                         03S0429
*           IF BSCTR>0 THEN         /* IF THIS IS A PRE-SNA LINE     */
         C     @11,BSCTR(,RDATA)                                03S0430
         BNL   @RF00430                                         03S0430
*             IF RRTSWTCH=ON THEN   /* IF THIS IS A SWITCHED LINE    */
         TM    RRTSWTCH(@14),B'00000100'                        03S0431
         BNO   @RF00431                                         03S0431
*               CTERMDEL=ON;        /* SWITCHED LINE ENCOUNTERED FLAG*/
         OI    CTERMDEL(RDATA),B'00100000'                      03S0432
*             ELSE                                              03S0433
*               ;                                               03S0433
@RF00431 DS    0H                                               03S0434
*           ELSE                                                03S0434
*             ;                                                 03S0434
@RF00430 DS    0H                                               03S0435
*         ELSE                                                  03S0435
*           LINEDEL=ON;             /* SET RVTINDEX FOR ALL     03S0435
*                                      SUBORDINATE RESOURCES TO 0    */
         B     @RC00429                                         03S0435
@RF00429 OI    LINEDEL(RDATA),B'10000000'                       03S0435
*       END;                                                    03S0436
*     ELSE                                                      03S0437
*       IF LINEDEL=ON THEN          /* IF LINE RVTINDEX IS 0         */
         B     @RC00426                                         03S0437
@RF00426 TM    LINEDEL(RDATA),B'10000000'                       03S0437
         BNO   @RF00437                                         03S0437
*         RVTINDEX=0;               /* ZERO RVTINDEX FOR THIS   03S0438
*                                      RESOURCE                      */
         L     @14,ENTADDRV(,RDATA)                             03S0438
         SLR   @11,@11                                          03S0438
         STH   @11,RVTINDEX(,@14)                               03S0438
*       ELSE                                                    03S0439
*         IF RRTDEVCE=ON THEN       /* IF PRE-SNA TERM. OR PU        */
         B     @RC00437                                         03S0439
@RF00437 L     @14,ENTADDRR(,RDATA)                             03S0439
         TM    RRTDEVCE(@14),B'01000000'                        03S0439
         BNO   @RF00439                                         03S0439
*           IF RRTSNA=OFF THEN      /* IF PRE-SNA TERM.              */
         TM    RRTSNA(@14),B'00100000'                          03S0440
         BNZ   @RF00440                                         03S0440
*             IF CTERMDEL=OFF THEN  /* IF NOT ON SWITCHED LINE       */
         TM    CTERMDEL(RDATA),B'00100000'                      03S0441
         BNZ   @RF00441                                         03S0441
*               IF RRTCLSTR=OFF THEN/* IF NOT A CLUSTER              */
         TM    RRTCLSTR(@14),B'10000000'                        03S0442
         BNZ   @RF00442                                         03S0442
*                 IF RRTGPTRM=OFF THEN/* IF RRT INDICATES NOT GEN.
*                                      POLL                          */
         TM    RRTGPTRM(@14),B'00001000'                        03S0443
         BNZ   @RF00443                                         03S0443
*                   GPCLUST=OFF;    /* TURN OFF GEN. POLL CLUSTER SW.
*                                      ONLY SPECIFIC POLL FOR THIS
*                                      TERM.                         */
         NI    GPCLUST(RDATA),B'11101111'                       03S0444
*                 ELSE                                          03S0445
*                   IF RVTINDEX=0 THEN                          03S0445
         B     @RC00443                                         03S0445
@RF00443 L     @14,ENTADDRV(,RDATA)                             03S0445
         ICM   @11,3,RVTINDEX(@14)                              03S0445
         BZ    @RT00445                                         03S0445
*                     ;             /* IF INDEX ALREADY 0            */
*                   ELSE                                        03S0447
*                     IF GPCLUST=ON THEN/* IF TERM. UNDER GEN. POLL
*                                      CLUST.                        */
         TM    GPCLUST(RDATA),B'00010000'                       03S0447
         BNO   @RF00447                                         03S0447
*                       DO;                                     03S0448
*                         RPARM=RVTINDEX;/* SET PARAMETER WITH TTCIN */
         L     @14,ENTADDRV(,RDATA)                             03S0449
         LH    RPARM,RVTINDEX(,@14)                             03S0449
*                         CALL IEDQTNT;/* CONVERT TTCIN TO TTE       */
         L     @15,AVTRNMPT(,RAVT)                              03S0450
         BALR  @14,@15                                          03S0450
*                         RTRM=RPARM;/* TTE ADDR.                    */
         LR    RTRM,RPARM                                       03S0451
*                         TRMGP=ON; /* SET GEN. POLL FLAG IN TTE     */
         LR    @14,RTRM                                         03S0452
         AL    @14,@CF02819                                     03S0452
         OI    TRMGP-6(@14),B'00000100'                         03S0452
*                       END;                                    03S0453
*                     ELSE                                      03S0454
*                       ;                                       03S0454
@RF00447 DS    0H                                               03S0455
*               ELSE                                            03S0455
*                 IF RVTINDEX=0 THEN/* IF INDEX ALREADY 0            */
         B     @RC00442                                         03S0455
@RF00442 L     @14,ENTADDRV(,RDATA)                             03S0455
         ICM   @11,3,RVTINDEX(@14)                              03S0455
         BNZ   @RF00455                                         03S0455
*                   GPCLUST=OFF;    /* TURN OFF GEN. POLL CLUST. FLAG*/
         NI    GPCLUST(RDATA),B'11101111'                       03S0456
*                 ELSE                                          03S0457
*                   GPCLUST=ON;     /* TURN ON GEN. POLL CLUST. FLAG */
         B     @RC00455                                         03S0457
@RF00455 OI    GPCLUST(RDATA),B'00010000'                       03S0457
*             ELSE                                              03S0458
*               IF RRTCTERM=ON THEN                             03S0458
         B     @RC00441                                         03S0458
@RF00441 L     @14,ENTADDRR(,RDATA)                             03S0458
         TM    RRTCTERM(@14),B'01000000'                        03S0458
         BO    @RT00458                                         03S0458
*                 ;                 /* IF RRT INDICATES CTERM        */
*               ELSE                                            03S0460
*                 RVTINDEX=0;       /* ZERO INDEX FOR NON CTERM ON
*                                      SWITCHED LINE                 */
         L     @14,ENTADDRV(,RDATA)                             03S0460
         SLR   @11,@11                                          03S0460
         STH   @11,RVTINDEX(,@14)                               03S0460
*           ELSE                                                03S0461
*             IF RVTINDEX=0 THEN    /* IF PU INDEX 0                 */
         B     @RC00440                                         03S0461
@RF00440 L     @14,ENTADDRV(,RDATA)                             03S0461
         ICM   @11,3,RVTINDEX(@14)                              03S0461
         BNZ   @RF00461                                         03S0461
*               PUDEL=ON;           /* ZERO INDEX FOR ALL LU'S BELOW */
         OI    PUDEL(RDATA),B'01000000'                         03S0462
*             ELSE                                              03S0463
*               PUDEL=OFF;          /* PU OD                         */
         B     @RC00461                                         03S0463
@RF00461 NI    PUDEL(RDATA),B'10111111'                         03S0463
*         ELSE                                                  03S0464
*           IF PUDEL=OFF THEN                                   03S0464
         B     @RC00439                                         03S0464
@RF00439 TM    PUDEL(RDATA),B'01000000'                         03S0464
         BZ    @RT00464                                         03S0464
*             ;                     /* IF THIS LU'S PU'S INDEX ^0    */
*           ELSE                                                03S0466
*             RVTINDEX=0;           /* ZERO INDEX FOR LU.            */
         L     @14,ENTADDRV(,RDATA)                             03S0466
         SLR   @11,@11                                          03S0466
         STH   @11,RVTINDEX(,@14)                               03S0466
*     ENTCTR=ENTCTR-1;              /* DECREMENT LOOP CTR.           */
@RT00464 DS    0H                                               03S0467
@RC00439 DS    0H                                               03S0467
@RC00437 DS    0H                                               03S0467
@RC00426 L     @14,ENTCTR(,RDATA)                               03S0467
         BCTR  @14,0                                            03S0467
         ST    @14,ENTCTR(,RDATA)                               03S0467
*     BSCTR=BSCTR-1;                /* DECREMENT PRE-SNA CTR.        */
         L     @14,BSCTR(,RDATA)                                03S0468
         BCTR  @14,0                                            03S0468
         ST    @14,BSCTR(,RDATA)                                03S0468
*     ENTADDRV=ENTADDRV+LENGTH(RVTINDEX);/* POINT TO NEXT RVT ENTRY  */
         LA    @14,2                                            03S0469
         AL    @14,ENTADDRV(,RDATA)                             03S0469
         ST    @14,ENTADDRV(,RDATA)                             03S0469
*     ENTADDRR=ENTADDRR+ENTRYSIZ;   /* POINT TO NEXT RRT ENTRY       */
         L     @14,ENTADDRR(,RDATA)                             03S0470
         AL    @14,ENTRYSIZ(,RDATA)                             03S0470
         ST    @14,ENTADDRR(,RDATA)                             03S0470
*   END;                                                        03S0471
@DE00425 ICM   @14,15,ENTCTR(RDATA)                             03S0471
         BNZ   @DL00425                                         03S0471
*                                                               03S0472
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT CREATES THE HIERARCHY IN THE TTE'S BY SETTING THE*/
*   /* COHORT AND SUBSTRUCT POINTERS.                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               03S0472
*   ENTADDRV=RVTPTR+LENGTH(RVTINDEX);/* POINT TO 2ND RVT ENTRY       */
         L     @14,RVTPTR(,RDATA)                               03S0472
         LA    @11,2                                            03S0472
         ALR   @11,@14                                          03S0472
         ST    @11,ENTADDRV(,RDATA)                             03S0472
*   ENTADDRR=RSCPTR;                /* POINT TO 1ST RESOURCE ENTRY IN
*                                      RRT                           */
         MVC   ENTADDRR(4,RDATA),RSCPTR(RDATA)                  03S0473
*   RTRM=NCPTTEA;                   /* POINT TO NCP TTE              */
         L     RTRM,NCPTTEA(,RDATA)                             03S0474
*   TRMSUBST=RVTPTR->RVTINDEX;      /* SET NCP SUBSTRUCT POINTER TO
*                                      POINT TO NCP                  */
         LR    @11,RTRM                                         03S0475
         BCTR  @11,0                                            03S0475
         BCTR  @11,0                                            03S0475
         MVC   TRMSUBST-12(2,@11),RVTINDEX(@14)                 03S0475
*   ENTCTR=ENTRYNUM;                /* SET LOOP COUNTER TO NUMBER OF
*                                      RRT RESOURCE ENTRIES          */
         MVC   ENTCTR(4,RDATA),ENTRYNUM(RDATA)                  03S0476
*   DO WHILE(ENTCTR^=0);            /* LOOP THRU RRT AND RVT         */
         B     @DE00477                                         03S0477
@DL00477 DS    0H                                               03S0478
*     IF RVTINDEX^=0 THEN           /* IF RESOURCE TO BE INCLUDED IN
*                                      HIERARCHY                     */
         L     @14,ENTADDRV(,RDATA)                             03S0478
         LH    @14,RVTINDEX(,@14)                               03S0478
         LTR   @14,@14                                          03S0478
         BZ    @RF00478                                         03S0478
*       DO;                                                     03S0479
*         RPARM=RVTINDEX;           /* SET PARM. AS TTCIN OF RESOURCE*/
         LR    RPARM,@14                                        03S0480
