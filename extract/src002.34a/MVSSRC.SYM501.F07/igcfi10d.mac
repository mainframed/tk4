*                                                               03S0515
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT PERFORMS TWO FUNCTIONS WHICH RESULTS IN THE      */
*   /* COMPLETION OF THE HIERARCHY IN THE TERMINAL TABLE AND THE RVT.*/
*   /* 1) IT LOOPS THROUGH THE TERM. TABLE HIERARCHY ALREADY BUILT   */
*   /* LOCATING THOSE RESOURCES WHOSE SUBSTRUCT IS SET TO ITS TTCIN  */
*   /* (INDICATING THE RESOURCE IS LOWEST IN THE HIERARCY) AND SETS  */
*   /* THE SUBSTRUCT TO ZERO. (WHILE LOOPING, TRMINPG IS SET FOR SNA */
*   /* RESOURCES INDICATING ACTIVATE IN PROGRESS FOR A SUPERIOR      */
*   /* RESOURCE.) 2) IT CALLS RVTSET TO SET THE RVT HIERARCHY.       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               03S0515
*   ENTADDRV=RVTPTR;                /* POINT TO FIRST ENTRY IN RVT   */
         L     @14,RVTPTR(,RDATA)                               03S0515
         ST    @14,ENTADDRV(,RDATA)                             03S0515
*   ENTCTR=RVTNUM;                  /* SET LOOP CTR TO NO. OF RVT
*                                      ENTRIES                       */
         LH    @14,RVTNUM(,@14)                                 03S0516
         ST    @14,ENTCTR(,RDATA)                               03S0516
*   DO WHILE(ENTCTR^=0);            /* LOOP THOURGH HIERARCHY        */
         B     @DE00517                                         03S0517
@DL00517 DS    0H                                               03S0518
*     IF RVTINDEX^=0 THEN           /* IF RVT ENTRY CONTAINS TTCIN   */
         L     @14,ENTADDRV(,RDATA)                             03S0518
         LH    @14,RVTINDEX(,@14)                               03S0518
         LTR   @14,@14                                          03S0518
         BZ    @RF00518                                         03S0518
*       DO;                                                     03S0519
*         RPARM=RVTINDEX;           /* SET PARM. TO TTCIN            */
         LR    RPARM,@14                                        03S0520
*         CALL IEDQTNT;             /* CONVERT TTCIN TO TTE ADDR.    */
         L     @15,AVTRNMPT(,RAVT)                              03S0521
         BALR  @14,@15                                          03S0521
*         RTRM=RPARM;               /* COPY TTE ADDR.                */
         LR    RTRM,RPARM                                       03S0522
*         IF TRMSNA=ON THEN         /* IF THIS IS AN SNA RESOURCE    */
         LR    @14,RTRM                                         03S0523
         SH    @14,@CH02810                                     03S0523
         TM    TRMSNA(@14),B'00000010'                          03S0523
         BNO   @RF00523                                         03S0523
*           TRMINPG=ON;             /* TURN ON IN-PROGRESS BIT       */
         LA    @05,TRMINPG(,@14)                                03S0524
         LR    @07,@05                                          03S0524
         LA    @11,3                                            03S0524
         NR    @07,@11                                          03S0524
         LCR   @07,@07                                          03S0524
         ALR   @05,@07                                          03S0524
         L     @11,0(,@05)                                      03S0524
@GL00003 LR    @09,@11                                          03S0524
         O     @09,@CB02823+3(@07)                              03S0524
         CS    @11,@09,0(@05)                                   03S0524
         BNE   @GL00003                                         03S0524
*         ELSE                                                  03S0525
*           ;                                                   03S0525
@RF00523 DS    0H                                               03S0526
*         IF TRMSUBST=RVTINDEX THEN /* IF SUBSTRUCT MATCHES TTCIN OF
*                                      RESOURCE                      */
         LR    @14,RTRM                                         03S0526
         SH    @14,@CH02810                                     03S0526
         L     @11,ENTADDRV(,RDATA)                             03S0526
         SLR   @09,@09                                          03S0526
         ICM   @09,3,TRMSUBST(@14)                              03S0526
         CH    @09,RVTINDEX(,@11)                               03S0526
         BNE   @RF00526                                         03S0526
*           TRMSUBST=0;                                         03S0527
         SLR   @11,@11                                          03S0527
         STH   @11,TRMSUBST(,@14)                               03S0527
*         ELSE                                                  03S0528
*           ;                                                   03S0528
@RF00526 DS    0H                                               03S0529
*       END;                                                    03S0529
*     ELSE                                                      03S0530
*       ;                                                       03S0530
@RF00518 DS    0H                                               03S0531
*     ENTADDRV=ENTADDRV+LENGTH(RVTINDEX);/* POINT TO NEXT RVT ENTRY  */
         LA    @14,2                                            03S0531
         AL    @14,ENTADDRV(,RDATA)                             03S0531
         ST    @14,ENTADDRV(,RDATA)                             03S0531
*     ENTCTR=ENTCTR-1;              /* DECREMENT LOOP CTR.           */
         L     @14,ENTCTR(,RDATA)                               03S0532
         BCTR  @14,0                                            03S0532
         ST    @14,ENTCTR(,RDATA)                               03S0532
*   END;                                                        03S0533
@DE00517 ICM   @14,15,ENTCTR(RDATA)                             03S0533
         BNZ   @DL00517                                         03S0533
