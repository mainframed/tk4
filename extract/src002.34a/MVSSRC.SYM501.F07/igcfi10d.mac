         L     @14,ENTADDRR(,RDATA)                             04S0548
         CLC   NATODAF(2,@11),RRTRSID(@14)                      04S0548
         BE    @RF00548                                         04S0548
@RT00548 DS    0H                                               04S0549
*               RVTINDEX=0;         /* ENTRY                 @YM08444*/
         L     @14,ENTADDRV(,RDATA)                             04S0549
