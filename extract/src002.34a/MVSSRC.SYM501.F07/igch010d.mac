         TITLE 'IGCH010D, HOLD RESOURCE COMMAND PROCESSOR              *
                        '
IGCH010D CSECT ,                                                01S0001
@PROLOG  BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                                   /*
IGCH010D IEDHJN AFTERHJN,HJN           MODULE IDENTIFICATION MACRO
*   ROPCAVT=RPARM;                  /* SET THE BASE POINTER FOR THE
*                                      OPERATOR CONTROL ADDRESS 02S0337
*                                      VECTOR TABLE                  */
         LR    ROPCAVT,RPARM                                    02S0337
*   RFY                                                         02S0338
*     RPARM UNRSTD;                 /* RELEASE THE PARAMETER REGISTER
*                                      FOR USE BY THE COMPILER       */
*   REG10=ADDR(CMDTEXT)+CMDLNGTH;   /* SET THE BASE REGISTER TO BE
*                                      USED FOR ADDRESSING AUTOMATIC
*                                      DATA WITH THE POINTER TO THE
*                                      OPERATOR CONTROL RESIDENT WORK
*                                      AREA DISPLACED BY THE AREA
*                                      USED FOR THE HOLD COMMAND AND
*                                      ITS DATA                      */
         L     @09,OPCWORK(,ROPCAVT)                            02S0339
         LA    REG10,CMDTEXT(,@09)                              02S0339
         AH    REG10,CMDLNGTH(,@09)                             02S0339
*   GEN REFS(REG10) NOSETS;                                     02S0340
*                                   /*
         USING @DATD,REG10             INFORM THE ASSEMBLER OF THE
*                                      BASE REGISTER TO BE USED FOR
*                                      ADDRESSING AUTOMATIC DATA
*   RSAVE=ADDR(OPCSAVE);            /* SET THE SAVE AREA POINTER
*                                      REGISTER WITH THE POINTER TO
*                                      THE OPERATOR CONTROL SAVE AREA*/
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          02S0341
*                                                               02S0342
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS: THIS SEGMENT DOES THE PROCESSING THAT IS NECESSARY TO*/
*   /* PERFORM THE FUNCTION OF THE MODULE.                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0342
*                                                               03S0342
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS.INITIAL: THIS SEGMENT DOES THE INITIAL PROCESSING.    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               03S0342
*   RAVT=OPCAVTPT;                  /* SET THE BASE POINTER FOR THE
*                                      TCAM ADDRESS VECTOR TABLE     */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          03S0342
*   ROPCE=OPCCOPCE;                 /* SET THE BASE POINTER FOR THE
*                                      OPERATOR CONTROL ELEMENT      */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         03S0343
*   IF OCOPTCDE=OCVARY THEN                                     03S0344
         CLI   OCOPTCDE(ROPCE),X'28'                            03S0344
         BNE   @RF00344                                         03S0344
*     REG10=ADDR(CMDTEXT);          /* ON A VARY COMMAND RESET REG10
*                                      TO EIGHT INTO THE OPCTL  03S0345
*                                      WORKAREA              @OY20230*/
         L     REG10,OPCWORK(,ROPCAVT)                          03S0345
         LA    REG10,CMDTEXT(,REG10)                            03S0345
*   RETCODE=0;                      /* INITIALIZE THE RETURN CODE TO
*                                      INDICATE SUCCESSFUL PROCESSING
*                                      OF THE HOLD COMMAND           */
@RF00344 SLR   RETCODE,RETCODE                                  03S0346
*   REG00=0;                        /* INITIALIZE REGISTER ZERO (0)
*                                      TO INDICATE THAT AN      03S0347
*                                      APPROPRIATE MESSAGE NUMBER HAS
*                                      NOT BEEN DETERMINED           */
         SLR   REG00,REG00                                      03S0347
*   IF OPCRSTRT=OFF                 /* RESTART IS NOT IN PROGRESS    */
*       &OCOPTCDE^=OCVARY           /* THE HOLD COMMAND WAS     02S0348
*                                      EXTERNALLY BUILT              */
*       &OCTOTE=OFF                 /* THE INPUT IS NOT FROM TOTE    */
*       &OCMPP=OFF                  /* THE INPUT IS NOT FROM AN 02S0348
*                                      APPLICATION PROGRAM           */
*     THEN                          /* PARSE THE REMAINING OPERANDS
*                                      THAT ARE IN THE OPERATOR 02S0348
*                                      CONTROL RESIDENT WORK AREA    */
         TM    OPCRSTRT(ROPCAVT),B'00100000'                    02S0348
         BNZ   @RF00348                                         02S0348
         CLI   OCOPTCDE(ROPCE),X'28'                            02S0348
         BE    @RF00348                                         02S0348
         TM    OCTOTE(ROPCE),B'01100000'                        02S0348
         BNZ   @RF00348                                         02S0348
*                                                               03S0349
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS.PARSE: THIS SEGMENT PARSES THE REMAINING OPERANDS   */
*     /* THAT ARE IN THE OPERATOR CONTROL RESIDENT WORK AREA.        */
*     /*                                                             */
*     /***************************************************************/
*                                                               03S0349
*     DO;                                                       03S0349
*       IF CMDTEXT(1:3)='TP='       /* THE INITIAL CHARACTERS ARE
*                                      CORRECT                       */
*           &CMDLNGTH>3             /* THERE ARE OTHER OPERANDS      */
*           &CMDLNGTH<=15           /* THE COMMAND IS NOT GREATER
*                                      THAN THE MAXIMUM COMMAND 03S0350
*                                      LENGTH                        */
*         THEN                      /* CONTINUE SCANNING THE COMMAND
*                                      TEXT FOR VALID OPERANDS       */
         L     @11,OPCWORK(,ROPCAVT)                            03S0350
         CLC   CMDTEXT(3,@11),@CC01842                          03S0350
         BNE   @RF00350                                         03S0350
         LA    @07,3                                            03S0350
         LH    @05,CMDLNGTH(,@11)                               03S0350
         CR    @05,@07                                          03S0350
         BNH   @RF00350                                         03S0350
         C     @05,@CF00104                                     03S0350
         BH    @RF00350                                         03S0350
*                                                               04S0351
*         /***********************************************************/
*         /*                                                         */
*         /* PROCESS.PARSE.OPERANDS: THIS SEGMENT CONTINUES SCANNING */
*         /* THE COMMAND FOR VALID OPERANDS.                         */
*         /*                                                         */
*         /***********************************************************/
*                                                               04S0351
*         DO;                                                   04S0351
*           PTRSTOP=ADDR(CMDTEXT)+CMDLNGTH-1;/* SET THE "STOP"  04S0352
*                                      POINTER FOR THE COMMAND TEXT
*                                      SCAN                          */
         L     @01,OPCWORK(,ROPCAVT)                            04S0352
         LA    @01,CMDTEXT(,@01)                                04S0352
         ALR   @05,@01                                          04S0352
         BCTR  @05,0                                            04S0352
         LR    PTRSTOP,@05                                      04S0352
*           DO PTRCHAR=ADDR(CMDTEXT)+3 TO PTRSTOP/* SCAN THE COMMAND
*                                      AFTER "TP="                   */
*                 WHILE(SCANCHAR^=',');/* A COMMA IS NOT ENCOUNTERED */
         ALR   @01,@07                                          04S0353
         LR    PTRCHAR,@01                                      04S0353
         B     @DE00353                                         04S0353
@DL00353 CLI   SCANCHAR(PTRCHAR),C','                           04S0353
         BE    @DC00353                                         04S0353
*           END;                                                04S0354
         AL    PTRCHAR,@CF00062                                 04S0354
@DE00353 CR    PTRCHAR,PTRSTOP                                  04S0354
         BNH   @DL00353                                         04S0354
@DC00353 DS    0H                                               04S0355
*           OCBACKUP=CMDTEXT(4:PTRCHAR-ADDR(CMDTEXT));/* SAVE THE
*                                      RESOURCE NAME OPERAND         */
         MVI   OCBACKUP+1(ROPCE),C' '                           04S0355
         MVC   OCBACKUP+2(6,ROPCE),OCBACKUP+1(ROPCE)            04S0355
         L     @11,OPCWORK(,ROPCAVT)                            04S0355
         LA    @07,CMDTEXT(,@11)                                04S0355
         LR    @01,PTRCHAR                                      04S0355
         SLR   @01,@07                                          04S0355
         SL    @01,@CF00256                                     04S0355
         EX    @01,@SM01894                                     04S0355
*           IF PTRCHAR=ADDR(CMDTEXT)+CMDLNGTH/* THERE IS NO RELATIVE
*                                      LINE NUMBER OPERAND           */
*               &CMDLNGTH<=11       /* THE LENGTH OF THE RESOURCE
*                                      OPERAND DOES NOT EXCEED THE
*                                      MAXIMUM LENGTH                */
*             THEN                  /* INDICATE THAT THE COMMAND IS
*                                      NOT FOR A LINK                */
         LH    @11,CMDLNGTH(,@11)                               04S0356
         ALR   @07,@11                                          04S0356
         CR    PTRCHAR,@07                                      04S0356
         BNE   @RF00356                                         04S0356
         C     @11,@CF01844                                     04S0356
         BH    @RF00356                                         04S0356
*             OCRLN=0;              /* SET THE RELATIVE LINE NUMBER
*                                      INPUT FIELD TO ZERO           */
         MVI   OCRLN(ROPCE),X'00'                               04S0357
*           ELSE                    /* CHECK FOR A CORRECT RELATIVE
*                                      LINE NUMBER OPERAND           */
*             IF SCANCHAR=','       /* THE SCAN STOPPED ON A COMMA   */
*                 &CMDLNGTH-EVAL(PTRCHAR-ADDR(CMDTEXT))-1>0/* THE
*                                      RELATIVE LINE NUMBER OPERAND
*                                      EXISTS                        */
*                 &CMDLNGTH-EVAL(PTRCHAR-ADDR(CMDTEXT))-1<=3/* THE
*                                      RELATIVE LINE NUMBER OPERAND
*                                      LENGTH DOES NOT EXCEED THE
*                                      MAXIMUM                       */
*               THEN                /* PROCESS THE RELATIVE LINE
*                                      NUMBER OPERAND                */
         B     @RC00356                                         04S0358
@RF00356 CLI   SCANCHAR(PTRCHAR),C','                           04S0358
         BNE   @RF00358                                         04S0358
         L     @11,OPCWORK(,ROPCAVT)                            04S0358
         LA    @07,CMDTEXT(,@11)                                04S0358
         LR    @01,PTRCHAR                                      04S0358
         SLR   @01,@07                                          04S0358
         LCR   @01,@01                                          04S0358
         AH    @01,CMDLNGTH(,@11)                               04S0358
         LR    @15,@01                                          04S0358
         BCTR  @15,0                                            04S0358
         LTR   @15,@15                                          04S0358
         BNP   @RF00358                                         04S0358
         LA    @14,3                                            04S0358
         CR    @15,@14                                          04S0358
         BH    @RF00358                                         04S0358
*                                                               05S0359
*               /*****************************************************/
*               /*                                                   */
*               /* PROCESS.PARSE.OPERANDS.RELATIVE: THIS SEGMENT     */
*               /* PROCESSES THE CHARACTER RELATIVE LINE NUMBER      */
*               /* OPERAND.                                          */
*               /*                                                   */
*               /*****************************************************/
*                                                               05S0359
*               DO;                                             05S0359
*                 CHARRLN='000';    /* SET THE CHARACTER FIELD FOR
*                                      THE RELATIVE LINE NUMBER TO
*                                      CHARACTER ZEROS               */
         MVC   CHARRLN(3),@CC01846                              05S0360
*                 CHARRLN(4-(CMDLNGTH-EVAL(PTRCHAR-ADDR(CMDTEXT))-1):3)
*                     =CMDTEXT(PTRCHAR-ADDR(CMDTEXT)+1:CMDLNGTH);/*
*                                      OBTAIN THE CHARACTER RELATIVE
*                                      LINE NUMBER                   */
         LCR   @01,@01                                          05S0361
         AL    @01,@CF00062                                     05S0361
         AL    @01,@CF00256                                     05S0361
         LA    @15,CHARRLN-1(@01)                               05S0361
         SLR   @14,@01                                          05S0361
         LCR   @07,@07                                          05S0361
         ALR   @07,PTRCHAR                                      05S0361
         ALR   @11,@07                                          05S0361
         EX    @14,@SM01896                                     05S0361
*                 DO PTRCHAR=ADDR(CHARRLN) TO ADDR(CHARRLN)+LENGTH(
*                       CHARRLN)    /* SCAN THE RELATIVE LINE NUMBER
*                                      OPERAND                       */
*                       WHILE(RETCODE=0);/* THE CHARACTERS ARE  05S0362
*                                      NUMERIC                       */
         LA    PTRCHAR,CHARRLN                                  05S0362
         B     @DE00362                                         05S0362
@DL00362 LTR   RETCODE,RETCODE                                  05S0362
         BNZ   @DC00362                                         05S0362
*                   IF SCANCHAR<'0'×SCANCHAR>'9'/* THE CHARACTER IS
*                                      NOT NUMERIC                   */
*                     THEN          /* INDICATE THAT AN INVALID 05S0363
*                                      OPERAND HAS BEEN ENCOUNTERED  */
         CLI   SCANCHAR(PTRCHAR),C'0'                           05S0363
         BL    @RT00363                                         05S0363
         CLI   SCANCHAR(PTRCHAR),C'9'                           05S0363
         BNH   @RF00363                                         05S0363
@RT00363 DS    0H                                               05S0364
*                     DO;                                       05S0364
*                       RETCODE=4;  /* SET THE RETURN CODE           */
         LA    RETCODE,4                                        05S0365
*                       REG00=MSGINVOP;/* SET REGISTER ZERO (0) WITH
*                                      THE APPROPRIATE MESSAGE NUMBER*/
         LA    REG00,18                                         05S0366
*                     END;                                      05S0367
*                 END;                                          05S0368
@RF00363 AL    PTRCHAR,@CF00062                                 05S0368
@DE00362 LA    @11,CHARRLN                                      05S0368
         AL    @11,@CF00075                                     05S0368
         CR    PTRCHAR,@11                                      05S0368
         BNH   @DL00362                                         05S0368
@DC00362 DS    0H                                               05S0369
*                 IF RETCODE=0      /* THE RELATIVE LINE NUMBER IS
*                                      NUMERICS                      */
*                   THEN            /* CONVERT THE CHARACTER RELATIVE
*                                      LINE NUMBER                   */
         SLR   @11,@11                                          05S0369
         CR    RETCODE,@11                                      05S0369
         BNE   @RF00369                                         05S0369
*                                                               06S0370
*                   /*************************************************/
*                   /*                                               */
*                   /* PROCESS.PARSE.OPERANDS.RELATIVE.CONVERT: THIS */
*                   /* SEGMENT CONVERTS THE CHARACTER RELATIVE LINE  */
*                   /* NUMBER TO A POSITIVE BINARY INTEGER.          */
*                   /*                                               */
*                   /*************************************************/
*                                                               06S0370
*                   DO;                                         06S0370
*                     PACK(PACKRLN,CHARRLN);/* PACK THE RELATIVE LINE
*                                      NUMBER                        */
         PACK  PACKRLN(2),CHARRLN(3)                            06S0371
*                     CVB(FIXEDRLN,PACKRLN);/* CONVERT THE PACKED
*                                      RELATIVE LINE NUMBER TO A
*                                      BINARY FIXED-POINT INTEGER    */
         XC    @TS00001(8),@TS00001                             06S0372
         MVC   @TS00001+6(2),PACKRLN                            06S0372
         CVB   FIXEDRLN,@TS00001                                06S0372
*                     IF FIXEDRLN>0&FIXEDRLN<=255/* TEST FOR A  06S0373
*                                      RELATIVE LINE NUMBER THAT IS
*                                      IN RANGE                      */
*                       THEN        /* SET THE RELATIVE LINE NUMBER
*                                      INPUT FIELD                   */
         CR    FIXEDRLN,@11                                     06S0373
         BNH   @RF00373                                         06S0373
         C     FIXEDRLN,@CF01850                                06S0373
         BH    @RF00373                                         06S0373
*                       OCRLN=FIXEDRLN;                         06S0374
         STC   FIXEDRLN,OCRLN(,ROPCE)                           06S0374
*                     ELSE          /* INDICATE THAT AN INVALID 06S0375
*                                      OPERAND WAS ENCOUNTERED       */
*                       DO;                                     06S0375
         B     @RC00373                                         06S0375
@RF00373 DS    0H                                               06S0376
*                         RETCODE=4;/* SET THE RETURN CODE           */
         LA    RETCODE,4                                        06S0376
*                         REG00=MSGINVOP;/* SET REGISTER ZERO (0)
*                                      WITH THE APPROPRIATE MESSAGE
*                                      NUMBER                        */
         LA    REG00,18                                         06S0377
*                       END;                                    06S0378
*                   END;                                        06S0379
*               END;                                            05S0380
*             ELSE                  /* INDICATE THAT AN INVALID 04S0381
*                                      OPERAND WAS ENCOUNTERED       */
*               DO;                                             04S0381
         B     @RC00358                                         04S0381
@RF00358 DS    0H                                               04S0382
*                 RETCODE=4;        /* SET THE RETURN CODE           */
         LA    RETCODE,4                                        04S0382
*                 REG00=MSGINVOP;   /* SET REGISTER ZERO (0) WITH THE
*                                      APPROPRIATE MESSAGE NUMBER    */
         LA    REG00,18                                         04S0383
*               END;                                            04S0384
*         END;                                                  04S0385
*       ELSE                        /* INDICATE THAT AN ERROR HAS
*                                      BEEN DETECTED                 */
*         DO;                                                   03S0386
         B     @RC00350                                         03S0386
@RF00350 DS    0H                                               03S0387
*           RETCODE=4;              /* SET THE RETURN CODE           */
         LA    RETCODE,4                                        03S0387
*           REG00=MSGINVOP;         /* SET REGISTER ZERO (0) WITH THE
*                                      APPROPRIATE MESSAGE NUMBER    */
         LA    REG00,18                                         03S0388
*         END;                                                  03S0389
*     END;                                                      03S0390
@RC00350 DS    0H                                               03S0391
*   IF RETCODE=0                    /* NO ERRORS HAVE BEEN      02S0391
*                                      ENCOUNTERED                   */
*     THEN                          /* VERIFY THAT THE RESOURCE CAN
*                                      BE HELD AND CONTINUE     02S0391
*                                      PROCESSING                    */
@RF00348 LTR   RETCODE,RETCODE                                  02S0391
         BNZ   @RF00391                                         02S0391
*     DO;                                                       02S0392
*                                                               03S0393
*       /*************************************************************/
*       /*                                                           */
*       /* PROCESS.VERIFY: THIS SEGMENT VERIFIES THAT THE OPERANDS   */
*       /* DESCRIBE A RESOURCE THAT IS KNOWN TO TCAM AND THAT THE    */
*       /* RESOURCE TYPE CAN BE HELD.                                */
*       /*                                                           */
*       /*************************************************************/
*                                                               03S0393
*       DO;                                                     03S0393
*         OPCDOUBL=OCBACKUP;        /* SET THE ARGUMENT FOR THE 03S0394
*                                      EXTERNAL ROUTINES THAT VERIFY
*                                      THE RESOURCE NAME             */
         MVC   OPCDOUBL(8,ROPCAVT),OCBACKUP(ROPCE)              03S0394
*                                                               04S0395
*         /***********************************************************/
*         /*                                                         */
*         /* PROCESS.VERIFY.NAME: THIS SEGMENT VERIFIES THAT THE     */
*         /* RESOURCE NAME IS KNOWN TO TCAM.                         */
*         /*                                                         */
*         /***********************************************************/
*                                                               04S0395
*         DO;                                                   04S0395
*           RFY                                                 04S0396
*             REG01 RSTD;           /* RESTRICT THE REGISTER THAT
*                                      WILL CONTAIN THE RESULT FROM
*                                      THE TRMOFLOC EXTERNAL ROUTINE */
*           CALL TRMOFLOC;          /* CHECK THE RESOURCE NAME       */
         L     @15,OPCTOFLK(,ROPCAVT)                           04S0397
         BALR  @14,@15                                          04S0397
*TRMOGOOD:                          /* (REG14+0)                     */
*           GO TO AFTERTRM;         /* CONTINUE THE VERIFICATION
*                                      PROCESSING                    */
         B     AFTERTRM                                         04S0398
*TRMOFBAD:                          /* (REG14+4)                     */
*           RETCODE=32;             /* SET THE RETURN CODE TO   04S0399
*                                      INDICATE THAT THE RESOURCE IS
*                                      UNKNOWN TO TCAM               */
TRMOFBAD LA    RETCODE,32                                       04S0399
*           REG00=16;               /* SET REGISTER ZERO (0) WITH THE
*                                      APPROPRIATE MESSAGE NUMBER    */
         LA    REG00,16                                         04S0400
*                                                               05S0401
*           /*********************************************************/
*           /*                                                       */
*           /* PROCESS.VERIFY.NAMERLN: THIS SEGMENT VERIFIES THAT THE*/
*           /* LINEGROUP NAME AND RELATIVE LINE NUMBER COMBINATION IS*/
*           /* KNOWN TO TCAM.                                        */
*           /*                                                       */
*           /*********************************************************/
*                                                               05S0401
*           DO;                                                 05S0401
*             RFY                                               05S0402
*               REG01 RSTD;         /* RESTRICT THE ARGUMENT REGISTER*/
*             REG01=OCRLN;          /* SET THE RELAIVE LINE NUMBER
*                                      ARGUMENT                      */
         SLR   REG01,REG01                                      05S0403
         IC    REG01,OCRLN(,ROPCE)                              05S0403
*             RFY                                               05S0404
*               REG00 RSTD;         /* RESTRICT THE REGISTER THAT
*                                      WILL CONTAIN THE RESULT FROM
*                                      THE DCBLOCAT EXTERNAL ROUTINE */
*             CALL DCBLOCAT;                                    05S0405
         L     @15,OPCDCBLK(,ROPCAVT)                           05S0405
         BALR  @14,@15                                          05S0405
*DCBLGOOD:                          /* (REG14+0)                     */
*             GO TO AFTERDCB;       /* CONTINUE THE VERIFICATION
*                                      PROCESSING                    */
         B     AFTERDCB                                         05S0406
*DCBLOBAD:                          /* (REG14+4)                     */
*             RETCODE=32;           /* SET THE RETURN CODE TO   05S0407
*                                      INDICATE THAT THE RESOURCE IS
*                                      NOT KNOWN TO TCAM             */
DCBLOBAD LA    RETCODE,32                                       05S0407
*             REG00=16;             /* SET REGISTER ZERO (0) WITH THE
*                                      APPROPRIATE MESSAGE NUMBER    */
         LA    REG00,16                                         05S0408
*             GO TO ENDNAMER;                                   05S0409
         B     ENDNAMER                                         05S0409
*AFTERDCB:                                                      05S0410
*             RETCODE=0;            /* GOOD RETURN           @YM08564*/
AFTERDCB SLR   RETCODE,RETCODE                                  05S0410
*             RTRM=REG00;           /* SET THE BASE POINTER FOR THE
*                                      RESOURCE ENTRY STRUCTURE      */
         LR    RTRM,REG00                                       05S0411
*             REG00=0;              /* INDICATE THAT NO MESSAGE 05S0412
*                                      NUMBER HAS BEEN SET           */
         SLR   REG00,REG00                                      05S0412
*           END;                                                05S0413
*           GO TO ENDNAMER;         /*                       @YM08564*/
         B     ENDNAMER                                         04S0414
*AFTERTRM:                                                      04S0415
*           RTRM=REG01;             /* SET THE BASE POINTER FOR THE
*                                      RESOURCE ENTRY STRUCTURE      */
AFTERTRM LR    RTRM,REG01                                       04S0415
*           RFY                                                 04S0416
*             REG01 UNRSTD;         /* RELEASE THE RESULT REGISTER   */
*           REG00=0;                /* INDICATE THAT NO MESSAGE 04S0417
*                                      NUMBER HAS BEEN SET           */
         SLR   REG00,REG00                                      04S0417
*ENDNAMER:                          /*                       @YM08564*/
*         END;                                                  04S0418
ENDNAMER DS    0H                                               04S0419
*         IF RETCODE=0              /* NO ERRORS HAVE BEEN FOUND     */
*           THEN                    /* VERIFY THAT THE RESOURCE TYPE
*                                      CAN BE HELD AND CONTINUE 03S0419
*                                      PROCESSING                    */
         LTR   RETCODE,RETCODE                                  03S0419
         BNZ   @RF00419                                         03S0419
*                                                               04S0420
*           /*********************************************************/
*           /*                                                       */
*           /* PROCESS.VERIFY.TYPE: THIS SEGMENT VERIFIES THAT THE   */
*           /* RESOURCE TYPE CAN BE HELD.                            */
*           /*                                                       */
*           /*********************************************************/
*                                                               04S0420
*           DO;                                                 04S0420
*             IF TRMLIST=ON         /* LIST ENTRY                    */
*                 ×TRMPROC=ON       /* PROCESS ENTRY                 */
*                 ×TRMPREF=ON&(TRMTYPE=TRMGRP×TRMTYPE=TRMLNCP×TRMTYPE=
*                 TRMSSCP×TRMTYPE=TRMNSDLC)/* IF TYRMTYPE IS GROUP,
*                                      NCP,SSCP,BSC LINE     @YM06873*/
*                 ×AVTAS=0          /* HOLD PROCESSING IS NOT IN THE
*                                      SYSTEM                        */
*               THEN                /* INDICATE THAT AN ERROR HAS
*                                      BEEN DETECTED                 */
         TM    TRMLIST(RTRM),B'01100000'                        04S0421
         BNZ   @RT00421                                         04S0421
         TM    TRMPREF(RTRM),B'00010000'                        04S0421
         BNO   @GL00010                                         04S0421
         LR    @11,RTRM                                         04S0421
         SL    @11,@CF01880                                     04S0421
         CLI   TRMTYPE(@11),X'00'                               04S0421
         BE    @RT00421                                         04S0421
         CLI   TRMTYPE(@11),X'0F'                               04S0421
         BE    @RT00421                                         04S0421
         CLI   TRMTYPE(@11),X'FF'                               04S0421
         BE    @RT00421                                         04S0421
         CLI   TRMTYPE(@11),X'20'                               04S0421
         BE    @RT00421                                         04S0421
@GL00010 L     @11,AVTAS(,RAVT)                                 04S0421
         LTR   @11,@11                                          04S0421
         BNZ   @RF00421                                         04S0421
@RT00421 DS    0H                                               04S0422
*               DO;                                             04S0422
*                 RETCODE=4;        /* SET THE RETURN CODE           */
         LA    RETCODE,4                                        04S0423
*                 REG00=60;         /* SET REGISTER ZERO (0) WITH THE
*                                      APPROPRIATE MESSAGE NUMBER    */
         LA    REG00,60                                         04S0424
*               END;                                            04S0425
*             ELSE                  /* CONTINUE THE VERIFICATION     */
*               IF TRMPREF=OFF&DCTUMASK=ON/* GENERAL POLL            */
*                   ×TRMPREF=ON&TRMTYPE=TRMCLUST/* CLUSTER           */
*                 THEN              /* INDICATE AN ERROR HAS BEEN
