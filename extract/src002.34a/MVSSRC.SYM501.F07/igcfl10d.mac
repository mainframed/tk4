         TITLE 'IGCFL10D - ACTIVATE NCP SYNCH. PROCESSOR               *
                        '
IGCFL10D CSECT ,                                                01S0001
@PROLOG  BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                                   /*
IGCFL10D IEDHJN AFTERHJN,HJN
*                                                               01S0487
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZATION                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0487
*   ROPCAVT=RPARM;                  /* SET POINTER TO OP. CTL. AVT   */
         LR    ROPCAVT,RPARM                                    01S0487
*   ROPCE=OPCCOPCE;                 /* SET POINTER TO CURRENT OPCE   */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         01S0488
*   RAVT=OPCAVTPT;                  /* SET POINTER TO TCAM AVT       */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          01S0489
*   RSAVE=ADDR(OPCSAVE);            /* SET POINTER TO SAVE AREA      */
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          01S0490
*   RWRKAREA=OPCWORK;               /* OP. CTL. WORKAREA BASE - USED
*                                      FOR TEMPORARY STORAGE         */
*                                                               01S0491
         L     RWRKAREA,OPCWORK(,ROPCAVT)                       01S0491
*   /*****************************************************************/
*   /*                                                               */
*   /* BASE COMPILER GENERATED DSECT (CREATED BY REENTRANT OPTION)   */
*   /* FOR TEMPORARY STORAGE ON WORK AREA BASE.                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0492
*   GEN REFS(RWRKAREA);                                         01S0492
*                        /*
         USING @DATD,RWRKAREA    */
*   FLAGS=''B;                      /* INIT FLAGS FIELD              */
         MVI   FLAGS,X'00'                                      01S0493
*   OPCMODID=CNTLMOD;               /* XCTL IS TO CONTROL MOD. UNLESS
*                                      PROCESSING DETERMINES THAT
*                                      ANOTHER SHOULD BE USED        */
*                                                               01S0494
         MVC   OPCMODID(2,ROPCAVT),CNTLMOD                      01S0494
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE 50% OF TCAM'S LINE UNITS                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0495
*   HALFUNIT=AVTNOLBF;              /* # OF TCAM'S LINE UNITS        */
         LH    HALFUNIT,AVTNOLBF(,RAVT)                         01S0495
*   SRL(HALFUNIT,1);                /* DIVIDE BY TWO FOR 50%         */
*                                                               01S0496
         SRL   HALFUNIT,1                                       01S0496
*   /*****************************************************************/
*   /*                                                               */
*   /* OBTAIN ADDR. OF NEG. PREFIX OF ORIG. OPCE                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0497
*   ORGNOPCE=OCPTRSAV-LENGTH(IEDQOPCN);/* OBTAIN THE ADDR. OF THE
*                                      START OF THE NEG. PREFIX IN
*                                      THE ORIG. OPCE                */
         L     ORGNOPCE,OCPTRSAV(,ROPCE)                        01S0497
         SL    ORGNOPCE,@CF00045                                01S0497
*                                                               01S0498
*   /*****************************************************************/
*   /*                                                               */
*   /* MAIN PROCDURE                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0498
*   IF OCREDRVE=ON THEN             /* IF THIS IS A REDRIVE OPCE     */
*                                                               01S0498
         TM    OCREDRVE(ROPCE),B'00010000'                      01S0498
         BNO   @RF00498                                         01S0498
*     /***************************************************************/
*     /*                                                             */
*     /* THIS IS A REDRIVE OPCE WITH ATTACHED UNITS.                 */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0499
*     CALL REDRIVE;                                             01S0499
         BAL   @14,REDRIVE                                      01S0499
*   ELSE                                                        01S0500
*     DO;                                                       01S0500
*                                                               01S0500
         B     @RC00498                                         01S0500
@RF00498 DS    0H                                               01S0501
*       /*************************************************************/
*       /*                                                           */
*       /* THIS IS A RESPONSE OPCE OR A REQUEST FROM IGCFL10D.       */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0501
*       ORGNOPCE->OCRESPCT=ORGNOPCE->OCRESPCT-1;/* DECREMENT RSP. CT.*/
         LH    @14,OCRESPCT(,ORGNOPCE)                          01S0501
         N     @14,@CF01733                                     01S0501
         BCTR  @14,0                                            01S0501
         STH   @14,OCRESPCT(,ORGNOPCE)                          01S0501
*                                                               02S0502
*       /*************************************************************/
*       /*                                                           */
*       /* ----- THIS SEGMENT PROCESSES RESPONSE OPCE'S -----        */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0502
*       RPARM=OCTRMTBL;             /* OBTAIN RESOURCE'S ....        */
         LH    RPARM,OCTRMTBL(,ROPCE)                           02S0502
         N     RPARM,@CF01733                                   02S0502
*       CALL IEDQTNT;               /* .... TTE ADDR.                */
         L     @15,AVTRNMPT(,RAVT)                              02S0503
         BALR  @14,@15                                          02S0503
*       RTRM=RPARM;                 /* COPY TTE ADDR.                */
         LR    RTRM,RPARM                                       02S0504
*       IF OCSWITCH=OCDACTLK THEN   /* IF DACTLINK RESPONSE          */
         CLI   OCSWITCH(ROPCE),X'60'                            02S0505
         BNE   @RF00505                                         02S0505
*         DO;                                                   02S0506
*                                                               03S0507
*           /*********************************************************/
*           /*                                                       */
*           /* ----- THIS SEGMENT PROCESSED DACTLINK RESPONSE OPCE'S.*/
*           /* IF THE RESPONSE WAS FOR A BSC-SS LINE, THE OPCE IS    */
*           /* REFORMATTED INTO A SWITCH TO EP OR NCP DEPENDING ON   */
*           /* THE LINE TTE. -----                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                               03S0507
*           IF TRMTYPE=TRMSDLC THEN                             03S0507
         LR    @14,RTRM                                         03S0507
         AL    @14,@CF01150                                     03S0507
         CLI   TRMTYPE-11(@14),X'21'                            03S0507
         BE    @RT00507                                         03S0507
*             ;                     /* DO NOTHING                    */
*           ELSE                                                03S0509
*             DO;                   /* BUILD 'SWITCH' COMMAND        */
*               ORGNOPCE->OCRESPCT=ORGNOPCE->OCRESPCT+1;/* INCREMENT
*                                      RESP. COUNT                   */
         LA    @14,1                                            03S0510
         LH    @06,OCRESPCT(,ORGNOPCE)                          03S0510
         N     @06,@CF01733                                     03S0510
         ALR   @14,@06                                          03S0510
         STH   @14,OCRESPCT(,ORGNOPCE)                          03S0510
*               TRMTBLSV=OCTRMTBL;  /* SAVE ....                     */
         LH    @14,OCTRMTBL(,ROPCE)                             03S0511
         N     @14,@CF01733                                     03S0511
         STH   @14,TRMTBLSV                                     03S0511
*               UNITSV=OCUNIT;      /* ....KEY ....                  */
         L     @14,OCUNIT(,ROPCE)                               03S0512
         ST    @14,UNITSV                                       03S0512
*               PTRSAVSV=OCPTRSAV;  /* .... FIELDS                   */
         L     PTRSAVSV,OCPTRSAV(,ROPCE)                        03S0513
*               IEDQOPCN=''B;       /* CLEAR PREFIX                  */
         LR    @06,ROPCE                                        03S0514
         AL    @06,@CF01734                                     03S0514
         XC    IEDQOPCN(8,@06),IEDQOPCN(@06)                    03S0514
*               IEDQOPCE=''B;       /* CLEAR OPCE                    */
         XC    IEDQOPCE(56,ROPCE),IEDQOPCE(ROPCE)               03S0515
*               OCTRMTBL=TRMTBLSV;  /* RESTORE ....                  */
         LH    @06,TRMTBLSV                                     03S0516
         N     @06,@CF01733                                     03S0516
         STH   @06,OCTRMTBL(,ROPCE)                             03S0516
*               OCUNIT=UNITSV;      /* .... KEY ....                 */
         ST    @14,OCUNIT(,ROPCE)                               03S0517
*               OCPTRSAV=PTRSAVSV;  /* .... FIELDS                   */
         ST    PTRSAVSV,OCPTRSAV(,ROPCE)                        03S0518
*               OCWTG=THISMOD;      /* SET TO RETURN TO THIS MOD.    */
         MVC   OCWTG(2,ROPCE),THISMOD                           03S0519
*               OCELMPRI=PRIHIFLG;  /* SET PRIORITY                  */
         MVI   OCELMPRI(ROPCE),X'E9'                            03S0520
*               OCPRIFLG=OPRA0;     /* SET FLAG                      */
         MVI   OCPRIFLG(ROPCE),X'01'                            03S0521
*               OCACTREQ=ON;        /* SET A/D REQUEST FLAG          */
         OI    OCACTREQ(ROPCE),B'00001000'                      03S0522
*               IF TRMEPMOD=ON THEN /* IF TERM. GEN'D AS EP          */
         LR    @14,RTRM                                         03S0523
         AL    @14,@CF01736                                     03S0523
         TM    TRMEPMOD-1(@14),B'10000000'                      03S0523
         BNO   @RF00523                                         03S0523
*                 OCSWITCH=OCSWEP;  /* SET TO SWITCH TO EP           */
         MVI   OCSWITCH(ROPCE),X'C0'                            03S0524
*               ELSE                                            03S0525
*                 OCSWITCH=OCSWNCP; /* SET TO SWITCH TO NCP          */
         B     @RC00523                                         03S0525
@RF00523 MVI   OCSWITCH(ROPCE),X'D0'                            03S0525
*               OPCMODID=SSCPMOD;   /* SET TO XCTL TO SSCP           */
@RC00523 MVC   OPCMODID(2,ROPCAVT),SSCPMOD                      03S0526
*               FRERUFLG=ON;        /* SET FLAG TO FREE ONLY RU      */
         OI    FRERUFLG,B'01000000'                             03S0527
*             END;                                              03S0528
*           FREUTFLG=ON;            /* SET FLAG TO FREE UNITS        */
@RT00507 OI    FREUTFLG,B'10000000'                             03S0529
*         END;                                                  02S0530
*       ELSE                                                    02S0531
*         IF OCSWITCH=OCSWEP×OCSWITCH=OCSWNCP THEN/* IF SWITCH  02S0531
*                                      RESPONSE                      */
         B     @RC00505                                         02S0531
@RF00505 CLI   OCSWITCH(ROPCE),X'C0'                            02S0531
         BE    @RT00531                                         02S0531
         CLI   OCSWITCH(ROPCE),X'D0'                            02S0531
         BNE   @RF00531                                         02S0531
@RT00531 DS    0H                                               02S0532
*           DO;                                                 02S0532
*                                                               03S0533
*             /*******************************************************/
*             /*                                                     */
*             /* ----- THIS SEGMENT PROCESSES 'SWITCH' RESPONSES. IF */
*             /* THE NCP COULD NOT ACCOMPLISH THE REQUEST, TRMEPMOD  */
*             /* IS RESET. -----                                     */
*             /*                                                     */
*             /*******************************************************/
*                                                               03S0533
*             IF OCPARM=OCRCBNEG THEN/* IF BAD RESPONSE WITH SENSE   */
         CLC   OCPARM(2,ROPCE),@CH01146                         03S0533
         BNE   @RF00533                                         03S0533
*               IF OCSWITCH=OCSWEP THEN/* SWITCH TO EP RESPONSE      */
         CLI   OCSWITCH(ROPCE),X'C0'                            03S0534
         BNE   @RF00534                                         03S0534
*                 IF SNSMAJ=SNSRQREJ&SNSMIN=SNSPRCNS THEN/* IF LINE
*                                      NOT SWITCHABLE                */
         LA    @14,12                                           03S0535
         AL    @14,OCUNIT(,ROPCE)                               03S0535
         CLI   SNSMAJ(@14),X'08'                                03S0535
         BNE   @RF00535                                         03S0535
         CLI   SNSMIN(@14),X'0C'                                03S0535
         BNE   @RF00535                                         03S0535
*                   TRMEPMOD=OFF;   /* SET AS NCP DEVICE             */
         LR    @14,RTRM                                         03S0536
         AL    @14,@CF01736                                     03S0536
         NI    TRMEPMOD-1(@14),B'01111111'                      03S0536
*                 ELSE                                          03S0537
*                   ;                                           03S0537
@RF00535 DS    0H                                               03S0538
*               ELSE                                            03S0538
*                 IF SNSMAJ=SNSRQREJ&SNSMIN=SNSMODIN THEN/* IF I/0 IN
*                                      PROGRESS                      */
         B     @RC00534                                         03S0538
@RF00534 LA    @14,12                                           03S0538
         AL    @14,OCUNIT(,ROPCE)                               03S0538
         CLI   SNSMAJ(@14),X'08'                                03S0538
         BNE   @RF00538                                         03S0538
         CLI   SNSMIN(@14),X'09'                                03S0538
         BNE   @RF00538                                         03S0538
*                   TRMEPMOD=ON;    /* SET AS EP DEVICE              */
         LR    @14,RTRM                                         03S0539
         AL    @14,@CF01736                                     03S0539
         OI    TRMEPMOD-1(@14),B'10000000'                      03S0539
*                 ELSE                                          03S0540
*                   ;                                           03S0540
@RF00538 DS    0H                                               03S0541
*             ELSE                                              03S0541
*               ;                                               03S0541
@RF00533 DS    0H                                               03S0542
*             FREUTFLG=ON;          /* SET TO FREE UNITS             */
@RC00533 OI    FREUTFLG,B'10000000'                             03S0542
*           END;                                                02S0543
*         ELSE                                                  02S0544
*           IF OCSWITCH=OCOPYDSM THEN/* COPY DESTINATION MODE   02S0544
*                                      RESPONSE                      */
         B     @RC00531                                         02S0544
@RF00531 CLI   OCSWITCH(ROPCE),X'E1'                            02S0544
         BNE   @RF00544                                         02S0544
*             DO;                                               02S0545
*                                                               03S0546
*               /*****************************************************/
*               /*                                                   */
*               /* ----- THIS SEGMENT PERFORMS 'COPY DESTINATION     */
*               /* MODE' PROCESSING. THE MODE BYTES CONTAINED IN THE */
*               /* 1ST TWO BYTES FOLLOWING THE BDU ARE PLACE IN THE  */
*               /* DEVICE DEPENDENT FIELD EXCEPT FOR THREE BITS. THE */
*               /* MODCINTO AND MODCINTE ARE MAINTAINED AS SPECIFIED */
*               /* IN THE DEVICE DEPENDENT FIELD AND THE MODCMNTR IS */
*               /* SET TO 0. IF THE END RESULT IS NOT THE SAME AS THE*/
*               /* BYTES IN THE RU, A REPLACE (SET) DESTINATION MODE */
*               /* IS PERFORMED. -----                               */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0546
*               IF BDUSYRSP=GOODRSP THEN/* IF A GOOD RSP. TO COPY    */
         L     @14,OCUNIT(,ROPCE)                               03S0546
         CLI   BDUSYRSP+12(@14),X'60'                           03S0546
         BNE   @RF00546                                         03S0546
*                 DO;                                           03S0547
*                   RZERO=TRMRNTMK; /* SET MASK FOR MODE BYTES TO
*                                      ....                  @YM05695*/
         LA    RZERO,16                                         03S0548
*                   RPARM=RTRM;     /* .... OBTAIN ADDR. OF ....     */
         LR    RPARM,RTRM                                       03S0549
*                   CALL IEDQTL;    /* .... MODE BYTES IN DEV. DEP.
*                                      FIELDS                        */
         L     @15,AVTDDFT(,RAVT)                               03S0550
         BALR  @14,@15                                          03S0550
*                   DEVDPADR=RTNCODE;/* SET BASE FOR MODE FIELD 03S0551
*                                                            @YM05740*/
         ST    RTNCODE,DEVDPADR                                 03S0551
*                   RESULT1=MODCUR&MASK1;/* BITS THAT MUST BE ON ARE
*                                      1 IN RESULT1                  */
         L     @14,DEVDPADR                                     03S0552
         MVC   RESULT1(2),MODCUR(@14)                           03S0552
         NC    RESULT1(2),@CB01580                              03S0552
*                   RESULT2=PRUDATA(7:8)&MASK2;/* RECORDS SETTINGS OF
*                                      3 CRITICAL BITS IN RU @YM05695*/
         L     @11,OCUNIT(,ROPCE)                               03S0553
         MVC   RESULT2(2),PRUDATA+6(@11)                        03S0553
         NC    RESULT2(2),@CB01644                              03S0553
*                   IF RESULT1=RESULT2 THEN/* IF 3 BITS IN RU ALREADY
*                                      SET OK                        */
         CLC   RESULT1(2),RESULT2                               03S0554
         BNE   @RF00554                                         03S0554
*                     DO;           /* THEN REPLACE NOT NEEDED       */
*                       MODCUR=PRUDATA(7:8);/* SET UP CURRENT MODE
*                                      BYTES FIELD           @YM05695*/
         MVC   MODCUR(2,@14),PRUDATA+6(@11)                     03S0556
*                       MODNEW=MODCUR;/* COPY TO NEW MODE BYTES FIELD*/
         MVC   MODNEW(2,@14),MODCUR(@14)                        03S0557
*                       FREUTFLG=ON;/* SET FLAG TO FREE UNITS        */
         OI    FREUTFLG,B'10000000'                             03S0558
*                     END;                                      03S0559
*                   ELSE            /* REPLACE MUST BE DONE          */
*                     DO;                                       03S0560
         B     @RC00554                                         03S0560
@RF00554 DS    0H                                               03S0561
*                       MODCUR=PRUDATA(7:8)&MASK3;/* MOVE RU MODE
*                                      BYTES INTO DEV. DEP. FIELD
*                                      EXCEPT THAT THE 3 CRITICAL
*                                      BITS ARE ZEROED       @YM05695*/
         L     @14,DEVDPADR                                     03S0561
         L     @06,OCUNIT(,ROPCE)                               03S0561
         MVC   MODCUR(2,@14),PRUDATA+6(@06)                     03S0561
         NC    MODCUR(2,@14),@CB01646                           03S0561
*                       MODCUR=MODCUR×RESULT1;/* SET CRITICAL BITS AS
*                                      THEY ARE SUPPOSED TO BE       */
         OC    MODCUR(2,@14),RESULT1                            03S0562
*                       MODNEW=MODCUR;/* COPY INTO NEW MODE BYTES
*                                      FIELD                         */
         MVC   MODNEW(2,@14),MODCUR(@14)                        03S0563
*                       PRUDATA(7:8)=MODCUR;/* SET DATA IN RU   03S0564
*                                                            @YM05695*/
         MVC   PRUDATA+6(2,@06),MODCUR(@14)                     03S0564
*                                                               04S0565
*                       /*********************************************/
*                       /*                                           */
*                       /* --- THIS SEGMENT BUILDS REPLACE DEV.      */
*                       /* SESSION INFO. OPCE- PACKED RU PAIRS ----  */
*                       /*                                           */
*                       /*********************************************/
*                                                               04S0565
*                       TRMTBLSV=OCTRMTBL;/* SAVE OCTRMTBL           */
         LH    @14,OCTRMTBL(,ROPCE)                             04S0565
         N     @14,@CF01733                                     04S0565
         STH   @14,TRMTBLSV                                     04S0565
*                       PTRSAVSV=OCPTRSAV;/* SAVE                    */
         L     PTRSAVSV,OCPTRSAV(,ROPCE)                        04S0566
*                       UNITSV=OCUNIT;/* SAVE                        */
         ST    @06,UNITSV                                       04S0567
*                       IEDQOPCN=''B;/* CLEAR NEG. PREFIX            */
         LA    @14,8                                            04S0568
         LR    @05,ROPCE                                        04S0568
         SLR   @05,@14                                          04S0568
         XC    IEDQOPCN(8,@05),IEDQOPCN(@05)                    04S0568
*                       IEDQOPCE=''B;/* CLEAR OPCE                   */
         XC    IEDQOPCE(56,ROPCE),IEDQOPCE(ROPCE)               04S0569
*                       OCTRMTBL=TRMTBLSV;/* RESOURCE TTCIN          */
         LH    @05,TRMTBLSV                                     04S0570
         N     @05,@CF01733                                     04S0570
         STH   @05,OCTRMTBL(,ROPCE)                             04S0570
*                       OCPTRSAV=PTRSAVSV;/* POINTER TO ORIG OPCE    */
         ST    PTRSAVSV,OCPTRSAV(,ROPCE)                        04S0571
*                       OCUNIT=UNITSV;/* POINTER TO ATTACHED PACKED
*                                      RU                            */
         ST    @06,OCUNIT(,ROPCE)                               04S0572
*                       OCSWITCH=OCRPLDSM;/* REPLACE CMND.           */
         MVI   OCSWITCH(ROPCE),X'E2'                            04S0573
*                       OCELMPRI=PRIMEDFG;                      04S0574
         MVI   OCELMPRI(ROPCE),X'DF'                            04S0574
*                       OCPRIFLG=OPR02;                         04S0575
         MVI   OCPRIFLG(ROPCE),X'08'                            04S0575
*                       OCWTG=THISMOD;/* SET TO RETURN TO THIS MODULE*/
         MVC   OCWTG(2,ROPCE),THISMOD                           04S0576
*                       OCACTREQ=ON;/* SET FOR ACTIVATE REQUEST      */
         OI    OCACTREQ(ROPCE),B'00001000'                      04S0577
*                       PRUTTCIN=OCTRMTBL;/* COPY TTCIN      @YM06512*/
         LH    @11,OCTRMTBL(,ROPCE)                             04S0578
         N     @11,@CF01733                                     04S0578
         STH   @11,PRUTTCIN(,@06)                               04S0578
*                       PRURHTH(1:3)=TRHFID0;/* SET FID0 RH FLAGS    */
         LCR   @14,@14                                          04S0579
         ALR   @14,@06                                          04S0579
         MVC   PRURHTH(3,@14),@CB01384                          04S0579
*                       PRURHTH(4:4)=TTH0BYT0;/* SET FID0 TH FLAGS   */
         MVI   PRURHTH+3(@14),X'0C'                             04S0580
*                       BDUMODIF=CD0CSDM;/* BDU MODIFIER FOR    04S0581
*                                      "REPLACE"                     */
         AL    @06,@CF00239                                     04S0581
         MVI   BDUMODIF(@06),X'9A'                              04S0581
*                       BDUFNFLG=''B;/* CLEAR FUNCTION FLAGS         */
         MVI   BDUFNFLG(@06),X'00'                              04S0582
*                       BDUHEADR=ON;/* SET HEADER FLAG       @YM06147*/
         OI    BDUHEADR(@06),B'01000000'                        04S0583
*                       BDUBDUFG=''B;/* CLEAR BDU FLAGS              */
         MVI   BDUBDUFG(@06),X'00'                              04S0584
*                       BDUSYRSP=''B;/* CLEAR SYSTEM RESPONSE        */
         MVI   BDUSYRSP(@06),X'00'                              04S0585
*                       BDUEXRSP=''B;/* CLEAR EXTENDED RESPONSE      */
         MVI   BDUEXRSP(@06),X'00'                              04S0586
*                       ORGNOPCE->OCRESPCT=ORGNOPCE->OCRESPCT+1;/*
*                                      UPDATE RESPONSE COUNT         */
         LA    @14,1                                            04S0587
         LH    @11,OCRESPCT(,ORGNOPCE)                          04S0587
         N     @11,@CF01733                                     04S0587
         ALR   @14,@11                                          04S0587
         STH   @14,OCRESPCT(,ORGNOPCE)                          04S0587
*                       OPCMODID=RUDSPMOD;/* SET TO XCTL TO RU  04S0588
*                                      DISPATCHER                    */
         MVC   OPCMODID(2,ROPCAVT),RUDSPMOD                     04S0588
*                     END;                                      03S0589
*                 END;                                          03S0590
*               ELSE                                            03S0591
*                 FREUTFLG=ON;      /* SET FLAG TO FREE UNITS   03S0591
*                                                            @YM06046*/
         B     @RC00546                                         03S0591
@RF00546 OI    FREUTFLG,B'10000000'                             03S0591
*             END;                                              02S0592
*           ELSE                    /* MUST BE REPLACE DEST. MODE
*                                      RESP.                         */
*             FREUTFLG=ON;          /* SET 'FREE UNIT' FLAG          */
         B     @RC00544                                         02S0593
@RF00544 OI    FREUTFLG,B'10000000'                             02S0593
*       IF FREUTFLG=ON THEN         /* IF RESPONSE PROCESSING HAS
*                                      INDICATED THAT UNITS SHOULD BE
*                                      FREED                         */
@RC00544 DS    0H                                               02S0594
@RC00531 DS    0H                                               02S0594
@RC00505 TM    FREUTFLG,B'10000000'                             02S0594
         BNO   @RF00594                                         02S0594
*                                                               03S0595
*         /***********************************************************/
*         /*                                                         */
*         /* ----- THIS SEGMENT FREES UNITS -----                    */
*         /*                                                         */
*         /***********************************************************/
*                                                               03S0595
*         IF ORGNOPCE->OCRESPCT=0×FRERUFLG=ON THEN/* IF RESPONSE
*                                      COUNT IS 0 OR ONLY AN ATTACHED
*                                      RU IS TO BE FREED             */
         LH    @14,OCRESPCT(,ORGNOPCE)                          03S0595
         N     @14,@CF01733                                     03S0595
         LTR   @14,@14                                          03S0595
         BZ    @RT00595                                         03S0595
         TM    FRERUFLG,B'01000000'                             03S0595
         BNO   @RF00595                                         03S0595
@RT00595 DS    0H                                               03S0596
