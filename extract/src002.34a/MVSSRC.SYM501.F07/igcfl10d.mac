*/********************************************************************/
*/*              REDRIVE SUBROUTINE                                  */
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE PERFORMS THE REDRIVE PROCESSING:                    */
*/* 1. THE RVT IS SCANNED STARTING WITH THE ENTRY ADDR. (ENTRYRVT)   */
*/*    CONTAINED IN OCMODNME OF THE REDRIVE OPCE. ON THE FIRST PASS, */
*/*    THIS IS SET UP WITH THE 2ND ENTRY IN THE RVT.                 */
*/* 2. USING RVTINDEX, THE TTE IS OBTAINED AND THE TERMINAL IS       */
*/*    VERIFIED AS A BSC-SS TERM.                                    */
*/* 3. IF SO, A COPY DESTINATION MODE OPCE IS BUILT IN               */
*/*    THE UNIT POINTED TO BY OCUNIT OF THE REDRIVE OPCE. THE NEXT   */
*/*    UNIT IS USED TO BUILD THE PACKED RU.  OCUNIT OF THE REDRIVE   */
*/*    OPCE IS SET TO THE ADDR. OF THE UNIT FOLLOWING THE SECOND     */
*/*    UNIT.                                                         */
*/* 4. THE OPCE-RU PAIR JUST BUILT IS PUT INTO THE CHAIN FOR THE     */
*/*    POST TO AVTOPCOB.                                             */
*/* 5. IF THERE ARE 2 OR MORE UNITS LEFT, THE RVT SCAN IS CONTINED   */
*/*    AND STEPS 2-4 REPEATED. IF NOT, AND THERE ARE STILL MORE      */
*/*    COMMANDS TO BUILD, THE REDRIVE IS PLACE ON THE BUFFER WAIT    */
*/*    CHAIN.  IF ALL COMMANDS HAVE BEEN BUILT, THE REDRIVE OPCE     */
*/*    IS FREED.                                                     */
*/*                                                                  */
*/********************************************************************/
*                                                               01S0705
*REDRIVE:                                                       01S0705
*   PROC OPTIONS(RETREG,SAVE(14));                              01S0705
REDRIVE  ST    @14,@SA00002                                     01S0705
*   IF OCBUFGOT>=2 THEN             /* IF TWO OR MORE UNITS ATTACHED */
         CLC   OCBUFGOT(2,ROPCE),@CH00131                       01S0706
         BL    @RF00706                                         01S0706
*     DO;                                                       01S0707
*                                                               01S0707
*       /*************************************************************/
*       /*                                                           */
*       /* THERE ARE ENOUGH UNITS                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0708
*       OPCAQWD1=OCUNIT;            /* SET FIRST WORD OF AQCTL PARM.
*                                      LIST TO POINT TO FIRST UNIT   */
         L     @14,OCUNIT(,ROPCE)                               01S0708
         ST    @14,OPCAQWD1(,ROPCAVT)                           01S0708
*       DO UNTIL(OCBUFGOT<2);       /* LOOP WHILE ENOUGH UNITS REMAIN*/
@DL00709 DS    0H                                               01S0710
*         RFY                                                   01S0710
*           IEDQOPCE BASED(UNITPTR);/* REDEFINE BASE FOR LOOP        */
*         UNITPTR=ROPCE->OCUNIT;    /* POINT TO FIRST UNIT           */
         L     UNITPTR,OCUNIT(,ROPCE)                           01S0711
*         ROPCE->OCUNIT=OCUNIT->OCUNIT;/* DELINK FIRST AND SECOND
*                                      UNIT                          */
         L     @14,OCUNIT(,UNITPTR)                             01S0712
         L     @11,OCUNIT(,@14)                                 01S0712
         ST    @11,OCUNIT(,ROPCE)                               01S0712
*         OCELEM=ROPCE->OCUNIT;     /* MOVE LINK FOR POST TO AVTOPCOB*/
         ST    @11,OCELEM(,UNITPTR)                             01S0713
*         PRUTIC=PRUINTIC;          /* SET TIC FIELD OF RU INVALID   */
*                                                               01S0714
         MVC   PRUTIC(4,@14),@CB01351                           01S0714
*         /***********************************************************/
*         /*                                                         */
*         /* SET COMMON FIELDS IN OPCE                               */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0715
*         OCPTRSAV=ROPCE->OCPTRSAV; /* SET ADDR. OF ORIG. OPCE       */
         L     @14,OCPTRSAV(,ROPCE)                             01S0715
         ST    @14,OCPTRSAV(,UNITPTR)                           01S0715
*         OCSWITCH=OCOPYDSM;        /* SET COMMAND CODE              */
         MVI   OCSWITCH(UNITPTR),X'E1'                          01S0716
*         OCLINK=ADDR(AVTOPCOB);    /* SET QCB ADDR.                 */
         LA    @14,AVTOPCOB(,RAVT)                              01S0717
         ST    @14,OCLINK(,UNITPTR)                             01S0717
*         OCELMPRI=PRIMEDFG;        /* SET UP POST PRIORITY          */
         MVI   OCELMPRI(UNITPTR),X'DF'                          01S0718
*         OCPRIFLG=OPR02;           /* SET TO ROUTE TO RU DISPATCHER */
         MVI   OCPRIFLG(UNITPTR),X'08'                          01S0719
*         OCWTG=THISMOD;            /* SET TO RETURN TO THIS MOD.    */
         MVC   OCWTG(2,UNITPTR),THISMOD                         01S0720
*         OCACTREQ=ON;              /* ACTIVATE REQUEST              */
         OI    OCACTREQ(UNITPTR),B'00001000'                    01S0721
*         OPCAQWD2=UNITPTR;         /* 'END OF CHAIN' ADDR. IN AQCTL
*                                      PARM. LIST                    */
*                                                               01S0722
         ST    UNITPTR,OPCAQWD2(,ROPCAVT)                       01S0722
*         /***********************************************************/
*         /*                                                         */
*         /* SCAN RVT UNTIL THE TTE OF A BSC-SS TERM IS FOUND. FINISH*/
*         /* BUILDING OPCE AND BUILD PACKED RU.                      */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0723
*                                                               02S0723
*         /***********************************************************/
*         /*                                                         */
*         /* ------- THIS SEGMENT BUILDS "COPY DESTINATION MODE"     */
*         /* OPCE- PACKED RU PAIRS. -----                            */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0723
*         RFY                                                   02S0723
*           IEDQTRM BASED(RPARM);   /* SET TTE BASE                  */
*         RFY                                                   02S0724
*           IEDQOPCE BASED(ROPCE);  /* SET BASE AS REDRIVE OPCE      */
*         DO UNTIL(BUILTFLG=ON);    /* LOOP THRU RVT UNTIL PRE-SNA
*                                      TERM. FOUND                   */
*                                                               02S0725
@DL00725 DS    0H                                               02S0726
*           /*********************************************************/
*           /*                                                       */
*           /* OBTAIN NEXT RVT ENTRY ADDR. FROM REDRIVE OPCE AND SET */
*           /* OCTRMTBL OF OPCE BEING BUILT TO TTCIN CONTAINED IN RVT*/
*           /* ENTRY.                                                */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0726
*           UNITPTR->OCTRMTBL=RVTINDEX;/* SET TTCIN FOR OPCE BEING
*                                      BUILT                         */
         L     @14,ENTRYRVT(,ROPCE)                             02S0726
         LH    @11,RVTINDEX(,@14)                               02S0726
         STH   @11,OCTRMTBL(,UNITPTR)                           02S0726
*           ENTRYRVT=ENTRYRVT+LENGTH(RVTINDEX);/* UPDATE RVT ENTRY
*                                      PTR. IN R/D OPCE TO POINT TO
*                                      NEXT ENTRY                    */
         AL    @14,@CF00131                                     02S0727
         ST    @14,ENTRYRVT(,ROPCE)                             02S0727
*           RPARM=UNITPTR->OCTRMTBL;/* OBTAIN ADDR. OF ....          */
         LR    RPARM,@11                                        02S0728
*           CALL IEDQTNT;           /* .... TTE                      */
         L     @15,AVTRNMPT(,RAVT)                              02S0729
         BALR  @14,@15                                          02S0729
*           IF TRMTYPE=TRMPSNA THEN /* IF PRE-SNA TERM.              */
         LR    @14,RPARM                                        02S0730
         AL    @14,@CF01150                                     02S0730
         CLI   TRMTYPE-11(@14),X'4F'                            02S0730
         BNE   @RF00730                                         02S0730
*             DO;                                               02S0731
*               RFY                                             02S0732
*                 IEDQOPCE BASED(UNITPTR);/* RESET BASE AS OPCE BEING
*                                      BUILT                         */
*                                                               02S0732
*               /*****************************************************/
*               /*                                                   */
*               /* BUILD PACKED RU                                   */
*               /*                                                   */
*               /*****************************************************/
*                                                               02S0733
*                                                               03S0733
*               /*****************************************************/
*               /*                                                   */
*               /* ----- THIS SEGMENT BUILDS THE COPY DEST. MODE     */
*               /* PACKED RU----                                     */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0733
*               PRUTTCIN=OCTRMTBL;  /* SET RESOURCE TTCIN            */
         L     @14,OCUNIT(,UNITPTR)                             03S0733
         LH    @11,OCTRMTBL(,UNITPTR)                           03S0733
         N     @11,@CF01733                                     03S0733
         STH   @11,PRUTTCIN(,@14)                               03S0733
*               PRUDATCT=LENGTH(IEDBDU);/* DATA LENGTH IS BDU LENGTH */
         MVC   PRUDATCT(2,@14),@CH00071                         03S0734
*               BDUCMND=CD0CTRL;    /* CONTROL BDU                   */
         LA    @11,12                                           03S0735
         ALR   @11,@14                                          03S0735
         MVI   BDUCMND(@11),X'08'                               03S0735
*               BDUMODIF=CD0CCDM;   /* COPY DEST. CODE MODIFIER      */
         MVI   BDUMODIF(@11),X'18'                              03S0736
*               BDUHEADR=ON;        /* TURN ON HEADER FLAG   @YM06147*/
         OI    BDUHEADR(@11),B'01000000'                        03S0737
*               PRURHTH(1:3)=TRHFID0;/* SET FID0 RH FLAGS            */
         SL    @14,@CF00045                                     03S0738
         MVC   PRURHTH(3,@14),@CB01384                          03S0738
*               PRURHTH(4:4)=TTH0BYT0;/* SET FID0 TH FLAGS           */
         MVI   PRURHTH+3(@14),X'0C'                             03S0739
*               BUILTFLG=ON;        /* RU BUILT, RETURN TO REDRIVE   */
         OI    BUILTFLG,B'00001000'                             02S0740
*             END;                                              02S0741
*           ELSE                                                02S0742
*             BUILTFLG=OFF;         /* CMND. NOT BUILT FOR THIS TERM.
*                                      - CHECK NEXT RVT ENTRY        */
         B     @RC00730                                         02S0742
@RF00730 NI    BUILTFLG,B'11110111'                             02S0742
*         END;                                                  02S0743
@RC00730 DS    0H                                               02S0743
@DE00725 TM    BUILTFLG,B'00001000'                             02S0743
         BNO   @DL00725                                         02S0743
*         RFY                                                   02S0744
*           IEDQTRM BASED(RTRM);    /* RESET BASE FOR TTE            */
*         ROPCE->OCBUFGOT=ROPCE->OCBUFGOT-2;/* DECREMENT # UNITS GOT */
         LH    @14,OCBUFGOT(,ROPCE)                             01S0745
         N     @14,@CF01733                                     01S0745
         BCTR  @14,0                                            01S0745
         BCTR  @14,0                                            01S0745
         STH   @14,OCBUFGOT(,ROPCE)                             01S0745
*         RFY                                                   01S0746
*           IEDQOPCE BASED(ROPCE);  /* RESET BASE AS REDRIVE OPCE    */
*       END;                                                    01S0747
*                                                               01S0747
@DE00709 CLC   OCBUFGOT(2,ROPCE),@CH00131                       01S0747
         BNL   @DL00709                                         01S0747
*       /*************************************************************/
*       /*                                                           */
*       /* POST OPCE'S TO AVTOPCOB                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0748
*       OPCAQREQ=CHAINFLG;          /* FINISH BUILDING PARM. LIST BY
*                                      SETTING FLAG FOR CHAIN POST   */
         MVI   OPCAQREQ(ROPCAVT),X'B4'                          01S0748
*       RPARM=ADDR(OPCAQCTL);       /* SET REG 1 TO POINT TO PARM.
*                                      LIST                          */
         LA    RPARM,OPCAQCTL(,ROPCAVT)                         01S0749
*       SVC(102);                   /* POST IT                       */
         SVC   102                                              01S0750
*     END;                          /* END OF REDRIVE PROCESSING     */
*   ELSE                                                        01S0752
*     ;                             /* NOT ENOUGH UNITS, DON'T DO
*                                      REDRIVE                       */
*                                                               01S0752
@RF00706 DS    0H                                               01S0753
*   /*****************************************************************/
*   /*                                                               */
*   /* IF STILL MORE OPCE - RU'S TO BUILD, PLACE REDRIVE ELEMENT BACK*/
*   /* ON WAIT CHAIN                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0753
*   IF OCBUFNED=0 THEN              /* IF PROCESSING DONE            */
         LH    @14,OCBUFNED(,ROPCE)                             01S0753
         N     @14,@CF01733                                     01S0753
         LTR   @14,@14                                          01S0753
         BNZ   @RF00753                                         01S0753
*     DO;                                                       01S0754
*       RPARM=ROPCE;                /* FREE REDRIVE ....             */
         LR    RPARM,ROPCE                                      01S0755
*       CALL FREEBUFF;              /* .... OPCE                     */
         L     @15,OPCFREBF(,ROPCAVT)                           01S0756
         BALR  @14,@15                                          01S0756
*       RZERO=1;                    /* DECREMENT OPCADGOT AND ....   */
         LA    RZERO,1                                          01S0757
*       CALL DECRCT;                /* .... INITIATE REDRIVE    01S0758
*                                      PROCESSING FOR ANOTHER REDRIVE
*                                      OPCE                          */
         L     @15,OPCDECR(,ROPCAVT)                            01S0758
         BALR  @14,@15                                          01S0758
*     END;                                                      01S0759
*   ELSE                            /* PROCESSING NOT FINISHED       */
*     DO;                                                       01S0760
         B     @RC00753                                         01S0760
@RF00753 DS    0H                                               01S0761
*       OCWTG=THISMOD;              /* SET TO RETURN                 */
         MVC   OCWTG(2,ROPCE),THISMOD                           01S0761
*       OCLINK=OPCBUFWT;            /* PLACE REDRIVE OPCE ....       */
         L     @14,OPCBUFWT(,ROPCAVT)                           01S0762
         ST    @14,OCLINK(,ROPCE)                               01S0762
*       OPCBUFWT=ROPCE;             /* .... ON FRONT OF WAIT CHAIN   */
         ST    ROPCE,OPCBUFWT(,ROPCAVT)                         01S0763
*     END;                                                      01S0764
*   END REDRIVE;                                                01S0765
@EL00002 DS    0H                                               01S0765
@EF00002 DS    0H                                               01S0765
@ER00002 L     @14,@SA00002                                     01S0765
         BR    @14                                              01S0765
*   END IGCFL10D                                                01S0766
