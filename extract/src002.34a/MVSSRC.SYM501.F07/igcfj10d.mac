         CLC   OCBUFNED(2,ROPCE),@CH00919                       02S0637
         BNH   @RF00637                                         02S0637
*       OUTFLAG=ON;                 /* SET TO GET OUT                */
         OI    OUTFLAG,B'01000000'                              02S0638
*     ELSE                                                      02S0639
*       ;                                                       02S0639
@RF00637 DS    0H                                               02S0640
