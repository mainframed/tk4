@RF00636 DS    0H                                               02S0641
*   RFY                                                         02S0641
*     REG14 RSTD;                   /* RESTRICT REG                  */
*   REG14=BLDOSAV;                  /* RESET RETURN ADDR             */
         L     REG14,BLDOSAV                                    02S0642
*   RFY                                                         02S0643
*     REG14 UNRSTD;                 /* RELEASE REG                   */
*   END BLDOPCE;                                                02S0644
@EL00003 DS    0H                                               02S0644
@EF00003 DS    0H                                               02S0644
@ER00003 BR    @14                                              02S0644
*                                                               02S0645
*   /*****************************************************************/
*   /*                                                               */
*   /* BLDER: THIS PROC BUILDS THE OPCES FOR THE REQUIRED ACTION     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0645
*BLDER:                                                         02S0645
*   PROC OPTIONS(RETREG,SAVE(14));                              02S0645
BLDER    ST    @14,@SA00004                                     02S0645
*   OPCAQWD=''B;                    /* CLEAR THE SVC LIST            */
         XC    OPCAQWD(12,ROPCAVT),OPCAQWD(ROPCAVT)             02S0646
*   OPCAQREQ=CHAINFLG;              /* SET THE REQUEST CODE          */
         MVI   OPCAQREQ(ROPCAVT),X'B4'                          02S0647
*   CALL HIERLOOP;                  /* AND BUILD THE OPCES           */
         BAL   @14,HIERLOOP                                     02S0648
*   RPARM=ADDR(OPCAQCTL);           /* POINT TO SVC LIST             */
         LA    RPARM,OPCAQCTL(,ROPCAVT)                         02S0649
*   SVC(102);                       /* AND POST THEM                 */
         SVC   102                                              02S0650
*   IF OCBUFNED^=0 THEN             /* IF STILL NEED MORE OPCES TO
*                                      FINISH                        */
         ICM   @11,3,OCBUFNED(ROPCE)                            02S0651
         BZ    @RF00651                                         02S0651
*     DO;                           /* THEN                          */
*       OCWTG=THISMOD;              /* SET RETURN TO THIS MODULE     */
         MVC   OCWTG(2,ROPCE),THISMOD                           02S0653
*       OCREDRVE=ON;                /* TURN ON REDRIVE FLAG          */
         OI    OCREDRVE(ROPCE),B'00010000'                      02S0654
*       OCLINK=OPCBUFWT;            /* PUT OPCE ON REDRIVE CHAIN     */
         MVC   OCLINK(4,ROPCE),OPCBUFWT(ROPCAVT)                02S0655
*       OPCBUFWT=ROPCE;             /* AND WAIT                      */
         ST    ROPCE,OPCBUFWT(,ROPCAVT)                         02S0656
*     END;                                                      02S0657
*   ELSE                            /* ELSE FREE THE REDRIVE OPCE    */
*     DO;                                                       02S0658
         B     @RC00651                                         02S0658
@RF00651 DS    0H                                               02S0659
*       REG00=1;                    /* SET DECR COUNT TO 1           */
         LA    REG00,1                                          02S0659
*       CALL DECRCT;                /* DECREMENT OPCADGOT            */
         L     @15,OPCDECR(,ROPCAVT)                            02S0660
         BALR  @14,@15                                          02S0660
*       RPARM=ROPCE;                /* POINT TO OPCE                 */
         LR    RPARM,ROPCE                                      02S0661
*       CALL FREEBUF;               /* AND FREE IT                   */
         L     @15,OPCFREBF(,ROPCAVT)                           02S0662
         BALR  @14,@15                                          02S0662
*     END;                                                      02S0663
*   OPCMODID=TO00;                  /* SET TO GO TO IGC0010D         */
@RC00651 MVC   OPCMODID(2,ROPCAVT),TO00                         02S0664
*   OUTFLAG=ON;                     /* SET TO GET OUT                */
         OI    OUTFLAG,B'01000000'                              02S0665
*   END BLDER;                                                  02S0666
@EL00004 DS    0H                                               02S0666
@EF00004 DS    0H                                               02S0666
@ER00004 L     @14,@SA00004                                     02S0666
         BR    @14                                              02S0666
*                                                               02S0667
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS.GETBUFS: THIS PROCEDURE DETERMINES IF ANY UNITS MAY BE*/
*   /* OBTAINED, AND IF SO, HOW MANY, AND THEN GETS THEM AND ATTACHES*/
*   /* THEM TO THE REDRIVE OPCE                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0667
*GETBUFS:                                                       02S0667
*   PROC OPTIONS(RETREG,SAVE(14));                              02S0667
GETBUFS  ST    @14,@SA00005                                     02S0667
*   IF OPCADGOT<HALFUNIT THEN       /* IF ACT/DEACT HAS LT 50%       */
         LH    @11,HALFUNIT                                     02S0668
         SLR   @09,@09                                          02S0668
         ICM   @09,3,OPCADGOT(ROPCAVT)                          02S0668
         CR    @09,@11                                          02S0668
         BNL   @RF00668                                         02S0668
*     DO;                           /* THEN                          */
*       IF(OCBUFNED+OPCADGOT)>HALFUNIT THEN/* IF NO NEED PLUS        */
         SLR   @05,@05                                          02S0670
         ICM   @05,3,OCBUFNED(ROPCE)                            02S0670
         LR    @07,@05                                          02S0670
         ALR   @07,@09                                          02S0670
         CR    @07,@11                                          02S0670
         BNH   @RF00670                                         02S0670
*         DO;                       /* NO A/D ALREADY HAS IS GT 50%
*                                      THEN                          */
*           GETNO=(HALFUNIT-OPCADGOT);/* SET TO GET UP TO 50%        */
         SLR   @11,@09                                          02S0672
         LR    GETNO,@11                                        02S0672
*           OCBUFNED=OCBUFNED-GETNO;/* SET NO STILL NEED =REMAINDER  */
         SLR   @05,GETNO                                        02S0673
         STH   @05,OCBUFNED(,ROPCE)                             02S0673
*         END;                                                  02S0674
*       ELSE                        /* ELSE GET ALL NEEDED           */
*         DO;                                                   02S0675
         B     @RC00670                                         02S0675
@RF00670 DS    0H                                               02S0676
*           GETNO=OCBUFNED;         /* SET TO GET ALL                */
         SLR   GETNO,GETNO                                      02S0676
         ICM   GETNO,3,OCBUFNED(ROPCE)                          02S0676
*           OCBUFNED=0;             /* SET NO STILL NEED=0           */
         SLR   @11,@11                                          02S0677
         STH   @11,OCBUFNED(,ROPCE)                             02S0677
*         END;                                                  02S0678
*       OCBUFGOT=GETNO;             /* SET NO HAVE =NO TO GET        */
@RC00670 STH   GETNO,OCBUFGOT(,ROPCE)                           02S0679
*       OPCADGOT=OPCADGOT+GETNO;    /* INCREMENT OPCADGOT            */
         SLR   @11,@11                                          02S0680
         ICM   @11,3,OPCADGOT(ROPCAVT)                          02S0680
         ALR   @11,GETNO                                        02S0680
         STH   @11,OPCADGOT(,ROPCAVT)                           02S0680
*     END;                                                      02S0681
*   ELSE                            /* ELSE MUST PUT REDRIVE OPCE ON
*                                      WAIT CHAIN AND WAIT FOR       */
*     DO;                           /* AVAILABLE UNITS ON REDRIVE    */
         B     @RC00668                                         02S0682
@RF00668 DS    0H                                               02S0683
*       OPCMODID=TO00;              /* SET TO GO TO IGC0010D @OY20680*/
         MVC   OPCMODID(2,ROPCAVT),TO00                         02S0683
*       OCWTG=THISMOD;              /* SET RETURN TO THIS MODULE     */
         MVC   OCWTG(2,ROPCE),THISMOD                           02S0684
*       OCREDRVE=ON;                /* SET ON REDRIVE FLAG           */
         OI    OCREDRVE(ROPCE),B'00010000'                      02S0685
*       OCLINK=OPCBUFWT;            /* GET CHAIN ANCHOR AND PUT IN
*                                      LINK FIELD                    */
         MVC   OCLINK(4,ROPCE),OPCBUFWT(ROPCAVT)                02S0686
*       OPCBUFWT=ROPCE;             /* CHAIN IN THIS REDRIVE OPCE    */
         ST    ROPCE,OPCBUFWT(,ROPCAVT)                         02S0687
*       OUTFLAG=ON;                 /* SET TO GET OUT                */
         OI    OUTFLAG,B'01000000'                              02S0688
*       GETNO=0;                    /* SET NO TO GET =0              */
         SLR   GETNO,GETNO                                      02S0689
*     END;                                                      02S0690
*   DO WHILE GETNO>0;               /* LOOP TO GET UNITS             */
@RC00668 B     @DE00691                                         02S0691
@DL00691 DS    0H                                               02S0692
*     IF GETNO>255 THEN             /* MUST LOOP IF NEED MORE THAN
*                                      255                           */
         LA    @11,255                                          02S0692
         CR    GETNO,@11                                        02S0692
         BNH   @RF00692                                         02S0692
*       DO;                         /* THEN                          */
*         GETNO=GETNO-255;          /* SUB 255 FROM NO TO GET        */
         SLR   GETNO,@11                                        02S0694
*         RPARM=255;                /* SET FOR REQUEST               */
         LR    RPARM,@11                                        02S0695
*       END;                                                    02S0696
*     ELSE                          /* ELSE NEED ONLY ONE PASS TO GET
*                                      ALL NEEDED                    */
*       DO;                                                     02S0697
         B     @RC00692                                         02S0697
@RF00692 DS    0H                                               02S0698
*         RPARM=GETNO;              /* SET REQUEST VALUE             */
         LR    RPARM,GETNO                                      02S0698
*         GETNO=0;                  /* AND RESET GETNO TO 0          */
         SLR   GETNO,GETNO                                      02S0699
*       END;                                                    02S0700
*     CALL ALLOCBUF;                /* GET BUFF UNITS                */
@RC00692 L     @15,OPCGETBF(,ROPCAVT)                           02S0701
         BALR  @14,@15                                          02S0701
*     RFY                                                       02S0702
*       REG09 RSTD;                 /* RESTRICT WORK REG             */
*     REG09=ROPCE;                  /* SET REG TO POINT TO REDRIVE
*                                      OPCE                          */
         LR    REG09,ROPCE                                      02S0703
*     DO WHILE REG09->TIC=VALTIC;   /* LOOP TO FIND END OF UNIT CHAIN*/
         B     @DE00704                                         02S0704
@DL00704 DS    0H                                               02S0705
*       REG09=REG09->OCUNIT;        /* UPDATE PTR TO NEXT UNIT       */
         L     REG09,OCUNIT(,REG09)                             02S0705
*     END;                          /* AT END OF LOOP REG09 PTS TO
*                                      LAST UNIT OR REDRIVE OPCE     */
@DE00704 TM    TIC(REG09),B'00000011'                           02S0706
         BZ    @DL00704                                         02S0706
*     REG09->OCUNIT=RPARM;          /* ATTACH GOTTEN UNITS           */
         ST    RPARM,OCUNIT(,REG09)                             02S0707
*     RFY                                                       02S0708
*       REG09 UNRSTD;               /* RELEASE REG                   */
*   END;                                                        02S0709
@DE00691 LTR   GETNO,GETNO                                      02S0709
         BP    @DL00691                                         02S0709
*   END GETBUFS;                                                02S0710
@EL00005 DS    0H                                               02S0710
@EF00005 DS    0H                                               02S0710
@ER00005 L     @14,@SA00005                                     02S0710
         BR    @14                                              02S0710
*                                                               02S0711
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE CLEANS UP FROM AN ACTIVATION OF THE NCP BROUGHT  */
*   /* ABOUT BY AN F ACT=NCP. THE IPL'ED FLAG IN THE DEVICE DEPENDENT*/
*   /* FIELDS OF THE NCP IS TURNED OFF IF ACTIVATION DID NOT FAIL    */
*   /* BECAUSE OF A SIB SHORTAGE. IGCFD10D IS LINKED TO TO DELETE THE*/
*   /* HIERARCHY, THE SAT ENTRY FOR THIS NCP, THE NAT ENTRIES, AND TO*/
*   /* FREE THE RVT.                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0711
*CLNUPNCP:                                                      02S0711
*   PROC OPTIONS(RETREG,SAVE(14));                              02S0711
CLNUPNCP ST    @14,@SA00006                                     02S0711
*   IF OCPTRSAV->OCPARM^=SIBSGMSG THEN/* IF NOT SIB SHORTAGE         */
         L     @11,OCPTRSAV(,ROPCE)                             02S0712
         CLC   OCPARM(2,@11),@CH01667                           02S0712
         BE    @RF00712                                         02S0712
*     DO;                                                       02S0713
*       RPARM=OCTRMTBL;             /* SET PARM. WITH NCP TTCIN      */
         SLR   RPARM,RPARM                                      02S0714
         ICM   RPARM,3,OCTRMTBL(ROPCE)                          02S0714
*       CALL NCPDVCDP;              /* OBTAIN DEVICE DEPENDENT FIELDS*/
         BAL   @14,NCPDVCDP                                     02S0715
*       NCPIPLD=OFF;                /* TURN OFF IPL'ED FLAG          */
         NI    NCPIPLD(RTNCODE),B'11110111'                     02S0716
*     END;                                                      02S0717
*   ELSE                                                        02S0718
*     ;                                                         02S0718
@RF00712 DS    0H                                               02S0719
*   OPCCOPCE=OCPTRSAV;              /* MAKE ORIG. OPCE THE CURRENT
*                                      OPCE                          */
         MVC   OPCCOPCE(4,ROPCAVT),OCPTRSAV(ROPCE)              02S0719
*   RPARM=ROPCAVT;                  /* SET ADDR. OF OP. CTL. AVT     */
         LR    RPARM,ROPCAVT                                    02S0720
*   GEN;                                                        02S0721
*          /*
         LINK  EP=IGCFD10D         */
*   END CLNUPNCP;                                               02S0722
@EL00006 DS    0H                                               02S0722
@EF00006 DS    0H                                               02S0722
@ER00006 L     @14,@SA00006                                     02S0722
         BR    @14                                              02S0722
*                                                               02S0723
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE HANDLES THE TERMINATION OF NCP ACTIVATION IF THE */
*   /* ACTIVATION ENDS IN THIS MODULE (IE. IF CASCADING IS NOT       */
*   /* PERFORMED. THE ORIG. (F ACT) OPCE IS SET AS CURRENT, THE      */
*   /* PREVIOUS CURRENT OPCE IS FREED (ALONG WITH ANY ATTACHED RU),  */
*   /* OPCADGOT IS DECREMENTED, THE IN-PROGRESS BITS TURNED OFF, THE */
*   /* COMP. CODE IS SET IN R0, AND THE OPCMODID IS SET WITH THE     */
*   /* APPROPRIATE MOD. ID                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0723
*TERMPROC:                                                      02S0723
*   PROC OPTIONS(RETREG,SAVE(14));                              02S0723
TERMPROC ST    @14,@SA00007                                     02S0723
*   RPARM=ROPCE;                    /* ADDR. OF OPCE TO BE FREED     */
         LR    RPARM,ROPCE                                      02S0724
*   CALL FREEBUF;                   /* FREE BUFFER AND RU (IF ANY)   */
         L     @15,OPCFREBF(,ROPCAVT)                           02S0725
         BALR  @14,@15                                          02S0725
*   RZERO=1;                        /* NUMBER OF A/D UNITS FREED     */
         LA    RZERO,1                                          02S0726
*   CALL DECRCT;                    /* DECREMENT OPCADGOT, START
*                                      REDRIVE                       */
         L     @15,OPCDECR(,ROPCAVT)                            02S0727
         BALR  @14,@15                                          02S0727
*   RZERO=TOFFINPG;                 /* TURN OFF IN-PROG BITS AND ....*/
         SLR   RZERO,RZERO                                      02S0728
*   RPARM=OPCCOPCE;                 /* .... REMOVE ORIG. OPCE ....   */
         L     RPARM,OPCCOPCE(,ROPCAVT)                         02S0729
*   CALL PROGRTNE;                  /* .... FROM A/D WAIT CHAIN      */
         L     @15,OPCPGCK(,ROPCAVT)                            02S0730
         BALR  @14,@15                                          02S0730
*   ROPCE=OPCCOPCE;                 /* SET OPCE BASE TO CURRENT OPCE
*                                      WHICH IS ORIG.                */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         02S0731
*   RZERO=OCPARM;                   /* SET REG. 0 WITH COMP. CODE    */
         LH    RZERO,OCPARM(,ROPCE)                             02S0732
*   IF OCWTG^=0 THEN                                            02S0733
         ICM   @11,3,OCWTG(ROPCE)                               02S0733
         BZ    @RF00733                                         02S0733
*     DO;                                                       02S0734
*       OPCMODID=OCWTG;             /* SET TO RETURN TO CALLER       */
         MVC   OPCMODID(2,ROPCAVT),OCWTG(ROPCE)                 02S0735
*       OCWTG=''B;                  /* REINITIALIZE                  */
         XC    OCWTG(2,ROPCE),OCWTG(ROPCE)                      02S0736
*     END;                                                      02S0737
*   ELSE                                                        02S0738
*     IF OCTOTE=ON THEN             /* IF TOTE ENTERED COMMAND       */
         B     @RC00733                                         02S0738
@RF00733 TM    OCTOTE(ROPCE),B'01000000'                        02S0738
         BNO   @RF00738                                         02S0738
*       DO;                                                     02S0739
*         OPCMODID=OUTPTMOD;        /* SET TO XCTL TO OUTPUT WRITER  */
         MVC   OPCMODID(2,ROPCAVT),OUTPTMOD                     02S0740
*         IF RZERO=GOODMSG THEN     /* IF SUCCESSFUL                 */
         CH    RZERO,@CH01663                                   02S0741
         BNE   @RF00741                                         02S0741
*           RZERO=TOTEGOOD;         /* GOOD RETURN CODE FOR TOTE     */
         SLR   RZERO,RZERO                                      02S0742
*         ELSE                                                  02S0743
*           RZERO=TOTEBAD;          /* BAD RETURN CODE FOR TOTE      */
         B     @RC00741                                         02S0743
@RF00741 LA    RZERO,4                                          02S0743
*       END;                                                    02S0744
*     ELSE                                                      02S0745
*       IF RZERO=GOODMSG THEN       /* IF GOOD RETURN CODE           */
         B     @RC00738                                         02S0745
@RF00738 CH    RZERO,@CH01663                                   02S0745
         BNE   @RF00745                                         02S0745
*         DO;                                                   02S0746
*           OPCMODID=OPCKPMOD;      /* SET TO XCTL TO OP. CTL CKPT.  */
         MVC   OPCMODID(2,ROPCAVT),OPCKPMOD                     02S0747
*           OCWTG=MSGMOD0D;         /* SET TO GO TO MSG. MOD. AFTER
*                                      CHECK POINT.                  */
         MVC   OCWTG(2,ROPCE),MSGMOD0D                          02S0748
*         END;                                                  02S0749
*       ELSE                                                    02S0750
*         IF RZERO^=IPLRQMSG THEN   /* IF NOT IPL REQ'ED             */
         B     @RC00745                                         02S0750
@RF00745 CH    RZERO,@CH01665                                   02S0750
         BE    @RF00750                                         02S0750
*           OPCMODID=MSGMOD0H;      /* SET TO XCTL TO IGC0H10D       */
         MVC   OPCMODID(2,ROPCAVT),MSGMOD0H                     02S0751
*         ELSE                                                  02S0752
*           OPCMODID=MSGMOD0C;      /* SET TO XCTL TO IGC0C10D  02S0752
*                                                            @YM07763*/
         B     @RC00750                                         02S0752
@RF00750 MVC   OPCMODID(2,ROPCAVT),MSGMOD0C                     02S0752
*   END TERMPROC;                                               02S0753
@EL00007 DS    0H                                               02S0753
@EF00007 DS    0H                                               02S0753
@ER00007 L     @14,@SA00007                                     02S0753
         BR    @14                                              02S0753
*                                                               02S0754
