         TITLE 'IGCFJ10D, ACTIVATE NCP RESPONSE PROCESSOR              *
                        '
IGCFJ10D CSECT ,                                                01S0001
@PROLOG  BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                                   /*
IGCFJ10D IEDHJN AFTERHJN,HJN           MODULE IDENTIFICATION MACRO
*   ROPCAVT=RPARM;                  /* SET THE BASE POINTER FOR THE
*                                      OPERATOR CONTROL ADDRESS 02S0369
*                                      VECTOR TABLE                  */
         LR    ROPCAVT,RPARM                                    02S0369
*   REG10=OPCWORK;                  /* SET THE BASE REGISTER TO BE
*                                      USED FOR ADDRESSING AUTOMATIC
*                                      DATA WITH THE POINTER TO THE
*                                      OPERATOR CONTROL RESIDENT WORK
*                                      AREA                          */
         L     REG10,OPCWORK(,ROPCAVT)                          02S0370
*   GEN REFS(REG10) NOSETS;                                     02S0371
*                                   /*
         USING @DATD,REG10             INFORM THE ASSEMBLER OF THE
*                                      BASE REGISTER TO BE USED FOR
*                                      ADDRESSING AUTOMATIC DATA
*   RSAVE=ADDR(OPCSAVE);            /* SET THE SAVE AREA POINTER
*                                      REGISTER WITH THE POINTER TO
*                                      THE OPERATOR CONTROL SAVE AREA*/
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          02S0372
*                                                               02S0373
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS: THIS SEGMENT DOES THE PROCESSING THAT IS NECESSARY TO*/
*   /* PERFORM THE FUNCTION OF THE MODULE.                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0373
*                                                               03S0373
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS.INITIAL: THIS SEGMENT DOES THE INITIAL PROCESSING.    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               03S0373
*   DO;                                                         03S0373
*     RAVT=OPCAVTPT;                /* SET THE BASE POINTER FOR THE
*                                      TCAM ADDRESS VECTOR TABLE     */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          03S0374
*     ROPCE=OPCCOPCE;               /* SET THE BASE POINTER FOR THE
*                                      OPERATOR CONTROL ELEMENT      */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         03S0375
*     HALFUNIT=(AVTNOLBF/2);        /* INITIALIZE THE THRESHOLD UNIT
*                                      COUNT                         */
         LH    @06,AVTNOLBF(,RAVT)                              03S0376
         SRDA  @06,32                                           03S0376
         D     @06,@CF00067                                     03S0376
         STH   @07,HALFUNIT                                     03S0376
*     FLAGS=''B;                    /* INIT FLAGS                    */
         SLR   @11,@11                                          03S0377
         ST    @11,FLAGS                                        03S0377
*   END;                                                        03S0378
*                                                               03S0379
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS.MAINSEG: THIS SEGMENT ROUTES THE OPCES TO THE CORRECT */
*   /* ROUTINES FOR PROCESSING                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               03S0379
*   IF OCREDRVE=OFF THEN            /* IF NOT REDRIVE OPCE THEN      */
         TM    OCREDRVE(ROPCE),B'00010000'                      03S0379
         BNZ   @RF00379                                         03S0379
*     DO;                                                       03S0380
*       IF OCSWITCH^=OCACTNCP THEN  /* IF NOT ACTPU RESP THEN        */
         CLI   OCSWITCH(ROPCE),X'70'                            03S0381
         BE    @RF00381                                         03S0381
*         IF OCSWITCH^=OCSETTD      /* IF NOT SET TIME-DATE RESP THEN*/
*           THEN                                                03S0382
         CLI   OCSWITCH(ROPCE),X'E0'                            03S0382
         BNE   @RT00382                                         03S0382
*           ;                       /* FALL THROUGH                  */
*         ELSE                      /* ELSE PROCESS SET TIME-DATE
*                                      RESPONSE                      */
*           DO;                                                 03S0384
*                                                               04S0385
*             /*******************************************************/
*             /*                                                     */
*             /* SET CONTROL VECTOR RESPONSE PROCESSOR. IF COMMAND   */
*             /* WAS SUCCESSFUL, TURN ON IPLED FLAG IN NCP DEVICE    */
*             /* DEPENDENT FIELDS. IF UNSUCCESSFUL, SET IPL'ED       */
*             /* REQUIRED MSG. AS COMPLETION CODE AND GO CLEANUP.    */
*             /*                                                     */
*             /*******************************************************/
*                                                               04S0385
*             IF OCPARM>0 THEN      /* IF COMMAND WAS SUCCESSFUL     */
         ICM   @11,3,OCPARM(ROPCE)                              04S0385
         BNP   @RF00385                                         04S0385
*               DO;                                             04S0386
*                 RPARM=OCTRMTBL;   /* SET TTCIN OF NCP AS PARM.     */
         SLR   RPARM,RPARM                                      04S0387
         ICM   RPARM,3,OCTRMTBL(ROPCE)                          04S0387
*                 CALL NCPDVCDP;    /* OBTAIN DEVICE DEPENDENT FIELD */
         BAL   @14,NCPDVCDP                                     04S0388
*                 NCPIPLD=ON;       /* TURN ON IPL'ED FLAG           */
         OI    NCPIPLD(RTNCODE),B'00001000'                     04S0389
*                                                               05S0390
*                 /***************************************************/
*                 /*                                                 */
*                 /* PROCESS.MAINSEG.SETCVR.DACTCHK: THIS SEGMENT    */
*                 /* PERFORMS THE PROCESSING NECESSARY TO SEE IF ANY */
*                 /* DEACT LINKS ARE REQUIRED FOR ANY LINKS. IF THE  */
*                 /* ORIGINAL COMMAND WAS AN 'IDLE' THEN ALL LINKS   */
*                 /* ARE DEACTIVATED. IF NOT AN 'IDLE' THEN ALL      */
*                 /* PRE-SNA AND ANY SNA LINES WITHOUT THEIR REMEMBER*/
*                 /* BIT ON ARE DEACTIVATED. IF NO DEACTS ARE        */
*                 /* NECESSARY THEN THE OPCE IS SET UP TO GO TO THE  */
*                 /* ACTLINK SEG TO PERFORM ANY ACTLINK AND RESTART  */
*                 /* PROCESSING                                      */
*                 /*                                                 */
*                 /***************************************************/
*                                                               05S0390
*                 OCWTG=THISMOD;    /* SET UP RETURN TO THIS MODULE  */
         MVC   OCWTG(2,ROPCE),THISMOD                           05S0390
*                 OCSWITCH=OCDACTLK;/* SET UP AS REDRIVE OPCE FOR
*                                      DACT LINKS                    */
         MVI   OCSWITCH(ROPCE),X'60'                            05S0391
*                 OCREDRVE=ON;                                  05S0392
         OI    OCREDRVE(ROPCE),B'00010000'                      05S0392
*                 IF TIC=VALTIC THEN/* IF THERE IS AN ATTACHED RU    */
         TM    TIC(ROPCE),B'00000011'                           05S0393
         BNZ   @RF00393                                         05S0393
*                   DO;                                         05S0394
*                     RPARM=OCUNIT; /* THEN POINT TO IT              */
         L     RPARM,OCUNIT(,ROPCE)                             05S0395
*                     CALL FREEBUF; /* AND FREE IT                   */
         L     @15,OPCFREBF(,ROPCAVT)                           05S0396
         BALR  @14,@15                                          05S0396
*                     OCUNIT=PRUINTIC;/* SET INVALID TIC     @YM05695*/
         MVC   OCUNIT(4,ROPCE),@CF01412                         05S0397
*                   END;                                        05S0398
*                 ELSE                                          05S0399
*                   ;                                           05S0399
@RF00393 DS    0H                                               05S0400
*                 RPARM=OCPTRSAV-LENGTH(IEDQOPCN);/* BACK UP TO ORIG
*                                      OPCE NEG PREF                 */
         L     RPARM,OCPTRSAV(,ROPCE)                           05S0400
         SH    RPARM,@CH00047                                   05S0400
*                 IF RPARM->OCIDLE=ON THEN/* IF ORIG OPCE IS 'IDLE'  */
         TM    OCIDLE(RPARM),B'00000001'                        05S0401
         BNO   @RF00401                                         05S0401
*                   ACTFLAG=1;      /* THEN SET CNTL TO COUNT ALL
*                                      LINES                         */
         MVC   ACTFLAG(4),@CF00063                              05S0402
*                 ELSE                                          05S0403
*                   ACTFLAG=3;      /* ELSE SET TO COUNT PSNA AND
*                                      INACTIVE SNA LINES            */
         B     @RC00401                                         05S0403
@RF00401 MVC   ACTFLAG(4),@CF00076                              05S0403
*                 IF TRMSUBST=0 THEN/* IF NO SUBORDINATE RESOURCES
*                                      THEN DONE                     */
@RC00401 LR    @11,RTRM                                         05S0404
         BCTR  @11,0                                            05S0404
         BCTR  @11,0                                            05S0404
         ICM   @09,3,TRMSUBST-12(@11)                           05S0404
         BNZ   @RF00404                                         05S0404
*                   DO;                                         05S0405
*                     OCPTRSAV->OCPARM=GOODMSG;/* SET COMPLETION MSG */
         L     @11,OCPTRSAV(,ROPCE)                             05S0406
         MVC   OCPARM(2,@11),@CH01663                           05S0406
*                     OPCCOPCE=OCPTRSAV;/* SET ORIG OPCE AS CURRENT  */
         ST    @11,OPCCOPCE(,ROPCAVT)                           05S0407
*                     CALL TERMPROC;/* PERFORM TERMINATION PROCESSING*/
         BAL   @14,TERMPROC                                     05S0408
*                     OUTFLAG=ON;   /* SET TO BYPASS ACTLINK         */
         OI    OUTFLAG,B'01000000'                              05S0409
*                   END;                                        05S0410
*                 ELSE                                          05S0411
*                   DO;             /* ELSE PROCESS SUBORDINATE 05S0411
*                                      RESOURCES                     */
         B     @RC00404                                         05S0411
@RF00404 DS    0H                                               05S0412
*                     OCBUFNED=0;   /* INIT THE COUNTER FIELDS       */
         SLR   @11,@11                                          05S0412
         STH   @11,OCBUFNED(,ROPCE)                             05S0412
*                     OCBUFGOT=0;                               05S0413
         STH   @11,OCBUFGOT(,ROPCE)                             05S0413
*                     CALL HIERLOOP;/* PERFORM THE COUNT             */
         BAL   @14,HIERLOOP                                     05S0414
*                     IF OCBUFNED^=0 THEN/* IF ANY DEACTS TO BE DONE */
         ICM   @11,3,OCBUFNED(ROPCE)                            05S0415
         BZ    @RF00415                                         05S0415
*                       DO;         /* THEN                          */
*                         RPARM=OCPTRSAV-LENGTH(IEDQOPCN);/* BACK UP
*                                      TO ORIG OPCE NEG PREF         */
         L     RPARM,OCPTRSAV(,ROPCE)                           05S0417
         SH    RPARM,@CH00047                                   05S0417
*                         RPARM->OCRESPCT=OCBUFNED;/* AND SET   05S0418
*                                      OUTSTANDING RESPONSE COUNT    */
         MVC   OCRESPCT(2,RPARM),OCBUFNED(ROPCE)                05S0418
*                         CALL GETBUFS;/* AND GO GET BUFS            */
         BAL   @14,GETBUFS                                      05S0419
*                         IF OUTFLAG=OFF THEN/* IF COULD OBTAIN ANY
*                                      BUFFERS THEN                  */
         TM    OUTFLAG,B'01000000'                              05S0420
         BNZ   @RF00420                                         05S0420
*                           DO;                                 05S0421
*                             ACTFLAG=ACTFLAG+1;/* SET CNTL TO BUILD
*                                      DEACTS                        */
         LA    @11,1                                            05S0422
         AL    @11,ACTFLAG                                      05S0422
         ST    @11,ACTFLAG                                      05S0422
*                             CALL BLDER;/* AND GO BUILD THEM AND
*                                      POST THEM                     */
         BAL   @14,BLDER                                        05S0423
*                           END;                                05S0424
*                         ELSE                                  05S0425
*                           ;                                   05S0425
@RF00420 DS    0H                                               05S0426
*                       END;                                    05S0426
*                     ELSE          /* ELSE SET UP TO DO ACT LINKS
*                                      AND STARTLINES                */
*                       DO;                                     05S0427
         B     @RC00415                                         05S0427
@RF00415 DS    0H                                               05S0428
*                         OCSWITCH=OCACTLK;/* SET ACTLINK CODE       */
         MVI   OCSWITCH(ROPCE),X'50'                            05S0428
*                         OCREDRVE=OFF;/* AND RESET REDRIVE SWITCH   */
         NI    OCREDRVE(ROPCE),B'11101111'                      05S0429
*                       END;                                    05S0430
*                   END;                                        05S0431
*               END;                                            04S0432
*             ELSE                  /* UNSUCCESSFUL SET CTL. VECTOR  */
*               DO;                                             04S0433
         B     @RC00385                                         04S0433
@RF00385 DS    0H                                               04S0434
*                 OCPTRSAV->OCPARM=IPLRQMSG;/* SET IPL REQ'ED AS
*                                      COMP. CODE                    */
         L     @11,OCPTRSAV(,ROPCE)                             04S0434
         MVC   OCPARM(2,@11),@CH01665                           04S0434
*                 CALL CLNUPNCP;    /* CLEANUP F ACT=NCP             */
         BAL   @14,CLNUPNCP                                     04S0435
*                 CALL TERMPROC;    /* TERMINATE PROCESSING          */
         BAL   @14,TERMPROC                                     04S0436
*                 OUTFLAG=ON;       /* SET TO BYPASS ACTLINK         */
         OI    OUTFLAG,B'01000000'                              04S0437
*               END;                                            04S0438
*           END;                                                03S0439
*       ELSE                        /* ELSE PROCESS ACTPU RESPONSE   */
*         DO;                                                   03S0440
         B     @RC00381                                         03S0440
@RF00381 DS    0H                                               03S0441
*                                                               04S0441
*           /*********************************************************/
*           /*                                                       */
*           /* ACTPU(NCP) RESPONSE PROCESSOR. IF OPCE INDICATES THAT */
*           /* NCP IS ACTIVE OR WAS ALREADY ACTIVE, THE ATTACHED RU  */
*           /* IS FREED, THE OPCE IS REFORMATTED INTO A SET CTL.     */
*           /* VECTOR OPCE, AND SET TO XCTL TO SSCP. IF BAD RESPONSE,*/
*           /* SET COMPLETION CODE AND CALL ROUTINES TO CLEAN UP NCP */
*           /* ACTIVATION AND TERMINATE PROCESSING                   */
*           /*                                                       */
*           /*********************************************************/
*                                                               04S0441
*           IF OCPARM>0 THEN        /* IF NCP ACTIVATED OR ALREADY
*                                      ACTIVE                        */
         ICM   @11,3,OCPARM(ROPCE)                              04S0441
         BNP   @RF00441                                         04S0441
*             DO;                   /* FREE RU AND REFORMAT INTO SET
*                                      CONTROL VECTOR OPCE           */
*               RPARM=OCUNIT;       /* FREE ATTACHED ....            */
         L     RPARM,OCUNIT(,ROPCE)                             04S0443
*               CALL FREEBUF;       /* .... RU                       */
         L     @15,OPCFREBF(,ROPCAVT)                           04S0444
         BALR  @14,@15                                          04S0444
*               OCUNIT=PRUINTIC;    /* SET INVALID TIC       @YM05695*/
         MVC   OCUNIT(4,ROPCE),@CF01412                         04S0445
*               OCSWITCH=OCSETTD;   /* INDICATES SET CTL. VECTOR
*                                      (TIME/ DATE)                  */
         MVI   OCSWITCH(ROPCE),X'E0'                            04S0446
*               OCWTG=THISMOD;      /* SET TO RETURN TO 'FJ' FROM
*                                      SSCP                          */
         MVC   OCWTG(2,ROPCE),THISMOD                           04S0447
*               OCRESP=OFF;         /* SET OFF OCRESP                */
         NI    OCRESP(ROPCE),B'11111110'                        04S0448
*               OPCMODID=SSCPMOD;   /* SET TO XCTL TO SSCP           */
         MVC   OPCMODID(2,ROPCAVT),SSCPMOD                      04S0449
*             END;                                              04S0450
*           ELSE                                                04S0451
*             DO;                   /* DO ACTPU(NCP) ERROR PROCESSING*/
         B     @RC00441                                         04S0451
@RF00441 DS    0H                                               04S0452
*               IF OCPARM=OCRCBNEG×OCPARM=OCRCBPOS THEN/* IF IPL
*                                      REQUIRED                      */
         LH    @11,OCPARM(,ROPCE)                               04S0452
         CH    @11,@CH01067                                     04S0452
         BE    @RT00452                                         04S0452
         CH    @11,@CH01071                                     04S0452
         BNE   @RF00452                                         04S0452
@RT00452 DS    0H                                               04S0453
*                 OCPTRSAV->OCPARM=IPLRQMSG;/* SET COMPLETION CODE IN
*                                      ORIG. OPCE                    */
         L     @11,OCPTRSAV(,ROPCE)                             04S0453
         MVC   OCPARM(2,@11),@CH01665                           04S0453
*               ELSE                                            04S0454
*                 IF OCPARM=OCRCBSIB THEN/* IF SIB SHORTAGE          */
         B     @RC00452                                         04S0454
@RF00452 CLC   OCPARM(2,ROPCE),@CH01069                         04S0454
         BNE   @RF00454                                         04S0454
*                   OCPTRSAV->OCPARM=SIBSGMSG;/* SET COMP. CODE IN
*                                      ORIG. OPCE                    */
         L     @11,OCPTRSAV(,ROPCE)                             04S0455
         MVC   OCPARM(2,@11),@CH01667                           04S0455
*                 ELSE                                          04S0456
*                   DO;                                         04S0456
         B     @RC00454                                         04S0456
@RF00454 DS    0H                                               04S0457
*                     RPARM=OCPTRSAV-LENGTH(IEDQOPCN);/* POINT TO
*                                      NEG. PREFIX OF ORIG.          */
         L     @11,OCPTRSAV(,ROPCE)                             04S0457
         LR    RPARM,@11                                        04S0457
         SH    RPARM,@CH00047                                   04S0457
*                     RPARM->OCLDNME=OCBACKUP;/* PLACE LOAD MOD. NAME
*                                      IN PREFIX                     */
         MVC   OCLDNME(8,RPARM),OCBACKUP(ROPCE)                 04S0458
*                     OCPTRSAV->OCPARM=LDMODMSG;/* SET COMP. CODE IN
*                                      ORIG OPCE FOR LOAD MODULE NAME
*                                      MISMATCH                      */
         MVC   OCPARM(2,@11),@CH01669                           04S0459
*                   END;                                        04S0460
*               CALL CLNUPNCP;      /* CLEAN UP F ACT=NCP            */
@RC00454 DS    0H                                               04S0461
@RC00452 BAL   @14,CLNUPNCP                                     04S0461
*               CALL TERMPROC;      /* TERMINATE PROCESSING          */
         BAL   @14,TERMPROC                                     04S0462
*             END;                                              04S0463
*           OUTFLAG=ON;             /* SET TO BYPASS ACTLINK         */
@RC00441 OI    OUTFLAG,B'01000000'                              04S0464
*         END;                                                  03S0465
*       IF OUTFLAG=OFF THEN         /* IF FLAG NOT SET THEN          */
@RC00381 TM    OUTFLAG,B'01000000'                              03S0466
         BNZ   @RF00466                                         03S0466
*         DO;                                                   03S0467
*                                                               04S0468
*           /*********************************************************/
*           /*                                                       */
*           /* ACTLINK: THIS SEGMENT CHECKS TO SEE IF RESTART        */
*           /* PROCESSING IS NECESSARY AND IF SO CONTROL IS PASSED TO*/
*           /* IGCFF10D TO SEND RESTART INFO TO THE NCP. IGCFF10D    */
*           /* RETURNS TO THIS MODULE TO HAVE THE ACTLINK PROCESSING */
*           /* PERFORMED(OCSWITCH=0). MODULES IGCFL-FQ10D RETURN TO  */
*           /* THIS MODULE ALSO TO HAVE THE ACTLINK PROCESSING       */
*           /* PERFORMED                                             */
*           /*                                                       */
*           /*********************************************************/
*                                                               04S0468
*           IF OCSWITCH^=0 THEN     /* IF NOT RETURN FROM FF THEN SEE
*                                      IF RESTART                    */
         CLI   OCSWITCH(ROPCE),0                                04S0468
         BE    @RF00468                                         04S0468
*             DO;                   /* NECESSARY                     */
*               RPARM=OCPTRSAV->OCTRMTBL;/* SET UP NCP TTCIN IN RPARM*/
         L     @11,OCPTRSAV(,ROPCE)                             04S0470
         SLR   @09,@09                                          04S0470
         ICM   @09,3,OCTRMTBL(@11)                              04S0470
         LR    RPARM,@09                                        04S0470
*               OCTRMTBL=OCPTRSAV->OCTRMTBL;/* AND IN OCTRMTBL  04S0471
*                                                            @YM05695*/
         STH   @09,OCTRMTBL(,ROPCE)                             04S0471
*               CALL NCPDVCDP;      /* OBTAIN ADDR. OF NCP DEVICE
*                                      DEPENDENT FIELDS              */
         BAL   @14,NCPDVCDP                                     04S0472
*               IF NCPCKPT^=0 THEN  /* IF POINTER TO CK.PT. D.S.
*                                      EXITS                         */
         SLR   @11,@11                                          04S0473
         L     @09,NCPCKPT(,RTNCODE)                            04S0473
         CLR   @09,@11                                          04S0473
         BE    @RF00473                                         04S0473
*                 IF DCBOFOPN=ON&DCBIOBAD^=0&NCPREST=ON THEN/* IF
*                                      OPEN & RESTART                */
         TM    DCBOFOPN+40(@09),B'00010000'                     04S0474
         BNO   @RF00474                                         04S0474
         C     @11,DCBIOBAD+20(,@09)                            04S0474
         BE    @RF00474                                         04S0474
         TM    NCPREST(RTNCODE),B'00010000'                     04S0474
         BNO   @RF00474                                         04S0474
*                   DO;                                         04S0475
*                     FINIFLAG=ON;  /* SET ON FINIFLAG               */
         OI    FINIFLAG,B'10000000'                             04S0476
*                     OPCMODID=ADCKPMOD;/* SET TO XCTL TO A/D CK. PT.
*                                      MOD.                          */
         MVC   OPCMODID(2,ROPCAVT),ADCKPMOD                     04S0477
*                   END;                                        04S0478
*                 ELSE                                          04S0479
*                   ;                                           04S0479
@RF00474 DS    0H                                               04S0480
*               ELSE                                            04S0480
*                 ;                                             04S0480
@RF00473 DS    0H                                               04S0481
*             END;                                              04S0481
*           ELSE                                                04S0482
*             ;                                                 04S0482
@RF00468 DS    0H                                               04S0483
*           IF FINIFLAG=OFF THEN    /* PERFORM ACTLINK PROCESSING    */
         TM    FINIFLAG,B'10000000'                             04S0483
         BNZ   @RF00483                                         04S0483
*             DO;                                               04S0484
*                                                               05S0485
*               /*****************************************************/
*               /*                                                   */
*               /* THIS SEGMENT CHECKS TO SEE IF THE ORIG CMND. WAS  */
*               /* AN F ACT=NCP,IDLE. IF IT WAS PROCESSING IS        */
*               /* TERMINATED AND A GOOD COMPLETION CODE IS GIVEN. IF*/
*               /* NOT, THE NUMBER OF LINES TO ACTIVATE IS           */
*               /* DETERMINED. IF NO LINES ARE TO BE ACTIVATED,      */
*               /* PROCESSING IS THE SAME AS IF IDLE. OTHERWISE, THE */
*               /* ATTACHED RU IS FREED AND ACTIVATE LINES PROCESSING*/
*               /* IS STARTED.                                       */
*               /*                                                   */
*               /*****************************************************/
*                                                               05S0485
*               RPARM=OCPTRSAV-LENGTH(IEDQOPCN);/* POINT TO NEG.
*                                      PREFIX OF ORIG.               */
         L     @11,OCPTRSAV(,ROPCE)                             05S0485
         LR    RPARM,@11                                        05S0485
         SH    RPARM,@CH00047                                   05S0485
*               IF RPARM->OCIDLE=ON THEN/* IF IDLE FLAG ON IN ORIG.  */
         TM    OCIDLE(RPARM),B'00000001'                        05S0486
         BNO   @RF00486                                         05S0486
*                 DO;                                           05S0487
*                   OCPTRSAV->OCPARM=GOODMSG;/* SET GOOD COMP. CODE  */
         MVC   OCPARM(2,@11),@CH01663                           05S0488
*                   OPCCOPCE=OCPTRSAV;/* MAKE ORIG. OPCE THE CURRENT
*                                      OPCE                          */
         ST    @11,OPCCOPCE(,ROPCAVT)                           05S0489
*                   CALL TERMPROC;  /* TERMINATE PROCESSING          */
         BAL   @14,TERMPROC                                     05S0490
*                 END;                                          05S0491
*               ELSE                                            05S0492
*                 DO;               /* NOT IDLE                      */
         B     @RC00486                                         05S0492
@RF00486 DS    0H                                               05S0493
*                   IF TIC=VALTIC THEN/* IF UNIT ATTACHED            */
         TM    TIC(ROPCE),B'00000011'                           05S0493
         BNZ   @RF00493                                         05S0493
*                     DO;                                       05S0494
*                       RPARM=OCUNIT;/* POINT TO ATTACHED RU         */
         L     RPARM,OCUNIT(,ROPCE)                             05S0495
*                       CALL FREEBUF;/* FREE THE RU                  */
         L     @15,OPCFREBF(,ROPCAVT)                           05S0496
         BALR  @14,@15                                          05S0496
*                       OCUNIT=PRUINTIC;/* AND SET INVALID TIC       */
         MVC   OCUNIT(4,ROPCE),@CF01412                         05S0497
*                     END;                                      05S0498
*                   ELSE                                        05S0499
*                     ;                                         05S0499
@RF00493 DS    0H                                               05S0500
*                   ACTFLAG=5;      /* SET FLAG TO COUNT ACTLINKS TO
*                                      BE DONE                       */
         MVC   ACTFLAG(4),@CF00165                              05S0500
*                   OCBUFNED=0;     /* INIT THE BUFS NEED AND THE    */
         SLR   @11,@11                                          05S0501
         STH   @11,OCBUFNED(,ROPCE)                             05S0501
*                   OCBUFGOT=0;     /* BUFS HAVE FIELDS              */
         STH   @11,OCBUFGOT(,ROPCE)                             05S0502
*                   CALL HIERLOOP;  /* GO GET COUNT                  */
         BAL   @14,HIERLOOP                                     05S0503
