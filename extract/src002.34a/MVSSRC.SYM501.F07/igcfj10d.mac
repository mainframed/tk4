         L     @15,AVTRNMPT(,RAVT)                              02S0755
         BALR  @14,@15                                          02S0755
*   RTRM=RPARM;                     /* COPY TTE ADDR.                */
         LR    RTRM,RPARM                                       02S0756
*   RZERO=TRMNCPIM;                 /* SET UP MASK                   */
         LA    RZERO,8                                          02S0757
