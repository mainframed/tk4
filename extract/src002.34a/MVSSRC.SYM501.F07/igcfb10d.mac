         TITLE 'IGCFB10D - RVT AND HIERARCHY BUILD - LOAD I            *
                        '
IGCFB10D CSECT ,                                                01S0001
@PROLOG  BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                                   /*
IGCFB10D IEDHJN AFTERHJN,HJN
*                                                               01S0404
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZATION                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0404
*   ROPCAVT=RPARM;                  /* SET POINTER TO OP. CTL. AVT   */
         LR    ROPCAVT,RPARM                                    01S0404
*   RAVT=OPCAVTPT;                  /* SET POINTER TO TCAM AVT       */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          01S0405
*   ROPCE=OPCCOPCE;                 /* SET POINTER TO CURRENT OPCE   */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         01S0406
*   RSAVE=ADDR(OPCSAVE);            /* SET POINTER TO SAVE AREA      */
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          01S0407
*   RDATA=OPCWORK;                  /* SET DATA POINTER TO OP. CTL.
*                                      WORK AREA                     */
         L     RDATA,OPCWORK(,ROPCAVT)                          01S0408
*   FLAGS1='00'X;                   /* CLEAR FLAG BYTE 1             */
         MVI   FLAGS1(RDATA),X'00'                              01S0409
*                                                               01S0410
*   /*****************************************************************/
*   /*                                                               */
*   /* USING OCTRMTBL, GET NCP TTE ADDR.                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0410
*   RPARM=OCTRMTBL;                 /* CONTAINS TTCIN OF NCP TTE     */
         LH    RPARM,OCTRMTBL(,ROPCE)                           01S0410
         N     RPARM,@CF02944                                   01S0410
*   RBRNCH=AVTRNMPT;                /* ADDR. OF CONVERSION ROUTINE   */
         L     RBRNCH,AVTRNMPT(,RAVT)                           01S0411
*   CALL SUBRTNE;                   /* SUBRTNE BASED ON RBRNCH       */
         BALR  @14,@15                                          01S0412
*   RTRM=RPARM;                     /* COPY TTE ADDRESS              */
         LR    RTRM,RPARM                                       01S0413
*   NCPTTEA=RTRM;                   /* SAVE NCP TTE ADDRESS IN W.A.  */
         ST    RTRM,NCPTTEA(,RDATA)                             01S0414
*   RZERO=TRMNCPIM;                 /* PARM TELLING IEDQTL TO LOOK
*                                      FOR NCP DEVICE DEPENDENT 01S0415
*                                      FIELDS                        */
         LA    RZERO,8                                          01S0415
*   RBRNCH=AVTDDFT;                 /* ADDR. OF ROUTINE TO LOCATE
*                                      DEVICE DEPENDENT FIELDS       */
         L     RBRNCH,AVTDDFT(,RAVT)                            01S0416
*   CALL SUBRTNE;                   /* RETURNS ADDR. IN R15(RRETCODE)*/
*                                                               01S0417
         BALR  @14,@15                                          01S0417
*   /*****************************************************************/
*   /*                                                               */
*   /* DEVICE DEPENDENT FIELD OF NCP CONTAINS THE IPL TEXT NAME FOR  */
*   /* THE NCP. ADD AN 'R' TO THIS NAME TO OBTAIN THE NAME OF THE    */
*   /* LOAD MODULE CONTAINING THE RRT AND MOVE THIS NAME INTO THE    */
*   /* PARAMETER LIST FOR THE BLDL MACRO IN ORDER TO LOCATE THE RRT  */
*   /* MODULE                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0418
*   IPLTXTNM=RRETCODE->NCPTXID;     /* STORE IPL TEXT NAME FROM 01S0418
*                                      DEVICE DEPENDENT FIELD        */
         MVI   IPLTXTNM+8(RDATA),C' '                           01S0418
         MVC   IPLTXTNM(8,RDATA),NCPTXID(RRETCODE)              01S0418
*   DO BYTEPTR=ADDR(IPLTXTNM) BY 1 WHILE(BYTECHAR^=' ');/* LOCATE
*                                      FIRST BLANK IN NAME           */
         LA    @14,IPLTXTNM(,RDATA)                             01S0419
         ST    @14,BYTEPTR(,RDATA)                              01S0419
         B     @DE00419                                         01S0419
@DL00419 DS    0H                                               01S0420
*   END;                                                        01S0420
         LA    @14,1                                            01S0420
         AL    @14,BYTEPTR(,RDATA)                              01S0420
         ST    @14,BYTEPTR(,RDATA)                              01S0420
@DE00419 L     @14,BYTEPTR(,RDATA)                              01S0420
         CLI   BYTECHAR(@14),C' '                               01S0420
         BNE   @DL00419                                         01S0420
*   BYTECHAR='R';                   /* ADD 'R' TO END OF NAME        */
         MVI   BYTECHAR(@14),C'R'                               01S0421
*   BLDLNAME=IPLTXTNM(1:8);         /* STORE IPL TEXT NAME IN BLDL
*                                      PARM LIST                     */
         MVC   BLDLNAME(8,RDATA),IPLTXTNM(RDATA)                01S0422
*   BLDNTRYN=1;                     /* NUMBER OF BLDL ENTRIES        */
         MVC   BLDNTRYN(2,RDATA),@CH00077                       01S0423
*   BLDNTRYL=LENGTH(BLDLNTRY);      /* LENGTH OF BLDL ENTRY          */
         MVC   BLDNTRYL(2,RDATA),@CH00120                       01S0424
*   RPARM=0;                        /* PARM FOR BLDL                 */
         SLR   RPARM,RPARM                                      01S0425
*   RZERO=ADDR(BLDLIST);            /* ADDR. OF BLDL PARAMETER LIST  */
         LA    RZERO,BLDLIST(,RDATA)                            01S0426
*   GEN REFS(RZERO,RPARM) SETS(RRETCODE,BLDLDATA);              01S0427
* SYSTEM DEPENDENCY FOR MACRO
         BLDL   (RPARM),(RZERO)
*   IF RRETCODE=0 THEN              /* IF LOAD MODULE LOCATED        */
         LTR   RRETCODE,RRETCODE                                01S0428
         BNZ   @RF00428                                         01S0428
*     DO;                                                       01S0429
*                                                               01S0429
*       /*************************************************************/
*       /*                                                           */
*       /* LOAD RRT                                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0430
*       GEN REFS(BLDLNAME,RDATA);                               01S0430
*                                   /*
* SYSTEM DEPENDENCY FOR MACROS
         LOAD  DE=BLDLNAME(RDATA)                           @YM02942*/
*       RRTPTR=RZERO;               /* SAVE RRT ADDR.                */
*                                                               01S0431
         ST    RZERO,RRTPTR(,RDATA)                             01S0431
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK RRT HEADER AND DETERMINE STORAGE NEED FOR RVT       */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0432
*                                                               02S0432
*       /*************************************************************/
*       /*                                                           */
*       /* THIS SEGMENT CHECKS THE FIRST BYTE OF THE RESOURCE        */
*       /* RESOLUTION TABLE TO DETERMINE IF IT IS FOR AN NCP6 OR     */
*       /* PRE-NCP6. THE APPROPRIATE PROCESSING ROUTINE IS THEN      */
*       /* CALLED. IF ANY ERRORS ARE DETECTED IN THE PROCESSING      */
*       /* ROUTINE, OCPARM AND RZERO ARE SET WITH THE 'ERROR IN RRT' */
*       /* MSG. NO.                                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0432
*       IF RRTPTR->BYTECHAR^='00'X THEN/* IF THIS IS AN NCP6 RRT     */
         L     @14,RRTPTR(,RDATA)                               02S0432
         CLI   BYTECHAR(@14),X'00'                              02S0432
         BE    @RF00432                                         02S0432
*         DO;                                                   02S0433
*                                                               02S0433
*           /*********************************************************/
*           /*                                                       */
*           /* PROCESS RRT FOR NCP6                                  */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0434
*                                                               03S0434
*           /*********************************************************/
*           /*                                                       */
*           /* THIS SEGMENT IS EXECUTED WHEN A RESOURCE RESOLUTION   */
*           /* TABLE HAS BEEN LOADED INDICATING THE AN NCP6.         */
*           /*                                                       */
*           /*********************************************************/
*                                                               03S0434
*           RFY                                                 03S0434
*             RSTSEG BASED(ENTRYPTR);/* SEGMENT HEADER FORMAT        */
*           RFY                                                 03S0435
*             RSTHDR BASED(ENTRYPTR);/* HEADER SEGMENT ENTRY FORMAT  */
*           RFY                                                 03S0436
*             RSTSYSEN BASED(ENTRYPTR);/* SYSTEM SEGMENT ENTRY FORMAT*/
*           RFY                                                 03S0437
*             RSTHOST BASED(ENTRYPTR);/* HOST SEGMENT ENTRY FORMAT   */
*           RFY                                                 03S0438
*             IEDQQCB BASED(TRMDESTQ);/* QCB ADDR.                   */
*           RFY                                                 03S0439
*             IHADCB BASED(QCBDCBAD);/* DCB ADDR.                    */
*                                                               03S0440
*           /*********************************************************/
*           /*                                                       */
*           /* THE SUBROUTINE SEARCHER IS USED TO OBTAIN             */
*           /* ADDRESSABILITY TO THE SYSTEM SEGMENT ENTRY IN THE     */
*           /* HEADER SEGMENT                                        */
*           /*                                                       */
*           /*********************************************************/
*                                                               03S0440
*           SRCH=RSTSYSEG;          /* SET PARM FOR SYSTEM SEG. ENTRY*/
         MVI   SRCH(RDATA),X'01'                                03S0440
*           CALL SEARCHER;          /* SETS ENTRYPTR TO ADDR. OF
*                                      SYSTEM SEGMENT ENTRY IN HEADER
*                                      SEGMENT                       */
*                                                               03S0441
         BAL   @14,SEARCHER                                     03S0441
*           /*********************************************************/
*           /*                                                       */
*           /* ADDRESSABILITY TO THE SYSTEM SEGMENT ENTRIES IS       */
*           /* OBTAINED AND THE THE SUBAREA LENGTH IS OBTAINED       */
*           /*                                                       */
*           /*********************************************************/
*                                                               03S0442
*           ENTRYPTR=RSTSGOFF+RRTPTR;/* ADD OFFSET OF SYSTEM SEGMENT
*                                      TO THE RST ADDRESS TO POINT TO
*                                      SYSTEM SEGMENT                */
         L     @14,ENTRYPTR(,RDATA)                             03S0442
         ICM   @11,15,RSTSGOFF(@14)                             03S0442
         AL    @11,RRTPTR(,RDATA)                               03S0442
         ST    @11,ENTRYPTR(,RDATA)                             03S0442
*           ENTRYPTR=ENTRYPTR+RSTSEGHL;/* ADD SEGMENT HEADER LENGTH
*                                      TO OBTAIN ADDRESS OF SYSTEM
*                                      SEGMENT ENTRIES               */
         SLR   @14,@14                                          03S0443
         IC    @14,RSTSEGHL(,@11)                               03S0443
         ALR   @14,@11                                          03S0443
         ST    @14,ENTRYPTR(,RDATA)                             03S0443
*           IF RSTSALTH=SAVTSUBL THEN/* IF SUBAREA LENGTH IN RST IS
*                                      EQUAL TO LENGTH IN SAVT       */
         L     @11,AVTSAVTP(,RAVT)                              03S0444
         SLR   @09,@09                                          03S0444
         ICM   @09,3,RSTSALTH(@14)                              03S0444
         SLR   @14,@14                                          03S0444
         IC    @14,SAVTSUBL(,@11)                               03S0444
         CR    @09,@14                                          03S0444
         BNE   @RF00444                                         03S0444
*             DO;                                               03S0445
*                                                               03S0445
*               /*****************************************************/
*               /*                                                   */
*               /* SUBAREA LENGTH IS EQUAL. NEXT, USE SEARCHER TO    */
*               /* OBTAIN THE ADDR. OF THE HOST SEGMENT ENTRY IN THE */
*               /* HEADER SEGMENT.                                   */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0446
*               SRCH=RSTHSTSG;      /* PARM INDICATING THAT THE HOST
*                                      SEGMENT ENTRY SHOULD BE  03S0446
*                                      LOCATED                       */
         MVI   SRCH(RDATA),X'02'                                03S0446
*               CALL SEARCHER;      /* SETS ENTRYPTR WITH THE ADDR.
*                                      OF THE HOST SEGMENT ENTRY IN
*                                      THE HEADER SEGMENT            */
         BAL   @14,SEARCHER                                     03S0447
*               IF ERRFLG=OFF THEN  /* IF NO ERROR WAS ENCOUNTERED   */
         TM    ERRFLG(RDATA),B'10000000'                        03S0448
         BNZ   @RF00448                                         03S0448
*                 DO;                                           03S0449
*                                                               03S0449
*                   /*************************************************/
*                   /*                                               */
*                   /* LOCATE THE BEGINNING OF THE ENTRIES IN THE    */
*                   /* HOST SEGMENT                                  */
*                   /*                                               */
*                   /*************************************************/
*                                                               03S0450
*                   ENTRYPTR=RRTPTR+RSTSGOFF;/* ADD THE OFFSET OF THE
*                                      HOST SEGMENT TO THE ADDR. OF
*                                      THE RST TO OBTAIN        03S0450
*                                      ADDRESSABILITY TO THE HOST
*                                      SEGMENT                       */
         L     @14,ENTRYPTR(,RDATA)                             03S0450
         ICM   @11,15,RSTSGOFF(@14)                             03S0450
         AL    @11,RRTPTR(,RDATA)                               03S0450
         ST    @11,ENTRYPTR(,RDATA)                             03S0450
*                   CTR=RSTNOENT;   /* SET COUNTER TO NUMBER OF 03S0451
*                                      ENTRIES IN HOST SEGMENT       */
         SLR   @14,@14                                          03S0451
         ICM   @14,3,RSTNOENT(@11)                              03S0451
         ST    @14,CTR(,RDATA)                                  03S0451
*                   ENTRYSIZ=RSTENTSZ;/* COPY ENTRY SIZE             */
         SLR   @14,@14                                          03S0452
         ICM   @14,3,RSTENTSZ(@11)                              03S0452
         ST    @14,ENTRYSIZ(,RDATA)                             03S0452
*                   ENTRYPTR=ENTRYPTR+RSTSEGHL;/* ADD SEGMENT HEADER
*                                      LENGTH TO OBTAIN ADDR. OF
*                                      START OF ENTRIES              */
*                                                               03S0453
         SLR   @14,@14                                          03S0453
         IC    @14,RSTSEGHL(,@11)                               03S0453
         ALR   @14,@11                                          03S0453
         ST    @14,ENTRYPTR(,RDATA)                             03S0453
*                   /*************************************************/
*                   /*                                               */
*                   /* ATTEMPT TO LOCATE TCAM'S SUBAREA IN HOST      */
*                   /* SEGMENT ENTRIES                               */
*                   /*                                               */
*                   /*************************************************/
*                                                               03S0454
*                   ERRFLG=ON;      /* INIT FOR LOOP                 */
         OI    ERRFLG(RDATA),B'10000000'                        03S0454
*                   DO WHILE(CTR^=0);/* CTR INITIALLY SET TO NUMBER
*                                      OF ENTRIES                    */
         B     @DE00455                                         03S0455
@DL00455 DS    0H                                               03S0456
*                     RZERO=SAVTSUBV;/* COPY TCAM SUBAREA            */
         L     @14,AVTSAVTP(,RAVT)                              03S0456
         SLR   RZERO,RZERO                                      03S0456
         IC    RZERO,SAVTSUBV(,@14)                             03S0456
*                     ISOSUBA=(8*LENGTH(SAVTSUBV))-SAVTSUBL;/* NO.
*                                      BITS TO RT. JUSTIFY           */
         LA    @11,8                                            03S0457
         SLR   @09,@09                                          03S0457
         IC    @09,SAVTSUBL(,@14)                               03S0457
         SLR   @11,@09                                          03S0457
         STC   @11,ISOSUBA(,RDATA)                              03S0457
*                     SRL(RZERO,ISOSUBA);/* RIGHT JUSTIFY SUBAREA    */
         SRL   RZERO,0(@11)                                     03S0458
*                     IF RZERO=RSTHSTSA THEN/* IF THIS IS TCAM'S
*                                      SUBAREA                       */
         L     @14,ENTRYPTR(,RDATA)                             03S0459
         SLR   @11,@11                                          03S0459
         ICM   @11,3,RSTHSTSA(@14)                              03S0459
         CR    RZERO,@11                                        03S0459
         BNE   @RF00459                                         03S0459
*                       DO;                                     03S0460
*                         ERRFLG=OFF;/* TURN OFF ERROR FLAG          */
         NI    ERRFLG(RDATA),B'01111111'                        03S0461
*                         CTR=0;    /* SET TO EXIT LOOP              */
         SLR   @14,@14                                          03S0462
         ST    @14,CTR(,RDATA)                                  03S0462
*                       END;                                    03S0463
*                     ELSE                                      03S0464
*                       DO;                                     03S0464
         B     @RC00459                                         03S0464
@RF00459 DS    0H                                               03S0465
*                         ENTRYPTR=ENTRYPTR+ENTRYSIZ;/* POINT TO NEXT
*                                      ENTRY                         */
         L     @14,ENTRYPTR(,RDATA)                             03S0465
         AL    @14,ENTRYSIZ(,RDATA)                             03S0465
         ST    @14,ENTRYPTR(,RDATA)                             03S0465
*                         CTR=CTR-1;/* DECREMENT LOOP COUNTER        */
         L     @14,CTR(,RDATA)                                  03S0466
         BCTR  @14,0                                            03S0466
         ST    @14,CTR(,RDATA)                                  03S0466
*                       END;                                    03S0467
*                   END;            /* END OF LOOP                   */
@RC00459 DS    0H                                               03S0468
@DE00455 L     @14,CTR(,RDATA)                                  03S0468
         LTR   @14,@14                                          03S0468
         BNZ   @DL00455                                         03S0468
*                   IF ERRFLG=OFF THEN/* IF TCAM'S SUBAREA FOUND     */
         TM    ERRFLG(RDATA),B'10000000'                        03S0469
         BNZ   @RF00469                                         03S0469
*                     IF(RSTKEYLN=AVTKEYLE)&/* IF NCP AND TCAM UNIT
*                                      SIZE IS THE SAME              */
*                         ((RSTBUFPD>=MINBUFPD)&(RSTBUFPD<=(AVTKEYLE-
*                         TXTBFPSZ)))/* IF CORRECT PAD SIZE SPECIFIED
*                                      IN THE RRT                    */
*                       THEN                                    03S0470
         L     @14,ENTRYPTR(,RDATA)                             03S0470
         LH    @11,AVTKEYLE(,RAVT)                              03S0470
         SLR   @09,@09                                          03S0470
         ICM   @09,3,RSTKEYLN(@14)                              03S0470
         CR    @11,@09                                          03S0470
         BNE   @RF00470                                         03S0470
         SLR   @09,@09                                          03S0470
         ICM   @09,3,RSTBUFPD(@14)                              03S0470
         C     @09,@CF01873                                     03S0470
         BL    @RF00470                                         03S0470
         SL    @11,@CF02913                                     03S0470
         CR    @09,@11                                          03S0470
         BH    @RF00470                                         03S0470
*                       DO;                                     03S0471
*                         PADSIZE=RSTBUFPD;/* SAVE PADSIZE FOR LATER
*                                      USE                   @VS44922*/
*                                                               03S0472
         STC   @09,PADSIZE(,RDATA)                              03S0472
*                         /*******************************************/
*                         /*                                         */
*                         /* SET UNIT COUNT IN DCB                   */
*                         /*                                         */
*                         /*******************************************/
*                                                               03S0473
*                         DCBUNTCT=RSTUNITN;/* SET NUMBER OF UNITS IN
*                                      BUFFER                        */
*                                                               03S0473
         L     @11,TRMDESTQ-1(,RTRM)                            03S0473
         L     @11,QCBDCBAD-1(,@11)                             03S0473
         MVC   DCBUNTCT+48(1,@11),RSTUNITN+1(@14)               03S0473
*                         /*******************************************/
*                         /*                                         */
*                         /* LOCATE RESOURCE SEGMENT USING SEARCHER  */
*                         /*                                         */
*                         /*******************************************/
*                                                               03S0474
*                         SRCH=RSTRSORC;/* PARM TO INDICATE RESOURCE
*                                      SEG.                          */
         MVI   SRCH(RDATA),X'04'                                03S0474
*                         CALL SEARCHER;/* SETS ENTRYPTR TO THE ADDR.
*                                      OF THE RESOURCE SEGMENT ENTRY
*                                      IN THE HEADER SEGMENT         */
*                                                               03S0475
         BAL   @14,SEARCHER                                     03S0475
*                         /*******************************************/
*                         /*                                         */
*                         /* OBTAIN NUMBER OF ENTRIES AND NUMBER OF  */
*                         /* PRE-SNA ENTRIES. ALSO, OBTAIN ADDR. OF  */
*                         /* FIRST ENTRY IN THE RESOURCE SEGMENT     */
*                         /*                                         */
*                         /*******************************************/
*                                                               03S0476
*                         ENTRYPTR=RSTSGOFF+RRTPTR;/* ADD OFFSET OF
*                                      RESOURCE SEGMENT TO THE RST
*                                      ADDRESS TO POINT TO RESOURCE
*                                      SEGMENT                       */
         L     @14,ENTRYPTR(,RDATA)                             03S0476
         ICM   @11,15,RSTSGOFF(@14)                             03S0476
         AL    @11,RRTPTR(,RDATA)                               03S0476
         ST    @11,ENTRYPTR(,RDATA)                             03S0476
*                         ENTRYNUM=RSTNOENT;                    03S0477
         SLR   @14,@14                                          03S0477
         ICM   @14,3,RSTNOENT(@11)                              03S0477
         ST    @14,ENTRYNUM(,RDATA)                             03S0477
*                         PSNANUM=RSTPSNAN;/* COPY NO. OF PRE-SNA
*                                      RESOURCES                     */
         SLR   @14,@14                                          03S0478
         ICM   @14,3,RSTPSNAN(@11)                              03S0478
         ST    @14,PSNANUM(,RDATA)                              03S0478
*                         ENTRYSIZ=RSTENTSZ;/* COPY ENTRY LENGTH     */
         SLR   @14,@14                                          03S0479
         ICM   @14,3,RSTENTSZ(@11)                              03S0479
         ST    @14,ENTRYSIZ(,RDATA)                             03S0479
*                         RSCPTR=ENTRYPTR+RSTSEGHL;/* ADD SEGMENT
*                                      HEADER LENGTH TO OBTAIN  03S0480
*                                      ADDRESS OF RESOURCE SEG. 03S0480
*                                      ENTRIES                       */
         SLR   @14,@14                                          03S0480
         IC    @14,RSTSEGHL(,@11)                               03S0480
         ALR   @14,@11                                          03S0480
         ST    @14,RSCPTR(,RDATA)                               03S0480
*                       END;                                    03S0481
*                     ELSE                                      03S0482
*                       ERRFLG=ON;  /* INDICATE ERROR                */
         B     @RC00470                                         03S0482
@RF00470 OI    ERRFLG(RDATA),B'10000000'                        03S0482
*                   ELSE                                        03S0483
*                     ;                                         03S0483
@RF00469 DS    0H                                               03S0484
*                 END;                                          03S0484
*               ELSE                                            03S0485
*                 ;                                             03S0485
@RF00448 DS    0H                                               03S0486
*             END;                                              03S0486
*           ELSE                                                03S0487
*             ERRFLG=ON;            /* INDICATE WRONG SUBAREA LGTH.  */
         B     @RC00444                                         03S0487
@RF00444 OI    ERRFLG(RDATA),B'10000000'                        03S0487
*                                                               03S0488
*           /*********************************************************/
*           /*                                                       */
*           /* THIS ROUTINE SEARCHES THE HEADER SEGMENT (NCP6 RRT)   */
*           /* ENTRIES AND LOCATES THE ENTRY FOR THE SEGMENT TYPE AS */
*           /* SPECIFIED BY THE CODE CONTAINED IN THE PARAMETER SRCH.*/
*           /* ON EXIT, ENTRYPTR POINTS TO THIS ENTRY                */
*           /*                                                       */
*           /*********************************************************/
*                                                               03S0488
*SEARCHER:                                                      03S0488
*           PROC OPTIONS(NOSAVE,NOSAVEAREA);                    03S0488
         B     @PB00002                                         03S0488
SEARCHER DS    0H                                               03S0489
*           RFY                                                 03S0489
*             RSTSEG BASED(RRTPTR); /* BASE SEGMENT HEADER ON RRTPTR
*                                      FOR HEADER SEGMENT'S HEADER   */
*           RFY                                                 03S0490
*             RRET RSTD;                                        03S0490
*           ENTRYPTR=RRTPTR+RSTSEGHL;/* POINT TO FIRST ENTRY IN 03S0491
*                                      HEADER SEGMENT                */
         L     @11,RRTPTR(,RDATA)                               03S0491
         SLR   @09,@09                                          03S0491
         IC    @09,RSTSEGHL(,@11)                               03S0491
         ALR   @09,@11                                          03S0491
         ST    @09,ENTRYPTR(,RDATA)                             03S0491
*           LOOPSW=ON;              /* INIT LOOP SWITCH              */
         OI    LOOPSW(RDATA),B'00100000'                        03S0492
*           DO UNTIL(LOOPSW=OFF);                               03S0493
@DL00493 DS    0H                                               03S0494
*             IF SRCH=RSTSEGTY THEN /* IF PARM MATCHES SEGMENT TYPE  */
         L     @11,ENTRYPTR(,RDATA)                             03S0494
         CLC   SRCH(1,RDATA),RSTSEGTY(@11)                      03S0494
         BNE   @RF00494                                         03S0494
*               LOOPSW=OFF;         /* SET TO GET OUT OF LOOP        */
         NI    LOOPSW(RDATA),B'11011111'                        03S0495
*             ELSE                                              03S0496
*               IF SRCH=RSTEND THEN                             03S0496
         B     @RC00494                                         03S0496
@RF00494 CLI   SRCH(RDATA),X'FF'                                03S0496
