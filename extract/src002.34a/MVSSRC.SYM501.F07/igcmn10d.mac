         TITLE 'IGCMN10D, OP CTL START/STOP PIU TRACE ROUTINE          *
                        '
IGCMN10D CSECT ,                                                   0001
@PROLOG  BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*                                   /*
IGCMN10D IEDHJN AFTERHJN,HJN            IDENTIFICATION MACRO
*                                                                  0259
*/********************************************************************/
*/*                                                                  */
*/*           SET UP REGISTERS                                       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0259
*   ROPCAVT=RPARM;                  /* OP CNTRL AVT ADDR     @Y17XARW*/
         LR    ROPCAVT,RPARM                                       0259
*   OPCFLAG1=0;                     /* INITIALIZE FLAGS      @Y17XARW*/
         MVI   OPCFLAG1(ROPCAVT),X'00'                             0260
*   OCUNIT=INVALTIC;                /* INIT INVALID TIC      @YM07774*/
         L     @15,INVALTIC                                        0261
         ST    @15,OCUNIT(,ROPCE)                                  0261
*   GEN(LM     3,7,OPCSAVE(ROPCAVT));/*                      @Y17XARW*/
         LM     3,7,OPCSAVE(ROPCAVT)
*/********************************************************************/
*/*                                                                  */
*/*           CAN A PIU TRACE BE PERFORMED                   @Y17XARW*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0263
*   IF SAVTPIUT=0                   /* IS PIU TRACE          @Y17XARW*/
*     THEN                          /* TABLE PRESENT?                */
         L     @15,AVTSAVTP(,IEDQAPTR)                             0263
         L     @15,SAVTPIUT(,@15)                                  0263
         LTR   @15,@15                                             0263
         BNZ   @RF00263                                            0263
*     DO;                           /* IF NOT, THEN                  */
*       RMSGNUM=NOTPRSNT;           /* LOAD ERROR MESSAGE NO.        */
         LH    RMSGNUM,NOTPRSNT                                    0265
*       GOTO ERRHANDL;                                             0266
         B     ERRHANDL                                            0266
*     END;                                                         0267
*                                                                  0267
*/********************************************************************/
*/*                                                                  */
*/*           SCAN THE OPERANDS                                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0268
*   IF OCSWITCH^='04'X              /* IF ELEMENT DOES'T INDICATE    */
*     THEN                          /* PIU TRACE STOP,       @Y17XARW*/
@RF00263 CLI   OCSWITCH(ROPCE),X'04'                               0268
         BE    @RF00268                                            0268
*     DO;                           /* THEN                          */
*       GOTO ERRHAND1;                                             0270
         B     ERRHAND1                                            0270
*     END;                                                         0271
*   ELSE                                                           0272
*     ;                                                            0272
*                                                                  0272
@RF00268 DS    0H                                                  0273
*/********************************************************************/
*/*                                                                  */
*/*           SET DEFAULTS FOR COMMAND TYPES                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0273
*   LINENAME='0'B;                  /* DEFAULT TO GRPNAME            */
*   NCPNAME='0'B;                   /* DEFAULT TO GRPNAME    @Y17XARW*/
*   GROUPALL='1'B;                  /* DEFAULT TO GROUP WITH ALL     */
         OI    GROUPALL(RWORKA),B'01000000'                        0275
         NI    LINENAME(RWORKA),B'01011111'                        0275
*   OCRLN='00'X;                    /* INITIALIZE OCRLN TO 0         */
         MVI   OCRLN(ROPCE),X'00'                                  0276
*                                                                  0277
*/********************************************************************/
*/*                                                                  */
*/*           SCAN FOR GRPNAME,,                                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0277
*   IF NEXTCHAR=','                 /* IS RLN DEFALTED TO ALL?       */
*     THEN                          /* IF SO,                        */
         CLI   NEXTCHAR(RADVPTR),C','                              0277
         BNE   @RF00277                                            0277
*     DO;                           /* THEN                          */
*       RADVPTR=RADVPTR+1;          /* ADD ONE TO FORWARD PTR AND    */
         LA    RADVPTR,1(,RADVPTR)                                 0279
*       WORKREG=WORKREG-1;          /* SUBTRACT ONE FROM             */
         BCTR  WORKREG,0                                           0280
*     END;                          /* REMANING LENGTH               */
*/********************************************************************/
*/*                                                                  */
*/*           SCAN FOR GRPNAME,ALL,                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0282
*   ELSE                                                           0282
*     DO;                                                          0282
         B     @RC00277                                            0282
@RF00277 DS    0H                                                  0283
*       IF ALLCHAR='ALL'            /* IS RLN SPECIFYED ALL,         */
*         THEN                      /* IF YES                        */
         CLC   ALLCHAR(3,RADVPTR),@CC01361                         0283
         BNE   @RF00283                                            0283
*         DO;                       /* THEN                          */
*           RADVPTR=RADVPTR+3;      /* ADD THREE TO FWRD PT          */
         LA    @15,3                                               0285
         ALR   RADVPTR,@15                                         0285
*           WORKREG=WORKREG-3;      /* AND SUBTRACT THREE FROM     0286
*                                      REMANING LENGTH               */
         SLR   WORKREG,@15                                         0286
*           IF NEXTCHAR^=','        /* IS THERE AN INPROPER          */
*             THEN                  /* DELIMITER                     */
         CLI   NEXTCHAR(RADVPTR),C','                              0287
         BE    @RF00287                                            0287
*             DO;                                                  0288
*               GOTO ERRHAND1;                                     0289
         B     ERRHAND1                                            0289
*             END;                                                 0290
*           ELSE                                                   0291
*             DO;                                                  0291
@RF00287 DS    0H                                                  0292
*               RADVPTR=RADVPTR+1;  /* ADV PAST COMMAND              */
         LA    RADVPTR,1(,RADVPTR)                                 0292
*               WORKREG=WORKREG-1;  /* SUB 1 FROM LEN                */
         BCTR  WORKREG,0                                           0293
*             END;                                                 0294
*         END;                                                     0295
*                                                                  0295
*/********************************************************************/
*/*                                                                  */
*/*           SCAN FOR GRPNAME,RLN,                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0296
*       ELSE                                                       0296
*         DO;                                                      0296
         B     @RC00283                                            0296
@RF00283 DS    0H                                                  0297
*           IF NEXTCHAR>='0'&NEXTCHAR<250 THEN/* THEN CALL THE     0297
*                                      CONVERT TO                    */
         CLI   NEXTCHAR(RADVPTR),C'0'                              0297
         BL    @RF00297                                            0297
         CLI   NEXTCHAR(RADVPTR),250                               0297
         BNL   @RF00297                                            0297
*             DO;                   /* BINARY ROUTINE                */
*               GROUPALL='0'B;      /* INDICATE NOT ALL BUT RLN ON 0299
*                                      GRPNAME                       */
         NI    GROUPALL(RWORKA),B'10111111'                        0299
*               CALL CNVTBIN;                                      0300
         BAL   @14,CNVTBIN                                         0300
*             END;                                                 0301
*                                                                  0302
*/********************************************************************/
*/*                                                                  */
*/*     IF NOT A GRPNAME, SET LINENAME AND NCPNAME INDICATOR @Y17XARW*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0302
*           ELSE                                                   0302
*             DO;                   /* NOT GRPNAME                   */
         B     @RC00297                                            0302
@RF00297 DS    0H                                                  0303
*               LINENAME='1'B;      /* SET LINENAME CMD TYPE         */
*               NCPNAME='1'B;       /* SET NCPNAME TYPE      @Y17XARW*/
         OI    LINENAME(RWORKA),B'10100000'                        0304
*             END;                  /*                       @Y17XARW*/
*         END;                                                     0306
@RC00297 DS    0H                                                  0307
*     END;                                                         0307
*                                                                  0307
@RC00283 DS    0H                                                  0308
*/********************************************************************/
*/*                                                                  */
*/*           IS COMMAND A START OR STOP                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0308
*   IF ONCHAR='ON'                  /* IF THE 'ON' OPERAND           */
*     THEN                          /* IS PRESENT,                   */
@RC00277 CLC   ONCHAR(2,RADVPTR),@CC01364                          0308
         BNE   @RF00308                                            0308
*     DO;                           /* THEN                          */
*       OCSWITCH='05'X;             /* INDICATE IT IN THE ELEMENT    */
         MVI   OCSWITCH(ROPCE),X'05'                               0310
*       RADVPTR=RADVPTR+2;          /* AND UPDATE ADVANSED POINTR    */
         LA    @15,2                                               0311
         ALR   RADVPTR,@15                                         0311
*       WORKREG=WORKREG-2;          /* AND REMANING LENGTH.          */
         SLR   WORKREG,@15                                         0312
*     END;                                                         0313
*   ELSE                                                           0314
*     DO;                                                          0314
         B     @RC00308                                            0314
@RF00308 DS    0H                                                  0315
*       IF OFFCHAR='OFF'            /* IF THE 'OFF' OPERAND          */
*         THEN                      /* IS PRESENT, THEN              */
         CLC   OFFCHAR(3,RADVPTR),@CC01365                         0315
         BNE   @RF00315                                            0315
*         DO;                       /* INDICATE IT IN                */
*           RADVPTR=RADVPTR+3;      /* THE ELEMENT AND               */
         LA    @15,3                                               0317
         ALR   RADVPTR,@15                                         0317
*           WORKREG=WORKREG-3;      /* UPDATE ADVANSED PTR AND     0318
*                                      REMANING LENGTH.              */
         SLR   WORKREG,@15                                         0318
*         END;                                                     0319
*       ELSE                        /* IF THE 'ON' OR 'OFF'          */
*         DO;                       /* OPERANDS AREN'T PRESENT       */
         B     @RC00315                                            0320
@RF00315 DS    0H                                                  0321
*           GOTO ERRHAND1;                                         0321
         B     ERRHAND1                                            0321
*         END;                                                     0322
*     END;                                                         0323
@RC00315 DS    0H                                                  0324
*                                                                  0324
*/********************************************************************/
*/*                                                                  */
*/*           CHECK FOR REMAINING OPERANDS                           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0324
*   IF WORKREG^=0                   /* IF THE BTRACE CMD CONTAINS    */
*     THEN                                                         0324
@RC00308 SLR   @15,@15                                             0324
         CR    WORKREG,@15                                         0324
         BE    @RF00324                                            0324
*     IF WORKREG>0                  /* CHARACTERS FOLLOWING THE      */
*       THEN                                                       0325
         CR    WORKREG,@15                                         0325
         BNH   @RF00325                                            0325
*       IF NEXTCHAR^=' '            /* 'ON/OFF' OPERAND AND THE      */
*         THEN                      /* NEXT CHARACTER ISN'T A        */
         CLI   NEXTCHAR(RADVPTR),C' '                              0326
         BE    @RF00326                                            0326
*         DO;                       /* BLANK THEN CALL               */
*           GOTO ERRHAND1;                                         0328
         B     ERRHAND1                                            0328
*         END;                                                     0329
*       ELSE                                                       0330
*         ;                                                        0330
@RF00326 DS    0H                                                  0331
*     ELSE                          /* IF REMANING LENGTH IS         */
*       DO;                         /* LESS THAN ZERO THEN           */
         B     @RC00325                                            0331
@RF00325 DS    0H                                                  0332
*         GOTO ERRHAND1;                                           0332
         B     ERRHAND1                                            0332
*       END;                                                       0333
*   ELSE                                                           0334
*     ;                                                            0334
@RF00324 DS    0H                                                  0335
*                                                                  0335
*/********************************************************************/
*/*                                                                  */
*/*      FIND TTE FOR GRPNAME, LINENAME, OR NCPNAME.         @Y17XARW*/
*/*  THEN SET TRACE BITS IN ALL RESOURCES FOUND IN HIERARCHY @Y17XARW*/
*/*  BELOW INPUT TTE(NCP, LINE, OR GROUP), AS WELL AS BITS   @Y17XARW*/
*/*  IN NCP AND LINE TTE'S.  TRACE BITS ARE NOT SET IN GROUP @Y17XARW*/
*/*  TTES.                                                   @Y17XARW*/
*/********************************************************************/
*                                                                  0335
*   OCBACKUP=OPCDOUBL;              /* PLACE LINE NAME IN ELEMENT    */
@RC00324 MVC   OCBACKUP(8,ROPCE),OPCDOUBL(ROPCAVT)                 0335
*   SWITCHES='00'X;                 /* ZERO THE SWITCH FIELD         */
         MVI   SWITCHES(RWORKA),X'00'                              0336
*   IF LINENAME='0'B&NCPNAME='0'B&  /* IF THIS IS GROUP      @Y17XARW*/
*       GROUPALL='0'B THEN          /* WITH RLN REQUEST,     @Y17XARW*/
         TM    LINENAME(RWORKA),B'11100000'                        0337
         BNZ   @RF00337                                            0337
*     DO;                           /* FIND LINE ENTRY       @Y17XARW*/
*       CALL FINDENTY;              /* GOTO DCBLOCAT         @Y17XARW*/
         BAL   @14,FINDENTY                                        0339
*       IEDQTPTR=RMSGNUM;           /* SETUP LINE ENTRY      @Y17XARW*/
         STCM  RMSGNUM,7,IEDQTPTR(RWORKA)                          0340
*       IF TRMPREF='0'B×((TRMTYPE^=TRMSDLC)&(TRMTYPE^=TRMNSDLC))/* IF
*                                      THIS IS NOT A LINE NOR        */
*         THEN                                                     0341
         L     @15,IEDQTPTR-1(,RWORKA)                             0341
         LA    @15,0(,@15)                                         0341
         TM    TRMPREF(@15),B'00010000'                            0341
         BZ    @RT00341                                            0341
         SL    @15,@CF01393                                        0341
         CLI   TRMTYPE(@15),X'21'                                  0341
         BE    @RF00341                                            0341
         CLI   TRMTYPE(@15),X'20'                                  0341
         BNE   @RT00341                                            0341
*         GOTO ERRHAND1;            /* A 3705 ENTRY,         @Y17XARW
*                                      GOTO ERROR RTN        @Y17XARW*/
*     END;                          /*                       @Y17XARW*/
*   ELSE                                                           0344
*     CALL TTESRCH;                 /* ELSE FIND TTE FOR NCPNAME,  0344
*                                      LINENAME OR GRPALL    @Y17XARW*/
         B     @RC00337                                            0344
@RF00337 BAL   @14,TTESRCH                                         0344
*   IF GROUPALL='1'B THEN           /* IF GROUPALL RQST      @Y17XARW*/
@RC00337 TM    GROUPALL(RWORKA),B'01000000'                        0345
         BNO   @RF00345                                            0345
*     CALL SCANGRP;                 /* SCAN GROUP TTE        @Y17XARW*/
         BAL   @14,SCANGRP                                         0346
*   ELSE                            /* ELSE SCAN LINE OR     @Y17XARR*/
*     DO;                           /* NCP TTE               @Y17XARW*/
         B     @RC00345                                            0347
@RF00345 DS    0H                                                  0348
*       IF TRMLINE='1'B THEN        /* LINE?                 @Y17XARR*/
         L     @15,IEDQTPTR-1(,RWORKA)                             0348
         LA    @15,0(,@15)                                         0348
         TM    TRMLINE(@15),B'10000000'                            0348
         BNO   @RF00348                                            0348
*         SAVELINE=IEDQTPTR;        /* SAVE LINE TTE         @Y17XARR*/
         LA    @14,OPCWRKSP+12(,ROPCAVT)                           0349
         ST    @15,SAVELINE(,@14)                                  0349
*       ELSE                                                       0350
*         ;                         /* FALL THROUGH          @Y17XARR*/
@RF00348 DS    0H                                                  0351
*       CALL SCANTTE;               /* SCAN                  @Y17XARR*/
         BAL   @14,SCANTTE                                         0351
*     END;                          /*                       @Y17XARR*/
*                                                                  0353
*/********************************************************************/
*/*                                                                  */
*/*           SELECT THE PROPER COMPLETION MESSAGE                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0353
*INDICATE:                                                         0353
*   IF TURNEDON='1'B                /* IF THE TRACE BIT HAS          */
*     THEN                          /* BEEN TURNED ON AND            */
@RC00345 DS    0H                                                  0353
INDICATE TM    TURNEDON(RWORKA),B'00010000'                        0353
         BNO   @RF00353                                            0353
*     IF WASON='1'B                 /* IF A TRACE BIT WAS ON THEN SET*/
*       THEN                        /* ERRMSG FOR SAME PIU   @Y17XARW*/
         TM    WASON(RWORKA),B'01000000'                           0354
         BNO   @RF00354                                            0354
*       RMSGNUM=SOMEON;             /* TRACE BITS ALREADY ON.        */
         LH    RMSGNUM,SOMEON                                      0355
*     ELSE                          /* IF TRACE BIT WAS TURNED ON    */
*       RMSGNUM=TRACEON;            /* THEN SET PROPER MSG.          */
         B     @RC00354                                            0356
@RF00354 LH    RMSGNUM,TRACEON                                     0356
*   ELSE                                                           0357
*     IF TURNDOFF='1'B              /* IF TRACE BIT HAS              */
*       THEN                        /* BEEN TURNED OFF AND           */
         B     @RC00353                                            0357
@RF00353 TM    TURNDOFF(RWORKA),B'00100000'                        0357
         BNO   @RF00357                                            0357
*       IF WASOFF='1'B              /* IF A TRACE BIT WAS ON         */
*         THEN                      /* THEN SET ERRMSG FOR SOME      */
         TM    WASOFF(RWORKA),B'10000000'                          0358
         BNO   @RF00358                                            0358
*         RMSGNUM=SOMEOFF;          /* PIU TRACE BITS        @Y17XARW
*                                      ALREADY ON            @Y17XARW*/
         LH    RMSGNUM,SOMEOFF                                     0359
*       ELSE                        /* IF TRACE WAS TURNED OFF       */
*         RMSGNUM=TRACEOFF;         /* THEN SET PROPER MSG.          */
         B     @RC00358                                            0360
@RF00358 LH    RMSGNUM,TRACEOFF                                    0360
*     ELSE                          /* IF THE PIU            @Y17XARW*/
*       IF WASON='1'B               /* TRACE WAS ALREADY             */
*         THEN                      /* ON THEN SET UP                */
         B     @RC00357                                            0361
@RF00357 TM    WASON(RWORKA),B'01000000'                           0361
         BNO   @RF00361                                            0361
*         RMSGNUM=TRWASON;          /* PROPER MESSAGE.               */
         LH    RMSGNUM,TRWASON                                     0362
*       ELSE                        /* SET UP FOR MESSAGE SAYING     */
*         RMSGNUM=TRWASOFF;         /* PIU TRACE             @Y17XARW
*                                      ALREADY OFF           @Y17XARW*/
         B     @RC00361                                            0363
@RF00361 LH    RMSGNUM,TRWASOFF                                    0363
*   OPCLDNME(4:5)='M3';             /* MESSAGE WRITER MODULE         */
@RC00361 DS    0H                                                  0364
@RC00357 DS    0H                                                  0364
@RC00353 MVC   OPCLDNME+3(2,ROPCAVT),@CC01369                      0364
*                                                                  0365
*/********************************************************************/
*/*                                                                  */
*/*           SET UP TO EXIT                                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0365
*XCTL:                                                             0365
*   IF LINENAME='1'B×NCPNAME='1'B THEN/* IF THIS IS LINENME  @Y17XARW*/
XCTL     TM    LINENAME(RWORKA),B'10100000'                        0365
         BZ    @RF00365                                            0365
*     DO;                           /* OR NCPNAME THEN       @Y17XARW*/
*       OCLINE='1'B;                /* INDICATE LINE NAME            */
         OI    OCLINE(ROPCE),B'00010000'                           0367
*       OCRLN='00'X;                /* CLEAR RLN INDICATOR           */
         MVI   OCRLN(ROPCE),X'00'                                  0368
*     END;                                                         0369
*   ELSE                            /* NO, GRPNAME REQUEST           */
*     DO;                                                          0370
         B     @RC00365                                            0370
@RF00365 DS    0H                                                  0371
*       OCLINE='0'B;                /* INDICATE NOT LINE BUT GROUP   */
         NI    OCLINE(ROPCE),B'11101111'                           0371
*       IF GROUPALL='1'B THEN       /* GRPNAME,ALL CMD REQUEST ?     */
         TM    GROUPALL(RWORKA),B'01000000'                        0372
         BNO   @RF00372                                            0372
*         OCRLN='00'X;              /* YES, CLEAR RLN INDICATOR      */
         MVI   OCRLN(ROPCE),X'00'                                  0373
*     END;                                                         0374
@RF00372 DS    0H                                                  0375
*   RPARM=ROPCAVT;                  /* PASS AVT ADDRESS              */
@RC00365 LR    RPARM,ROPCAVT                                       0375
*   GEN(XCTL  SF=(E,OPCXCTL(ROPCAVT)));                            0376
         XCTL  SF=(E,OPCXCTL(ROPCAVT))
*   GEN(EJECT );                                                   0377
         EJECT
*ERRHAND1:                                                         0378
*   RMSGNUM=INVALMSG;               /* SET UP FOR MSG HANDLER        */
ERRHAND1 LH    RMSGNUM,INVALMSG                                    0378
*ERRHANDL:                                                         0379
*                                                                  0379
*/********************************************************************/
*/*                                                                  */
*/*           THIS ROUTINE WILL PASS CONTROL TO A MESSAGE HANDLING   */
*/*        MODULE, WHICH WILL PRINT THE APPOPRIATE ERROR MESSAGE.    */
*/*           TO USE THIS SUBROUTINE REG. 0 MUST CONTAIN THE         */
*/*        ERROR MESSAGE NUMBER.                                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0379
*   OPCLDNME(4:5)='03';             /* ERR MSG MODULE SET UP         */
ERRHANDL MVC   OPCLDNME+3(2,ROPCAVT),@CC01373                      0379
*   GOTO XCTL;                      /* EXIT                          */
         B     XCTL                                                0380
*SETTRACE:                                                         0381
*                                                                  0381
*/********************************************************************/
*/*                                                                  */
*/*        THIS SUBROUTINE WILL START OR STOP THE PIUTRACE   @Y17XARW*/
*/*        FACILITY. IT WILL INDICATE THE FUNCTION PERFORMED AND     */
*/*        THE PREVIOUS STATE OF THE PIU TRACE.              @Y17XARW*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0381
*   RESPECIFY                                                      0381
*    (RRET,                                                        0381
*     RREGSAV) RSTD;                /*                       @YM07774*/
SETTRACE DS    0H                                                  0382
*   RREGSAV=RRET;                   /* SAVE RETURN REG       @YM07774*/
         LR    RREGSAV,RRET                                        0382
*   RESPECIFY                                                      0383
*     RRET UNRSTD;                  /*                       @YM07774*/
*   SWITCHES='0'B;                  /* CLEAR SWITCH SET      @Y17XARR*/
         MVI   SWITCHES(RWORKA),B'00000000'                        0384
*   IF OCSWITCH='04'X               /* ARE WE TO TURN OFF            */
*     THEN                          /* PIU TRACE?            @Y17XARW*/
         CLI   OCSWITCH(ROPCE),X'04'                               0385
         BNE   @RF00385                                            0385
*     DO;                           /* IF SO, THEN                   */
*       IF TRMTRACE='0'B            /* IS TRACE ALREADY OFF?         */
*         THEN                                                     0387
         L     @08,IEDQTPTR-1(,RWORKA)                             0387
         AL    @08,@CF01410                                        0387
         TM    TRMTRACE-3(@08),B'00001000'                         0387
         BNZ   @RF00387                                            0387
*         WASOFF='1'B;              /* INDICATE IF TRACE ALREADY     */
         OI    WASOFF(RWORKA),B'10000000'                          0388
*       ELSE                        /* OFF.                          */
*         DO;                       /* ELSE TURN                     */
         B     @RC00387                                            0389
@RF00387 DS    0H                                                  0390
*           IF OCUNIT=INVALTIC THEN                                0390
         CLC   OCUNIT(4,ROPCE),INVALTIC                            0390
         BNE   @RF00390                                            0390
*             DO;                   /* IF FIRST ENTRY        @YM07774*/
*               RPARM=1;            /* SET NUMBER OF BFRS@YM07020    */
         LA    RPARM,1                                             0392
*               CALL GETBFR;        /* REQUEST A BUFFER      @YM07020*/
         L     @15,OPCGETBF(,ROPCAVT)                              0393
         BALR  @14,@15                                             0393
*               OCUNIT=RPARM;       /* LINK BFR IN TIC       @YM07020*/
         ST    RPARM,OCUNIT(,ROPCE)                                0394
*             END;                                                 0395
*           RPARM=OCUNIT;           /* INIT RPARM            @YM07774*/
@RF00390 L     RPARM,OCUNIT(,ROPCE)                                0396
*           SBTFLAGS=''B;           /* CLEAR FLAGS           @ZM47713*/
         MVI   SBTFLAGS(IEDQSBTR),X'00'                            0397
*           SBTECB=''B;             /* CLEAR ECB             @ZM47713*/
         SLR   @08,@08                                             0398
         ST    @08,SBTECB(,IEDQSBTR)                               0398
*           SBTQCBA=AVTSBTA;        /* SET QCB ADDRESS       @YM07020*/
         MVC   SBTQCBA(3,IEDQSBTR),AVTSBTA+1(IEDQAPTR)             0399
*           SBTECBN='1'B;           /* SET POST REQUEST      @YM07020*/
*           SBTNI='1'B;             /* REQUEST AND FUNCTION  @YM07020*/
         OI    SBTECBN(IEDQSBTR),B'10010000'                       0401
*           SBTPRI=PRISBT;          /* SET POST PRIORITY     @YM07020*/
         MVI   SBTPRI(IEDQSBTR),X'EC'                              0402
*           SBTDATA='FF'X;          /* SET ALL BITS ON       @YM07020*/
         MVI   SBTDATA(IEDQSBTR),X'FF'                             0403
*           SBTADDR=ADDR(TRMTRACE); /* SET ADDR OF BYTE      @YM07020*/
         L     @08,IEDQTPTR-1(,RWORKA)                             0404
         LA    @08,0(,@08)                                         0404
         AL    @08,@CF01410                                        0404
         STCM  @08,7,SBTADDR(IEDQSBTR)                             0404
