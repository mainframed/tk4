*     RERR UNRSTD;                  /*                       @Y17XART*/
*   RELEASE(RSPTR,RCNT);                                           0368
*   END SCAN;                                                      0369
*CONCTEST:                                                         0370
*   PROC OPTIONS(DONTSAVE,NOSAVEAREA);                             0370
CONCTEST DS    0H                                                  0371
*   GEN(EJECT) ;                                                   0371
*                                                                  0371
         EJECT
*/* THIS ROUTINE TEST THE TERMINALS VALIDITY. IT INSURES THAT THE    */
*/* TERMINAL IS NOT ANOTHER CONCENTRATOR AND ALSO CHECKS TO BE SURE  */
*/* THAT THE TERMINAL IS ATTACHED TO THE CORRECT CONCENTRATOR. IF    */
*/* THE TESTS FAIL AN ERROR MESSAGE IS ISSUED AND THE COMMAND IS     */
*/* ABORTED.                                                         */
*                                                                  0372
*   RESTRICT(ROPCE,ROPCAVT);                                       0372
*   RESTRICT(RQCB,RCONC);                                          0373
*   RESPECIFY                                                      0374
*     RLINK RSTD;                   /*                       @Y17XART*/
*   OPCSAVE=RLINK;                  /* SAVE RETURN REG               */
         ST    RLINK,OPCSAVE(,ROPCAVT)                             0375
*   RESPECIFY                                                      0376
*     RLINK UNRSTD;                 /*                       @Y17XART*/
*   RESPECIFY                                                      0377
*     RPARM RSTD;                   /*                       @Y17XART*/
*   IF TRMSTATE(1:3)^='000'B THEN                                  0378
         TM    TRMSTATE(RPARM),B'11100000'                         0378
         BZ    @RF00378                                            0378
*     DO;                           /* SINGLE ENTRY                  */
*       RESPECIFY                                                  0380
*         RERR RSTD;                /*                       @Y17XART*/
*       RERR=90;                    /* SET ERR MSG NUMBER            */
         LA    RERR,90                                             0381
*       GOTO ERREXIT;               /* EXIT                          */
         B     ERREXIT                                             0382
*       RESPECIFY                                                  0383
*         RERR UNRSTD;              /*                       @Y17XART*/
*     END;                                                         0384
*   IF TRMCONC^='1'B THEN                                          0385
@RF00378 TM    TRMCONC(RPARM),B'10000000'                          0385
         BO    @RF00385                                            0385
*     DO;                           /* CONCENTRATOR                  */
*       RESPECIFY                                                  0387
*         RERR RSTD;                /*                       @Y17XART*/
