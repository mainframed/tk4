SCAN     DS    0H                                                  0342
*   GEN(EJECT) ;                                                   0342
*                                                                  0342
         EJECT
*/* THIS ROUTINE SCANS FOR A COMMA OR A BLANK FROM WHERE THE SCAN    */
*/* POINTER (REG 7) IS POINTING. IF FOUND, IT INITIALIZES OCBACKUP   */
*/* WITH THE TERMINAL NAME. OTHERWISE AN ERROR MSG IS ISSUED.        */
*                                                                  0343
*   RESTRICT(ROPCE,ROPCAVT);                                       0343
*   RESTRICT(RSPTR,RSCAN,RCNT);                                    0344
*   GEN(SPACE 2) ;                                                 0345
         SPACE 2
*   RESPECIFY                                                      0346
*     RLINK RSTD;                   /*                       @Y17XART*/
*   OPCSAVE=RLINK;                  /* SAVE RETURN REG               */
         ST    RLINK,OPCSAVE(,ROPCAVT)                             0347
*   RESPECIFY                                                      0348
*     RLINK UNRSTD;                 /*                       @Y17XART*/
*   RSPTR=RSCAN;                    /* INIT SCAN PTR                 */
         LR    RSPTR,RSCAN                                         0349
*   DO RCNT=0 TO 8 BY 1;            /* SCAN FOR A COMMA              */
         SLR   RCNT,RCNT                                           0350
@DL00350 DS    0H                                                  0351
*     RSPTR=RSCAN+RCNT;             /* UPDATE SCAN PTR               */
         LR    RSPTR,RSCAN                                         0351
         ALR   RSPTR,RCNT                                          0351
*     IF DIGIT=','×                 /* IS THIS CHARACTER A COMMA     */
*         DIGIT=' ' THEN                                           0352
         CLI   DIGIT(RSPTR),C','                                   0352
         BE    @RT00352                                            0352
         CLI   DIGIT(RSPTR),C' '                                   0352
         BNE   @RF00352                                            0352
@RT00352 DS    0H                                                  0353
*       DO;                         /* OR A BLANK                    */
*         OCBACKUP=' ';             /* YES, BLANK RCVING FIELD       */
         MVI   OCBACKUP+1(ROPCE),C' '                              0354
         MVC   OCBACKUP+2(6,ROPCE),OCBACKUP+1(ROPCE)               0354
         MVI   OCBACKUP(ROPCE),C' '                                0354
*         OCBACKUP=FIELD(1:RCNT);   /* MOVE TERMINAL NAME            */
         MVI   OCBACKUP+1(ROPCE),C' '                              0355
         MVC   OCBACKUP+2(6,ROPCE),OCBACKUP+1(ROPCE)               0355
         LR    @12,RCNT                                            0355
         BCTR  @12,0                                               0355
         EX    @12,@SM01366                                        0355
*         OPCDOUBL=OCBACKUP;        /* SET FOR TRMLKUP       @XA06345*/
         MVC   OPCDOUBL(8,ROPCAVT),OCBACKUP(ROPCE)                 0356
*         RSCAN=RSCAN+RCNT+1;       /* UPDATE SCAN POINTER           */
         LR    @12,RSCAN                                           0357
         ALR   @12,RCNT                                            0357
         LA    @12,1(,@12)                                         0357
         LR    RSCAN,@12                                           0357
*         RESPECIFY                                                0358
*           RLINK RSTD;             /*                       @Y17XART*/
*         RLINK=OPCSAVE;            /* RESTORE RETURN ADDRESS        */
         L     RLINK,OPCSAVE(,ROPCAVT)                             0359
*         RETURN;                   /* RETURN TO CALLER              */
@EL00003 DS    0H                                                  0360
@EF00003 DS    0H                                                  0360
@ER00003 BR    @14                                                 0360
*         RESPECIFY                                                0361
*           RLINK UNRSTD;           /*                       @Y17XART*/
*       END;                                                       0362
*   END;                                                           0363
@RF00352 AL    RCNT,@CF00086                                       0363
         C     RCNT,@CF00071                                       0363
         BNH   @DL00350                                            0363
*   RESPECIFY                                                      0364
*     RERR RSTD;                    /*                       @Y17XART*/
*   RERR=18;                        /* ERROR MSG NUMBER              */
         LA    RERR,18                                             0365
