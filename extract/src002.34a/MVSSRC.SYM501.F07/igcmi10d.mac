*/*****************************FREEWA*****************************   */
*                                                                  0541
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SUBROUTINE FREES THE IPL/DUMP WORKAREA AND TURNS OFF THE */
*   /* IN PROGRESS BITS                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0541
*FREEWA:                                                           0541
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0541
FREEWA   DS    0H                                                  0542
*   RFY                                                            0542
*     RRETURN RSTD;                                                0542
*   OCTRSAVE=RRETURN;               /* SAVE RETURN ADDRESS           */
         ST    RRETURN,OCTRSAVE(,RTRAIL)                           0543
*   IF OCIPLWA^=NONE THEN           /* THERE IS A WORK AREA TO FREE  */
         L     @15,OCIPLWA(,ROPCE)                                 0544
         LTR   @15,@15                                             0544
         BZ    @RF00544                                            0544
*     DO;                                                          0545
*                                                                  0545
*       /*************************************************************/
*       /*                                                           */
*       /* FREE THE WORKAREA                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0546
*       RFY                                                        0546
*        (REG7,                                                    0546
*         REG1,                                                    0546
*         REG5) RSTD;                                              0546
*       REG7=ADDR(BLKADDR);         /* ADR OF FIRST BLOCK ADDRESS    */
         LA    REG7,BLKADDR(,@15)                                  0547
*       REG1=BLKADDR;               /* FIRST BLOCK ADDR              */
         L     REG1,BLKADDR(,@15)                                  0548
*       DO WHILE REG1^=NONE;        /* FREE ALL BLOCKS               */
         B     @DE00549                                            0549
@DL00549 DS    0H                                                  0550
*         RFY                                                      0550
*           ADDRBLK BASED(REG7);                                   0550
*         REG7=REG7+NXTBLK;         /* ADDR OF NEXT BLOCK ADDR       */
         AL    REG7,@CF02165                                       0551
*         REG5=ADDRBLK;             /* NEXT BLOCK ADDR               */
         L     REG5,ADDRBLK(,REG7)                                 0552
*         GEN REFS(REG1) EXIT NOFLOWS SEQFLOW;                     0553
    FREEMAIN R,LV=64,A=(1),SP=250
*         REG1=REG5;                                               0554
         LR    REG1,REG5                                           0554
*       END;                                                       0555
@DE00549 LTR   REG1,REG1                                           0555
         BNZ   @DL00549                                            0555
*       RFY                                                        0556
*         REG5 UNRSTD;                                             0556
*       REG1=WRKSIZE;               /* LENGTH OF WORKAREA            */
         LA    REG1,2048                                           0557
*       REG7=OCIPLWA;               /* ADDRESS OF WORKAREA           */
         L     REG7,OCIPLWA(,ROPCE)                                0558
*       GEN REFS(REG1) EXIT NOFLOWS SEQFLOW;                       0559
    FREEMAIN R,LV=(1),A=(7),SP=250
*       OCIPLWA=NONE;               /* CLEAR WORKAREA ADDRESS      0560
*                                                            @YM03717*/
         SLR   @15,@15                                             0560
         ST    @15,OCIPLWA(,ROPCE)                                 0560
*       RFY                                                        0561
*        (REG7,                                                    0561
*         REG1) UNRSTD;                                            0561
*     END;                                                         0562
*                                                                  0562
*   /*****************************************************************/
*   /*                                                               */
*   /* TURN OFF THE IN PROGRESS BITS USING COMPETING RESOURCES       */
*   /* ROUTINE                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0563
*   RFY                                                            0563
*    (REG1,                                                        0563
*     RSAV,                                                        0563
*     REG0,                                                        0563
*     RTRM) RSTD;                                                  0563
@RF00544 DS    0H                                                  0564
*   REG0=NONE;                      /* TO SIGNAL RESET INPROGRESS  0564
*                                      BITS                          */
         SLR   REG0,REG0                                           0564
*   RTRM=OCTRTTE;                   /* TTE ADDRESSABILITY            */
         L     RTRM,OCTRTTE(,RTRAIL)                               0565
*   REG1=ROPCE;                     /* OPCE ADDRESS                  */
         LR    REG1,ROPCE                                          0566
*   RSAV=ADDR(OPCSAVE);             /* SAVE AREA                     */
         LA    RSAV,OPCSAVE(,ROPCAVT)                              0567
*   CALL PROGRTNE;                                                 0568
         L     @15,OPCPGCK(,ROPCAVT)                               0568
         BALR  @14,@15                                             0568
*   RFY                                                            0569
*    (REG1,                                                        0569
*     REG0,                                                        0569
*     RSAV) UNRSTD;                                                0569
*   TRMIPLDM=OFF;                   /* TURN OFF IPL/DUMP IN PROGRESS */
         LR    @05,RTRM                                            0570
         AL    @05,@CF02299                                        0570
         LA    @09,TRMIPLDM-1(,@05)                                0570
         LR    @07,@09                                             0570
         LA    @15,3                                               0570
         NR    @07,@15                                             0570
         LCR   @07,@07                                             0570
         ALR   @09,@07                                             0570
         L     @15,0(,@09)                                         0570
@GL00001 LR    @11,@15                                             0570
         N     @11,@CB02295+3(@07)                                 0570
         CS    @15,@11,0(@09)                                      0570
         BNE   @GL00001                                            0570
*   RRETURN=OCTRSAVE;               /* RESTORE RETURN ADDRESS        */
         L     RRETURN,OCTRSAVE(,RTRAIL)                           0571
*   RFY                                                            0572
*    (RRETURN,                                                     0572
*     RTRM) UNRSTD;                                                0572
*   END FREEWA;                                                    0573
*                                                                  0573
@EL00004 DS    0H                                                  0573
@EF00004 DS    0H                                                  0573
@ER00004 BR    @14                                                 0573
*/**************************END OF FREEWA*************************   */
