         /************************************************************/
         /*                                                          */
         /*  'SWAPT   ' FUNCTION DATA TABLE DECLARATIONS             */
         /*                                                          */
         /************************************************************/
 %IF BASEFDT = 'YES'
   %THEN
     %GO TO BASEFDT1;

         DCL 1 FDTTBL      BDY(WORD),
 %GO TO BASEFDT2;
 %BASEFDT1:;

         DCL 1 FDTTBL      BASED   BDY(WORD),
 %BASEFDT2:;
               2 FDTVERB   CHAR(8),    /* FUNCTION VERBNAME          */
               2 FDTPTR(*) PTR(31);    /* SCALAR DATA POINTERS       */
