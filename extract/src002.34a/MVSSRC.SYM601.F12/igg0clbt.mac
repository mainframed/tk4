*             CALL IGGPBMR;         /* BIT SET FUNCTION              */
Z009BMR  L     @15,@CV00042                                        0188
         BALR  @14,@15                                             0188
*           END;                    /* END SET                       */
         LA    @10,1                                               0189
         AH    @10,CCAGVCT(,R11)                                   0189
@DE00182 STH   @10,CCAGVCT(,R11)                                   0189
         SLR   @05,@05                                             0189
         IC    @05,CTVNOEXT+566(,R11)                              0189
         CR    @10,@05                                             0189
         BNH   @DL00182                                            0189
