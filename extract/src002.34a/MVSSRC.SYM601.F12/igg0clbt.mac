*         DO;                       /* SET SPACE ALLOCATED           */
*           CCASRID=CCACBASE;       /* SET UP TO RETRIEVE BASE       */
*                                                                  0177
         MVC   CCASRID(3,R11),CCACBASE(R11)                        0177
*           /*********************************************************/
*           /*                                                       */
*           /* CALL TO MODULE IGG0CLBA                               */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0178
*Z008GREC:                                                         0178
*           CALL IGGPGREC;          /* REGET BASE                    */
Z008GREC L     @15,@CV00043                                        0178
         BALR  @14,@15                                             0178
*           CCABMFLG=0;             /* CLEAR FLAGS                   */
         MVI   CCABMFLG(R11),X'00'                                 0179
*           CCABMST=OFF;            /* INDICATE ALLOCATED            */
*           CCABMSET=ON;            /* USE BIT SET FUNCTION          */
         OI    CCABMSET(R11),B'00100000'                           0181
         NI    CCABMST(R11),B'01111111'                            0181
*           DO CCAGVCT=1 TO CTVNOEXT;/* SET ALL EXTENTS ALLOCATED    */
         LA    @10,1                                               0182
         B     @DE00182                                            0182
@DL00182 DS    0H                                                  0183
*             CCABMTRK=CTVEXTT(CCAGVCT);/* SET RELATIVE TRK NO.      */
         LH    @10,CCAGVCT(,R11)                                   0183
         LR    @05,@10                                             0183
         SLA   @05,2                                               0183
         LA    @15,566                                             0183
         ALR   @15,R11                                             0183
         ST    @05,@TF00001                                        0183
         ALR   @05,@15                                             0183
         ICM   @05,12,CTVEXTT-4(@05)                               0183
         SRA   @05,16                                              0183
         STH   @05,CCABMTRK(,R11)                                  0183
*             CCABMLIM=CTVEXTNN(CCAGVCT);/* SET NO. TRKS IN EXTENT   */
         LR    @05,@15                                             0184
         AL    @05,@TF00001                                        0184
         ICM   @05,12,CTVEXTNN-4(@05)                              0184
         SRA   @05,16                                              0184
         STH   @05,CCABMLIM(,R11)                                  0184
*             IF CCAGVCT=CTVNOEXT THEN/* IF LAST EXTENT SET          */
         SLR   @05,@05                                             0185
         IC    @05,CTVNOEXT(,@15)                                  0185
         CR    @10,@05                                             0185
         BNE   @RF00185                                            0185
*               CCABMLST=ON;        /* INDICATE LAST CALL            */
         OI    CCABMLST(R11),B'00001000'                           0186
*             ELSE                                                 0187
*               ;                   /* NOT LAST CALL                 */
*                                                                  0187
@RF00185 DS    0H                                                  0188
