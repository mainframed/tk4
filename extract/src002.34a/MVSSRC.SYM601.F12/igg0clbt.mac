*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   MODULE NAME =  IGGPXLT2 (IGG0CLBT)                               *
**                                                                    *
**   DESCRIPTIVE NAME = DERIVED ALTER MODIFY PROCESSING               *
**                                                                    *
**   FUNCTION =                                                       *
**      THIS SUB-FUNCTION INSURES THAT NON-EXISTENT DERIVED FIELDS    *
**      ARE NOT ALTERED. FOR EXISTENT ALTERABLE FIELDS, IGGPALT2      *
**      (IGG0CLAX) IS INVOKED.                                        *
**                                                                    *
**   INPUT =                                                          *
**      SAME AS IGGPALT2 (IGG0CLAX) - MODIFY                          *
**                                                                    *
**   OUTPUT =                                                         *
**      SAME AS IGGPALT2 (IGG0CLAX) - MODIFY                          *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0139
*   /*****************************************************************/
*   /*                                                               */
*   /* ALLOW NO ALTERS OF THE OLD VOLUME ENTRY SPACE HEADER          */
*   /* OCCURRENCES, NEW OR OLD DATA SET DIRECTORY OR SPACE DESCRIPTOR*/
*   /* OCCURRENCES                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0139
*   IF FIELDNM='SPACEHDR'×          /* IF UNALLOWABLE SPACE          */
*       FIELDNM='NODSDSP '×         /* DESCRIPTOR ALTER OR E         */
*       FIELDNM='NODSPEXT'×         /* DIRECTORY ALTER               */
*       FIELDNM='NODSPACE'×FIELDNM='NODSET  '×DICTGC=CTVDSPDC×(DICTGC=
*       CTVDSDGC&^(FIELDNM='CRADIRCT'×FIELDNM='DSCRETS '×FIELDNM=  0139
*       'DSSUMTT ')) THEN                                          0139
         L     @10,CCAFLPT(,R11)                                   0139
         L     @05,CTGFLDNM(,@10)                                  0139
         CLC   FIELDNM(8,@05),@CC02978                             0139
         BE    @RT00139                                            0139
         CLC   FIELDNM(8,@05),@CC02979                             0139
         BE    @RT00139                                            0139
         CLC   FIELDNM(8,@05),@CC02980                             0139
         BE    @RT00139                                            0139
         CLC   FIELDNM(8,@05),@CC02981                             0139
         BE    @RT00139                                            0139
         CLC   FIELDNM(8,@05),@CC02982                             0139
         BE    @RT00139                                            0139
         AL    @10,@CF00078                                        0139
         CLI   DICTGC(@10),7                                       0139
         BE    @RT00139                                            0139
         CLI   DICTGC(@10),8                                       0139
         BNE   @RF00139                                            0139
         CLC   FIELDNM(8,@05),@CC02983                             0139
         BE    @RF00139                                            0139
         CLC   FIELDNM(8,@05),@CC02984                             0139
         BE    @RF00139                                            0139
         CLC   FIELDNM(8,@05),@CC02985                             0139
         BE    @RF00139                                            0139
@RT00139 DS    0H                                                  0140
*     DO;                           /* NEGATE ALTER                  */
*       CCANF=ON;                   /* END PROCESSING OF GROUP       */
         OI    CCANF(R11),B'00001000'                              0141
*       CALL EXIT;                  /* EXIT                          */
         BAL   @14,EXIT                                            0142
*     END;                          /* END NEGATE                    */
*   ELSE                            /* USE ALTER-2                   */
*                                                                  0144
*     /***************************************************************/
*     /*                                                             */
*     /* CALL TO MODULE IGG0CLAX                                     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0144
*Z005ALT2:                                                         0144
*     CALL IGGPALT2;                /* CALL ALTER-2                  */
         B     @RC00139                                            0144
@RF00139 DS    0H                                                  0144
Z005ALT2 L     @15,@CV00039                                        0144
         BALR  @14,@15                                             0144
*   CALL EXIT;                      /* RETURN TO CALLER              */
@RC00139 BAL   @14,EXIT                                            0145
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */
         DS 0F
