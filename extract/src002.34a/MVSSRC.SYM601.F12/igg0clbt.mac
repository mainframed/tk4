*                                                                  0159
*   DCL                                                            0159
*     SPHDATA CHAR(85) BASED(CCACPE6);/* MAP SPACE OCCURRENCE        */
*   IF FIELDNM^='SPACEHDR' THEN                                    0160
         L     @10,CCAFLPT(,R11)                                   0160
         L     @10,CTGFLDNM(,@10)                                  0160
         CLC   FIELDNM(8,@10),@CC02978                             0160
         BNE   @RT00160                                            0160
*     ;                             /* IF NOT SPACE HEADER, NO     0161
*                                      INDUCED PROCESSING            */
*   ELSE                            /* SPACE HEADER                  */
*     DO;                           /* CHECK IT BEFORE DELETE        */
*       CCADICT='40020806'X;        /* SET DICTIONARY INFO           */
         MVC   CCADICT(4,R11),@CB02998                             0163
*       CCAGVEXT=0;                 /* FIRST TIME CALL               */
*                                                                  0164
         SLR   @10,@10                                             0164
         STCM  @10,7,CCAGVEXT(R11)                                 0164
*       /*************************************************************/
*       /*                                                           */
*       /* CALL TO MODULE IGG0CLBA                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0165
*Z007GVAL:                                                         0165
