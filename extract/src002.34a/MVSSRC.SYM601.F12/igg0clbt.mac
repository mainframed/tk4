Z007GVAL L     @15,@CV00041                                        0165
         BALR  @14,@15                                             0165
*       IF CCAGVNF=ON×CCACD1^=RCS×  /* IF NOT FOUND OR ERROR         */
*           CCAGVNE=ON×CCAGVNC=ON×CCAGVNBS=ON THEN                 0166
         TM    CCAGVNF(R11),B'00010000'                            0166
         BO    @RT00166                                            0166
         CLI   CCACD1(R11),0                                       0166
         BNE   @RT00166                                            0166
         TM    CCAGVNE(R11),B'00101010'                            0166
         BZ    @RF00166                                            0166
@RT00166 DS    0H                                                  0167
*         DO;                       /* EXIT                          */
*           CCANF=ON;               /* INDICATE NOT FOUND            */
         OI    CCANF(R11),B'00001000'                              0168
*           CALL EXIT;              /* EXIT                          */
         BAL   @14,EXIT                                            0169
*         END;                      /* END EXIT                      */
*       ELSE                                                       0171
*         ;                         /* FOUND                         */
@RF00166 DS    0H                                                  0172
*       RFY                                                        0172
*         CTVDSPAC BASED(ADDR(CCACHAIN));/* MAP AREA FOR OCCURR      */
*       CTVDSPAC=SPHDATA;           /* COPY SPACE OCCURRENCE         */
         LA    @10,566                                             0173
         ALR   @10,R11                                             0173
         L     @05,CCACPE6(,R11)                                   0173
         MVC   CTVDSPAC(85,@10),SPHDATA(@05)                       0173
*       IF CTVUNIQ=ON THEN                                         0174
         TM    CTVUNIQ(@10),B'10000000'                            0174
         BO    @RT00174                                            0174
