@RF00117 L     @10,CCALCPL(,R11)                                   0143
         ST    @10,CCAFLPT(,R11)                                   0143
*       CCALCPL=CCACPL;             /* SAVE CATALOG MANAGEMENT     0144
*                                      CALLER'S CPL POINTER          */
         L     @05,CCACPL(,R11)                                    0144
         ST    @05,CCALCPL(,R11)                                   0144
*       CCACPL=CCAFLPT;             /* SET CPL POINTER FIELD TO    0145
*                                      ADDRESS OF CPL TO BE PROCESSED*/
         ST    @10,CCACPL(,R11)                                    0145
*       CCASRCH=CCASRCH&&CCASRCH;   /* CLEAR SRCH ARG FLD            */
         XC    CCASRCH(44,R11),CCASRCH(R11)                        0146
*       CCASRCH(1:6)=LSVSN;         /* SET VOLUME SERIAL NUMBER INTO
*                                      SEARCH ARGUMENT FOR GET     0147
*                                      OPERATION                     */
*                                                                  0147
         L     @10,CTGENT(,@10)                                    0147
         MVC   CCASRCH(6,R11),LSVSN(@10)                           0147
*       /*************************************************************/
*       /*                                                           */
*       /* GET FIRST RECORD OF VOLUME ENTRY IN CATALOG OF ORIGINAL   */
*       /* CALL TO CATALOG MANAGEMENT                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0148
*       DO;                                                        0148
*         CCAF1KEY=ON;              /* TRUE NAME GET                 */
*         CCAF1KGE=OFF;             /* NOT GET NEXT                  */
*         CCAF1UP=OFF;              /* NOT GET UPD                   */
         OI    CCAF1KEY(R11),B'00010000'                           0151
         NI    CCAF1KGE(R11),B'11110101'                           0151
*         CCAURAB=ADDR(CCARAB1);    /* SET RAB PTR                   */
*                                                                  0152
         LA    @10,CCARAB1(,R11)                                   0152
         ST    @10,CCAURAB(,R11)                                   0152
*         /***********************************************************/
*         /*                                                         */
*         /* CALL TO MODULE IGG0CLBI                                 */
*         /*                                                         */
*         /***********************************************************/
