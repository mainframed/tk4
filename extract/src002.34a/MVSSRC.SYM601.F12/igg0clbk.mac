         B     @RC00125                                            0127
@RF00125 DS    0H                                                  0128
*               DO;                                                0128
*                 IF MNATFULL=ON THEN/* TRACKING TABLE FULL          */
*                                                                  0129
         TM    MNATFULL(R11),B'10000000'                           0129
         BNO   @RF00129                                            0129
