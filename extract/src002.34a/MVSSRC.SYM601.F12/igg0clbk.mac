*/********************************************************************/
*                                                                  0203
*IGGPLDCS:                                                         0203
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */
IGGPLDCS DS    0H                                                  0204
*   GEN(ENTRY IGGPLDCS );           /* IDENTIFY ENTRY-POINT SYMBOL   */
         ENTRY IGGPLDCS
*Z017:                              /* LABEL FOR PROCEDURE TRACKING  */
*Z017LDCS:                          /* LABEL FOR PROCEDURE TRACKING  */
*   RFY                                                            0205
*    (R5,                                                          0205
*     R6,                                                          0205
*     R7,                                                          0205
*     R8,                                                          0205
*     R9,                                                          0205
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */
Z017     DS    0H                                                  0205
Z017LDCS DS    0H                                                  0206
*   RFY                                                            0206
*    (R2,                                                          0206
*     R3,                                                          0206
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */
         STM 12,14,12(13)
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */
         LA 13,12(13)
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */
         BALR 12,0
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */
         LA 15,*-IGGSEGST(0,0)
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */
*                                                                  0211
         SR 12,15
*   /*****************************************************************/
*   /*                                                               */
*   /* LSPACE DATA COLLECTION SUBFUNCTION                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0212
*   IF CCACD1=RCS THEN                                             0212
         CLI   CCACD1(R11),0                                       0212
         BNE   @RF00212                                            0212
*     IF CCASMFPT=N THEN            /* LSPACE DATA HAS NOT BEEN    0213
*                                      COLLECTED ON PREVIOUS CALL TO
*                                      THIS SUBFUNCTION FOR THIS   0213
*                                      LSPACE REQUEST                */
         SLR   @10,@10                                             0213
         C     @10,CCASMFPT(,R11)                                  0213
         BNE   @RF00213                                            0213
*       DO;                                                        0214
*                                                                  0214
*         /***********************************************************/
*         /*                                                         */
*         /* SET UP CCA FOR EXTRACT OPERATION                        */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0215
*         CCACD2=N;                 /* CLEAR ERROR CODE FIELD FOR USE
*                                      BY EXTRACT                    */
         MVI   CCACD2(R11),X'00'                                   0215
*         CCAREUSE=ON;              /* ALLOW EXTRACT TO RE-USE RECORD
*                                      AREA BLOCKS                   */
         OI    CCAREUSE(R11),B'00000100'                           0216
*         CCAF2NVC=ON;              /* NO VALIDITY CHECKING OF FPLS,
*                                      WORKAREAS, OR CPL TO BE DONE
*                                      BY EXTRACT                    */
         OI    CCAF2NVC(R11),B'01000000'                           0217
*         CCAR1UR=OFF;              /* ALLOW EXTRACT TO MODIFY RABS
*                                      1, 2, AND 3                   */
         NI    CCAR1UR(R11),B'01111111'                            0218
*         CCAR2UR=OFF;                                             0219
         NI    CCAR2UR(R11),B'01111111'                            0219
*         CCAR3UR=OFF;                                             0220
         NI    CCAR3UR(R11),B'01111111'                            0220
*         CCAR4UR=ON;               /* PREVENT MODIFICATION OF RABS 4
*                                      AND 5 BY EXTRACT              */
         OI    CCAR4UR(R11),B'10000000'                            0221
*         CCAR5UR=ON;                                              0222
         OI    CCAR5UR(R11),B'10000000'                            0222
*         CCARABB=ADDR(CCARAB1);    /* SET ADDRESS OF BASE RAB FOR 0223
*                                      EXTRACT                       */
         LA    @05,CCARAB1(,R11)                                   0223
         ST    @05,CCARABB(,R11)                                   0223
*         CCARABF=ADDR(CCARAB1);    /* SET ADDRESS OF FIRST RAB FOR
*                                      EXTRACT                       */
         ST    @05,CCARABF(,R11)                                   0224
*         CCARABL=ADDR(CCARAB3);    /* SET ADDRESS OF LAST RAB FOR 0225
*                                      EXTRACT                       */
         LA    @05,CCARAB3(,R11)                                   0225
         ST    @05,CCARABL(,R11)                                   0225
*         CCAEXT=ON;                /* INDICATE EXTRACT REQUEST      */
*                                                                  0226
         OI    CCAEXT(R11),B'00000010'                             0226
*         /***********************************************************/
*         /*                                                         */
*         /* RECORD AREAS 4 AND 5 USED AS WORKAREA FOR LSPACE FOR    */
*         /* CONSTRUCTION OF CPL, FPLS, FIELD VALUES, CONTROL VALUES,*/
*         /* AND EXTRACT RETURN AREA CLEAR RECORD AREA               */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0227
*         LWARCL(1:256)=LWARCL(1:256)&&LWARCL(1:256);              0227
         L     @05,CCAR4REC(,R11)                                  0227
         XC    LWARCL(256,@05),LWARCL(@05)                         0227
*         LWARCL(257:512)=LWARCL(257:512)&&LWARCL(257:512);        0228
         XC    LWARCL+256(256,@05),LWARCL+256(@05)                 0228
*         LWARCL(513:768)=LWARCL(513:768)&&LWARCL(513:768);        0229
         XC    LWARCL+512(256,@05),LWARCL+512(@05)                 0229
*         LWARCL(769:1024)=LWARCL(769:1024)&&LWARCL(769:1024);     0230
         XC    LWARCL+768(256,@05),LWARCL+768(@05)                 0230
*         CCAXCPL=CCAR4REC;         /* SET ADDRESS OF EXTRACT CPL    */
