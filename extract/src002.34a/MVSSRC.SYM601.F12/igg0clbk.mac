Z012SMFL L     @15,@CV00038                                        0168
         BALR  @14,@15                                             0168
*   ELSE                                                           0169
*     ;                             /* GET UNSUCCESSFUL/NO MSG. AREA */
*                                                                  0169
@RF00167 DS    0H                                                  0170
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL LSPACE ERROR MESSAGE PROCESSING                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0170
*Z013LEMP:                                                         0170
*   CALL IGGPLEMP;                  /* CALL LSPACE ERROR MSG. ROUTINE*/
Z013LEMP BAL   @14,IGGPLEMP                                        0170
*   IF CCALSP=ON THEN               /* IF INTERNAL CALL TO LSPACE    */
*                                                                  0171
         TM    CCALSP(R11),B'10000000'                             0171
         BNO   @RF00171                                            0171
*     /***************************************************************/
*     /*                                                             */
*     /* SWAP CPL POINTERS BACK TO SITUATION WHEN ENTERED            */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0172
*     DO;                                                          0172
*       CCAFLPT=CCALCPL;            /* SAVE CATALOG MANAGEMENT     0173
*                                      CALLER'S CPL POINTER          */
         L     @10,CCALCPL(,R11)                                   0173
         ST    @10,CCAFLPT(,R11)                                   0173
*       CCALCPL=CCACPL;             /* RESTORE LSPACE INTERNAL CALL
*                                      CPL POINTER                   */
         L     @05,CCACPL(,R11)                                    0174
         ST    @05,CCALCPL(,R11)                                   0174
*       CCACPL=CCAFLPT;             /* RESTORE CATALOG MANAGEMENT  0175
*                                      CALLER'S CPL POINTER          */
         ST    @10,CCACPL(,R11)                                    0175
*     END;                                                         0176
*   IF CCADEFUN=ON THEN             /* DEFINE UNIQUE         @ZA27024*/
@RF00171 TM    CCADEFUN(R11),B'00000010'                           0177
         BNO   @RF00177                                            0177
*     DO;                           /*                       @ZA27024*/
*       CCARAB2(1:20)=CCASAVMN->RABSAV(1:20);/* RESTORE RAB2 @ZA27024*/
         L     @10,CCASAVMN(,R11)                                  0179
         MVC   CCARAB2(20,R11),RABSAV(@10)                         0179
*       CCAR2REC->SAVRAB2(1:256)=CCASAVMN->RECSAV(1:256);/* RESTORE
*                                      THE FIRST HALF OF THE RECORD
*                                                            @ZA27024*/
         L     @05,CCAR2REC(,R11)                                  0180
         MVC   SAVRAB2(256,@05),RECSAV(@10)                        0180
*       CCAR2REC->SAVRAB2(257:512)=CCASAVMN->RECSAV(257:512);/*    0181
*                                      RESTORE THE SECOND HALF OF THE
*                                      RECORD                @ZA27024*/
         MVC   SAVRAB2+256(256,@05),RECSAV+256(@10)                0181
*       CCARAB3(1:20)=CCASAVMN->RABSAV1(1:20);/* RESTORE RAB3      0182
*                                                            @ZA30372*/
         MVC   CCARAB3(20,R11),RABSAV1(@10)                        0182
*       CCAR3REC->SAVRAB3(1:256)=CCASAVMN->RECSAV1(1:256);/* RESTORE
*                                      THE FIRST HALF OF THE RECORD
*                                                            @ZA30372*/
         L     @05,CCAR3REC(,R11)                                  0183
         MVC   SAVRAB3(256,@05),RECSAV1(@10)                       0183
*       CCAR3REC->SAVRAB3(257:512)=CCASAVMN->RECSAV1(257:512);/*   0184
*                                      RESTORE THE SECOND HALF OF THE
*                                      RECORD                @ZA30372*/
         MVC   SAVRAB3+256(256,@05),RECSAV1+256(@10)               0184
*       CCAMNLEN='430'X;            /* SET F/M LEN           @ZA27024*/
         LA    @05,1072                                            0185
         STCM  @05,7,CCAMNLEN(R11)                                 0185
*       CCAMNADR=CCASAVMN;          /* SET ADDR.             @ZA27024*/
         ST    @10,CCAMNADR(,R11)                                  0186
*       RFY                                                        0187
*         R1 RSTD;                  /*                       @ZA27024*/
*       R1=ADDR(CCAMSSPL);          /* POINT TO STRG MGMT P.L.       */
         LA    R1,CCAMSSPL(,R11)                                   0188
*       CCAMNSPL=USUBPOOL;          /* STORE SUBPOOL VALUE           */
         MVI   CCAMNSPL(R11),X'FC'                                 0189
*       DO;                                                        0190
*         IF MNATFULL=ON THEN       /* TRACKING TABLE FULL           */
*                                                                  0191
         TM    MNATFULL(R11),B'10000000'                           0191
         BNO   @RF00191                                            0191
*           /*********************************************************/
*           /*                                                       */
