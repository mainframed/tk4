         L     @05,CCAR3REC(,R11)                                  0140
         MVC   RECSAV1(256,@10),SAVRAB3(@05)                       0140
*           CCASAVMN->RECSAV1(257:512)=CCAR3REC->SAVRAB3(257:512);/*
*                                      SAVE THE SECOND HALF OF RECORD
*                                                            @ZA30372*/
         MVC   RECSAV1+256(256,@10),SAVRAB3+256(@05)               0141
*         END;                      /*                       @ZA27024*/
*                                                                  0142
*       /*************************************************************/
*       /*                                                           */
*       /* INTERNAL CALL INDICATED BY CCALSP ON, CCALCPL CONTAINS    */
