*                                                                  0114
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *  */
*/*                                                                  */
*/* IGGPLSP  - VSAM LSPACE                                           */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *  */
*                                                                  0114
*   CCASMFPT=N;                     /* CLEAR SMF DATA POINTER        */
         SLR   @10,@10                                             0114
         ST    @10,CCASMFPT(,R11)                                  0114
*   IF CCALSP=ON THEN               /* INTERNAL CALL TO LSPACE       */
         TM    CCALSP(R11),B'10000000'                             0115
         BNO   @RF00115                                            0115
*     DO;                                                          0116
*       IF CCADEFUN=ON THEN         /* DEFINE UNIQUE ?       @ZA27024*/
         TM    CCADEFUN(R11),B'00000010'                           0117
         BNO   @RF00117                                            0117
*         DO;                       /* YES DEF UNIQUE        @ZA27024*/
*           CCAMNLEN='430'X;        /* SET THE GET/MAIN CORE LENGTH
*                                      NEEDED                @ZA27024*/
         LA    @10,1072                                            0119
         STCM  @10,7,CCAMNLEN(R11)                                 0119
*           RFY                                                    0120
*             R1 RSTD;              /*                       @ZA27024*/
*           R1=ADDR(CCAMSSPL);      /* POINT TO STRG MGMT P.L.       */
         LA    R1,CCAMSSPL(,R11)                                   0121
*           CCAMNSPL=USUBPOOL;      /* STORE SUBPOOL VALUE           */
*                                                                  0122
         MVI   CCAMNSPL(R11),X'FC'                                 0122
*           /*********************************************************/
*           /*                                                       */
*           /* ALLOCATE MAIN STORAGE FROM USER SUBPOOL 252           */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0123
*           GEN(GETMAIN LC,MF=(E,(1)));/* GET MAIN STORAGE SPACE     */
         GETMAIN LC,MF=(E,(1))
*           GEN(LR R1,R15);         /* PICK UP RETURN CODE           */
         LR R1,R15
*           IF R1^=NOGETMSE THEN    /* CHECK FOR ERROR RETURN        */
*                                                                  0125
         LTR   R1,R1                                               0125
         BZ    @RF00125                                            0125
*             /*******************************************************/
*             /*                                                     */
*             /* SET CCAMODID='BK', CCAREASN=RSN2, CCARETRN=RCINSSWA */
