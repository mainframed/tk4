*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**   MODULE NAME =  IGGPINEX(IGG0CLBP)                                *
**                                                                    *
**   DESCRIPTIVE NAME =                                               *
**      INITIALIZE EXTRACT PARM LIST FOR UPDATE EXTEND.               *
**                                                                    *
**   FUNCTION =                                                       *
**      THIS SUB-FUNCTION IS CALLED BY IGGPSPAC TO INITIALIZE THE     *
**      EXTRACT PARAMETER LIST SO THE  DATA SPACE HEADER AND DATA SET *
**      DIRECTORY INFORMATION CAN BE OBTAINED FROM THE VOLUME ENTRY   *
**      BEING EXTENDED.  IT ALSO PERFORMS INITIALIZATION SO THAT      *
**      SUBSEQUENT MODIFIES CAN BE DONE.                              *
**                                                                    *
**   INPUT =                                                          *
**      UPSPARM - UPDATE EXTEND SPACE PARAMETER WORK AREA.            *
**         UPSPWA - EXTRACT/MODIFY CPL/FPL AREA (ZEROED BY IGGPSPAC   *
**                  CALLER)                                           *
**                                                                    *
**   OUTPUT =                                                         *
**      IGGCCA OUTPUT:                                                *
**         CCAXCPL - EXTRACT CPL ADDRESS POINTS TO UPSPWA.            *
**         CCAREUSE - REUSE OF EXTRACT RECORD AREAS IS REQUESTED.     *
**         CCAF2NVC - NO VALIDITY CHECK IS REQUESTED.                 *
**         CCARABB,CCARABF,CCARABL - BASE,FIRST,AND LAST RECORD AREA  *
**                                   ADDRESSES INITIALIZED USING 3,4, *
**                                   AND 5 RESPECTIVELY.              *
**         CCAEXT,CCAMOD - EXTRACT AND MODIFY INDICATED.              *
**         CCAMODPA - NO PUT ADD INDICATED FOR MODIFY.                *
**                                                                    *
**      UPSPARM:                                                      *
**         UPSPWA - INITIALIZED WITH AN EXTRACT CPL WHICH IS TO BE    *
**                  USED TO OBTAIN AT MOST ONE OCCURRENCE WITH A      *
**                  PARTICULAR SEQUENCE NUMBER.  THE EXTRACT RETURN   *
**                  AREA FOLLOWS THE EXTRACT CPL/FPL'S BUILT BY THIS  *
**                  ROUTINE. THE FOLLOWING ADDITIONAL FIELDS NEED TO  *
**                  BE INITIALIZED:                                   *
**                  CTGFLDNM - THE NAME OF THE OCCURRENCE TO BE       *
**                             EXTRACTED (FIRST FPL).                 *
**                  UPREL - THE SEQUENCE NUMBER OF THE OCCURRENCE TO  *
**                          BE RETRIEVED.                             *
**                  CTGFLDCD - THE TYPE OF TEST TO BE USED AGAINST    *
**                             UPCNT FOR RETRIEVING THE OCCURRENCE    *
**                             (2ND FPL)                              *
**                  CTGFLDGC - THE GROUP CODE OF THE OCCURRENCE (2ND  *
**                             FPL).                                  *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0410
*   RFY                                                            0410
*     CTGPL BASED(CCAXCPL);         /* MAP EXTRACT CPL               */
*   CCAXCPL=ADDR(UPSPWA);           /* EXTRACT CPL AREA              */
         L     @10,CCAR3REC(,R11)                                  0411
         SH    @10,@CH00597                                        0411
         LA    @05,UPSPWA(,@10)                                    0411
         ST    @05,CCAXCPL(,R11)                                   0411
*   CCAREUSE=OFF;                   /* DONT REUSE RECORD AREAS       */
         NI    CCAREUSE(R11),B'11111011'                           0412
*   CCAF2NVC=ON;                    /* NO ADDR VALIDITY CHECK        */
         OI    CCAF2NVC(R11),B'01000000'                           0413
*   CCARABB=ADDR(CCARAB3);          /* BASE RECORD AREA              */
         LA    @15,CCARAB3(,R11)                                   0414
         ST    @15,CCARABB(,R11)                                   0414
*   CCARABF=ADDR(CCARAB3);          /* FIRST RECORD AREA             */
         ST    @15,CCARABF(,R11)                                   0415
*   CCARABL=ADDR(CCARAB5);          /* LAST RECORD AREA              */
         LA    @15,CCARAB5(,R11)                                   0416
         ST    @15,CCARABL(,R11)                                   0416
*   CCAEXT=ON;                      /* EXTRACT INDICATOR             */
*   CCAMOD=ON;                      /* MODIFY INDICATOR              */
         OI    CCAEXT(R11),B'00000011'                             0418
*   CCAMODPA=OFF;                   /* NO PUT ADD                    */
         NI    CCAMODPA(R11),B'11101111'                           0419
*   CTGNOFLD=FPLNUM;                /* NUMBER FPLS                   */
         MVI   CTGNOFLD(@05),X'02'                                 0420
*   CTGFIELD(1)=CCAXCPL+LENGTH(CTGPL)+/* POINT TO THE FIRST FIELD    */
*       FPLNUM*LENGTH(CTGFIELD);    /* PARM LIST AREA                */
         LA    @15,36                                              0421
         ALR   @15,@05                                             0421
         ST    @15,CTGFIELD(,@05)                                  0421
*   RFY                                                            0422
*     CTGFL BASED(CTGFIELD(1));     /* MAP CURRENT FPL               */
*   CTGFLCHN=CTGFIELD(1)+LENGTH(CTGFL)/* POINT TO FOLLOWING          */
*       +NODAT*LENGTH(CTGFLDAT);    /* TEST FPL                      */
         LA    @14,24                                              0423
         LR    @09,@15                                             0423
         ALR   @09,@14                                             0423
         ST    @09,CTGFLCHN(,@15)                                  0423
*   R2=CTGFLCHN;                    /* BASE FOR NEXT TEST FPL        */
         LR    R2,@09                                              0424
*   RFY                                                            0425
*     CTGFL BASED(R2);              /* MAP TEST FPL                  */
*   CTGFLDNM=ADDR(RELREPNO);        /* POINT TO FIELD NAME           */
         LA    @15,@CC00143                                        0426
         ST    @15,CTGFLDNM(,R2)                                   0426
*   CTGFLDNO=NODAT;                 /* INDICATE TEST DATA            */
         MVI   CTGFLDNO(R2),X'01'                                  0427
*   CTGFLPT(1)=ADDR(UPREL);         /* POINT TO TEST DATA            */
         LA    @15,UPREL(,@10)                                     0428
         ST    @15,CTGFLPT(,R2)                                    0428
*   CTGFLNG(1)=LENGTH(UPREL);       /* LENGTH TEST DATA              */
         LA    @15,2                                               0429
         ST    @15,CTGFLNG(,R2)                                    0429
*   CTGFLCHN=ADDR(CTGFL)+LENGTH(CTGFL)/* POINT TO NEXT TEST FPL      */
*       +NODAT*LENGTH(CTGFLDAT);    /* AREA                          */
         LR    @09,R2                                              0430
         ALR   @09,@14                                             0430
         ST    @09,CTGFLCHN(,R2)                                   0430
*   R2=CTGFLCHN;                    /* BASE FOR TEST FPL             */
         LR    R2,@09                                              0431
*   CTGFLDNM=ADDR(CNTREPNO);        /* POINT TO FIELD NAME           */
         LA    @09,@CC00141                                        0432
         ST    @09,CTGFLDNM(,R2)                                   0432
*   CTGFLDNO=NODAT;                 /* SET NO. DATA/LNG PAIR         */
         MVI   CTGFLDNO(R2),X'01'                                  0433
*   CTGFLDCD=CTGFLDLE;              /* TEST CONDITION                */
         MVI   CTGFLDCD(R2),X'C0'                                  0434
*   CTGFLPT(1)=ADDR(UPCNT);         /* POINT TO TEST DATA            */
         LA    @09,UPCNT(,@10)                                     0435
         ST    @09,CTGFLPT(,R2)                                    0435
*   UPCNT=1;                        /* LIMIT NO. OF OCCURRENCES    0436
*                                      RETURNED TO ONE               */
         MVC   UPCNT(2,@10),@CH00088                               0436
*   CTGFLNG(1)=LENGTH(UPCNT);       /* LENGTH TEST DATA              */
         ST    @15,CTGFLNG(,R2)                                    0437
*   CTGWKA=ADDR(CTGFL)+LENGTH(CTGFL)/* SET ADDR OF WORK AREA         */
*       +NODAT*LENGTH(CTGFLDAT);    /* AFTER LAST FPL                */
         ALR   @14,R2                                              0438
         ST    @14,CTGWKA(,@05)                                    0438
*   WKALEN1=EXTWALEN;               /* EXTRACT WORK AREA LENGTH      */
         MVC   WKALEN1(2,@14),@CH00061                             0439
*   CALL EXIT;                      /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0440
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */
         DS 0F
*   GEN(DC CL8'IGGPSRH1');          /* NAME OF FOLLOWING PROCEDURE   */
         DC CL8'IGGPSRH1'
*   END;                            /* END OF INNER PROCEDURE        */
@EL00003 DS    0H                                                  0443
@EF00003 DS    0H                                                  0443
@ER00003 BR    @14                                                 0443
@PB00003 DS    0H                                                  0444
