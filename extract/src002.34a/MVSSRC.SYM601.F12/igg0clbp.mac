         MVC   CCAPROB(4,R11),@CF05478                             0461
*     ELSE                          /* DATA SET DIRECTORY INFO IN  0462
*                                      EXTRACT WORK AREA             */
*       DO;                         /* SAVE DIRECTORY INFO           */
         B     @RC00460                                            0462
@RF00460 DS    0H                                                  0463
*         UPDIRSP=FLDATA(1:LENGTH(UPDIRSP));/* COPY DIRECTORY INTO 0463
*                                      PARAMETER WORK AREA           */
         L     @10,CCAR3REC(,R11)                                  0463
         LR    @05,@10                                             0463
         SH    @05,@CH00597                                        0463
         L     @01,CCAXCPL(,R11)                                   0463
         L     @15,CTGFIELD(,@01)                                  0463
         L     @01,CTGFLPT(,@15)                                   0463
         MVC   UPDIRSP(12,@05),FLDATA(@01)                         0463
*         RFY                                                      0464
*           CTVDSGRP BASED(ADDR(UPDIRSP));/* MAP DIRECTORY           */
*         IF CTVDSXCT^=0&CTVSPSXC(1)^=0 THEN/* IF NON-EMPTY DIRECT.
*                                      ELEMENT EXISTS                */
         AH    @10,@CH05480                                        0465
         ICM   @14,3,CTVDSXCT(@10)                                 0465
         BZ    @RF00465                                            0465
         CLI   CTVSPSXC(@10),0                                     0465
         BE    @RF00465                                            0465
