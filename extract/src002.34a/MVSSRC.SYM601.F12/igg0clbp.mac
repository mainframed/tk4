         L     @10,CCAXCPL(,R11)                                   0460
         L     @10,CTGFIELD(,@10)                                  0460
         ICM   @05,15,CTGFLPT(@10)                                 0460
         BNZ   @RF00460                                            0460
*       /*************************************************************/
*       /*                                                           */
*       /* SET CCAMODID='BP', CCAREASN=RSN4, CCARETRN=RCDSNF         */
*       /*                                                           */
