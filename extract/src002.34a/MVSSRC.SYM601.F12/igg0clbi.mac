         NI    CCAVRIND(R11),B'11110011'                           0235
*/********************************************************************/
*/*     RECOVERABLE  CATALOG  AND  NOT  CATALOG  OPEN  OR  BUILD     */
*/********************************************************************/
*                                                                  0236
*   IF CAXF2REC=ON&                 /* RECOVERABLE CATALOG AND       */
*       CCAF2COB='00'B THEN         /* NOT BUILD/OPEN                */
         L     @10,CCAACB(,R11)                                    0236
         L     @10,ACBUAPTR(,@10)                                  0236
         TM    CAXF2REC(@10),B'00000010'                           0236
         BNO   @RF00236                                            0236
         TM    CCAF2COB(R11),B'00000110'                           0236
         BNZ   @RF00236                                            0236
*     DO WHILE CCALPIND=OFF;        /* CHECK FOR BUFFERED RECS       */
         B     @DE00237                                            0237
@DL00237 DS    0H                                                  0238
*       RFY                                                        0238
*        (R5,                                                      0238
*         R6) RSTD;                 /* NEED R5, R6 FOR POINTERS      */
*       IF CCARANCA(1)^=ZERO&       /* NORMAL RECORD BUFFER CHAIN    */
*           CCAVRIND=OFF THEN       /* AND NOT YET EXAMINED          */
         L     @10,CCARANCA(,R11)                                  0239
         LTR   @10,@10                                             0239
         BZ    @RF00239                                            0239
         TM    CCAVRIND(R11),B'00000100'                           0239
         BNZ   @RF00239                                            0239
*         DO;                       /* SET UP TO SCAN NORMAL CHN     */
*           R1=CCARANCA(1);         /* POINT TO FIRST RECORD         */
         LR    R1,@10                                              0241
*           R6=ADDR(CCARANCA(1))    /* POINT TO PSEUDO PREVIOUS      */
*               -LENGTH(BUFRECRD);  /* RECORD ON CHAIN               */
         LA    R6,CCARANCA(,R11)                                   0242
         SH    R6,@CH03290                                         0242
*         END;                                                     0243
*       ELSE                        /* CHECK VOL RECORD CHAIN        */
*         DO;                       /* CHECK FOR VOLUME RECORDS      */
         B     @RC00239                                            0244
@RF00239 DS    0H                                                  0245
*           IF CCARAVCA(1)^=ZERO THEN/* VOL BUFFER CHAIN EXISTS      */
         L     @10,CCARAVCA(,R11)                                  0245
         LTR   @10,@10                                             0245
         BZ    @RF00245                                            0245
*             DO;                                                  0246
*               R1=CCARAVCA(1);     /* SET ADDRESS OF VOL REC CHN    */
         LR    R1,@10                                              0247
*               R6=ADDR(CCARAVCA(1))/* POINT TO PSEUDO               */
*                   -LENGTH(BUFRECRD);/* PREVIOUS RECORD ON CHN      */
         LA    R6,CCARAVCA(,R11)                                   0248
         SH    R6,@CH03290                                         0248
*               CCAVRIND=ON;        /* INDICATE VOL REC BUFFER CHAIN */
         OI    CCAVRIND(R11),B'00000100'                           0249
*             END;                                                 0250
*           ELSE                    /* NO VOL RECORD BUFFER CHAIN    */
*             CCALPIND=ON;          /* INDICATE END RECORD CHECK     */
         B     @RC00245                                            0251
@RF00245 OI    CCALPIND(R11),B'00001000'                           0251
*         END;                                                     0252
@RC00245 DS    0H                                                  0253
*       DO WHILE(CCALPIND=OFF&R1^=0);/* CHECK FOR RECORD             */
@RC00239 B     @DE00253                                            0253
@DL00253 DS    0H                                                  0254
*         IF(CCAF1KEY=ON&           /* KEYED REQUEST, FULL NAME      */
*             CCASRCH=RCDNAME)×     /* RECORD FOUND IN CHAIN, CI     */
*             (CCASRCIN=RCDCINO) THEN/* RECORD FOUND IN CHAIN        */
         TM    CCAF1KEY(R11),B'00010000'                           0254
         BNO   @GL00006                                            0254
         CLC   CCASRCH(44,R11),RCDNAME(R1)                         0254
         BE    @RT00254                                            0254
@GL00006 CLC   CCASRCIN(3,R11),RCDCINO(R1)                         0254
         BNE   @RF00254                                            0254
@RT00254 DS    0H                                                  0255
*           DO;                     /* MOVE RECORD TO RA             */
*             CCALPIND=ON;          /* SET LOOP END INDICATOR        */
         OI    CCALPIND(R11),B'00001000'                           0256
*             R5=ADDR(BUFRECRD);    /* GET ADDR OF RECORD START      */
         LR    R5,R1                                               0257
*             TORECAR(1:256)=R5->TORECAR(1:256);/* MOVE REC          */
         L     @10,CCAURAB(,R11)                                   0258
         L     @10,CCACPE1(,@10)                                   0258
         MVC   TORECAR(256,@10),TORECAR(R5)                        0258
*             TORECAR(257:508)=R5->TORECAR(257:508);               0259
         MVC   TORECAR+256(252,@10),TORECAR+256(R5)                0259
*             GOTO GETFNDEX;        /* SET UP CPE POINTERS           */
         B     GETFNDEX                                            0260
*           END;                                                   0261
*         ELSE                      /* RECORD NOT FOUND ON CHAIN     */
*           DO;                     /* CHECK NEXT RECORD ON CHAIN    */
@RF00254 DS    0H                                                  0263
*             R6=R1;                /* SET PREVIOUS RECORD POINTER   */
         LR    R6,R1                                               0263
*             R1=BUFNEXTB;          /* POINT TO NEXT RECORD ON CHAIN */
         L     R1,BUFNEXTB(,R1)                                    0264
*           END;                                                   0265
*       END;                                                       0266
@DE00253 TM    CCALPIND(R11),B'00001000'                           0266
         BNZ   @DC00253                                            0266
         LTR   R1,R1                                               0266
         BNZ   @DL00253                                            0266
@DC00253 DS    0H                                                  0267
*       IF CCAVRIND=ON THEN         /* VOL RECORD CHAIN SCAN DONE    */
         TM    CCAVRIND(R11),B'00000100'                           0267
         BNO   @RF00267                                            0267
*         CCALPIND=ON;              /* INDICATE CHAIN SCAN COMPLETE  */
         OI    CCALPIND(R11),B'00001000'                           0268
*       ELSE                        /* INDICATE VOL RECORD CHAIN SCAN*/
*         CCAVRIND=ON;              /* INDICATE CHAIN SCAN COMPLETE  */
         B     @RC00267                                            0269
@RF00267 OI    CCAVRIND(R11),B'00000100'                           0269
*       RFY                                                        0270
*        (R5,                                                      0270
*         R6) UNRSTD;               /* FREE USE OF R5, R6            */
@RC00267 DS    0H                                                  0271
*     END;                                                         0271
@DE00237 TM    CCALPIND(R11),B'00001000'                           0271
         BZ    @DL00237                                            0271
*   ELSE                                                           0272
*     ;                             /* NOT RECOVERABLE CATALOG OR NOT
*                                      OPEN/BUILD OF CATALOG         */
@RF00236 DS    0H                                                  0273
*/********************************************************************/
*/*               NOT  OPEN  OR  BUILD  OF  A  CATALOG               */
*/********************************************************************/
*                                                                  0273
*   RFY                                                            0273
*     CATRCD BASED(CCACPE1);        /* SPECIFY BASE FOR RECORD       */
*   RFY                                                            0274
*     CTENTREC BASED(CCACPE1);      /* SPECIFY BASE FOR RECORD       */
*   IF CCAF2COB='00'B THEN          /* IF NOT CATLG BUILD/OPEN,      */
         TM    CCAF2COB(R11),B'00000110'                           0275
         BNZ   @RF00275                                            0275
*     DO;                           /* THEN DO NORMAL PROCESSING     */
*       DO ONCE=ONCE WHILE(CCAF1KEY=ON);/* DO IF KEYED GET REQ       */
         TM    CCAF1KEY(R11),B'00010000'                           0277
         BNO   @DC00277                                            0277
*         IF CCAF1KGE=ON THEN       /* IF GET NEXT REQUEST           */
         TM    CCAF1KGE(R11),B'00001000'                           0278
         BNO   @RF00278                                            0278
*           DO;                                                    0279
*             XIOOPT=187;           /* GET OPTIONS                   */
         MVI   XIOOPT(R11),X'BB'                                   0280
*Z009XIO:                                                          0281
