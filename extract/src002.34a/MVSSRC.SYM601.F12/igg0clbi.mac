         SLR   @10,@10                                             0297
         ICM   @10,7,CCASRCIN(R11)                                 0297
         SLA   @10,9                                               0297
         ST    @10,CCADDDD(,R11)                                   0297
*       IF CCAF1UP=ON&              /* GET FOR UPDATE                */
*           CCAROREQ=OFF THEN       /* NOT READ-ONLY REQUEST TYPE    */
         TM    CCAF1UP(R11),B'00000010'                            0298
         BNO   @RF00298                                            0298
         TM    CCAROREQ(R11),B'00000100'                           0298
         BNZ   @RF00298                                            0298
*         DO;                                                      0299
