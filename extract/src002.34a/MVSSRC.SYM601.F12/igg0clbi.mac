         SH    R13,@CH00072                                        0313
*             IF RPLRTNCD^='00'X THEN/* ERROR?               @ZA02285*/
         L     @10,CCARPL1(,R11)                                   0314
         CLI   RPLRTNCD(@10),0                                     0314
         BE    @RF00314                                            0314
