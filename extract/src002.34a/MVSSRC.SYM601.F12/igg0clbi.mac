*           CALL IGGPUCCT;          /* DO CRA CI # TRANSLATION       */
Z011UCCT BAL   @14,IGGPUCCT                                        0291
*           IF CCACD1^=RCS THEN     /* CI TRANSLATION FAILED         */
         CLI   CCACD1(R11),0                                       0292
         BE    @RF00292                                            0292
*             CALL EXIT;            /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0293
