*       ->CAXSCHWA->ACCTRANT;                                      0586
*                                                                  0586
         L     @10,CVTPTR                                          0586
         L     @10,CVTCBSP(,@10)                                   0586
         L     @10,CBSCRACA(,@10)                                  0586
         L     @10,CAXSCHWA(,@10)                                  0586
         L     R2,ACCTRANT(,@10)                                   0586
*/********************************************************************/
*/*              SEARCH  CI  NUMBER  TRANSLATION  TABLE              */
*/********************************************************************/
*                                                                  0587
*   RFY                                                            0587
*     R1 RSTD;                      /* NEED R1 FOR TABLE INDEX       */
*   R1=ONE;                         /* START WITH FIRST TABLE ENTRY  */
         LA    R1,1                                                0588
*   DO WHILE((CCASRCIN^=CTTCATCI(R1))/* DO WHILE ENTRY NOT FOUND AND */
*         &(R1<=CTTENTNO));         /* ANOTHER ENTRY EXISTS          */
         B     @DE00589                                            0589
@DL00589 DS    0H                                                  0590
*     R1=R1+ONE;                    /* INCREMENT TABLE ENTRY INDEX   */
         AH    R1,@CH00037                                         0590
*   END;                            /* END TRANSLATE TABLE SEARCH    */
@DE00589 LR    @10,R1                                              0591
         SLA   @10,2                                               0591
         ALR   @10,R2                                              0591
         CLC   CCASRCIN(3,R11),CTTCATCI-4(@10)                     0591
         BE    @DC00589                                            0591
         C     R1,CTTENTNO(,R2)                                    0591
         BNH   @DL00589                                            0591
@DC00589 DS    0H                                                  0592
