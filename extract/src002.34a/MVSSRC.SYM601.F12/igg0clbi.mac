Z010XIO  BAL   @14,IGGPXIO                                         0285
*           END;                                                   0286
*         CCASRID=ARATNCI;          /* EXTRACT CI # FROM TN ENTRY    */
@RC00278 L     @10,CCAR0REC(,R11)                                  0287
         MVC   CCASRID(3,R11),ARATNCI(@10)                         0287
*       END;                        /* END OF KEYED GET PROCESS START
