*       ELSE                        /* NOT GET FOR UPDATE            */
*         DO;                                                      0303
         B     @RC00298                                            0303
@RF00298 DS    0H                                                  0304
*           XIOOPT=140;             /* GET OPTIONS                   */
         MVI   XIOOPT(R11),X'8C'                                   0304
*Z013XIO:                                                          0305
*           CALL IGGPXIO;           /* GET ENTRY RCD         @ZA02285
*                                      OP=URA-ADR-NUP-NCK-CIE----    */
Z013XIO  BAL   @14,IGGPXIO                                         0305
*         END;                      /*                       @ZA02285*/
*                                                                  0306
