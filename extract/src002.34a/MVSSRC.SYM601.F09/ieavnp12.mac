         TITLE 'IEAVNP12 - NIP LOCATE                                  *
                        '
IEAVNP12 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVNP12  77.018'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
*
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
* WILL ASSEMBLE PROPERLY.
         STM R2,R3,LOCPTR
*   ERRCD=ZERO;                                                    0238
         SLR   @15,@15                                             0238
         ST    @15,ERRCD                                           0238
*   SW='00000000'B;                 /* SET ALL FLAGS OFF             */
         MVI   SW,B'00000000'                                      0239
*   DICTSTR=ADDR(ZDT);              /* DICTIONARY STARTING ADD       */
         LA    @15,ZDT                                             0240
         ST    @15,DICTSTR                                         0240
*   DICTEND=ADDR(ZCS);                                             0241
         LA    @15,ZCS                                             0241
         ST    @15,DICTEND                                         0241
*   DICTTYP=ADDR(ZTYP);             /* ITYPENT FLD OF DICT           */
         LA    @15,ZTYP                                            0242
         ST    @15,DICTTYP                                         0242
*   DICTTYP=DICTTYP->DICTST;        /* DISPLAC IN CATALOG            */
         SLR   @14,@14                                             0243
         IC    @14,DICTST(,@15)                                    0243
         ST    @14,DICTTYP                                         0243
*   WKLEN=LENGTH(CATLGBUF)*TEN;     /* GET CORE TO HOLD CATALOG      */
         MVC   WKLEN(2),@CH02590                                   0244
*   CALL GETWORK;                   /* RECORD READ                   */
         BAL   @14,GETWORK                                         0245
*   CATBFPTR=WKCORE;                /* SAVE POINTER                  */
         L     @15,WKCORE                                          0246
         ST    @15,CATBFPTR                                        0246
*   DO IX=TEN TO 1 BY-1;            /* INIT CATBUFPTRS               */
         LA    IX,10                                               0247
@DL00247 DS    0H                                                  0248
*     BFCTR(IX)=ZERO;               /* USE COUNT TO 0                */
         SLR   @06,@06                                             0248
         LR    @15,IX                                              0248
         SLA   @15,3                                               0248
         STC   @06,BFCTR-8(@15)                                    0248
*     BFCI(IX)=ZERO;                /* CI NBR IS 0                   */
         LA    @14,BFCI-8(@15)                                     0249
         STCM  @06,7,0(@14)                                        0249
*     BFPTR(IX)=ADDR(CCACPE(IX));   /* BUFFER POINTER                */
         LR    @06,IX                                              0250
         SLA   @06,4                                               0250
         LA    @06,CCACPE-16(@06)                                  0250
         ST    @06,BFPTR-8(@15)                                    0250
*   END;                                                           0251
         BCTR  IX,0                                                0251
         LTR   IX,IX                                               0251
         BP    @DL00247                                            0251
*   BFCTR(1)=H255;                  /* INITIAL ENTRY REC NEEDED      */
         MVI   BFCTR,X'FF'                                         0252
*   GEN SETS(CTGPLPTR);                                            0253
        L     R1,@SA00001+K4           POINT TO CALLERS SAVEAREA
        L     R1,O24(,R1)              LOAD CALLERS PARM LIST PTR
        ST    R1,CTGPLPTR              STORE CATLG PARM LIST POINTER
*   CALL CTLBKPTR;                  /* SET POINTERS TO THE CATALOG 0254
*                                      CONTROL BLOCKS THAT WE NEED   */
         BAL   @14,CTLBKPTR                                        0254
*   IF CTGAM0=OFFX THEN             /* DO WE HAVE A O.S. LOCATE?     */
         L     @09,CTGPLPTR                                        0255
         TM    CTGAM0(@09),B'00000001'                             0255
         BNZ   @RF00255                                            0255
*     DO;                           /* YES DO                        */
*       CALL MAPOSCPL;              /* CREATE A CPL FPL FROM THE THE
*                                      CAMLST THE USER GAVE          */
         BAL   @14,MAPOSCPL                                        0257
*       CALL GETTRNM;               /* GET TRUE NAME RECORD          */
         BAL   @14,GETTRNM                                         0258
*       CALL FINDCI;                /* GET A RECORD BY CI NBR        */
         BAL   @14,FINDCI                                          0259
*       CPX=CPE0;                   /* INIT RECORD POINTER           */
         ST    CPE0,CPX                                            0260
*       CTGFLPTR=CTGFIELD(ONE);     /* SET PTR TO FIELD REQUEST      */
         L     @08,CTGPLPTR                                        0261
         L     @08,CTGFIELD(,@08)                                  0261
         ST    @08,CTGFLPTR                                        0261
*       CALL CTINTFAC;              /* CATLG INTERFACE               */
         BAL   @14,CTINTFAC                                        0262
*       CAMWORK=CAMPTR3;            /* POINT TO OUTPUT AREA          */
         L     @08,CAMLSTPT                                        0263
         L     CAMWORK,CAMPTR3(,@08)                               0263
*       CALL MAPCPLOS;              /* MAP FROM CPL-FPL FORM TO THAT
*                                      REQUIRED BY O.S.              */
         BAL   @14,MAPCPLOS                                        0264
*     END;                                                         0265
*   ELSE                            /* WE HAVE A VSAM LOCATE         */
*     DO;                                                          0266
         B     @RC00255                                            0266
@RF00255 DS    0H                                                  0267
*       IF CTGNAME=ON THEN          /* 44 BYTE NAME?                 */
         L     @09,CTGPLPTR                                        0267
         TM    CTGNAME(@09),B'00000100'                            0267
         BNO   @RF00267                                            0267
*         DO;                                                      0268
*           CALL GETTRNM;           /* GET TRUE NAME RECORD          */
         BAL   @14,GETTRNM                                         0269
*         END;                                                     0270
*       ELSE                        /* LOCATE BY CI NBR              */
*         CIWORK=CIWORK2;           /* MOVE CI NBR FROM CPL          */
         B     @RC00267                                            0271
@RF00267 L     @01,CTGPLPTR                                        0271
         L     @01,CTGENT(,@01)                                    0271
         MVC   CIWORK(3),CIWORK2(@01)                              0271
*       CALL FINDCI;                /* GET PROPER CI INTO CORE       */
@RC00267 BAL   @14,FINDCI                                          0272
*       CPX=CPE0;                                                  0273
         ST    CPE0,CPX                                            0273
*       GPFLDPTR=CTGWKA+FOUR;       /* SET OUTPUT POINTER            */
         L     @01,CTGPLPTR                                        0274
         L     @15,CTGWKA(,@01)                                    0274
         LA    GPFLDPTR,4                                          0274
         ALR   GPFLDPTR,@15                                        0274
*       CTWKLIMT=WORKLEN+CTGWKA;    /* SET WORK AREA LIMIT           */
         LH    @08,WORKLEN(,@15)                                   0275
         ALR   @08,@15                                             0275
         ST    @08,CTWKLIMT                                        0275
*       DO IX=1 TO CTGNOFLD;        /* PROCESS ALL FIELDS REQUEST    */
         LA    IX,1                                                0276
         B     @DE00276                                            0276
@DL00276 DS    0H                                                  0277
*         CTGFLPTR=CTGFIELD(IX);    /* FIELD PTR FOR CAT INTERFAC    */
         LR    @08,IX                                              0277
         SLA   @08,2                                               0277
         L     @06,CTGPLPTR                                        0277
         L     @08,CTGFIELD-4(@08,@06)                             0277
         ST    @08,CTGFLPTR                                        0277
*         CTGFLPT(ONE)=GPFLDPTR;    /* SET OUTPUT POINTER            */
         ST    GPFLDPTR,CTGFLPT(,@08)                              0278
*         CALL CTINTFAC;            /* CATALOG INTERFACE             */
         BAL   @14,CTINTFAC                                        0279
*         CTGFLNG(ONE)=GPFLDPTR-CTGFLPT(ONE);/* SET LENGTH           */
         L     @08,CTGFLPTR                                        0280
         LR    @06,GPFLDPTR                                        0280
         SL    @06,CTGFLPT(,@08)                                   0280
         ST    @06,CTGFLNG(,@08)                                   0280
*         IF CTGFLNG(ONE)=ZERO THEN /* SET OUTPUT PTR ONLY IF DATA   */
         SLR   @15,@15                                             0281
         CR    @06,@15                                             0281
         BNE   @RF00281                                            0281
*           CTGFLPT(ONE)=ZERO;      /* INDICATE NO VALUE             */
         ST    @15,CTGFLPT(,@08)                                   0282
*       END;                                                       0283
@RF00281 AL    IX,@CF00077                                         0283
@DE00276 L     @08,CTGPLPTR                                        0283
         SLR   @06,@06                                             0283
         IC    @06,CTGNOFLD(,@08)                                  0283
         CR    IX,@06                                              0283
         BNH   @DL00276                                            0283
*     END;                                                         0284
*EXIT:                                                             0285
*   IF OSSW=ON THEN                 /* WAS IT AN O.S. TYPE LOCATE?   */
@RC00255 DS    0H                                                  0285
EXIT     TM    OSSW,B'00000001'                                    0285
         BNO   @RF00285                                            0285
*     DO;                           /* YES DO                        */
*       FREEAD=CTGPLPTR;            /* ADDRESS OF CORE TO FREE       */
         L     @15,CTGPLPTR                                        0287
         ST    @15,FREEAD                                          0287
*       FREELEN=CTGDDNM;            /* LENGTH OF CORE TO FREE        */
         L     @15,CTGDDNM(,@15)                                   0288
         ST    @15,FREELEN                                         0288
*       CALL FREEWORK;              /* FREE WORKING STORAGE          */
         BAL   @14,FREEWORK                                        0289
*     END;                                                         0290
*   FREELEN=LENGTH(CATLGBUF)*TEN;   /* BUFFER LENGTH                 */
@RF00285 MVC   FREELEN(4),@CF02590                                 0291
*   FREEAD=CATBFPTR;                                               0292
         L     @15,CATBFPTR                                        0292
         ST    @15,FREEAD                                          0292
*   CALL FREEWORK;                  /* CALL FREEMAIN ROUTINE         */
         BAL   @14,FREEWORK                                        0293
*   RETURN CODE(ERRCD);             /* RETURN TO USER (NORMAL EXIT)  */
*                                                                  0294
         L     @09,ERRCD                                           0294
         L     @13,4(,@13)                                         0294
         L     @00,@SIZDATD                                        0294
         LR    @01,@11                                             0294
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@09                                             0294
         L     @14,12(,@13)                                        0294
         LM    @00,@12,20(@13)                                     0294
         BR    @14                                                 0294
*   /*****************************************************************/
*   /*                                                               */
*   /* E X I T                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0295
*MAPOSCPL:                                                         0295
*                                                                  0295
*   /*****************************************************************/
*   /*                                                               */
*   /* MAP AN OS CAMLIST INTO A VSAM CPL-FPL COMBINATION             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0295
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0295
@EL00001 L     @13,4(,@13)                                         0295
@EF00001 L     @00,@SIZDATD                                        0295
         LR    @01,@11                                             0295
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0295
         BR    @14                                                 0295
MAPOSCPL DS    0H                                                  0296
*   GEN(ST     R14,RSAVA);                                         0296
         ST     R14,RSAVA
*   WKLEN=LENGTH(CTGPL)+FOUR        /* GET CORE FOR SIMULATED CPL    */
*       +LENGTH(CTGFL)              /* AND AN FPL                    */
*       +H44                        /* ROOM TO COPY NAME             */
*       +H272;                      /* AND A CATALOG WORK AREA       */
         MVC   WKLEN(2),@CH02591                                   0297
*   CALL GETWORK;                   /* GET WORKING STORAGE           */
         BAL   @14,GETWORK                                         0298
*   OSSW=ON;                        /* SET O.S. LOCATE FLAG          */
         OI    OSSW,B'00000001'                                    0299
*   CAMLSTPT=CTGPLPTR;              /* SAVE THE CAMLST POINTER       */
         L     @09,CTGPLPTR                                        0300
         ST    @09,CAMLSTPT                                        0300
*   CTGPLPTR=WKCORE;                                               0301
         L     @09,WKCORE                                          0301
         ST    @09,CTGPLPTR                                        0301
*   CTGDDNM=WKLEN;                  /* SAVE FOR FREEMAIN             */
         LH    @15,WKLEN                                           0302
         ST    @15,CTGDDNM(,@09)                                   0302
*   CTGFLPTR=WKCORE+LENGTH(CTGPL)+FOUR;/* LEAVE ROOM FOR CTGFIELD    */
         LA    @15,32                                              0303
         ALR   @15,@09                                             0303
         ST    @15,CTGFLPTR                                        0303
*   CTGOPTN1='84'X;                 /* SET OPTION FLAGS              */
         MVI   CTGOPTN1(@09),X'84'                                 0304
*   CTGOPTN2='00'X;                 /*                               */
         MVI   CTGOPTN2(@09),X'00'                                 0305
*   CTGOPTN3='20'X;                                                0306
         MVI   CTGOPTN3(@09),X'20'                                 0306
*   CTGOPTN4='00'X;                 /* SET LAST OPTION BYTE          */
         MVI   CTGOPTN4(@09),X'00'                                 0307
*   CTGENT=ADDR(CTGFL)+LENGTH(CTGFL);/* POINTER TO DATA SET NAME     */
         AL    @15,@CF00727                                        0308
         ST    @15,CTGENT(,@09)                                    0308
*   DSNAME=' ';                     /* SET AREA TO BLANKS            */
         MVI   DSNAME+1(@15),C' '                                  0309
         MVC   DSNAME+2(42,@15),DSNAME+1(@15)                      0309
         MVI   DSNAME(@15),C' '                                    0309
*   DO IX=1 TO 44 WHILE(CAMPTR1->DSNAME(IX:IX)^=' ');              0310
         LA    IX,1                                                0310
@DL00310 L     @09,CAMLSTPT                                        0310
         L     @09,CAMPTR1(,@09)                                   0310
         LR    @06,@09                                             0310
         ALR   @06,IX                                              0310
         BCTR  @06,0                                               0310
         CLI   DSNAME(@06),C' '                                    0310
         BE    @DC00310                                            0310
*     DSNAME(IX:IX)=CAMPTR1->DSNAME(IX:IX);                        0311
         L     @06,CTGPLPTR                                        0311
         L     @06,CTGENT(,@06)                                    0311
         ALR   @06,IX                                              0311
         BCTR  @06,0                                               0311
         ALR   @09,IX                                              0311
         BCTR  @09,0                                               0311
         MVC   DSNAME(1,@06),DSNAME(@09)                           0311
*   END;                                                           0312
         AL    IX,@CF00077                                         0312
         C     IX,@CF02048                                         0312
         BNH   @DL00310                                            0312
@DC00310 DS    0H                                                  0313
*   CTGWKA=WKCORE+WKLEN-H272;       /* POINT TO WORK AREA            */
         L     @15,CTGPLPTR                                        0313
         LH    @14,WKLEN                                           0313
         AL    @14,WKCORE                                          0313
         SL    @14,@CF02483                                        0313
         ST    @14,CTGWKA(,@15)                                    0313
*   WORKLEN=H260;                   /* SET WORK AREA LENGTH TO WHAT
*                                      WE CAN MAP BACK               */
         LA    @07,260                                             0314
         STH   @07,WORKLEN(,@14)                                   0314
*   GPFLDPTR=CTGWKA+FOUR;           /* SET START                     */
         LA    GPFLDPTR,4                                          0315
         ALR   GPFLDPTR,@14                                        0315
*   CTWKLIMT=CTGWKA+WORKLEN;        /* SET END OF AREA               */
         ALR   @14,@07                                             0316
         ST    @14,CTWKLIMT                                        0316
*   CTGNOFLD=ONE;                   /* INDICATE ONE FIELD            */
         MVI   CTGNOFLD(@15),X'01'                                 0317
*   CTGFIELD(ONE)=CTGFLPTR;         /* POINT TO FPL                  */
         L     @14,CTGFLPTR                                        0318
         ST    @14,CTGFIELD(,@15)                                  0318
*   CTGFLDNM=ADDR('OSLOCATE');      /* POINT TO COMBO FIELD FOR    0319
*                                      ANSWERING OS LOCATES          */
         LA    @15,@CC02522                                        0319
         ST    @15,CTGFLDNM(,@14)                                  0319
*   GEN(L      R14,RSAVA);          /* RESTORE RETURN ADDRESS        */
         L      R14,RSAVA
*   END MAPOSCPL;                                                  0321
@EL00002 DS    0H                                                  0321
@EF00002 DS    0H                                                  0321
@ER00002 BR    @14                                                 0321
*MAPCPLOS:                                                         0322
*                                                                  0322
*   /*****************************************************************/
*   /*                                                               */
*   /* MAP THE CATALOG DATA FROM THE CPL-FPL-WORKAREA INTO THE CAMLST*/
*   /* WORK AREA                                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0322
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0322
MAPCPLOS DS    0H                                                  0323
*   GEN(ST     R14,RSAVA);          /* SAVE RETURN ADDRESS           */
         ST     R14,RSAVA
*   HLD=GPFLDPTR-TWELVE;            /* SAVE ADDRESS OF LAST ENTRY    */
         LR    HLD,GPFLDPTR                                        0324
         SL    HLD,@CF00729                                        0324
*   GPFLDPTR=CTGWKA+FOUR;           /* POINT TO START OF 1ST VALUE   */
         LA    GPFLDPTR,4                                          0325
         L     @07,CTGPLPTR                                        0325
         AL    GPFLDPTR,CTGWKA(,@07)                               0325
*   IF GPFLDPTR>HLD THEN            /* ARE THERE VALUES?             */
         CR    GPFLDPTR,HLD                                        0326
         BNH   @RF00326                                            0326
*     DO;                                                          0327
*       ERRCD=RCDSNF;               /* NO, SET ERROR CODE            */
         MVC   ERRCD(4),@CF01430                                   0328
*       RETURN TO EXIT;             /* BAIL OUT                      */
         LA    @14,EXIT                                            0329
@EL00003 DS    0H                                                  0329
@EF00003 DS    0H                                                  0329
@ER00003 BR    @14                                                 0329
*     END;                                                         0330
*   ELSE                            /* TRANSFER THE DATA             */
*     DO;                                                          0331
@RF00326 DS    0H                                                  0332
*       CAMVOLPT(ONE)=GPVOLPTR;     /* MOVE ONE VOLUME PTR           */
         MVC   CAMVOLPT(12,CAMWORK),GPVOLPTR(GPFLDPTR)             0332
*       GPFLDPTR=GPFLDPTR+TWELVE;   /* STEP WORKAREA POINTER         */
         AL    GPFLDPTR,@CF00729                                   0333
*       IX=ONE;                     /* INITIALIZE INDEX              */
         LA    IX,1                                                0334
*       DO GPFLDPTR=GPFLDPTR TO HLD BY TWELVE/* LOOP THROUGH VALUES  */
*             WHILE(IX^<TWENTY);    /* COLLECTED FROM THE CATALOG    */
         B     @DE00335                                            0335
@DL00335 C     IX,@CF01432                                         0335
         BL    @DC00335                                            0335
*         DO IX1=1 TO IX            /* NO DUPLICATE VOLUME ENTRIES   */
*               WHILE(CAMVOLSR(IX1)^=GPVOLSER);                    0336
         LA    IX1,1                                               0336
         B     @DE00336                                            0336
@DL00336 LR    @03,IX1                                             0336
         MH    @03,@CH00729                                        0336
         ALR   @03,CAMWORK                                         0336
         AL    @03,@CF02604                                        0336
         CLC   CAMVOLSR-6(6,@03),GPVOLSER(GPFLDPTR)                0336
         BE    @DC00336                                            0336
*         END;                                                     0337
         AL    IX1,@CF00077                                        0337
@DE00336 CR    IX1,IX                                              0337
         BNH   @DL00336                                            0337
@DC00336 DS    0H                                                  0338
*         IF IX1=IX THEN            /* DID WE FIND A DUPLICATE?      */
         CR    IX1,IX                                              0338
         BNE   @RF00338                                            0338
*           DO;                     /* NO MOVE VOLUME POINTER        */
*             CAMVOLPT(IX)=GPVOLPTR;/*                               */
         LR    @08,IX                                              0340
         MH    @08,@CH00729                                        0340
         ALR   @08,CAMWORK                                         0340
         AL    @08,@CF02605                                        0340
         MVC   CAMVOLPT-2(12,@08),GPVOLPTR(GPFLDPTR)               0340
*             IX=IX+ONE;            /* STEP INDEX                    */
         AL    IX,@CF00077                                         0341
*           END;                                                   0342
*       END;                                                       0343
@RF00338 AL    GPFLDPTR,@CF00729                                   0343
@DE00335 CR    GPFLDPTR,HLD                                        0343
         BNH   @DL00335                                            0343
@DC00335 DS    0H                                                  0344
*       CAMWKCNT=IX;                /* SET COUNT OF ENTRIES          */
         STH   IX,CAMWKCNT(,CAMWORK)                               0344
*     END;                                                         0345
*   GEN(L      R14,RSAVA);          /* RESTORE ADDRESS               */
         L      R14,RSAVA
*   END MAPCPLOS;                                                  0347
         B     @EL00003                                            0347
*CTINTFAC:                                                         0348
*                                                                  0348
*   /*****************************************************************/
*   /*                                                               */
*   /* INTERFACE WITH THE CATALOG ROUTINE TRAP TEST FIELDS           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0348
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0348
CTINTFAC DS    0H                                                  0349
*   GEN(ST   R14,RSAVAA);                                          0349
         ST   R14,RSAVAA
*   IF CTGFLCHN^=ZERO THEN          /* IS THERE A TEST FIELD?        */
         L     @08,CTGFLPTR                                        0350
         L     @06,CTGFLCHN(,@08)                                  0350
         LTR   @06,@06                                             0350
         BZ    @RF00350                                            0350
*     DO;                           /* YES DO                        */
*       SAVFLPTR=CTGFLPTR;          /* SAVE FIELD POINTER            */
         LR    SAVFLPTR,@08                                        0352
*       CTGFLPTR=CTGFLCHN;          /* USE TEST FIELD POINTER        */
         ST    @06,CTGFLPTR                                        0353
*       IF FLNAME^='ITYPEXT ' THEN  /* IS SSW TEST                   */
         L     @08,CTGFLDNM(,@06)                                  0354
         CLC   FLNAME(8,@08),@CC02529                              0354
         BE    @RF00354                                            0354
*         DO;                       /* NO ERROR                      */
*           ERRCD=RCINFNAM;         /* ERROR CODE                    */
         MVC   ERRCD(4),@CF02084                                   0356
*           RETURN TO EXIT;         /* BAIL OUT                      */
         LA    @14,EXIT                                            0357
@EL00004 DS    0H                                                  0357
@EF00004 DS    0H                                                  0357
@ER00004 BR    @14                                                 0357
*         END;                                                     0358
*       ELSE                        /* SSW TEST                      */
*         SWSSW=ON;                 /* INTRESTED IN SSW DATA         */
@RF00354 OI    SWSSW,B'00001000'                                   0359
*       IX8=CTGFLDCD;               /* SET PTR TO TEST FIELD         */
         L     @08,CTGFLPTR                                        0360
         SLR   @06,@06                                             0360
         IC    @06,CTGFLDCD(,@08)                                  0360
         ST    @06,IX8                                             0360
*       IX9=CTGFLPT(ONE);                                          0361
         L     @06,CTGFLPT(,@08)                                   0361
         ST    @06,IX9                                             0361
*       IXA=CTGFLNG(ONE);                                          0362
         L     IXA,CTGFLNG(,@08)                                   0362
*       CTGFLPTR=SAVFLPTR;          /* RESTOTRE POINTERS             */
         ST    SAVFLPTR,CTGFLPTR                                   0363
*     END;                                                         0364
*   CALL CATLG;                     /* NO TEST CALL CATLG            */
@RF00350 BAL   @14,CATLG                                           0365
*   SWSSW=OFFX;                                                    0366
*   SWBADTST=OFFX;                  /* TURN OFF BAD SSW TEST         */
         NI    SWSSW,B'11110101'                                   0367
*   GEN(L    R14,RSAVAA);                                          0368
         L    R14,RSAVAA
*   END CTINTFAC;                                                  0369
         B     @EL00004                                            0369
*CATLG:                                                            0370
*                                                                  0370
*   /*****************************************************************/
*   /*                                                               */
*   /* GET REQUESTED FIELD(S) FROM THE CATALOG                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0370
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0370
CATLG    DS    0H                                                  0371
*   GEN(ST     R14,RSAVA);                                         0371
         ST     R14,RSAVA
*   CPE0=CPX;                                                      0372
         L     CPE0,CPX                                            0372
*   SWCOMB=OFFX;                    /* SET COMBINATION SW OFF        */
         NI    SWCOMB,B'01111111'                                  0373
*   DICTWK(1:2)=FLNAME(1:2);        /* FIRST HALVE OF CONDENSED NM   */
         L     @08,CTGFLPTR                                        0374
         L     @08,CTGFLDNM(,@08)                                  0374
