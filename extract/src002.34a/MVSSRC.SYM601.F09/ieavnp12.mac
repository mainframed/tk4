*   DO IX1=1 TO TEN                 /* SEE IF REC IS IN BUFFER       */
*         WHILE(BFCI(IX1)^=ZERO);   /* OR FIND AN AVAILABLE SLOT     */
         LA    @15,1                                               0521
         LR    IX1,@15                                             0521
@DL00521 LR    @15,IX1                                             0521
         SLA   @15,3                                               0521
         L     @14,BFCI-9(@15)                                     0521
         LA    @14,0(,@14)                                         0521
         LTR   @14,@14                                             0521
         BZ    @DC00521                                            0521
*     IF BFCI(IX1)=CIWORK THEN      /* DID WE FIND IT?               */
         SLR   @05,@05                                             0522
         ICM   @05,7,CIWORK                                        0522
         CR    @14,@05                                             0522
         BNE   @RF00522                                            0522
*       DO;                         /* YES, DO                       */
*         CPE0=BFPTR(IX1);          /* SET OUTPUT POINTER            */
         L     CPE0,BFPTR-8(@15)                                   0524
*         IF BFCTR(IX1)^=H255 THEN  /* IF NOT THE MAIN CI            */
