*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0517
FINDCI   DS    0H                                                  0518
*   GEN(ST     R14,RSAVC);                                         0518
         ST     R14,RSAVC
*   IX2=ONE;                        /* INITIALIZE SECONDARY INDEX    */
         LA    @08,1                                               0519
         ST    @08,IX2                                             0519
*   CPE0=ZERO;                      /* INIT OUTPUT AREA              */
