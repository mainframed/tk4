         IC    @14,BFCTR-8(@15)                                    0525
         C     @14,@CF02480                                        0525
         BE    @RF00525                                            0525
*           BFCTR(IX1)=BFCTR(IX1)+ONE;/* INCR USE COUNTER            */
         AL    @14,@CF00077                                        0526
         STC   @14,BFCTR-8(@15)                                    0526
*       END;                                                       0527
*     ELSE                          /* NO UNUSED SLOTS               */
*       IF BFCTR(IX1)<BFCTR(IX2) THEN/* SMALLEST USE CNT?            */
         B     @RC00522                                            0528
