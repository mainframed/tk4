         SLA   CPE0,4                                              0543
         LA    CPE0,CCACPE-16(CPE0)                                0543
*       CCACPE1=ADDR(CATLGBUF(IX1));                               0544
         LR    @15,IX1                                             0544
         SLA   @15,9                                               0544
         L     @01,CATBFPTR                                        0544
         AL    @01,@CF02614                                        0544
         LA    @15,CATLGBUF(@15,@01)                               0544
         ST    @15,CCACPE1(,CPE0)                                  0544
*       CCACPE7=CCACPE1+NREPLF;     /* START OF VARIABLE FLDS        */
         SLR   @14,@14                                             0545
         IC    @14,NREPLF(,@15)                                    0545
         ALR   @14,@15                                             0545
         ST    @14,CCACPE7                                         0545
*       DO IX2=2 TO NREPCV;         /* FIND END OF VARIABLE FLDS     */
         LA    @15,2                                               0546
         B     @DE00546                                            0546
@DL00546 DS    0H                                                  0547
*         CCACPE7=CCACPE7+VARLEN+TWO;                              0547
         L     @14,CCACPE7                                         0547
         ICM   @05,12,VARLEN(@14)                                  0547
         SRA   @05,16                                              0547
         ALR   @05,@14                                             0547
         AL    @05,@CF00035                                        0547
         ST    @05,CCACPE7                                         0547
*       END;                                                       0548
         AL    @15,@CF00077                                        0548
@DE00546 ST    @15,IX2                                             0548
         L     @14,CCACPE1(,CPE0)                                  0548
         SLR   @05,@05                                             0548
         IC    @05,NREPCV(,@14)                                    0548
         CR    @15,@05                                             0548
         BNH   @DL00546                                            0548
*       CCACPE2=CCACPE7;            /* PTR TO CTL INFO FOR REPEAT  0549
*                                      FIELDS                        */
         L     @15,CCACPE7                                         0549
         ST    @15,CCACPE2(,CPE0)                                  0549
*       CCACPE3=CCACPE2+REPCNT*(LENGTH(CATGOPT))+/* POINTER TO       */
*           LENGTH(EXTRCDPT)+LENGTH(REPCNT);/* GRP OCCURRENCES       */
         SLR   @14,@14                                             0550
         IC    @14,REPCNT(,@15)                                    0550
         MH    @14,@CH00845                                        0550
         ALR   @15,@14                                             0550
         AL    @15,@CF00716                                        0550
         ST    @15,CCACPE3(,CPE0)                                  0550
