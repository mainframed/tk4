*       IF BFCTR(IX1)^=H255 THEN    /* IF NOT THE MAIN REC           */
         SLR   @15,@15                                             0538
         IC    @15,BFCTR-8(@06)                                    0538
         C     @15,@CF02480                                        0538
         BE    @RF00538                                            0538
*         BFCTR(IX1)=ONE;           /* INIT THE USE COUNT            */
         LA    @15,1                                               0539
