*SMFUPD:                                                           1852
*                                                                  1852
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE TESTS FOR THE USERS OPTION OF SMF RECORDING IN   */
*   /* THE SMCA WHICH IS POINTED TO BY THE CVT. A GETMAIN IS ISSUED  */
*   /* FOR A MESSAGE AREA IN WHICH TO BUILD SMF TYPE 14, 15, AND 64  */
*   /* RECORDS. DEPENDING ON THE OPTION, DATA SET STATISTICS ARE     */
*   /* GATHERED AND PUT IN THE MESSAGE AREA. THE SMF WRITER (SVC 83) */
*   /* IS INVOKED WITH REGISTER 1 POINTING TO THE MESSAGE AREA TO    */
*   /* WRITE OUT THE SMF RECORD TO THE SYS1.MAN DATA SET. THE MESSAGE*/
*   /* AREA IS THEN FREED.                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1852
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1852
SMFUPD   DS    0H                                                  1853
*   RFY                                                            1853
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/
*   EOVSAV1=RET;                    /* SAVE RETURN ADDRESS.          */
         ST    RET,EOVSAV1(,RWORK)                                 1854
*   RFY                                                            1855
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/
*   EOVFUNC=PDF206;                 /* SET FUNCTION CODE - SMF UPDATE
*                                                            @Y30DSJH*/
         MVI   EOVFUNC(RWORK),X'CE'                                1856
*   IF CVTSMCA^=ZERO&               /* SMCA EXISTS           @ZA07545*/
*       SMCAUSER=ON&SMCAMAN=ON&     /* USER & SMF RECORDING  @ZA07545*/
*       SMCADSA=ON&                 /* DATA SET ACCOUNTING   @ZA07545*/
*       TCBTCTB^=ZERO THEN          /* TCT EXISTS            @ZA29395*/
         L     @10,CVTPTR                                          1857
         L     @10,CVTSMCA(,@10)                                   1857
         LTR   @10,@10                                             1857
         BZ    @RF01857                                            1857
         TM    SMCAUSER(@10),B'11000000'                           1857
         BNO   @RF01857                                            1857
         TM    SMCADSA(@10),B'00010000'                            1857
         BNO   @RF01857                                            1857
         L     @10,PSATOLD                                         1857
         L     @10,TCBTCTB-1(,@10)                                 1857
         LA    @10,0(,@10)                                         1857
         LTR   @10,@10                                             1857
         BZ    @RF01857                                            1857
*     DO;                                                          1858
*       EOVSMFPL=EOVSMFPL&&EOVSMFPL;/* CLR SMF PARAMETER LIST        */
         XC    EOVSMFPL(24,RWORK),EOVSMFPL(RWORK)                  1859
*       SMFDSNAM=EOVLOCDS(1);       /* POINT TO DATA SET NAME.       */
         L     @10,EOVLOCDS(,RWORK)                                1860
         ST    @10,SMFDSNAM(,RWORK)                                1860
*       SMFCTNAM=ADDR(CAXCNAM);     /* POINT TO CATALOG DSNAME.      */
         L     @10,EOVAMBPT(,RWORK)                                1861
         L     @10,AMBCACB(,@10)                                   1861
         L     @10,ACBUAPTR(,@10)                                  1861
         LA    @10,CAXCNAM(,@10)                                   1861
         ST    @10,SMFCTNAM(,RWORK)                                1861
*       IF ALLOCSP=ON THEN          /* ALLOCATE REQUEST.             */
         TM    ALLOCSP(RWORK),B'00000010'                          1862
         BNO   @RF01862                                            1862
*         DO;                                                      1863
*           IF EOVCAND=ON THEN      /* PRIMARY ALLOCATION?   @ZA17050*/
         TM    EOVCAND(RWORK),B'01000000'                          1864
         BNO   @RF01864                                            1864
*             SMFAQTY=EOVPALOC/LPMBPTRK;/* PRI ALLOC QTY IN TRKS   1865
*                                                            @ZA17050*/
         L     @10,EOVLPMB(,RWORK)                                 1865
         L     @01,EOVPALOC(,RWORK)                                1865
         SLR   @00,@00                                             1865
         D     @00,LPMBPTRK(,@10)                                  1865
         ST    @01,SMFAQTY(,RWORK)                                 1865
*           ELSE                                                   1866
*             SMFAQTY=EOVSALOC/LPMBPTRK;/* SECONDARY QTY IN TRKS   1866
*                                                            @ZA17050*/
         B     @RC01864                                            1866
@RF01864 L     @10,EOVLPMB(,RWORK)                                 1866
         L     @01,EOVSALOC(,RWORK)                                1866
         SLR   @00,@00                                             1866
         D     @00,LPMBPTRK(,@10)                                  1866
         ST    @01,SMFAQTY(,RWORK)                                 1866
*           SMFOUTSP=ON;            /* OUT OF SPACE INDICATION.      */
@RC01864 OI    SMFOUTSP(RWORK),B'00100000'                         1867
*         END;                                                     1868
*       ELSE                        /* MOUNT REQUEST.                */
*         SMFVOLSW=ON;              /* VOLUME SWITCH INDICATION.     */
         B     @RC01862                                            1869
@RF01862 OI    SMFVOLSW(RWORK),B'01000000'                         1869
*       SMFAMB=ADDR(IDAAMB);        /* POINT TO AMB.                 */
@RC01862 L     @10,EOVAMBPT(,RWORK)                                1870
         ST    @10,SMFAMB(,RWORK)                                  1870
*       SMFID=64;                   /* SET RECORD IDENTIFIER.        */
         MVI   SMFID(RWORK),X'40'                                  1871
*       IF ALLOCSP=OFF&CATSSDAT=ON THEN/* IF MOUNT BY RBA AND      1872
*                                      SEQUENCE SET WITH DATA      1872
*                                                            @ZA11087*/
         TM    ALLOCSP(RWORK),B'00000010'                          1872
         BNZ   @RF01872                                            1872
         TM    CATSSDAT(RWORK),B'10000000'                         1872
         BNO   @RF01872                                            1872
*         SMFTIOT=TCBTIO+(AMBPAMBL->AMBLDTA->AMBTIOT);/* POINT TO  1873
*                                      DATA TIOT ENTRY       @ZA11087*/
         L     @10,AMBPAMBL(,@10)                                  1873
         L     @10,AMBLDTA(,@10)                                   1873
         LH    @10,AMBTIOT(,@10)                                   1873
         L     @07,PSATOLD                                         1873
         AL    @10,TCBTIO(,@07)                                    1873
         ST    @10,SMFTIOT(,RWORK)                                 1873
*       ELSE                        /* ALLOCATE SPACE OR NOT SEQUENCE
*                                      SET                   @XA11087*/
*         SMFTIOT=DXTIOTAD;         /* SET UP TIOT POINTER   @OZ04708*/
         B     @RC01872                                            1874
@RF01872 L     @10,EOVRCORE(,RWORK)                                1874
         L     @10,DXTIOTAD(,@10)                                  1874
         ST    @10,SMFTIOT(,RWORK)                                 1874
*       RSTATRTN=IDAR192S;          /* NOTE EXIT TO IDA0192S @Y021139*/
@RC01872 L     @10,EOVRCORE(,RWORK)                                1875
         MVC   RSTATRTN(4,@10),@CC05358                            1875
*       RFY                                                        1876
*         R1 RSTD;                  /* RESTRICT REGISTER     @Z30SSJH*/
*       R1=ADDR(SMFPL);             /* POINT TO SMF PARAMETER LIST.  */
*                                                                  1877
         LA    R1,SMFPL(,RWORK)                                    1877
*       /*************************************************************/
*       /*                                                           */
*       /* CALL SMF ROUTINE TO WRITE TYPE 64 RECORD.                 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  1878
*       CALL IDA0192S;              /* CALL SMF UPDATE               */
         L     @15,@CV05556                                        1878
         BALR  @14,@15                                             1878
*       RFY                                                        1879
*         R1 UNRSTD;                /* DROP REG RESTRICTION  @Z30SSJH*/
*       RSTATRTN=IDAR557A;          /* NOTE RETURN TO 557A   @Y30DSJH*/
         L     @10,EOVRCORE(,RWORK)                                1880
         MVC   RSTATRTN(4,@10),@CC05376                            1880
*     END;                                                         1881
*   RFY                                                            1882
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/
@RF01857 DS    0H                                                  1883
*   RET=EOVSAV1;                    /* RESTORE RETURN ADDRESS.       */
         L     RET,EOVSAV1(,RWORK)                                 1883
*   RFY                                                            1884
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/
*   END SMFUPD;                                                    1885
@EL00029 DS    0H                                                  1885
@EF00029 DS    0H                                                  1885
@ER00029 BR    @14                                                 1885
*CATLOCDS:                                                         1886
*                                                                  1886
*   /*****************************************************************/
*   /*                                                               */
*   /* CATALOG LOCATE DATA SET INFORMATION - THIS ROUTINE LOCATES THE*/
*   /* DATA SET NAME AND ALSO THE ALLOCATION QUANTITY IF THE REQUEST */
*   /* IS FOR ALLOCATE SPACE.                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1886
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1886
CATLOCDS DS    0H                                                  1887
*   RFY                                                            1887
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/
*   EOVSAV4=RET;                    /* SAVE RETURN ADDRESS.          */
         ST    RET,EOVSAV4(,RWORK)                                 1888
*   RFY                                                            1889
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/
*                                                                  1889
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD CATALOG PARAMETER LIST.                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1890
*   R6=CTGPLSZ+(4*CTGFLSZ);         /* FIND LENGTH OF LISTS  @ZA17050*/
         L     R6,CTGFLSZ                                          1890
         SLA   R6,2                                                1890
         AL    R6,CTGPLSZ                                          1890
*   R6=R6+(3*LENGTH(CTGFIELD));     /*                       @ZA17050*/
         AL    R6,@CF00118                                         1891
*   CALL CATSETUP;                  /* INITIALIZE CPL.       @ZA13505*/
         BAL   @14,CATSETUP                                        1892
*   CTGNOFLD=1;                     /* ONE CHAIN OF FIELD LISTS.     */
         L     @10,RCATW(,RWORK)                                   1893
         MVI   CTGNOFLD(@10),X'01'                                 1893
*   RCATF=RCATF+(3*LENGTH(CTGFIELD));/* PT TO 1ST FPL        @ZA17050*/
         LA    @07,12                                              1894
         AL    @07,RCATF(,RWORK)                                   1894
         ST    @07,RCATF(,RWORK)                                   1894
*   CTGFIELD(1)=RCATF;              /* POINT PARAMETER LIST TO FIRST
*                                      FIELD LIST                    */
         ST    @07,CTGFIELD(,@10)                                  1895
*   CTGFLDNM=ADDR(DSNAME);          /* POINT TO FIELD NAME.          */
         LA    @15,@CC00490                                        1896
         ST    @15,CTGFLDNM(,@07)                                  1896
*   CTGFLDNO=1;                     /* ONE FIELD DATA.               */
         MVI   CTGFLDNO(@07),X'01'                                 1897
*   IF ALLOCSP=ON THEN              /* ALLOCATE REQUEST.             */
         TM    ALLOCSP(RWORK),B'00000010'                          1898
         BNO   @RF01898                                            1898
*     DO;                                                          1899
*       CTGNOFLD=4;                 /* REQUEST 4 FIELDS      @ZA17050*/
         MVI   CTGNOFLD(@10),X'04'                                 1900
*       CTGFIELD(2)=RCATF+CTGFLSZ;  /* POINT CTGPL TO 2ND FIELD LIST */
         L     @15,CTGFLSZ                                         1901
         ALR   @07,@15                                             1901
         ST    @07,CTGFIELD+4(,@10)                                1901
*       RCATF=CTGFIELD(2);          /* BASE NEW FIELD LIST           */
         ST    @07,RCATF(,RWORK)                                   1902
*       CTGFLDNO=1;                 /* ONE DATA FIELD        @ZA17050*/
         MVI   CTGFLDNO(@07),X'01'                                 1903
*       CTGFLDNM=ADDR(PSPACENM);    /* POINT TO PRIMARY ALLOCATION 1904
*                                      FIELD NAME.                   */
         LA    @14,@CC00492                                        1904
         ST    @14,CTGFLDNM(,@07)                                  1904
*       CTGFIELD(3)=RCATF+CTGFLSZ;  /* PT CPL TO THIRD FIELD @ZA17050*/
         ALR   @07,@15                                             1905
         ST    @07,CTGFIELD+8(,@10)                                1905
*       RCATF=CTGFIELD(3);          /* BASE NEW FIELD LIST   @ZA17050*/
         ST    @07,RCATF(,RWORK)                                   1906
*       CTGFLDNM=ADDR(SSPACENM);    /* POINT TO SECONDARY ALLOCATION
*                                      FIELD. NAME.                  */
         LA    @14,@CC00494                                        1907
         ST    @14,CTGFLDNM(,@07)                                  1907
*       CTGFLDNO=1;                 /* ONE FIELD DATA.               */
         MVI   CTGFLDNO(@07),X'01'                                 1908
*       CTGFIELD(4)=RCATF+CTGFLSZ;  /* PT CPL TO THIRD FIELD @ZA17050*/
         ALR   @07,@15                                             1909
         ST    @07,CTGFIELD+12(,@10)                               1909
*       RCATF=CTGFIELD(4);          /* BASE NEW FIELD LIST   @ZA17050*/
         ST    @07,RCATF(,RWORK)                                   1910
*       CTGFLDNM=ADDR(SPOPTION);    /* SPACE OPTIONS         @ZA13505*/
         LA    @10,@CC00496                                        1911
         ST    @10,CTGFLDNM(,@07)                                  1911
*       CTGFLDNO=1;                 /* ONE FIELD DATA        @ZA13505*/
         MVI   CTGFLDNO(@07),X'01'                                 1912
*     END;                                                         1913
*   RFY                                                            1914
*    (R1,                                                          1914
*     R15) RSTD;                    /* RESTRICT REGISTERS    @Z30SSJH*/
@RF01898 DS    0H                                                  1915
*   R1=RCATW;                       /* POINT TO CATALOG PARAMETER  1915
*                                      LIST.                         */
         L     R1,RCATW(,RWORK)                                    1915
*   GEN(CATLG MF=(E,(1)));          /* ISSUE CATALOG MACRO.          */
         CATLG MF=(E,(1))
*   RFY                                                            1917
*     R1 UNRSTD;                    /* DROP REG RESTRICTION  @Z30SSJH*/
*                                                                  1917
*   /*****************************************************************/
*   /*                                                               */
*   /* TEST IF CATALOG ERROR.                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1918
*   CATRC=R15;                      /* SAVE CATALOG RETURN CODE.     */
         STH   R15,CATRC(,RWORK)                                   1918
*   RFY                                                            1919
*     R15 UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/
*   IF CATRC^=0 THEN                /* CATALOG ERROR.                */
         LH    @10,CATRC(,RWORK)                                   1920
         N     @10,@CF05661                                        1920
         LTR   @10,@10                                             1920
         BZ    @RF01920                                            1920
*     DO;                                                          1921
*       IF EOVEXIT=OFF THEN         /* EOV EXITING           @Z30SSJH*/
         TM    EOVEXIT(RWORK),B'01000000'                          1922
         BNZ   @RF01922                                            1922
*         DO;                       /* NO, OK TO FLAG ERROR  @Z30SSJH*/
*           CALL CATERR;            /* SET PROBLEM CODE.             */
         BAL   @14,CATERR                                          1924
*           EOVERR=MNTERR;          /* SET UNABLE TO MOUNT ERROR.    */
         MVI   EOVERR(RWORK),X'04'                                 1925
*         END;                      /*                       @Z30SSJH*/
*     END;                                                         1927
*   ELSE                            /* NO CATLOG ERROR.              */
*     DO;                                                          1928
*                                                                  1928
         B     @RC01920                                            1928
@RF01920 DS    0H                                                  1929
*       /*************************************************************/
*       /*                                                           */
*       /* TEST IF FIELD DATA RETURNED, AND SAVE POINTERS    @ZA13505*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  1929
*       DO I=1 TO CTGNOFLD;         /* TEST ALL FIELDS       @ZA13505*/
         LA    @10,1                                               1929
         B     @DE01929                                            1929
@DL01929 DS    0H                                                  1930
*         RCATF=CTGFIELD(I);        /* POINT TO FIELD LIST   @ZA27118*/
         L     @10,I(,RWORK)                                       1930
         SLA   @10,2                                               1930
         L     @07,RCATW(,RWORK)                                   1930
         L     @07,CTGFIELD-4(@10,@07)                             1930
         ST    @07,RCATF(,RWORK)                                   1930
*         EOVLOCDS(I)=CTGFLPT(1);   /* SAVE POINTER          @ZA13505*/
         L     @07,CTGFLPT(,@07)                                   1931
         ST    @07,EOVLOCDS-4(@10,RWORK)                           1931
*         IF EOVLOCDS(I)=0 THEN     /* IF NO FIELD DATA      @ZA13505*/
         LTR   @07,@07                                             1932
         BNZ   @RF01932                                            1932
*           DO;                     /*                       @ZA13505*/
*             EOVPDCDE=PP32;        /* SET PROBLEM CODE      @ZA13505*/
         MVI   EOVPDCDE(RWORK),X'20'                               1934
*             EOVERR=MNTERR;        /* SET EOV ERROR         @ZA13505*/
         MVI   EOVERR(RWORK),X'04'                                 1935
*           END;                                                   1936
*       END;                                                       1937
@RF01932 LA    @10,1                                               1937
         AL    @10,I(,RWORK)                                       1937
@DE01929 ST    @10,I(,RWORK)                                       1937
         L     @07,RCATW(,RWORK)                                   1937
         SLR   @15,@15                                             1937
         IC    @15,CTGNOFLD(,@07)                                  1937
         CR    @10,@15                                             1937
         BNH   @DL01929                                            1937
*     END;                                                         1938
*   RFY                                                            1939
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/
@RC01920 DS    0H                                                  1940
*   RET=EOVSAV4;                    /* RESTORE RETURN ADDRESS        */
         L     RET,EOVSAV4(,RWORK)                                 1940
*   RFY                                                            1941
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/
*   END CATLOCDS;                                                  1942
@EL00030 DS    0H                                                  1942
@EF00030 DS    0H                                                  1942
@ER00030 BR    @14                                                 1942
*TERM:                                                             1943
*                                                                  1943
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE WILL PERFORM THE FOLLOWING FUNCTIONS FOR ENDING  */
*   /* EOV REQUEST: SAVE THE ERROR CODE, FREE THE EOV WORK AREA, AND */
*   /* FREE THE CATALOG WORKAREA.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1943
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1943
TERM     DS    0H                                                  1944
*   IF RMCATUPD=OFF THEN            /* NOT CATALOG UPDATE REQ.       */
         TM    RMCATUPD(RWORK),B'00000100'                         1944
         BNZ   @RF01944                                            1944
*     DO;                                                          1945
*       IF EOVFUNC^=PDF201&         /* NOT EOVINIT FUNCTION  @Y30DSJH*/
*           EOVERR^=0&              /* ERROR OCCURRED        @Y30DSJH*/
*           EOVERR^=IOBLKERR&       /* NOT IOMB LOCK ERROR   @Y30DSJH*/
*           EOVERR^=REDRVERR THEN   /* NOT REDRIVE ERROR     @Y30DSJH*/
         CLI   EOVFUNC(RWORK),201                                  1946
         BE    @RF01946                                            1946
         CLI   EOVERR(RWORK),0                                     1946
         BE    @RF01946                                            1946
         CLI   EOVERR(RWORK),12                                    1946
         BE    @RF01946                                            1946
         CLI   EOVERR(RWORK),12                                    1946
         BE    @RF01946                                            1946
*         CALL RESET;               /* RESET CONTROL BLOCKS          */
         BAL   @14,RESET                                           1947
*       IF EOVERR=0 THEN            /* NO EOV ERROR                  */
@RF01946 CLI   EOVERR(RWORK),0                                     1948
         BNE   @RF01948                                            1948
*         DO;                                                      1949
*           EOVCBLPT=ADDR(EOVCBL1); /* POINT TO CONTROL BLOCK LIST -
*                                      REQ. AMB                      */
         LA    @10,EOVCBL1(,RWORK)                                 1950
         ST    @10,EOVCBLPT(,RWORK)                                1950
*           CALL FREECB;            /* FREE OLD DEB & EDB FOR REQ AMB*/
         BAL   @14,FREECB                                          1951
*           IF EOV2AMBS=ON THEN     /* 2 AMB'S PROCESSED             */
         TM    EOV2AMBS(RWORK),B'00001000'                         1952
         BNO   @RF01952                                            1952
*             DO;                                                  1953
*               IF AMBTYPE=AMBDATA THEN/* EOV FOR DATA       @Z30SSJH*/
         L     @10,EOVAMBPT(,RWORK)                                1954
         TM    AMBTYPE(@10),B'01000000'                            1954
         BNZ   @RF01954                                            1954
*                 EOVAMBPT=AMBLIX;  /* YES, GET OTHER AMB    @Z30SSJH*/
         L     @10,AMBPAMBL(,@10)                                  1955
         L     @10,AMBLIX(,@10)                                    1955
         ST    @10,EOVAMBPT(,RWORK)                                1955
*               ELSE                /* EOV IS FOR INDEX      @Z30SSJH*/
*                 EOVAMBPT=AMBLDTA; /* PROCESS DATA          @Z30SSJH*/
         B     @RC01954                                            1956
@RF01954 L     @10,EOVAMBPT(,RWORK)                                1956
         L     @10,AMBPAMBL(,@10)                                  1956
         L     @10,AMBLDTA(,@10)                                   1956
         ST    @10,EOVAMBPT(,RWORK)                                1956
*               EOVCBLPT=ADDR(EOVCBL2);/* POINT TO CONTROL BLOCK LIST
*                                      FOR 2ND AMB                   */
@RC01954 LA    @10,EOVCBL2(,RWORK)                                 1957
         ST    @10,EOVCBLPT(,RWORK)                                1957
*               CALL FREECB;        /* FREE OLD DEB & EDB FOR 2ND AMB*/
         BAL   @14,FREECB                                          1958
*               EOVAMBPT=EOVAMB;    /* RESTORE REQ AMB       @Z30SSJH*/
         L     @10,EOVAMB(,RWORK)                                  1959
         ST    @10,EOVAMBPT(,RWORK)                                1959
*             END;                                                 1960
*         END;                                                     1961
@RF01952 DS    0H                                                  1962
*       IF AMBLCAT=OFF THEN         /* NOT CATALOG DATA SET  @Y30DSJH*/
@RF01948 L     @10,EOVAMBPT(,RWORK)                                1962
         L     @10,AMBPAMBL(,@10)                                  1962
         TM    AMBLCAT(@10),B'00000010'                            1962
         BNZ   @RF01962                                            1962
*         IF EOVIOBF=0 THEN         /* WERE IOMB'S LOCKED    @Y30DSJH*/
         L     @10,EOVIOBF(,RWORK)                                 1963
         LTR   @10,@10                                             1963
         BNZ   @RF01963                                            1963
*           CALL UNLOCK;            /* YES, UNLOCK IOMB'S    @Y30DSJH*/
         BAL   @14,UNLOCK                                          1964
*       IF EOVERR=0 THEN            /* DID EOV ERROR OCCUR   @Y30DSJH*/
@RF01963 DS    0H                                                  1965
@RF01962 CLI   EOVERR(RWORK),0                                     1965
         BNE   @RF01965                                            1965
*         IF ALLOCSP=ON THEN        /* NO, ALLOC SPACE REQ   @Y30DSJH*/
         TM    ALLOCSP(RWORK),B'00000010'                          1966
         BNO   @RF01966                                            1966
*           DO;                     /*                       @Y30DSJH*/
*             CALL PRFMT;           /* CALL PREFORMAT RTN    @Y30DSJH*/
         BAL   @14,PRFMT                                           1968
*             IF AMBTYPE=AMBDATA THEN/* DATA AMB             @Y30DSJH*/
         L     @10,EOVAMBPT(,RWORK)                                1969
         TM    AMBTYPE(@10),B'01000000'                            1969
         BNZ   @RF01969                                            1969
*               IF EOVSSWIX=ON THEN /* SEQENCE SET WITH DATA @ZA08325*/
         TM    EOVSSWIX(RWORK),B'01000000'                         1970
         BNO   @RF01970                                            1970
*                 IF AMBCREAT^=ON THEN/* YES, CREATE MODE@Y30DSJH    */
         TM    AMBCREAT(@10),B'10000000'                           1971
         BO    @RF01971                                            1971
*                   DO;             /* CREATE MODE           @Y30DSJH*/
*                     EOVAMBPT=AMBPIXP;/* PT TO PRIME INDEX AMB    1973
*                                                            @Y30DSJH*/
         L     @10,AMBPIXP(,@10)                                   1973
         ST    @10,EOVAMBPT(,RWORK)                                1973
*                     CALL PRFMT;   /* CALL PREFORMAT RTN    @Y30DSJH*/
         BAL   @14,PRFMT                                           1974
*                     EOVAMBPT=EOVAMB;/* RESTORE AMB POINTER @Y30DSJH*/
         L     @10,EOVAMB(,RWORK)                                  1975
         ST    @10,EOVAMBPT(,RWORK)                                1975
*                   END;            /*                       @Y30DSJH*/
*           END;                    /*                       @Y30DSJH*/
@RF01971 DS    0H                                                  1977
