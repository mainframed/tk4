*                                      COMBINATION                   */
*     DO;                                                          0610
         B     @RC00608                                            0610
@RF00608 DS    0H                                                  0611
*       IF SWCOMB=OFFX THEN         /* IF WE DON'T HAVE A COMBINATION
*                                      FIELD                         */
         TM    SWCOMB,B'10000000'                                  0611
         BNZ   @RF00611                                            0611
*         IX2=ONE;                  /* SET LOOP COUNT TO 1           */
         LA    IX2,1                                               0612
*       ELSE                        /* WE HAVE A COMBINATION FIELD   */
*         DO;                                                      0613
         B     @RC00611                                            0613
@RF00611 DS    0H                                                  0614
*           IX=ADDR(ZCS)+DICTST-ONE;/* POINT TO START OF DICTIONARY  */
         LA    @10,ZCS                                             0614
         SLR   @05,@05                                             0614
         IC    @05,DICTST(,DICTPTR)                                0614
         ALR   @10,@05                                             0614
         BCTR  @10,0                                               0614
         ST    @10,IX                                              0614
*           IX2=DICTLC;             /* LOOP COUNT = NBR OF FIELD     */
         SLR   IX2,IX2                                             0615
         IC    IX2,DICTLC(,DICTPTR)                                0615
*         END;                                                     0616
*       SWREPT=ON;                  /* TURN REPEATING SW ON          */
@RC00611 DS    0H                                                  0618
*       SWVALUE=ON;                 /* SET VALUE FLAG ON             */
         OI    SWREPT,B'00110000'                                  0618
*       GRPSEQNO=ONE;               /* SET THE GROUP SEQUENCE NBR    */
         MVI   GRPSEQNO,X'01'                                      0619
*       DO WHILE(SWREPT=ON&SWVALUE=ON);/* GET REPEATING VALUES IF ANY*/
         B     @DE00620                                            0620
@DL00620 DS    0H                                                  0621
*         SWREPT=OFFX;                                             0621
*         SWVALUE=OFFX;                                            0622
*         SWDEL=OFFX;                                              0623
         NI    SWREPT,B'11001101'                                  0623
*         DO IX1=1 TO IX2           /* GET ALL VALUES OF COMB        */
*               WHILE(SWDEL=OFFX);                                 0624
         LA    IX1,1                                               0624
         B     @DE00624                                            0624
@DL00624 TM    SWDEL,B'00000010'                                   0624
         BNZ   @DC00624                                            0624
*           IF SWCOMB=ON THEN       /* COMBINATION FIELD?            */
         TM    SWCOMB,B'10000000'                                  0625
         BNO   @RF00625                                            0625
*             DICTPTR=COMBPTR(IX1)*EIGHT-/* SET DICTPTR USING THE    */
*                 EIGHT+ADDR(ZDT);  /* COMB PART OF DICTIONARY       */
         L     @10,IX                                              0626
         AL    @10,@CF02429                                        0626
         SLR   DICTPTR,DICTPTR                                     0626
         IC    DICTPTR,COMBPTR(IX1,@10)                            0626
         SLA   DICTPTR,3                                           0626
         SL    DICTPTR,@CF00051                                    0626
         LA    @10,ZDT                                             0626
         ALR   DICTPTR,@10                                         0626
*           IF DICTNAME='REEP' THEN /* IS RELREPNO THE FIELD?        */
@RF00625 CLC   DICTNAME(4,DICTPTR),@CC02444                        0627
         BNE   @RF00627                                            0627
*             SWREEP=ON;            /* SET SW TO GET RELREPNO        */
         OI    SWREEP,B'01000000'                                  0628
*           ELSE                    /* NO RELREPNO FIELD             */
*             IF DICTGC=ZERO THEN   /* DO WE HAVE A GROUP?           */
         B     @RC00627                                            0629
@RF00627 CLI   DICTGC(DICTPTR),0                                   0629
         BNE   @RF00629                                            0629
*               CALL MOVFIXED;      /* MOVE DATA                     */
         BAL   @14,MOVFIXED                                        0630
*             ELSE                  /* WE HAVE A GROUP               */
*               CALL GETGROUP;      /* GET A GROUP ELEMENT           */
         B     @RC00629                                            0631
@RF00629 BAL   @14,GETGROUP                                        0631
*         END;                                                     0632
@RC00629 DS    0H                                                  0632
@RC00627 AL    IX1,@CF00079                                        0632
@DE00624 CR    IX1,IX2                                             0632
         BNH   @DL00624                                            0632
@DC00624 DS    0H                                                  0633
*         GRPSEQNO=GRPSEQNO+ONE;    /* STEP GROUP SEQUENCE NBR       */
         LA    @10,1                                               0633
         SLR   @05,@05                                             0633
         IC    @05,GRPSEQNO                                        0633
         ALR   @10,@05                                             0633
         STC   @10,GRPSEQNO                                        0633
*       END;                                                       0634
@DE00620 TM    SWREPT,B'00110000'                                  0634
         BO    @DL00620                                            0634
*       FLLENGTH=GPFLDPTR-CATWKPTR; /* SET FIELD LENGTH              */
         LR    FLLENGTH,GPFLDPTR                                   0635
         SL    FLLENGTH,CATWKPTR                                   0635
*     END;                                                         0636
*   END CTINTRFC;                                                  0637
@EL00010 DS    0H                                                  0637
@EF00010 DS    0H                                                  0637
@ER00010 LM    @14,@06,@SA00010                                    0637
         LM    @08,@12,@SA00010+36                                 0637
