*   COREADR2=COREADR2+CORELEN;      /* STEP POINTER                  */
         L     @14,CORELEN                                         0804
         ALR   @15,@14                                             0804
         ST    @15,COREADR2                                        0804
*   COREPTR(CORENBR)=COREADDR;                                     0805
         SLA   @03,3                                               0805
         ST    COREADDR,COREPTR-8(@03,@10)                         0805
*   COREPOOL(CORENBR)=MAINPOOL;     /* BUILD CORE TABLE ENTRY FOR    */
         L     @15,MAINPOOL                                        0806
         STC   @15,COREPOOL-8(@03,@10)                             0806
