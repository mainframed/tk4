*   IF COREPTR1=ZERO THEN           /* FIRST TIME IN?                */
         L     @00,COREPTR1                                        0773
         LTR   @00,@00                                             0773
         BNZ   @RF00773                                            0773
*     DO;                           /* YES DO                        */
*       CALL GETMAIN;               /* GET A BLOCK                   */
         BAL   @14,GETMAIN                                         0775
*       COREPTR1=COREADR2;          /* PTR TO TOP OF COREBLK CHAIN   */
         L     @10,COREADR2                                        0776
         ST    @10,COREPTR1                                        0776
*       COREPTRN=COREADR2;          /* PTR TO CURRENT COREBLK        */
         ST    @10,COREPTRN                                        0777
*       COREBKPL=MAINPOOL;                                         0778
         MVC   COREBKPL(1,@10),MAINPOOL+3                          0778
*       CORESIZE=LENGTH(COREBLK);                                  0779
         LA    @15,96                                              0779
         STCM  @15,7,CORESIZE(@10)                                 0779
*       COREADR2=COREADR2+LENGTH(COREBLK);/* UPDATE AVAILABLE CORE 0780
*                                      PTR                           */
         ALR   @10,@15                                             0780
         ST    @10,COREADR2                                        0780
*       COREAVIL=COREAVIL-LENGTH(COREBLK);/* UPDATE CORE AMOUNT    0781
*                                      AVAILABLE                     */
         LCR   @15,@15                                             0781
         AL    @15,COREAVIL                                        0781
         ST    @15,COREAVIL                                        0781
*     END;                                                         0782
*   IF CORENBR=TEN THEN             /* IS CURRENT COREBLK USED UP?   */
@RF00773 L     @10,COREPTRN                                        0783
         CLC   CORENBR(2,@10),@CH00932                             0783
         BNE   @RF00783                                            0783
*     DO;                           /* YES, CREATE A NEW ONE         */
*       IF COREAVIL<LENGTH(COREBLK) THEN/* ENOUGH CORE?              */
         L     @10,COREAVIL                                        0785
         C     @10,@CF00940                                        0785
         BNL   @RF00785                                            0785
*         DO;                       /* NO GET MORE                   */
*           CALL FREEMAIN;          /* RELEASE WHATS LEFT            */
         BAL   @14,FREEMAIN                                        0787
*           CALL GETMAIN;           /* GET ANOTHER BLOCK             */
         BAL   @14,GETMAIN                                         0788
*         END;                                                     0789
*       CORELAST=COREADR2;          /* CHAIN NEW BLOCK               */
@RF00785 L     @10,COREADR2                                        0790
         L     @15,COREPTRN                                        0790
