*               CALL SWAIT;         /* GO TO SYSTEM WAIT ROUTINE     */
         BAL   @14,SWAIT                                           0662
*             END;                                                 0663
*         END;                                                     0664
@RF00659 DS    0H                                                  0665
*       CALL MOVGROUP;              /* MOVE ELEMENT OF GROUP         */
@RF00650 BAL   @14,MOVGROUP                                        0665
*     END;                                                         0666
