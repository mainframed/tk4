         LA    @03,1                                               0802
         AH    @03,CORENBR(,@10)                                   0802
         STH   @03,CORENBR(,@10)                                   0802
*   COREADDR=COREADR2;              /* PTR FOR CALLER                */
         L     @15,COREADR2                                        0803
