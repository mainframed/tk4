*       COREPTRN=COREADR2;          /* STEP BASE                     */
         ST    @10,COREPTRN                                        0791
*       COREBKPL=MAINPOOL;                                         0792
         MVC   COREBKPL(1,@10),MAINPOOL+3                          0792
*       CORESIZE=LENGTH(COREBLK);                                  0793
         LA    @15,96                                              0793
         STCM  @15,7,CORESIZE(@10)                                 0793
*       COREADR2=COREADR2+          /* AVAILABLE CORE PTR            */
*           LENGTH(COREBLK);                                       0794
         ALR   @10,@15                                             0794
         ST    @10,COREADR2                                        0794
*       COREAVIL=COREAVIL-          /* AVAILABLE CORE SIZE           */
*           LENGTH(COREBLK);                                       0795
         LCR   @15,@15                                             0795
         AL    @15,COREAVIL                                        0795
         ST    @15,COREAVIL                                        0795
*     END;                                                         0796
*   IF CORELEN>COREAVIL THEN        /* ENOUGH CORE FOR REQUEST?      */
