*   SWCOMB=OFFX;                    /* SET COMBINATION SW OFF        */
*   SWREEP=OFFX;                                                   0599
         NI    SWCOMB,B'00111111'                                  0599
*   GPFLDPTR=CATWKPTR;              /* SET OUTPUT AREA PTR           */
         L     GPFLDPTR,CATWKPTR                                   0600
*   GPFIELD(1:100)=GPFIELD(1:100)&& /* CLEAR FRONT OF AREA           */
*       GPFIELD(1:100);             /* END USED ONLY ON LAST CALL    */
         XC    GPFIELD(100,GPFLDPTR),GPFIELD(GPFLDPTR)             0601
*   DICTWK(1:2)=FLNAME(1:2);        /* FIRST HALVE OF CONDENSED NM   */
         MVC   DICTWK(2),FLNAME                                    0602
*   DICTWK(3:4)=FLNAME(5:6);        /* SECOND HALVE OF COND NAME     */
         MVC   DICTWK+2(2),FLNAME+4                                0603
*   DO DICTPTR=DICTSTR TO DICTEND BY 8/* SEARCH DICTIONARY FOR       */
*         WHILE(DICTWK^=DICTNAME);  /* REQUESTED ENTRY (NULL DO LOOP)*/
         L     DICTPTR,DICTSTR                                     0604
         B     @DE00604                                            0604
@DL00604 CLC   DICTWK(4),DICTNAME(DICTPTR)                         0604
         BE    @DC00604                                            0604
*   END;                                                           0605
         AL    DICTPTR,@CF00051                                    0605
@DE00604 C     DICTPTR,DICTEND                                     0605
         BNH   @DL00604                                            0605
@DC00604 DS    0H                                                  0606
*   IF DICTCOMB=ON THEN             /* COMBINATION FIELD             */
         TM    DICTCOMB(DICTPTR),B'00100000'                       0606
         BNO   @RF00606                                            0606
*     SWCOMB=ON;                                                   0607
         OI    SWCOMB,B'10000000'                                  0607
*   IF DICTGC=ZERO&DICTCOMB=OFFX THEN/* FIXED LENGTH FIELD           */
@RF00606 CLI   DICTGC(DICTPTR),0                                   0608
         BNE   @RF00608                                            0608
         TM    DICTCOMB(DICTPTR),B'00100000'                       0608
         BNZ   @RF00608                                            0608
*     CALL MOVFIXED;                /* MOVE A FIXED LENGTH FIELD     */
         BAL   @14,MOVFIXED                                        0609
