*GETGROUP:                                                         0638
*                                                                  0638
*   /*****************************************************************/
*   /*                                                               */
*   /* GET A VALUE FROM A GROUP OCCURENCE ENTRY IN THE CATLAOG       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0638
*   PROC;                                                          0638
GETGROUP STM   @14,@07,@SA00011                                    0638
         STM   @10,@12,@SA00011+40                                 0638
*   CALL FINDGOP;                   /* FIND THE PROPER GOP           */
         BAL   @14,FINDGOP                                         0639
*   IF(CCACPE5<CCACPE3)&            /* DID WE FIND A VALUE?          */
*       (CATGODEL=OFFX) THEN        /* THAT WASN'T DELETED?          */
         C     CCACPE5,CCACPE3(,CPE0)                              0640
         BNL   @RF00640                                            0640
         TM    CATGODEL(CCACPE5),B'01000000'                       0640
         BNZ   @RF00640                                            0640
*     DO;                           /* YES CONTINUE                  */
*       IF SWREEP=ON THEN           /* RELATIVE REP NBR NEEDED?      */
         TM    SWREEP,B'01000000'                                  0642
         BNO   @RF00642                                            0642
*         DO;                       /* YES DO IT                     */
*           RELREPNO=GRPSEQNO;      /* SET RELATIVE REPETITION NBR   */
         SLR   @10,@10                                             0644
         IC    @10,GRPSEQNO                                        0644
         STH   @10,RELREPNO(,GPFLDPTR)                             0644
*           GPFLDPTR=GPFLDPTR+TWO;  /* STEP OUTPUT POINTER           */
         AL    GPFLDPTR,@CF00037                                   0645
*           SWREEP=OFFX;            /* TURN SW OFF                   */
         NI    SWREEP,B'10111111'                                  0646
*         END;                                                     0647
*       IF DICTREP=ON THEN          /* IS REPEATING FIELD?           */
@RF00642 TM    DICTREP(DICTPTR),B'01000000'                        0648
         BNO   @RF00648                                            0648
*         SWREPT=ON;                /* SET REPEAT SW                 */
         OI    SWREPT,B'00100000'                                  0649
*       IF CATGOEXT=ON THEN         /* IS ENTRY IN ANOTHER RECORD    */
@RF00648 TM    CATGOEXT(CCACPE5),B'10000000'                       0650
         BNO   @RF00650                                            0650
*         DO;                       /* YES, GO GET IT                */
*           IF CATGOCI>EIGHT THEN   /* VALID RECORD NBR?             */
         CLC   CATGOCI(3,CCACPE5),@CF00051+1                       0652
         BNH   @RF00652                                            0652
*             DO;                   /* NO                            */
*               NVTFLWSC=NVTWCN10;  /* SET ERROR CODE                */
         L     @10,NVTPTR                                          0654
         MVI   NVTFLWSC(@10),X'5F'                                 0654
*               CALL SWAIT;         /* GO TO SYSTEM WAIT ROUTINE     */
         BAL   @14,SWAIT                                           0655
*             END;                                                 0656
*           CPE0=ADDR(CCACPE(CATGOCI+ONE));/* RESET RECORD BASE      */
@RF00652 SLR   CPE0,CPE0                                           0657
         ICM   CPE0,7,CATGOCI(CCACPE5)                             0657
         SLA   CPE0,4                                              0657
         LA    CPE0,CCACPE(CPE0)                                   0657
*           CALL FINDGOP;           /* FIND THE PROPER GOP           */
         BAL   @14,FINDGOP                                         0658
*           IF CCACPE5^<CCACPE3    /* MUST BE THERE                 */
*               CATGOEXT=ON        /* MUST NOT BE EXTERNAL          */
*               CATGODEL=ON THEN    /* RECD'S MUST AGREE             */
         C     CCACPE5,CCACPE3(,CPE0)                              0659
         BNL   @RT00659                                            0659
         TM    CATGOEXT(CCACPE5),B'11000000'                       0659
