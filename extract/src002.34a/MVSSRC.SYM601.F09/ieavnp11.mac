         ALR   @10,@03                                             0807
         STCM  @14,7,CORELNTH-8(@10)                               0807
*   COREAVIL=COREAVIL-CORELEN;      /* AVAILABLE CORE SIZE           */
         LCR   @14,@14                                             0808
         AL    @14,COREAVIL                                        0808
         ST    @14,COREAVIL                                        0808
