         TITLE 'IEAVNP11-NIP MASTER CATLG OPEN                         *
                        '
IEAVNP11 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVNP11  75.330'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @11,0                                               0001
@PSTART  LA    @12,4095(,@11)                                      0001
         USING @PSTART,@11                                         0001
         USING @PSTART+4095,@12                                    0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
*
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
* WILL ASSEMBLE PROPERLY.
         STM R2,R3,LOCPTR
*   SW='00000000'B;                 /* INITIALIZE SWITCHES           */
         MVI   SW,B'00000000'                                      0174
*   COREPTR1=ZERO;                                                 0175
         SLR   @10,@10                                             0175
         ST    @10,COREPTR1                                        0175
*   WKLEN=LENGTH(CATLGBUF)*TEN+     /* GET CORE FOR THE CATALOG      */
*       LENGTH(AMBWKBUF);           /* RECORDS AND AMBWORK AREAS     */
         MVC   WKLEN(2),@CH02492                                   0176
*   CALL GETWORK;                   /* GET WORKING STORAGE           */
         BAL   @14,GETWORK                                         0177
*   DATAWAPT=WKCORE;                /* SET PTR TO DATA AMBWORK       */
         L     @10,WKCORE                                          0178
         ST    @10,DATAWAPT                                        0178
*   INDXWAPT=DATAWAPT+              /* SET PTR TO INDEX AMBWORK      */
*       LENGTH(AMBWKBUF)/TWO;                                      0179
         LA    @07,1536                                            0179
         LR    @15,@10                                             0179
         ALR   @15,@07                                             0179
         ST    @15,INDXWAPT                                        0179
*   CATBFPTR=WKCORE+LENGTH(AMBWKBUF);                              0180
         LA    @14,3072                                            0180
         ALR   @14,@10                                             0180
         ST    @14,CATBFPTR                                        0180
*   CWAPT=DATAWAPT;                 /* ADDRESSABILITY                */
         LR    CWAPT,@10                                           0181
*   AMALEN=LENGTH(AMBWKBUF)/TWO;    /* SET LENGTH FOR FREEMAIN ECT   */
         STH   @07,AMALEN(,CWAPT)                                  0182
*   CWAPT=INDXWAPT;                 /* ADDRESSABILITY                */
         LR    CWAPT,@15                                           0183
*   AMALEN=LENGTH(AMBWKBUF)/TWO;    /* SET LENGTH FOR FREEMAIN ECT   */
         STH   @07,AMALEN(,CWAPT)                                  0184
*   DICTSTR=ADDR(ZDT);              /* INITIALIZE DICTIONARY STR ADDR*/
         LA    @10,ZDT                                             0185
         ST    @10,DICTSTR                                         0185
*   DICTEND=ADDR(ZCS);              /* INITIALIZE DICTIONARY END ADDR*/
         LA    @10,ZCS                                             0186
         ST    @10,DICTEND                                         0186
*   DICTTYP=ADDR(ZTYP);             /* I ADDR OF ITYPEXT DICT FLD    */
         LA    @10,ZTYP                                            0187
         ST    @10,DICTTYP                                         0187
*   DICTTYP=DICTTYP->DICTST;        /* DISPL TO ITYPEXT FLD          */
         SLR   @15,@15                                             0188
         IC    @15,DICTST(,@10)                                    0188
         ST    @15,DICTTYP                                         0188
*   CALL RDDIR;                     /* READ SYS1.NUCLEUS DIRECTORY   */
         BAL   @14,RDDIR                                           0189
*   CALL RDMEMBR;                   /* READ 1 RECORD FROM THE MEMBER
*                                      SYSCATLG                      */
         BAL   @14,RDMEMBR                                         0190
*   CALL VOLMNT;                    /* SEE TO IT THAT THE CATALOG  0191
*                                      VOLUME IS MOUNTED             */
         BAL   @14,VOLMNT                                          0191
*   CALL BLDBLOCK;                  /* CREAT DEB, DCB, IOB, & ECB TO
*                                      READ USING EXCP               */
         BAL   @14,BLDBLOCK                                        0192
*   CALL RDDSCB;                    /* READ F1 & F4 DSCB TO GET    0193
*                                      ADDRESS OF MASTER CATALOG     */
         BAL   @14,RDDSCB                                          0193
*   CALL RDCATLG;                   /* READ THE 10 CATALOG SELF    0194
*                                      DEFINING RECORDS              */
         BAL   @14,RDCATLG                                         0194
*   CALL LDAMBWRK;                  /* THANSFER DATA FROM THE CATLG
*                                      RECORDS TH THE AMBWORKS       */
         BAL   @14,LDAMBWRK                                        0195
*   CALL BLDACB;                    /* BUILD AN ACB FOR THE CATLG    */
         BAL   @14,BLDACB                                          0196
*   CALL BLDCAXWA;                  /* BUILD A CAXWA FOR THE CATLG   */
         BAL   @14,BLDCAXWA                                        0197
*   CALL BLDAMCBS;                  /* BUILD AN AMCBS FOR THE CATLG  */
         BAL   @14,BLDAMCBS                                        0198
*   CALL CALLOPEN;                  /* CALL NIP-VSAM-OPEN            */
         BAL   @14,CALLOPEN                                        0199
*   ACBDDNM(1:4)=COREPTR1;          /* SAVE CORE MAPPING BLK PTR     */
         L     @10,ACBPTR                                          0200
         MVC   ACBDDNM(4,@10),COREPTR1                             0200
*   FREELEN=LENGTH(CATLGBUF)*TEN+   /* FREE CATALOG IO BUFFER        */
*       LENGTH(AMBWKBUF);           /* AND AMBWORK AREA BUFFER       */
         MVC   FREELEN(2),@CH02492                                 0201
*   FREEAD=DATAWAPT;                                               0202
         L     @10,DATAWAPT                                        0202
         ST    @10,FREEAD                                          0202
*   CALL FREEWORK;                  /* GET RID OF THE REMAINING      */
         BAL   @14,FREEWORK                                        0203
*   CALL FREEMAIN;                  /* CORE HELD FOR CONTROL BLOCKS  */
*                                                                  0204
         BAL   @14,FREEMAIN                                        0204
*   /*****************************************************************/
*   /*                                                               */
*   /* E X I T                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0205
*CALLOPEN:                                                         0205
*                                                                  0205
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL THE NIP-VSAM-OPEN MODULE                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0205
*   PROC;                                                          0205
@EL00001 L     @13,4(,@13)                                         0205
@EF00001 DS    0H                                                  0205
@ER00001 LM    @14,@12,12(@13)                                     0205
         BR    @14                                                 0205
CALLOPEN STM   @14,@12,@SA00002                                    0205
*   PARMLIST(1)=ADDR(IFGACB);       /* SET PARAMET LIST              */
         L     @10,ACBPTR                                          0206
         ST    @10,PARMLIST                                        0206
*   PARMLIST(2)=DATAWAPT;           /* DATA WORK AREA                */
         L     @10,DATAWAPT                                        0207
         ST    @10,PARMLIST+4                                      0207
*   PARMLIST(THREE)=INDXWAPT;       /* INDEX WORK AREA               */
         L     @10,INDXWAPT                                        0208
         ST    @10,PARMLIST+8                                      0208
*   RFY                                                            0209
*    (R0,                                                          0209
*     R1,                                                          0209
*     R15) RSTD;                                                   0209
*   FF=ONE;                         /* NBR OF FIELDS FOR BLDL        */
         MVC   FF(2),@CH00079                                      0210
*   LL=H58;                         /* WORK AREA LENGTH FOR BLDL     */
         MVC   LL(2),@CH02332                                      0211
*   BLDLNAME='IEAVNP1A';            /* FIND NIP VSAM OPEN            */
         MVC   BLDLNAME(8),@CC02388                                0212
*   R1=NVTDCBSN;                                                   0213
         L     @14,NVTPTR                                          0213
         L     R1,NVTDCBSN(,@14)                                   0213
*   GEN REFS(PARMLIST) SETS(R14,R15,TEMPSR);                       0214
        STM   R2,R3,TEMPSR           SAVE REGS
        LM    R2,R3,LOCPTR           BASE REGS FOR NVT/CVT
        BLDL  (R1),BLDLTBL
        LM    R2,R3,LOCPTR           BASE REGS FOR NVT/CVT
        L     R1,NVTDCBSN(,R2)
        LOAD  DE=BLDLNAME,DCB=(R1)   LOAD NIP VSAM OPEN
        LA    R1,PARMLIST            PTR TO PARMLIST
        LTR    R15,R0                TEST RETURN CODE
        BZ    NOLOAD                 LOAD OK, IF NO BRANCH
        BALR  R14,R15                GO TO NIP VSAM OPEN
        DELETE DE=BLDLNAME           DELETE NIP VSAM OPEN
NOLOAD  LM    R2,R3,TEMPSR           BR HERE IF LOAD FAILED
*   IF R15^=ZERO THEN               /* IF OPEN FAILED                */
         LTR   R15,R15                                             0215
         BZ    @RF00215                                            0215
*     DO;                                                          0216
*       NVTFLWSC=NVTWCN11;          /* QUIT                          */
         L     @14,NVTPTR                                          0217
         MVI   NVTFLWSC(@14),X'64'                                 0217
*       CALL SWAIT;                 /* GO TO SYSTEM WAIT ROUTINE     */
         BAL   @14,SWAIT                                           0218
*     END;                                                         0219
*   CORELAST=R1;                    /* SAVE PTR TO CORE MAP BLOCK    */
@RF00215 L     @14,COREPTRN                                        0220
         ST    R1,CORELAST(,@14)                                   0220
*   RFY                                                            0221
*    (R0,                                                          0221
*     R1,                                                          0221
*     R15) UNRSTD;                                                 0221
*   END CALLOPEN;                                                  0222
@EL00002 DS    0H                                                  0222
@EF00002 DS    0H                                                  0222
@ER00002 LM    @14,@12,@SA00002                                    0222
         BR    @14                                                 0222
*RDDIR:                                                            0223
*                                                                  0223
*   /*****************************************************************/
*   /*                                                               */
*   /* READ THE DIRECTORY OF SYS1.NUCLEUS & CONVERT TTR TO MMBBCCHHR */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0223
*   PROC;                                                          0223
RDDIR    STM   @14,@12,@SA00003                                    0223
*   FF=ONE;                         /* NBR OF ITEMS IN BLDLLIST      */
         MVC   FF(2),@CH00079                                      0224
*   LL=LENGTH(BLDLTBL)-FOUR;                                       0225
         MVC   LL(2),@CH02494                                      0225
*   BLDLNAME='SYSCATLG';            /* NAME TO SEARCH ON             */
         MVC   BLDLNAME(8),@CC02393                                0226
*   RFY                                                            0227
*    (R0,                                                          0227
*     R1,                                                          0227
*     R15) RSTD;                                                   0227
*   R1=NVTDCBSN;                    /* ADDR OF DCB FOR SYS1.NUCLEUS  */
         L     @14,NVTPTR                                          0228
         L     R1,NVTDCBSN(,@14)                                   0228
*   GEN REFS(R1) SETS(R0,R1,R15,TTRK);                             0229
        STM   R2,R3,TEMPSR
        LM    R2,R3,LOCPTR
        BLDL  (R1),BLDLTBL
        LM    R2,R3,TEMPSR
*   IF R15^=ZERO THEN               /* SUCESSFUL?                    */
         LTR   R15,R15                                             0230
         BZ    @RF00230                                            0230
*     DO;                           /* NO, DO                        */
*       NVTFLWSC=NVTWCN07;          /* SET WAIT CODE                 */
         L     @14,NVTPTR                                          0232
         MVI   NVTFLWSC(@14),X'5C'                                 0232
*       CALL SWAIT;                 /* GO TO SYSTEM WAIT ROUTINE     */
         BAL   @14,SWAIT                                           0233
*     END;                                                         0234
*   RFY                                                            0235
*     IHADCB BASED(NVTDCBSN);                                      0235
@RF00230 DS    0H                                                  0236
*   R1=DCBDEBAD;                    /* ADDR OF DEB OF SYS1.NUCLEUS TO
*                                      R1                            */
         L     @14,NVTPTR                                          0236
         L     @14,NVTDCBSN(,@14)                                  0236
         L     R1,DCBDEBAD+40(,@14)                                0236
*   R0=TTRK;                        /* TTR TO BE CONVERTED           */
         L     R0,TTRK                                             0237
*   R15=CVTPCNVT;                   /* PT TO CONVERT ROUTINE         */
         L     @10,CVTPTR                                          0238
         L     R15,CVTPCNVT(,@10)                                  0238
*   R14=ADDR(DRWSEEK);              /* ADDRESS OF OUTPUT AREA        */
         LA    R14,DRWAAREA                                        0239
         LA    R14,DRWSEEK(,R14)                                   0239
*   GEN SETS(DRWSEEK);                                             0240
        STM   R0,R15,TEMPSR            SAVE REGS
        LA    R8,TEMPSR
        LR    R2,R14                         POINTER TO OUTPUT AREA
        BALR  R14,R15                  GOTO TTR CONVERT RTN
        LM    R0,R15,0(R8)             RECOVER REGS
*   RFY                                                            0241
*    (R0,                                                          0241
*     R1,                                                          0241
*     R15) UNRSTD;                                                 0241
*   END RDDIR;                                                     0242
@EL00003 DS    0H                                                  0242
@EF00003 DS    0H                                                  0242
@ER00003 LM    @14,@12,@SA00003                                    0242
         BR    @14                                                 0242
*RDMEMBR:                                                          0243
*                                                                  0243
*   /*****************************************************************/
*   /*                                                               */
*   /* READ THE MEMBER SYSCATLG FROM SYS1.NUCLEUS                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0243
*   PROC;                                                          0243
*                                                                  0243
RDMEMBR  STM   @14,@12,@SA00004                                    0243
*/* BUILD DEB                                                        */
*                                                                  0244
*   DRWDEXT='01'X;                  /* NUMBER OF EXTENTS = 1         */
         LA    @10,DRWAAREA                                        0244
         MVI   DRWDEXT(@10),X'01'                                  0244
*   DRWDDCB=ADDR(DRWDCB);           /* DCB PTR                       */
         LA    @15,DRWDCB(,@10)                                    0245
         STCM  @15,7,DRWDDCB(@10)                                  0245
*   DRWDID='0F'X;                   /* SET DEB ID                    */
         MVI   DRWDID(@10),X'0F'                                   0246
*   DRWDAPP=CVTXAPG;                /* IOS APP.TAB ADDR              */
         L     @01,CVTPTR                                          0247
         MVC   DRWDAPP(3,@10),CVTXAPG+1(@01)                       0247
*   DRWDEXS='04'X;                  /* EXT SCALE                     */
         MVI   DRWDEXS(@10),X'04'                                  0248
*   DRWDMOD='00'X;                  /* SET FILE MASK                 */
         MVI   DRWDMOD(@10),X'00'                                  0249
*   DRWECC='FFFFFFFF'X;             /* SET TO END OF PACK            */
         MVC   DRWECC(4,@10),@CB02401                              0250
*   DRWNTK='7FFF'X;                 /* NBR OF TRKS                   */
         MVC   DRWNTK(2,@10),@CB02402                              0251
*   DRWDEBP=ADDR(DRWDEB);           /* DEBDEB PTR                    */
         ST    @10,DRWDEBP(,@10)                                   0252
*   DRWDUCB=DCBDEBA->DRWDUCB;       /* UCB POINTER IN DEB            */
         L     @01,NVTPTR                                          0253
         L     @01,NVTDCBSN(,@01)                                  0253
         L     @01,DCBDEBA+39(,@01)                                0253
         LA    @01,0(,@01)                                         0253
         MVC   DRWDUCB(3,@10),DRWDUCB(@01)                         0253
*/* BUILD AN IOB                                                     */
*                                                                  0254
*   DRWIECB=ADDR(DRWECB);           /* ECB PTR INTO IOB              */
         LA    @15,DRWECB(,@10)                                    0254
         ST    @15,DRWIECB(,@10)                                   0254
*   DRWSCP=ADDR(DRWCCW1);           /* START OF CCW'S                */
         LA    @15,DRWCCW1(,@10)                                   0255
         ST    @15,DRWSCP(,@10)                                    0255
*   DRWIDCB=ADDR(DRWDCB);           /* DCB PTR INTO IOB              */
         LA    @14,DRWDCB(,@10)                                    0256
         ST    @14,DRWIDCB(,@10)                                   0256
*   DRWIOB(1:2)='0200'X;            /* SET IOB UNRELATED FLAG CLEAR
*                                      OTHER FLAGS                   */
         MVC   DRWIOB(2,@10),@CB02403                              0257
*   DRWECB=ZERO;                    /* CLEAR ECB                     */
         SLR   @14,@14                                             0258
         ST    @14,DRWECB(,@10)                                    0258
*/* BUILD CHANNEL PROGRAM TO READ MEMBER                             */
*                                                                  0259
*   RFY                                                            0259
*     R1 RSTD;                                                     0259
*   RFY                                                            0260
*     WCCW BASED(R1);                                              0260
*   R1=ADDR(DRWCCW1);               /* PT TO CHAN PGM AREA           */
         LR    R1,@15                                              0261
*   CCWCMD='31'X;                   /* SEARCH ID EQUAL COMMAND       */
         MVI   CCWCMD(R1),X'31'                                    0262
*   CCWDTA=ADDR(DRWSEEK)+THREE;     /* ARGUMENT (STARTING PLACE      */
         LA    @00,DRWSEEK(,@10)                                   0263
         AL    @00,@CF00295                                        0263
         STCM  @00,7,CCWDTA(R1)                                    0263
*   CCWFLG='40'X;                   /* COMMAND CHAINING              */
         MVI   CCWFLG(R1),X'40'                                    0264
*   CCWBTE=FIVE;                    /* BYTE COUNT                    */
         MVC   CCWBTE(2,R1),@CH00936                               0265
*   R1=R1+EIGHT;                    /* NEXT CCW                      */
         LA    @14,8                                               0266
         ALR   R1,@14                                              0266
*   CCWCMD='08'X;                   /* TIC                           */
         MVI   CCWCMD(R1),X'08'                                    0267
*   CCWDTA=ADDR(DRWCCW1);           /* TIC BACK TO CCW1              */
         STCM  @15,7,CCWDTA(R1)                                    0268
*   R1=R1+EIGHT;                                                   0269
         ALR   R1,@14                                              0269
*   CCWCMD='06'X;                   /* READ DATA                     */
         MVI   CCWCMD(R1),X'06'                                    0270
*   CCWDTA=ADDR(DRWDSCB);           /* BUFFER AREA ADDR              */
         LA    @00,DRWDSCB(,@10)                                   0271
         STCM  @00,7,CCWDTA(R1)                                    0271
*   CCWBTE=H80;                     /* BYTE COUNT                    */
         MVC   CCWBTE(2,R1),@CH02334                               0272
*   R1=ADDR(DRWIOB);                /* POINT TO IOB                  */
         LA    R1,DRWIOB(,@10)                                     0273
*   GEN REFS(DRWIOB,R1) SETS(SYSCATLG);                            0274
        EXCP (1)                          EXECUTE CHANNEL PROGTAM
*   R1=ADDR(DRWECB);                                               0275
         LA    R1,DRWAAREA                                         0275
         LA    R1,DRWECB(,R1)                                      0275
*   GEN SETS(DRWECB);                                              0276
        WAIT ECB=(1)                      WAIT FOR CHANNEL PROG
*   IF DRWECMP^='7F'X THEN          /* CHECK FOR ERROR               */
         LA    @10,DRWAAREA                                        0277
         CLI   DRWECMP(@10),127                                    0277
         BE    @RF00277                                            0277
*     DO;                           /* QUIT                          */
*       R1=ADDR(DRWIOB);            /* POINT TO IOB                  */
         LA    R1,DRWIOB(,@10)                                     0279
*       GEN(LM 2,3,LOCPTR);         /* CVT AND NVT POINTERS          */
         LM 2,3,LOCPTR
*       GEN(IEAPMNIP TYPE=SENSE,PARAM=R1);/* TYPE SENSE DATE         */
         IEAPMNIP TYPE=SENSE,PARAM=R1
*       NVTFLWSC=NVTWCN07;          /* SET WAIT CODE                 */
         L     @10,NVTPTR                                          0282
         MVI   NVTFLWSC(@10),X'5C'                                 0282
*       CALL SWAIT;                 /* GO TO SYSTEM WAIT ROUTINE     */
         BAL   @14,SWAIT                                           0283
*     END;                                                         0284
*   RFY                                                            0285
*     R1 UNRSTD;                                                   0285
*   END RDMEMBR;                                                   0286
@EL00004 DS    0H                                                  0286
@EF00004 DS    0H                                                  0286
@ER00004 LM    @14,@12,@SA00004                                    0286
         BR    @14                                                 0286
*VOLMNT:                                                           0287
*                                                                  0287
*   /*****************************************************************/
*   /*                                                               */
*   /* VERIFY THAT THE VOLUME IS MOUNTED AND GET UCB POINTER         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0287
*   PROC;                                                          0287
VOLMNT   STM   @14,@05,@SA00005                                    0287
         STM   @07,@12,@SA00005+32                                 0287
*   CWAPT=DATAWAPT;                 /* ADDRESSABILTIY FOR AMBWORK    */
         L     CWAPT,DATAWAPT                                      0288
*   RFY                                                            0289
*     NIPMNTPL BASED(ADDR(MNTPARM));/* ADDRESSING FOR PARM LIST      */
*   NMNTVS=NCVOLSER;                /* MOVE VOLUME SERIAL NBR TO PARM*/
         LA    @10,MNTPARM                                         0290
         LA    @07,DRWAAREA                                        0290
         MVC   NMNTVS(6,@10),NCVOLSER(@07)                         0290
*   NMNTDSA=ADDR(NCDSNAME);                                        0291
         LA    @07,NCDSNAME(,@07)                                  0291
         STCM  @07,15,NMNTDSA(@10)                                 0291
*   RFY                                                            0292
*     R1 RSTD;                                                     0292
*   GEN REFS(NVTMOUNT) SETS(R0,R1,R14,R15);                        0293
         STM   R2,R3,TEMPSR
        LM    R2,R3,LOCPTR
        IEAPMNIP TYPE=MOUNT,PARAM=MNTPARM,OPT=(,,LI)
        B     PSTPRM                    BR AROUND PARM LIST
MNTPARM  IEAPMNIP TYPE=MOUNTPL,PARAM=CTDATANM,OPT=PTR
PSTPRM   LM    R2,R3,TEMPSR
*   CTVOLUCB=R1;                    /* SAVE UCB ADDR                 */
         LA    @10,232                                             0294
         LR    @07,CWAPT                                           0294
         ALR   @07,@10                                             0294
         ST    R1,CTVOLUCB(,@07)                                   0294
*   CWAPT=INDXWAPT;                 /* INDEX AMBWORK TOO             */
         L     CWAPT,INDXWAPT                                      0295
*   CTVOLUCB=R1;                                                   0296
         ALR   @10,CWAPT                                           0296
         ST    R1,CTVOLUCB(,@10)                                   0296
*   RFY                                                            0297
*     R1 UNRSTD;                                                   0297
*   END VOLMNT;                                                    0298
@EL00005 DS    0H                                                  0298
@EF00005 DS    0H                                                  0298
@ER00005 LM    @14,@05,@SA00005                                    0298
         LM    @07,@12,@SA00005+32                                 0298
         BR    @14                                                 0298
*BLDBLOCK:                                                         0299
*                                                                  0299
*   /*****************************************************************/
*   /*                                                               */
*   /* CREATE A DCB,DEB,IOB,AND ECB FOR EXCP TO ALLOW ACCESS TO ALL  */
*   /* OF THE CATLG VOLUME                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0299
*   PROC;                                                          0299
BLDBLOCK STM   @14,@12,@SA00006                                    0299
*   RFY                                                            0300
*    (R0,                                                          0300
*     R1,                                                          0300
*     R14,                                                         0300
*     R15) RSTD;                                                   0300
*   R0=LENGTH(IGGMDRWA);            /* LENGTH TO CLEAR               */
         LA    R0,716                                              0301
*   GEN SETS(DRWAAREA);                                            0302
*                                                                  0302
*                                     /* CLEAR BUILD AREA*/
        STM   R2,R5,TEMPSR             SAVE REGS
        SR    R3,R3
        LA    R4,DRWAAREA
        LR    R5,R0
        MVCL  R4,R2
        LM    R2,R5,TEMPSR
*/* BUILD DEB                                                        */
*                                                                  0303
*   DRWDEXT='01'X;                  /* NUMBER OF EXTENTS = 1         */
         LA    @10,DRWAAREA                                        0303
         MVI   DRWDEXT(@10),X'01'                                  0303
*   DRWDDCB=ADDR(DRWDCB);           /* DCB PTR                       */
         LA    @07,DRWDCB(,@10)                                    0304
         STCM  @07,7,DRWDDCB(@10)                                  0304
*   DRWDID='0F'X;                   /* SET DEB ID                    */
         MVI   DRWDID(@10),X'0F'                                   0305
*   DRWDAPP=CVTXAPG;                /* IOS APP.TAB ADDR              */
         L     @07,CVTPTR                                          0306
         MVC   DRWDAPP(3,@10),CVTXAPG+1(@07)                       0306
*   DRWDEXS='04'X;                  /* EXT SCALE                     */
