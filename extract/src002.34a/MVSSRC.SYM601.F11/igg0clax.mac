*                     END;          /* END UPDATE FIELD              */
*                 END;              /* END PROCESS FIELD             */
@RC00130 DS    0H                                                  0140
*         END;                      /* END MODIFY FIELD INFO         */
@RC00124 DS    0H                                                  0140
@RC00122 DS    0H                                                  0140
@RT00120 DS    0H                                                  0140
@DE00118 TM    CCAGVNC(R11),B'00100000'                            0140
         BNO   @DC00118                                            0140
         CLI   CCACD1(R11),0                                       0140
         BE    @DL00118                                            0140
@DC00118 DS    0H                                                  0141
*       END;                        /* END PROCESS FIELD             */
*     IF CTGFLDNO>1 THEN            /* SEE IF MORE ONE FIELD PTR     */
@RC00112 L     @10,CCAFLPT(,R11)                                   0142
         CLI   CTGFLDNO(@10),1                                     0142
         BNH   @RF00142                                            0142
*       CCAIDPT2=CTGFLPT(1+CCACNIX);/* SET DATA PTR                  */
         LH    @05,CCACNIX(,R11)                                   0143
         SLA   @05,3                                               0143
         L     @10,CTGFLPT(@05,@10)                                0143
         ST    @10,CCAIDPT2(,R11)                                  0143
