         SLR   @10,R4                                              0310
         ST    @10,@TF00001                                        0310
         ALR   @10,R3                                              0310
         LR    @05,R2                                              0310
         SL    @05,@TF00001                                        0310
         BCTR  @05,0                                               0310
         LR    @15,R3                                              0310
         SLR   @15,R4                                              0310
         AL    @15,@TF00001                                        0310
         EX    @05,@SM02892                                        0310
*     R2=R2-R4;                     /* DECR SEGMENT MOVE             */
         SLR   R2,R4                                               0311
*   END;                            /* END SHIFT                     */
@DE00309 CR    R2,R4                                               0312
         BNH   @DC00309                                            0312
         LTR   R4,R4                                               0312
         BNZ   @DL00309                                            0312
@DC00309 DS    0H                                                  0313
*   R2=CCACPE7-CCACPE3;             /* DISPLACEMENT TO VAR LEN FIELD */
         L     @10,CCAURAB(,R11)                                   0313
         L     R2,CCACPE7(,R11)                                    0313
         SL    R2,CCACPE3(,@10)                                    0313
*   RFY                                                            0314
*     CATGOPT BASED(R3);            /* GOP MAP                       */
*   R3=ADDR(REPGOPT);               /* POINT TO FIRST GOP            */
         L     R3,CCACPE2(,@10)                                    0315
         LA    R3,REPGOPT(,R3)                                     0315
*   DO CCAGVCT=1 TO REPCNT;         /* UPDATE DISPLACEMENTS          */
         LA    @10,1                                               0316
         B     @DE00316                                            0316
@DL00316 DS    0H                                                  0317
*     IF CATGOEXT=OFF&CATGODEL=OFF& /* IF THIS IS NON-DELETED,       */
*         CATGODSP>R2 THEN          /* NON-EXT GOP FOR UPDATE        */
         TM    CATGOEXT(R3),B'11000000'                            0317
         BNZ   @RF00317                                            0317
