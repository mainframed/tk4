         B     @RC00142                                            0144
@RF00142 L     @10,CCAIDPT2(,R11)                                  0144
         AL    @10,CCAILNG(,R11)                                   0144
         ST    @10,CCAIDPT2(,R11)                                  0144
*   END;                            /* END PROCESS FIELD LIST        */
@RC00142 DS    0H                                                  0145
@DE00099 TM    CCAELC2(R11),B'00000100'                            0145
         BNZ   @DC00099                                            0145
         CLI   CCACD1(R11),0                                       0145
         BE    @DL00099                                            0145
@DC00099 DS    0H                                                  0146
*   CALL EXIT;                      /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0146
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */
         DS 0F
