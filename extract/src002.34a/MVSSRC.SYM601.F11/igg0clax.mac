         B     @RC00124                                            0126
@RF00124 DS    0H                                                  0127
*                   IF DIC2REP=ON THEN/* IF REPEATING                */
         TM    DIC2REP+372(R11),B'01000000'                        0127
         BNO   @RF00127                                            0127
*                     CCANF=OFF;    /* FOUND                         */
         NI    CCANF(R11),B'11110111'                              0128
*                   ELSE                                           0129
*                     ;             /* NON-REPEATING                 */
@RF00127 DS    0H                                                  0130
