         B     @PB00003                                            0151
IGGPMVAR DS    0H                                                  0152
*Z007:                              /* LABEL FOR PROCEDURE TRACKING  */
*Z007MVAR:                          /* LABEL FOR PROCEDURE TRACKING  */
*   RFY                                                            0152
*    (R5,                                                          0152
*     R6,                                                          0152
*     R7,                                                          0152
*     R8,                                                          0152
*     R9,                                                          0152
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */
Z007     DS    0H                                                  0152
Z007MVAR DS    0H                                                  0153
*   RFY                                                            0153
*    (R2,                                                          0153
*     R3,                                                          0153
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */
         STM 12,14,12(13)
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA     0155
*                                      PROCEDURE                     */
         LA 13,12(13)
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**   MODULE NAME =  IGGPMVAR(IGG0CLAX)                                *
**                                                                    *
**   DESCRIPTIVE NAME =  MODIFY VARIABLE LENGTH FIELD                 *
**                                                                    *
**   FUNCTION =                                                       *
**      THIS SUB-FUNCTION MODIFIES A VARIABLE LENGTH FIELD.  IF NO    *
**      DISPLACEMENT IS SPECIFIED THE ENTIRE VARIABLE LENGTH FIELD IS *
**      REPLACED WITH USER SUPPLIED FIELD DATA. OTHERWISE, THE        *
**      VARIABLE LENGTH FIELD IS MODIFIED AT THE SPECIFIED DISPLACE-  *
**      MENT BY DELETING A CERTAIN PORTION AND INSERTING A NEW        *
**      PORTION.  THE PROCESSING IS AS FOLLOWS:                       *
**      1. SET UP FOR MODIFYING THE VARIABLE FIELD.                   *
**      2. IF THE MODIFICATION IS OCCURRING IN THE BASE RECORD OR IF  *
**         THE OCCURRENCE DOES NOT SPAN EXTENSION RECORDS, THEN:      *
**         A. IF THE MODIFICATION WILL REDUCE THE RECORD SIZE, CALL   *
**            IGGPSHNK. OR IF THE EXPANSION CAN BE CONTAINED WITHIN   *
**            THE CURRENT RECORD CALL IGGPEXPD TO COMPLETE THE MODIFY *
**            OPERATION.                                              *
**         B. IF THE MODIFICATION IS TO OCCUR WITHIN A BASE RECORD,   *
**            CALL IGGPMBGO TO MOVE THE AFFECTED OCCURRENCE INTO AN   *
**            EXTENSION RECORD AND CALL IGGPGVAL TO RE-ESTABLISH THE  *
**            OCCURRENCE'S LOCATION.  IF THE EXPANSION CAN NOW BE     *
**            DONE, CALL IGGPEXPD TO PERFORM THE MODIFICATION.        *
**         C. USE SUB-FUNCTIONS IGGPAXCI,IGGPGNEX,IGGPGREC,IGGPGVAL,  *
**            IGGPMGO, AND IGGPEXPD TO MOVE OCCURRENCES OUT OF THE    *
**            CURRENT EXTENSION RECORD UNTIL ONLY ONE REMAINS OR THE  *
**            MODIFY-EXPANSION HAS BEEN DONE.                         *
**      3. IF THE MODIFY OPERATION MUST STILL BE PERFORMED, CALL      *
**         IGGPDEIN.  THIS CALL WILL OCCUR WHEN THE GROUP OCCURRENCE  *
**         INVOLVED SPANS MORE THAN ONE EXTENSION RECORD.             *
**                                                                    *
**   INPUT =                                                          *
**      SEE IGGPMOD INPUT DESCRIBED IN MODULE IGG0CLAV.               *
**                                                                    *
**   OUTPUT =                                                         *
**      SEE IGGPMOD OUTPUT DESCRIBED IN MODULE IGG0CLAV.              *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0156
*   RFY                                                            0156
*     VARFIELD BASED(CCAIDPT2);     /* MAP USER INFO                 */
*   IF CTGFLDNO>1 THEN              /* MORE THAN ONE FIELD DATA      */
         L     @10,CCAFLPT(,R11)                                   0157
         CLI   CTGFLDNO(@10),1                                     0157
         BNH   @RF00157                                            0157
*     CCAILNG2=CTGFLNG(CCACNIX)-LENGTH(VARLEN);/* GET INSERT LEN     */
         LH    @05,CCACNIX(,R11)                                   0158
         SLA   @05,3                                               0158
         L     @10,CTGFLNG-8(@05,@10)                              0158
         BCTR  @10,0                                               0158
         BCTR  @10,0                                               0158
         ST    @10,CCAILNG2(,R11)                                  0158
*   ELSE                            /* ONE PAIR DATA/LENGTH          */
*     CCAILNG2=VARLEN;              /* GET INSERT LENGTH             */
         B     @RC00157                                            0159
@RF00157 L     @10,CCAIDPT2(,R11)                                  0159
         ICM   @10,12,VARLEN(@10)                                  0159
         SRA   @10,16                                              0159
         ST    @10,CCAILNG2(,R11)                                  0159
*   RFY                                                            0160
*     VARFIELD BASED(CCACPE7);      /* ORGINAL BASE                  */
@RC00157 DS    0H                                                  0161
*   IF CTGVLDSP^=0&CCAMOD=ON THEN   /* IF MODIFY OF VARIABLE LENGTH
*                                      FIELD                         */
         L     @10,CCAFLPT(,R11)                                   0161
         LH    @05,CTGVLDSP+2(,@10)                                0161
         LTR   @05,@05                                             0161
         BZ    @RF00161                                            0161
         TM    CCAMOD(R11),B'00000001'                             0161
         BNO   @RF00161                                            0161
*     DO;                           /* SET INSERT/DELETE INFO        */
*       CCADISP=CTGVLDSP;           /* DISPLACEMENT                  */
         STH   @05,CCADISP(,R11)                                   0163
*       CCADLEN=CTGFLNG(1);         /* DELETE LENGTH                 */
         L     @10,CTGFLNG(,@10)                                   0164
         STH   @10,CCADLEN(,R11)                                   0164
*       CCADIFF=CCAILNG2-CCADLEN;   /* DIFFERENCE                    */
         LCR   @10,@10                                             0165
         AL    @10,CCAILNG2(,R11)                                  0165
         STH   @10,CCADIFF(,R11)                                   0165
*     END;                          /* END SET                       */
*   ELSE                            /* NOT MODIFY OF VAR FIELD       */
*     DO;                           /* SET UP FOR ALTER              */
         B     @RC00161                                            0167
@RF00161 DS    0H                                                  0168
*       CCADISP=LENGTH(VARLEN);     /* DISPLACEMENT                  */
         LA    @10,2                                               0168
         STH   @10,CCADISP(,R11)                                   0168
*       CCADLEN=CCAILNG-LENGTH(VARLEN);/* DELETE LENGTH              */
         L     @05,CCAILNG(,R11)                                   0169
         LR    @15,@05                                             0169
         SLR   @15,@10                                             0169
         STH   @15,CCADLEN(,R11)                                   0169
*       CCADIFF=CCAILNG2-CCAILNG+LENGTH(VARLEN);/* DIFFERENCE        */
         LCR   @05,@05                                             0170
         AL    @05,CCAILNG2(,R11)                                  0170
         ALR   @05,@10                                             0170
         STH   @05,CCADIFF(,R11)                                   0170
*     END;                          /* END SET UP                    */
*   CCAGVNC=ON;                     /* PROCESSING NOT COMPLETE       */
@RC00161 OI    CCAGVNC(R11),B'00100000'                            0172
*   IF CCAGVEXT=0×CCAURAB=CCARABB THEN/* IF OCCURRENCE TOTALLY IN ONE
*                                      RECORD                        */
         ICM   @10,7,CCAGVEXT(R11)                                 0173
         BZ    @RT00173                                            0173
         CLC   CCAURAB(4,R11),CCARABB(R11)                         0173
         BNE   @RF00173                                            0173
@RT00173 DS    0H                                                  0174
*     DO;                           /* PROCESS RECORD                */
*       IF CCADIFF<=0 THEN          /* IF THIS IS SHRINK OF VAR    0175
*                                      LENGTH FIELD                  */
         LH    @10,CCADIFF(,R11)                                   0175
         LTR   @10,@10                                             0175
         BP    @RF00175                                            0175
*         DO;                       /* SHRINK FIELD                  */
*Z008SHNK:                                                         0177
*           CALL IGGPSHNK;          /* SHRINK FIELD                  */
Z008SHNK BAL   @14,IGGPSHNK                                        0177
*           CCAGVNC=OFF;            /* PROCESSING COMPLETE           */
         NI    CCAGVNC(R11),B'11011111'                            0178
*         END;                      /* END SHRINK FIELD              */
*       ELSE                        /* EXPAND OF FIELD               */
*         IF RCDSIZ+CCADIFF<=MXRCD THEN/* IF VAR LEN FIELD WILL FIT  */
         B     @RC00175                                            0180
@RF00175 L     @10,CCAURAB(,R11)                                   0180
         L     @10,CCACPE1(,@10)                                   0180
         ICM   @10,12,RCDSIZ(@10)                                  0180
         SRA   @10,16                                              0180
         AH    @10,CCADIFF(,R11)                                   0180
         C     @10,@CF00055                                        0180
         BH    @RF00180                                            0180
*           DO;                     /* EXPAND FIELD                  */
*Z009EXPD:                                                         0182
*             CALL IGGPEXPD;        /* EXPAND FIELD                  */
Z009EXPD BAL   @14,IGGPEXPD                                        0182
*             CCAGVNC=OFF;          /* PROCESSING COMPLETE           */
         NI    CCAGVNC(R11),B'11011111'                            0183
*           END;                    /* END EXPAND                    */
*         ELSE                      /* VAR LEN FIELD NOT FIT         */
*           DO;                     /* MOVE OCCURRENCES              */
         B     @RC00180                                            0185
@RF00180 DS    0H                                                  0186
*             IF CCAURAB=CCARABB THEN/* IF BASE RCD AREA             */
         CLC   CCAURAB(4,R11),CCARABB(R11)                         0186
         BNE   @RF00186                                            0186
*               DO;                 /* MOVE BASE OCCURRENCES         */
*Z010MBGO:                                                         0188
*                 CALL IGGPMBGO;    /* MOVE BASE OCCURRENCES         */
Z010MBGO BAL   @14,IGGPMBGO                                        0188
*                 CCAGVEXT=0;       /* NO EXTENSION                  */
*                                                                  0189
         SLR   @10,@10                                             0189
         STCM  @10,7,CCAGVEXT(R11)                                 0189
*                 /***************************************************/
*                 /*                                                 */
*                 /* CALL TO MODULE IGG0CLBA                         */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0190
*Z011GVAL:                                                         0190
*                 CALL IGGPGVAL;    /* RE-EST DATA PTR               */
Z011GVAL L     @15,@CV00038                                        0190
         BALR  @14,@15                                             0190
*                 IF RCDSIZ+CCADIFF<=MXRCD&/* IF VAR LEN FIELD FIT 0191
*                                      NOW                           */
*                     CCACD1=RCS THEN/* AND NO PREVIOUS ERROR        */
         L     @10,CCAURAB(,R11)                                   0191
         L     @10,CCACPE1(,@10)                                   0191
         ICM   @10,12,RCDSIZ(@10)                                  0191
         SRA   @10,16                                              0191
         AH    @10,CCADIFF(,R11)                                   0191
         C     @10,@CF00055                                        0191
         BH    @RF00191                                            0191
         CLI   CCACD1(R11),0                                       0191
         BNE   @RF00191                                            0191
*                   DO;             /* EXPAND FIELD                  */
*Z012EXPD:                                                         0193
*                     CALL IGGPEXPD;/* EXPAND FIELD                  */
Z012EXPD BAL   @14,IGGPEXPD                                        0193
*                     CCAGVNC=OFF;  /* PROCESSING COMPLETE           */
         NI    CCAGVNC(R11),B'11011111'                            0194
*                   END;            /* END EXPAND                    */
*                 ELSE              /* INFO NOT FIT                  */
*                   CCAGVNC=ON;     /* STILL INCOMPLETE              */
         B     @RC00191                                            0196
@RF00191 OI    CCAGVNC(R11),B'00100000'                            0196
*               END;                /* END MOVE BASE                 */
*             ELSE                                                 0198
*               ;                   /* NOT BASE                      */
@RF00186 DS    0H                                                  0199
*             CCARBU1=ON;           /* DONT USE BUFFER               */
@RC00186 L     @10,CCAURAB(,R11)                                   0199
         OI    CCARBU1(@10),B'01000000'                            0199
*             IF CCAGVNC=OFF×CCACD1^=RCS×/* IF THIS PROCESSING       */
*                 REPCNT=1 THEN                                    0200
         TM    CCAGVNC(R11),B'00100000'                            0200
         BZ    @RT00200                                            0200
         CLI   CCACD1(R11),0                                       0200
         BNE   @RT00200                                            0200
         L     @10,CCACPE2(,@10)                                   0200
         CLI   REPCNT(@10),1                                       0200
         BE    @RT00200                                            0200
*               ;                   /* IS COMPLETE                   */
*             ELSE                  /* PROCESSING NOT COMPLETE       */
