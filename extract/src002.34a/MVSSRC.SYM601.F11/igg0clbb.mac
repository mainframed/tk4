         SLR   @05,@05                                             0538
         IC    @05,CCANDEXT(,R11)                                  0538
         MH    @05,@CH00155                                        0538
         STH   @05,EXTILEN+96(,@10)                                0538
*   DO R2=1 TO CCANDEXT;            /* GET EXTENT DESCRIPTORS        */
