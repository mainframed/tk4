*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**   MODULE NAME =  IGGPUALL (IGG0CLBB)                               *
**                                                                    *
**   DESCRIPTIVE NAME =  UNIQUE DATA SET ALLOCATION                   *
**                                                                    *
**   FUNCTION =                                                       *
**      THIS SUB-FUNCTION INTERFACES WITH IGGPSPAC (IGG0CLBP) TO      *
**      OBTAIN MORE EXTENTS FROM DADSM FOR A NEW UNIQUE DATA SPACE OR *
**      AN EXISTING UNIQUE DATA SPACE.  THE PROCESSING IS AS FOLLOWS: *
**      1. BUILD IGGPSPAC PARAMETER LIST FOR A UNIQUE DATA SET.       *
**      2. READ ASSOCIATED VOLUME ENTRY INTO RECORD AREA 3.           *
**      3. CALL IGGPSPAC.                                             *
**      IGGPSPAC WILL RETURN THE NEWLY ACQUIRED EXTENTS IN THE DADSM  *
**      EXTENTS SAVE AREA OF THE CCA (THE SAME AS VSAM SUB-ALLOCATE   *
**      (IGG0CLAR) DOES).                                             *
**                                                                    *
**   INPUT =                                                          *
**      RECORD AREAS 3,4, AND 5 AVAILABLE FOR USE (CCARBUR OFF).      *
**      BASE CATALOG RECORD OF THE DATA SET BEING EXTENDED IN RAB 1.  *
**         CSORT - ALLOCATION UNIT.                                   *
**         CNAME - DATA SET NAME.                                     *
**         CREUSE - REUSE FLAG.                               @Y30SSRS*
**         CPRIM - PRIMARY ALLOCATION QUANTITY.                       *
**         CSEC - SECONDARY ALLOCATION QUANTITY.                      *
**      UPREPARM PARAMETERS:                                          *
**         UPEXTLN - LENGTH OF OLD VOLUME OCCURRENCE EXTENTS.         *
**         UPNIEXT - NO. OF INDEX EXTENTS OR ZERO IF NOT SEQ. SET     *
**                   WITH DATA.                               @XA02392*
**      UPENTVOL PARAMETERS:                                          *
**         UPVOL - VOLUME SERIAL.                                     *
**         UPPHYB - PHYSICAL BLOCK SIZE.                              *
**         UPSSDS - DATA SET DIRECTORY SEQUENCE NO.                   *
**                                                                    *
**   OUTPUT =                                                         *
**      VOLUME ENTRY IN RECORD AREA 3.                                *
**      CCA EXTRACT/MODIFY INTERFACE FLAGS AND ADDRESSES CHANGED.     *
**      CCADESA - DADSM EXTENTS SAVE AREA.                            *
**         CCANDEXT - NUMBER OF NEW EXTENTS OBTAINED FOR UNIQUE DATA  *
**                    SET.                                            *
**         CCAEXTDE(*) - ARRAY OF EXTENT DESCRIPTORS INITIALIZED.     *
**            CCAEXTSS - DESCRIPTOR SPACE SEQUENCE NO.                *
**            CCAEXTAD - EXTENT STARTING CCHH.                        *
**            CCAEXTTH - NUMBER OF TRACKS IN EXTENT.                  *
**      UPREPARM PARAMETERS:                                          *
**         UPSMFL - SET ON, TO INDICATE LSPACE SMF RECORD 69 SHOULD   *
**                  BE WRITTEN.                                       *
**      CCACD1 - RCS, NO ERROR.                                       *
**             - RCNOSP, NO SPACE USER VOLUME.                        *
**             - RCDSEXT, DATA SET HAS MAXIMUM NO. OF EXTENTS.        *
**             - ANY ERROR RETURNED FROM IGGPSPAC.                    *
**      NEW DATA SPACE CREATED OR OLD DATA SPACE EXTENDED.            *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0362
*   DCL                                                            0362
*     MXVOLEXT FIXED(31) CONSTANT(16);/* MAX UNIQUE EXTENTS/VOL      */
*   UPSPARM=UPSPARM&&UPSPARM;       /* ZERO SPACE WORK AREA          */
         L     @10,CCAR3REC(,R11)                                  0363
         SL    @10,@CF00544                                        0363
         XC    UPSPARM(220,@10),UPSPARM(@10)                       0363
*   UPSOPT=CSORT;                   /* SPACE OPTIONS                 */
         L     @05,CCARABB(,R11)                                   0364
         L     @05,CCACPE1(,@05)                                   0364
         MVC   UPSOPT(1,@10),CSORT(@05)                            0364
*   UPSUNIQ=ON;                     /* UNIQUE DATA SET               */
         OI    UPSUNIQ(@10),B'10000000'                            0365
*   UPUDSNPT=ADDR(CNAME);           /* UNIQUE DATA SET NAME          */
         LA    @15,CNAME(,@05)                                     0366
         ST    @15,UPUDSNPT(,@10)                                  0366
*   UPMXBLK=UPPHYB;                 /* BLOCKSIZE                     */
         L     @15,CCAR2REC(,R11)                                  0367
         LA    @14,18                                              0367
         ALR   @14,@15                                             0367
         MVC   UPMXBLK(2,@10),UPPHYB+2(@14)                        0367
*   UPSECQTY=CSEC;                  /* SECONDARY ALLOC       @ZA07574*/
         MVC   UPSECQTY(3,@10),CSEC(@05)                           0368
*   UPDSDIR=UPSSDS;                 /* DATA SET DIRECTORY SEQ. NO.   */
         ICM   @14,12,UPSSDS(@14)                                  0369
         SRA   @14,16                                              0369
         STCM  @14,7,UPDSDIR(@10)                                  0369
*   IF UPEXTLN=0 THEN               /* IF NEW VOLUME                 */
         LH    @15,UPEXTLN(,@15)                                   0370
         LTR   @15,@15                                             0370
         BNZ   @RF00370                                            0370
*     UPSPQTY=CPRIM;                /* USE PRIMARY ALLOCATION        */
         MVC   UPSPQTY(3,@10),CPRIM(@05)                           0371
*   ELSE                            /* OLD VOLUME WITH EXTENTS       */
*     IF CSEC=0 THEN                /* IF NO SECONDARY QUANTITY      */
*                                                                  0372
         B     @RC00370                                            0372
@RF00370 L     @10,CCARABB(,R11)                                   0372
         L     @10,CCACPE1(,@10)                                   0372
         L     @10,CSEC-1(,@10)                                    0372
         LA    @10,0(,@10)                                         0372
         LTR   @10,@10                                             0372
         BNZ   @RF00372                                            0372
*       /*************************************************************/
*       /*                                                           */
*       /* SET CCAMODID='BB', CCAREASN=RSN4, CCARETRN=RCNOSP         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0373
*       CCAPROB=IGGSEGID+(RSN4*K256)+RCNOSP;/* INDICATE NO SPACE   0373
*                                                            @Y30SSRS*/
         MVC   CCAPROB(4,R11),@CF03389                             0373
*     ELSE                          /* USE SECONDARY QUANTITY        */
*       UPSPQTY=CSEC;               /* SET QUANTITY                  */
         B     @RC00372                                            0374
@RF00372 L     @10,CCAR3REC(,R11)                                  0374
         L     @05,CCARABB(,R11)                                   0374
         L     @05,CCACPE1(,@05)                                   0374
         AL    @10,@CF03422                                        0374
         MVC   UPSPQTY-1(3,@10),CSEC(@05)                          0374
*   IF UPNOEXT>=MXVOLEXT THEN       /* IF MAX EXTENTS ON VOLUME      */
*                                                                  0375
@RC00372 DS    0H                                                  0375
@RC00370 L     @10,CCAR2REC(,R11)                                  0375
         CLI   UPNOEXT+18(@10),16                                  0375
         BL    @RF00375                                            0375
*     /***************************************************************/
*     /*                                                             */
*     /* SET CCAMODID='BB', CCAREASN=RSN8, CCARETRN=RCNOSP           */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0376
*     CCAPROB=IGGSEGID+(RSN8*K256)+RCNOSP;/* INDICATE NO SPACE     0376
*                                                            @Y30SSRS*/
         MVC   CCAPROB(4,R11),@CF03392                             0376
*   ELSE                                                           0377
*     ;                             /* MAX EXTENTS NOT REACHED       */
@RF00375 DS    0H                                                  0378
*   IF MAX(AMDNEXT,UPNIEXT)>MXEXT-MXALEX/* IF D S MAX EXTS   @Y30SSRS*/
*     THEN                          /* REACHED               @Y30SSRS*/
*                                                                  0378
         L     @10,CCAR2REC(,R11)                                  0378
         LA    @05,UPREND(,@10)                                    0378
         LH    @05,AMDNEXT+176(,@05)                               0378
         LH    @10,UPNIEXT(,@10)                                   0378
         CR    @05,@10                                             0378
         BNL   *+6
         LR    @05,@10                                             0378
         C     @05,@CF00655                                        0378
         BNH   @RF00378                                            0378
*     /***************************************************************/
*     /*                                                             */
*     /* SET CCAMODID='BB', CCAREASN=RSN0, CCARETRN=RCDSEXT          */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0379
*     CCAPROB=IGGSEGID+(RSN0*K256)+RCDSEXT;/* SET ERROR CODE @Y30SSRS*/
         MVC   CCAPROB(4,R11),@CF03393                             0379
*   ELSE                                                           0380
*     ;                             /* EXTENTS CAN BE ADDED TO DATA
*                                      SET                           */
@RF00378 DS    0H                                                  0381
*   IF CCACD1=RCS THEN              /* IF NO PREVIOUS ERRORS         */
         CLI   CCACD1(R11),0                                       0381
         BNE   @RF00381                                            0381
*     DO;                           /* CALL DADSM FOR SPACE          */
*       CCASRCH=CCASRCH&&CCASRCH;   /* ZERO SEARCH ARG               */
         XC    CCASRCH(44,R11),CCASRCH(R11)                        0383
*       CCASRCH(1:LENGTH(UPVOL))=UPVOL;/* SET VOLSER                 */
         L     @10,CCAR2REC(,R11)                                  0384
         MVC   CCASRCH(6,R11),UPVOL+18(@10)                        0384
*       DO;                                                        0385
*         CCAF1KEY=ON;              /* TRUE NAME GET                 */
*         CCAF1KGE=OFF;             /* NOT GET NEXT                  */
*         CCAF1UP=ON;               /* GET FOR UPD                   */
         OI    CCAF1KEY(R11),B'00010010'                           0388
         NI    CCAF1KGE(R11),B'11110111'                           0388
*         CCAURAB=ADDR(CCARAB3);    /* SET RAB PTR                   */
*                                                                  0389
         LA    @10,CCARAB3(,R11)                                   0389
         ST    @10,CCAURAB(,R11)                                   0389
*         /***********************************************************/
*         /*                                                         */
*         /* CALL TO MODULE IGG0CLBI                                 */
