Z030SPAC L     @15,@CV00042                                        0394
         BALR  @14,@15                                             0394
*           UPSMFL=ON;              /* INDICATE LSPACE SMF           */
         L     @10,CCAR2REC(,R11)                                  0395
         OI    UPSMFL(@10),B'00000100'                             0395
*         END;                      /* END CALL DADSM                */
*       ELSE                        /* PREVIOUS ERROR                */
*         IF CCACD1=RCENT THEN      /* IF VOL ENTRY NOT FOUND        */
