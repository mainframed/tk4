*     DO;                           /* GET INDEX NO. OF EXTENTS      */
*       IF WKAICI=0 THEN            /* IF NO INDEX CI NO     @Y30SSRS*/
         L     @10,CCACPL(,R11)                                    0163
         L     @10,CTGWKA(,@10)                                    0163
         ICM   @05,7,WKAICI(@10)                                   0163
         BNZ   @RF00163                                            0163
*         DO;                       /* ERROR SITUATION       @Y30SSRS*/
