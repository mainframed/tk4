         TITLE 'IGG0CLBA, TESTS                                        *
                        '
IGG0CLBA CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGG0CLBA  78.152'                                 0001
         DROP  @15
@PROLOG  DS    0H                                                  0002
         USING @MAINENT,@15
         USING @DATD-(CCATEMPS-IGGCCA),R11
         DS 0F
         DC CL8'IGGPTSTS'
         EJECT
         B     @PB00002                                            0072
IGGPTSTS DS    0H                                                  0073
         ENTRY IGGPTSTS
         DROP @15
Z003     DS    0H                                                  0075
Z003TSTS DS    0H                                                  0076
         STM 12,14,12(13)
         LA 13,12(13)
         BALR 12,0
IGGSEGST DS 0H
         USING *,12
*   CCAURAB=CCARABF;                /* GET FIRST RCD AREA PTR        */
         L     @10,CCARABF(,R11)                                   0083
         ST    @10,CCAURAB(,R11)                                   0083
*   DO WHILE CCAURAB<=CCARABL;      /* FREE ALL BUFFERS              */
         B     @DE00084                                            0084
@DL00084 DS    0H                                                  0085
*     CCARBU1=OFF;                  /* MAKE BUFFER AVAILABLE         */
*     CCARBU2=OFF;                  /* MAKE BUFFER AVAILABLE         */
         L     @10,CCAURAB(,R11)                                   0086
         NI    CCARBU1(@10),B'10011111'                            0086
*     CCAURAB=CCAURAB+LENGTH(CCARAB);/* GET NEXT RCD AREA PTR        */
         AL    @10,@CF00042                                        0087
         ST    @10,CCAURAB(,R11)                                   0087
*   END;                            /* END FREEING                   */
@DE00084 CLC   CCAURAB(4,R11),CCARABL(R11)                         0088
         BNH   @DL00084                                            0088
*   IF CCATNREL=1 THEN              /* CHECK IF FIRST CALL FOR THIS
*                                      FIELD CHAIN                   */
         CLC   CCATNREL(2,R11),@CH00048                            0089
         BNE   @RF00089                                            0089
*     CCATNO=0;                     /* SET NUMBER OF SUCCESSFUL    0090
*                                      RELREPNOS (TOTAL)             */
         SLR   @10,@10                                             0090
         STH   @10,CCATNO(,R11)                                    0090
*   ELSE                                                           0091
*     ;                             /* NOT FIRST CALL                */
@RF00089 DS    0H                                                  0092
*   CCALREL=CCATNREL;               /* GET NEXT RELREPNO FOR       0092
*                                      RELREPNOS                     */
         LH    @10,CCATNREL(,R11)                                  0092
         STH   @10,CCALREL(,R11)                                   0092
*   CCATNUM=0;                      /* NUMBER SUCCESSFUL RELREPNOS 0093
*                                      INITIALLY ZERO                */
         SLR   @10,@10                                             0093
         STH   @10,CCATNUM(,R11)                                   0093
*   CCATEX=OFF;                     /* ENTER PROCESSING LOOP         */
         NI    CCATEX(R11),B'11111011'                             0094
*   DO WHILE(CCACD1=RCS&CCATEX=OFF);/* PROCESS TESTS FOR ALL       0095
*                                      RELREPNOS                     */
         B     @DE00095                                            0095
@DL00095 DS    0H                                                  0096
*     CCATEST=CTGFLCHN;             /* GET FIRST TEST                */
         L     @10,CCAFLPT(,R11)                                   0096
         L     @10,CTGFLCHN(,@10)                                  0096
         ST    @10,CCATEST(,R11)                                   0096
*     CCATHIT=ON;                   /* ASSUME SUCCESSFUL FOR THIS  0097
*                                      RELREPNO                      */
         OI    CCATHIT(R11),B'00001000'                            0097
*     CCATCOMP=OFF;                 /* ASSUME NOT COMPLETE           */
         NI    CCATCOMP(R11),B'11111110'                           0098
*     CCATEX1=OFF;                  /* BEGIN PROCESSING EACH TEST    */
         NI    CCATEX1(R11),B'11111101'                            0099
*     DO WHILE(CCATEST^=0&CCATEX1=OFF&/* PROCESS THIS                */
*           CCACD1=RCS);            /* PARTICULAR TEST               */
         B     @DE00100                                            0100
@DL00100 DS    0H                                                  0101
*       RFY                                                        0101
*         CTGFL BASED(CCATEST);     /* TEST POINTER BASE             */
*       CTGFLDTS=OFF;               /* ASSUME TEST SUCCESSFUL        */
         L     @10,CCATEST(,R11)                                   0102
         NI    CTGFLDTS(@10),B'11111110'                           0102
*       IF DICTFLGS=DTSCC&          /* IS THIS CNTREPNO FIELD        */
*           DICTST=DTCNT THEN       /* TO BE PROCESSED               */
         LA    @05,4                                               0103
         ALR   @05,@10                                             0103
         CLI   DICTFLGS(@05),X'60'                                 0103
         BNE   @RF00103                                            0103
         CLI   DICTST(@05),2                                       0103
         BNE   @RF00103                                            0103
*         DO;                       /* PROCESS CNTREPNO              */
*           IF CCATNO>=USERD(1:2) THEN/* HAS LIMIT OF RELREPNOS BEEN
*                                      EXCEEDED                      */
         L     @10,CTGFLPT(,@10)                                   0105
         LH    @05,CCATNO(,R11)                                    0105
         SLR   @15,@15                                             0105
         ICM   @15,3,USERD(@10)                                    0105
         CR    @05,@15                                             0105
         BL    @RF00105                                            0105
*             DO;                   /* INDICATE FINISHED             */
*               CCATHIT=OFF;        /* NOT SUCCESSFUL FOR THIS     0107
*                                      RELREPNO                      */
*               CCATEX=ON;          /* PROCESS NO MORE RELREPNOS     */
*               CCATEX1=ON;         /* PROCESS NO MORE TESTS         */
         OI    CCATEX(R11),B'00000110'                             0109
         NI    CCATHIT(R11),B'11110111'                            0109
*               CCATCOMP=ON;        /* TESTS COMPLETE                */
         OI    CCATCOMP(R11),B'00000001'                           0110
*             END;                  /* END INDICATE FINISHED         */
*           ELSE                                                   0112
*             ;                     /* LIMIT NOT EXCEEDED            */
@RF00105 DS    0H                                                  0113
*         END;                      /* END PROCESS CNTREPNO          */
*       ELSE                        /* NOT CNTREPND FIELD            */
*         DO;                       /* PROCESS TEST FIELD            */
         B     @RC00103                                            0114
@RF00103 DS    0H                                                  0115
*           CCARBAL=0;              /* SET INITIAL DISPLACEMENT INTO
*                                      RBA HOLDER FIELD              */
         SLR   @10,@10                                             0115
         STH   @10,CCARBAL(,R11)                                   0115
*           CCALFT=ON;              /* INDICATE FIRST TIME           */
         OI    CCALFT(R11),B'00000010'                             0116
*           CCAGVEXT=0;             /* NO EXTENSION                  */
         STCM  @10,7,CCAGVEXT(R11)                                 0117
*           CCADICT=CTGFLDWA;       /* SET DICTIONARY INFO           */
         L     @10,CCATEST(,R11)                                   0118
         MVC   CCADICT(4,R11),CTGFLDWA(@10)                        0118
*           CCAGVNC=ON;             /* START GETVAL PROCESSING       */
         OI    CCAGVNC(R11),B'00100000'                            0119
*           DO WHILE(CCACD1=RCS&    /* TEST VALUE BY MULTIPLE        */
*                 CCAGVNC=ON);      /* CALLS TO GETVAL               */
         B     @DE00120                                            0120
@DL00120 DS    0H                                                  0121
*Z004GVAL:                                                         0121
*             CALL IGGPGVAL;        /* CALL GETVAL                   */
Z004GVAL BAL   @14,IGGPGVAL                                        0121
*             IF CCACD1^=RCS×       /* CHECK FOR ERROR OR NO         */
*                 CCAGVNBS=ON THEN  /* BUFFER SPACE                  */
         CLI   CCACD1(R11),0                                       0122
         BNE   @RT00122                                            0122
         TM    CCAGVNBS(R11),B'00001000'                           0122
         BNO   @RF00122                                            0122
@RT00122 DS    0H                                                  0123
*               DO;                 /* PROCESS CONDITION             */
*                 CCATHIT=OFF;      /* NOT SUCCESSFUL FOR THIS     0124
*                                      RELREPNO                      */
*                 CCATEX=ON;        /* EXIT FROM RELREPNO LOOP       */
*                 CCATEX1=ON;       /* EXIT FROM TEST LOOP           */
         OI    CCATEX(R11),B'00000110'                             0126
         NI    CCATHIT(R11),B'11110111'                            0126
*                 CCAGVNC=OFF;      /* EXIT FROM GETVAL LOOP         */
         NI    CCAGVNC(R11),B'11011111'                            0127
*                 CCALREL=CCALREL-1;/* SET UP FOR NEXT RELREPNO      */
         LH    @10,CCALREL(,R11)                                   0128
         BCTR  @10,0                                               0128
         STH   @10,CCALREL(,R11)                                   0128
*               END;                /* END PROCESS CONDITION         */
*             ELSE                  /* THERE WAS BUFFER SPACE        */
*               IF CCAGVNF=ON×      /* IF NOT FOUND OR FIELD         */
*                   (CCASUPFD=ON&   /* SUPPRESSION IS IN EFFECT      */
*                   DICTGC=PSWDGC) THEN/* FOR THIS GROUP CODE        */
         B     @RC00122                                            0130
@RF00122 TM    CCAGVNF(R11),B'00010000'                            0130
         BO    @RT00130                                            0130
         TM    CCASUPFD(R11),B'00001000'                           0130
         BNO   @RF00130                                            0130
         L     @10,CCATEST(,R11)                                   0130
         CLI   DICTGC+4(@10),4                                     0130
         BNE   @RF00130                                            0130
@RT00130 DS    0H                                                  0131
*                 DO;               /* PROCESS NOT FOUND             */
*                   CCATHIT=OFF;    /* NOT SUCCESSFUL FOR THIS     0132
*                                      RELREPNO                      */
         NI    CCATHIT(R11),B'11110111'                            0132
*                   CCATCOMP=ON;    /* COMPLETE                      */
         OI    CCATCOMP(R11),B'00000001'                           0133
*                   CCATEX=ON;      /* EXIT FROM RELREPNO LOOP       */
*                   CCATEX1=ON;     /* EXIST FROM TEST LOOP          */
         OI    CCATEX(R11),B'00000110'                             0135
*                   CCAGVNC=OFF;    /* EXIT FROM GETVAL LOOP         */
         NI    CCAGVNC(R11),B'11011111'                            0136
*                 END;              /* END PROCESS NOT FOUND         */
*               ELSE                /* RELREPNO FOUND                */
*                 DO;               /* PERFORM COMPARISON            */
         B     @RC00130                                            0138
@RF00130 DS    0H                                                  0139
*                   IF CCALFT=ON THEN/* IF FIRST TIME                */
         TM    CCALFT(R11),B'00000010'                             0139
         BNO   @RF00139                                            0139
*                     DO;           /* MAKE BUFFER UNAVAILABLE       */
*                       IF CCAGOPLN=CATGOPLN THEN/* IF NOT VOLUME  0141
*                                                            @XM01569*/
         CLC   CCAGOPLN(2,R11),@CH00065                            0141
         BNE   @RF00141                                            0141
*                         CCARBU2=ON;/* SAVE I/O             @XM01569*/
         L     @10,CCAURAB(,R11)                                   0142
         OI    CCARBU2(@10),B'00100000'                            0142
*                       ELSE                                       0143
*                         ;         /* VOLUME RCD            @XM01569*/
@RF00141 DS    0H                                                  0144
*                       CCARABPT=CCAURAB;/* SAVE RAB ADDR            */
         L     @10,CCAURAB(,R11)                                   0144
         ST    @10,CCARABPT(,R11)                                  0144
*                       CCALFT=OFF; /* NOT FIRST TIME HEREAFTER      */
         NI    CCALFT(R11),B'11111101'                             0145
*                     END;          /* END BUFFER UNAVAILABLE        */
*                   ELSE                                           0147
*                     ;             /* NOT FIRST TIME                */
@RF00139 DS    0H                                                  0148
*Z005TCMP:                                                         0148
*                   CALL IGGPTCMP;  /* CALL TESTS COMPARE            */
Z005TCMP BAL   @14,IGGPTCMP                                        0148
*                   IF CCAGVNC=OFF& /* IF COMPLETE AND NO HIT        */
*                       CCATHIT=OFF THEN/* WAS GOTTEN                */
         TM    CCAGVNC(R11),B'00100000'                            0149
         BNZ   @RF00149                                            0149
         TM    CCATHIT(R11),B'00001000'                            0149
         BNZ   @RF00149                                            0149
*                     DO;           /* MAKE BUFFER AVAIL             */
*                       CCARABPT->CCARBU2=OFF;/* MAKE BUFFER AVAIL   */
         L     @10,CCARABPT(,R11)                                  0151
         NI    CCARBU2(@10),B'11011111'                            0151
*                     END;          /* END MAKE BUFFER AVAIL         */
*                   ELSE                                           0153
*                     ;             /* INCOMPLETE OR HIT             */
@RF00149 DS    0H                                                  0154
*                 END;              /* END PERFORM COMPARISON        */
*           END;                    /* END TEST VALUE                */
@RC00130 DS    0H                                                  0155
@RC00122 DS    0H                                                  0155
@DE00120 CLI   CCACD1(R11),0                                       0155
         BNE   @DC00120                                            0155
         TM    CCAGVNC(R11),B'00100000'                            0155
         BO    @DL00120                                            0155
@DC00120 DS    0H                                                  0156
*         END;                      /* END PROCESS TEST FIELD        */
*       CCATEST=CTGFLCHN;           /* GET NEXT TEST                 */
@RC00103 L     @10,CCATEST(,R11)                                   0157
         L     @10,CTGFLCHN(,@10)                                  0157
         ST    @10,CCATEST(,R11)                                   0157
*     END;                          /* END PROCESS TEST              */
@DE00100 L     @10,CCATEST(,R11)                                   0158
         LTR   @10,@10                                             0158
         BZ    @DC00100                                            0158
         TM    CCATEX1(R11),B'00000010'                            0158
         BNZ   @DC00100                                            0158
         CLI   CCACD1(R11),0                                       0158
         BE    @DL00100                                            0158
@DC00100 DS    0H                                                  0159
*     IF CCATHIT=ON THEN            /* IF ALL TESTS SUCCESSFUL       */
         TM    CCATHIT(R11),B'00001000'                            0159
         BNO   @RF00159                                            0159
*       DO;                         /* SUCCESSFUL PROCESSING         */
*         IF CCAGOPLN=CATGOPLN THEN /* IF NOT VOL            @XM01569*/
         CLC   CCAGOPLN(2,R11),@CH00065                            0161
         BNE   @RF00161                                            0161
*           CCARABB->CCARBU1=ON;    /* SET BUFFER UNAVAILABLE      0162
*                                                            @XM01569*/
         L     @10,CCARABB(,R11)                                   0162
         OI    CCARBU1(@10),B'01000000'                            0162
*         ELSE                                                     0163
*           ;                       /* VOLUME RCD            @XM01569*/
@RF00161 DS    0H                                                  0164
*         CCATNUM=CCATNUM+1;        /* INCR NO. SUCCESSFUL RELREPNOS */
         LA    @10,1                                               0164
         LH    @05,CCATNUM(,R11)                                   0164
         ALR   @05,@10                                             0164
         STH   @05,CCATNUM(,R11)                                   0164
*         CCATREL(CCATNUM)=CCALREL; /* SAVE RELREPNO                 */
         ALR   @05,@05                                             0165
         LH    @15,CCALREL(,R11)                                   0165
         STH   @15,CCATREL-2(@05,R11)                              0165
*         CCATNO=CCATNO+1;          /* INCR TOTAL NO. OF SUCCESSFUL
*                                      RELREPNOS                     */
         AH    @10,CCATNO(,R11)                                    0166
         STH   @10,CCATNO(,R11)                                    0166
*       END;                        /* END SUCCESSFUL PROCESSING     */
*     ELSE                                                         0168
*       ;                           /* TESTS NOT SUCCESSFUL          */
@RF00159 DS    0H                                                  0169
*     CCALREL=CCALREL+1;            /* GET NEXT RELREPNO             */
         LA    @10,1                                               0169
         AH    @10,CCALREL(,R11)                                   0169
         STH   @10,CCALREL(,R11)                                   0169
*     CCATNREL=CCALREL;             /* SET NEXT RELREPNO             */
         STH   @10,CCATNREL(,R11)                                  0170
*     IF CCATNUM>=TESTNUM THEN      /* LIMIT OF RELREPNOS REACHED    */
         LH    @10,CCATNUM(,R11)                                   0171
         C     @10,@CF00044                                        0171
         BL    @RF00171                                            0171
*       DO;                         /* PROCESS FOR RETURN            */
*         CCATEX=ON;                /* EXIT FROM PROCESSING RELREPNOS*/
         OI    CCATEX(R11),B'00000100'                             0173
*       END;                        /* END PROCESS                   */
*     ELSE                                                         0175
*       ;                           /* LIMIT NOT REACHED             */
@RF00171 DS    0H                                                  0176
*     RFY                                                          0176
*       CTGFL BASED(CCAFLPT);       /* ORIGINAL BASE                 */
*     CCATEST=CTGFLCHN;             /* GET FIRST TEST                */
         L     @10,CCAFLPT(,R11)                                   0177
         L     @10,CTGFLCHN(,@10)                                  0177
         ST    @10,CCATEST(,R11)                                   0177
*     RFY                                                          0178
*       CTGFL BASED(CCATEST);       /* TEST FIELD PTR BASE           */
*     IF DICTREP=ON THEN            /* SEE IF REPEATING              */
         TM    DICTREP+4(@10),B'01000000'                          0179
         BNO   @RF00179                                            0179
*       IF CCATCOMP=ON THEN         /* ARE TESTS COMPLETE            */
         TM    CCATCOMP(R11),B'00000001'                           0180
         BNO   @RF00180                                            0180
*         DO WHILE(CCATEST^=0);     /* SET TEST FIELD INDICATOR      */
         B     @DE00181                                            0181
@DL00181 DS    0H                                                  0182
*           IF CCATNO>0 THEN        /* IF SUCCESSFUL RELREPNOS       */
         LH    @10,CCATNO(,R11)                                    0182
         LTR   @10,@10                                             0182
         BNP   @RF00182                                            0182
*             CTGFLDTS=OFF;         /* TEST SUCCESSFUL               */
         L     @10,CCATEST(,R11)                                   0183
         NI    CTGFLDTS(@10),B'11111110'                           0183
*           ELSE                    /* NO SUCCESSFUL RELREPNOS       */
*             CTGFLDTS=ON;          /* TEST UNSUCCESSFUL             */
         B     @RC00182                                            0184
@RF00182 L     @10,CCATEST(,R11)                                   0184
         OI    CTGFLDTS(@10),B'00000001'                           0184
*           CCATEST=CTGFLCHN;       /* GET NEXT TEST                 */
@RC00182 L     @10,CCATEST(,R11)                                   0185
         L     @10,CTGFLCHN(,@10)                                  0185
         ST    @10,CCATEST(,R11)                                   0185
*         END;                      /* END TEST PROCESSING           */
@DE00181 L     @10,CCATEST(,R11)                                   0186
         LTR   @10,@10                                             0186
         BNZ   @DL00181                                            0186
*       ELSE                                                       0187
*         ;                         /* TESTS NOT COMPLETE            */
@RF00180 DS    0H                                                  0188
*     ELSE                          /* NOT REPEATING                 */
*       DO;                         /* MUST EXIT WITHOUT RETURN      */
         B     @RC00179                                            0188
@RF00179 DS    0H                                                  0189
*         CCATCOMP=ON;              /* TESTS COMPLETE                */
         OI    CCATCOMP(R11),B'00000001'                           0189
*         CCATEX=ON;                /* EXIT FROM RELREPNO LOOP       */
         OI    CCATEX(R11),B'00000100'                             0190
*       END;                        /* END EXIT PROCESSING           */
*   END;                            /* END PROCESS RELREPNOS         */
@RC00179 DS    0H                                                  0192
@DE00095 CLI   CCACD1(R11),0                                       0192
         BNE   @DC00095                                            0192
         TM    CCATEX(R11),B'00000100'                             0192
         BZ    @DL00095                                            0192
@DC00095 DS    0H                                                  0193
*   CCAURAB=CCARABF;                /* GET FIRST RCD AREA PTR        */
         L     @10,CCARABF(,R11)                                   0193
         ST    @10,CCAURAB(,R11)                                   0193
*   DO WHILE(CCAURAB<=CCARABL);     /* FREE ALL BUFFERS              */
         B     @DE00194                                            0194
@DL00194 DS    0H                                                  0195
*     CCARBU1=OFF;                  /* FREE BUFFER                   */
*     CCARBU2=OFF;                  /* FREE BUFFER                   */
         L     @10,CCAURAB(,R11)                                   0196
         NI    CCARBU1(@10),B'10011111'                            0196
*     CCAURAB=CCAURAB+LENGTH(CCARAB);/* GET NEXT RAB                 */
         AL    @10,@CF00042                                        0197
         ST    @10,CCAURAB(,R11)                                   0197
*   END;                            /* END FREEING                   */
@DE00194 CLC   CCAURAB(4,R11),CCARABL(R11)                         0198
         BNH   @DL00194                                            0198
*   IF CCAGOPLN=VOLGOPLN THEN       /* IF VOLUME RCD         @XM01569*/
         CLC   CCAGOPLN(2,R11),@CH00063                            0199
         BNE   @RF00199                                            0199
*     DO;                           /* REREAD BASE RCD       @XM01569*/
*       CCASRID=CCACBASE;           /* SET BASE RCD ARG      @XM01569*/
         MVC   CCASRID(3,R11),CCACBASE(R11)                        0201
*Z006GREC:                                                         0202
*       CALL IGGPGREC;              /* READ BASE             @XM01569*/
Z006GREC BAL   @14,IGGPGREC                                        0202
*     END;                          /* END REREAD            @XM01569*/
*   ELSE                                                           0204
*     ;                             /* NOT VOLUME RCD        @XM01569*/
@RF00199 DS    0H                                                  0205
*   RFY                                                            0205
*     CTGFL BASED(CCAFLPT);         /* ORIGINAL BASE                 */
*   CALL EXIT;                      /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0206
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */
         DS 0F
*   GEN(DC CL8'IGGPTCMP');          /* NAME OF FOLLOWING PROCEDURE   */
         DC CL8'IGGPTCMP'
*   END;                            /* END OF INNER PROCEDURE        */
@EL00002 DS    0H                                                  0209
@EF00002 DS    0H                                                  0209
@ER00002 BR    @14                                                 0209
@PB00002 DS    0H                                                  0210
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */
         EJECT
