         OI    CCAGVEX(R11),B'00000100'                            0322
*     ELSE                          /* FOUND BUFFER FOR RCD          */
*       DO;                         /* LOCATE VALUE                  */
         B     @RC00321                                            0323
@RF00321 DS    0H                                                  0324
*Z011LVAL:                                                         0324
*         CALL IGGPLVAL;            /* CALL LOCVAL                   */
Z011LVAL BAL   @14,IGGPLVAL                                        0324
*         IF CCAGVNF=ON THEN        /* SEE IF NOT FOUND VALUE        */
         TM    CCAGVNF(R11),B'00010000'                            0325
         BNO   @RF00325                                            0325
