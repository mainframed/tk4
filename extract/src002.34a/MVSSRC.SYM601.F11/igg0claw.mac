*   GEN(ENTRY IGGPPREC );           /* IDENTIFY ENTRY-POINT SYMBOL   */
         ENTRY IGGPPREC
*Z044:                              /* LABEL FOR PROCEDURE TRACKING  */
*Z044PREC:                          /* LABEL FOR PROCEDURE TRACKING  */
*   RFY                                                            0531
*    (R5,                                                          0531
*     R6,                                                          0531
*     R7,                                                          0531
*     R8,                                                          0531
*     R9,                                                          0531
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */
Z044     DS    0H                                                  0531
Z044PREC DS    0H                                                  0532
*   RFY                                                            0532
*    (R2,                                                          0532
*     R3,                                                          0532
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */
         STM 12,14,12(13)
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */
         LA 13,12(13)
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */
         BALR 12,0
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */
         LA 15,*-IGGSEGST(0,0)
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE     0537
*                                      PROCEDURE                     */
         SR 12,15
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**   MODULE NAME =  IGGPPREC (IGG0CLAW)                               *
**                                                                    *
**   DESCRIPTIVE NAME =  PUT CATALOG RECORD                           *
**                                                                    *
**   FUNCTION =                                                       *
**      THIS SUB-FUNCTION INTERFACES WITH THE CATALOG I/O PUT SUB-    *
**      FUNCTION TO UPDATE AND ADD RECORDS TO THE CATALOG DATA SET.   *
**      IT IS CALLED FROM SUB-FUNCTIONS IGGPGREC (IN MODULE           *
**      IGG0CLBA), IGGPGNEX(IGG0CLAW),IGGPDEL2(IGG0CLAV),IGGPMVAR     *
**      (IGGOCLAX),IGGPSNK2(IGGOCLBW),IGGPDOWN(IGG0CLBW),IGGPRISE     *
**      (IGG0CLBW).  THE PROCESSING IS AS FOLLOWS:                    *
**      1. IF PLAIN UPDATE, CALL IGGPPUPC TO UPDATE THE CATALOG RECORD*
**         AND THEN CALL IGGPSMF FOR SMF ALTER PROCESSING.            *
**      2. IF ADD, CALL IGGPPAD TO ADD THE CATALOG RECORD. TURN OFF   *
**         THE RAB PUT ADD BIT.                                       *
**      3. TURN OFF THE RAB WRITE BIT.                                *
**                                                                    *
**   INPUT =                                                          *
**      CCAURAB - RAB ADDRESS INDICATING RECORD AREA CONTAINING       *
**                CATALOG RECORD TO BE UPDATED OR ADDED.              *
**      CCARBPA - IF ON, INDICATES ADD. OTHERWISE, INDICATES UPDATE.  *
**                                                                    *
**   OUTPUT =                                                         *
**      CCARBPA - SET OFF, TO INDICATE PUT ADD DONE                   *
**      CCARBWR - SET OFF, TO INDICATE WRITE DONE.                    *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0538
*   IF CCARBPA=OFF THEN             /* IF PUT-FOR-UPDATE             */
         L     @10,CCAURAB(,R11)                                   0538
         TM    CCARBPA(@10),B'00001000'                            0538
         BNZ   @RF00538                                            0538
*     DO;                           /* ISSUE UPDATE                  */
*       DO;                                                        0540
*                                                                  0540
*         /***********************************************************/
*         /*                                                         */
*         /* CALL TO MODULE IGG0CLAG                                 */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0541
*Z046PUPC:                                                         0541
*         CALL IGGPPUPC;            /* PUT-UPDATE RECORD             */
Z046PUPC L     @15,@CV00038                                        0541
         BALR  @14,@15                                             0541
*       END;                        /* PUT-UPDATE                    */
*                                                                  0542
*       /*************************************************************/
*       /*                                                           */
*       /* CALL TO MODULE IGG0CLB3                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0543
*Z047SMF:                                                          0543
*       CALL IGGPSMF;               /* CALL SMF FOR ALTER ENTRY    0543
*                                      PROCESSING                    */
Z047SMF  L     @15,@CV00040                                        0543
         BALR  @14,@15                                             0543
*     END;                          /* END ISSUE UPDATE              */
*   ELSE                            /* PUT-ADD                       */
*     DO;                           /* ISSUE ADD                     */
         B     @RC00538                                            0545
@RF00538 DS    0H                                                  0546
*       RCDCRAVL=CCACRAVL;          /* SET VOLSER            @Y30SSRS*/
         L     @10,CCAURAB(,R11)                                   0546
         L     @10,CCACPE1(,@10)                                   0546
         MVC   RCDCRAVL(6,@10),CCACRAVL(R11)                       0546
*       RCDCRADT=CCACRADT;          /* SET DEV TYPE          @Y30SSRS*/
         MVC   RCDCRADT(4,@10),CCACRADT(R11)                       0547
*       DO;                                                        0548
*                                                                  0548
*         /***********************************************************/
*         /*                                                         */
*         /* CALL TO MODULE IGG0CLAG                                 */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0549
*Z049PAD:                                                          0549
*         CALL IGGPPAD;             /* PUT-ADD RECORD                */
Z049PAD  L     @15,@CV00039                                        0549
         BALR  @14,@15                                             0549
*       END;                        /* PUT ADD                       */
*       CCARBPA=OFF;                /* PUT ADD DONE                  */
         L     @10,CCAURAB(,R11)                                   0551
         NI    CCARBPA(@10),B'11110111'                            0551
*     END;                          /* END ISSUE ADD                 */
*   CCARBWR=OFF;                    /* WRITE DONE                    */
@RC00538 L     @10,CCAURAB(,R11)                                   0553
         NI    CCARBWR(@10),B'11101111'                            0553
*   CALL EXIT;                      /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0554
*   END;                            /* END OF INNER PROCEDURE        */
@EL00009 DS    0H                                                  0555
@EF00009 DS    0H                                                  0555
@ER00009 BR    @14                                                 0555
@PB00009 DS    0H                                                  0556
