         LH    @15,CCASL(,R11)                                     0280
         LR    @14,@15                                             0280
         BCTR  @14,0                                               0280
         LCR   @15,@15                                             0280
         AL    @15,@CF00068                                        0280
         ALR   @15,R11                                             0280
         EX    @14,@SM02897                                        0280
*       CCACPE3=CCACPE3+CCAGOPLN;   /* UPDATE CPE3                   */
         L     @05,CCAURAB(,R11)                                   0281
         L     @15,CCACPE3(,@05)                                   0281
         ALR   @15,@10                                             0281
