         SLR   @15,@10                                             0275
         AL    @15,@TF00001                                        0275
         EX    @14,@SM02893                                        0275
*         R2=R2-CCAGOPLN;           /* MOVE NEXT PIECE               */
         SLR   R2,@10                                              0276
*       END;                        /* END MOVE                      */
@DE00274 LH    @10,CCAGOPLN(,R11)                                  0277
         CR    R2,@10                                              0277
