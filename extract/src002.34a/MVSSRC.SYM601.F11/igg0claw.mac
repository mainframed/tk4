*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**   MODULE NAME =  IGGPGNEX(IGG0CLAW)                                *
**                                                                    *
**   DESCRIPTIVE NAME =  GET NEW EXTENSION                            *
**                                                                    *
**   FUNCTION =                                                       *
**      THIS SUB-FUNCTION GETS AN AVAILABLE  RECORD AREA BLOCK AND    *
**      FORMATS A NEW EXTENSION RECORD IN IT.  IN ADDITION, IF THE    *
**      RECORD AREA CHOSEN CONTAINS A CATALOG RECORD TO BE WRITTEN,   *
**      THIS I/O IS PERFORMED BEFORE THE RECORD AREA IS USED.         *
**                                                                    *
**   INPUT =                                                          *
**      IGGCCA INPUT:                                                 *
**         CCACD1 - IF NON-ZERO, AN IMMEDIATE RETURN IS MADE          *
**         CCAASCI(1) - CONTROL INTERVAL NUMBER ASSIGNED FOR NEW      *
**                      EXTENSION RECORD.                             *
**                                                                    *
**   OUTPUT =                                                         *
**      IGGCCA OUTPUT:                                                *
**         CCAURAB - ADDRESS OF RAB CONTAINING NEWLY FORMATTED        *
**                   EXTENSION RECORD.                                *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0160
*   IF CCACD1^=RCS THEN                                            0160
         CLI   CCACD1(R11),0                                       0160
         BNE   @RT00160                                            0160
*     ;                             /* IF ERROR, DONT PROCESS        */
*   ELSE                            /* NO ERROR                      */
*     DO;                           /* INITLZ EXT RCD                */
*       CCAURAB=CCARABB;            /* GET BASE RCD AREA             */
         L     @10,CCARABB(,R11)                                   0163
         ST    @10,CCAURAB(,R11)                                   0163
*       IF EXTGOCI=CCAASCI(1) THEN                                 0164
         L     @10,CCACPE2(,@10)                                   0164
         CLC   EXTGOCI(3,@10),CCAASCI(R11)                         0164
         BE    @RT00164                                            0164
*         ;                         /* CHECK IF BASE EXTENSION       */
*       ELSE                        /* NOT BASE EXTENSION            */
*         DO;                       /* CHECK FOR FREE BUFFER         */
*           CCAURAB=CCARABF;        /* GET FIRST RCD AREA PTR        */
         L     @10,CCARABF(,R11)                                   0167
         ST    @10,CCAURAB(,R11)                                   0167
*           CCAEGREC=OFF;           /* START LOOP                    */
         NI    CCAEGREC(R11),B'11111110'                           0168
*           DO WHILE(CCAURAB<=CCARABL&/* FIND FREE BUFFER FOR NEW    */
*                 CCAEGREC=OFF);    /* EXTENSION                     */
         B     @DE00169                                            0169
@DL00169 DS    0H                                                  0170
*             IF CCARBUR=OFF&       /* FIND NON-BASE BUFFER          */
*                 CCARBU1=OFF&      /* AVAILABLE FOR USE             */
*                 CCARBU2=OFF&CCAURAB^=CCARABB THEN                0170
         L     @10,CCAURAB(,R11)                                   0170
         TM    CCARBUR(@10),B'11100000'                            0170
         BNZ   @RF00170                                            0170
         C     @10,CCARABB(,R11)                                   0170
         BE    @RF00170                                            0170
*               DO;                 /* BUFFER AVAILABLE              */
*                 CCAEGREC=ON;      /* INDICATE FOUND                */
         OI    CCAEGREC(R11),B'00000001'                           0172
*               END;                /* END BUFFER                    */
*             ELSE                  /* UNAVAILABLE                   */
*               CCAURAB=CCAURAB+LENGTH(CCARAB);/* GET NEXT RAB       */
         B     @RC00170                                            0174
@RF00170 LA    @10,20                                              0174
         AL    @10,CCAURAB(,R11)                                   0174
         ST    @10,CCAURAB(,R11)                                   0174
*           END;                    /* END FIND                      */
@RC00170 DS    0H                                                  0175
@DE00169 CLC   CCAURAB(4,R11),CCARABL(R11)                         0175
         BH    @DC00169                                            0175
         TM    CCAEGREC(R11),B'00000001'                           0175
         BZ    @DL00169                                            0175
@DC00169 DS    0H                                                  0176
*         END;                      /* END CHECK                     */
*       IF CCARBWR=ON THEN          /* SEE IF RCD AREA MUST BE     0177
*                                      UPDATED                       */
@RT00164 L     @10,CCAURAB(,R11)                                   0177
         TM    CCARBWR(@10),B'00010000'                            0177
         BNO   @RF00177                                            0177
*Z015PREC:                                                         0178
*         CALL IGGPPREC;            /* CALL PUT                      */
Z015PREC BAL   @14,IGGPPREC                                        0178
*       ELSE                                                       0179
*         ;                         /* NO UPDATE NECESSARY           */
@RF00177 DS    0H                                                  0180
*       DO;                         /* INITLZ RCD                    */
*         RFY                                                      0181
*           RCDAR1 BASED(CCACPE1);  /* MAP NEW RECORD                */
*         RCDAR1(1:MXM)=RCDAR1(1:MXM)&&RCDAR1(1:MXM);/* ZERO RCD     */
         L     @10,CCAURAB(,R11)                                   0182
         L     @10,CCACPE1(,@10)                                   0182
         XC    RCDAR1(256,@10),RCDAR1(@10)                         0182
*         RCDAR1(MXM+1:MXRCD)=RCDAR1(MXM+1:MXRCD)&&/* ZERO THE NEW   */
*             RCDAR1(MXM+1:MXRCD);  /* EXTENSION RCD                 */
         XC    RCDAR1+256(249,@10),RCDAR1+256(@10)                 0183
*         RCDKEYTY=LOWKEYTY;        /* SET KEY TYPE                  */
         MVI   RCDKEYTY(@10),X'00'                                 0184
*         RCDCINO=CCAASCI(1);       /* INITLZ CI NUMBER              */
         MVC   RCDCINO(3,@10),CCAASCI(R11)                         0185
*         IF CCAGOPLN=VOLGOPLN THEN /* IF VOLUME ENTRY               */
         CLC   CCAGOPLN(2,R11),@CH00059                            0186
         BNE   @RF00186                                            0186
*           RCDID=VOLEXTID;         /* SET VOLUME ID                 */
         MVI   RCDID(@10),C'W'                                     0187
*         ELSE                      /* CATALOG ENTRY                 */
*           RCDID=CATEXTID;         /* SET CATALOG ID                */
         B     @RC00186                                            0188
@RF00186 L     @10,CCAURAB(,R11)                                   0188
         L     @10,CCACPE1(,@10)                                   0188
         MVI   RCDID(@10),C'E'                                     0188
*         RCDSIZ=LENGTH(CATRCD)+LENGTH(REPCNTRL);/* RCD SIZE         */
@RC00186 L     @10,CCAURAB(,R11)                                   0189
         L     @05,CCACPE1(,@10)                                   0189
         MVC   RCDSIZ(2,@05),@CH02875                              0189
*         NREPLF=LENGTH(CATRCD);    /* NON-REPEATING LEN FIXED       */
         LA    @15,49                                              0190
         STC   @15,NREPLF(,@05)                                    0190
*         NREPCV=0;                 /* NON-REPEATING CNT VAR         */
         MVI   NREPCV(@05),X'00'                                   0191
*         CCACPE2=CCACPE1+NREPLF;   /* REPEATING CNTRL INFO PTR      */
         ALR   @05,@15                                             0192
         ST    @05,CCACPE2(,@10)                                   0192
*         CCACPE3=CCACPE2+LENGTH(REPCNTRL);/* PTR FIRST POSSIBLE   0193
*                                      OCCURRENCE                    */
         AL    @05,@CF00059                                        0193
         ST    @05,CCACPE3(,@10)                                   0193
*         CCACPE4=CCACPE3;          /* END OF RECORD                 */
         ST    @05,CCACPE4(,@10)                                   0194
*         CCARBWR=ON;               /* THIS RECORD MUST BE WRITTEN   */
*         CCARBPA=ON;               /* WRITE BY PUT ADD              */
         OI    CCARBWR(@10),B'00011000'                            0196
*       END;                        /* END INITLZ RCD                */
*     END;                          /* END INITLZ EXT RCD            */
*   CALL EXIT;                      /* RETURN TO CALLER              */
@RT00160 BAL   @14,EXIT                                            0199
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */
         DS 0F
*   GEN(DC CL8'IGGPGREL');          /* NAME OF FOLLOWING PROCEDURE   */
         DC CL8'IGGPGREL'
*   END;                            /* END OF INNER PROCEDURE        */
@EL00003 DS    0H                                                  0202
@EF00003 DS    0H                                                  0202
@ER00003 BR    @14                                                 0202
@PB00003 DS    0H                                                  0203
