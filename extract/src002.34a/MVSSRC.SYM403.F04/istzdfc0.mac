*/********************************************************************/
*/*                                                                  */
*/* SEGMENT-                                                         */
*/*      MOVE                                                        */
*/*                                                                  */
*/* FUNCTION-                                                        */
*/*      THIS SEGMENT IS CALLED FOR EACH LCCW TO MOVE THE DATA IN    */
*/*      THE LCCW TO THE DATA BUFFER.                                */
*/*                                                                  */
*/********************************************************************/
*                                                                  0609
*   /*****************************************************************/
*   /*                                                               */
*   /* SEGMENT(MOVE)START                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0609
*   DO;                                                            0609
*     GOTO MOVE@;                                                  0610
*MOVE:                                                             0611
*   END;                            /* MOVE: SUBROUTINE              */
MOVE     DS    0H                                                  0612
*   IF LCCWCNT=0 THEN                                              0612
         L     @14,LSCLCCWA(,LCPBREG)                              0612
         LH    @14,LCCWCNT(,@14)                                   0612
         N     @14,@CF04478                                        0612
         LTR   @14,@14                                             0612
         BZ    @RT00612                                            0612
*     ;                             /* IF DATA COUNT IS ZERO @ZA08950*/
*/*   THE FOLLOWING LINE DELETED BY....                      @ZA29724*/
*/*   RTNCD=SKIPMOVE;               ** INDICATE NO DATA MOVE @ZA08950*/
*                                                                  0614
*   ELSE                            /* IF DATA EXISTS        @ZA08950*/
*     DO;                           /* PERFORM THE DATA MOVE @ZA29724*/
*       ZCRMDATA=ON;                /* INDICATE THAT SOME DATA HAS 0615
*                                      BEEN MOVED            @ZA29724*/
         OI    ZCRMDATA(ZCRREG),B'00000001'                        0615
*       IF LCCWNTVC=OFF             /* LCCW DATA TO BE VALIDATED   0616
*                                                            @XM07391*/
*           &LCCWIDAT=OFF THEN      /* AND DATA NOT IN LCCW  @XL03HEN*/
*                                                                  0616
         L     @14,LSCLCCWA(,LCPBREG)                              0616
         TM    LCCWNTVC(@14),B'00011000'                           0616
         BNZ   @RF00616                                            0616
*         /***********************************************************/
*         /*                                                         */
*         /* SEGMENT(VALIDAT)REQUEST                                 */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0617
*         DO;                                                      0617
*           GOTO VALIDAT;                                          0618
         B     VALIDAT                                             0618
*VALIDAT#:                                                         0619
*         END;                      /* VALIDATE LCCW DATA            */
*       ELSE                                                       0620
*         ;                         /* IF NO NEED TO VALIDATE        */
@RF00616 DS    0H                                                  0621
*       IF RTNCD=SUCCESS THEN                                      0621
@RC00616 LTR   RTNCD,RTNCD                                         0621
         BNZ   @RF00621                                            0621
*         DO;                       /* IF USER DATA VALIDATED        */
*           IF LCCWIDAT=ON THEN     /* IF DATA IS IN LCCW    @XL03HEN*/
         L     @14,LSCLCCWA(,LCPBREG)                              0623
         TM    LCCWIDAT(@14),B'00001000'                           0623
         BNO   @RF00623                                            0623
*             ZCRFBPTR=ADDR(LCCWDATA);/* SET A(FROMBUFF)     @XL03HEN*/
         LA    @14,LCCWDATA(,@14)                                  0624
         ST    @14,ZCRFBPTR(,ZCRREG)                               0624
*           ELSE                    /* IF DATA ADDRESSED BY LCCW     */
*             ZCRFBPTR=LCCWDATA;    /* GET A(FROMBUFF)               */
         B     @RC00623                                            0625
@RF00623 L     @14,LSCLCCWA(,LCPBREG)                              0625
         L     @14,LCCWDATA(,@14)                                  0625
         ST    @14,ZCRFBPTR(,ZCRREG)                               0625
*           ZCRMLNG=LCCWCNT;        /* GET LENGTH FOR ISTZDFD0       */
@RC00623 L     @14,LSCLCCWA(,LCPBREG)                              0626
         LH    @14,LCCWCNT(,@14)                                   0626
         N     @14,@CF04478                                        0626
         STH   @14,ZCRMLNG(,ZCRREG)                                0626
*/********************************************************************
* *                                                                  *
* * IF THE DATA IN THE CURRENT LCCW CAN FIT IN THE BUFFER TO WHICH   *
* * THE DATA IS BEING MOVED, THE CALL TO ISTZDFD0 IS SKIPPED AND THE *
* * MOVE IS DONE IN-LINE.                                            *
* *                                                          @Z40GKWP*
* ********************************************************************/
*                                                                  0627
*           RFY                                                    0627
*             ISTZOBUF BASED(ZCRTBPTR);/* SET BASE FOR ZOBUF @Z40GKWP*/
*           RFY                                                    0628
*             ISTFDB BASED(ADDR(ZOBFDB));/* SET BASE FOR FDB @Z40GKWP*/
*           RFY                                                    0629
*             WORKREG RSTD;         /*                       @Z40GKWP*/
*           WORKREG=ZOBLNG2-((FDBDATAA+FDBDATCT)-ZCRTBPTR)+ZOBHDSIZ;
         L     @11,ZCRTBPTR(,ZCRREG)                               0630
         LH    WORKREG,FDBDATCT(,@11)                              0630
         AL    WORKREG,FDBDATAA(,@11)                              0630
         SLR   WORKREG,@11                                         0630
         LCR   WORKREG,WORKREG                                     0630
         LH    @00,ZOBLNG2(,@11)                                   0630
         N     @00,@CF04478                                        0630
         ALR   WORKREG,@00                                         0630
         AL    WORKREG,@CF03954                                    0630
*                                   /* GET LNGTH OF AREA INTO WHICH
*                                      THE DATA IS TO BE MOVED     0631
*                                                            @Z40GKWP*/
*           IF ZCRMLNG<=WORKREG THEN/* IF THERE IS ENOUGH SPACE IN 0631
*                                      THE BUFFER FOR ALL OF THE DATA
