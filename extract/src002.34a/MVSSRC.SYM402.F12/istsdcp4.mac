*   RFY                                                            0172
*     ISTPHROA BASED(PHR1REG);                                     0172
*   DEVCHPTR=MLCPTR4;               /* SET UP ISTDEVCH PTR           */
         L     DEVCHPTR,MLCPTR4(,MLCAREG)                          0173
*   DEVTCODE=PHRF2VAL(1);           /* PICK UP DEVICE CODE           */
         MVC   DEVTCODE(1,DEVCHPTR),PHRF2VAL+3(PHR1REG)            0174
*   IF DEVTCODE='F2'X THEN          /* IS DEVICE A 2740-2            */
         CLI   DEVTCODE(DEVCHPTR),X'F2'                            0175
         BNE   @RF00175                                            0175
*     DO;                                                          0176
*       DEVTCODE=DEV2740;                                          0177
         MVI   DEVTCODE(DEVCHPTR),X'01'                            0177
*       DEVMCODE=DEVMOD2;                                          0178
         MVI   DEVMCODE(DEVCHPTR),X'01'                            0178
*     END;                                                         0179
*   ELSE                                                           0180
*     DO;                           /* SET RECORD MODE FOR 3270 TERMS*/
         B     @RC00175                                            0180
@RF00175 DS    0H                                                  0181
*       IF DEVTCODE=DEV3275×DEVTCODE=DEV3277×DEVTCODE=DEV3284×DEVTCODE=
*           DEV3286 THEN                                           0181
         CLI   DEVTCODE(DEVCHPTR),X'1C'                            0181
         BE    @RT00181                                            0181
         CLI   DEVTCODE(DEVCHPTR),X'19'                            0181
         BE    @RT00181                                            0181
         CLI   DEVTCODE(DEVCHPTR),X'1A'                            0181
         BE    @RT00181                                            0181
         CLI   DEVTCODE(DEVCHPTR),X'1B'                            0181
         BNE   @RF00181                                            0181
@RT00181 DS    0H                                                  0182
*         MLCPTR3->RCPRRECD='1'B;                                  0182
         L     @10,MLCPTR3(,MLCAREG)                               0182
         OI    RCPRRECD(@10),B'01000000'                           0182
*       ELSE                                                       0183
*         ;                                                        0183
@RF00181 DS    0H                                                  0184
*     END;                                                         0184
*   RC=0;                           /* SET SUCCESSFUL RETURN CODE    */
@RC00175 SLR   RC,RC                                               0185
*   GPR13=RNCSAV2(2);                                              0186
         L     GPR13,RNCSAV2+4(,RNCAREG)                           0186
*   RETURN CODE(RC);                                               0187
         LR    @15,RC                                              0187
         L     @14,12(,@13)                                        0187
         LM    @00,@12,20(@13)                                     0187
         BR    @14                                                 0187
