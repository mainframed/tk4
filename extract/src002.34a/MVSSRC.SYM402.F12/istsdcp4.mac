*   RNCSAV1(3)=GPR13;               /* FORWARD POINT                 */
         ST    GPR13,RNCSAV1+8(,RNCAREG)                           0169
*   DATDREG=ADDR(RNCDAT2);          /* GET OUR DATA AREA             */
