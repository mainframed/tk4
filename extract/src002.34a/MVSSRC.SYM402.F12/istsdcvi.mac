CVIPSUBP DS    0H                                                  0363
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : CVIPSUBP -- SUBPARM HANDLER                      */
*/*                                                                  */
*/*  FUNCTION : THIS ROUTINE GETS THE STORAGE BLOCK FOR VIDDATA      */
*/*             IT THEN CALLS CVIPANLZ -SUBPARM ANAALYZER ROUTINE    */
*/*             TO CHECK VALIDITY OF BSCID AND TERMNAME AND BUILD    */
*/*             THE SKELETON VIDDATA.                                */
*/*                                                                  */
*/*  INPUT : R2 - PTR TO PHR OUTPUT AREA                             */
*/*          R3 - PTR TO VIDHDR                                      */
*/*          R5 - PTR TO MLCA                                        */
*/*          R6 - PTR TO RNCA                                        */
*/*                                                                  */
*/*  OUTPUT: BUILDSW =ON =SUCCESSFUL BUILD                           */
*/*                   OFF : ERROR IN PARAM                           */
*/*                                                                  */
*/*  ROUTINES CALLED: ISTSDCC7 - WORK AREA HANDLER (EXTERNAL)        */
*/*                   CVIPANLZ - SUBPARM ANALYZER  (INTERNAL)        */
*/*                   CVIPQDAT - QUEUE DATA BLOCK  (INTERNAL)        */
*/*  MACRO USED : ERRFMT                                             */
*/********************************************************************/
*                                                                  0363
*   SRETSAVE=GPR14;                 /* SAVE CALLER'S RET ADDR        */
         ST    GPR14,SRETSAVE                                      0363
*   IF VALIDBR=ON                   /* IS THIS ROUTINE ENTERED       */
*     THEN                          /* BY A CALL FROM VIDSEQ PARM    */
         TM    VALIDBR,B'00001000'                                 0364
         BNO   @RF00364                                            0364
*CVILVLBR:                                                         0365
*     DO;                           /* HANDLER ? YES,CONTINUE        */
CVILVLBR DS    0H                                                  0366
*       BUILDSW=OFF;                /* INIT BUILDSW TO OFF           */
         NI    BUILDSW,B'01111111'                                 0366
*/********************************************************************/
*/* IF NO. OF SUB-PARAMS IS TWO,IT IS CORRECT,SO CALL WORK AREA      */
*/* HANDLER TO GET STORAGE FOR VIDDATA BLOCK                         */
*/********************************************************************/
*                                                                  0367
*       IF PHRF5NSP=2               /* NO OF SUB-PARAMS=2 ?          */
*         THEN                      /* YES,CONTINUE PROCESSING       */
         CLC   PHRF5NSP(4,PHRREG),@CF00096                         0367
         BNE   @RF00367                                            0367
*CVILSUB2:                                                         0368
*         DO;                       /* GET STORAGE-BUILD DATA BLK    */
CVILSUB2 DS    0H                                                  0369
*           GPR0=LENGTH(ISTDDTVI);  /* GET LEN OF VIDDATA BLOCK      */
         LA    GPR0,44                                             0369
*           GPR1=0;                 /* R1 =0                         */
         SLR   GPR1,GPR1                                           0370
*           CALL ISTSDCC7;          /* CALL WORK AREA HANDLER        */
         L     @15,@CV01223                                        0371
         BALR  @14,@15                                             0371
*/********************************************************************/
*/* STORAGE BLOCK OBTAINED FOR SKELETON VIDDATA                      */
*/********************************************************************/
*                                                                  0372
*           IF GPR15=0              /* RC =0 ?                       */
*             THEN                                                 0372
         LTR   GPR15,GPR15                                         0372
         BNZ   @RF00372                                            0372
*             DO;                   /* YES,SUCCESSFUL                */
*               DDTVIREG=GPR1;      /* SET UP VIDDATA PTR            */
         LR    DDTVIREG,GPR1                                       0374
*               ISTDDTVI=ISTDDTVI&&ISTDDTVI;/* ZERO DATA BLK         */
         XC    ISTDDTVI(44,DDTVIREG),ISTDDTVI(DDTVIREG)            0375
*               CALL CVIPANLZ;      /* CALL SUBPARM ANALYZER         */
         BAL   @14,CVIPANLZ                                        0376
*/********************************************************************/
*/* SUBPARM ANALYZER WILL CHECK VALIDITY OF BSCID AND TERM NAME AND  */
*/* ENTER THEM INTO VIDDATA BLOCK                                    */
*/********************************************************************/
*                                                                  0377
*               IF BUILDSW=ON       /* PARAMS VALID ?                */
*                 THEN                                             0377
         TM    BUILDSW,B'10000000'                                 0377
         BNO   @RF00377                                            0377
*                 CALL CVIPQDAT;    /* YES,QUEUE VIDDATA BLK         */
         BAL   @14,CVIPQDAT                                        0378
*               ELSE                /* ERROR IN PARAM-SEND MSG       */
*/* ERRFMT    LEVEL(VALUE)  ERROR(INVAL)  ACTION(IGNORE)  STRING('vidse
*')                                                                  */
*                                                                  0379
*                 DO;                                              0379
         B     @RC00377                                            0379
@RF00377 DS    0H                                                  0380
*                   DCL                                            0380
*                     1 CMZP0014 STATIC LOCAL,/* PARAMETER LIST    0380
*                                      PASSED TO ERROR FORMAT ROUTINE*/
*                      2 CMZL0014 FIXED(8) INIT(5),/* LEVEL PARAMETER
*                                      (CLEV)                        */
*                      2 CMZE0014 FIXED(8) INIT(4),/* ERROR PARAMETER
*                                      (CERR)                        */
*                      2 CMZA0014 CHAR(1) INIT('1'),/* ACTION      0380
*                                      PARAMETER (CACT)              */
*                      2 CMZS0014 FIXED(8) INIT(6),/* LENGTH OF    0380
*                                      STRING PARAM                  */
*                      2 CMZM0014 CHAR(6) INIT('vidseq');/* PARAMETER
*                                      STRING                        */
*                   DCL                                            0381
*                     CMZR0014 REGISTER(1);/* ESTABLISH PARAMeter    */
*                   RESPECIFY                                      0382
*                     CMZR0014 RSTD;/* REGISTER FOR                  */
*                   CMZR0014=ADDR(CMZP0014);/* ADDRESS OF PARAMETER
*                                      LIST                          */
         LA    CMZR0014,CMZP0014                                   0383
*                   CALL ISTSDCC2;  /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01277                                        0384
         BALR  @14,@15                                             0384
*                   RESPECIFY                                      0385
*                     CMZR0014 UNRSTD;                             0385
*                 END;                                             0386
*             END;                                                 0387
*           ELSE                                                   0388
*             ;                     /* RC FROM WORK AREA HANDLER IS
*                                      NOT ZERO,SEND ERROR MSG       */
@RF00372 DS    0H                                                  0389
*/********************************************************************/
*/* NO STORAGE AVAILABLE FOR VIDDATA                                 */
*/********************************************************************/
*                                                                  0389
*         END CVILSUB2;             /* END SUBPARM=2 LEG             */
*       ELSE                                                       0390
*                                                                  0390
*/********************************************************************/
*/* NO. OF SUBPARAMS IS NOT TWO -ERROR MSG                           */
*/********************************************************************/
*/* ERRFMT    LEVEL(VALUE)  ERROR(INVAL)  ACTION(IGNORE)  STRING('vidse
*')                                                                  */
*                                                                  0390
*         DO;                                                      0390
         B     @RC00367                                            0390
@RF00367 DS    0H                                                  0391
*           DCL                                                    0391
*             1 CMZP0015 STATIC LOCAL,/* PARAMETER LIST PASSED TO  0391
*                                      ERROR FORMAT ROUTINE          */
*              2 CMZL0015 FIXED(8) INIT(5),/* LEVEL PARAMETER (CLEV) */
*              2 CMZE0015 FIXED(8) INIT(4),/* ERROR PARAMETER (CERR) */
*              2 CMZA0015 CHAR(1) INIT('1'),/* ACTION PARAMETER    0391
*                                      (CACT)                        */
*              2 CMZS0015 FIXED(8) INIT(6),/* LENGTH OF STRING PARAM */
*              2 CMZM0015 CHAR(6) INIT('vidseq');/* PARAMETER STRING */
*           DCL                                                    0392
*             CMZR0015 REGISTER(1); /* ESTABLISH PARAMeter           */
*           RESPECIFY                                              0393
*             CMZR0015 RSTD;        /* REGISTER FOR                  */
*           CMZR0015=ADDR(CMZP0015);/* ADDRESS OF PARAMETER LIST     */
         LA    CMZR0015,CMZP0015                                   0394
*           CALL ISTSDCC2;          /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01277                                        0395
         BALR  @14,@15                                             0395
*           RESPECIFY                                              0396
*             CMZR0015 UNRSTD;                                     0396
*         END;                                                     0397
*     END CVILVLBR;                 /* END NORMAL CASE               */
*/********************************************************************/
*/* USER HAS XFF AS A KEYWORD PARAM = INVALID PARAM,SEND ERROR MSG   */
*/********************************************************************/
*                                                                  0399
*   ELSE                                                           0399
*                                                                  0399
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */
*                                                                  0399
*     DO;                                                          0399
         B     @RC00364                                            0399
@RF00364 DS    0H                                                  0400
*       DCL                                                        0400
*         1 CMZP0016 STATIC LOCAL,  /* PARAMETER LIST PASSED TO ERROR
*                                      FORMAT ROUTINE                */
*          2 CMZL0016 FIXED(8) INIT(4),/* LEVEL PARAMETER (CLEV)     */
*          2 CMZE0016 FIXED(8) INIT(4),/* ERROR PARAMETER (CERR)     */
*          2 CMZA0016 CHAR(1) INIT('1'),/* ACTION PARAMETER (CACT)   */
*          2 CMZS0016 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM     */
*       DCL                                                        0401
*         CMZR0016 REGISTER(1);     /* ESTABLISH PARAMeter           */
*       RESPECIFY                                                  0402
*         CMZR0016 RSTD;            /* REGISTER FOR                  */
*       CMZR0016=ADDR(CMZP0016);    /* ADDRESS OF PARAMETER LIST     */
         LA    CMZR0016,CMZP0016                                   0403
*       CALL ISTSDCC2;              /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01277                                        0404
         BALR  @14,@15                                             0404
*       RESPECIFY                                                  0405
*         CMZR0016 UNRSTD;                                         0405
*     END;                                                         0406
*   GPR14=SRETSAVE;                 /* RESTORE RETURN ADDR           */
@RC00364 L     GPR14,SRETSAVE                                      0407
*   RETURN;                                                        0408
@EL00004 DS    0H                                                  0408
@EF00004 DS    0H                                                  0408
@ER00004 BR    @14                                                 0408
*CVIPANLZ:                                                         0409
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0409
*                                                                  0409
CVIPANLZ DS    0H                                                  0410
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : CVIPANLZ -- SUBPARM ANALYZER                     */
*/*                                                                  */
*/*  FUNCTION : THIS ROUTINE CHECKS VALIDITY OF BSCID AND TERM NAME  */
*/*             AND CALLS EBCHEX TO TRANSLATE BSCID TO HEX REPRESENT-*/
*/*             ATION. IF VALID,SKELETON VIDDATA BLOCK IS BUILT.     */
*/*                                                                  */
*/*  INPUT : R2 -  PTR TO PHR OUTPUT AREA                            */
*/*          R3 -  PTR TO VIDHDR                                     */
*/*          R4 -  PTR TO VIDDATA BLOCK TO BE BUILT                  */
*/*          R5 -  PTR TO MLCA                                       */
*/*          R6 -  PTR TO RNCA                                       */
*/*          BUILDSW=OFF,SET BY CVIPSUBP ROUTINE                     */
*/*                                                                  */
*/*  OUTPUT: BUILDSW =ON ,SUCCESSFUL BUILD                           */
*/*                   OFF,INVALID BSCID OR TERM NAME                 */
*/*                                                                  */
*/*  ROUTINES CALLED : ISTSDCC4  -EBCHEX     (EXTERNAL)              */
*/*                                                                  */
*/*  MACRO USED:  ERRFMT                                             */
*/********************************************************************/
*                                                                  0410
*   ARETSAVE=GPR14;                 /* SAVE CALLER'S RET ADDR        */
         LR    ARETSAVE,GPR14                                      0410
*/********************************************************************/
*/* BSCID IS PUNCHED ON CARDS IN HEX REPRESENTATION OF ITS EBCDIC    */
*/* CHARS WHICH IS AGAIN TRANSLATED INTO EBCDIC CODE BY THE TIME IT  */
*/* RESIDES IN CORE -EXAMPLE: BSCID IS 'A'. IT IS PUNCHED AS 'C1' ON */
*/* CARD. IN CORE,IT IS REPRESENTED AS X'C3F1'.                      */
*/********************************************************************/
*/* CHECK VALIDITY OF BSCID                                          */
*/********************************************************************/
*                                                                  0411
*   IF PHRF5SL(1)//2=0              /* IS BSCID LEN AN EVEN NO.      */
*       &PHRF5SL(1)>1               /* AND >1 AND ^>40               */
*       &PHRF5SL(1)^>40             /* AND ALL CHARS ALPHANUM ?      */
*       &(PHRF5VF(1)=PHRVFAOKPHRF5VF(1)=PHRVFNA1)/* YES- CONTINUE 0411
*                                      PROCESSING                    */
*     THEN                                                         0411
*                                                                  0411
         LH    @10,PHRF5SL(,PHRREG)                                0411
         LR    @08,@10                                             0411
         SRDA  @08,32                                              0411
         D     @08,@CF00096                                        0411
         LTR   @08,@08                                             0411
         BNZ   @RF00411                                            0411
         C     @10,@CF00042                                        0411
         BNH   @RF00411                                            0411
         C     @10,@CF00061                                        0411
         BH    @RF00411                                            0411
         CLI   PHRF5VF(PHRREG),X'00'                               0411
         BE    @RT00411                                            0411
         CLI   PHRF5VF(PHRREG),X'10'                               0411
         BNE   @RF00411                                            0411
@RT00411 DS    0H                                                  0412
*/********************************************************************/
*/* CALL EBCHEX TO TRANSLATE BSCID BACK TO HEX REPRESENTATION OF ITS */
*/* EBCDIC CHARS,EX. TRANSLATE X'C3F1' TO X'C1' WHICH IS HEX OF C'A' */
*/********************************************************************/
*                                                                  0412
*CVILVAID:                                                         0412
*     DO;                                                          0412
CVILVAID DS    0H                                                  0413
*       GPR1=PHRF5SPT(1);           /* PTR TO SUBPARAM               */
         L     GPR1,PHRF5SPT(,PHRREG)                              0413
*       GPR0=PHRF5SL(1);            /* LENGTH OF SUBPARAM            */
         LH    GPR0,PHRF5SL(,PHRREG)                               0414
*       CALL ISTSDCC4;              /* CALL EBCHEX                   */
         L     @15,@CV01224                                        0415
         BALR  @14,@15                                             0415
*       IF GPR15=0                  /* RC=0? SUCCESSFUL              */
*         THEN                      /* YES BUILD SKEL VIDDATA        */
         LTR   GPR15,GPR15                                         0416
         BNZ   @RF00416                                            0416
*CVILBULD:                                                         0417
*         DO;                                                      0417
*                                                                  0417
CVILBULD DS    0H                                                  0418
*/********************************************************************/
*/* MOVE BSCID TO VIDDATA -R0 HAS LEN,R1 POINTS TO TRANSLATED BSCID  */
*/********************************************************************/
*                                                                  0418
*           DDTBSCLN=GPR0;          /* MOVE LEN OF BSCID TO VIDAT    */
         STC   GPR0,DDTBSCLN(,DDTVIREG)                            0418
*           DDTBSCID=TRANID(1:GPR0);/* MOVE BSCID TO VIDDATA TRANID
*                                      IS PTRED BY R1                */
         MVI   DDTBSCID+1(DDTVIREG),C' '                           0419
         MVC   DDTBSCID+2(21,DDTVIREG),DDTBSCID+1(DDTVIREG)        0419
         LR    @10,GPR0                                            0419
         BCTR  @10,0                                               0419
         EX    @10,@SM01398                                        0419
*           IF DDTBSCLN>DHDIDLEN    /* L'BSCID > MAX L'BSCID         */
*             THEN                                                 0420
         SLR   @10,@10                                             0420
         IC    @10,DDTBSCLN(,DDTVIREG)                             0420
         LH    @09,DHDIDLEN(,DHDVIREG)                             0420
         N     @09,@CF01400                                        0420
         CR    @10,@09                                             0420
         BNH   @RF00420                                            0420
*             DHDIDLEN=DDTBSCLN;    /* REPLACE MAX LEN               */
         STH   @10,DHDIDLEN(,DHDVIREG)                             0421
*           ELSE                                                   0422
*             ;                                                    0422
*                                                                  0422
@RF00420 DS    0H                                                  0423
*/********************************************************************/
*/* CHECK VALIDITY OF TERM NAME                                      */
*/********************************************************************/
*                                                                  0423
*           IF PHRF5SL(2)>0         /* LEN OF TERM NAME >0           */
*               &PHRF5SL(2)^>8      /* AND ^>8 AND IS A              */
*               &PHRF5VF(2)=PHRVFAOK/* VALID SYMBOLIC NAME           */
*/********************************************************************/
*/* MOVE TERM NAME TO VIDDATA                                        */
*/********************************************************************/
*                                                                  0423
*             THEN                                                 0423
         LH    @10,PHRF5SL+8(,PHRREG)                              0423
         LTR   @10,@10                                             0423
         BNP   @RF00423                                            0423
         C     @10,@CF00048                                        0423
         BH    @RF00423                                            0423
         CLI   PHRF5VF+8(PHRREG),X'00'                             0423
         BNE   @RF00423                                            0423
*             DO;                   /* YES,MOVE                      */
*               BUILDSW=ON;         /* SET BUILSW ON=SUCCESS         */
         OI    BUILDSW,B'10000000'                                 0425
*               DDTTRNM=TERMNAME(1:PHRF5SL(2));/* TERMN TO VID       */
         MVI   DDTTRNM+1(DDTVIREG),C' '                            0426
         MVC   DDTTRNM+2(6,DDTVIREG),DDTTRNM+1(DDTVIREG)           0426
         BCTR  @10,0                                               0426
         L     @09,PHRF5SPT+8(,PHRREG)                             0426
         EX    @10,@SM01401                                        0426
*               DHDNUME=DHDNUME+1;  /* INCREMENT NO. OF VIDDATA    0427
*                                      ENTRIES                       */
         LA    @10,1                                               0427
         LH    @09,DHDNUME(,DHDVIREG)                              0427
         N     @09,@CF01400                                        0427
         ALR   @10,@09                                             0427
         STH   @10,DHDNUME(,DHDVIREG)                              0427
*             END;                                                 0428
*           ELSE                                                   0429
*             ;                     /* INVALID TERM NAME             */
@RF00423 DS    0H                                                  0430
*         END CVILBULD;             /* END BUILD OF SKEL VIDDATA     */
*       ELSE                                                       0431
*         ;                         /* RC FROM EBCHEX NOT 0          */
@RF00416 DS    0H                                                  0432
