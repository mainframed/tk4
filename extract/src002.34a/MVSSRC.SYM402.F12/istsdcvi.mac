*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : CVIPQDAT -- QUEUE DATA BLOCK                     */
*/*                                                                  */
*/*  FUNCTION : THIS ROUTINE QUEUES THE SKELETON VIDDATA BLOCK TO    */
*/*             THE VIDHDR IN ASCENDING ORDER OF BSCID.              */
*/*                                                                  */
*/*  INPUT : R3 - PTR TO VIDHDR                                      */
*/*          R4 - PTR TO VIDDATA BLOCK TO BE QUEUED                  */
*/*          R5 - PTR TO MLCA                                        */
*/*          R6 - PTR TO RNCA                                        */
*/*                                                                  */
*/*  OUTPUT: BUILDSW= ON,SUCCESSFUL Q                                */
*/*                   OFF,INVALID PARAM,DUPLICATE BSCID              */
*/*                                                                  */
*/*  REGISTERS USED RSTD: R7 -LSTPTR ,PTR TO LOC OF LSTFWPTR         */
*/*                       R8 -NXTPTR ,PTR TO Q ELEMENT BEING SCANNED */
*/*                                                                  */
*/*  MACRO USED: ERRFMT                                              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0437
*   QRETSAVE=GPR14;                 /* SAVE RETURN ADDR              */
         ST    GPR14,QRETSAVE                                      0437
*   RFY                                                            0438
*     LSTPTR RSTD;                  /* RESPECIFY REGS AS RESTRICT    */
*   RFY                                                            0439
*     NXTPTR RSTD;                                                 0439
*                                                                  0439
*/********************************************************************/
*/* QUEUE VIDDATA BLOCK TO VIDHDR IN ASCENDING ORDER OF BSCID        */
*/********************************************************************/
*                                                                  0440
*   LSTPTR=ADDR(DHDDTPTR);          /* LOC OF VIDDATA PTR IN HDR     */
         LA    LSTPTR,DHDDTPTR(,DHDVIREG)                          0440
*   NXTPTR=DHDDTPTR;                /* PTR TO FIRST VIDDATA BLK      */
         L     NXTPTR,DHDDTPTR(,DHDVIREG)                          0441
*/********************************************************************/
*/* SEARCH LOOP                                                      */
*/********************************************************************/
*                                                                  0442
*CVILSRQU:                                                         0442
*   LOOPSRCH=OFF;                   /* INIT SEARCH LOOP SWITCH       */
CVILSRQU NI    LOOPSRCH,B'11111011'                                0442
*   IF LSTFWPTR=0                   /* END OF CHAIN ?                */
*     THEN                                                         0443
         L     @10,LSTFWPTR(,LSTPTR)                               0443
         LTR   @10,@10                                             0443
         BNZ   @RF00443                                            0443
*     LSTFWPTR=DDTVIREG;            /* YES,Q DATA BLOCK TO END OF  0444
*                                      CHAIN                         */
         ST    DDTVIREG,LSTFWPTR(,LSTPTR)                          0444
*   ELSE                                                           0445
*     IF DDTVIREG->DDTBSCID<NXTPTR->DDTBSCID                       0445
*                                                                  0445
*/********************************************************************/
*/*             IF BSCID OF BLOCK TO BE ADDED IS LESS THAN THAT OF   */
*/*             Q ELEMENT,THEN Q IT IN FRONT OF Q ELEMENT            */
*/********************************************************************/
*                                                                  0445
*       THEN                                                       0445
         B     @RC00443                                            0445
@RF00443 CLC   DDTBSCID(23,DDTVIREG),DDTBSCID(NXTPTR)              0445
         BNL   @RF00445                                            0445
*       DO;                                                        0446
*         LSTFWPTR=DDTVIREG;        /* LAST FWD PTR=ADDR(NEW BLK)    */
         ST    DDTVIREG,LSTFWPTR(,LSTPTR)                          0447
*         DDTVIREG->DDTFWPTR=NXTPTR;/* FWD PTR OF NEW BLOCK          */
         ST    NXTPTR,DDTFWPTR(,DDTVIREG)                          0448
*       END;                        /* PTR TO NEXT Q ELEM            */
*     ELSE                                                         0450
*       IF DDTVIREG->DDTBSCID=NXTPTR->DDTBSCID                     0450
*                                                                  0450
*/********************************************************************/
*/* IF BSCID OF NEW ELEM = BSCID OF Q ELEM - ERROR CONDITION         */
*/********************************************************************/
*                                                                  0450
*         THEN                                                     0450
         B     @RC00445                                            0450
@RF00445 CLC   DDTBSCID(23,DDTVIREG),DDTBSCID(NXTPTR)              0450
         BNE   @RF00450                                            0450
*         DO;                                                      0451
*                                                                  0451
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)  STRING('vidseq
*)                                                                   */
*                                                                  0452
*           DO;                                                    0452
*             DCL                                                  0453
*               1 CMZP0017 STATIC LOCAL,/* PARAMETER LIST PASSED TO
*                                      ERROR FORMAT ROUTINE          */
*                2 CMZL0017 FIXED(8) INIT(4),/* LEVEL PARAMETER    0453
*                                      (CLEV)                        */
*                2 CMZE0017 FIXED(8) INIT(4),/* ERROR PARAMETER    0453
*                                      (CERR)                        */
*                2 CMZA0017 CHAR(1) INIT('1'),/* ACTION PARAMETER  0453
*                                      (CACT)                        */
*                2 CMZS0017 FIXED(8) INIT(6),/* LENGTH OF STRING   0453
*                                      PARAM                         */
*                2 CMZM0017 CHAR(6) INIT('vidseq');/* PARAMETER    0453
*                                      STRING                        */
*             DCL                                                  0454
*               CMZR0017 REGISTER(1);/* ESTABLISH PARAMeter          */
*             RESPECIFY                                            0455
*               CMZR0017 RSTD;      /* REGISTER FOR                  */
*             CMZR0017=ADDR(CMZP0017);/* ADDRESS OF PARAMETER LIST   */
         LA    CMZR0017,CMZP0017                                   0456
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01277                                        0457
         BALR  @14,@15                                             0457
*             RESPECIFY                                            0458
*               CMZR0017 UNRSTD;                                   0458
*           END;                                                   0459
*           BUILDSW=OFF;                                           0460
         NI    BUILDSW,B'01111111'                                 0460
*         END;                                                     0461
*                                                                  0461
*/********************************************************************/
*/* BSCID OF NEW ELEM IS GREATER THAN Q ELEM,BUMP TO NEXT Q ELEM     */
*/********************************************************************/
*                                                                  0462
*       ELSE                                                       0462
*         DO;                                                      0462
         B     @RC00450                                            0462
@RF00450 DS    0H                                                  0463
*           LOOPSRCH=ON;            /* SET SEARCH LOOP SW=ON         */
         OI    LOOPSRCH,B'00000100'                                0463
*           LSTPTR=ADDR(LSTFWPTR->DDTFWPTR);/* LOC OF FWD PTR IN Q 0464
*                                      ELEM                          */
         L     LSTPTR,LSTFWPTR(,LSTPTR)                            0464
*           NXTPTR=NXTPTR->DDTFWPTR;/* BUMP TO NEXT Q ELEM           */
         L     NXTPTR,DDTFWPTR(,NXTPTR)                            0465
*         END;                                                     0466
*                                                                  0466
*/********************************************************************/
*/* TEST LOOP SWITCH TO DECIDE CONTINUE SEARCH OR DONE               */
*/********************************************************************/
*                                                                  0467
*   IF LOOPSRCH=ON                  /* LOOP SWITCH IS ON             */
*     THEN                                                         0467
@RC00450 DS    0H                                                  0467
@RC00445 DS    0H                                                  0467
@RC00443 TM    LOOPSRCH,B'00000100'                                0467
         BO    @RT00467                                            0467
