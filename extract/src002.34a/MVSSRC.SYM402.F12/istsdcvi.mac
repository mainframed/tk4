         ST    GPR1,NXTPAIR                                        0337
*                 IF NXTBYTE=' '    /* PHR RETURNS UPDATED PARM      */
*                   THEN                                           0338
         CLI   NXTBYTE(GPR1),C' '                                  0338
         BNE   @RF00338                                            0338
*                   SBLOOPSW=OFF;   /* STRING PTR POINTS TO A        */
         NI    SBLOOPSW,B'11101111'                                0339
*                 ELSE                                             0340
*                   ;               /* BLANK,VIDSEQ STRING         0340
*                                      EXHAUSTED,GET OUT OF LOOP     */
@RF00338 DS    0H                                                  0341
*                 VALIDBR=ON;       /* SET VALID BRANCH FLAG ON      */
         OI    VALIDBR,B'00001000'                                 0341
*                 GPR15=GPR0;       /* GET KEYWORD ROUTINE ADDR      */
         LR    GPR15,GPR0                                          0342
*                 CALL PARMHDLR;    /* CALL SUB PARAMETER HANDLER    */
         BALR  @14,@15                                             0343
*                 VALIDBR=OFF;      /* CVIPSUBP                      */
         NI    VALIDBR,B'11110111'                                 0344
*               END;                /* SET VALID BR FLAG OFF         */
*             ELSE                                                 0346
*               DO;                 /* RC NOT ZERO-PARAM ERROR       */
         B     @RC00335                                            0346
@RF00335 DS    0H                                                  0347
*/* ERRFMT    LEVEL(VALUE)  ERROR(INVAL)  ACTION(IGNORE)  STRING('vidse
