*             DUMMYPRM='XFF=';      /* SHOULD BE,SET UP DUMMY PARAM
*                                      XFF=(A,B)                     */
         MVC   DUMMYPRM(4,GPR1),@CC01327                           0331
*             GPR0=ADDR(VIDKWTBL);  /* PTR TO KEYWORD TABLE          */
         LA    GPR0,VIDKWTBL                                       0332
