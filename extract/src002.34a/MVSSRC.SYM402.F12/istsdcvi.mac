*                   DCL                                            0349
*                     CMZR0013 REGISTER(1);/* ESTABLISH PARAMeter    */
*                   RESPECIFY                                      0350
*                     CMZR0013 RSTD;/* REGISTER FOR                  */
*                   CMZR0013=ADDR(CMZP0013);/* ADDRESS OF PARAMETER
*                                      LIST                          */
         LA    CMZR0013,CMZP0013                                   0351
*                   CALL ISTSDCC2;  /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01277                                        0352
         BALR  @14,@15                                             0352
*                   RESPECIFY                                      0353
*                     CMZR0013 UNRSTD;                             0353
*                 END;                                             0354
*                 BUILDSW=OFF;      /* TERMINATE PROCESSING          */
         NI    BUILDSW,B'01111111'                                 0355
*               END;                                               0356
*           END CVILSBLP;           /* END OF DO WHILE LOOP          */
@RC00335 DS    0H                                                  0357
@DE00329 TM    SBLOOPSW,B'10010000'                                0357
         BO    @DL00329                                            0357
*         END;                      /* END OF NORMAL CASE            */
