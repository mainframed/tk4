         TITLE 'ISTSDCVI - VTAM SYSTEM DEFINITION VIDLIST PROCESSOR    *
                        '
ISTSDCVI CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ISTSDCVI  75.130'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*                                                                  0192
*/********************************************************************/
*/*                                                                  */
*/*                      VIDLIST PROCESSOR MAINLINE                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0192
*   RNCSAV1(2)=GPR13;               /* SET UP BACKWARD POINTER       */
         ST    GPR13,RNCSAV1+4(,RNCAREG)                           0192
*   GPR13=ADDR(RNCSAV1);            /* SET UP PTR TO OUR SAVEAREA    */
         LA    GPR13,RNCSAV1(,RNCAREG)                             0193
*   MLCSAV3(3)=GPR13;               /* SET UP FORWARD POINTER        */
         ST    GPR13,MLCSAV3+8(,MLCAREG)                           0194
*   DATDREG=ADDR(RNCDAT1);          /* SET UP DATA REG               */
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0195
*   GENERATE NOREFS NOSETS;                                        0196
*                                                                  0196
           USING @DATD,11               ADDRESSABILITY FOR DATA
*/********************************************************************/
*/*               EXAMINE INPUT FOR MACRO LABEL                      */
*/********************************************************************/
*                                                                  0197
*   BUILDSW=ON;                     /* SET BUILD SWITCH ON           */
         OI    BUILDSW,B'10000000'                                 0197
*   IF MLCNAME(1)=' '               /* IF LABEL ON VIDLIST MACRO     */
*     THEN                          /* IS MISSING                    */
         CLI   MLCNAME(MLCAREG),C' '                               0198
         BNE   @RF00198                                            0198
*     IF RNCDIVT=0                  /* NO HDR BLOCK EXISTS YET       */
*       THEN                        /* USER ERROR -FORGOT LABEL      */
         L     @10,RNCDIVT(,RNCAREG)                               0199
         LTR   @10,@10                                             0199
         BNZ   @RF00199                                            0199
*       DO;                         /* ERROR MSG- LABEL MISSING      */
*/* ERRFMT    LEVEL(MACRO)  ERROR(SYN)  ACTION(IGNORE)               */
*                                                                  0201
*         DO;                                                      0201
*           DCL                                                    0202
*             1 CMZP0006 STATIC LOCAL,/* PARAMETER LIST PASSED TO  0202
*                                      ERROR FORMAT ROUTINE          */
*              2 CMZL0006 FIXED(8) INIT(3),/* LEVEL PARAMETER (CLEV) */
*              2 CMZE0006 FIXED(8) INIT(6),/* ERROR PARAMETER (CERR) */
*              2 CMZA0006 CHAR(1) INIT('1'),/* ACTION PARAMETER    0202
*                                      (CACT)                        */
*              2 CMZS0006 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM */
*           DCL                                                    0203
*             CMZR0006 REGISTER(1); /* ESTABLISH PARAMeter           */
*           RESPECIFY                                              0204
*             CMZR0006 RSTD;        /* REGISTER FOR                  */
*           CMZR0006=ADDR(CMZP0006);/* ADDRESS OF PARAMETER LIST     */
         LA    CMZR0006,CMZP0006                                   0205
*           CALL ISTSDCC2;          /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01277                                        0206
         BALR  @14,@15                                             0206
*           RESPECIFY                                              0207
*             CMZR0006 UNRSTD;                                     0207
*         END;                                                     0208
*         BUILDSW=OFF;              /* SET BUILD SWITCH OFF          */
         NI    BUILDSW,B'01111111'                                 0209
*       END;                                                       0210
*     ELSE                                                         0211
*       DHDVIREG=RNCDIVT;           /* NO LABEL,BUT HDR EXIST,OK     */
         B     @RC00199                                            0211
@RF00199 L     DHDVIREG,RNCDIVT(,RNCAREG)                          0211
*/********************************************************************/
*/* NORMAL CASE - LABEL EXISTS ON MACRO - BUILD SKEL VIDHDR          */
*/********************************************************************/
*                                                                  0212
*   ELSE                                                           0212
*     DO;                           /* SET PARAMS FOR STORAGE HANDLER*/
         B     @RC00198                                            0212
@RF00198 DS    0H                                                  0213
*       GPR0=LENGTH(ISTDHDVI);      /* LENGTH OF HDR BLOCK REQED     */
         LA    GPR0,36                                             0213
*       GPR1=0;                     /* R1 CONTAINS ZERO              */
         SLR   GPR1,GPR1                                           0214
*       CALL ISTSDCC7;              /* CALL WORK AREA HANDLER        */
         L     @15,@CV01223                                        0215
         BALR  @14,@15                                             0215
*       IF GPR15=0                  /* RC=0 ?                        */
*         THEN                      /* YES,BUILD SKELETON VIDHDR     */
         LTR   GPR15,GPR15                                         0216
         BNZ   @RF00216                                            0216
*         DO;                                                      0217
*           DHDVIREG=GPR1;          /* SET UP HDR PTR                */
         LR    DHDVIREG,GPR1                                       0218
*           ISTDHDVI=ISTDHDVI&&ISTDHDVI;/* ZERO OUT HDR              */
         XC    ISTDHDVI(36,DHDVIREG),ISTDHDVI(DHDVIREG)            0219
*           DHDFWPTR=RNCDIVT;       /* QUEUE SKEL VIDHDR TO RNCA     */
         L     @10,RNCDIVT(,RNCAREG)                               0220
         ST    @10,DHDFWPTR(,DHDVIREG)                             0220
*           RNCDIVT=GPR1;           /* AS LIFO 0                     */
         ST    GPR1,RNCDIVT(,RNCAREG)                              0221
*           DHDHNAME=MLCNAME;       /* MOVE LABEL OF MACRO TO HDR    */
         MVC   DHDHNAME(8,DHDVIREG),MLCNAME(MLCAREG)               0222
*         END;                                                     0223
*                                                                  0223
*/********************************************************************/
*/* NO STORAGE AVAILABLE FOR VID HEADER BLOCK -ERROR MSG             */
*/********************************************************************/
*                                                                  0224
*       ELSE                                                       0224
*         BUILDSW=OFF;              /* END PROCESSING                */
         B     @RC00216                                            0224
@RF00216 NI    BUILDSW,B'01111111'                                 0224
*     END;                                                         0225
@RC00216 DS    0H                                                  0226
*   IF BUILDSW=ON                   /* WAS HDR BLOCK BUILT OK ?      */
*     THEN                          /* YES-BUILD VIDDATA BLOCK       */
@RC00198 TM    BUILDSW,B'10000000'                                 0226
         BNO   @RF00226                                            0226
*     DO;                                                          0227
*       CALL CVIPDATA;              /* CALL VIDDATA BUILD ROUTINE    */
         BAL   @14,CVIPDATA                                        0228
*       IF BUILDSW=ON               /* WAS DATA BLK BUILT OK ?       */
*         THEN                                                     0229
         TM    BUILDSW,B'10000000'                                 0229
         BO    @RT00229                                            0229
*         ;                         /* YES-EXIT                      */
*       ELSE                                                       0231
*         RNCDIVT=DHDFWPTR;         /* NO -DEQUEUE HDR BLOCK         */
         L     @10,DHDFWPTR(,DHDVIREG)                             0231
         ST    @10,RNCDIVT(,RNCAREG)                               0231
*     END;                                                         0232
*   ELSE                                                           0233
*     ;                                                            0233
@RF00226 DS    0H                                                  0234
*   GPR13=RNCSAV1(2);               /* RESTORE CALLER'S REGS         */
@RC00226 L     GPR13,RNCSAV1+4(,RNCAREG)                           0234
*   RETURN CODE(0);                 /* RETURN TO CALLER              */
         SLR   @15,@15                                             0235
         L     @14,12(,@13)                                        0235
         LM    @00,@12,20(@13)                                     0235
         BR    @14                                                 0235
*CVIPDATA:                                                         0236
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0236
*                                                                  0236
CVIPDATA DS    0H                                                  0237
*/********************************************************************/
*/*  ROUTINE NAME : CVIPDATA -- VIDDATA BLOCK BUILD                  */
*/*                                                                  */
*/*  FUNCTION : THIS ROUTINE SEARCHES THE VIDLIST PARAMETER LIST     */
*/*             FOR THE VIDSEQ KEYWORD PARAMETER. ONCE FOUND,THE     */
*/*             VIDSEQ PARAMETER HANDLER (CVIPVSEQ) IS CALLED TO     */
*/*             ANALYZE THE PARAMETER AND BUILD VIDDATA BLOCK.       */
*/*                                                                  */
*/*  INPUT : R3 - PTR TO VID HEADER BLOCK                            */
*/*          R5 - PTR TO MLCA                                        */
*/*          R6 - PTR TO RNCA                                        */
*/*                                                                  */
*/*  OUTPUT : BUILDSW SWITCH - ON : SUCCESSFUL VIDDATA BUILD         */
*/*                            OFF : ERROR IN PARAMETER LIST OF      */
*/*                                  VIDLIST,VIDHDR SHOULD BE DEQUEUD*/
*/*                                                                  */
*/*  ROUTINES CALLED : ISTINCPH -PHRFCN1,  PHR FUNCTION 1 (EXTERNAL) */
*/*                    CVIPVSEQ -VIDSEQ PARM HANDLER (INTERNAL)      */
*/*                                                                  */
*/*  MACRO USED : ERRFMT                                             */
*/*                                                                  */
*/********************************************************************/
*/*                         INITIALIZE SWITCHES                      */
*/********************************************************************/
*                                                                  0237
*   VIDSEQSW=OFF;                   /* VIDSEQ PARM NOT FOUND YET     */
*   LOOPSW=ON;                      /* INIT LOOP SW TO SEARCH FOR  0238
*                                      VIDSEQ PARM                   */
         OI    LOOPSW,B'00100000'                                  0238
         NI    VIDSEQSW,B'10111111'                                0238
*   NXTKYWRD=MLCKWD;                /* OBTAIN CURRENT KEYWORD PTR    */
         L     @10,MLCKWD(,MLCAREG)                                0239
         ST    @10,NXTKYWRD                                        0239
*   DRETSAVE=GPR14;                 /* SAVE RETURN @ TO CALLER       */
         ST    GPR14,DRETSAVE                                      0240
*/********************************************************************/
*/* CALL PHR FUNCTION 1 TO REMOVE OUTMOST PARENTHESIS IF VIDSEQ      */
*/* KEYWORD IS FOUND.                                                */
*/********************************************************************/
*                                                                  0241
*CVILLOOP:                                                         0241
*   MLCKWD=NXTKYWRD;                /* UPDATE MLCKWD TO NEXT PTR     */
CVILLOOP L     @10,NXTKYWRD                                        0241
         ST    @10,MLCKWD(,MLCAREG)                                0241
*   GPR0=ADDR(VIDKWTBL);            /* PTR TO KEYWORD TABLE          */
         LA    GPR0,VIDKWTBL                                       0242
*   GPR1=NXTKYWRD;                  /* PTR TO CURRENT KEYWORD        */
         LR    GPR1,@10                                            0243
*   PHRREG=ADDR(RNCPHR1);           /* PTR TO PHR OUTPUT AREA        */
         LA    PHRREG,RNCPHR1(,RNCAREG)                            0244
*   CALL ISTINCPH;                  /* CALL PHR ROUTINE -FN1         */
         L     @15,@CV01222                                        0245
         BALR  @14,@15                                             0245
*   NXTKYWRD=GPR1;                  /* SAVE UPDATED STRING PTR       */
         ST    GPR1,NXTKYWRD                                       0246
*/********************************************************************/
*/* ANALYZE RETURN CODES FROM PHR                                    */
*/* RC=0, SUCCESS                                                    */
*/********************************************************************/
*                                                                  0247
*   IF GPR15=0                      /* RC=0,VIDSEQ LOCATED           */
*     THEN                                                         0247
         LTR   GPR15,GPR15                                         0247
         BNZ   @RF00247                                            0247
*     DO;                                                          0248
*       GPR15=GPR0;                 /* R15 PTR TO PARM HANDLER       */
         LR    GPR15,GPR0                                          0249
*       CALL PARMHDLR;              /* CALL VIDSEQ PARM HANDLER-     */
         BALR  @14,@15                                             0250
*     END;                          /* NORMAL CASE                   */
*/********************************************************************/
*/* RC=4,ARG NOT FOUND                                               */
*/********************************************************************/
*                                                                  0252
*   ELSE                                                           0252
*     IF GPR15=4                    /* RC= 4 ?                       */
*       THEN                        /* YES,ARG NOT FOUND             */
         B     @RC00247                                            0252
@RF00247 C     GPR15,@CF00040                                      0252
         BNE   @RF00252                                            0252
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */
*                                                                  0253
*       DO;                                                        0253
*         DCL                                                      0254
*           1 CMZP0007 STATIC LOCAL,/* PARAMETER LIST PASSED TO ERROR
*                                      FORMAT ROUTINE                */
*            2 CMZL0007 FIXED(8) INIT(4),/* LEVEL PARAMETER (CLEV)   */
*            2 CMZE0007 FIXED(8) INIT(4),/* ERROR PARAMETER (CERR)   */
*            2 CMZA0007 CHAR(1) INIT('1'),/* ACTION PARAMETER (CACT) */
*            2 CMZS0007 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM   */
*         DCL                                                      0255
*           CMZR0007 REGISTER(1);   /* ESTABLISH PARAMeter           */
*         RESPECIFY                                                0256
*           CMZR0007 RSTD;          /* REGISTER FOR                  */
*         CMZR0007=ADDR(CMZP0007);  /* ADDRESS OF PARAMETER LIST     */
         LA    CMZR0007,CMZP0007                                   0257
*         CALL ISTSDCC2;            /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01277                                        0258
         BALR  @14,@15                                             0258
*         RESPECIFY                                                0259
*           CMZR0007 UNRSTD;                                       0259
*       END;                                                       0260
*     ELSE                                                         0261
*                                                                  0261
*/********************************************************************/
*/* RC=8, STRING EXHAUSTED                                           */
*/********************************************************************/
*                                                                  0261
*       IF GPR15=8                  /* RC=8 ?                        */
*         THEN                                                     0261
         B     @RC00252                                            0261
@RF00252 C     GPR15,@CF00048                                      0261
         BNE   @RF00261                                            0261
*         LOOPSW=OFF;               /* YES,STRING EXHAUSTED,GET      */
         NI    LOOPSW,B'11011111'                                  0262
*       ELSE                        /* OUT OF SEARCH LOOP            */
*/********************************************************************/
*/* RC=12, SYNTAX ERROR                                              */
*/********************************************************************/
*                                                                  0263
*         DO;                                                      0263
         B     @RC00261                                            0263
@RF00261 DS    0H                                                  0264
*           IF GPR15=12             /* RC=12 ?                       */
*             THEN                  /* YES,SYNTAX ERROR              */
         C     GPR15,@CF00032                                      0264
         BNE   @RF00264                                            0264
*/* ERRFMT    LEVEL(PARM)  ERROR(SYN)  ACTION(IGNORE)                */
*                                                                  0265
*             DO;                                                  0265
*               DCL                                                0266
*                 1 CMZP0008 STATIC LOCAL,/* PARAMETER LIST PASSED TO
*                                      ERROR FORMAT ROUTINE          */
*                  2 CMZL0008 FIXED(8) INIT(4),/* LEVEL PARAMETER  0266
*                                      (CLEV)                        */
*                  2 CMZE0008 FIXED(8) INIT(6),/* ERROR PARAMETER  0266
*                                      (CERR)                        */
*                  2 CMZA0008 CHAR(1) INIT('1'),/* ACTION PARAMETER
*                                      (CACT)                        */
*                  2 CMZS0008 FIXED(8) INIT(0);/* LENGTH OF STRING 0266
*                                      PARAM                         */
*               DCL                                                0267
*                 CMZR0008 REGISTER(1);/* ESTABLISH PARAMeter        */
*               RESPECIFY                                          0268
*                 CMZR0008 RSTD;    /* REGISTER FOR                  */
*               CMZR0008=ADDR(CMZP0008);/* ADDRESS OF PARAMETER LIST */
         LA    CMZR0008,CMZP0008                                   0269
*               CALL ISTSDCC2;      /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01277                                        0270
         BALR  @14,@15                                             0270
*               RESPECIFY                                          0271
*                 CMZR0008 UNRSTD;                                 0271
*             END;                                                 0272
*           ELSE                                                   0273
*                                                                  0273
*/********************************************************************/
*/* RC=16 OR 20 ERROR COND. ANY OTHER RC IS AN ERROR                 */
*/********************************************************************/
*/* ERRFMT    LEVEL(VALUE)  ERROR(INVAL)  ACTION(IGNORE)             */
*                                                                  0273
*             DO;                                                  0273
         B     @RC00264                                            0273
@RF00264 DS    0H                                                  0274
*               DCL                                                0274
*                 1 CMZP0009 STATIC LOCAL,/* PARAMETER LIST PASSED TO
*                                      ERROR FORMAT ROUTINE          */
*                  2 CMZL0009 FIXED(8) INIT(5),/* LEVEL PARAMETER  0274
*                                      (CLEV)                        */
*                  2 CMZE0009 FIXED(8) INIT(4),/* ERROR PARAMETER  0274
*                                      (CERR)                        */
*                  2 CMZA0009 CHAR(1) INIT('1'),/* ACTION PARAMETER
*                                      (CACT)                        */
*                  2 CMZS0009 FIXED(8) INIT(0);/* LENGTH OF STRING 0274
*                                      PARAM                         */
*               DCL                                                0275
*                 CMZR0009 REGISTER(1);/* ESTABLISH PARAMeter        */
*               RESPECIFY                                          0276
*                 CMZR0009 RSTD;    /* REGISTER FOR                  */
*               CMZR0009=ADDR(CMZP0009);/* ADDRESS OF PARAMETER LIST */
         LA    CMZR0009,CMZP0009                                   0277
*               CALL ISTSDCC2;      /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01277                                        0278
         BALR  @14,@15                                             0278
*               RESPECIFY                                          0279
*                 CMZR0009 UNRSTD;                                 0279
*             END;                                                 0280
*           BUILDSW=OFF;            /* FOR RC=>12,TERMINATE PROC     */
@RC00264 DS    0H                                                  0282
*           VIDSEQSW=ON;            /* SET SW TO SKIP NEXT ERROR MSG */
         OI    VIDSEQSW,B'01000000'                                0282
         NI    BUILDSW,B'01111111'                                 0282
*         END;                                                     0283
*                                                                  0283
*/********************************************************************/
*/* ANALYZE OF RC ENDED                                              */
*/* LOOP TO SCAN FOR NEXT KYWRD ONLY IF LOOPSW=ON & BUILDSW=ON       */
*/********************************************************************/
*                                                                  0284
*   IF LOOPSW=ON&                   /* LOOP IF BOTH ARE ON           */
*       BUILDSW=ON                  /* TO SCAN NEXT KYWRD            */
*     THEN                                                         0284
@RC00261 DS    0H                                                  0284
@RC00252 DS    0H                                                  0284
@RC00247 TM    LOOPSW,B'10100000'                                  0284
         BO    @RT00284                                            0284
*     GOTO CVILLOOP;                /* LOOP TO SCAN NEXT KYWRD       */
*/********************************************************************/
*/* GET OUT OF LOOP EITHER BECAUSE BUILDSW=OFF OR STRING EXHAUSTED   */
*/********************************************************************/
*                                                                  0286
*   ELSE                                                           0286
*     IF VIDSEQSW=ON                /* WAS VIDSEQ PARM FOUND?        */
*       THEN                                                       0286
         TM    VIDSEQSW,B'01000000'                                0286
         BO    @RT00286                                            0286
*       ;                           /* YES,RETURN                    */
*     ELSE                                                         0288
*       DO;                         /* NO,ERROR MSG-PARM MISSING     */
*/* ERRFMT    LEVEL(PARM)  ERROR(MISS)  ACTION(IGNORE)               */
*                                                                  0289
*         DO;                                                      0289
*           DCL                                                    0290
*             1 CMZP0010 STATIC LOCAL,/* PARAMETER LIST PASSED TO  0290
*                                      ERROR FORMAT ROUTINE          */
*              2 CMZL0010 FIXED(8) INIT(4),/* LEVEL PARAMETER (CLEV) */
*              2 CMZE0010 FIXED(8) INIT(8),/* ERROR PARAMETER (CERR) */
*              2 CMZA0010 CHAR(1) INIT('1'),/* ACTION PARAMETER    0290
*                                      (CACT)                        */
*              2 CMZS0010 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM */
*           DCL                                                    0291
*             CMZR0010 REGISTER(1); /* ESTABLISH PARAMeter           */
*           RESPECIFY                                              0292
*             CMZR0010 RSTD;        /* REGISTER FOR                  */
*           CMZR0010=ADDR(CMZP0010);/* ADDRESS OF PARAMETER LIST     */
         LA    CMZR0010,CMZP0010                                   0293
*           CALL ISTSDCC2;          /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01277                                        0294
         BALR  @14,@15                                             0294
*           RESPECIFY                                              0295
*             CMZR0010 UNRSTD;                                     0295
*         END;                                                     0296
*         BUILDSW=OFF;              /* TERMINATE PROCESSING          */
         NI    BUILDSW,B'01111111'                                 0297
*       END;                                                       0298
*                                                                  0298
*/********************************************************************/
*/* RETURN TO CALLER                                                 */
*/********************************************************************/
*                                                                  0299
*   GPR14=DRETSAVE;                 /* RESTORE RETURN ADDRESS        */
@RT00286 L     GPR14,DRETSAVE                                      0299
*   RETURN;                         /* RETURN TO CALLER              */
@EL00002 DS    0H                                                  0300
@EF00002 DS    0H                                                  0300
@ER00002 BR    @14                                                 0300
*CVIPVSEQ:                                                         0301
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0301
*                                                                  0301
CVIPVSEQ DS    0H                                                  0302
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : CVIPVSEQ -- VIDSEQ PARM HANDLER                  */
*/*                                                                  */
*/*  FUNCTION : THIS ROUTINE IS BRANCHED TO WHEN VIDSEQ PARAM IS     */
*/*             FOUND. THIS ROUTINE CALLS PHR FUNCTION 5 TO PROCESS  */
*/*             EACH PAIR OF SUBPARAMETERS OF VIDSEQ. IF RC FROM PHR */
*/*             INDICATES SUCCESSFUL RESULTS,THEN THE SUBPARAMETER   */
*/*             HANDLER (CVIPSUBP) IS CALLED TO BUILD A VIDDATA      */
*/*             BLOCK FOR EACH PAIR OF SUB-PARAMS. THIS PROCESS      */
*/*             IS CONTINUED UNTIL VIDSEQ STRING IS EXHAUSTED OR     */
*/*             ERROR IS FOUND AND PROCESS IS TO BE TERMINATED.      */
*/*                                                                  */
*/*  INPUT : PHRREG   (R2) - PTR TO PHR OUTPUT AREA                  */
*/*          DHDVIREG (R3) - PTR TO VIDHDR                           */
*/*          MLCAREG  (R5) - PTR TO MLCA                             */
*/*          RNCAREG  (R6) - PTR TO RNCA                             */
*/*                                                                  */
*/*  OUTPUT : BUILDSW = ON : SUCCESSFUL VIDDATA BUILD                */
*/*                     OFF: ERROR IN PARAM LIST OF VIDLIST          */
*/*                          VIDHDR SHOULD BE DEQUEUED               */
*/*                                                                  */
*/*  ROUTINES CALLED : ISTINCPH - PHR FUNCTION 5 ISOLATES THE VIDSEQ */
*/*                               SUBPARAMETERS                      */
*/*                    CVIPSUBP - SUB PARAM HANDLER (XFF=DUMMY PARAM)*/
*/*                               PROCESSES VIDSEQ SUBPARAMETERS     */
*/*                                                                  */
*/*  MACRO USED : ERRFMT - ISSUES ERROR MESSAGES                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0302
*   VRETSAVE=GPR14;                 /* SAVE RETURN REG               */
         ST    GPR14,VRETSAVE                                      0302
*   IF VIDSEQSW=ON                  /* IF VIDSEQ IS ALREADY FOUND    */
*     THEN                          /* THEN THIS IS DUPLICATE PRM    */
         TM    VIDSEQSW,B'01000000'                                0303
         BNO   @RF00303                                            0303
*/* ERRFMT    LEVEL(PARM)  ERROR(DUP)  ACTION(USEFIRST)              */
*                                                                  0304
*     DO;                                                          0304
*       DCL                                                        0305
*         1 CMZP0011 STATIC LOCAL,  /* PARAMETER LIST PASSED TO ERROR
*                                      FORMAT ROUTINE                */
*          2 CMZL0011 FIXED(8) INIT(4),/* LEVEL PARAMETER (CLEV)     */
*          2 CMZE0011 FIXED(8) INIT(3),/* ERROR PARAMETER (CERR)     */
*          2 CMZA0011 CHAR(1) INIT('4'),/* ACTION PARAMETER (CACT)   */
*          2 CMZS0011 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM     */
*       DCL                                                        0306
*         CMZR0011 REGISTER(1);     /* ESTABLISH PARAMeter           */
*       RESPECIFY                                                  0307
*         CMZR0011 RSTD;            /* REGISTER FOR                  */
*       CMZR0011=ADDR(CMZP0011);    /* ADDRESS OF PARAMETER LIST     */
         LA    CMZR0011,CMZP0011                                   0308
*       CALL ISTSDCC2;              /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01277                                        0309
         BALR  @14,@15                                             0309
*       RESPECIFY                                                  0310
*         CMZR0011 UNRSTD;                                         0310
*     END;                                                         0311
*   ELSE                                                           0312
*     DO;                                                          0312
*                                                                  0312
         B     @RC00303                                            0312
@RF00303 DS    0H                                                  0313
*/********************************************************************/
*/* NORMAL CASE - VIDSEQ KYWRD LOCATED FIRST TIME                    */
*/********************************************************************/
*                                                                  0313
*       VIDSEQSW=ON;                /* SET VIDSEQ TO FOUND           */
         OI    VIDSEQSW,B'01000000'                                0313
*       IF PHRF1SLN>253             /* STRING EXCEED MAX LENGTH      */
*         THEN                      /* (255 INCL BRACKETS)ALLOWD?    */
         L     @10,PHRF1SLN(,PHRREG)                               0314
         C     @10,@CF01318                                        0314
         BNH   @RF00314                                            0314
*         DO;                       /* YES,GIVE ERROR MSG            */
*/* ERRFMT    LEVEL(VALUE)  ERROR(EXTRA)  ACTION(IGNORE)             */
*                                                                  0316
*           DO;                                                    0316
*             DCL                                                  0317
*               1 CMZP0012 STATIC LOCAL,/* PARAMETER LIST PASSED TO
*                                      ERROR FORMAT ROUTINE          */
*                2 CMZL0012 FIXED(8) INIT(5),/* LEVEL PARAMETER    0317
*                                      (CLEV)                        */
*                2 CMZE0012 FIXED(8) INIT(7),/* ERROR PARAMETER    0317
*                                      (CERR)                        */
*                2 CMZA0012 CHAR(1) INIT('1'),/* ACTION PARAMETER  0317
*                                      (CACT)                        */
*                2 CMZS0012 FIXED(8) INIT(0);/* LENGTH OF STRING   0317
*                                      PARAM                         */
*             DCL                                                  0318
*               CMZR0012 REGISTER(1);/* ESTABLISH PARAMeter          */
*             RESPECIFY                                            0319
*               CMZR0012 RSTD;      /* REGISTER FOR                  */
*             CMZR0012=ADDR(CMZP0012);/* ADDRESS OF PARAMETER LIST   */
         LA    CMZR0012,CMZP0012                                   0320
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01277                                        0321
         BALR  @14,@15                                             0321
*             RESPECIFY                                            0322
*               CMZR0012 UNRSTD;                                   0322
*           END;                                                   0323
*           BUILDSW=OFF;            /* TERMINATE PROCESSING          */
         NI    BUILDSW,B'01111111'                                 0324
*         END;                                                     0325
*       ELSE                        /* STRING LENGTH WITHIN LIMIT    */
*         DO;                                                      0326
         B     @RC00314                                            0326
@RF00314 DS    0H                                                  0327
*           NXTPAIR=PHRF1SAD;       /* SET UP PTR TO SUB-PARAM       */
         L     @10,PHRF1SAD(,PHRREG)                               0327
         ST    @10,NXTPAIR                                         0327
*           SBLOOPSW=ON;            /* INITIALIZE SUBLOOP SW CNTL    */
         OI    SBLOOPSW,B'00010000'                                0328
*/********************************************************************/
*/* DO-WHILE LOOP-CONTINUE LOOPING TO PROCESS EACH PAIR OF SUB-PARMS */
*/*                AS LONG AS SBLOOPSW=ON AND BUILDSW=ON             */
*/********************************************************************/
*                                                                  0329
*CVILSBLP:                                                         0329
*           DO WHILE(SBLOOPSW=ON&BUILDSW=ON);                      0329
CVILSBLP B     @DE00329                                            0329
@DL00329 DS    0H                                                  0330
*             GPR1=NXTPAIR-4;       /* PTR GPR1 TO WHERE DUMMY       */
         L     GPR1,NXTPAIR                                        0330
