         LA    PHRREG,RNCPHR1(,RNCAREG)                            0333
*/********************************************************************/
*/* CALL PHR TO PROCESS SUBPARAMS,XFF=(BSCID,TERMNAME)               */
*/********************************************************************/
*                                                                  0334
*             CALL ISTINCPH;        /* CALL PHR FUNCTION 5           */
         L     @15,@CV01222                                        0334
         BALR  @14,@15                                             0334
*             IF GPR15=0            /* RC=0,SUCCESS                  */
*               THEN                                               0335
         LTR   GPR15,GPR15                                         0335
         BNZ   @RF00335                                            0335
*               DO;                                                0336
