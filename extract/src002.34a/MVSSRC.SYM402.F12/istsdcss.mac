*       )-ADDR(MODELDCB)));                                        0372
         LA    @15,MODELEND                                        0372
         LA    @14,MODELDCB                                        0372
         SLR   @15,@14                                             0372
         BCTR  @15,0                                               0372
         EX    @15,@SM03317                                        0372
*   RFY                                                            0373
*     RC RSTD;                                                     0373
*                                                                  0374
*/*VTOPEN FILE((ADRDCB))                                             */
*                                                                  0374
*   DO;                             /* DECLARE AND RESTRICT REG 2    */
*     DCL                                                          0375
*       GPR02F FIXED(31) REG(02);                                  0375
*     DCL                                                          0376
*       GPR02P PTR(31) REG(02);                                    0376
*     RFY                                                          0377
*      (GPR02P) RSTD;               /* INVOKE XREGSET MACRO TO VERIFY
*                                      FILE OPERAND AND PUT IT IN  0377
*                                      REGISTER 2                    */
*/*XREGSET  PID('FILE')PPARM((ADRDCB))RREG(2,GPR02P)                 */
*                                                                  0378
*     DO;                                                          0378
*       GPR02P=ADRDCB;              /* SET RREG SYMBOL TO VALUE OF 0379
*                                      PPARM OPERAND                 */
         LR    GPR02P,ADRDCB                                       0379
*     END;                          /* XREGSET                       */
*     GEN REFS(GPR02P);                                            0381
         OPEN   ((2))
*                                   /* RESTRICT REGISTER 15          */
*     DCL                                                          0382
*       GPR15F FIXED(31) REG(15);                                  0382
*     DCL                                                          0383
*       GPR15P PTR(31) REG(15);                                    0383
*     RFY                                                          0384
*      (GPR15P) RSTD;                                              0384
*     GPR02F=GPR02F+40;             /* ADD OFFSET OF 40 TO DCBPTR TO
*                                      POINT TO IHADCS24 WHICH IS  0385
*                                      BASE FOR DCBOFLGS             */
         AL    GPR02F,@CF00124                                     0385
*     IF GPR02P->DCBOFOPN='1'B THEN /* IF OPEN WAS SUCCESSFUL        */
         TM    DCBOFOPN(GPR02P),B'00010000'                        0386
         BNO   @RF00386                                            0386
*       GPR15F=0;                   /* PUT 0 IN RETURN CODE REG      */
         SLR   GPR15F,GPR15F                                       0387
*     ELSE                          /* IF OPEN WAS UNSUCCESSFUL      */
*       GPR15F=4;                   /* PUT 4 IN RETURN CODE REG    0388
*                                      UNRESTRICT REG 15             */
         B     @RC00386                                            0388
@RF00386 LA    GPR15F,4                                            0388
*     RFY                                                          0389
*      (GPR15P) UNRSTD;             /* UNRESTRICT REG 2              */
@RC00386 DS    0H                                                  0390
*     RFY                                                          0390
*      (GPR02P) UNRSTD;                                            0390
*   END;                            /* VTOPEN OPEN SYS1.VTAMOBJ    0391
*                                                            @ZA29748*/
*   IF RC^=0 THEN                                                  0392
         LTR   RC,RC                                               0392
         BZ    @RF00392                                            0392
*     DO;                                                          0393
*       MSGIDADR=ADDR('304I');                                     0394
         LA    @14,@CC02765                                        0394
         ST    @14,MSGIDADR                                        0394
*       SSRC=4;                                                    0395
         MVC   SSRC(4),@CF00040                                    0395
*     END;                                                         0396
*   ELSE                                                           0397
*                                                                  0397
*/********************************************************************/
*/* OPEN SUCCESSFUL:                                                 */
*/********************************************************************/
*                                                                  0397
*     DO;                                                          0397
         B     @RC00392                                            0397
@RF00392 DS    0H                                                  0398
*       GEN REFS(ADRDCB,MLCCONFG) SETS(MLCDCB,RC,R0,R1,R14)        0398
*           (FIND (ADRDCB),MLCCONFG(,MLCAPTR),D);                  0398
         FIND (ADRDCB),MLCCONFG(,MLCAPTR),D
