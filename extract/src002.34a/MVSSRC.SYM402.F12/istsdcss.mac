*     ISTNCSPL BASED(MLCNCSPL);     /*                       @D32CKQH*/
*   MLCSAV3(2)=BASEREG;                                            0364
         ST    BASEREG,MLCSAV3+4(,MLCAPTR)                         0364
*   SAVAREA(3)=ADDR(MLCSAV3);                                      0365
         LA    @04,MLCSAV3(,MLCAPTR)                               0365
         ST    @04,SAVAREA+8(,BASEREG)                             0365
*   BASEREG=ADDR(MLCSAV3);                                         0366
         LR    BASEREG,@04                                         0366
