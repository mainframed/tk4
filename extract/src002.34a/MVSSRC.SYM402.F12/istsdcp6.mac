*           CMZR0003=ADDR(CMZP0003);/* ADDRESS OF PARAMETER LIST     */
         LA    CMZR0003,CMZP0003                                   0213
*           CALL ISTSDCC2;          /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01220                                        0214
         BALR  @14,@15                                             0214
*           RESPECIFY                                              0215
*             CMZR0003 UNRSTD;                                     0215
*         END;                                                     0216
*       ELSE                                                       0217
