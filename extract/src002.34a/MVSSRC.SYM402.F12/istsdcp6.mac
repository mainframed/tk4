*                                      SDCP6'S                       */
         ST    SAVREG,RNCSAV2+4(,RNCAPTR)                          0176
*   SAVREG=ADDR(RNCSAV2);           /* SET UP ISTSDCP6'S SAVE ADDR   */
         LA    SAVREG,RNCSAV2(,RNCAPTR)                            0177
*   GENERATE REFS(RNCDAT2,RNCSAV2,SAVREG) NOSETS;                  0178
     USING  @DATD-(RNCDAT2-RNCSAV2),SAVREG  ADDRESSABILITY FOR DATA
*   RETADR=GPR14;                                                  0179
         LR    RETADR,GPR14                                        0179
*   IF LPACE=0                      /* PACING?                       */
*     THEN                                                         0180
         LTR   LPACE,LPACE                                         0180
         BNZ   @RF00180                                            0180
*     DO;                                                          0181
*       RLUNCPPN=0;                 /* DEFAULT TO (0,0) FOR PACING   */
         L     @15,MLCPTR6(,MLCAPTR)                               0182
         MVI   RLUNCPPN(@15),X'00'                                 0182
*       RLUNCPPM=0;                                                0183
         MVI   RLUNCPPM(@15),X'00'                                 0183
*     END;                                                         0184
*   ELSE                                                           0185
*     DO;                                                          0185
         B     @RC00180                                            0185
@RF00180 DS    0H                                                  0186
*       IF RNCGLSBD='1'B            /* LOCAL SUBAREA VPACING DFLTS 0186
*                                      (1,1)                 @D32CKQH*/
*         THEN                                                     0186
         TM    RNCGLSBD(RNCAPTR),B'00000001'                       0186
         BNO   @RF00186                                            0186
*         RLUPACEN=1;               /*                       @D32CKQH*/
         L     @15,MLCPTR6(,MLCAPTR)                               0187
         MVI   RLUPACEN(@15),X'01'                                 0187
*       ELSE                                                       0188
*         RLUPACEN=2;               /* OTHER VPACING DFLTS (2,1)   0188
*                                                            @D32CKQH*/
         B     @RC00186                                            0188
@RF00186 L     @15,MLCPTR6(,MLCAPTR)                               0188
         MVI   RLUPACEN(@15),X'02'                                 0188
*       RLUPACEM=1;                                                0189
@RC00186 L     @15,MLCPTR6(,MLCAPTR)                               0189
         MVI   RLUPACEM(@15),X'01'                                 0189
*     END;                                                         0190
*   IF PHRF8NSP<=1                  /* M VALUE NOT EXIST?            */
*     THEN                                                         0191
@RC00180 L     @15,PHRF8NSP(,PHROAPTR)                             0191
         CH    @15,@CH00048                                        0191
         BH    @RF00191                                            0191
*     DO;                                                          0192
*       IF PHRF8DFT(1)='1'B         /* N DEFAULTED?                  */
*         THEN                                                     0193
         TM    PHRF8DFT(PHROAPTR),B'10000000'                      0193
         BO    @RT00193                                            0193
*         ;                                                        0194
*       ELSE                                                       0195
*         DO;                                                      0195
*           IF LPACE=0              /* PACING?                       */
*             THEN                                                 0196
         LTR   LPACE,LPACE                                         0196
         BNZ   @RF00196                                            0196
*             DO;                                                  0197
*               RLUNCPPN=PHRF8NUM(1);/* SET BOTH TO N VALUE FOR    0198
*                                      PACING                        */
         L     @15,MLCPTR6(,MLCAPTR)                               0198
         MVC   RLUNCPPN(1,@15),PHRF8NUM(PHROAPTR)                  0198
*               RLUNCPPM=RLUNCPPN;                                 0199
         MVC   RLUNCPPM(1,@15),RLUNCPPN(@15)                       0199
*             END;                                                 0200
*           ELSE                                                   0201
*             DO;                                                  0201
         B     @RC00196                                            0201
@RF00196 DS    0H                                                  0202
*               RLUPACEN=PHRF8NUM(1);/* SET BOTH TO N VALUE FOR    0202
*                                      VPACING                       */
         L     @15,MLCPTR6(,MLCAPTR)                               0202
         MVC   RLUPACEN(1,@15),PHRF8NUM(PHROAPTR)                  0202
*               RLUPACEM=RLUPACEN;                                 0203
         MVC   RLUPACEM(1,@15),RLUPACEN(@15)                       0203
*             END;                                                 0204
*         END;                                                     0205
*     END;                                                         0206
*   ELSE                                                           0207
*     DO;                                                          0207
         B     @RC00191                                            0207
@RF00191 DS    0H                                                  0208
*       IF PHRF8NUM(1)=0×PHRF8DFT(1)='1'B/* N=0 OR DEFAULT?          */
*         THEN                                                     0208
*                                                                  0208
         CLI   PHRF8NUM(PHROAPTR),0                                0208
         BE    @RT00208                                            0208
         TM    PHRF8DFT(PHROAPTR),B'10000000'                      0208
         BNO   @RF00208                                            0208
@RT00208 DS    0H                                                  0209
*/* ERRFMT    LEVEL(VALUE)  ERROR(INVAL)  ACTION(TAKEDEF)            */
