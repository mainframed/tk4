*             THEN                                                 0218
         LTR   LPACE,LPACE                                         0218
         BNZ   @RF00218                                            0218
*             RLUNCPPN=PHRF8NUM(1); /* SET N VALUE FOR PACING        */
         L     @15,MLCPTR6(,MLCAPTR)                               0219
         MVC   RLUNCPPN(1,@15),PHRF8NUM(PHROAPTR)                  0219
*           ELSE                                                   0220
*             RLUPACEN=PHRF8NUM(1); /* SET N VALUE FOR VPACING       */
         B     @RC00218                                            0220
@RF00218 L     @15,MLCPTR6(,MLCAPTR)                               0220
         MVC   RLUPACEN(1,@15),PHRF8NUM(PHROAPTR)                  0220
*           IF PHRF8DFT(2)='1'B     /* M DEFAULTED?                  */
*             THEN                                                 0221
@RC00218 TM    PHRF8DFT+2(PHROAPTR),B'10000000'                    0221
         BNO   @RF00221                                            0221
*             DO;                                                  0222
*               IF LPACE=0          /* PACING?                       */
*                 THEN                                             0223
         LTR   LPACE,LPACE                                         0223
         BNZ   @RF00223                                            0223
*                 RLUNCPPM=RLUNCPPN;/* SET M=N FOR PACING            */
         L     @15,MLCPTR6(,MLCAPTR)                               0224
         MVC   RLUNCPPM(1,@15),RLUNCPPN(@15)                       0224
*               ELSE                                               0225
*                 RLUPACEM=RLUPACEN;/* SET M=N FOR VPACING           */
         B     @RC00223                                            0225
@RF00223 L     @15,MLCPTR6(,MLCAPTR)                               0225
         MVC   RLUPACEM(1,@15),RLUPACEN(@15)                       0225
*             END;                                                 0226
*           ELSE                                                   0227
*             DO;                                                  0227
         B     @RC00221                                            0227
@RF00221 DS    0H                                                  0228
*               IF PHRF8NUM(2)<=PHRF8NUM(1)/* M<=N?                  */
*                 THEN                                             0228
         CLC   PHRF8NUM+2(1,PHROAPTR),PHRF8NUM(PHROAPTR)           0228
         BH    @RF00228                                            0228
*                 DO;                                              0229
*                   IF LPACE=0      /* PACING?                       */
*                     THEN                                         0230
         LTR   LPACE,LPACE                                         0230
         BNZ   @RF00230                                            0230
*                     RLUNCPPM=PHRF8NUM(2);/* SET M VALUE FOR PACING */
         L     @15,MLCPTR6(,MLCAPTR)                               0231
         MVC   RLUNCPPM(1,@15),PHRF8NUM+2(PHROAPTR)                0231
*                   ELSE                                           0232
*                     RLUPACEM=PHRF8NUM(2);/* SET M VALUE FOR VPACING*/
         B     @RC00230                                            0232
@RF00230 L     @15,MLCPTR6(,MLCAPTR)                               0232
         MVC   RLUPACEM(1,@15),PHRF8NUM+2(PHROAPTR)                0232
*                 END;                                             0233
