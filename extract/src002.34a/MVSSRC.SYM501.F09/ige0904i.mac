*                   REG10 RSTD;     /*                       @OY18083*/
*                 REG10=ADDR(COMPSAVE);/* SET ADDRESS COMPILER SAVE
*                                      AREA                  @OY18083*/
         LA    REG10,COMPSAVE(,RMDR)                            04S0427
