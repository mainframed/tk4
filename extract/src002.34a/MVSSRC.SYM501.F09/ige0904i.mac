         IC    @09,RURECID(,@11)                                05S0433
         STC   @09,MDRRECID(,RMDR)                              05S0433
*                       RFY                                     05S0434
*                         RPARM RSTD;                           05S0434
*                       RPARM=RUNA; /* MOVE NETWORK ADDRESS          */
         SLR   RPARM,RPARM                                      05S0435
         ICM   RPARM,3,RUNA(@11)                                05S0435
*                       RFY                                     05S0436
*                         REG15 RSTD;                           05S0436
*                       CALL IEDIAP03;/* GET TTCIN FOR NETWORK  05S0437
