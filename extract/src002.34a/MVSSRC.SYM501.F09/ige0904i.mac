         LA    @11,16                                           05S0476
         ALR   @11,SOHDLEN                                      05S0476
         STH   @11,MDRWTOL(,RMDR)                               05S0476
*                           MDRDATA1(1:SOHDLEN)=SOHDATA1(1:SOHDLEN);
         LR    @07,SOHDLEN                                      05S0477
         BCTR  @07,0                                            05S0477
         EX    @07,@SM03157                                     05S0477
*                                   /* MOVE MESSAGE DATA TO DATA AREA*/
*                           WTOENDFG=WTOENDFI;/* SET WTO END FLAGS
*                                      FOR MESSAGE                   */
         LA    @09,WTOHDR(,RMDR)                                05S0478
         ALR   @09,@11                                          05S0478
         MVC   WTOENDFG(4,@09),@CB03017                         05S0478
*                           IF MDRCDE8=ON/* IF THE MESSAGE IS TO GO
*                                      TO THE SYSTEM TP CONSOLE      */
*                             THEN                              05S0479
         TM    MDRCDE8(RMDR),B'00000010'                        05S0479
         BNO   @RF00479                                         05S0479
*                             WTOCDE8=ON;/* ..THEN SO INDICATE       */
         OI    WTOCDE8(@09),B'00000001'                         05S0480
*                           ELSE                                05S0481
*                             IF MDRCDE10=ON/* IF THE MESSAGE IS TO
*                                      GO TO THE SYSTEM MAINTENANCE
*                                      CONSOLE                       */
*                               THEN                            05S0481
         B     @RC00479                                         05S0481
@RF00479 TM    MDRCDE10(RMDR),B'00000100'                       05S0481
         BNO   @RF00481                                         05S0481
*                               WTOCDE10=ON;/* ..THEN SO INDICATE    */
         LA    @11,WTOHDR(,RMDR)                                05S0482
         AH    @11,MDRWTOL(,RMDR)                               05S0482
         OI    WTOCDE10(@11),B'01000000'                        05S0482
*                             ELSE                              05S0483
*                               WTOCDE2=ON;/* ..ELSE INDICATE THAT
*                                      THE MESSAGE IS TO GO TO THE
*                                      SYSTEM MASTER CONSOLE         */
         B     @RC00481                                         05S0483
@RF00481 LA    @11,WTOHDR(,RMDR)                                05S0483
         AH    @11,MDRWTOL(,RMDR)                               05S0483
         OI    WTOCDE2(@11),B'01000000'                         05S0483
*                         END;                                  05S0484
@RC00481 DS    0H                                               05S0484
@RC00479 DS    0H                                               05S0485
*                         CASEINDX=WTO;/* SETUP WTO EXECUTION        */
         LA    CASEINDX,2                                       04S0485
*                       END;                                    04S0486
*                     ELSE                                      04S0487
*                       DO;         /* ..ELSE PROCESS SOH%E AND SOH%R
*                                      RECORDS                       */
         B     @RC00470                                         04S0487
@RF00470 DS    0H                                               04S0488
