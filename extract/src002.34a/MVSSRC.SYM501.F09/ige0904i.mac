         L     @14,IEDQNPTR(,RAVT)                              05S0441
         LA    NAMEADDR,82                                      05S0441
         ALR   NAMEADDR,@14                                     05S0441
         LR    @01,REG15                                        05S0441
         BCTR  @01,0                                            05S0441
         SLR   @11,@11                                          05S0441
         IC    @11,TNTENLEN(,@14)                               05S0441
         LA    @14,3                                            05S0441
         ALR   @14,@11                                          05S0441
         MR    @00,@14                                          05S0441
         ALR   NAMEADDR,@01                                     05S0441
*                           RFY                                 05S0442
*                             REG15 UNRSTD;                     05S0442
*                           MDRNAME='';/* BLANK ENTRY NAME FOR SHORT
*                                      NAMES                         */
         MVI   MDRNAME(RMDR),C' '                               05S0443
         MVC   MDRNAME+1(7,RMDR),MDRNAME(RMDR)                  05S0443
*                           MDRNAME(1:TNTENLEN)=TTENAME(1:TNTENLEN);
         BCTR  @11,0                                            05S0444
         EX    @11,@SM03151                                     05S0444
*                                   /* MOVE ENTRY NAME TO DATA AREA  */
*                         END;                                  05S0445
*                       ELSE                                    05S0446
