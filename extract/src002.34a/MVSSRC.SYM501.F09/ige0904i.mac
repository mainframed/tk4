*                       /*
IGE0904I IEDHJN AFTERHJN,HJN           MODULE IDENTIFICATION MACRO
*     RAVT=TCXAVT;                  /* ESTABLISH AVT ADDRESS         */
         L     @09,CVTPTR                                       02S0391
         L     @09,TCXPTR(,@09)                                 02S0391
         L     RAVT,TCXAVT(,@09)                                02S0391
*     IOSBPTR=RPARM;                /* SET UP IOSB ADDRESS   @G36XREB*/
         LR    IOSBPTR,RPARM                                    02S0392
*     RFY                                                       02S0393
*       RPARM UNRSTD;                                           02S0393
*     RLCB=IOSUSE;                  /* GET LCB ADDRESS FROM IOSB
*                                                            @G36XREB*/
         L     RLCB,IOSUSE(,IOSBPTR)                            02S0394
*     IF LCBGETMN=ON                /* IF A GETMAIN HAS ALREADY BEEN
*                                      DONE(WHICH WILL BE TRUE IF WE
*                                      ARE RETURNING FROM IGE0625F TO
*                                      PROCESS THE NEXT RECORD ON THE
*                                      LCB OBR CHAIN)                */
*       THEN                                                    02S0395
         TM    LCBGETMN(RLCB),B'10000000'                       02S0395
         BNO   @RF00395                                         02S0395
*       RMDR=LCBRECAD-LENGTH(MDRFREE);/* ..THEN SETUP GETMAINED AREA
*                                      ADDRESSABILITY                */
         L     RMDR,LCBRECAD-1(,RLCB)                           02S0396
         LA    RMDR,0(,RMDR)                                    02S0396
         BCTR  RMDR,0                                           02S0396
         BCTR  RMDR,0                                           02S0396
*     ELSE                                                      02S0397
*                                                               03S0397
*       /*************************************************************/
*       /*                                                           */
*       /* PROCESS.GETMAIN: THIS SEGMENT GETMAINS A STORAGE AREA IN  */
*       /* WHICH TO FORMAT INFORMATION FOR IGE0625F AND BUILD        */
*       /* MESSAGES FOR THE SYSTEM OPERATOR.                         */
*       /*                                                           */
*       /*************************************************************/
*                                                               03S0397
*       DO;                                                     03S0397
         B     @RC00395                                         03S0397
@RF00395 DS    0H                                               03S0398
*         RFY                                                   03S0398
*           RCORESZE RSTD;                                      03S0398
*         RCORESZE=MAX(RMSLNMAX,AVTKEYLE-F0GARLEN-SRHDRLDF)+LENGTH(
*             TPERHDR)+76;          /* CALCULATE MAXIMUM ACTUAL CORE
*                                      SIZE NEEDED FOR ANY RECORD ON
*                                      THE OBR QUEUE         @OY18083*/
         LH    RCORESZE,AVTKEYLE(,RAVT)                         03S0399
         SL    RCORESZE,@CF03144                                03S0399
         LA    @09,35                                           03S0399
         CR    RCORESZE,@09                                     03S0399
         BNL   *+6
         LR    RCORESZE,@09                                     03S0399
         AL    RCORESZE,@CF03145                                03S0399
*         RCORESZE=RCORESZE+7;      /* THESE TWO INSTRUCTIONS ROUNDUP
*                                      CORE SIZE TO DOUBLEWORD  03S0400
*                                      MULTIPLE                      */
         AL    RCORESZE,@CF00150                                03S0400
*         RCORESZE=RCORESZE&ROUNDMSK;                           03S0401
         N     RCORESZE,@CF03047                                03S0401
*         RFY                                                   03S0402
*          (REG15,                                              03S0402
*           RPARM) RSTD;                                        03S0402
*         GEN CODE REFS(RCORESZE) SETS(REG00,RPARM) NODEFS NOEXIT
*             NOFLOWS SEQFLOW;                                  03S0403
*        /*
 GETMAIN R,LV=(RCORESZE),SP=254
*         IF REG15=0                /* IF THE GETMAIN WORKED         */
*           THEN                                                03S0404
         LTR   REG15,REG15                                      03S0404
         BNZ   @RF00404                                         03S0404
*           DO;                     /* ..THEN SETUP GETMAINED AREA
*                                      ADDRESSABILITY                */
*             RMDR=RPARM;           /* MOVE CORE ADDRESS             */
         LR    RMDR,RPARM                                       03S0406
*             RFY                                               03S0407
*              (RPARM,                                          03S0407
*               REG15) UNRSTD;                                  03S0407
*             LCBRECAD=RMDR+LENGTH(MDRFREE);/* SAVE CORE ADDRESS     */
         LA    @09,2                                            03S0408
         ALR   @09,RMDR                                         03S0408
         STCM  @09,7,LCBRECAD(RLCB)                             03S0408
*             MDRFREE=RCORESZE;     /* SAVE CORE SIZE                */
         STH   RCORESZE,MDRFREE(,RMDR)                          03S0409
*             RFY                                               03S0410
*               RCORESZE UNRSTD;                                03S0410
*             LCBGETMN=ON;          /* INDICATE GETMAIN DONE         */
         OI    LCBGETMN(RLCB),B'10000000'                       03S0411
*           END;                                                03S0412
*         ELSE                                                  03S0413
*           DO WHILE LCBOBRCH^=0;   /* ..ELSE FREE ALL RECORDS ON THE
*                                      LCB OBR QUEUE                 */
         B     @RC00404                                         03S0413
@RF00404 B     @DE00413                                         03S0413
@DL00413 DS    0H                                               03S0414
*             CALL IPCSOBRQ;        /* GET A RECORD FROM THE OBR
*                                      QUEUE                         */
         BAL   @14,IPCSOBRQ                                     03S0414
*             CALL IPCSTCXQ;        /* PUT THE RECORD ON THE TCX
*                                      READY QUEUE FOR POSTING TO
*                                      BUFFER DISPOSITION            */
         BAL   @14,IPCSTCXQ                                     03S0415
*           END;                                                03S0416
@DE00413 L     @09,LCBOBRCH(,RLCB)                              03S0416
         LTR   @09,@09                                          03S0416
         BNZ   @DL00413                                         03S0416
*       END;                                                    03S0417
@RC00404 DS    0H                                               03S0418
*                                                               03S0418
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS.HANDLE: THIS SEGMENT PROCESSES THE RECORDS ON THE   */
*     /* LCB OBR QUEUE.                                              */
*     /*                                                             */
*     /***************************************************************/
*                                                               03S0418
*     DO UNTIL 1^=1;                /* LOOP UNTIL WE EXIT            */
@RC00395 DS    0H                                               03S0418
@DL00418 DS    0H                                               03S0419
*       IF LCBGETMN=ON              /* IF THERE IS A GETMAINED  03S0419
*                                      STORAGE AREA AVAILABLE        */
*         THEN                                                  03S0419
         TM    LCBGETMN(RLCB),B'10000000'                       03S0419
         BNO   @RF00419                                         03S0419
*         DO;                       /* ..THEN PROCESS THE NEXT RECORD
*                                      ON THE LCB OBR QUEUE          */
*           IF LCBOBRCH^=0          /* IF THERE ARE MORE RECORDS ON
*                                      THE LCB OBR QUEUE             */
*             THEN                                              03S0421
         L     @09,LCBOBRCH(,RLCB)                              03S0421
         LTR   @09,@09                                          03S0421
         BZ    @RF00421                                         03S0421
*             DO;                   /* ..THEN PROCESS THE NEXT RECORD*/
*               CALL IPCSOBRQ;      /* GET THE NEXT RECORD ON THE
*                                      QUEUE                         */
         BAL   @14,IPCSOBRQ                                     03S0423
