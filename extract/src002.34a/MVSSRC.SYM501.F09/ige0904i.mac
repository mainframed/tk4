*                                      ERRONEOUS NAME                */
         B     @RC00439                                         05S0446
@RF00439 MVC   MDRNAME(8,RMDR),@CC03013                         05S0446
*                       MDRNETA=RUNA;/* SET NETWORK ADDRESS IN DATA
*                                      AREA                          */
@RC00439 LA    @11,PRFPIUO(,RPRF)                               05S0447
         SLR   @09,@09                                          05S0447
         IC    @09,PRFPIUO(,RPRF)                               05S0447
         ALR   @09,@11                                          05S0447
         MVC   MDRNETA(2,RMDR),RUNA(@09)                        05S0447
*                       PIUDLEN=TTHDCF-LENGTH(IEDRH)-LENGTH(RUCNTL);
         SLR   PIUDLEN,PIUDLEN                                  05S0448
         ICM   PIUDLEN,3,TTHDCF(@09)                            05S0448
         SL    PIUDLEN,@CF00032                                 05S0448
*                                   /* CALCULATE PIU DATA LENGTH     */
*                       UNITDLEN=ADDR(PRFPIUO)+AVTKEYLE-ADDR(RUDATA);
         AH    @11,AVTKEYLE(,RAVT)                              05S0449
         LA    UNITDLEN,RUDATA(,@09)                            05S0449
         SLR   @11,UNITDLEN                                     05S0449
         LR    UNITDLEN,@11                                     05S0449
*                                   /* CALCULATE HEAD UNIT DATA 05S0450
*                                      LENGTH                        */
*                       IF PIUDLEN<=UNITDLEN/* IF THE PIU IS    05S0450
*                                      CONTAINED IN ONE BUFFER UNIT  */
*                         THEN                                  05S0450
         CR    PIUDLEN,UNITDLEN                                 05S0450
         BH    @RF00450                                         05S0450
*                         MDRDATA(1:PIUDLEN)=RUDATA(1:PIUDLEN);/*
*                                      ..THEN MOVE ALL OF THE PIU'S
*                                      DATA IN ONE OPERATION         */
         LR    @11,PIUDLEN                                      05S0451
         BCTR  @11,0                                            05S0451
         EX    @11,@SM03153                                     05S0451
*                       ELSE                                    05S0452
*                         DO;       /* ..ELSE MOVE ALL OF THE PIU'S
*                                      DATA IN MULTIPLE OPERATIONS   */
         B     @RC00450                                         05S0452
@RF00450 DS    0H                                               05S0453
*                           MDRDATA(1:UNITDLEN)=RUDATA(1:UNITDLEN);/*
*                                      MOVE THE FIRST UNIT'S DATA    */
         LR    @11,UNITDLEN                                     05S0453
         BCTR  @11,0                                            05S0453
         LA    @09,PRFPIUO(,RPRF)                               05S0453
         SLR   @07,@07                                          05S0453
         IC    @07,PRFPIUO(,RPRF)                               05S0453
         ALR   @09,@07                                          05S0453
         EX    @11,@SM03153                                     05S0453
*                           DO LOOPINDX=UNITDLEN TO PIUDLEN-1 BY
*                                 AVTKEYLE;/* MOVE THE REMAINING
*                                      UNITS' DATA-LOOPINDX IS THE
*                                      INDEX INTO THE DATA AREA OF
*                                      THE TARGET OF THE CURRENT
*                                      UNIT'S DATA                   */
         LR    LOOPINDX,UNITDLEN                                05S0454
         B     @DE00454                                         05S0454
@DL00454 DS    0H                                               05S0455
*                             RPRF=PRFTIC;/* GET NEXT UNIT ADDRESS   */
         L     RPRF,PRFTIC(,RPRF)                               05S0455
*                             LENDATTM=MIN(AVTKEYLE,(PIUDLEN-LOOPINDX))
*                                 ; /* CALCULATE SIZE OF DATA TO MOVE*/
         LR    LENDATTM,PIUDLEN                                 05S0456
         SLR   LENDATTM,LOOPINDX                                05S0456
         LH    @11,AVTKEYLE(,RAVT)                              05S0456
         CR    @11,LENDATTM                                     05S0456
         BNH   *+6
         LR    @11,LENDATTM                                     05S0456
         LR    LENDATTM,@11                                     05S0456
*                             MDRDATA(LOOPINDX+1:LOOPINDX+LENDATTM)=
*                                 UNITDATA(1:LENDATTM);/* MOVE THE
*                                      DATA FROM THE UNIT TO THE DATA
*                                      AREA                          */
         LR    @11,RMDR                                         05S0457
         ALR   @11,LOOPINDX                                     05S0457
         LR    @15,LENDATTM                                     05S0457
         BCTR  @15,0                                            05S0457
         EX    @15,@SM03155                                     05S0457
*                           END;                                05S0458
         AH    LOOPINDX,AVTKEYLE(,RAVT)                         05S0458
@DE00454 LR    @11,PIUDLEN                                      05S0458
         BCTR  @11,0                                            05S0458
         CR    LOOPINDX,@11                                     05S0458
         BNH   @DL00454                                         05S0458
*                         END;                                  05S0459
*                       MDRWTO=''B; /* CLEAR AN UNUSED FIELD IN THE
*                                      DATA AREA                     */
@RC00450 XC    MDRWTO(18,RMDR),MDRWTO(RMDR)                     05S0460
*                     END;                                      05S0461
*                     CASEINDX=XCTL;/* SETUP XCTL TO IGE0625F FOR
*                                      WRITE TO LOGREC               */
         LA    CASEINDX,3                                       04S0462
*                   END;                                        04S0463
*                 ELSE                                          04S0464
*                   DO;             /* ..ELSE PROCESS FID0 SOH  04S0464
*                                      RECORDS                       */
         B     @RC00428                                         04S0464
@RF00428 DS    0H                                               04S0465
*                     DATADDR=ADDR(SOHDATA0);/* SETUP DATA ADDRESS   */
         LA    @11,PRFPIUO(,RPRF)                               04S0465
         SLR   @09,@09                                          04S0465
         IC    @09,PRFPIUO(,RPRF)                               04S0465
         ALR   @11,@09                                          04S0465
         LA    @09,SOHDATA0(,@11)                               04S0465
         ST    @09,DATADDR                                      04S0465
*                     IF SOHSTX=DLE /* IF THERE IS A DLE IN THE DATA
*                                      FOR TRANSPARENT MODE          */
*                       THEN                                    04S0466
         CLI   SOHSTX(@11),X'10'                                04S0466
         BNE   @RF00466                                         04S0466
*                       DATADDR=DATADDR+1;/* ..THEN SKIP THE DLE     */
         AL    @09,@CF00095                                     04S0467
         ST    @09,DATADDR                                      04S0467
*                     ELSE                                      04S0468
*                       ;                                       04S0468
@RF00466 DS    0H                                               04S0469
*                     SOHDLEN=MIN((ADDR(PRFPIUO)+AVTKEYLE),(TTHDCF+ADDR
*                         (PIURH)))-ADDR(SOHDATA1);/* DETERMINE 04S0469
*                                      BUFFER UNIT DATA LENGTH       */
         LA    @11,PRFPIUO(,RPRF)                               04S0469
         LH    SOHDLEN,AVTKEYLE(,RAVT)                          04S0469
         ALR   SOHDLEN,@11                                      04S0469
         SLR   @09,@09                                          04S0469
         IC    @09,PRFPIUO(,RPRF)                               04S0469
         ALR   @11,@09                                          04S0469
         LA    @09,PIURH(,@11)                                  04S0469
         SLR   @07,@07                                          04S0469
         ICM   @07,3,TTHDCF(@11)                                04S0469
         ALR   @09,@07                                          04S0469
         CR    SOHDLEN,@09                                      04S0469
         BNH   *+6
         LR    SOHDLEN,@09                                      04S0469
         L     @09,DATADDR                                      04S0469
         LA    @07,SOHDATA1(,@09)                               04S0469
         SLR   SOHDLEN,@07                                      04S0469
*                     IF SOHTYPE=SOHC/* IF THE RECORD IS AN SOH%C PIU*/
*                       THEN                                    04S0470
         CLI   SOHTYPE(@11),C'C'                                04S0470
         BNE   @RF00470                                         04S0470
*                       DO;         /* ..THEN PROCESS AN SOH%C PIU   */
*                                                               05S0472
*                         /*******************************************/
*                         /*                                         */
*                         /* PROCESS.HANDLE.BLDREC.BLDWTO: THIS      */
*                         /* SEGMENT PROCESSES SOH%C RECORDS.        */
*                         /*                                         */
*                         /*******************************************/
*                                                               05S0472
*                         DO;                                   05S0472
*                           MDRFLAGS=WTOBEGFI;/* INITIALIZE WTO FLAGS*/
         MVC   MDRFLAGS(2,RMDR),@CB03015                        05S0473
*                           MDRMSGID=MSGNUM;/* MOVE MESSAGE NUMBER
*                                      INTO DATA AREA                */
         MVI   MDRMSGID+7(RMDR),C' '                            05S0474
         MVC   MDRMSGID(7,RMDR),@CC03019                        05S0474
*                           MDREBCCU=UCBNAME;/* MOVE CHANNEL AND UNIT
