         L     @11,AVTSAVTP(,RAVT)                              05S0437
         L     @15,SAVTNAX(,@11)                                05S0437
         BALR  @14,@15                                          05S0437
*                       RFY                                     05S0438
*                         RPARM UNRSTD;                         05S0438
*                       IF REG15^=0 /* IF A TTCIN WAS FOUND          */
*                         THEN                                  05S0439
         LTR   REG15,REG15                                      05S0439
         BZ    @RF00439                                         05S0439
*                         DO;       /* ..THEN MOVE RESOURCE NAME TO
*                                      DATA AREA                     */
*                           NAMEADDR=ADDR(TNTFIRST)+(REG15-1)*(TNTENLEN
*                               +LENGTH(TNTTRMAD));/* CALCULATE ENTRY
