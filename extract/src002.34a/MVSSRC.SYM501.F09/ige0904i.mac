         LA    @09,28                                           05S0431
         SLR   @07,@07                                          05S0431
         ICM   @07,3,TTHDCF(@11)                                05S0431
         ALR   @09,@07                                          05S0431
         STH   @09,MDRBUFSZ(,RMDR)                              05S0431
*                       MDRSECID=TCAM370X;/* SETUP A SECONDARY ID
