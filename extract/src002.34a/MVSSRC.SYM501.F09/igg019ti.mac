*   /*                                                               */
*   /* PROCESS.READPIU.POSTERB: IF BUFFER UNITS ARE NEEDED FROM THE  */
*   /* BUFFER FREE POOL TO REPLENISH THE READ COUNT, THEN POST THE   */
*   /* ERB TO GET THEM.                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                               04S0408
*   IF LCBRBCT2^=ZERO THEN          /* ARE UNITS NEEDED              */
         CLI   LCBRBCT2(RLCB),0                                 04S0408
         BE    @RF00408                                         04S0408
*     DO;                           /* YES, POST THE ERB             */
*       LCBDLNKN=OFF;               /* INDICATE ERB POSTED           */
         NI    LCBDLNKN(RLCB),B'11111110'                       04S0410
*       LCBRCQCB=RLCB;              /* SET QCB TO WHICH BUFFERS WILL
*                                      BE TPOSTED                    */
         ST    RLCB,LCBRCQCB(,RLCB)                             04S0411
*       LCBERBQB=ADDR(AVTBFREB);    /* QUEUE ERB TO AVAILABLE BUFFER
*                                      ROUTINE                       */
         LA    @10,AVTBFREB(,RAVT)                              04S0412
         STCM  @10,7,LCBERBQB(RLCB)                             04S0412
*       LCBERBPY=PRIINTRQ;          /* SET PRIORITY                  */
         MVI   LCBERBPY(RLCB),X'E4'                             04S0413
*       RFY                                                     04S0414
*         RPARM RSTD;                                           04S0414
*       RPARM=ADDR(LCBERB);         /* MAKE THE ERB THE ELEMENT TO
*                                      TPOST                         */
         LA    RPARM,LCBERB(,RLCB)                              04S0415
*       CALL DSPPOSTR;              /* CALL DISPATCHER TO TPOST 04S0416
*                                      ELEMENT                       */
         LA    @15,DSPPOSTR(,RDISP)                             04S0416
         BALR  @14,@15                                          04S0416
*       RFY                                                     04S0417
*         RPARM UNRSTD;                                         04S0417
*     END;                                                      04S0418
