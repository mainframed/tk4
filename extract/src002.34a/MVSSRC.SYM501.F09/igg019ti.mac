*   /*                                                               */
*   /* PROCESS.EXIT: POST LCB TO ITSELF IF THE ERB HAS NOT BEEN      */
*   /* POSTED. SET LCB FIELDS AND POST ANY READ PIU'S TO PATH        */
*   /* CONTROL.                                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                               03S0419
*   IF LCBDLNKN=ON&                 /* IS THE ...                    */
*       LCBERBCH=ZERO THEN          /* ... ERB POSTED                */
@RF00408 TM    LCBDLNKN(RLCB),B'00000001'                       03S0419
         BNO   @RF00419                                         03S0419
         L     @10,LCBERBCH-1(,RLCB)                            03S0419
         LA    @10,0(,@10)                                      03S0419
         LTR   @10,@10                                          03S0419
         BNZ   @RF00419                                         03S0419
*     DO;                           /* NO, POST THE LCB TO ITSELF    */
*       LCBQCBA=RLCB;               /* SET LCB AS THE QCB            */
         STCM  RLCB,7,LCBQCBA(RLCB)                             03S0421
*       LCBPRI=PRIRESUM;            /* SET PRIORITY          @YM08511*/
         MVI   LCBPRI(RLCB),X'E5'                               03S0422
*       RFY                                                     03S0423
*         RPARM RSTD;                                           03S0423
*       RPARM=RLCB;                 /* MAKE THE LCB THE ELEMENT TO
*                                      TPOST                         */
         LR    RPARM,RLCB                                       03S0424
*       CALL DSPPOSTR;              /* CALL DISPATCHER TO TPOST 03S0425
*                                      ELEMENT                       */
         LA    @15,DSPPOSTR(,RDISP)                             03S0425
         BALR  @14,@15                                          03S0425
*       RFY                                                     03S0426
*         RPARM UNRSTD;                                         03S0426
*     END;                                                      03S0427
*   LCBSTAT1=CLEAR;                 /* CLEAR STATE FIELD             */
@RF00419 MVI   LCBSTAT1(RLCB),X'00'                             03S0428
*   RFY                                                         03S0429
*    (REG9,                                                     03S0429
*     REG10) RSTD;                  /*                       @YM04622*/
*   GEN REFS(CSMASK1,LCBRECVN)      /*                       @YM04622*/
*       SETS(REG9,REG10,LCBRECVN);                              03S0430
*                              /* @YM04622*/
*                                     /*
         L     REG10,LCBRECVN-2(,RLCB)  LOAD WORD CONTAINING   @YM04622
*                                       LCBRECVN               @YM04622
CSLOOP1  EQU   *                                               @YM04622
         LR    REG9,REG10                                      @YM04622
         O     REG9,CSMASK1             TURN ON LCBRECVN IN 9  @YM04622
         CS    REG10,REG9,LCBRECVN-2(RLCB) CS LCBRECVN         @YM04622
         BNE   CSLOOP1                  DO AGAIN IF NOT EQUAL  @YM04622
*   RFY                                                         03S0431
*    (REG9,                                                     03S0431
*     REG10) UNRSTD;                /*                       @YM04622*/
*   LCBTSTSW=CLEAR;                 /* CLEAR TEST-AND-SET SWITCH     */
         MVI   LCBTSTSW(RLCB),X'00'                             03S0432
*   RFY                                                         03S0433
*    (RPARM,                                                    03S0433
*     RSTCB,                                                    03S0433
*     RQCB,                                                     03S0433
*     RRETURN) RSTD;                                            03S0433
*   IF READBTU^=ZERO THEN           /* IS READ BTU AVAILABLE         */
         L     @15,READBTU-1(,RLCB)                             03S0434
         LA    @15,0(,@15)                                      03S0434
         LTR   @15,@15                                          03S0434
         BZ    @RF00434                                         03S0434
*     DO;                           /* YES                           */
*       RPARM=READBTU;              /* SET CURRENT PIU TO READ PIU   */
         LR    RPARM,@15                                        03S0436
*       RPARM->PRFQCBA=SAVTIPCQ;    /* SET DSPBYPAS QCB ADDRESS      */
         L     @15,AVTSAVTP(,RAVT)                              03S0437
         L     @15,SAVTIPCQ(,@15)                               03S0437
         STCM  @15,7,PRFQCBA(RPARM)                             03S0437
*       RPARM->PRFPRI=PRIFLDH;      /* SET DSPBYPAS PRIORITY         */
         MVI   PRFPRI(RPARM),X'E4'                              03S0438
*       RQCB=SAVTIPCQ;              /* QUEUE TO PATH CONTROL @YM03681*/
         LR    RQCB,@15                                         03S0439
*       RSTCB=QCBSTCHN;             /* SET STCB POINTER      @YM03681*/
         L     RSTCB,QCBSTCHN-1(,RQCB)                          03S0440
         LA    RSTCB,0(,RSTCB)                                  03S0440
*       RRETURN=ADDR(DSPBYPAS);     /* EXIT TO BY-PASS               */
         LA    RRETURN,DSPBYPAS(,RDISP)                         03S0441
*     END;                                                      03S0442
*   ELSE                            /* READ BTU NOT AVAILABLE        */
*     RRETURN=ADDR(DSPDISP);        /* RETURN TO DISPATCHER          */
         B     @RC00434                                         03S0443
@RF00434 LA    RRETURN,DSPDISP(,RDISP)                          03S0443
*   RFY                                                         03S0444
*    (RPARM,                                                    03S0444
*     RSTCB,                                                    03S0444
*     RQCB,                                                     03S0444
*     RRETURN) UNRSTD;                                          03S0444
