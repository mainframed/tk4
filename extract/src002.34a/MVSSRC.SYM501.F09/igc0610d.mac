         TITLE 'IGC0610D, OPERATOR CONTROL CHECKPOINT/RESTART ROUTINE  *
                        '
IGC0610D CSECT ,                                                01S0001
@PROLOG  BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                                   /*
IGC0610D IEDHJN AFTERHJN,HJN           MODULE IDENTIFICATION MACRO
*   ROPCAVT=RPARM;                  /* SET THE BASE POINTER FOR THE
*                                      OPERATOR CONTROL ADDRESS 02S0385
*                                      VECTOR TABLE                  */
         LR    ROPCAVT,RPARM                                    02S0385
*   RDATA=OPCWORK;                  /* SET THE BASE REGISTER TO BE
*                                      USED FOR ADDRESSING AUTOMATIC
*                                      DATA WITH THE POINTER TO THE
*                                      OPCTL RESIDENT WORK AREA      */
         L     RDATA,OPCWORK(,ROPCAVT)                          02S0386
*   GEN REFS(RDATA) NOSETS;                                     02S0387
*                                   /*
         USING @DATD,RDATA             INFORM THE ASSEMBLER OF THE
*                                      BASE REGISTER TO BE USED FOR
*                                      ADDRESSING AUTOMATIC DATA
*   RSAVE=ADDR(OPCSAVE);            /* SET THE SAVEAREA POINTER 02S0388
*                                      REGISTER WITH THE POINTER TO
*                                      THE OPERATOR CONTROL SAVE AREA*/
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          02S0388
*                                                               02S0389
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS: THIS SEGMENT DOES THE PROCESSING THAT IS NECESSARY TO*/
*   /* PERFORM THE FUNCTION OF THE MODULE.                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0389
*                                                               03S0389
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS.INITIAL: THIS SEGMENT DOES THE INITIAL PROCESSING.    */
*   /* GENERAL REGISTERS ARE INITIALIZED. WORKAREAS AND STATE FIELDS */
*   /* WHICH ARE DEFINED IN THE AUTOMATIC DATA AREA WHICH RESIDES IN */
*   /* THE OP CONTROL WORK AREA ARE CLEARED.                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               03S0389
*   DO;                                                         03S0389
*     RAVT=OPCAVTPT;                /* SET THE BASE POINTER FOR 03S0390
*                                      TCAM'S AVT                    */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          03S0390
*     ROPCE=OPCCOPCE;               /* SET THE BASE POINTER FOR THE
*                                      CURRENT OPCTL ELEMENT         */
         L     @10,OPCCOPCE(,ROPCAVT)                           03S0391
         LR    ROPCE,@10                                        03S0391
*     STATES=0;                     /* INITIALIZE STATE FIELDS       */
         MVI   STATES,X'00'                                     03S0392
*   END;                                                        03S0393
*   IF OPCCOPCE=ADDR(AVTCRELE)      /* IS THE CURRENT ELEMENT THE
*                                      CKPT REQUEST ELEMENT          */
*     THEN                          /* THIS IS A RETURN FROM THE CKPT
*                                      SUBTASK                       */
         LA    @09,AVTCRELE(,RAVT)                              02S0394
         CR    @10,@09                                          02S0394
         BNE   @RF00394                                         02S0394
*                                                               03S0395
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS.RETCKPT: THIS SEGMENT PROCESSES THE RETURN FROM THE */
*     /* CHECKPOINT SUBTASK TO OPERATOR CONTROL. THE TOP ELEMENT ON  */
*     /* THE OP CONTROL CHECKPOINT QUEUE IS REMOVED. IF ANOTHER      */
*     /* ELEMENT IS ON THE QUEUE, THEN A SUBROUTINE IS CALLED TO POST*/
*     /* THE CHECKPOINT SUBTASK.                                     */
*     /*                                                             */
*     /***************************************************************/
*                                                               03S0395
*     DO;                                                       03S0395
*       OPCCOPCE=OPCCKPTF;          /* MAKE TOP ELEMENT ON QUEUE TO
*                                      CKPT THE CURRENT OPERATOR
*                                      CONTROL ELEMENT               */
         L     @10,OPCCKPTF(,ROPCAVT)                           03S0396
         ST    @10,OPCCOPCE(,ROPCAVT)                           03S0396
*       ROPCE=OPCCOPCE;             /* RESET CURRENT ELEMENT POINTER */
         LR    ROPCE,@10                                        03S0397
*       OCCKPKEY=TIC;               /* RESTORE TIC COMMAND TO THE
*                                      OPCE                          */
         MVI   OCCKPKEY(ROPCE),X'08'                            03S0398
*       OPCCKPTF=OCLINK;            /* GET LINK FROM CURRENT ELEMENT
*                                      AND MAKE IT THE TOP ELEMENT ON
*                                      THE QUEUE                     */
         L     @10,OCLINK(,ROPCE)                               03S0399
         ST    @10,OPCCKPTF(,ROPCAVT)                           03S0399
*       AVTCKENV=OFF;               /* RESET ENVIRONMENT CKPT REQUEST
*                                      FLAG                          */
         NI    AVTCKENV(RAVT),B'11011111'                       03S0400
*       IF OPCCKPTF^=0              /* CHECK FOR QUEUE NOT EMPTY     */
*         THEN                      /* THERE IS AN ELEMENT ON THE
*                                      QUEUE                         */
         LTR   @10,@10                                          03S0401
         BZ    @RF00401                                         03S0401
*         IF AVTCKHLT=OFF           /* TEST FOR HALT COMMAND    03S0402
*                                      PROCESSED BY CKPT             */
*           THEN                    /* HALT COMMAND HAS NOT BEEN
*                                      PROCESSED                     */
         TM    AVTCKHLT(RAVT),B'00001000'                       03S0402
         BNZ   @RF00402                                         03S0402
*           CALL POSTCKPT;          /* CALL SUBROUTINE TO POST THE
*                                      CKPT SUBTASK                  */
         BAL   @14,POSTCKPT                                     03S0403
*     END;                                                      03S0404
*   ELSE                            /* NOT A RETURN FROM CHECKPOINT
*                                      SUBTASK                       */
*     IF OPCRSTRT=ON                /* TEST FOR A RESTART OF TCAM    */
*       THEN                        /* THIS IS A RESTART             */
         B     @RC00394                                         02S0405
@RF00394 TM    OPCRSTRT(ROPCAVT),B'00100000'                    02S0405
         BNO   @RF00405                                         02S0405
*       DO;                                                     02S0406
*         AVTOPCCC=0;               /* CLEAR OP CONTROL COMPLETION
*                                      CODE                          */
         MVI   AVTOPCCC(RAVT),X'00'                             02S0407
*         GEN NODEFS NOEXIT NOFLOWS REFS(AVTCKPEC) SEQFLOW SETS(REG00,
*             RPARM);                                           02S0408
              POST  AVTCKPEC(RAVT)    POST CKPT ECB COMPLETE
*         TCAMRST=ON;               /* SET TCAM RESTART STATE        */
         OI    TCAMRST,B'10000000'                              02S0409
*       END;                                                    02S0410
*     ELSE                          /* NOT RESTART                   */
*       IF OCTOTE=ON                /* TEST FOR TOTE REQUEST         */
*         THEN                      /* THIS IS A TOTE REQUEST        */
         B     @RC00405                                         02S0411
@RF00405 TM    OCTOTE(ROPCE),B'01000000'                        02S0411
         BNO   @RF00411                                         02S0411
*         DO;                                                   02S0412
*           OPCMODID=WRITEMOD;      /* SET TO EXIT TO THE OPCTL WRITE
*                                      MODULE                        */
         MVC   OPCMODID(2,ROPCAVT),@CC02517                     02S0413
*           REG00=OCPARM;           /* SET RETURN CODE               */
         LH    REG00,OCPARM(,ROPCE)                             02S0414
*           OCWTG=0;                /* CLEAR THE WHERE TO GO FIELD   */
         SLR   @10,@10                                          02S0415
         STH   @10,OCWTG(,ROPCE)                                02S0415
*           EXITSET=ON;             /* EXIT PARAMETERS HAVE BEEN SET */
         OI    EXITSET,B'01000000'                              02S0416
*         END;                                                  02S0417
*       ELSE                        /* NOT TOTE => PROCESS THE  02S0418
*                                      COMMAND                       */
*         DO;                                                   02S0418
         B     @RC00411                                         02S0418
@RF00411 DS    0H                                               02S0419
*           OCCKPKEY=0;             /* CLEAR REQUEST KEY FIELD       */
         MVI   OCCKPKEY(ROPCE),X'00'                            02S0419
*           IF OCOPTCDE=OCHALT×OCOPTCDE=OCCLOSEQ×OCOPTCDE=OCCLOSEF THEN
         CLI   OCOPTCDE(ROPCE),X'3C'                            02S0420
         BE    @RT00420                                         02S0420
         CLI   OCOPTCDE(ROPCE),X'10'                            02S0420
         BE    @RT00420                                         02S0420
         CLI   OCOPTCDE(ROPCE),X'11'                            02S0420
         BNE   @RF00420                                         02S0420
@RT00420 DS    0H                                               02S0421
*                                   /* IF THIS IS HALT, MPP FLUSH, OR
*                                      MPP QUICK, PROCESS CMD   02S0421
*                                                            @YM08529*/
*                                                               03S0421
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS.HALT: THIS SEGMENT PROCESSES A HALT TCAM    */
*             /* COMMAND. IF TCAM CHECKPOINT IS ACTIVE, THEN REQUEST */
*             /* AN ENVIRONMENT CHECKPOINT. OTHERWISE, TPOST THE     */
*             /* COMPLETION ELEMENT TO INDICATE THAT CLOSEDOWN IS    */
*             /* COMPLETE.                                           */
*             /*                                                     */
*             /*******************************************************/
*                                                               03S0421
*             DO;                                               03S0421
*               IF AVTCKTAC=ON      /* TEST FOR TCAM CKPT ACTIVE     */
*                 THEN              /* TCAM CKPT IS ACTIVE           */
         TM    AVTCKTAC(RAVT),B'10000000'                       03S0422
         BNO   @RF00422                                         03S0422
*                 DO;                                           03S0423
*                   OCCKPINC=ON;    /* TREAT HALT COMMAND AS AN 03S0424
*                                      INCIDENT ...                  */
*                   OCCKPENV=ON;    /* ... THAT REQUESTS A FORCED
*                                      ENVIRONMENT                   */
         OI    OCCKPINC(ROPCE),B'10010000'                      03S0425
*                   AVTCMCPN=ON;    /* INDICATE CHKPT REQUESTED 03S0426
*                                                            @YM08529*/
         OI    AVTCMCPN(RAVT),B'01000000'                       03S0426
*                   CALL QUEUE;     /* QUEUE THE HALT COMMAND FOR
*                                      CKPT                          */
         BAL   @14,QUEUE                                        03S0427
*                 END;                                          03S0428
*               ELSE                /* TCAM CKPT IS NOT ACTIVE       */
*                 DO;                                           03S0429
         B     @RC00422                                         03S0429
@RF00422 DS    0H                                               03S0430
*                   RPARM=ADDR(AVTCLOSB);/* ELEMENT TO TPOST IS THE
*                                      COMPLETION ELEMENT            */
         LA    RPARM,AVTCLOSB(,RAVT)                            03S0430
*                   RPARM->RECBPRI=PRICLSDN;/* SET PRIORITY          */
         MVI   RECBPRI(RPARM),X'10'                             03S0431
*                   POSTTCAM=ON;    /* REQUEST POST OF TCAM          */
         OI    POSTTCAM,B'00100000'                             03S0432
*                   CALL UPDRDYQ;   /* REQUEST THAT ELEMENT BE PUT ON
*                                      THE ASYNCHRONOUS READY QUEUE  */
         BAL   @14,UPDRDYQ                                      03S0433
*                 END;                                          03S0434
*             END;                                              03S0435
*           ELSE                    /* NOT A HALT TCAM COMMAND       */
*             IF(AVTCKTAC=ON×AVTCKNAC=ON)/* TEST FOR CKPT ACTIVE     */
*               THEN                /* EITHER TCAM OR 3705 CKPT IS
*                                      ACTIVE                        */
         B     @RC00420                                         02S0436
@RF00420 TM    AVTCKTAC(RAVT),B'11000000'                       02S0436
         BZ    @RF00436                                         02S0436
*                                                               03S0437
*               /*****************************************************/
*               /*                                                   */
*               /* PROCESS.COMMAND: THIS SEGMENT DETERMINES THE TYPE */
*               /* OF COMMAND. EACH COMMAND CAUSES A CHECKPOINT      */
*               /* REQUEST TO BE QUEUED FOR THE CHECKPOINT SUBTASK.  */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0437
*               DO;                                             03S0437
*                 IF OCOPTCDE=OCMODIFY/* TEST FOR COMMAND TYPE       */
*                   THEN            /* THIS IS A MODIFY COMMAND      */
         CLI   OCOPTCDE(ROPCE),X'44'                            03S0438
         BNE   @RF00438                                         03S0438
*                                                               04S0439
*                   /*************************************************/
*                   /*                                               */
*                   /* PROCESS.COMMAND.MODIFY: THIS SEGMENT PROCESSES*/
*                   /* THE MODIFY COMMANDS. SPECIAL PROCESSING IS    */
*                   /* DONE FOR ACTIVATE/DEACTIVTE COMMANDS AND FOR  */
*                   /* 3705 RESTART COMMANDS. THE OTHER COMMANDS ARE */
*                   /* PROCESSED VIA A COMMAND TABLE SEARCH TO       */
*                   /* DETERMINE WHETHER A 3705 CHECKPOINT OR AN     */
*                   /* INCIDENT CHECKPOINT IS REQUIRED.              */
*                   /*                                               */
*                   /*************************************************/
*                                                               04S0439
*                   DO;                                         04S0439
*                     IF(OCSWITCH=OCACTVTE×OCSWITCH=OCDEACTE)/* TEST
*                                      COMMAND TYPE                  */
*                       THEN        /* THIS IS AN ACT/DEACT CMD      */
         CLI   OCSWITCH(ROPCE),X'22'                            04S0440
         BE    @RT00440                                         04S0440
         CLI   OCSWITCH(ROPCE),X'23'                            04S0440
         BNE   @RF00440                                         04S0440
@RT00440 DS    0H                                               04S0441
*                                                               05S0441
*                       /*********************************************/
*                       /*                                           */
*                       /* PROCESS.COMMAND.MODIFY.ACTDEACT: THIS     */
*                       /* SEGMENT PROCESSES ACTIVATE/DEACTIVATE     */
*                       /* COMMANDS.ACTIVTE 3705 COMMANDS RESULT IN A*/
*                       /* REQUEST FOR 3705 CHECKPOINT. ACTIVATE AND */
*                       /* DEACTIVTE 3705 COMMANDS RESULT IN A       */
*                       /* REQUEST FOR AN ENVIRONMENT CHECKPOINT IF  */
*                       /* ONE HAS NOT ALREADY BEEN REQUESTED. OTHER */
*                       /* TYPES OF ACT/DEACT'S RESULT IN A REQUEST  */
*                       /* FOR AN INCIDENT CHECKPOINT.               */
*                       /*                                           */
*                       /*********************************************/
*                                                               05S0441
*                       DO;                                     05S0441
*                         IF AVTVTMCP=ON&OCADNCP=ON/* TEST FOR TYPE
*                                      OF ACT/DEACT COMMAND          */
*                           THEN    /* THIS IS AN ACT/DEACT OF AN NCP*/
         TM    AVTVTMCP(RAVT),B'10000000'                       05S0442
         BNO   @RF00442                                         05S0442
         LR    @10,ROPCE                                        05S0442
         AL    @10,@CF02576                                     05S0442
         TM    OCADNCP-2(@10),B'00010000'                       05S0442
         BNO   @RF00442                                         05S0442
*                           DO;                                 05S0443
*                             IF OCSWITCH=OCACTVTE/* TEST COMMAND
*                                      TYPE                          */
*                               THEN/* ACTIVATE COMMAND              */
         CLI   OCSWITCH(ROPCE),X'22'                            05S0444
         BNE   @RF00444                                         05S0444
*                               OCCKPNCP=ON;/* INDICATE THAT 3705
*                                      CKPT REQUESTED                */
         OI    OCCKPNCP(ROPCE),B'01000000'                      05S0445
*                             ELSE  /* NOT ACTIVATE => DEACTIVTE     */
*                               OCCKPINC=ON;/* INDICATE THAT A TCAM
*                                      CKPT IS REQUESTED             */
         B     @RC00444                                         05S0446
@RF00444 OI    OCCKPINC(ROPCE),B'10000000'                      05S0446
*                             IF AVTCKREQ=OFF/* TEST FOR EXISTING
*                                      ENVIRONMENT REQUEST ON THE OP
*                                      CONTROL CKPT QUEUE            */
*                               THEN/* NO EXISTING ENVIRONMENT  05S0447
*                                      REQUEST                       */
@RC00444 TM    AVTCKREQ(RAVT),B'00010000'                       05S0447
         BNZ   @RF00447                                         05S0447
*                               OCCKPENV=ON;/* INDICATE THAT AN 05S0448
*                                      ENVIRONMANT CKPT IS NEEDED    */
         OI    OCCKPENV(ROPCE),B'00010000'                      05S0448
*                           END;                                05S0449
*                         ELSE      /* ACT/DEACT NOT FOR THE NCP     */
*                           OCCKPINC=ON;/* INDICATE THAT AN INCIDENT
*                                      CKPT IS TO BE TAKEN           */
         B     @RC00442                                         05S0450
@RF00442 OI    OCCKPINC(ROPCE),B'10000000'                      05S0450
*                       END;                                    05S0451
*                     ELSE          /* NOT AN ACT/DEACT              */
*                       IF OCSWITCH=OCRSTART/* TEST COMMAND TYPE     */
*                         THEN      /* THIS IS A 3705 RESTART        */
         B     @RC00440                                         04S0452
@RF00440 CLI   OCSWITCH(ROPCE),X'F1'                            04S0452
         BNE   @RF00452                                         04S0452
*                         OCCKPRST=ON;/* INDICATE THAT 3705 RESTART
*                                      IS REQUESTED                  */
         OI    OCCKPRST(ROPCE),B'00100000'                      04S0453
*                       ELSE        /* NOT AN 3705 RESTART           */
*                         DO;                                   04S0454
         B     @RC00452                                         04S0454
@RF00452 DS    0H                                               04S0455
*                           OCCKPINC=ON;/* ASSUME THAT AN INCIDENT
*                                      CKPT WILL BE TAKEN IF THIS IS
*                                      NOT A 3705 CKPT REQUEST       */
         OI    OCCKPINC(ROPCE),B'10000000'                      04S0455
*                           RPARM=ADDR(MODTABLE);/* GET ADDR OF THE
*                                      TABLE OF VALID 3705 CKPT 04S0456
*                                      MODIFY TYPES                  */
         LA    RPARM,MODTABLE                                   04S0456
*                           DO COUNT=1 TO NUMENT WHILE(OCCKPINC=ON);
         LA    COUNT,1                                          04S0457
         B     @DE00457                                         04S0457
@DL00457 TM    OCCKPINC(ROPCE),B'10000000'                      04S0457
         BNO   @DC00457                                         04S0457
*                                   /* SCAN THROUGH THE TABLE   04S0458
*                                      COMPARING THE CURRENT COMMAND
*                                      WITH THE TABLE ENTRIES        */
*                             IF OCSWITCH=TABENTRY/* COMPARE COMMAND
*                                      TO CURRENT TABLE ENTRY        */
*                               THEN/* COMMAND EQUALS THE CURRENT
*                                      TABLE ENTRY                   */
         CLC   OCSWITCH(1,ROPCE),TABENTRY(RPARM)                04S0458
         BNE   @RF00458                                         04S0458
*                               DO;                             04S0459
*                                 OCCKPNCP=ON;/* REQUEST A 3705 CKPT */
*                                 OCCKPINC=OFF;/* RESET REQUEST FOR
*                                      INCIDENT CKPT                 */
         OI    OCCKPNCP(ROPCE),B'01000000'                      04S0461
         NI    OCCKPINC(ROPCE),B'01111111'                      04S0461
*                               END;                            04S0462
*                             ELSE  /* COMMAND IS NOT EQUAL TO  04S0463
*                                      CURRENT ENTRY                 */
*                               RPARM=RPARM+LENGTH(TABENTRY);/* POINT
*                                      TO NEXT ENTRY IN THE TABLE    */
         B     @RC00458                                         04S0463
@RF00458 AL    RPARM,@CF00061                                   04S0463
*                           END;                                04S0464
@RC00458 AL    COUNT,@CF00061                                   04S0464
@DE00457 SLR   @10,@10                                          04S0464
         IC    @10,NUMENT                                       04S0464
         CR    COUNT,@10                                        04S0464
         BNH   @DL00457                                         04S0464
@DC00457 DS    0H                                               04S0465
*                         END;                                  04S0465
*                   END;                                        04S0466
*                 ELSE              /* NOT A MODIFY COMMAND          */
*                   IF OCOPTCDE=OCTCHNG/* TEST COMMAND TYPE          */
*                     THEN          /* THIS IS A TCHNG COMMAND       */
         B     @RC00438                                         03S0467
@RF00438 CLI   OCOPTCDE(ROPCE),X'56'                            03S0467
         BNE   @RF00467                                         03S0467
*                     OCCKPNCP=ON;  /* INDICATE THAT 3705 CKPT IS
*                                      REQUESTED                     */
         OI    OCCKPNCP(ROPCE),B'01000000'                      03S0468
*                   ELSE            /* NOT A TCHNG COMMAND           */
*                                                               04S0469
*                     /***********************************************/
*                     /*                                             */
*                     /* PROCESS.COMMAND.OTHERCMD: THIS SEGMENT      */
*                     /* PROCESSES THOSE COMMANDS THAT ARE NOT       */
*                     /* MODIFIES. IF INVITATION LISTS ARE           */
*                     /* CHECKPOINTED, THEN THE COMMAND IS           */
*                     /* CHECKPOINTED. SPECIAL CHECKPOINT PROCESSING */
*                     /* IS REQUIRED BY AN ICHNG COMMAND IF IT IS    */
*                     /* DIRECTED TO 3705 LINE. OTHER COMMANDS GET   */
*                     /* INCIDENT CHECKPOINTS. IF INVITATION LIST ARE*/
*                     /* NOT CHECKPOINTED, THEN ICHNG, START TERM    */
*                     /* (E), STOP TERM (E), AND STOP TERM (EM)      */
*                     /* COMMANDS ARE NOT CHECKPOINTED.              */
*                     /*                                             */
*                     /***********************************************/
*                                                               04S0469
*                     DO;                                       04S0469
         B     @RC00467                                         04S0469
@RF00467 DS    0H                                               04S0470
*                       IF AVTSTAIN=ON/* TEST COLD START VALUE OF
*                                      INVITATION LIST CKPT          */
*                         THEN      /* INVITATION LIST (AND RELATED
*                                      TYPE FIELDS) ARE TO BE CKPT'ED*/
         TM    AVTSTAIN(RAVT),B'00000100'                       04S0470
         BNO   @RF00470                                         04S0470
*                         DO;                                   04S0471
*                           OCCKPINC=ON;/* DEFAULT TO REQUEST AN
*                                      INCIDENT CKPT                 */
         OI    OCCKPINC(ROPCE),B'10000000'                      04S0472
*                           IF OCOPTCDE=OCICHNG/* TEST COMMAND TYPE  */
*                             THEN  /* THIS IS AN ICHNG COMMAND      */
         CLI   OCOPTCDE(ROPCE),X'49'                            04S0473
         BNE   @RF00473                                         04S0473
*                             IF(OCMODNME(1:4)^='00000000'X&OCMODNME(1:
*                                 4)^='00000001'X)/* TEST ICHNG TYPE */
*                               THEN/* THIS IS NOT AN ACT OR DEACT
*                                      ICHNG                         */
         CLC   OCMODNME(4,ROPCE),@CB02547                       04S0474
         BE    @RF00474                                         04S0474
         CLC   OCMODNME(4,ROPCE),@CB02548                       04S0474
         BE    @RF00474                                         04S0474
*                               DO;                             04S0475
*                                 RPARM=OCTRMTBL;/* GET TNT OFFSET
*                                      FOR THE LINE                  */
         LH    RPARM,OCTRMTBL(,ROPCE)                           04S0476
         N     RPARM,@CF02578                                   04S0476
*                                 CALL IEDQTNT;/* GET TERMINAL ENTRY
*                                      ADDRESS                       */
         L     @15,AVTRNMPT(,RAVT)                              04S0477
         BALR  @14,@15                                          04S0477
*                                 RTRM=RPARM-LENGTH(IEDNTRM);/* GET
*                                      ADDRESS OF TERM ENTRY PREFIX  */
         LR    RTRM,RPARM                                       04S0478
         SL    RTRM,@CF02324                                    04S0478
*                                 IF TRMPREF=ON/* TEST TYPE OF  04S0479
*                                      RESOURCE                      */
*                                   THEN/* THIS IS A 3705 RESOURCE   */
         TM    TRMPREF(RTRM),B'00010000'                        04S0479
         BNO   @RF00479                                         04S0479
*                                   DO;                         04S0480
*                                    OCCKPNCP=ON;/* REQUEST 3705 CKPT*/
*                                    OCCKPINC=OFF;/* RESET REQUEST
*                                      FOR INCIDENT CKPT             */
         OI    OCCKPNCP(ROPCE),B'01000000'                      04S0482
         NI    OCCKPINC(ROPCE),B'01111111'                      04S0482
*                                   END;                        04S0483
*                               END;                            04S0484
*                         END;                                  04S0485
*                       ELSE        /* DO NOT CKPT COMMANDS THAT
*                                      AFFECT INVITATION LISTS OR
*                                      RELATED FIELDS                */
*                                                               05S0486
*                         /*******************************************/
*                         /*                                         */
*                         /* PROCESS.COMMAND.OTHERCMD.INVLIST: THIS  */
*                         /* SEGMENT DETERMINES WHETHER OR NOT THIS  */
*                         /* COMMAND SHOULD BE CHECKPOINTED IF       */
*                         /* INVITATION LISTS ARE NOT CHECKPOINTED.  */
*                         /*                                         */
*                         /*******************************************/
*                                                               05S0486
*                         DO;                                   05S0486
         B     @RC00470                                         05S0486
@RF00470 DS    0H                                               05S0487
*                           IF OCOPTCDE=OCICHNG/* TEST COMMAND TYPE  */
*                             THEN                              05S0487
         CLI   OCOPTCDE(ROPCE),X'49'                            05S0487
         BE    @RT00487                                         05S0487
*                             ;     /* THIS IS AN ICHNG COMMAND => NO
*                                      CKPT                          */
*                           ELSE    /* NOT AN ICHNG                  */
*                             IF OCOPTCDE=OCVARY/* TEST COMMAND TYPE */
*                               THEN/* THIS IS A VARY COMMAND        */
         CLI   OCOPTCDE(ROPCE),X'28'                            05S0489
         BNE   @RF00489                                         05S0489
*                               DO;                             05S0490
*                                 IF SSTERM1=ON&/* TEST FOR ...      */
*                                     SSTERM3=ON&/* ... START ...    */
*                                     SSTERM4=ON/* ...... TERM,B
*                                      COMMAND                       */
*                                   THEN/* THIS IS A START TERM,B    */
         TM    SSTERM1(ROPCE),B'00001011'                       05S0491
         BNO   @RF00491                                         05S0491
*                                   OCCKPINC=ON;/* REQUEST AN   05S0492
*                                      INCIDENT CKPT                 */
         OI    OCCKPINC(ROPCE),B'10000000'                      05S0492
*                                 ELSE/* NOT START TERM,B            */
*                                   IF SSTERM2=ON&/* TEST FOR STOP
*                                      TERM ...                      */
