*       OPCCKPTF=ROPCE;             /* FIRST ON QUEUE IS THE CURRENT
*                                      ELEMENT                       */
         ST    ROPCE,OPCCKPTF(,ROPCAVT)                         02S0607
*       OPCCKPTL=ROPCE;             /* LAST ON QUEUE IS THE CURRENT
*                                      ELEMEENT                      */
         ST    ROPCE,OPCCKPTL(,ROPCAVT)                         02S0608
*       CALL POSTCKPT;              /* POST THE OP CONTROL REQUEST
*                                      ELEMENT TO CKPT               */
         BAL   @14,POSTCKPT                                     02S0609
*     END;                                                      02S0610
*   ELSE                            /* QUEUE IS NOT EMPTY            */
*     DO;                                                       02S0611
         B     @RC00605                                         02S0611
@RF00605 DS    0H                                               02S0612
*       OPCCKPTL->OCLINK=ROPCE;     /* MAKE LAST ELEMENT ON THE QUEUE
*                                      LINK TO THE CURRENT ELEMENT   */
         L     @10,OPCCKPTL(,ROPCAVT)                           02S0612
         ST    ROPCE,OCLINK(,@10)                               02S0612
*       OPCCKPTL=ROPCE;             /* MAKE CURRENT THE LAST ON THE
*                                      QUEUE                         */
         ST    ROPCE,OPCCKPTL(,ROPCAVT)                         02S0613
*       IF OCCKPENV=ON              /* TEST FOR ENVIRONMENT NEEDED   */
*         THEN                      /* AN ENVIRONMENT CKPT WILL BE
*                                      REQUESTED                     */
         TM    OCCKPENV(ROPCE),B'00010000'                      02S0614
         BNO   @RF00614                                         02S0614
*         AVTCKREQ=ON;              /* INDICATE THAT AN ENV CKPT WILL
*                                      BE REQUESTED                  */
         OI    AVTCKREQ(RAVT),B'00010000'                       02S0615
*     END;                                                      02S0616
@RF00614 DS    0H                                               02S0617
*   IF OCATTACH=ON                  /* TEST FOR OPCE'S ATTACHED 02S0617
*                                                            @YM06958*/
*     THEN                          /* OPCE'S ARE ATTACHED   @YM06958*/
@RC00605 TM    OCATTACH(ROPCE),B'00000010'                      02S0617
         BNO   @RF00617                                         02S0617
*     DO;                           /*                       @YM06958*/
*       OPCNEXT=OCELMPTR;           /* MOVE ATTACHED OPCE'S SO THAT
*                                      THE OP CONTROL DISPATCHER CAN
*                                      PROCESS THEM          @YM06958*/
         L     @10,OCELMPTR-1(,ROPCE)                           02S0619
         LA    @10,0(,@10)                                      02S0619
         ST    @10,OPCNEXT(,ROPCAVT)                            02S0619
*       OCATTACH=OFF;               /* INDICATE THAT NO OPCE'S ARE
*                                      ATTACHED              @YM06958*/
         NI    OCATTACH(ROPCE),B'11111101'                      02S0620
*     END;                          /*                       @YM06958*/
*   OPCMODID=DISPATCH;              /* EXIT TO THE OPCTL DISPATCHER  */
@RF00617 MVC   OPCMODID(2,ROPCAVT),@CC02515                     02S0622
*   REG00=0;                        /* CLEAR FOR ENTRY TO OPCTL 02S0623
*                                      DISPATCHER                    */
         SLR   REG00,REG00                                      02S0623
*   EXITSET=ON;                     /* EXIT PARAMETERS HAVE BEEN SET */
         OI    EXITSET,B'01000000'                              02S0624
*   END QUEUE;                                                  02S0625
@EL00003 DS    0H                                               02S0625
@EF00003 DS    0H                                               02S0625
@ER00003 L     @14,@SA00003                                     02S0625
         BR    @14                                              02S0625
*                                                               02S0626
*   /*****************************************************************/
*   /*                                                               */
*   /* UPDRDYQ: THIS SUBROUTINE PLACES AN ELEMENT ADDRESSED BY RPARM */
*   /* ON THE ASYNCHRONOUS READY QUEUE. IF THE POSTTCAM STATE BIT IS */
*   /* SET, THEN POST THE TCAM MCP'S ECB COMPLETE.                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0626
*UPDRDYQ:                                                       02S0626
*   PROC OPTIONS(RETREG,SAVE(14));                              02S0626
UPDRDYQ  ST    @14,@SA00004                                     02S0626
*   RFY                                                         02S0627
*     REG15 RSTD;                                               02S0627
*   REG15=CVTAQAVT;                 /* GET ADDRESS OF TCX            */
         L     @10,CVTPTR                                       02S0628
         L     REG15,CVTAQAVT(,@10)                             02S0628
*   REG00=TCXREADY;                 /* GET ADDR OF TOP ELEMENT ON
*                                      ASYNCHRONOUS READY QUEUE      */
         L     REG00,TCXREADY(,REG15)                           02S0629
*AGAIN:                                                         02S0630
*   RPARM->RECBLINK=REG00;          /* LINK CURRENT ELEMENT TO TOP ON
*                                      THE QUEUE                     */
AGAIN    STCM  REG00,7,RECBLINK(RPARM)                          02S0630
*   CS(REG00,RPARM,TCXREADY);       /* UPDATE ASYNCHRONOUS READY
*                                      QUEUE                         */
         CS    REG00,@01,TCXREADY(REG15)                        02S0631
*   BC(7,AGAIN);                    /* IF QUEUE WAS UPDATED     02S0632
*                                      ASYNCHRONOUSLY, TRY AGAIN     */
         BC    7,AGAIN                                          02S0632
*   IF POSTTCAM=ON                  /* TEST FOR REQUEST TO POST TCAM
*                                      MCP'S ECB COMPLETE    @YM05746*/
*     THEN                          /* POST OF TCAM IS REQUESTED
*                                                            @YM05746*/
         TM    POSTTCAM,B'00100000'                             02S0633
         BNO   @RF00633                                         02S0633
*     DO;                           /*                       @YM05746*/
*       GEN NODEFS NOEXIT NOFLOWS REFS(AVTOSECB) SEQFLOW SETS(REG00,
*           RPARM);                                             02S0635
           POST  AVTOSECB(RAVT)    POST TCAM COMPLETE
*     END;                          /*                       @YM05746*/
*   RFY                                                         02S0637
*     REG15 UNRSTD;                                             02S0637
*   END UPDRDYQ;                                                02S0638
@EL00004 DS    0H                                               02S0638
@EF00004 DS    0H                                               02S0638
@ER00004 L     @14,@SA00004                                     02S0638
         BR    @14                                              02S0638
