         TITLE 'IGG0CLAU, VSAM SUBALLOCATION ROUTINE                   *
                        '
IGG0CLAU CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGG0CLAU  75.012'                                 0001
         DROP  @15
@PROLOG  DS    0H                                                  0002
         USING @MAINENT,@15
         USING @DATD-(CCATEMPS-IGGCCA),R11
         DS 0F
         DC CL8'IGGPSALS'
         EJECT
         B     @PB00002                                            0088
IGGPSALS DS    0H                                                  0089
         ENTRY IGGPSALS
         DROP @15
Z003     DS    0H                                                  0091
Z003SALS DS    0H                                                  0092
         STM 12,14,12(13)
         LA 13,12(13)
         BALR 12,0
IGGSEGST DS 0H
         USING *,12
*                                                                  0099
*   /*****************************************************************/
*   /*                                                               */
*   /* CLEAR WORK AREA TO ZEROS                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0100
*   R2=ADDR(SALWRKPT);              /* POINT TO WORK AREA            */
         L     R2,CCAALPTR(,R11)                                   0100
         LA    R2,SALWRKPT+22(,R2)                                 0100
*   R3=LENGTH(SALAXWA)-LENGTH(SALFIL);/* SET LENGTH TO CLEAR         */
         LA    R3,468                                              0101
*   R5=ZERO;                        /* SET CLEAR CHARACTER           */
         SLR   R5,R5                                               0102
*   GEN(MVCL  R2,R4);               /* CLEAR OUT THE AREA            */
*                                                                  0103
         MVCL  R2,R4
*   /*****************************************************************/
*   /*                                                               */
*   /* CLEAR OUTPUT AREA TO ZEROS                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0104
*   R2=ADDR(CCADESA);               /* POINT TO OUTPUT AREA          */
         LA    R2,CCADESA(,R11)                                    0104
*   R3=LENGTH(CCADESA);             /* SET LENGTH OF AREA            */
         LA    R3,44                                               0105
*   GEN(MVCL  R2,R4);               /* CLEAR                         */
*                                                                  0106
         MVCL  R2,R4
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE THE NUMBER OF ENTRYS WHICH CAN BE MADE IN THE EXTENT*/
*   /* TABLE                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0107
*   SALEXTBN=((ADDR(SALAXWA)+LENGTH(SALAXWA))-/*                     */
*       ADDR(SALEXTBL))/LENGTH(SALEXTB);/*                           */
*                                                                  0107
         L     @10,CCAALPTR(,R11)                                  0107
         LA    @15,22                                              0107
         ALR   @15,@10                                             0107
         LA    @00,SALAXWA(,@10)                                   0107
         AL    @00,@CF02648                                        0107
         LA    @14,SALEXTBL(,@15)                                  0107
         SLR   @00,@14                                             0107
         SRDA  @00,32                                              0107
         D     @00,@CF00051                                        0107
         STC   @01,SALEXTBN(,@15)                                  0107
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE EXTRACT PARAMETER LISTS                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0108
*   CCAXCPL=ADDR(SALPARM);          /* POINT CCA TO PARM LIST        */
         LA    @14,SALPARM(,@15)                                   0108
         ST    @14,CCAXCPL(,R11)                                   0108
*   RESPECIFY                                                      0109
*     CTGPL BASED(CCAXCPL);         /*                               */
*   CTGWKA=ADDR(SALWLN1);           /* SET RETURN AREA POINTER       */
         LA    @09,SALWLN1(,@15)                                   0110
         ST    @09,CTGWKA(,@14)                                    0110
*   CTGNOFLD=ONE;                   /* NO. FIELD PTRS. IS 1          */
         MVI   CTGNOFLD(@14),X'01'                                 0111
*   CTGFIELD(1)=CCAXCPL+LENGTH(CTGPL)+/* SET CHAINING FIELD          */
*       LENGTH(CTGFIELD);           /* TO FIRST FL                   */
         LA    @09,32                                              0112
         ALR   @09,@14                                             0112
         ST    @09,CTGFIELD(,@14)                                  0112
*   R2=CTGFIELD(1);                 /* USE R2 AS BASE                */
         LR    R2,@09                                              0113
*   RESPECIFY                                                      0114
*     CTGFL BASED(R2);              /*                               */
*   CTGFLDNM=ADDR(DEXTENTS);        /* SET DICT. NAME OF FIELDS TO BE
*                                      RETURNED                      */
         LA    @14,DEXTENTS                                        0115
         ST    @14,CTGFLDNM(,R2)                                   0115
*   CTGFLCHN=R2+LENGTH(CTGFL)+      /* SET CHAIN FIELD               */
*       LENGTH(CTGFLDAT);           /*                               */
         LA    @14,24                                              0116
         LR    @09,R2                                              0116
         ALR   @09,@14                                             0116
         ST    @09,CTGFLCHN(,R2)                                   0116
*   R2=R2+LENGTH(CTGFL)+            /* MOVE BASE TO NEXT FL          */
*       LENGTH(CTGFLDAT);           /*                               */
         ALR   R2,@14                                              0117
*   CTGFLDNO=ONE;                   /* SET NO. PAIRS TO 1            */
         MVI   CTGFLDNO(R2),X'01'                                  0118
*   CTGFLDCD=CTGFLDGT;              /* CRITERIA = GREATER THAN       */
         MVI   CTGFLDCD(R2),X'20'                                  0119
*   CTGFLDGC=DSGPCD;                /* DATA SPACE GROUP CODE         */
         MVI   CTGFLDGC(R2),X'06'                                  0120
*   CTGFLDNM=ADDR(RELREPNO);        /* SET FIELD NAME                */
         LA    @09,RELREPNO                                        0121
         ST    @09,CTGFLDNM(,R2)                                   0121
*   CTGFLNG(1)=LENGTH(SALM);        /* SET DATA LENGTH               */
         LA    @09,2                                               0122
         ST    @09,CTGFLNG(,R2)                                    0122
*   CTGFLPT(1)=ADDR(SALM);          /* SET ADDR. OF SEQ NUMBER       */
         LA    @08,SALM(,@15)                                      0123
         ST    @08,CTGFLPT(,R2)                                    0123
*   CTGFLCHN=R2+LENGTH(CTGFL)+      /* CHAIN TO NEXT FL              */
*       LENGTH(CTGFLDAT);           /*                               */
         LR    @08,R2                                              0124
         ALR   @08,@14                                             0124
         ST    @08,CTGFLCHN(,R2)                                   0124
*   R2=R2+LENGTH(CTGFL)+            /* SET BASE TO NEXT FL           */
*       LENGTH(CTGFLDAT);           /*                               */
         ALR   R2,@14                                              0125
*   CTGFLDCD=ONE;                   /* NO. FIELDS TO 1               */
         MVI   CTGFLDCD(R2),X'01'                                  0126
*   CTGFLDCD=CTGFLDLE;              /* LESS THAN OR EQUAL SELECTION
*                                      CRITERIA                      */
         MVI   CTGFLDCD(R2),X'C0'                                  0127
*   CTGFLDNM=ADDR(CNTREPNO);        /* SET FIELD NAME                */
         LA    @14,CNTREPNO                                        0128
         ST    @14,CTGFLDNM(,R2)                                   0128
*   CTGFLNG(1)=LENGTH(SALM);        /* SET DATA LENGTH               */
         ST    @09,CTGFLNG(,R2)                                    0129
*   CTGFLPT(1)=ADDR(SALN);          /* SET DATA ADDRESS              */
         LA    @14,SALN(,@15)                                      0130
         ST    @14,CTGFLPT(,R2)                                    0130
*   SALN=MAXSPHDR;                  /* SET MAX NO SPACE HDRS         */
         STH   @09,SALN(,@15)                                      0131
*   RESPECIFY                                                      0132
*     CTGFL BASED(ADDR(SALFL(1)));  /* LEAVE ADDRESSABILITY TO FIRST
*                                      FL                            */
*                                                                  0132
*   /*****************************************************************/
*   /*                                                               */
*   /* SET TOTAL NUMBER OF TRACKS REQUESTED IN WORK AREA             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0133
*   RESPECIFY                                                      0133
*     CATRCD BASED(CCAR3REC);       /*                               */
*   IF SALOPTNS=SACYLOPT THEN       /* IS THIS CYL. ALLOC            */
         CLI   SALOPTNS(@10),192                                   0134
         BNE   @RF00134                                            0134
*     SALTOTTR=SALUNITS*CTVNOTRK;   /* YES, COMPUTE NO. TRACKS IN THE
*                                      REQUEST                       */
         L     @01,CCAR3REC(,R11)                                  0135
         ICM   @01,12,CTVNOTRK+48(@01)                             0135
         SRA   @01,16                                              0135
         ICM   @10,12,SALUNITS(@10)                                0135
         SRA   @10,16                                              0135
         MR    @00,@10                                             0135
         STH   @01,SALTOTTR(,@15)                                  0135
*   ELSE                            /* NO                            */
*     SALTOTTR=SALUNITS;            /* USE NO. TRACKS ASKED          */
*                                                                  0136
         B     @RC00134                                            0136
@RF00134 L     @10,CCAALPTR(,R11)                                  0136
         MVC   SALTOTTR+22(2,@10),SALUNITS(@10)                    0136
*   /*****************************************************************/
*   /*                                                               */
*   /* SET LENGTH OF EXTRACT RETURN AREA                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0137
*   SALWLN1=ADDR(SALEXTBL)-ADDR(SALWLN1);/*                          */
*                                                                  0137
@RC00134 LA    @10,22                                              0137
         AL    @10,CCAALPTR(,R11)                                  0137
         LA    @15,SALEXTBL(,@10)                                  0137
         LA    @14,SALWLN1(,@10)                                   0137
         LCR   @14,@14                                             0137
         ALR   @14,@15                                             0137
         STH   @14,SALWLN1(,@10)                                   0137
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE BASE OF EXTENT TABLE TO TOP OF TABLE               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0138
*   SALWRKPT=ADDR(SALEXTBL);                                       0138
         STCM  @15,15,SALWRKPT(@10)                                0138
*SALSA1:                            /*                               */
*                                                                  0139
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL TO MODULE IGG0CLAZ                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0139
*Z004EXT:                                                          0139
*   CALL IGGPEXT;                   /* USE EXTRACT ROUTINE TO GET  0139
*                                      DATA SPACE GROUPS             */
SALSA1   DS    0H                                                  0139
Z004EXT  L     @15,@CV02833                                        0139
         BALR  @14,@15                                             0139
*   IF CCACD1^=RCS THEN             /* WAS THERE AN ERROR            */
         CLI   CCACD1(R11),0                                       0140
         BE    @RF00140                                            0140
*     CALL EXIT;                    /* YES-DONT CONTINUE             */
         BAL   @14,EXIT                                            0141
*   ELSE                                                           0142
*     ;                             /*                               */
@RF00140 DS    0H                                                  0143
*   IF CTGFLPT(1)^=ZERO THEN        /* WERE ANY DSG RETURNED         */
         LA    @10,22                                              0143
         AL    @10,CCAALPTR(,R11)                                  0143
         LA    @15,SALFL(,@10)                                     0143
         L     @15,CTGFLPT(,@15)                                   0143
         LTR   @15,@15                                             0143
         BZ    @RF00143                                            0143
*     DO;                           /* YES, BEGIN PROCESSING         */
*       SALGRPPT=ADDR(SALFXEND);    /* SET BASE OF DATA SPACE GROUPS */
         LA    @15,SALFXEND(,@10)                                  0145
         STCM  @15,15,SALGRPPT(@10)                                0145
*       RESPECIFY                                                  0146
*         SALDASPG BASED(SALGRPPT); /*                               */
*       RESPECIFY                                                  0147
*         SALHDRXT BASED(SALXDPT);  /*                               */
*       IF SALWLN2>LENGTH(SALDASPG)+/* ADD UP SPACE REQD FOR 1       */
*           LENGTH(SALWLN2)+        /* GRP AND SEE IF MORE           */
*           LENGTH(SALWLN1) THEN    /* WAS RETURNED                  */
         CLC   SALWLN2(2,@10),@CH00136                             0148
         BNH   @RF00148                                            0148
*         SALSHCT=TWO;              /* YES, TWO GROUPS               */
         MVI   SALSHCT(@10),X'02'                                  0149
*       ELSE                        /* NO                            */
*         SALSHCT=ONE;              /* ONLY ONE GROUP                */
         B     @RC00148                                            0150
@RF00148 L     @10,CCAALPTR(,R11)                                  0150
         MVI   SALSHCT+22(@10),X'01'                               0150
*       DO SALSHIP=1 TO SALSHCT;    /* LOOP COVERS ALL DATA SPACE  0151
*                                      GROUPS GOTTEN                 */
@RC00148 LA    @10,1                                               0151
         B     @DE00151                                            0151
@DL00151 DS    0H                                                  0152
*         SALGRPPT=SALGRPPT+(SALSHIP-ONE)/* ADDRESS PROPER DATA      */
*             *LENGTH(SALDASPG);    /* SPACE GROUP BY THE LAST     0152
*                                      EXTRACT                       */
         LA    @10,22                                              0152
         AL    @10,CCAALPTR(,R11)                                  0152
         SLR   @15,@15                                             0152
         IC    @15,SALSHIP(,@10)                                   0152
         BCTR  @15,0                                               0152
         MH    @15,@CH00134                                        0152
         ICM   @14,15,SALGRPPT(@10)                                0152
         ALR   @15,@14                                             0152
         STCM  @15,15,SALGRPPT(@10)                                0152
*         SALXDPT=SALGRPPT+LENGTH(SALWKHDR);/* POINT TO 1ST. EXTENT  */
         LA    @14,4                                               0153
         ALR   @14,@15                                             0153
         STCM  @14,15,SALXDPT(@10)                                 0153
*         IF SALUNIQ^=ON THEN       /* IS THIS GROUP UNIQUE          */
         TM    SALUNIQ(@15),B'10000000'                            0154
         BO    @RF00154                                            0154
*           DO SALXIN=1 TO SALNOEXT;/* NO, LOOP COVERS ALL EXTENTS IN
*                                      THIS GROUP                    */
         LA    @10,1                                               0155
         B     @DE00155                                            0155
@DL00155 DS    0H                                                  0156
*             SALWRKPT->SALEXTTT=SALXTT;/* MOVE TT TO EXTENT TABLE   */
         L     @10,CCAALPTR(,R11)                                  0156
         LA    @15,22                                              0156
         ALR   @15,@10                                             0156
         ICM   @14,15,SALWRKPT(@15)                                0156
         ICM   @09,15,SALXDPT(@15)                                 0156
         LH    @08,SALXTT(,@09)                                    0156
         N     @08,@CF02905                                        0156
         STH   @08,SALEXTTT(,@14)                                  0156
*             SALWRKPT->SALEXTNN=SALXNN;/* MOVE NN TO EXTENT TABLE   */
         LH    @09,SALXNN(,@09)                                    0157
         STH   @09,SALEXTNN(,@14)                                  0157
*             SALWRKPT->SALEXTSS=SALXIN+/* COMPUTE SEQ. NO. AND      */
*                 SIXTN*(SALSEQ-ONE);/* PLACE IT IN EXTENT TBL       */
         ICM   @01,15,SALGRPPT(@15)                                0158
         LH    @07,SALSEQ(,@01)                                    0158
         N     @07,@CF02905                                        0158
         BCTR  @07,0                                               0158
         SLA   @07,4                                               0158
         SLR   @06,@06                                             0158
         IC    @06,SALXIN(,@15)                                    0158
         ALR   @07,@06                                             0158
         STH   @07,SALEXTSS(,@14)                                  0158
*             IF SALOPTNS=SACYLOPT THEN/* IS THIS CYLINDER ALLOC     */
         CLI   SALOPTNS(@10),192                                   0159
         BNE   @RF00159                                            0159
*               DO;                 /* YES                           */
*                 R2=SALXTT//CTVNOTRK;/* FIND NO. TRKS. INTO CYL     */
         L     @10,CCAR3REC(,R11)                                  0161
         ICM   @10,12,CTVNOTRK+48(@10)                             0161
         SRA   @10,16                                              0161
         LR    @00,@08                                             0161
         SRDA  @00,32                                              0161
         DR    @00,@10                                             0161
         LR    R2,@00                                              0161
*                 IF R2^=ZERO THEN  /* AT TOP OF CYLINDER            */
         LTR   R2,R2                                               0162
         BZ    @RF00162                                            0162
*                   DO;             /* NO                            */
*                     SALWRKPT->SALEXTTT=((SALXTT/CTVNOTRK)*/* MOVE  */
*                         (CTVNOTRK))+CTVNOTRK;/* TT TO NEXT CYLINDER
*                                      BOUNDARY                      */
         LR    @01,@08                                             0164
         SLR   @00,@00                                             0164
         DR    @00,@10                                             0164
         MR    @00,@10                                             0164
         ALR   @01,@10                                             0164
         STH   @01,SALEXTTT(,@14)                                  0164
*                     SALWRKPT->SALEXTNN=SALXNN-(CTVNOTRK-R2);/*   0165
*                                      REDUCE NO. TRACKS BY AMOUNT 0165
*                                      DISCARDED TO GET TO THE     0165
*                                      BOUNDARY                      */
         SLR   @10,R2                                              0165
         SLR   @09,@10                                             0165
         STH   @09,SALEXTNN(,@14)                                  0165
*                   END;            /*                               */
*                 ELSE                                             0167
*                   ;               /* TRACKS AT CYLINDER TOP        */
@RF00162 DS    0H                                                  0168
*               END;                /*                               */
*             ELSE                                                 0169
*               ;                   /* NOT CYLINDER ALLOCATION       */
@RF00159 DS    0H                                                  0170
*             IF SALWRKPT->SALEXTNN^<SALCALN THEN/* IS NO. TRACKS IN
*                                      THIS EXTENT LESS THAN AN    0170
*                                      ALLOCATION UNIT               */
         L     @10,CCAALPTR(,R11)                                  0170
         LA    @15,22                                              0170
         ALR   @15,@10                                             0170
         ICM   @14,15,SALWRKPT(@15)                                0170
         LH    @09,SALEXTNN(,@14)                                  0170
         ICM   @10,12,SALCALN(@10)                                 0170
         SRA   @10,16                                              0170
         CR    @09,@10                                             0170
         BL    @RF00170                                            0170
*               DO;                 /* NO, SPACE IS USABLE           */
*                 SALEXTBA=SALEXTBA+ONE;/* BUMP COUNT OF ENTRYS      */
         LA    @10,1                                               0172
         SLR   @09,@09                                             0172
         IC    @09,SALEXTBA(,@15)                                  0172
         ALR   @10,@09                                             0172
         STC   @10,SALEXTBA(,@15)                                  0172
*                 SALWRKPT=SALWRKPT+/* POINT TO NEXT SPACE           */
*                     LENGTH(SALEXTB);/* IN EXTENT TABLE             */
         AL    @14,@CF00051                                        0173
         STCM  @14,15,SALWRKPT(@15)                                0173
*                 IF SALEXTBA=SALEXTBN THEN/* IS EXTENT TABLE FULL   */
         CLC   SALEXTBA(1,@15),SALEXTBN(@15)                       0174
         BNE   @RF00174                                            0174
*                   DO;             /* YES                           */
*Z005EDS:                                                          0176
*                     CALL IGGPEDS; /* GO PROCESS EXTENT TABLE       */
Z005EDS  BAL   @14,IGGPEDS                                         0176
*                     IF CCACD1^=RCS THEN/* I/O ERROR                */
         CLI   CCACD1(R11),0                                       0177
         BE    @RF00177                                            0177
*                       CALL EXIT;  /* YES, TERMINATE                */
         BAL   @14,EXIT                                            0178
*                     ELSE                                         0179
*                       ;           /* NO I/O ERROR                  */
@RF00177 DS    0H                                                  0180
*                     IF SALFIT=ON THEN/* WAS AN EXACT FIT FOUND     */
         L     @10,CCAALPTR(,R11)                                  0180
         TM    SALFIT+22(@10),B'10000000'                          0180
         BO    @RT00180                                            0180
*                       GO TO SALSB1;/* YES, SEARCH NO MORE          */
*                     ELSE                                         0182
*                       ;           /* EXACT FIT NOT FOUND           */
*                   END;            /*                               */
*                 ELSE                                             0184
*                   ;               /* EXTENT TABLE NOT FULL         */
@RF00174 DS    0H                                                  0185
*               END;                /*                               */
*             ELSE                                                 0186
*               ;                   /* TRKS GREATER THAN EXTENT      */
@RF00170 DS    0H                                                  0187
*             SALXDPT=SALXDPT+LENGTH(SALHDRXT);/* MOVE TO NEXT EXTENT*/
         LA    @10,22                                              0187
         AL    @10,CCAALPTR(,R11)                                  0187
         LA    @15,4                                               0187
         ICM   @14,15,SALXDPT(@10)                                 0187
         ALR   @15,@14                                             0187
         STCM  @15,15,SALXDPT(@10)                                 0187
*           END;                    /*                               */
         L     @01,CCAALPTR(,R11)                                  0188
         LA    @10,1                                               0188
         SLR   @00,@00                                             0188
         IC    @00,SALXIN+22(,@01)                                 0188
         ALR   @10,@00                                             0188
@DE00155 L     @01,CCAALPTR(,R11)                                  0188
         STC   @10,SALXIN+22(,@01)                                 0188
         L     @01,CCAALPTR(,R11)                                  0188
         ICM   @15,15,SALGRPPT+22(@01)                             0188
         SLR   @14,@14                                             0188
         IC    @14,SALNOEXT(,@15)                                  0188
         CR    @10,@14                                             0188
         BNH   @DL00155                                            0188
*         ELSE                                                     0189
*           ;                       /* UNIQUE GROUP                  */
@RF00154 DS    0H                                                  0190
*       END;                        /*                               */
         L     @01,CCAALPTR(,R11)                                  0190
         LA    @10,1                                               0190
         SLR   @00,@00                                             0190
         IC    @00,SALSHIP+22(,@01)                                0190
         ALR   @10,@00                                             0190
@DE00151 L     @01,CCAALPTR(,R11)                                  0190
         STC   @10,SALSHIP+22(,@01)                                0190
         L     @01,CCAALPTR(,R11)                                  0190
         SLR   @00,@00                                             0190
         IC    @00,SALSHCT+22(,@01)                                0190
         CR    @10,@00                                             0190
         BNH   @DL00151                                            0190
*       SALM=SALSEQ;                /* SET SEARCH PARM FOR EXTRACT   */
         LA    @10,22                                              0191
         AL    @10,CCAALPTR(,R11)                                  0191
         ICM   @01,15,SALGRPPT(@10)                                0191
         LH    @15,SALSEQ(,@01)                                    0191
         N     @15,@CF02905                                        0191
         STH   @15,SALM(,@10)                                      0191
*       GO TO SALSA1;               /* GO BACK AND GET MORE DATA   0192
*                                      SPACE GROUPS                  */
         B     SALSA1                                              0192
*     END;                          /*                               */
*   ELSE                                                           0194
*     ;                             /* NO DSG'S RETURNED             */
*                                                                  0194
@RF00143 DS    0H                                                  0195
*   /*****************************************************************/
*   /*                                                               */
*   /* ALL EXTENTS HAVE BEEN PROCESSED. TABLES WILL NOW BE PROCESSED */
*   /* IN ORDER TO PROVIDE OUTPUT EXTENTS.                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0195
*Z006EDS:                                                          0195
*   CALL IGGPEDS;                   /* PROCESS EXTENT TABLE          */
Z006EDS  BAL   @14,IGGPEDS                                         0195
*   IF CCACD1^=RCS THEN             /* I/O ERROR                     */
         CLI   CCACD1(R11),0                                       0196
         BE    @RF00196                                            0196
*     CALL EXIT;                    /* YES, TERMINATE                */
         BAL   @14,EXIT                                            0197
*   ELSE                                                           0198
*     ;                             /* NO ERROR                      */
@RF00196 DS    0H                                                  0199
*SALSB1:                            /*                               */
*   CCABMFLG=ZERO;                  /* CLEAR OPTIONS FLAG            */
SALSB1   MVI   CCABMFLG(R11),X'00'                                 0199
*   CCABMSET=ON;                    /* SHOW SET OPTION               */
         OI    CCABMSET(R11),B'00100000'                           0200
*   IF SALLRGNN^=ZERO THEN          /* LARGER OR EXACT EXTENT        */
         SLR   @10,@10                                             0201
         L     @01,CCAALPTR(,R11)                                  0201
         CH    @10,SALLRGNN+22(,@01)                               0201
