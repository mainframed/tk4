         B     @PB00003                                            0228
IGGPPUPC DS    0H                                                  0229
*   GEN(ENTRY IGGPPUPC );           /* IDENTIFY ENTRY-POINT SYMBOL   */
         ENTRY IGGPPUPC
*Z014:                              /* LABEL FOR PROCEDURE TRACKING  */
*Z014PUPC:                          /* LABEL FOR PROCEDURE TRACKING  */
*   RFY                                                            0230
*    (R5,                                                          0230
*     R6,                                                          0230
*     R7,                                                          0230
*     R8,                                                          0230
*     R9,                                                          0230
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */
Z014     DS    0H                                                  0230
Z014PUPC DS    0H                                                  0231
*   RFY                                                            0231
*    (R2,                                                          0231
*     R3,                                                          0231
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */
         STM 12,14,12(13)
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */
         LA 13,12(13)
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */
         BALR 12,0
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */
         LA 15,*-IGGSEGST(0,0)
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */
*                                                                  0236
         SR 12,15
*   /*****************************************************************/
*   /*                                                               */
*   /*                                                       @Y30SSSB*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0237
*   IF CCAF2COB^='00'B×             /* IF CATLG BUILD/OPEN OR        */
*       CCACRABU=ON THEN            /* CRA BUILD             @Y30SSSB*/
         TM    CCAF2COB(R11),B'00000110'                           0237
         BNZ   @RT00237                                            0237
         TM    CCACRABU(R11),B'00000001'                           0237
         BNO   @RF00237                                            0237
@RT00237 DS    0H                                                  0238
*     CALL EXIT;                    /* BYPASS UPDATE ACTION  @Y30SSSB*/
         BAL   @14,EXIT                                            0238
*   IF CAXF2REC=ON&                 /* RECOVERABLE CAT       @Y30SSSB*/
*       CCASCRA^=ON THEN            /* NOT SUPPRESS I/O      @Y30SSSB*/
*                                                                  0239
@RF00237 L     @10,CCAACB(,R11)                                    0239
         L     @10,ACBUAPTR(,@10)                                  0239
         TM    CAXF2REC(@10),B'00000010'                           0239
         BNO   @RF00239                                            0239
         TM    CCASCRA(R11),B'00100000'                            0239
         BO    @RF00239                                            0239
*     /***************************************************************/
*     /*                                                             */
*     /* CALL TO MODULE IGG0CLBO                                     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0240
*Z015RAPU:                                                         0240
*     CALL IGGPRAPU;                /*                       @Y30SSSB*/
Z015RAPU L     @15,@CV03156                                        0240
         BALR  @14,@15                                             0240
*   ELSE                                                           0241
*     ;                             /*                       @Y30SSSB*/
@RF00239 DS    0H                                                  0242
*   CCAMLRET=R13;                   /* IO ERR RETRN SV AR PTR        */
         ST    R13,CCAMLRET(,R11)                                  0242
*Z016TRPL:                                                         0243
*   CALL IGGPTRPL;                  /* TEST RPL GET-UPD STATUS       */
Z016TRPL BAL   @14,IGGPTRPL                                        0243
*   XIOOPT=0;                       /* PUT OPTIONS                   */
         MVI   XIOOPT(R11),X'00'                                   0244
*Z017XIO:                                                          0245
*   CALL IGGPXIO;                   /* PUT-UPD THE CATLG RECORD    0245
*                                      OP=URA-ADR-UPD-CHK-CIE----    */
Z017XIO  BAL   @14,IGGPXIO                                         0245
*   CALL EXIT;                      /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0246
