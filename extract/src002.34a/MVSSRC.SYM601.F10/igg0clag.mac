         L     @10,CCARPL1(,R11)                                   0275
         CLI   RPLERRCD(@10),16                                    0275
         BNE   @RF00275                                            0275
         L     @10,CCAURAB(,R11)                                   0275
         L     @10,CCACPE1(,@10)                                   0275
         CLI   RCDID(@10),C'D'                                     0275
         BNE   @RF00275                                            0275
         L     @10,CCAXCPL(,R11)                                   0275
         CLI   CTGTYPE(@10),C'P'                                   0275
         BE    @RT00275                                            0275
