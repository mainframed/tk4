*IGGPPDE:                                                          0251
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */
         B     @PB00004                                            0251
IGGPPDE  DS    0H                                                  0252
*   GEN(ENTRY IGGPPDE );            /* IDENTIFY ENTRY-POINT SYMBOL   */
         ENTRY IGGPPDE
*Z018:                              /* LABEL FOR PROCEDURE TRACKING  */
*Z018PDE:                           /* LABEL FOR PROCEDURE TRACKING  */
*   RFY                                                            0253
*    (R5,                                                          0253
*     R6,                                                          0253
*     R7,                                                          0253
*     R8,                                                          0253
*     R9,                                                          0253
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */
Z018     DS    0H                                                  0253
Z018PDE  DS    0H                                                  0254
*   RFY                                                            0254
*    (R2,                                                          0254
*     R3,                                                          0254
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */
         STM 12,14,12(13)
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */
         LA 13,12(13)
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */
         BALR 12,0
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */
         LA 15,*-IGGSEGST(0,0)
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */
         SR 12,15
*   GEN;                                                           0260
IGGPPDEC EQU   IGGPPDE                  /*ENTRY POINT FOR PUT-UPD CI*/
         ENTRY IGGPPDEC
*   IF CAXF2REC=ON&                 /* RECOVERABLE CAT       @Y30SSSB*/
*       CCASCRA=OFF THEN            /* NOT SUPPRESS I/O      @Y30SSSB*/
*                                                                  0261
         L     @10,CCAACB(,R11)                                    0261
         L     @10,ACBUAPTR(,@10)                                  0261
         TM    CAXF2REC(@10),B'00000010'                           0261
         BNO   @RF00261                                            0261
         TM    CCASCRA(R11),B'00100000'                            0261
         BNZ   @RF00261                                            0261
*     /***************************************************************/
*     /*                                                             */
*     /* CALL TO MODULE IGG0CLBO                                     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0262
*Z019RAPD:                                                         0262
*     CALL IGGPRAPD;                /* RECOVERY PROCESS      @Y30SSSB*/
Z019RAPD L     @15,@CV03158                                        0262
         BALR  @14,@15                                             0262
*   ELSE                                                           0263
*     ;                             /*                       @Y30SSSB*/
@RF00261 DS    0H                                                  0264
*   CCAMLRET=R13;                   /* IO ERR RETRN SV AR PTR        */
         ST    R13,CCAMLRET(,R11)                                  0264
*   LPSTOP=0;                       /* CLEAR LOOP STOP BYTE          */
         MVI   LPSTOP(R11),X'00'                                   0265
*   DO LPINDX=1 TO LENGTH(TNTYPES) WHILE(LPSTOP=0);/* TRUE-NAME TYPE
*                                      COMPARE LP                    */
         LA    @10,1                                               0266
         STC   @10,LPINDX(,R11)                                    0266
@DL00266 CLI   LPSTOP(R11),0                                       0266
         BNE   @DC00266                                            0266
*     IF RCDID=TNTYPES(LPINDX:LPINDX) THEN/* IS TYPE FOUND           */
         L     @10,CCAURAB(,R11)                                   0267
         L     @10,CCACPE1(,@10)                                   0267
         SLR   @05,@05                                             0267
         IC    @05,LPINDX(,R11)                                    0267
         LA    @15,TNTYPES-1(@05)                                  0267
         CLC   RCDID(1,@10),0(@15)                                 0267
         BNE   @RF00267                                            0267
*       LPSTOP=1;                   /* YES,STOP LOOP                 */
         MVI   LPSTOP(R11),X'01'                                   0268
*   END;                                                           0269
@RF00267 LA    @10,1                                               0269
         SLR   @05,@05                                             0269
         IC    @05,LPINDX(,R11)                                    0269
         ALR   @10,@05                                             0269
         STC   @10,LPINDX(,R11)                                    0269
         C     @10,@CF00845                                        0269
         BNH   @DL00266                                            0269
@DC00266 DS    0H                                                  0270
*   IF LPSTOP^=0 THEN               /* IF TRUE NAME ENTRY EXISTS     */
         CLI   LPSTOP(R11),0                                       0270
         BE    @RF00270                                            0270
*     DO;                           /* DELETE IT                     */
*       CCASRCH=RCDNAME;            /* SET UP SEARCH ARG             */
         L     @10,CCAURAB(,R11)                                   0272
         L     @10,CCACPE1(,@10)                                   0272
