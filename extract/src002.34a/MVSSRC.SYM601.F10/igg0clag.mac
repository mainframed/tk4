@RF00286 DS    0H                                                  0288
Z023TRPL BAL   @14,IGGPTRPL                                        0288
*   ARASGA=ARASGA&&ARASGA;          /* CLEAR ARA                     */
         L     @10,CCAR0REC(,R11)                                  0289
         XC    ARASGA(255,@10),ARASGA(@10)                         0289
*   ARASGB=ARASGB&&ARASGB;          /* MORE OF SAME                  */
         XC    ARASGB(250,@10),ARASGB(@10)                         0290
