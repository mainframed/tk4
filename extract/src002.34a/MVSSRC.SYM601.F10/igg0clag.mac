         TITLE 'IGG0CLAG - VSAM CATALOG I/O SUBFUNCTIONS               *
                        '
IGG0CLAG CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGG0CLAG  75.123'                                 0001
         DROP  @15
@PROLOG  DS    0H                                                  0002
         USING @MAINENT,@15
         USING @DATD-(CCATEMPS-IGGCCA),R11
         DS 0F
         DC CL8'IGGPISCI'
         EJECT
         B     @PB00002                                            0162
IGGPISCI DS    0H                                                  0163
         ENTRY IGGPISCI
         DROP @15
Z003     DS    0H                                                  0165
Z003ISCI DS    0H                                                  0166
         STM 12,14,12(13)
         LA 13,12(13)
         BALR 12,0
IGGSEGST DS 0H
*                                      INSURE ENOUGH CIS SO THAT   0171
*                                      CATLG WONT BE EXTENDED DURING
*                                      PROCESS OF THE CATLGS VOL   0171
*                                      ENTRY                         */
         USING *,12
*   IF RCDCINO=CATVOLCI&            /* CATLGS VOL ENTRY &            */
*       CAXEOV=OFF THEN             /* NOT EXTENDING CATLG           */
         L     @10,CCAURAB(,R11)                                   0172
         L     @10,CCACPE1(,@10)                                   0172
         CLC   RCDCINO(3,@10),@CF02782+1                           0172
         BNE   @RF00172                                            0172
         L     @10,CCAACB(,R11)                                    0172
         L     @10,ACBUAPTR(,@10)                                  0172
         TM    CAXEOV(@10),B'00010000'                             0172
         BNZ   @RF00172                                            0172
*     DO;                                                          0173
*       CCAMLRET=R13;               /* SAVE CALLERS RETURN POINT     */
         ST    R13,CCAMLRET(,R11)                                  0174
*       IF CCAF1LRD=OFF THEN        /* IS CCR READ REQD              */
         TM    CCAF1LRD(R11),B'00100000'                           0175
         BNZ   @RF00175                                            0175
*Z004RCCR:                                                         0176
*         CALL IGGPRCCR;            /* YES,GO READ CCR               */
Z004RCCR BAL   @14,IGGPRCCR                                        0176
*       IF CAXCDCI+(CAXHACI-CAXNFCI)<CIRESCNT+MRECIS THEN/* IF     0177
*                                      INSUFFICIENT FREE CIS         */
@RF00175 L     @10,CCAACB(,R11)                                    0177
         L     @10,ACBUAPTR(,@10)                                  0177
         SLR   @05,@05                                             0177
         ICM   @05,7,CAXHACI(@10)                                  0177
         SLR   @15,@15                                             0177
         ICM   @15,7,CAXNFCI(@10)                                  0177
         SLR   @05,@15                                             0177
         SLR   @15,@15                                             0177
         ICM   @15,7,CAXCDCI(@10)                                  0177
         ALR   @05,@15                                             0177
         C     @05,@CF00058                                        0177
         BNL   @RF00177                                            0177
*         DO;                       /* THEN FORCE CATLG EXTENSION    */
*           CCAASCIX=1;             /* IND ONE CI                    */
         MVI   CCAASCIX(R11),X'01'                                 0179
*           ARASGA=ARASGA&&ARASGA;  /* CLEAR ARA                     */
         L     @10,CCAR0REC(,R11)                                  0180
         XC    ARASGA(255,@10),ARASGA(@10)                         0180
*           ARASGB=ARASGB&&ARASGB;  /* CLEAR ARA                     */
         XC    ARASGB(250,@10),ARASGB(@10)                         0181
*           ARAID='F';              /* SET RCD ID OF FREE            */
         MVI   ARAID(@10),C'F'                                     0182
*           CCAFEOV=ON;             /* IND FORCE EOV                 */
         OI    CCAFEOV(R11),B'00000010'                            0183
*Z005ANCI:                                                         0184
*           CALL IGGPANCI;          /* GO FORCE CATLG EXTENSION      */
Z005ANCI BAL   @14,IGGPANCI                                        0184
*           CCAFEOV=OFF;            /* TURN OFF FORCE EOV SW         */
         NI    CCAFEOV(R11),B'11111101'                            0185
*           CCADDDD=CCAASCI(1)*CILEN;/* DELETE ASSIGNED CI           */
         SLR   @10,@10                                             0186
         ICM   @10,7,CCAASCI(R11)                                  0186
         SLA   @10,9                                               0186
         ST    @10,CCADDDD(,R11)                                   0186
*           XIOOPT=160;             /* GET OPTIONS                   */
         MVI   XIOOPT(R11),X'A0'                                   0187
*Z006XIO:                                                          0188
*           CALL IGGPXIO;           /* GET JUST ASSIGNED CI        0188
*                                      OP=ARA-ADR-UPD-CHK-CIE----    */
Z006XIO  BAL   @14,IGGPXIO                                         0188
*           ARAFCHN=CAXFDCI;        /* INSERT INTO DEL CI CHN        */
         L     @10,CCAR0REC(,R11)                                  0189
         L     @05,CCAACB(,R11)                                    0189
         L     @05,ACBUAPTR(,@05)                                  0189
         MVC   ARAFCHN(3,@10),CAXFDCI(@05)                         0189
*           XIOOPT=32;              /* PUT OPTIONS                   */
         MVI   XIOOPT(R11),X'20'                                   0190
*Z007XIO:                                                          0191
*           CALL IGGPXIO;           /* WRITE BACK CI               0191
*                                      OP=ARA-ADR-UPD-CHK-CIE----    */
Z007XIO  BAL   @14,IGGPXIO                                         0191
*           CAXFDCI=ARACI;          /* NEW FREE CHN PTR TO CAXWA     */
         L     @10,CCAACB(,R11)                                    0192
         L     @10,ACBUAPTR(,@10)                                  0192
         L     @05,CCAR0REC(,R11)                                  0192
         MVC   CAXFDCI(3,@10),ARACI(@05)                           0192
*           CAXCDCI=CAXCDCI+1;      /* INC CNT OF DEL CIS            */
         LA    @05,1                                               0193
         SLR   @15,@15                                             0193
         ICM   @15,7,CAXCDCI(@10)                                  0193
         ALR   @05,@15                                             0193
         STCM  @05,7,CAXCDCI(@10)                                  0193
*Z008CCCR:                                                         0194
*           CALL IGGPCCCR;          /* CHKPT THE CCR                 */
Z008CCCR BAL   @14,IGGPCCCR                                        0194
*           CCASRCIN=CATVOLCI;      /* ID OF CATLGS VOL ENTRY        */
         MVC   CCASRCIN(3,R11),@CF02782+1                          0195
*           DO;                                                    0196
*             CCAF1KEY=OFF;         /* GET BY CI NO.                 */
*             CCAF1KGE=OFF;         /* NOT GET NEXT                  */
*             CCAF1UP=ON;           /* GET FOR UPD                   */
*                                                                  0199
         OI    CCAF1UP(R11),B'00000010'                            0199
         NI    CCAF1KEY(R11),B'11100111'                           0199
*             /*******************************************************/
*             /*                                                     */
*             /* CALL TO MODULE IGG0CLBI                             */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0200
*Z010GET:                                                          0200
*             CALL IGGPGET;         /* GET RECORD                    */
Z010GET  L     @15,@CV03153                                        0200
         BALR  @14,@15                                             0200
*           END;                    /* REGET THE CATLGS VOL ENTRY    */
*         END;                                                     0202
*       ELSE                                                       0203
*         ;                                                        0203
@RF00177 DS    0H                                                  0204
*     END;                                                         0204
*   ELSE                                                           0205
*     ;                                                            0205
@RF00172 DS    0H                                                  0206
*   IF CAXF2REC=ON&                 /* RECOVERABLE CAT       @Y30SSSB*/
*       CCARAEOV=OFF&CCASCICK=OFF THEN/* NOT CRA EOV         @Y30SSSB*/
         L     @10,CCAACB(,R11)                                    0206
         L     @10,ACBUAPTR(,@10)                                  0206
         TM    CAXF2REC(@10),B'00000010'                           0206
         BNO   @RF00206                                            0206
         TM    CCARAEOV(R11),B'01000000'                           0206
         BNZ   @RF00206                                            0206
         TM    CCASCICK(R11),B'00010000'                           0206
         BNZ   @RF00206                                            0206
*     DO;                           /* CHK FREE CRA CIS      @Y30SSSB*/
*                                                                  0207
*       /*************************************************************/
*       /*                                                           */
*       /* CALL TO MODULE IGG0CLBO                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0208
*Z011RASC:                                                         0208
*       CALL IGGPRASC;              /*                       @Y30SSSB*/
Z011RASC L     @15,@CV03159                                        0208
         BALR  @14,@15                                             0208
*       IF CCACD1=RCS&              /* NO ERRORS IN CHK      @Y30SSSB*/
*           CCARAFEV=ON THEN        /* CRA EXTENDED          @Y30SSSB*/
         CLI   CCACD1(R11),0                                       0209
         BNE   @RF00209                                            0209
         TM    CCARAFEV(R11),B'00010000'                           0209
         BNO   @RF00209                                            0209
*         DO;                       /* REGET VOLUME REC      @Y30SSSB*/
*           CCASRCIN=RCDCINO;       /* SET VOL REC CI #      @Y30SSSB*/
         L     @10,CCAURAB(,R11)                                   0211
         L     @10,CCACPE1(,@10)                                   0211
         MVC   CCASRCIN(3,R11),RCDCINO(@10)                        0211
*           DO;                                                    0212
*             CCAF1KEY=OFF;         /* GET BY CI NO.                 */
*             CCAF1KGE=OFF;         /* NOT GET NEXT                  */
*             CCAF1UP=ON;           /* GET FOR UPD                   */
*                                                                  0215
         OI    CCAF1UP(R11),B'00000010'                            0215
         NI    CCAF1KEY(R11),B'11100111'                           0215
*             /*******************************************************/
*             /*                                                     */
*             /* CALL TO MODULE IGG0CLBI                             */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0216
*Z013GET:                                                          0216
*             CALL IGGPGET;         /* GET RECORD                    */
Z013GET  L     @15,@CV03153                                        0216
         BALR  @14,@15                                             0216
*           END;                    /*                       @Y30SSSB*/
*         END;                      /*                       @Y30SSSB*/
*       ELSE                                                       0219
*         ;                         /*                       @Y30SSSB*/
@RF00209 DS    0H                                                  0220
*       CCARAFEV=OFF;               /* CLEAR FLAG            @Y30SSSB*/
         NI    CCARAFEV(R11),B'11101111'                           0220
*     END;                          /*                       @Y30SSSB*/
*   ELSE                                                           0222
*     ;                             /*                       @Y30SSSB*/
@RF00206 DS    0H                                                  0223
*   CALL EXIT;                      /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0223
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */
         DS 0F
*   GEN(DC CL8'IGGPPUPC');          /* NAME OF FOLLOWING PROCEDURE   */
         DC CL8'IGGPPUPC'
*   END;                            /* END OF INNER PROCEDURE        */
@EL00002 DS    0H                                                  0226
@EF00002 DS    0H                                                  0226
@ER00002 BR    @14                                                 0226
@PB00002 DS    0H                                                  0227
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */
         EJECT
