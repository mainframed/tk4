*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**   MODULE NAME =  IGGPSFPL(IGG0CLAV)                                *
**                                                                    *
**   DESCRIPTIVE NAME =                                               *
**      SCAN MODIFY FIELD PARAMETER LISTS.                            *
**                                                                    *
**   FUNCTION =                                                       *
**      THIS SUB-FUNCTION SCANS EACH FPL AND DETERMINES WHETHER THE   *
**      MODIFY OPERATION IS AN ADD GROUP OCCURRENCE, A DELETE GROUP   *
**      OCCURRENCE, OR AN ALTER OF AN EXISTING GROUP OCCURRENCE.  IN  *
**      ADDITION, IT INTERFACES WITH TESTS (IGGPTSTS) TO DETERMINE    *
**      WHETHER ONLY PARTICULAR OCCURRENCES ARE TO BE DELETED OR      *
**      ALTERED.  THE PROCESSING IS AS FOLLOWS:                       *
**      1. PERFORM NECESSARY INITIALIZATION: SET THE GOP LENGTH TO 5  *
**         OR 6 BYTES DEPENDING ON WHETHER THE BASE ENTRY IS A NON-   *
**         VOLUME OR VOLUME RECORD. INITIALIZE THE USABLE RAB'S AS    *
**         AVAILABLE. CALL IGGPSMFG TO SET UP                         *
**         FOR SMF ALTER PROCESSING.                                  *
**      2. SCAN THE FPL'S. FOR EACH FPL DETERMINE:                    *
**         A. IF THERE ARE CHAINED TESTS, CALL IGGPTSTS TO DETERMINE  *
**            THOSE OCCURRENCES TO BE ALTERED OR DELETED.             *
**         B. IF THERE ARE NO CHAINED TESTS AND FIELD DATA IS PROVIDED*
**            IN THE FPL, CALL IGGPXDGO TO ADD A GROUP OCCURRENCE.    *
**         C. CALL IGGPXLT2 (GROUP OCCURRENCE TO BE ALTERED) OR       *
**            IGGPXEL2 (GROUP OCCURRENCE TO BE DELETED) FOR ALL       *
**            OCCURRENCES DETERMINED IN 2(A) OR 2(B).                 *
**      3. WRITE BACK ANY UN-WRITTEN CATALOG RECORDS IN THE USABLE    *
**         RAB'S.                                                     *
**                                                                    *
**   INPUT =  SEE IGGPMOD INPUT ABOVE                                 *
**                                                                    *
**   OUTPUT =  SEE IGGPMOD INPUT ABOVE                                *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0152
*   CCAF1UP=ON;                     /* DO GET-FOR-UPDATE             */
         OI    CCAF1UP(R11),B'00000010'                            0152
*   RFY                                                            0153
*     CCARAB BASED(CCARABB);        /* MAP BASE RAB                  */
*   CCACRAVL=RCDCRAVL;              /* SET CRA VOL           @Y30SSRS*/
         L     @10,CCARABB(,R11)                                   0154
         L     @10,CCACPE1(,@10)                                   0154
         MVC   CCACRAVL(6,R11),RCDCRAVL(@10)                       0154
*   CCACRADT=RCDCRADT;              /* SET CRA DEV           @Y30SSRS*/
         MVC   CCACRADT(4,R11),RCDCRADT(@10)                       0155
*   IF RCDID=VOLRCDID×RCDID=VOLEXTID/* IF THIS IS VOLUME             */
*     THEN                          /* RECORD                        */
         CLI   RCDID(@10),C'V'                                     0156
         BE    @RT00156                                            0156
         CLI   RCDID(@10),C'W'                                     0156
         BNE   @RF00156                                            0156
@RT00156 DS    0H                                                  0157
*     DO;                           /* SET GOP PARAMETERS            */
*       CCAGOPLN=VOLGOPLN;          /* SET GOP LENGTH                */
         MVC   CCAGOPLN(2,R11),@CH00087                            0158
*       CCASL=VOLSNLN;              /* SET SEQUENCE NO. LENGTH       */
         MVC   CCASL(2,R11),@CH00065                               0159
*     END;                          /* END SET                       */
*   ELSE                            /* NOT VOLUME RECORD             */
*     DO;                           /* SET GOP PARAMETERS            */
         B     @RC00156                                            0161
@RF00156 DS    0H                                                  0162
*       CCAGOPLN=CATGOPLN;          /* SET GOP LENGTH                */
         MVC   CCAGOPLN(2,R11),@CH00089                            0162
*       CCASL=CATSNLN;              /* SET SEQUENCE NO. LENGTH       */
         MVC   CCASL(2,R11),@CH00057                               0163
*     END;                          /* END SET                       */
*   CCACBASE=RCDCINO;               /* SAVE BASE RCD CI NO.          */
@RC00156 L     @10,CCARABB(,R11)                                   0165
         L     @10,CCACPE1(,@10)                                   0165
         MVC   CCACBASE(3,R11),RCDCINO(@10)                        0165
*   RFY                                                            0166
*     CCARAB BASED(CCAURAB);        /* ORGINAL BASE                  */
*   CCAURAB=CCARABF;                /* GET FIRST RAB                 */
         L     @10,CCARABF(,R11)                                   0167
         ST    @10,CCAURAB(,R11)                                   0167
*   DO WHILE CCAURAB<=CCARABL;      /* MAKE ALL BUFFERS AVAIL        */
         B     @DE00168                                            0168
@DL00168 DS    0H                                                  0169
*     IF CCAURAB^=CCARABB&          /* IF NOT BASE RAB               */
*         CCARBUR=OFF THEN          /* AND UNUSED RAB        @ZA04507*/
         L     @10,CCAURAB(,R11)                                   0169
         C     @10,CCARABB(,R11)                                   0169
         BE    @RF00169                                            0169
         TM    CCARBUR(@10),B'10000000'                            0169
         BNZ   @RF00169                                            0169
*       IF CCAREUSE=OFF×            /* NO REUSE IND          @ZA04507*/
*           RCDCINO=CCACBASE THEN   /* OR BASE RCD           @ZA04507*/
         TM    CCAREUSE(R11),B'00000100'                           0170
         BZ    @RT00170                                            0170
         L     @10,CCACPE1(,@10)                                   0170
         CLC   RCDCINO(3,@10),CCACBASE(R11)                        0170
         BNE   @RF00170                                            0170
@RT00170 DS    0H                                                  0171
*         CCACPE2=0;                /* NULL RCD AREA         @ZA04507*/
         L     @10,CCAURAB(,R11)                                   0171
         SLR   @05,@05                                             0171
         ST    @05,CCACPE2(,@10)                                   0171
*       ELSE                                                       0172
*         ;                         /* ALLOW REUSE           @ZA04507*/
@RF00170 DS    0H                                                  0173
*     ELSE                                                         0173
*       ;                           /* RAB MAY BE REUSED             */
@RF00169 DS    0H                                                  0174
*     CCARBU1=OFF;                  /* FREE BUFFER                   */
*     CCARBU2=OFF;                  /* FREE BUFFER                   */
         L     @10,CCAURAB(,R11)                                   0175
         NI    CCARBU1(@10),B'10011111'                            0175
*     IF CCAURAB^=CCARABB THEN      /* IF NOT BASE RAB               */
         C     @10,CCARABB(,R11)                                   0176
         BE    @RF00176                                            0176
*       CCARBWR=OFF;                /* ALLOW NO WRITE                */
         NI    CCARBWR(@10),B'11101111'                            0177
*     ELSE                                                         0178
*       ;                           /* ALLOW POSSIBLE WRITE          */
@RF00176 DS    0H                                                  0179
*     CCARBPA=OFF;                  /* INDICATE NO PUT ADD           */
         L     @10,CCAURAB(,R11)                                   0179
         NI    CCARBPA(@10),B'11110111'                            0179
*     CCAURAB=CCAURAB+LENGTH(CCARAB);/* GET NEXT RAB PTR             */
         AL    @10,@CF00145                                        0180
         ST    @10,CCAURAB(,R11)                                   0180
*   END;                            /* END FREEING                   */
@DE00168 CLC   CCAURAB(4,R11),CCARABL(R11)                         0181
         BNH   @DL00168                                            0181
*   CCAURAB=CCARABB;                /* SET BASE RAB                  */
         L     @10,CCARABB(,R11)                                   0182
         ST    @10,CCAURAB(,R11)                                   0182
*   IF CCAMODPA=ON&CCAF2CB=OFF THEN /* IF PUT ADD OF BASE            */
         TM    CCAMODPA(R11),B'00010000'                           0183
         BNO   @RF00183                                            0183
         TM    CCAF2CB(R11),B'00000010'                            0183
         BNZ   @RF00183                                            0183
*     DO;                           /* INDICATE PUT-ADD              */
*       CCAMODPA=OFF;               /* TURN OFF PUT ADD      @Y30SSRS
*                                      INDICATOR             @Y30SSRS*/
         NI    CCAMODPA(R11),B'11101111'                           0185
*       CCARBWR=ON;                 /* INDICATE WRITE                */
*       CCARBPA=ON;                 /* INDICATE PUT-ADD              */
         OI    CCARBWR(@10),B'00011000'                            0187
*     END;                          /* END INDICATE                  */
*   ELSE                                                           0189
*     ;                             /* NOT PUT ADD                   */
*                                                                  0189
@RF00183 DS    0H                                                  0190
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL TO MODULE IGG0CLB3                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0190
*Z012SMFG:                                                         0190
*   CALL IGGPSMFG;                  /* CALL GET SMF FOR ALTER        */
*                                                                  0190
Z012SMFG L     @15,@CV00047                                        0190
         BALR  @14,@15                                             0190
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN FIELD PARAMETER LISTS FOR PROCESSING MODIFY UPDATE       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0191
*   CCAIXFPL=1;                     /* GET FIRST FIELD LIST          */
         MVC   CCAIXFPL(2,R11),@CH00057                            0191
*   DO WHILE(CCAIXFPL<=CTGNOFLD&CCACD1=RCS);/* SCAN FIELDS           */
         B     @DE00192                                            0192
@DL00192 DS    0H                                                  0193
*     CCAFLPT=CTGFIELD(CCAIXFPL);   /* SAVE FIELD POINTER            */
         LH    @10,CCAIXFPL(,R11)                                  0193
         SLA   @10,2                                               0193
         L     @05,CCAXCPL(,R11)                                   0193
         L     @10,CTGFIELD-4(@10,@05)                             0193
         ST    @10,CCAFLPT(,R11)                                   0193
*     IF CCAFLPT^=0 THEN            /* IF FIELD LIST EXIST           */
         LTR   @10,@10                                             0194
         BZ    @RF00194                                            0194
*       DO;                         /* PROCESS EXISTENT LIST         */
*         CCATNREL=1;               /* NEXT RELREPNO EQ TO ONE       */
         MVC   CCATNREL(2,R11),@CH00057                            0196
*         CCATCOMP=OFF;             /* PROCESSING INCOMPLETE         */
         NI    CCATCOMP(R11),B'11111110'                           0197
*         DO WHILE(CCATCOMP=OFF&CCACD1=RCS);/* PROCESS FPL           */
         B     @DE00198                                            0198
@DL00198 DS    0H                                                  0199
*           IF CTGFLCHN^=0 THEN     /* IF CHAINED FIELD LIST, MUST BE
*                                      TEST                          */
         L     @10,CCAFLPT(,R11)                                   0199
         L     @10,CTGFLCHN(,@10)                                  0199
         LTR   @10,@10                                             0199
         BZ    @RF00199                                            0199
*             DO;                   /* PROCESS TESTS                 */
*                                                                  0200
*               /*****************************************************/
*               /*                                                   */
*               /* CALL TO MODULE IGG0CLBA                           */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0201
*Z013TSTS:                                                         0201
*               CALL IGGPTSTS;      /* CALL TESTS                    */
Z013TSTS L     @15,@CV00039                                        0201
         BALR  @14,@15                                             0201
