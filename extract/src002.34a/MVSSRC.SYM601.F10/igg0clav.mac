         OI    CATGODEL(@10),B'01000000'                           0304
         NI    CATGOEXT(@10),B'01111111'                           0304
*       END;                        /* END DELETE OCCURRENCE         */
*     ELSE                          /* EXTENSION GOP                 */
*       DO;                         /* DELETE EXTENSION      @Y30SSRS
*                                      G.O.                  @Y30SSRS*/
         B     @RC00297                                            0306
@RF00297 DS    0H                                                  0307
