         B     @PB00004                                            0147
IGGPSFPL DS    0H                                                  0148
*Z011:                              /* LABEL FOR PROCEDURE TRACKING  */
*Z011SFPL:                          /* LABEL FOR PROCEDURE TRACKING  */
*   RFY                                                            0148
*    (R5,                                                          0148
*     R6,                                                          0148
*     R7,                                                          0148
*     R8,                                                          0148
*     R9,                                                          0148
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */
Z011     DS    0H                                                  0148
Z011SFPL DS    0H                                                  0149
