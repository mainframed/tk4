*     ;                             /* IF DELETED, OR NOT FOUND OR 0296
*                                      ERROR, DONE                   */
*   ELSE                            /* NON-DELETED GOP               */
*     IF CATGOEXT=OFF THEN          /* IF NOT EXTENSION GOP          */
         L     @10,CCACPE5(,R11)                                   0297
         TM    CATGOEXT(@10),B'10000000'                           0297
         BNZ   @RF00297                                            0297
*       DO;                         /* DELETE OCCURRENCE             */
*         CCARABPT=CCAURAB;         /* POINT TO RAB                  */
         L     @05,CCAURAB(,R11)                                   0299
         ST    @05,CCARABPT(,R11)                                  0299
*         CCACPE51=CCACPE5;         /* POINT TO GOP TO BE DELETE     */
*                                                                  0300
         ST    @10,CCACPE51(,R11)                                  0300
*         /***********************************************************/
*         /*                                                         */
*         /* CALL TO MODULE IGG0CLAX                                 */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0301
*Z022DGO:                                                          0301
*         CALL IGGPDGO;             /* DELETE GRP OCCURRENCE         */
Z022DGO  L     @15,@CV00042                                        0301
         BALR  @14,@15                                             0301
*         CATGOCI=0;                /* ZERO CI                       */
         L     @10,CCACPE5(,R11)                                   0302
         SLR   @05,@05                                             0302
         STCM  @05,7,CATGOCI(@10)                                  0302
*         CATGODEL=ON;              /* MARK DELETED                  */
