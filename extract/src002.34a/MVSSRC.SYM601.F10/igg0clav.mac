*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**   MODULE NAME =  IGGPUPD(IGG0CLAV)                                 *
**                                                                    *
**   DESCRIPTIVE NAME =  UPDATE CATALOG FIELD                         *
**                                                                    *
**   FUNCTION =                                                       *
**      THIS SUB-FUNCTION PROCESSES SVC 26 UPDATE REQUESTS TO ALTER   *
**      INFORMATION WITHIN THE VSAM CATALOG.  THE PROCESSING IS AS    *
**      FOLLOWS:                                                      *
**      0. SET CCAXCPL WITH CCACPL.                           @Y30SSRS*
**      1. IF THE REQUEST IS TO EXTEND A VSAM DATA SET, CALL IGGPUPDE *
**         TO PERFORM THE EXTENSION.                                  *
**         IF THE REQUEST IS TO REUSE A VSAM DATA SET, CALL   @Y30SSRS*
**         IGGPRUS TO PERFORM REUSE PROCESSING.               @Y30SSRS*
**      2. INITIALIZE THE CCA TO INTERFACE WITH IGGPSFPL (SEE IGGPMOD *
**         INPUT).                                            @Y30SSRS*
**      3. SET UP FOR NO SMF ALTER PROCESSING.                @Y30SSRS*
**      4. CALL IGGPSFPL TO PERFORM THE UPDATE OPERATION.             *
**                                                                    *
**   INPUT =                                                          *
**      IGGCCA INPUT:                                                 *
**         CCACD1 - MUST BE ZERO                                      *
**         CCACPL - ADDRESS OF SVC 26 CALLER CPL                      *
**      IEZCTGPL AND IEZCTGFL INPUT (SEE IGGPMOD INPUT) ONLY 2(B) OF  *
**      THE MODIFY INTERFACE IS SUPPORTED AND ONLY FOR FIXED LENGTH   *
**      FIELDS.                                                       *
**                                                                    *
**   OUTPUT =                                                         *
**      SEE IGGPMOD OUTPUT (RETURN CODE RCMXGRP DOES NOT      @Y30SSRS*
**             APPLY).                                        @Y30SSRS*
**      CCASCRA IS SET OFF.                                   @Y30SSRS*
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0129
*   CCAXCPL=CCACPL;                 /* INTERNAL CPL PTR      @Y30SSRS*/
         L     @10,CCACPL(,R11)                                    0129
         ST    @10,CCAXCPL(,R11)                                   0129
*   IF CTGEXT=ON THEN               /* IF UPD-EXTEND REQ     @Y30SSRS*/
*                                                                  0130
         TM    CTGEXT(@10),B'10000000'                             0130
         BNO   @RF00130                                            0130
*     /***************************************************************/
*     /*                                                             */
*     /* CALL TO MODULE IGG0CLBB                                     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0131
*Z008UPDE:                                                         0131
*     CALL IGGPUPDE;                /* CALL UPDATE-EXTEND            */
Z008UPDE L     @15,@CV00045                                        0131
         BALR  @14,@15                                             0131
*   ELSE                            /* NOT UPDATE-EXTEND REQUEST     */
*     IF CTGREL=ON THEN             /* IF THIS IS REUSE      @Y30SSRS*/
*                                                                  0132
         B     @RC00130                                            0132
@RF00130 L     @10,CCAXCPL(,R11)                                   0132
         TM    CTGREL(@10),B'01000000'                             0132
         BNO   @RF00132                                            0132
*       /*************************************************************/
*       /*                                                           */
*       /* CALL TO MODULE IGG0CLB7                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0133
*Z009RUS:                                                          0133
*       CALL IGGPRUS;               /* CALL REUSE            @Y30SSRS*/
Z009RUS  L     @15,@CV00048                                        0133
         BALR  @14,@15                                             0133
*     ELSE                          /* PLAIN UPDATE          @Y30SSRS*/
*       DO;                         /* CATALOG UPDATE REQUEST        */
         B     @RC00132                                            0134
@RF00132 DS    0H                                                  0135
*         CCAMCPL=CCACPL;           /* COMMON CPL PTR        @Y30SSRS*/
         L     @10,CCACPL(,R11)                                    0135
         ST    @10,CCAMCPL(,R11)                                   0135
*         CCARABF=ADDR(CCARAB1);    /* POINT TO FIRST RAB            */
         LA    @10,CCARAB1(,R11)                                   0136
         ST    @10,CCARABF(,R11)                                   0136
*         CCARABL=ADDR(CCARAB3);    /* LAST RAB                      */
         LA    @05,CCARAB3(,R11)                                   0137
         ST    @05,CCARABL(,R11)                                   0137
*         CCARABB=ADDR(CCARAB1);    /* BASE RAB                      */
         ST    @10,CCARABB(,R11)                                   0138
*         CCASMFA=OFF;              /* NO SMF ALTER          @Y30SSRS*/
         NI    CCASMFA(R11),B'11011111'                            0139
*Z010SFPL:                                                         0140
*         CALL IGGPSFPL;            /* CALL SCAN FPL                 */
Z010SFPL BAL   @14,IGGPSFPL                                        0140
*       END;                        /* END CATALOG UPDATE            */
*   CALL EXIT;                      /* RETURN TO CALLER              */
@RC00132 DS    0H                                                  0142
@RC00130 BAL   @14,EXIT                                            0142
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */
         DS 0F
*   GEN(DC CL8'IGGPSFPL');          /* NAME OF FOLLOWING PROCEDURE   */
         DC CL8'IGGPSFPL'
*   END;                            /* END OF INNER PROCEDURE        */
@EL00003 DS    0H                                                  0145
@EF00003 DS    0H                                                  0145
@ER00003 BR    @14                                                 0145
@PB00003 DS    0H                                                  0146
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */
         EJECT
