         NI    SDWAREQ(GPR01P),B'01111111'                         0556
*     GPR14P=ERRSYS;                /* ACCESS COMPLETION CODE        */
         SR    GPR14P,GPR14P                                       0557
         ICM   GPR14P,7,ERRSYS                                     0557
*     GPR01P->SDWACMPC=GPR14P;      /* STORE COMPLETION CODE         */
         STCM  GPR14P,7,SDWACMPC(GPR01P)                           0558
*     RESPECIFY                                                    0559
*      (GPR00P,                                                    0559
*       GPR01P,                                                    0559
*       GPR14P,                                                    0559
*       GPR15P)UNRSTD;                                             0559
*   END;                            /* RECORD FOR RTM                */
*   GOTO RETRTM;                    /* GO TO COMMON RET        YM7519*/
*                                                                  0561
         B     RETRTM                                              0561
*/********************************************************************/
*/*  THE FOLLOWING ROUTINE IS GIVEN CONTROL BY MACRO IGGSTART WHEN IT*/
*/*  IS DETERMINED THAT A PREVIOUS RECOVERY ROUTINE FAILED AND NO SVC*/
*/*  DUMP WAS TAKEN. THIS ROUTINE CALLS SDUMP RTN TO TAKE AN SVC     */
*/*  DUMP AND THEN TRANSFERS CONTROL TO THE CLEANRTN.                */
*/********************************************************************/
*                                                                  0562
*DUMPRTN:                                                          0562
*   ;                               /* RTN TO GET SVC DUMP & RET     */
DUMPRTN  DS    0H                                                  0563
*   CALL SDUMP;                     /* GO TO TAKE SVC DUMP           */
*                                                                  0563
         BAL   @14,SDUMP                                           0563
*/********************************************************************/
*/*  THIS ROUTINE IS GIVEN CONTROL BY MACRO IGGSTART OR DUMPRTN WHEN */
*/*  AN ERROR OCCURRED ON OTHER THAN THE ESTAES RB LEVEL OR AN ERROR */
*/*  OTHER THAN A PROGRAM CHECK OCCURRED IN THE SVC.                 */
*/********************************************************************/
*                                                                  0564
*CLEANRTN:                                                         0564
*   ;                               /* CLEAN UP AND RETURN           */
CLEANRTN DS    0H                                                  0565
*   BLKCTR=PERCERR;                 /* INITIALIZE BLKID REG TO 10, 0565
*                                      INDICATING INITIAL BRANCH TO
*                                      DUMPRTN/CLEANRTN,NOT USER ERR */
*                                                                  0565
         LA    BLKCTR,10                                           0565
*/*  BEFORE GOING TO THE CLEANUP ROUTINE TO CLEAN UP RESOURCES       */
*/*  MUST SET UP DCBPTR WHICH WILL BE REQUIRED BY DEBCHECK(SUBROUTINE*/
*/*  CALLED BY IGCT105C).IF RELEX REQUEST, DCBPTR IS ALREADY SET UP  */
*/*  BECAUSE IF WAS PASSED BY USER. IF NOT RELEX MUST GET DCB OUT    */
*/*  OF IOB WHICH WAS PASSED. THE IOB MUST BE VALIDATED FIRST.       */
*/*  THE DCB WILL NOT BE VALIDATED. IF IN ERROR, THE 2ND LEVEL       */
*/*  ESTAE WILL RECOVER.                                             */
*                                                                  0566
*IOBCHCK:                                                          0566
*   ;                               /* VALIDATE THE PASSED IOB       */
IOBCHCK  DS    0H                                                  0567
*   IF RBGRS1<0 THEN                /* IF NOT RELEX REQUEST MUST     */
         L     @12,RBGRS1(,RBSECPTR)                               0567
         LTR   @12,@12                                             0567
         BNM   @RF00567                                            0567
*     DO;                           /* VALIDATE PASSED IOB           */
*       RFY                                                        0569
*        (IOBSTPTR,                                                0569
*         GPR01P,                                                  0569
*         GPR02P,                                                  0569
*         GPR04P)RSTD;                                             0569
*       GPR04P=(ADDR(RBLINK->RBOPSW(2)))-KEYOFFS;/* ADDR OF KEY-28   */
         L     GPR04P,RBLINK(,RBSECPTR)                            0570
         LA    GPR04P,RBOPSW+1(,GPR04P)                            0570
         SH    GPR04P,@CH02467                                     0570
*       IOBSTPTR=ABS(RBGRS1);       /* PUT IOB TO CHK IN BASE REG    */
         LR    IOBSTPTR,@12                                        0571
         LPR   IOBSTPTR,IOBSTPTR                                   0571
*       RFY                                                        0572
*         IOBSTDRD BASED(IOBSTPTR); /* BASE IOB ON COMPLEMENT OF REG
*                                      1 PASSED TO SVC 53            */
*       GPR01P=ADDR(IOBSTDRD);      /* BEGINNING OF IOB PROPER       */
         LR    GPR01P,IOBSTPTR                                     0573
*       GPR02P=ADDR(IOBDPLAD)-1;    /* END ADDR THROUGH IOB SIZE   0574
*                                      ENOUGH TO VALIDATE - INCLUDES
*                                      DCBPTR ADDR.                  */
         LA    GPR02P,IOBDPLAD(,IOBSTPTR)                          0574
         BCTR  GPR02P,0                                            0574
*       WKACLENX=CLEANRC1(7);       /* INITIALIZE CLEANUP IF ERR     */
         MVC   WKACLENX(1,RBSECPTR),CLEANRC1+6                     0575
*       CALL CVT0VL01;              /* VALIDATE 1ST PART OF IOB      */
         L     @12,CVTPTR                                          0576
         L     @15,CVT0VL01(,@12)                                  0576
         BALR  @14,@15                                             0576
*       BC(7,RECORD2);              /* IF ERROR,GO TO RECORD RTN     */
         BC    7,RECORD2                                           0577
*       DCBPTR=IOBDCBPT;            /* PICK UP DCB PTR               */
         L     DCBPTR,IOBDCBPT(,IOBSTPTR)                          0578
*       RFY                                                        0579
*        (IOBSTPTR,                                                0579
*         GPR01P,                                                  0579
*         GPR02P,                                                  0579
*         GPR04P)UNRSTD;                                           0579
*     END;                                                         0580
*   CALL IGCT105C;                  /* GO TO CLEAN UP RESOURCES      */
@RF00567 L     @15,@CV03373                                        0581
         BALR  @14,@15                                             0581
*   RFY                                                            0582
*     GPR15P RSTD;                  /* RESTRICT FOR CLEANUP RC       */
*   IF GPR15P^=0 THEN               /* IF CLEANRTN RC NOT 0, WAS     */
         LTR   GPR15P,GPR15P                                       0583
         BZ    @RF00583                                            0583
*     DO;                           /* ERROR IN CLEAN UP             */
*       WKACLENX=CLEANRC1(8);       /* SET RC FOR CLEANUP ABEND      */
         MVC   WKACLENX(1,RBSECPTR),CLEANRC1+7                     0585
*       GEN(ESTAE 0);               /* GET RID OF ESTAE              */
         ESTAE 0
*     END;                                                         0587
*   RFY                                                            0588
*     GPR15P UNRSTD;                                               0588
@RF00583 DS    0H                                                  0589
*RECORD2:                                                          0589
*   CALL RECORD;                    /* GO SET UP RECORDING AREA      */
RECORD2  BAL   @14,RECORD                                          0589
*   DO;                             /* SETRP RECORD(YES) DUMP(NO)  0590
*                                      WKAREA(SDWAPTR)               */
*     RESPECIFY                                                    0591
*      (GPR00P,                                                    0591
*       GPR01P,                                                    0591
*       GPR14P,                                                    0591
*       GPR15P)RSTD;                                               0591
*     GPR01P=SDWAPTR;                                              0592
         LR    GPR01P,SDWAPTR                                      0592
*     GPR01P->SDWARCDE=0;           /* SAVE RC VALUE                 */
         MVI   SDWARCDE(GPR01P),X'00'                              0593
*     GPR01P->SDWARCRD='1'B;        /* TURN ON RECORD INDICATOR      */
         OI    SDWARCRD(GPR01P),B'10000000'                        0594
*     GPR01P->SDWAREQ='0'B;         /* TURN OFF DUMP INDICATOR       */
         NI    SDWAREQ(GPR01P),B'01111111'                         0595
*     RESPECIFY                                                    0596
*      (GPR00P,                                                    0596
*       GPR01P,                                                    0596
*       GPR14P,                                                    0596
*       GPR15P)UNRSTD;                                             0596
*   END;                            /* REC ON LOGREC                 */
*   GOTO RETRTM;                    /* GO TO COMMON RET        YM7519*/
         B     RETRTM                                              0598
*                                                                  0599
*/********************************************************************/
*/*                         SUBROUTINES                              */
*/********************************************************************/
*/********************************************************************/
*/*  THE FOLLOWING SUBROUTINE VALIDITY CHECKS THE DEB. A BRANCH ENTRY*/
*/*  IS TAKEN WHICH REQUIRES THAT THE LOCAL LOCK BE HELD. INPUT TO   */
*/*  DEBCHK IS AS FOLLOWS:                                           */
*/*                           REGISTER 1 - DEB ADDRESS               */
*/*                           REGISTER 10 - TCB ADDRESS              */
*/*                           REGISTER 14 - RETURN ADDRESS           */
*/*                           REGISTER 15 - DEBCHK ROUTINE ADDRESS   */
*/*  DEBCHK RETURNS TO 4 BYTES PAST REGISTER 14 IF AN ERROR OCCURS.  */
*/*  REGISTER 15 WILL HAVE 4 IF THERE IS AN ERROR. REGISTERS 1-14    */
*/*  UPON ENTRY TO THIS SUBROUTINE ARE SAVED, AND RESTORED UPON EXIT.*/
*/*  INPUT TO THIS ROUTINE ARE: THE DCB ADDRESS IN DCBPTR(R6), SDWA  */
*/*  ADDRESS IN SDWAPTR(R5) AND THE SVRB ADDRESS IN RBSECPTR(R8).    */
*/*  THE LOCK BIT IS ON WHEN ENTRY IS FROM THE USERRTN OR ABEND RTN. */
*/*  WHEN THIS BIT IS ON THIS RTN ASSUMES THE LOCK IS BEING HELD.    */
*/*  THE DEB ADDRESS AND DEBCHK RETURN CODE ARE PASSED TO THE CALLING*/
*/*  ROUTINE IN THE SVRB.                                            */
*/********************************************************************/
*                                                                  0599
*DEBCHECK:                                                         0599
*   PROC OPTIONS(NOSAVE);           /* DEBCHECK SUBROUTINE           */
DEBCHECK DS    0H                                                  0600
*   RFY                                                            0600
*    (GPR01P,                                                      0600
*     GPR14P)RSTD;                                                 0600
*   STM(GPR01P,GPR14P,SAVESDWA);    /* SAVE REGS 1-14 IN SDWA        */
         STM   GPR01P,GPR14P,SAVESDWA(SDWAPTR)                     0601
*   RFY                                                            0602
*    (GPR01P,                                                      0602
*     GPR14P)UNRSTD;                                               0602
*   IF WKALOCK='0'B THEN            /* IF LOCK NOT ON, GET LOCK      */
         TM    WKALOCK(RBSECPTR),B'01000000'                       0603
         BNZ   @RF00603                                            0603
*     DO;                           /* SOME CALLERS DONT HAVE LCK    */
*                                                                  0604
*/*           GET THE LOCAL LOCK                                     */
*                                                                  0605
*DEBLOCK:                                                          0605
*       ;                           /* GET LOCAL LOCK FOR DEBCHK     */
DEBLOCK  DS    0H                                                  0606
*       DO;                         /* SETLOCK (OBTAIN) MODE(UNCOND)
*                                      TYPE(LOCAL) RELATED(DEBDCB,IGC
*                                      T005C,(DEBFREE))              */
*         RFY                                                      0607
*          (GPR11P,                                                0607
*           GPR12P,                                                0607
*           GPR13P,                                                0607
*           GPR14P)RSTD;                                           0607
*         GPR13P=PSALITA;           /* ADDRESS OF LOCK INTERFACE   0608
*                                      TABLE.                        */
         L     GPR13P,PSALITA                                      0608
*         GEN(LM GPR11P,GPR13P,576(GPR13P));/* LOAD LOCKWORD, CLHT 0609
*                                      AND ENTRY ADRS.               */
         LM GPR11P,GPR13P,576(GPR13P)
*         GEN(BALR GPR14P,GPR13P);  /* BALR TO SETLOCK RTN.          */
         BALR GPR14P,GPR13P
*         RFY                                                      0611
*          (GPR11P,                                                0611
*           GPR12P,                                                0611
*           GPR13P,                                                0611
*           GPR14P)UNRSTD;                                         0611
*       END;                        /* SETLOCK (OBTAIN) MODE(UNCOND)
*                                      TYPE(LOCAL) RELATED(DEBDCB,IGC
*                                      T005C,(DEBFREE))              */
*                                   /* GET LOCAL LOCK                */
*     END;                                                         0613
*                                                                  0613
*/*   GO TO DEBCHK ROUTINE - HOWEVER, SINCE THERE IS ONE FIELD IN    */
*/*   THE DEB WHICH DEBCHK ASSUMES IS IN KEY 5 CORE (DEBTBLOF) MUST  */
*/*   FIRST PASS THIS FIELD TO THE SYSTEM VALIDITY CHECK ROUTINE TO  */
*/*   ENSURE ITS CORE LOCATION.                                      */
*                                                                  0614
*   RFY                                                            0614
*    (GPR01P,                                                      0614
*     GPR02P,                                                      0614
*     GPR04P,                                                      0614
*     GPR10P,                                                      0614
*     GPR14P,                                                      0614
*     GPR15P)RSTD;                                                 0614
@RF00603 DS    0H                                                  0615
*   GPR01P=DCBDEBA;                 /* PUT DEB ADDR IN R1            */
         SR    GPR01P,GPR01P                                       0615
         ICM   GPR01P,7,DCBDEBA+40(DCBPTR)                         0615
*   GPR01P=GPR01P-LENGTH(DEBTBLOF); /* GET ADDR OF FIELD       YM5738*/
         BCTR  GPR01P,0                                            0616
         BCTR  GPR01P,0                                            0616
*   GPR02P=GPR01P+1;                /* END OF FIELD            YM5738*/
         LA    GPR02P,1                                            0617
         AR    GPR02P,GPR01P                                       0617
*   GPR04P=ADDR(DEBKEY)-KEYOFFS;    /* ADDR OF DEB KEY=5       YM5738*/
         LA    GPR04P,DEBKEY                                       0618
         SH    GPR04P,@CH02467                                     0618
*   CALL CVT0VL00;                  /* VALIDITY CHECK RTN      YM5738*/
         L     @12,CVTPTR                                          0619
         L     @15,CVT0VL00(,@12)                                  0619
         BALR  @14,@15                                             0619
*   BC(8,GOCHKDEB);                 /* DEBTBLOP IS GOOD        YM5738*/
         BC    8,GOCHKDEB                                          0620
*BADDEB:                                                           0621
*   ;                               /* DEBTBLOP IS BAD         YM5738*/
BADDEB   DS    0H                                                  0622
*   WKASVDEB=GPR01P;                /* PUT INVALID FIELD ADDR WHERE
*                                      DEB ADDR SHOULD GO      YM5738*/
         ST    GPR01P,WKASVDEB(,RBSECPTR)                          0622
*   WKADEBXC=DEBBAD;                /* RC=4 -INVALID DEB       YM5738*/
         MVI   WKADEBXC(RBSECPTR),X'04'                            0623
*   GOTO CHKLOCK;                   /* GO CHECK LOCK           YM5738*/
         B     CHKLOCK                                             0624
*   RFY                                                            0625
*    (GPR02P,                                                      0625
*     GPR04P)UNRSTD;                /* RELEASE REGS 2,4        YM5738*/
*                                                                  0625
*/*      READY TO GO TO DEBCHK ROUTINE                               */
*                                                                  0626
*GOCHKDEB:                                                         0626
*   ;                               /* GO TO DEBCHK ROUT       YM5738*/
GOCHKDEB DS    0H                                                  0627
*   GPR01P=GPR01P+LENGTH(DEBTBLOF); /* GET DEB ADDR IN R1      YM5738*/
         AH    GPR01P,@CH00114                                     0627
*   GPR10P=PSATOLD;                 /* PUT TCB ADDR IN R10           */
         L     GPR10P,PSATOLD                                      0628
*   CALL CVTDEBVR;                  /* BRANCH ENTRY TO DEBCHK        */
         L     @12,CVTPTR                                          0629
         L     @12,CVTEXT2(,@12)                                   0629
         L     @15,CVTDEBVR(,@12)                                  0629
         BALR  @14,@15                                             0629
*   GEN(NOP 0);                     /* USE UP 4 BYTES ON RETURN      */
         NOP 0
*   WKASVDEB=GPR01P;                /* SAVE VALIDATED DEB ADDR IN  0631
*                                      SVRB WORKAREA                 */
         ST    GPR01P,WKASVDEB(,RBSECPTR)                          0631
*   WKADEBXC=RETURNCD;              /* RC PASSED IN R15 BY DEBCHK    */
         STC   RETURNCD,WKADEBXC(,RBSECPTR)                        0632
*   RFY                                                            0633
*     DEBBASIC BASED(WKASVDEB);     /* EST ADDRESSABILITY TO DEB     */
*   IF RETURNCD=0&DEBDCBB^=DCBPTR THEN/* IF DEB VALID THEN ENSURE IS
*                                      POINTS TO DCB                 */
         LTR   RETURNCD,RETURNCD                                   0634
         BNZ   @RF00634                                            0634
         L     @12,WKASVDEB(,RBSECPTR)                             0634
         SR    @04,@04                                             0634
         ICM   @04,7,DEBDCBB(@12)                                  0634
         CR    DCBPTR,@04                                          0634
         BE    @RF00634                                            0634
*     WKADEBXC=DEBBAD;              /* NO,PUT IN RC=4,INVALID DEB    */
         MVI   WKADEBXC(RBSECPTR),X'04'                            0635
*CHKLOCK:                                                          0636
*   ;                               /* SHOULD LOCK BE FREE     YM5738*/
@RF00634 DS    0H                                                  0636
CHKLOCK  DS    0H                                                  0637
*   IF WKALOCK='0'B THEN            /* IF LOCK SW NOT ON, THEN       */
         TM    WKALOCK(RBSECPTR),B'01000000'                       0637
         BNZ   @RF00637                                            0637
*     DO;                           /* MUST FREE LOCK GOTTEN HERE    */
*                                                                  0638
*/*                   RELEASE THE LOCAL LOCK                         */
*                                                                  0639
*DEBFREE:                                                          0639
*       ;                           /* RELEASE LOCAL LOCK -DEBCHK    */
DEBFREE  DS    0H                                                  0640
*       DO;                         /* SETLOCK (RELEASE) TYPE(LOCAL)
*                                      RELATED(DEBDCB,IGCT005C,(DEBLO
*                                      CK))                          */
*         RFY                                                      0641
*          (GPR11P,                                                0641
*           GPR12P,                                                0641
*           GPR13P,                                                0641
*           GPR14P)RSTD;                                           0641
*         GPR13P=PSALITA;           /* ADDRESS OF LOCK INTERFACE   0642
*                                      TABLE.                        */
         L     GPR13P,PSALITA                                      0642
*         GEN(LM GPR11P,GPR13P,588(GPR13P));/* LOAD LOCKWORD, CLHT 0643
*                                      AND ENTRY ADRS.               */
         LM GPR11P,GPR13P,588(GPR13P)
*         GEN(BALR GPR14P,GPR13P);  /* BALR TO SETLOCK RTN.          */
         BALR GPR14P,GPR13P
*         RFY                                                      0645
*          (GPR11P,                                                0645
*           GPR12P,                                                0645
*           GPR13P,                                                0645
*           GPR14P)UNRSTD;                                         0645
*       END;                        /* SETLOCK (RELEASE) TYPE(LOCAL)
*                                      RELATED(DEBDCB,IGCT005C,(DEBLO
*                                      CK))                          */
*                                   /* FREE LOCAL LOCK               */
*     END;                                                         0647
*   RFY                                                            0648
*     GPR14P RSTD;                                                 0648
@RF00637 DS    0H                                                  0649
*   LM(GPR01P,GPR14P,SAVESDWA);     /* RESTORE REGS 1-14             */
         LM    GPR01P,GPR14P,SAVESDWA(SDWAPTR)                     0649
*   WKALOCK='0'B;                   /* ZERO LOCK SW IN ALL CASES     */
         NI    WKALOCK(RBSECPTR),B'10111111'                       0650
*   END DEBCHECK;                                                  0651
@EL00002 DS    0H                                                  0651
@EF00002 DS    0H                                                  0651
@ER00002 BR    @14                                                 0651
*   RFY                                                            0652
*    (GPR01P,                                                      0652
*     GPR10P,                                                      0652
*     GPR14P,                                                      0652
*     GPR15P)UNRSTD;                                               0652
*                                                                  0653
*/********************************************************************/
*/* INPUT TO THIS ROUTINE ARE: THE DCB ADDRESS IN DCBPTR(R6), SDWA   */
*/* ADDRESS IN SDWAPTR(R5) AND THE SVRB ADDRESS IN RBSECPTR(R8).     */
*/* THE LOCK BIT IS ON WHEN ENTRY IS FROM THE USERRTN OR ABEND RTN.  */
*/* WHEN THIS BIT IS ON THIS RTN ASSUMES THE LOCK IS BEING HELD.     */
*/* THE FOLLOWING SUBROUTINE RECEIVES CONTROL FROM THE DUMPRTN, THE  */
*/* SYSERRTN, AND THE 2ND LEVEL ESTAE TO TAKE AN SVC DUMP. A HEADER  */
*/* IS BUILT IN THE SDWA TO IDENTIFY THE DUMP. THE ONLY REGS CHANGED */
*/* BY THIS SUBROUTINE ARE 0,1 AND 15. UPON ENTRY THE LOCAL LOCK     */
*/* IS NOT HELD.  THE SDUMP SVC WILL RETURN A RETURN CODE IN R15,    */
*/* THE VALUES FOR WHICH ARE AS FOLLOWS:                             */
*/*                                00 - SDUMP SUCCESSFUL             */
*/*                                04 - PARTIAL SDUMP TAKEN          */
*/*                                08 - SDUMP UNSUCCESSFUL           */
*/* THE SDUMP RETURN CODE PRESENTED TO THE USER IS THIS VALUE +4.    */
*/*                                04 - SDUMP SUCCESSFUL             */
*/*                                08 - PARTIAL SDUMP TAKEN          */
*/*                                0C - SDUMP UNSUCCESSFUL           */
*/* A BIT IS SET IN THE SVRB WORKAREA TO IDENTIFY THE RETURN CODE.   */
*/********************************************************************/
*                                                                  0653
*SDUMP:                                                            0653
*   PROC OPTIONS(NOSAVE);           /* TAKE AN SVC DUMP              */
SDUMP    DS    0H                                                  0654
*   RFY                                                            0654
*    (GPR00P,                                                      0654
*     GPR01P,                                                      0654
*     GPR14P,                                                      0654
*     GPR15P)RSTD;                                                 0654
*   HEADER=HEADING;                 /* SET UP HEADER                 */
         MVC   HEADER(27,SDWAPTR),HEADING                          0655
*   JOBNAME=TIOCNJOB;               /* JOB NAME                      */
         L     @12,PSATOLD                                         0656
         L     @12,TCBTIO(,@12)                                    0656
         MVC   JOBNAME(8,SDWAPTR),TIOCNJOB(@12)                    0656
*   STEPNAME=TIOCSTEP;              /* STEP NAME                     */
         MVC   STEPNAME(8,SDWAPTR),TIOCSTEP(@12)                   0657
*   SDUMPLST=DUMPLIST;              /* MOVE LIST FORM TO VAR.AREA    */
         MVC   SDUMPLST(24,SDWAPTR),DUMPLIST                       0658
*   GPR01P=ADDR(SDUMPLST);          /* PUT DUMP LIST ADDR IN REG     */
         LA    GPR01P,SDUMPLST(,SDWAPTR)                           0659
*   GEN REFS(HEADER,GPR01P)SETS(SDUMPLST,GPR00P);                  0660
        SDUMP HDRAD=HEADER(SDWAPTR),MF=(E,(1))  TAKE SVC DUMP
*   SDWAEAS='1'B;                   /* SET DUMP TAKEN FLAG           */
         OI    SDWAEAS(SDWAPTR),B'00001000'                        0661
*   IF RETURNCD=SDUMPOK THEN        /* IF SDUMP SUCCESSFUL           */
         LTR   RETURNCD,RETURNCD                                   0662
         BNZ   @RF00662                                            0662
*     WKADMP04='1'B;                /* SAVE RETURN CODE = 4          */
         OI    WKADMP04(RBSECPTR),B'00001000'                      0663
*   IF RETURNCD=SDUMPART THEN       /* IF PARTIAL SDUMP TAKEN        */
@RF00662 CH    RETURNCD,@CH00095                                   0664
         BNE   @RF00664                                            0664
*     WKADMP08='1'B;                /* SAVE RETURN CODE = 8          */
         OI    WKADMP08(RBSECPTR),B'00010000'                      0665
*   IF RETURNCD=SDUMPBAD THEN       /* IF SDUMP UNSUCCESSFUL         */
@RF00664 CH    RETURNCD,@CH00048                                   0666
         BNE   @RF00666                                            0666
*     WKADMP0C='1'B;                /* SAVE RETURN CODE = C          */
         OI    WKADMP0C(RBSECPTR),B'00100000'                      0667
*   END SDUMP;                      /* RETURN TO MAIN ROUTINE        */
@EL00003 DS    0H                                                  0668
@EF00003 DS    0H                                                  0668
@ER00003 BR    @14                                                 0668
*   RFY                                                            0669
*    (GPR00P,                                                      0669
*     GPR01P,                                                      0669
*     GPR14P,                                                      0669
*     GPR15P)UNRSTD;                                               0669
*                                                                  0670
*/********************************************************************/
*/*  THE FOLLOWING SUBROUTINE IS CALLED BY THE SYSERRTN AND THE      */
*/*  CLEANRTN  TO SET UP THE SDWA WHEN A SYSTEM ERROR HAS BEEN       */
*/*  DETECTED. THE FOLLOWING RECORDING INFORMATION WILL BE PUT IN    */
*/*  THE VARIABLE PORTION OF THE SDWA (FROM WHERE RTM WILL GET IT TO */
*/*  WRT ON SYS1.LOGREC):  SVC AUDIT TRAIL BYTE                      */
*/*                        RETURN CODE FROM CLEANRTN                 */
*/*                        RETURN CODE FROM SDUMP                    */
*/*                        REGISTERS 0 AND 1 AT TIME SVC ISSUED      */
*/*                        RB OLD PSW CONTAINING THE ADDRESS+2 FROM  */
*/*                           WHERE THE SVC WAS ISSUED               */
*/*                        OLD ABEND CODE IF PERCOLATED ERROR        */
*/*  THE ONLY REGISTER CHANGED BY THIS SUBROUTINE IS REG 1.          */
*/*  THE LOCAL LOCK IS NOT HELD UPON ENTRY.                          */
*/********************************************************************/
*                                                                  0670
*RECORD:                                                           0670
*   PROC OPTIONS(NOSAVE);           /* PUT SYSTEM INFO IN SDWA       */
RECORD   DS    0H                                                  0671
*   RFY                                                            0671
*     GPR14P RSTD;                  /* RESTRICT RETURN ADDR          */
*   RECAUDIT=WKATRALX;              /* MV AUDIT TRAIL BIT IN SDWA    */
         MVC   RECAUDIT(1,SDWAPTR),WKATRALX(RBSECPTR)              0672
*   SDWAURAL=LENGTH(RECORDWK);      /* INIT LEN OF RECORDING INFO    */
         MVI   SDWAURAL(SDWAPTR),X'17'                             0673
