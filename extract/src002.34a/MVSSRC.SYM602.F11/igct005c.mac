         TITLE 'IGCT005C - EXCLUSIVE CONTROL TASK RECOVERY ROUTINE     *
                       '
IGCT005C CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(24)                                             0001
         DC    C'IGCT005C YM7519 02/11/74'                         0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @09,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@09                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
RBPRFXLN EQU 64
RBPRFLNA EQU 32
PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 200
SPDFID   EQU   X'FE'
         ENTRY BACRETRY
*   SDWAPTR=GPR01P;                 /* REG 1 ON ENTRY HAS SDWA       */
         LR    SDWAPTR,GPR01P                                      0158
*   RFY                                                            0159
*     GPR01P UNRSTD;                                               0159
*   RBSECPTR=SDWAPARM;              /* SDWA HAS SVRB ADDR            */
         L     RBSECPTR,SDWAPARM(,SDWAPTR)                         0160
*   WKALOCK='0'B;                   /* ENSURE DEBCHECK LOCK SW 0     */
         NI    WKALOCK(RBSECPTR),B'10111111'                       0161
*   IF RBGRS1^<0 THEN               /* IF RELEX,DCB ADDR PASSED      */
         L     @12,RBGRS1(,RBSECPTR)                               0162
         LTR   @12,@12                                             0162
         BM    @RF00162                                            0162
*     DCBPTR=RBGRS1;                /* GET DCB FROM SVRB SAVE        */
*                                                                  0163
         LR    DCBPTR,@12                                          0163
*/********************************************************************/
*/* THE FOLLOWING ROUTINE ANALYZES THE CAUSE AND STAGE OF THE ABEND  */
*/* BY DETERMINING IF THE ABEND WAS PERCOLATED, A DUMP WAS TAKEN,    */
*/* IF IT WAS A PROGRAM CHECK, IF THERE WAS PREVIOUS RECOVERY, IF    */
*/* THE CURRENT RB FAILED OR A PREVIOUS FRR FAILED. BASED ON THIS    */
*/* ANALYSIS THE CONTROL IS TRANSFERRED TO A ROUTINE TO: 1) CHECK    */
*/* FOR USER INPUT ERRORS,  2) CLEAN UP RESOURCES AND CONTINUE TO    */
*/* TERMINATE, OR  3) TAKE AN SVC DUMP BEFORE CONTINUING TO          */
*/* TERMINATE.                                                       */
*/*          INPUT TO THIS ROUTINE IS AS FOLLOWS:                    */
*/*                    REG  5 - SDWA ADDRESS                         */
*/*                    REG  6 - DCB ADDRESS (IF RELEX)               */
*/*                    REG  8 - SVRB ADDRESS                         */
*/*                    REG 13 - RTM SAVEAREA ADDRESS                 */
*/********************************************************************/
*                                                                  0164
*ANALYZE:                                                          0164
*   ;                               /* ANALYZE THE ERROR             */
@RF00162 DS    0H                                                  0164
ANALYZE  DS    0H                                                  0165
*   DO;                             /* ?IGGSTART USER(USERRTN)     0165
*                                      SDUMP(DUMPRTN) CLEAN(CLEANRTN)*/
*                                                                  0165
*/*  MACCOMP Y-2 SC1D0/SJ0204                                        */
*/*  MACSTAT Y-2 74050/YM7586                                        */
*/********************************************************************
*                                                                    */
*/* THIS ROUTINE ANALYSES FLAGS IN THE SDWA TO DETERMINE             */
*/* WHAT TYPE OF FUNCTION TO PERFORM                                 */
*/* THE FLAG COMBINATIONS TESTED AND THE ACTIONS TAKEN ARE SUMMARIZED*/
*/* IN THE FOLLOWING TABLE:                                          */
*/*                                                                  */
*/*   PREVIOUS  ×× CAUSE OF ERROR ENTRY                              */
*/*  PROCESSING ×× MCHK/PGIO/TEXC  OTHER                             */
*/*  ___________××________________________                           */
*/*       *EAS  ××        C          C                               */
*/*       *CLUP ××        C          C                               */
*/*       *STAF ××        S          S                               */
*/*       *PERC ××        C          U                               */
*/*       *NRBE ××        C          U                               */
*/*        NONE ××        C          U                               */
*/*                                                                  */
*/*       * THESE BITS ARE NOT ALL MUTUALLY EXCLUSIVE. THEY ARE      */
*/*         TESTED IN THE ORDER LISTED.                              */
*/*                                                                  */
*/*       FUNCTIONS TO BE PERFORMED:                                 */
*/*                                                                  */
*/* C (CLEANRTN)--CLEAN UP RESOURCES AND PERCOLATE ERROR             */
*/*                                                                  */
*/* S (DUMPRTN)--PREVIOUS RECOVERY ROUTINE FAILED.                   */
*/* TAKE A DUMP FOR IT, THEN TREAT AS C                              */
*/*                                                                  */
*/* U (USERRTN)--VALIDATE USER INPUT. IF IT IS IN ERROR,             */
*/* TAKE PROBLEM DETERMINATION ACTIONS AND THEN CLEAN UP.            */
*/* IF IT IS VALID, ASSUME A SYSTEM ERROR IN COVERED FUNCTION.       */
*/* TAKE A DUMP, THEN TREAT AS C                                     */
*/********************************************************************
*                                                                    */
*                                                                  0166
*     IF SDWAEAS='1'B               /* LOWER ESTAE ALREADY DUMPED    */
*         ×SDWACLUP='1'B THEN       /* OR RTM SAYS CLEANUP           */
         TM    SDWAEAS(SDWAPTR),B'00001000'                        0166
         BO    @RT00166                                            0166
         TM    SDWACLUP(SDWAPTR),B'10000000'                       0166
         BO    @RT00166                                            0166
*       GOTO CLEANRTN;              /* GO CLEAN UP & GET OUT         */
*     IF SDWASTAF='1'B THEN         /* A LOWER LEVEL ESTAE/FRR       */
         TM    SDWASTAF(SDWAPTR),B'10000000'                       0168
         BO    @RT00168                                            0168
*       GOTO DUMPRTN;               /* WAS EXECUTING AND FAILED      */
*                                   /* TAKE A DUMP FOR IT            */
*     IF SDWATEXC='1'B              /* SOME NON-PROGRAM TYPE         */
*         ×SDWAPGIO='1'B            /* ERROR HAS OCCURRED            */
*         ×SDWAMCHK='1'B THEN       /* GO CLEAN UP & GET OUT         */
         TM    SDWATEXC(SDWAPTR),B'10000011'                       0170
         BNZ   @RT00170                                            0170
*       GOTO CLEANRTN;                                             0171
*     GOTO USERRTN;                 /* CHECK USER INPUT              */
*                                                                  0172
         B     USERRTN                                             0172
*/********************************************************************
*                                                                    */
*                                                                  0173
*   END;                            /* ANALYZE ABEND CONDITION       */
*                                                                  0173
*/********************************************************************/
*/* THE FOLLOWING ROUTINE VALIDITY CHECKS ALL USER INPUT TO SVC 53   */
*/* TO DETERMINE IF THIS CAUSED THE ABEND IN EXCLUSIVE CONTROL.THESE */
*/* CONTROL BLOCKS/DATA AREAS WILL BE VALIDATED WHERE APPLICABLE:    */
*/* IOB, DCB, BLKREF FIELD, DECB, DEB, AND NEXT WAITING IOB.         */
*/*          INPUT TO THIS ROUTINE IS THE SVRB ADDRESS WHICH WAS     */
*/* PASSED IN THE SDWA FROM IGC0005C. THE SVRB WILL CONTAIN THE      */
*/* INPUT PARAMETERS TO RELEX:  REG 1 - DCB ADDRESS                  */
*/*                             REG 0 - BLKREF ADDRESS               */
*/* ENTRY TO SVC 53 FOR OTHER THAN A RELEX REQUEST WILL HAVE AS INPUT*/
*/*                             REG 1 - IOB ADDRESS,(COMPLEMENTED)   */
*/* EACH CONTROL BLOCK IS VALIDATED BY THE SYSTEM VALIDITY CHECK     */
*/* ROUTINE.  IF ONE IS FOUND TO BE IN ERROR, CERTAIN CONTROL BLOCKS */
*/* ALREADY VALIDATED ARE TRACED, A WTP IS ISSUED AND SETRP TO GIVE  */
*/* CONTROL TO THE RETRY ROUTINE.  IF ALL BLOCKS ARE VALID, CONTROL  */
*/* IS GIVEN TO THE SYSERRTN ROUTINE. THE REGISTER LABELED BLKCTR    */
*/* WILL BE USED TO IDENTIFY THE PARTICULAR CONTROL BLOCK BEING      */
*/* VALIDATED.  IT WILL BE INCREMENTED AFTER SUCCESSFUL VALIDATION   */
*/* OF EACH BLOCK.  THIS REGISTER IS USED THROUGHOUT THE ESTAE       */
*/* HEREAFTER AS AN INDEX TO IDENTIFY/SET THE RETURN CODE ASSOCIATED */
*/* WITH THE BLOCK FOUND TO BE IN ERROR.                             */
*/*      BLKCTR(R7) VALUES SHOULD BE INTERPRETED AS FOLLOWS:         */
*/*            1 = IOB ERROR (NOT RELEX)                             */
*/*            2 = DCB ERROR                                         */
*/*            3 = BLKREF ERROR (RELEX)                              */
*/*            4 = DECB ERROR (NOT RELEX)                            */
*/*            5 = DEB ERROR                                         */
*/*            6 = NEXT IOB ERROR                                    */
*/*            7 = SYSTEM ERROR                                      */
*/*           10 = ERROR IN PREVIOUS RECOVERY RTN OR ANOTHER RB LEVEL*/
*/*                (BRANCHED TO DUMPRTN OR CLEANRTN)                 */
*/********************************************************************/
*                                                                  0174
*USERRTN:                                                          0174
*   ;                               /* VALIDATE THE CONTROL BLKS     */
*                                                                  0174
USERRTN  DS    0H                                                  0175
*/*   GET THE LOCAL LOCK BEFORE VALIDATING THE CONTROL BLOCKS        */
*                                                                  0175
*   RFY                                                            0175
*     GPR00P RSTD;                  /* RESTRICT SAVEAREA SAVE REG    */
*   GPR00P=SAVEPTR;                 /* SAVE RTM SAVEAREA AROUND      */
         LR    GPR00P,SAVEPTR                                      0176
*SETLOCK:                                                          0177
*   ;                               /* GET LOCAL LOCK                */
SETLOCK  DS    0H                                                  0178
*   DO;                             /* SETLOCK (OBTAIN) MODE(UNCOND)
*                                      TYPE(LOCAL) RELATED(CNTRLBLK,I
*                                      GCT005C,(FREELOCK,FREELCKX,FRE
*                                      ELCK1,FREELCK2,FREELCK3))     */
*     RFY                                                          0179
*      (GPR11P,                                                    0179
*       GPR12P,                                                    0179
*       GPR13P,                                                    0179
*       GPR14P)RSTD;                                               0179
*     GPR13P=PSALITA;               /* ADDRESS OF LOCK INTERFACE   0180
*                                      TABLE.                        */
         L     GPR13P,PSALITA                                      0180
*     GEN(LM GPR11P,GPR13P,576(GPR13P));/* LOAD LOCKWORD, CLHT AND 0181
*                                      ENTRY ADRS.                   */
         LM GPR11P,GPR13P,576(GPR13P)
*     GEN(BALR GPR14P,GPR13P);      /* BALR TO SETLOCK RTN.          */
         BALR GPR14P,GPR13P
*     RFY                                                          0183
*      (GPR11P,                                                    0183
*       GPR12P,                                                    0183
*       GPR13P,                                                    0183
*       GPR14P)UNRSTD;                                             0183
*   END;                            /* SETLOCK (OBTAIN) MODE(UNCOND)
*                                      TYPE(LOCAL) RELATED(CNTRLBLK,I
*                                      GCT005C,(FREELOCK,FREELCKX,FRE
*                                      ELCK1,FREELCK2,FREELCK3))     */
*   SAVEPTR=GPR00P;                 /* RESTORE RTM SAVEAREA PTR      */
         LR    SAVEPTR,GPR00P                                      0185
*   RFY                                                            0186
*     GPR00P UNRSTD;                                               0186
*                                                                  0186
*/********************************************************************/
*/*       SET UP INPUT REGISTERS TO SYSTEM VALIDITY ROUTINE          */
*/*                REGISTER 1 - START OF AREA TO VERIFY              */
*/*                REGISTER 2 - END OF AREA TO VERIFY                */
*/*                REGISTER 4 - ADDRESS OF KEY - 28                  */
*/*                REGISTER 14- RETURN ADDRESS                       */
*/*                REGISTER 15- ADDRESS OF VALIDITY ROUTINE          */
*/* BLOCKS ARE VERIFIED FOR CORE OWNERSHIP AND ALIGNMENT, (WITH THE  */
*/* EXCEPTION OF THE DCB AND BLKREF FIELD, WHICH HAVE NO BOUNDARY    */
*/* REQUIREMENTS).                                                   */
*/********************************************************************/
*                                                                  0187
*   RFY                                                            0187
*    (GPR01P,                                                      0187
*     GPR02P,                                                      0187
*     GPR04P,                                                      0187
*     GPR14P,                                                      0187
*     GPR15P)RSTD;                  /* RESTRICT REGS - VALID CHK     */
*   GPR04P=(ADDR(RBLINK->RBOPSW(2)))-KEYOFFS;/* ADDR OF KEY-28       */
         L     GPR04P,RBLINK(,RBSECPTR)                            0188
         LA    GPR04P,RBOPSW+1(,GPR04P)                            0188
         SH    GPR04P,@CH02467                                     0188
*   BLKCTR=1;                       /* SET CTR TO FIRST BLOCK        */
         LA    BLKCTR,1                                            0189
*IOBCHK1:                                                          0190
*   ;                               /* VALIDATE THE PASSED IOB       */
IOBCHK1  DS    0H                                                  0191
*   RFY                                                            0191
*     WRKREG RSTD;                  /* RESTRICT WORKREG        YM7519*/
*   IF RBGRS1<0 THEN                /* IF NOT RELEX REQUEST MUST     */
         L     @12,RBGRS1(,RBSECPTR)                               0192
         LTR   @12,@12                                             0192
         BNM   @RF00192                                            0192
*     DO;                           /* VALIDATE PASSED IOB           */
*       RFY                                                        0194
*         IOBSTPTR RSTD;                                           0194
*       IOBSTPTR=ABS(RBGRS1);       /* PUT IOB TO CHK IN BASE REG    */
         LR    IOBSTPTR,@12                                        0195
         LPR   IOBSTPTR,IOBSTPTR                                   0195
*       RFY                                                        0196
*         IOBSTDRD BASED(IOBSTPTR); /* BASE IOB ON COMPLEMENT OF REG
*                                      1 PASSED TO SVC 53            */
*       GPR01P=ADDR(IOBDQADA);      /* VALIDATE PREFIX OF IOB 1ST    */
         LR    GPR01P,IOBSTPTR                                     0197
         AL    GPR01P,@CF03514                                     0197
*       GPR02P=GPR01P+VSPREFLG-1;   /* 8 BYTE PREFIX TO IOB IF     0198
*                                      VARIABLE SPANNED              */
         LR    GPR02P,GPR01P                                       0198
         SH    GPR02P,@CH03494                                     0198
*       CALL CVT0VL01;              /* VALIDATE IOB PREFIX+ALIGN     */
         L     @12,CVTPTR                                          0199
         L     @15,CVT0VL01(,@12)                                  0199
         BALR  @14,@15                                             0199
*       WRKREG=GPR15P;              /* SAVE RETURN CODE FROM CHCK    */
         LR    WRKREG,GPR15P                                       0200
*       GPR01P=ADDR(IOBSTDRD);      /* BEGINNING OF IOB PROPER       */
         LR    GPR01P,IOBSTPTR                                     0201
*       GPR02P=ADDR(IOBDPLAD)-1;    /* END ADDR THROUGH IOB SIZE     */
         LA    GPR02P,IOBDPLAD(,IOBSTPTR)                          0202
         BCTR  GPR02P,0                                            0202
*       CALL CVT0VL01;              /* VALIDATE 1ST PART OF IOB      */
         L     @12,CVTPTR                                          0203
         L     @15,CVT0VL01(,@12)                                  0203
         BALR  @14,@15                                             0203
*       BC(7,ABEND);                /* IF ERROR,GO TO ABEND RTN      */
         BC    7,ABEND                                             0204
*                                   /* VALIDATE 2ND HALF OF IOB      */
*       GPR02P=ADDR(IOBSTDRD)+IOBDIOBS-1;/* END ADDR OF IOB          */
         LR    GPR02P,IOBSTPTR                                     0205
         AH    GPR02P,IOBDIOBS(,IOBSTPTR)                          0205
         BCTR  GPR02P,0                                            0205
*       CALL CVT0VL01;              /* VALIDATE 2ND HALF OF IOB      */
         L     @12,CVTPTR                                          0206
         L     @15,CVT0VL01(,@12)                                  0206
         BALR  @14,@15                                             0206
*       BC(7,ABEND);                /* IF ERROR, GO TO ABEND RTN     */
         BC    7,ABEND                                             0207
*       DCBPTR=IOBDCBPT;            /* PICK UP DCB PTR               */
         L     DCBPTR,IOBDCBPT(,IOBSTPTR)                          0208
*     END;                                                         0209
*   ELSE                            /* IF RELEX ZERO REG       YM7519*/
*     WRKREG=0;                     /* TO GET BY NEXT TEST     YM7519*/
         B     @RC00192                                            0210
@RF00192 SR    WRKREG,WRKREG                                       0210
*   BLKCTR=BLKCTR+1;                /* UP BLKCTR TO DCB(=2)          */
@RC00192 AH    BLKCTR,@CH00102                                     0211
*DCBCHK:                                                           0212
*   ;                               /* VALIDITY CHECK THE DCB        */
DCBCHK   DS    0H                                                  0213
*   GPR01P=DCBPTR+NULLPREF;         /* DCB ADDR+NULL PREFIX          */
         LA    GPR01P,16                                           0213
         AR    GPR01P,DCBPTR                                       0213
*   GPR02P=GPR01P+DCBLEN-NULLPREF-1;/* ENDING ADDR OF DCB            */
         LR    GPR02P,GPR01P                                       0214
         SH    GPR02P,@CH03496                                     0214
*   CALL CVT0VL00;                  /* CALL SYS VAL CHCK,NO ALIGN    */
         L     @12,CVTPTR                                          0215
         L     @15,CVT0VL00(,@12)                                  0215
         BALR  @14,@15                                             0215
*   BC(7,ABEND);                    /* IF ERROR, GO TO ABEND RTN     */
         BC    7,ABEND                                             0216
*   IF DCBBFTKR='1'B&WRKREG^=0 THEN /* IF VS RECORDS AND PREFIX      */
         LA    @12,DCBIOBAD+16(,DCBPTR)                            0217
         TM    DCBBFTKR(@12),B'00100000'                           0217
         BNO   @RF00217                                            0217
         LTR   WRKREG,WRKREG                                       0217
         BZ    @RF00217                                            0217
*     DO;                           /* RETURN COD FROM VALIDATION    */
*       BLKCTR=IOBERR;              /* NOT 0, THEN IOB ERROR         */
         LA    BLKCTR,1                                            0219
*       GOTO ABEND;                 /* GO TO ABEND ROUTINE           */
         B     ABEND                                               0220
*       RFY                                                        0221
*         WRKREG UNRSTD;                                           0221
*     END;                                                         0222
*   ELSE                            /* CHECK TO SEE IF DATA SET      */
*     IF DCBOFOPN='0'B THEN         /* IS OPEN, IF NOT STILL         */
@RF00217 TM    DCBOFOPN+40(DCBPTR),B'00010000'                     0223
         BZ    @RT00223                                            0223
*       GOTO ABEND;                 /* IS A DCB ERROR, GO ABEND      */
*   BLKCTR=BLKCTR+1;                /* UP BLKID TO BLKREF(=3)        */
         AH    BLKCTR,@CH00102                                     0225
*BLKRFCHK:                                                         0226
*   ;                               /* VALIDITY CHECK BLKREF         */
BLKRFCHK DS    0H                                                  0227
*   IF RBGRS1^<0 THEN               /* IF NOT NEG,THERE IS BLKREF    */
         ICM   @12,15,RBGRS1(RBSECPTR)                             0227
         BM    @RF00227                                            0227
*     DO;                           /* WHICH IS ONLY FOR RELEX       */
*       GPR01P=RBGRS0;              /* IF RELEX,BLKREF IN R0         */
         L     GPR01P,RBGRS0(,RBSECPTR)                            0229
*       IF DCBOPTA='1'B×DCBOPTF='0'B THEN/* IF ACTUAL ADDR OR NO     */
         TM    DCBOPTA+52(DCBPTR),B'00001000'                      0230
         BNZ   @RT00230                                            0230
         TM    DCBOPTF+52(DCBPTR),B'00010000'                      0230
         BO    @RF00230                                            0230
@RT00230 DS    0H                                                  0231
*         GPR02P=GPR01P+MBBCCHHR-1; /* FEEDBACK, LENGTH IS 8         */
         LR    GPR02P,GPR01P                                       0231
         SH    GPR02P,@CH03494                                     0231
*       ELSE                        /* ELSE, TTR/BLKID IS 3 BYTES    */
*         GPR02P=GPR01P+TTRID-1;    /* PUT ENDING ADDR IN REG 2      */
         B     @RC00230                                            0232
@RF00230 LR    GPR02P,GPR01P                                       0232
         SH    GPR02P,@CH03497                                     0232
*       CALL CVT0VL00;              /* VALIDITY CHK BLKREF FIELD     */
@RC00230 L     @12,CVTPTR                                          0233
         L     @15,CVT0VL00(,@12)                                  0233
         BALR  @14,@15                                             0233
*       BC(7,ABEND);                /* IF ERROR GO TO ABEND RTN      */
         BC    7,ABEND                                             0234
*     END;                                                         0235
*   BLKCTR=BLKCTR+1;                /* UP BLKCTR TO DECB ID(=4)      */
@RF00227 AH    BLKCTR,@CH00102                                     0236
*DECBCHK:                                                          0237
*   ;                               /* VALIDITY CHK THE DECB         */
DECBCHK  DS    0H                                                  0238
*   IF RBGRS1<0 THEN                /* IF NOT RELEX REQUEST,         */
         ICM   @12,15,RBGRS1(RBSECPTR)                             0238
         BNM   @RF00238                                            0238
*     DO;                           /* IOB AND DECB PASSED TO SVC    */
*       GPR01P=IOBECBPT;            /* DECB ADDRESS IS IN IOB        */
         L     GPR01P,IOBECBPT(,IOBSTPTR)                          0240
*       RFY                                                        0241
*         IOBSTPTR UNRSTD;          /* RELEASE THE IOB BASE          */
*       GPR02P=GPR01P+DECBLEN-1;    /* ENDING ADDR OF DECB           */
         LR    GPR02P,GPR01P                                       0242
         SH    GPR02P,@CH03498                                     0242
*       CALL CVT0VL01;              /* GO TO VALIDATE DECB           */
         L     @12,CVTPTR                                          0243
         L     @15,CVT0VL01(,@12)                                  0243
         BALR  @14,@15                                             0243
*       BC(7,ABEND);                /* IF ERROR, GO TO ABEND RTN     */
         BC    7,ABEND                                             0244
*     END;                                                         0245
*   BLKCTR=BLKCTR+1;                /* UP BLKCTR TO DEB ID(=5)       */
@RF00238 AH    BLKCTR,@CH00102                                     0246
*DEBCHK:                                                           0247
*   ;                               /* VALIDITY CHECK THE DEB        */
DEBCHK   DS    0H                                                  0248
*   WKALOCK='1'B;                   /* SW-DEBCHECK WONT FREE LOCK    */
         OI    WKALOCK(RBSECPTR),B'01000000'                       0248
*   CALL DEBCHECK;                  /* GO TO DEBCHECK ROUTINE        */
         BAL   @14,DEBCHECK                                        0249
*   IF WKADEBXC^=0 THEN             /* CHECK DEBCHK RET COD FOR 0    */
         CLI   WKADEBXC(RBSECPTR),0                                0250
         BNE   @RT00250                                            0250
*     GOTO ABEND;                   /* DEB ERROR, THEN ABEND         */
*   ELSE                            /* ELSE UP BLKCTR TO NEXT IOB    */
*     BLKCTR=BLKCTR+1;              /* POSITION TO NEXT BLK          */
         AH    BLKCTR,@CH00102                                     0252
*IOBCHK2:                                                          0253
*   ;                               /* VALIDATE THE NEXT IOB         */
IOBCHK2  DS    0H                                                  0254
*   IF WKAIOBQX^=0 THEN             /* IF THERE IS NEXT IOB SAVED    */
         L     @12,WKAIOBQX(,RBSECPTR)                             0254
         LTR   @12,@12                                             0254
         BZ    @RF00254                                            0254
*     DO;                           /* BY SVC, MUST VALIDATE IT      */
*       RFY                                                        0256
*         IOBSTDRD BASED(WKAIOBQX); /* BASE IOB ON ADDR SAVED        */
*       IF DCBBFTKR='1'B THEN       /* IF VARIABLE SPANNED RECORD    */
         LA    @10,16                                              0257
         LR    @03,DCBPTR                                          0257
         AR    @03,@10                                             0257
         LA    @03,DCBIOBAD(,@03)                                  0257
         TM    DCBBFTKR(@03),B'00100000'                           0257
         BNO   @RF00257                                            0257
*         GPR01P=ADDR(IOBDQADA);    /* BEG IOB ADDR IS PREFIX        */
         SR    @12,@10                                             0258
         LA    GPR01P,IOBDQADA(,@12)                               0258
*       ELSE                        /* OTHERWISE IOB BEGINS AT       */
*         GPR01P=ADDR(IOBSTDRD);    /* STANDARD SECTION              */
         B     @RC00257                                            0259
@RF00257 L     GPR01P,WKAIOBQX(,RBSECPTR)                          0259
*       GPR02P=ADDR(IOBDPLAD)-1;    /* END ADDR THROUGH IOB SIZE     */
@RC00257 L     GPR02P,WKAIOBQX(,RBSECPTR)                          0260
         LA    GPR02P,IOBDPLAD(,GPR02P)                            0260
         BCTR  GPR02P,0                                            0260
*       CALL CVT0VL01;              /* VALIDATE 1ST PART OF IOB      */
         L     @12,CVTPTR                                          0261
         L     @15,CVT0VL01(,@12)                                  0261
         BALR  @14,@15                                             0261
*       BC(7,ABEND);                /* IF ERROR,GO TO ABEND RTN      */
         BC    7,ABEND                                             0262
*                                   /* VALIDATE 2ND HALF OF IOB      */
*       GPR02P=ADDR(IOBSTDRD)+IOBDIOBS-1;/* END ADDR OF IOB          */
         L     @12,WKAIOBQX(,RBSECPTR)                             0263
         LH    GPR02P,IOBDIOBS(,@12)                               0263
         AR    GPR02P,@12                                          0263
         BCTR  GPR02P,0                                            0263
*       CALL CVT0VL01;              /* VALIDATE 2ND HALF OF IOB      */
         L     @12,CVTPTR                                          0264
