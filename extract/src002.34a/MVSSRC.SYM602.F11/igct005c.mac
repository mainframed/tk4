*/********************************************************************/
*/*  THE FOLLOWING IS THE RETRY ENTRY POINT FOR THE SECOND LEVEL     */
*/*  ESTAE, BACKUP5C (PART OF IGCT105C). THE SECOND LEVEL ESTAE IS   */
*/*  ENTERED IF AN ERROR SHOULD OCCUR DURING THE PROCESSING OF THE   */
*/*  CLEANUP ROUTINE(THE MAIN SECTION OF IGCT105C). IF IT IS DETER-  */
*/*  MINED THAT THE ERROR WAS CAUSED BY OTHER THAN A PROGRAM CHECK   */
*/*  IN THE CLEANUP ROUTINE THIS BACRETRY ROUTINE IS ENTERED TO      */
*/*  CONTINUE PERCOLATION OF THE ABEND TO RTM. BY RETURNING TO       */
*/*  IGCT005C BEFORE RETURNING TO RTM THE SAVEAREA OBTAINED UPON     */
*/*  ENTRY TO THIS MODULE WILL BE FREED.                             */
*/********************************************************************/
*                                                                  0691
*BACRETRY:                          /* RETRY AFT ABEND IN CLEANUP    */
*   GEN(ESTAE 0);                   /* REMOV 2ND EST LEVEL     YM7519*/
BACRETRY ESTAE 0
*   DO;                             /* SETRP RECORD(YES) DUMP(NO)  0692
*                                      COMPCOD(ERRSYS)             0692
*                                      WKAREA(SDWAPTR)               */
*     RESPECIFY                                                    0693
*      (GPR00P,                                                    0693
*       GPR01P,                                                    0693
*       GPR14P,                                                    0693
*       GPR15P)RSTD;                                               0693
*     GPR01P=SDWAPTR;                                              0694
         LR    GPR01P,SDWAPTR                                      0694
*     GPR01P->SDWARCDE=0;           /* SAVE RC VALUE                 */
         MVI   SDWARCDE(GPR01P),X'00'                              0695
*     GPR01P->SDWARCRD='1'B;        /* TURN ON RECORD INDICATOR      */
         OI    SDWARCRD(GPR01P),B'10000000'                        0696
*     GPR01P->SDWAREQ='0'B;         /* TURN OFF DUMP INDICATOR       */
         NI    SDWAREQ(GPR01P),B'01111111'                         0697
*     GPR14P=ERRSYS;                /* ACCESS COMPLETION CODE        */
         SR    GPR14P,GPR14P                                       0698
         ICM   GPR14P,7,ERRSYS                                     0698
*     GPR01P->SDWACMPC=GPR14P;      /* STORE COMPLETION CODE         */
         STCM  GPR14P,7,SDWACMPC(GPR01P)                           0699
*     RESPECIFY                                                    0700
*      (GPR00P,                                                    0700
*       GPR01P,                                                    0700
*       GPR14P,                                                    0700
*       GPR15P)UNRSTD;                                             0700
*   END;                            /* RECORD TO LOGREC THE ERROR    */
*   GOTO RETRTM;                    /* GO TO COMMON RET        YM7519*/
*                                                                  0702
         B     RETRTM                                              0702
*/********************************************************************/
*/*              COMMON ROUTINE TO RETURN TO RTM                     */
*/********************************************************************/
*                                                                  0703
*RETRTM:                                                           0703
*   ;                               /* RETURN TO RTM           YM7519*/
RETRTM   DS    0H                                                  0704
*   RFY                                                            0704
*     WRKREG RSTD;                  /* RESTRICT SAVE REG       YM7519*/
*   WRKREG=SAVEPTR;                 /* PUT SAVEAREA PTR IN     YM7519*/
         LR    WRKREG,SAVEPTR                                      0705
*                                   /* R11,EXPECTED BY PLS     YM7519*/
*   RETURN;                         /* RETURN TO FREE SAVE     YM7519*/
@EL00001 L     @13,4(,@13)                                         0706
@EF00001 L     @00,@SIZDATD                                        0706
         LR    @01,@11                                             0706
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0706
         BR    @14                                                 0706
*   RFY                                                            0707
*     WRKREG UNRSTD;                /* RELEASE REG             YM7519*/
*                                                                  0707
*/********************************************************************/
*                                                                  0708
*   END IGCT005C                    /* END OF 1ST LEVEL ESTAE        */
