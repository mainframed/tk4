         TITLE ' /******************************************************
               *********'
IDCAM01  CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IDCAM01   75.325'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
*                                                                  0016
*        /************************************************************/
*        /*                                                          */
*        /*  RECEIVE CONTROL FROM TSO -                      @YL026UD*/
*        /*  OBTAIN COMMAND PROCESSOR PARM LIST ADDRESS      @YL026UD*/
*        /*                                                          */
*        /************************************************************/
*                                                                  0017
*        CPPLPTR = REG1;                                   /*@YL026UD*/
*                                                                  0017
         LR    CPPLPTR,REG1                                        0017
*        /************************************************************/
*        /*                                                          */
*        /*  INVOKE AMS TO PROCESS THE REQUESTED FUNCTION    @YL026UD*/
*        /*                                                          */
*        /************************************************************/
*                                                                  0018
*        GEN(LINK  EP=IDCSATO,MF=(E,(REG1)) );             /*@YL026UD*/
*                                                                  0018
         LINK  EP=IDCSATO,MF=(E,(REG1))
*        /************************************************************/
*        /*                                                          */
*        /*  AMS PROCESSING COMPLETED -                      @YL026UD*/
*        /*  EXAMINE THE AMS FINAL CONDITION CODE            @YL026UD*/
*        /*  TO DETERMINE IF THE REQUESTED FUNCTION          @YL026UD*/
*        /*  WAS SUCCESSFULLY COMPLETED                      @YL026UD*/
*        /*                                                          */
*        /************************************************************/
*                                                                  0019
*        AMSCOND = REG15;                                  /*@YL026UD*/
*                                                                  0019
         LR    AMSCOND,REG15                                       0019
*        IF AMSCOND > CONDGOOD                             /*@YL026UD*/
*          THEN                                            /*@YL026UD*/
         CH    AMSCOND,@CH00041                                    0020
         BNH   @RF00020                                            0020
*            DO;                                           /*@YL026UD*/
*              /******************************************************/
*              /*                                                    */
*              /*  AMS CONDITION CODE INDICATES PROCESSING   @YL026UD*/
*              /*  NOT SUCCESSFUL - ISSUE THE STACK AND      @YL026UD*/
*              /*  TCLEARQ MACROS TO DELETE SUBSEQUENT       @YL026UD*/
*              /*  COMMANDS QUEUED BY THE TSO USER           @YL026UD*/
*              /*                                                    */
*              /******************************************************/
*                                                                  0022
*              REG1 = ADDR(IOPLAREA);                      /*@YL026UD*/
         LA    REG1,IOPLAREA                                       0022
*              REG2 = ADDR(STPBAREA);                      /*@YL026UD*/
         LA    REG2,STPBAREA                                       0023
*              REG3 = CPPLUPT;                             /*@YL026UD*/
         L     REG3,CPPLUPT(,CPPLPTR)                              0024
*              REG4 = CPPLECT;                             /*@YL026UD*/
         L     REG4,CPPLECT(,CPPLPTR)                              0025
*              REG5 = ADDR(ECBAREA);                       /*@YL026UD*/
*                                                                  0026
         LA    REG5,ECBAREA                                        0026
*              GENERATE;                                   /*@YL026UD*/
*                                                                  0027
         STACK PARM=(2),UPT=(3),ECT=(4),ECB=(5),DELETE=ALL,MF=(E,(1))
*              GEN(TCLEARQ INPUT);                         /*@YL026UD*/
*                                                                  0028
         TCLEARQ INPUT
*            END;                                          /*@YL026UD*/
*                                                                  0029
*        /************************************************************/
*        /*                                                          */
*        /*  RETURN CONTROL TO TSO                           @YL026UD*/
*        /*                                                          */
*        /************************************************************/
*                                                                  0030
*        RETURN CODE(AMSCOND);                             /*@OZ05946*/
*                                                                  0030
@RF00020 L     @13,4(,@13)                                         0030
         L     @00,@SIZDATD                                        0030
         LR    @01,@11                                             0030
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@07                                             0030
         L     @14,12(,@13)                                        0030
         LM    @00,@12,20(@13)                                     0030
         BR    @14                                                 0030
*        END IDCAM01;                                      /*@YL026UD*/
@DATA    DS    0H
@CH00041 DC    H'8'
@DATD    DSECT
         DS    0F
@SA00001 DS    18F
@ZTEMPS  DS    C
@ZTEMPND EQU   *
@ZLEN    EQU   @ZTEMPND-@ZTEMPS
IDCAM01  CSECT
         DS    0F
@DATD    DSECT
         DS    0D
STPBAREA DS    CL8
IOPLAREA DS    CL16
ECBAREA  DS    CL4
         ORG   *+1-(*-@DATD)/(*-@DATD) INSURE DSECT DATA
@ENDDATD EQU   *
IDCAM01  CSECT
         DS    0F
@SIZDATD DC    AL1(0)
         DC    AL3(@ENDDATD-@DATD)
         DS    0D
FIXAREA  DS    CL50
         ORG   FIXAREA
@NM00001 DC    50X'00'
         ORG   FIXAREA+50
@00      EQU   00                      EQUATES FOR REGISTERS 0-15
@01      EQU   01
@02      EQU   02
@03      EQU   03
@04      EQU   04
@05      EQU   05
@06      EQU   06
@07      EQU   07
@08      EQU   08
@09      EQU   09
@10      EQU   10
@11      EQU   11
@12      EQU   12
@13      EQU   13
@14      EQU   14
@15      EQU   15
AMSCOND  EQU   @07
CPPLPTR  EQU   @06
REG1     EQU   @01
REG2     EQU   @02
REG3     EQU   @03
REG4     EQU   @04
REG5     EQU   @05
REG15    EQU   @15
CPPL     EQU   0
CPPLUPT  EQU   CPPL+4
CPPLECT  EQU   CPPL+12
         AGO   .@UNREFD                START UNREFERENCED COMPONENTS
CPPLPSCB EQU   CPPL+8
CPPLCBUF EQU   CPPL
.@UNREFD ANOP                          END UNREFERENCED COMPONENTS
@ENDDATA EQU   *
         END   IDCAM01,(C'PLS0019',0602,75325)
