         TITLE '*********  IDCDB01:  DEBUGGING AIDS  (DUMP FACILITY)  **
               *********'
        MACRO
        GETMAIN &R,&LV=
        L     REG15,0(REG1)           GDTTBL ADDRESS
        L     REG15,GDTPRL(REG15)     PROL ROUTINE ADDRESS
        CNOP  2,4                     BOUNDARY ALIGNMENT
        LA    REG14,*+14              RETURN ADDRESS
        BALR  REG1,REG15              LINKAGE TO PROL ROUTINE
        DC    A(MODID)                MODULE ID ADDRESS
        DC    A(@SIZDATD)             AUTOMATIC STORAGE SIZE ADDRESS
        MEND
IDCDB01  CSECT ,                                                   0002
@MAINENT DS    0H                                                  0002
         USING *,@15                                               0002
         B     @PROLOG                                             0002
         DC    AL1(16)                                             0002
         DC    C'IDCDB01   75.077'                                 0002
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0002
         BALR  @12,0                                               0002
@PSTART  DS    0H                                                  0002
         USING @PSTART,@12                                         0002
         L     @00,@SIZDATD                                        0002
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0002
         USING @DATD,@11                                           0002
         ST    @13,@SA00001+4                                      0002
         LM    @00,@01,20(@13)                                     0002
         ST    @11,8(,@13)                                         0002
         LR    @13,@11                                             0002
         MVC   @PC00001(12),0(@01)                                 0002
        ST    1,LISTPTR
        ST    1,LISTSCAN
*        DO LISTLN = 1 TO 255 WHILE(LISTTEST = '0'B);              0163
         LA    LISTLN,1                                            0163
@DL00163 L     @10,LISTSCAN                                        0163
         TM    LISTTEST(@10),B'10000000'                           0163
         BNZ   @DC00163                                            0163
*          LISTSCAN = LISTSCAN + 4;                                0164
         AL    @10,@CF00039                                        0164
         ST    @10,LISTSCAN                                        0164
*        END;                                                      0165
*                                                                  0165
*                                                                  0165
         AL    LISTLN,@CF00212                                     0165
         C     LISTLN,@CF00612                                     0165
         BNH   @DL00163                                            0165
@DC00163 DS    0H                                                  0166
*        IF LISTLN = DUMPARGS                                      0166
*          THEN                                                    0166
         C     LISTLN,@CF00155                                     0166
         BNE   @RF00166                                            0166
*            SYMLIST = DUMPLIST;                                   0167
         L     @10,@PC00001+8                                      0167
         L     @10,DUMPLIST(,@10)                                  0167
         ST    @10,SYMLIST                                         0167
*          ELSE                                                    0168
*            SYMLIST = NULLPTR;                                    0168
*                                                                  0168
         B     @RC00166                                            0168
@RF00166 SLR   @10,@10                                             0168
         ST    @10,SYMLIST                                         0168
*        OLDERID2 = NEWERID2;                                      0169
@RC00166 L     @10,@PC00001                                        0169
         L     @05,GDTTR2(,@10)                                    0169
         MVC   @TS00001(95),NEWERID2(@05)                          0169
         MVC   OLDERID2(95,@05),@TS00001                           0169
*        NEWID2 = DUMPID;                                          0170
*                                                                  0170
         L     @03,@PC00001+4                                      0170
         MVC   NEWID2(4,@05),DUMPID(@03)                           0170
*        /************************************************************/
*        /*                                                          */
*        /*  INITIALIZE 'UPRINT' MACRO ARGUMENTS                     */
*        /*                                                          */
*        /************************************************************/
*                                                                  0171
*        PRNTLIST = '00'X;                                         0171
         XC    PRNTLIST+1(19),PRNTLIST+1                           0171
         MVI   PRNTLIST,X'00'                                      0171
*        DDSTRU = ADDR(PRNTLIST);                                  0172
         LA    @03,PRNTLIST                                        0172
         ST    @03,DDSTRU                                          0172
*        DARGSMOD = MSGMOD;                                        0173
*                                                                  0173
         MVC   DARGSMOD(3,@03),@CC00603                            0173
*        /************************************************************/
*        /*                                                          */
*        /*  SAVE CURRENT STATE OF AMS TRACE TABLES - LOOPING        */
*        /*  BETWEEN DUMP ROUTINE AND TEXT PROCESSOR WOULD RENDER    */
*        /*  TRACE TABLES MEANINGLESS IF NOT RESTORED FOLLOWING      */
*        /*  DUMP PROCESSING                                         */
*        /*                                                          */
*        /************************************************************/
*                                                                  0174
*        TRC1SAVE = TRACE1;                                        0174
         L     @03,GDTTR1(,@10)                                    0174
         MVC   TRC1SAVE(100),TRACE1(@03)                           0174
*        TRC2SAVE = TRACE2;                                        0175
*                                                                  0175
         MVC   TRC2SAVE(100),TRACE2(@05)                           0175
*        /************************************************************/
*        /*                                                          */
*        /*  TURN OFF TEST OPTION - NECESSARY TO PREVENT LOOPING     */
*        /*  BETWEEN THE DUMP ROUTINE AND TEXT PROCESSOR WHILE       */
*        /*  OUTPUTTING DUMPS                                        */
*        /*                                                          */
*        /************************************************************/
*                                                                  0176
*        DBGPTR = GDTDBG;                                          0176
         L     DBGPTR,GDTDBG(,@10)                                 0176
*        GDTDBG = NULLPTR;                                         0177
*                                                                  0177
         SLR   @05,@05                                             0177
         ST    @05,GDTDBG(,@10)                                    0177
*        /************************************************************/
*        /*                                                          */
*        /*  DETERMINE IF DUMPING REQUESTED FOR INVOKING MODULE      */
*        /*  AREA - COMPARE MODULE IDENTIFIER (FROM TRACE TABLE)     */
*        /*  AGAINST TEST OPTION ('AREAS') IDENTIFIERS.              */
*        /*                                                          */
*        /************************************************************/
*                                                                  0178
*        AREAMTCH = NO;                                            0178
*                                                                  0178
         NI    AREAMTCH,B'01111111'                                0178
*        IF AREAPTR ^= NULLPTR                                     0179
*          THEN                                                    0179
         L     @10,GDTDBH(,@10)                                    0179
         C     @05,AREAPTR(,@10)                                   0179
         BE    @RF00179                                            0179
*            DO AREASCAN = 0 TO AREAINDX-1 WHILE(AREAMTCH = NO);   0180
*                                                                  0180
         LR    AREASCAN,@05                                        0180
         B     @DE00180                                            0180
@DL00180 TM    AREAMTCH,B'10000000'                                0180
         BNZ   @DC00180                                            0180
*              IF DUMPID(1:2) = AREANAME(AREASCAN+1)       /*@Y30SSLO*/
*                THEN                                              0181
         L     @10,@PC00001+4                                      0181
         LR    @05,AREASCAN                                        0181
         ALR   @05,@05                                             0181
         L     @15,@PC00001                                        0181
         L     @01,GDTDBH(,@15)                                    0181
         L     @01,AREAPTR(,@01)                                   0181
         ALR   @01,@05                                             0181
         CLC   DUMPID(2,@10),AREANAME(@01)                         0181
         BNE   @RF00181                                            0181
*                  AREAMTCH = YES;                                 0182
*                                                                  0182
         OI    AREAMTCH,B'10000000'                                0182
*            END;                                                  0183
*                                                                  0183
@RF00181 AL    AREASCAN,@CF00212                                   0183
@DE00180 L     @10,@PC00001                                        0183
         L     @10,GDTDBH(,@10)                                    0183
         L     @10,AREAPTR(,@10)                                   0183
         L     @10,AREAINDX(,@10)                                  0183
         BCTR  @10,0                                               0183
         CR    AREASCAN,@10                                        0183
         BNH   @DL00180                                            0183
@DC00180 DS    0H                                                  0184
*        /************************************************************/
*        /*                                                          */
*        /*  DETERMINE IF FULL REGION DUMP REQUESTED FOR INVOKING    */
*        /*  DUMP IDENTIFIER - COMPARE DUMP IDENTIFIER AGAINST FULL  */
*        /*  REGION DUMP ('FULL') IDENTIFIERS.                       */
*        /*                                                          */
*        /************************************************************/
*                                                                  0184
*        FULLMTCH = NO;                                            0184
*                                                                  0184
@RF00179 NI    FULLMTCH,B'01111111'                                0184
*        IF FULLPTR ^= NULLPTR                                     0185
*          THEN                                                    0185
         SLR   @10,@10                                             0185
         L     @05,@PC00001                                        0185
         L     @05,GDTDBH(,@05)                                    0185
         C     @10,FULLPTR(,@05)                                   0185
         BE    @RF00185                                            0185
*            DO FULLSCAN = 0 TO FULLINDX-1 WHILE(FULLMTCH = NO);   0186
*                                                                  0186
         LR    FULLSCAN,@10                                        0186
         B     @DE00186                                            0186
@DL00186 TM    FULLMTCH,B'10000000'                                0186
         BNZ   @DC00186                                            0186
*              IF DUMPID = FDUMPID(FULLSCAN+1)                     0187
*                THEN                                              0187
         L     @10,@PC00001+4                                      0187
         LR    @05,FULLSCAN                                        0187
         MH    @05,@CH00557                                        0187
         L     @03,@PC00001                                        0187
         L     @03,GDTDBH(,@03)                                    0187
         L     @03,FULLPTR(,@03)                                   0187
         ALR   @03,@05                                             0187
         CLC   DUMPID(4,@10),FDUMPID(@03)                          0187
         BNE   @RF00187                                            0187
*                  FULLMTCH = YES;                                 0188
*                                                                  0188
         OI    FULLMTCH,B'10000000'                                0188
*            END;                                                  0189
*                                                                  0189
@RF00187 AL    FULLSCAN,@CF00212                                   0189
@DE00186 L     @10,@PC00001                                        0189
         L     @10,GDTDBH(,@10)                                    0189
         L     @10,FULLPTR(,@10)                                   0189
         L     @10,FULLINDX(,@10)                                  0189
         BCTR  @10,0                                               0189
         CR    FULLSCAN,@10                                        0189
         BNH   @DL00186                                            0189
@DC00186 DS    0H                                                  0190
*        /************************************************************/
*        /*                                                          */
*        /*  DUMP TRACE TABLES IF REQUESTED VIA 'TRACE' OPTION, OR   */
*        /*  AUTOMATCIALLY IF TEST OPTION IS 'ON' FOR INVOKING       */
*        /*  MODULE AREA, OR IF FULL REGION DUMP REQUESTED FOR       */
*        /*  INVOKING DUMP IDENTIFIER.                               */
*        /*                                                          */
*        /************************************************************/
*                                                                  0190
*        IF AREAMTCH = YES × FULLMTCH = YES × TESTRACE ^= 0        0190
*          THEN                                                    0190
@RF00185 TM    AREAMTCH,B'10000000'                                0190
         BO    @RT00190                                            0190
         TM    FULLMTCH,B'10000000'                                0190
         BO    @RT00190                                            0190
         L     @10,@PC00001                                        0190
         L     @10,GDTDBH(,@10)                                    0190
         LH    @10,TESTRACE(,@10)                                  0190
         LTR   @10,@10                                             0190
         BZ    @RF00190                                            0190
@RT00190 DS    0H                                                  0191
*            DO;                                                   0191
*              /******************************************************/
*              /*                                                    */
*              /*  PRINT DUMP HEADER LINE CONTAINING DUMP IDENTIFIER */
*              /*                                                    */
*              /******************************************************/
*                                                                  0192
*              DARGSENT = HEADRMSG;                                0192
         L     @10,DDSTRU                                          0192
         MVI   DARGSENT(@10),X'07'                                 0192
*              DARGDBP = ADDR(DUMPID);                             0193
         L     @05,@PC00001+4                                      0193
         ST    @05,DARGDBP(,@10)                                   0193
*              DARGILP = LENGTH(DUMPID);                           0194
*                                                                  0194
         MVC   DARGILP(2,@10),@CH00039                             0194
*              CALL AMSTP011(GDTTBL,PRTFILE,DDSTRU);               0195
*                                                                  0195
         L     @10,@PC00001                                        0195
         ST    @10,@AL00001                                        0195
         LA    @05,@CF00110                                        0195
         ST    @05,@AL00001+4                                      0195
         LA    @05,DDSTRU                                          0195
         ST    @05,@AL00001+8                                      0195
         MVI   @AL00001+8,X'80'                                    0195
         L     @15,GDTPRT(,@10)                                    0195
         LA    @01,@AL00001                                        0195
         BALR  @14,@15                                             0195
*              /******************************************************/
*              /*                                                    */
*              /*  DUMP IDCAMS INTER- AND INTRA-MODULE TRACE TABLES  */
*              /*                                                    */
*              /******************************************************/
*                                                                  0196
*              DARGSENT = INTERMSG;                                0196
         L     @10,DDSTRU                                          0196
         MVI   DARGSENT(@10),X'09'                                 0196
*              DARGILP = LENGTH(TRC1SAVE);                         0197
         MVC   DARGILP(2,@10),@CH00140                             0197
*              DARGDBP = ADDR(TRC1SAVE);                           0198
*                                                                  0198
         LA    @05,TRC1SAVE                                        0198
         ST    @05,DARGDBP(,@10)                                   0198
*              CALL AMSTP011(GDTTBL,PRTFILE,DDSTRU);               0199
*                                                                  0199
         L     @10,@PC00001                                        0199
         ST    @10,@AL00001                                        0199
         LA    @05,@CF00110                                        0199
         ST    @05,@AL00001+4                                      0199
         LA    @05,DDSTRU                                          0199
         ST    @05,@AL00001+8                                      0199
         MVI   @AL00001+8,X'80'                                    0199
         L     @15,GDTPRT(,@10)                                    0199
         LA    @01,@AL00001                                        0199
         BALR  @14,@15                                             0199
*              DARGSENT = INTRAMSG;                                0200
         L     @10,DDSTRU                                          0200
         MVI   DARGSENT(@10),X'0A'                                 0200
*              DARGILP = LENGTH(TRC2SAVE);                         0201
         MVC   DARGILP(2,@10),@CH00140                             0201
*              DARGDBP = ADDR(TRC2SAVE);                           0202
*                                                                  0202
         LA    @05,TRC2SAVE                                        0202
         ST    @05,DARGDBP(,@10)                                   0202
*              CALL AMSTP011(GDTTBL,PRTFILE,DDSTRU);               0203
*                                                                  0203
         L     @10,@PC00001                                        0203
         ST    @10,@AL00001                                        0203
         LA    @05,@CF00110                                        0203
         ST    @05,@AL00001+4                                      0203
         LA    @05,DDSTRU                                          0203
         ST    @05,@AL00001+8                                      0203
         MVI   @AL00001+8,X'80'                                    0203
         L     @15,GDTPRT(,@10)                                    0203
         LA    @01,@AL00001                                        0203
         BALR  @14,@15                                             0203
*            END;                                                  0204
*                                                                  0204
*        /************************************************************/
*        /*                                                          */
*        /*  DUMP SELECTED SYMBOLIC VARIABLES IF REQUESTED           */
*        /*                                                          */
*        /************************************************************/
*                                                                  0205
*        IF AREAMTCH = YES × FULLMTCH = YES                        0205
*          THEN                                                    0205
@RF00190 TM    AREAMTCH,B'10000000'                                0205
         BO    @RT00205                                            0205
         TM    FULLMTCH,B'10000000'                                0205
         BNO   @RF00205                                            0205
@RT00205 DS    0H                                                  0206
*            DO;                                                   0206
*              /******************************************************/
*              /*                                                    */
*              /*  A DUMP LIST MUST BE SUPPLIED FOR A SYMBOLIC DUMP  */
*              /*                                                    */
*              /******************************************************/
*                                                                  0207
*              IF SYMLIST ^= NULLPTR                               0207
*                THEN                                              0207
         L     @10,SYMLIST                                         0207
         LTR   @10,@10                                             0207
         BZ    @RF00207                                            0207
*                  /**************************************************/
*                  /*                                                */
*                  /*  CALL THE AMS SYMBOLIC DUMP ROUTINE (IDCDB02)  */
*                  /*                                                */
*                  /**************************************************/
*                                                                  0208
*                  CALL IDCSACL0(GDTTBL,SYMDUMP,SYMLIST);          0208
*                                                                  0208
         L     @10,@PC00001                                        0208
         ST    @10,@AL00001                                        0208
         LA    @05,@CC00592                                        0208
         ST    @05,@AL00001+4                                      0208
         LA    @05,SYMLIST                                         0208
         ST    @05,@AL00001+8                                      0208
         MVI   @AL00001+8,X'80'                                    0208
         L     @15,GDTCAL(,@10)                                    0208
         LA    @01,@AL00001                                        0208
         BALR  @14,@15                                             0208
*            END;                                                  0209
*                                                                  0209
@RF00207 DS    0H                                                  0210
*        /************************************************************/
*        /*                                                          */
*        /*  OUTPUT FULL REGION DUMP IF REQUESTED                    */
*        /*                                                          */
*        /************************************************************/
*                                                                  0210
*        IF FULLMTCH = YES                                         0210
*          THEN                                                    0210
@RF00205 TM    FULLMTCH,B'10000000'                                0210
         BNO   @RF00210                                            0210
*BEGINCHK:   DO;                                                   0211
BEGINCHK DS    0H                                                  0212
*              /******************************************************/
*              /*                                                    */
*              /*  INCREMENT COUNT OF THE PASSES                     */
*              /*  THROUGH THIS REQUESTED DUMP POINT                 */
*              /*                                                    */
*              /******************************************************/
*                                                                  0212
*              REALBEG(FULLSCAN) = REALBEG(FULLSCAN) + 1;          0212
*                                                                  0212
         LR    @10,FULLSCAN                                        0212
         MH    @10,@CH00557                                        0212
         L     @05,@PC00001                                        0212
         L     @05,GDTDBH(,@05)                                    0212
         L     @05,FULLPTR(,@05)                                   0212
         LA    @03,1                                               0212
         AH    @03,REALBEG-12(@10,@05)                             0212
         STH   @03,REALBEG-12(@10,@05)                             0212
*              /******************************************************/
*              /*                                                    */
*              /*  DETERMINE IF 'BEGIN' DUMP THRESHOLD REACHED       */
*              /*                                                    */
*              /******************************************************/
*                                                                  0213
*              IF FDUMPBEG(FULLSCAN) <= REALBEG(FULLSCAN)          0213
*                THEN                                              0213
         AL    @05,@CF00623                                        0213
         CH    @03,FDUMPBEG-8(@10,@05)                             0213
         BL    @RF00213                                            0213
*COUNTCHK:         DO;                                             0214
COUNTCHK DS    0H                                                  0215
*                    /************************************************/
*                    /*                                              */
*                    /*  'BEGIN' DUMP THRESHOLD REACHED -            */
*                    /*  INCREMENT COUNT OF FULL REGION DUMPS        */
*                    /*  PRODUCED AT THIS DUMP POINT                 */
*                    /*                                              */
*                    /************************************************/
*                                                                  0215
*                    REALCNT(FULLSCAN) = REALCNT(FULLSCAN) + 1;    0215
*                                                                  0215
         LR    @10,FULLSCAN                                        0215
         MH    @10,@CH00557                                        0215
         L     @05,@PC00001                                        0215
         L     @05,GDTDBH(,@05)                                    0215
         L     @05,FULLPTR(,@05)                                   0215
         LA    @03,1                                               0215
         AH    @03,REALCNT-12(@10,@05)                             0215
         STH   @03,REALCNT-12(@10,@05)                             0215
*                    /************************************************/
*                    /*                                              */
*                    /*  DETERMINE IF 'COUNT' DUMP LIMIT REACHED     */
*                    /*                                              */
*                    /************************************************/
*                                                                  0216
*                    IF REALCNT(FULLSCAN) <= FDUMPCNT(FULLSCAN)    0216
*                      THEN                                        0216
         AL    @05,@CF00624                                        0216
         CH    @03,FDUMPCNT-10(@10,@05)                            0216
         BH    @RF00216                                            0216
*SNAPDUMP:               DO;                                       0217
SNAPDUMP DS    0H                                                  0218
*                          /******************************************/
*                          /*                                        */
*                          /*  'COUNT' DUMP LIMIT NOT REACHED -      */
*                          /*  PRODUCE FULL REGION (SNAP) DUMP       */
*                          /*                                        */
*                          /******************************************/
*                                                                  0218
*                          SNAPID = SNAPID + 1;                    0218
*                                                                  0218
         L     @10,@PC00001                                        0218
         L     @05,GDTDBH(,@10)                                    0218
         LA    @15,1                                               0218
         AH    @15,SNAPID(,@05)                                    0218
         STH   @15,SNAPID(,@05)                                    0218
*                          /******************************************/
*                          /*                                        */
*                          /*  ISSUE USNAP MACRO TO PRODUCE          */
*                          /*  FULL REGION (SNAP) DUMP               */
*                          /*                                        */
*                          /*  (IF UNABLE TO PRODUCE THE SNAP DUMP,  */
*                          /*  THE ABORT ROUTINE WILL BE INVOKED -   */
*                          /*  CONTROL WILL NOT BE RETURNED TO       */
*                          /*  THIS ROUTINE)                         */
*                          /*                                        */
*                          /******************************************/
*                                                                  0219
*                          CALL IDCSASN0(GDTTBL,SNAPID);           0219
*                                                                  0219
         ST    @10,@AL00001                                        0219
         LA    @05,SNAPID(,@05)                                    0219
         ST    @05,@AL00001+4                                      0219
         L     @15,GDTSNP(,@10)                                    0219
         LA    @01,@AL00001                                        0219
         BALR  @14,@15                                             0219
*                          /******************************************/
*                          /*                                        */
*                          /*  PRINT 'FULL REGION (SNAP) DUMP        */
*                          /*  PRODUCED' LINE CONTAINING THE         */
*                          /*  SNAP DUMP IDENTIFIER                  */
*                          /*                                        */
*                          /******************************************/
*                                                                  0220
*                          SNAPTXT1 = SNAPID;                      0220
         L     @10,@PC00001                                        0220
         L     @05,GDTDBH(,@10)                                    0220
         LH    @05,SNAPID(,@05)                                    0220
         STH   @05,SNAPTXT1                                        0220
*                          SNAPTXT2 = DUMPID;                      0221
*                                                                  0221
         L     @05,@PC00001+4                                      0221
         MVC   SNAPTXT2(4),DUMPID(@05)                             0221
*                          DARGSENT = SNAPMSG;                     0222
         L     @05,DDSTRU                                          0222
         MVI   DARGSENT(@05),X'08'                                 0222
*                          DARGDBP = ADDR(SNAPTEXT);               0223
         LA    @15,SNAPTEXT                                        0223
         ST    @15,DARGDBP(,@05)                                   0223
*                          DARGILP = LENGTH(SNAPTEXT);             0224
*                                                                  0224
         MVC   DARGILP(2,@05),@CH00149                             0224
*                          CALL AMSTP011(GDTTBL,PRTFILE,DDSTRU);   0225
*                                                                  0225
         ST    @10,@AL00001                                        0225
         LA    @05,@CF00110                                        0225
         ST    @05,@AL00001+4                                      0225
         LA    @05,DDSTRU                                          0225
         ST    @05,@AL00001+8                                      0225
         MVI   @AL00001+8,X'80'                                    0225
         L     @15,GDTPRT(,@10)                                    0225
         LA    @01,@AL00001                                        0225
         BALR  @14,@15                                             0225
*                        END SNAPDUMP;                             0226
*                                                                  0226
*                  END COUNTCHK;                                   0227
*                                                                  0227
@RF00216 DS    0H                                                  0228
*            END BEGINCHK;                                         0228
*                                                                  0228
@RF00213 DS    0H                                                  0229
*        /************************************************************/
*        /*                                                          */
*        /*  RESTORE TRACE TABLES AND TEST OPTION                    */
*        /*                                                          */
*        /************************************************************/
*                                                                  0229
*        TRACE1 = TRC1SAVE;                                        0229
@RF00210 L     @10,@PC00001                                        0229
         L     @05,GDTTR1(,@10)                                    0229
         MVC   TRACE1(100,@05),TRC1SAVE                            0229
*        TRACE2 = TRC2SAVE;                                        0230
*                                                                  0230
         L     @05,GDTTR2(,@10)                                    0230
         MVC   TRACE2(100,@05),TRC2SAVE                            0230
*        GDTDBG = DBGPTR;                                          0231
*                                                                  0231
         ST    DBGPTR,GDTDBG(,@10)                                 0231
*        /************************************************************/
*        /*                                                          */
*        /*  RETURN TO THE MODULE WHICH INVOKED THE DUMP ROUTINE     */
*        /*                                                          */
*        /************************************************************/
*                                                                  0232
*        CALL IDCSAEP0(GDTTBL,MODID);                              0232
*                                                                  0232
