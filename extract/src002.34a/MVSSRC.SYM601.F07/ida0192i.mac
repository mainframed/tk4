*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = DCB EXIT                                              *
**       THIS ROUTINE GIVES CONTROL TO THE ISAM DCB EXIT ROUTINE      *
**       IF ONE IS SPECIFIED IN THE DCB EXIT LIST.  REGISTERS ARE     *
**       SAVED IN THE SVRB BEFORE ISSUING A SYNCH TO GIVE THE EXIT    *
**       ROUTINE CONTROL.                                             *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = DATA MANAGEMENT KEY 5                 *
**                              SYSTEM KEY 0                          *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0421
*DCBEXIT:                                                          0421
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0421
DCBEXIT  DS    0H                                                  0422
*   RESPECIFY                                                      0422
*    (R15,                                                         0422
*     RLKRETSV,                                                    0422
*     RWORK2) RESTRICTED;                                          0422
*   RLKRETSV=R14;                   /* SAVE RETURN ADDR      @Y021130*/
         LR    RLKRETSV,R14                                        0423
*   IIFTEST=OFF;                                                   0424
         NI    IIFTEST(RIICB),B'11111011'                          0424
*   IF DCBEXLSA^=ZERO THEN          /* IF EXIT LIST IS PROVIDED      */
         L     @11,DCBEXLSA+27(,RDCB)                              0425
         LA    @11,0(,@11)                                         0425
         LTR   @11,@11                                             0425
         BZ    @RF00425                                            0425
*     DO RWORK2=DCBEXLSA BY LENGTH(EXLST) WHILE IIFTEST=OFF;       0426
         LR    RWORK2,@11                                          0426
         B     @DE00426                                            0426
@DL00426 DS    0H                                                  0427
*       IF EXCODE=EXDCB×            /* IF DCB EXIT FOUND,            */
*           EXCODE=EXDCBL THEN      /* PASS CONTROL                  */
         CLI   EXCODE(RWORK2),B'00000101'                          0427
         BE    @RT00427                                            0427
         CLI   EXCODE(RWORK2),B'10000101'                          0427
         BNE   @RF00427                                            0427
@RT00427 DS    0H                                                  0428
*         DO;                                                      0428
*           R15=EXADDR;             /* GET ADDR OF USER EXIT         */
         L     R15,EXADDR-1(,RWORK2)                               0429
         LA    R15,0(,R15)                                         0429
*           R14=ADDR(SVRB);         /* GET ADDR OF SVRB              */
         L     @11,CVTPTR                                          0430
         L     @11,CVTTCBDW(,@11)                                  0430
         L     @11,TCBPTR(,@11)                                    0430
         L     R14,SVRBPTR(,@11)                                   0430
*           DO;                     /* MODESET EXTKEY(ZERO)          */
*             SPKA(0);              /* SET PSW KEY                   */
         SLR   @11,@11                                             0432
         SPKA  0(@11)                                              0432
*           END;                    /* MODESET EXTKEY(ZERO) KEY ZERO
*                                      FOR STORE INTO SVRB   @Y021130*/
*                                                                  0433
*           /*********************************************************/
*           /*                                                       */
*           /* DCBDEBAD CONTAINS THE ADDRESS OF THE I/O SUPPORT WORK */
*           /* AREA TO ENABLE THE DCB EXIT ROUTINE TO REF- ERENCE THE*/
*           /* FORMAT 2 DSCB.                                @Y021130*/
*           /* DSCCORE (THE POINTER TO THE FORMAT 2 DSCB FOR ISAM) IS*/
*           /* INITIALIZED TO POINT TO HEX 40 BYTES BEFORE THE DCB   */
*           /* FIELD DCBNCHRI. THIS WILL SUPPORT THE FIELD DS2NOBYT  */
*           /* WHICH IS IN THE FORMAT 2 DSCB AT AN OFFSET OF HEX 40  */
*           /* BYTES BY MAKING THE OFF- SET TO DS2NOBYT COINCIDE WITH*/
*           /* THE FIELD DCBNCHRI. DCBNCHRI WAS INITIALIZED IN THE   */
*           /* ROUTINE DCBMERGE.                                     */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0434
*           DSCCORE=ADDR(DCBESETL)+2;/* SIMULATE THE F2 DSCB POINTER
*                                      FOR REFERENCE TO DS2NOBYT   0434
*                                                            @Y021130*/
         LA    @11,DCBESETL+52(,RDCB)                              0434
         AL    @11,@CF00098                                        0434
         ST    @11,DSCCORE(,RCORE)                                 0434
*           GEN SETS(XRBXSAV);                                     0435
*                                       /*                   @Y021130*/
                 STM R2,R13,XRBXSAV(R14)   SAVE REGS-EXTENDED SAVE A
*           DO;                     /* MODESET KEYADDR(DXUKEY)       */
*             SPKA(DXUKEY);         /* SET PSW KEY                   */
         SLR   @11,@11                                             0437
         IC    @11,DXUKEY(,RCORE)                                  0437
         SPKA  0(@11)                                              0437
*           END;                    /* MODESET KEYADDR(DXUKEY) USER
*                                      KEY FOR EXIT          @Y021130*/
*                                                                  0438
*           /*********************************************************/
*           /*                                                       */
*           /* AUDIT DCB EXIT IS BEING TAKEN                 @Y021130*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0439
*           AUDDCBEX=ON;            /* SET FLAG IN IICB      @Y021130*/
         OI    AUDDCBEX(RIICB),B'00100000'                         0439
*           GEN REFS(XRBREG);                                      0440
*                                       /*                   @Y021130*/
                 LM  R2,R13,XRBREG+8(R14)  RELOAD USER REGS
*           DCBOFUEX=OFF;           /* TURN ON DCB LOCK BIT (0)      */
         NI    DCBOFUEX+40(RDCB),B'11111101'                       0441
*           GEN(SYNCH (15));        /* GO TO USER DCB EXIT           */
         SYNCH (15)
*           R14=ADDR(SVRB);         /* FIND SVRB AGAIN RELOAD REGS   */
         L     @11,CVTPTR                                          0443
         L     @11,CVTTCBDW(,@11)                                  0443
         L     @11,TCBPTR(,@11)                                    0443
         L     R14,SVRBPTR(,@11)                                   0443
*           GEN REFS(XRBXSAV);                                     0444
                 LM  R2,R13,XRBXSAV(R14)
*           DO;                     /* MODESET EXTKEY(DATAMGT)       */
*             SPKA(80);             /* SET PSW KEY                   */
         LA    @11,80                                              0446
         SPKA  0(@11)                                              0446
*           END;                    /* MODESET EXTKEY(DATAMGT) ASSUME
*                                      DATA MANAGEMENT KEY   @Y021130*/
*           DSCCORE=ZERO;           /* RESET F2 DSCB POINTER TO ZERO
*                                                            @Y021130*/
         SLR   @11,@11                                             0448
         ST    @11,DSCCORE(,RCORE)                                 0448
*           DO;                     /* MODESET KEYADDR(DXUKEY)       */
*             SPKA(DXUKEY);         /* SET PSW KEY                   */
         SLR   @11,@11                                             0450
         IC    @11,DXUKEY(,RCORE)                                  0450
         SPKA  0(@11)                                              0450
*           END;                    /* MODESET KEYADDR(DXUKEY) USER
*                                      KEY FOR EXIT          @Y021130*/
*                                                                  0451
*           /*********************************************************/
*           /*                                                       */
*           /* AUDIT THAT DCB EXIT HAS RETURNED              @Y021130*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0452
*           AUDDCBRT=ON;            /* SET FLAG IN IICB      @Y021130*/
         OI    AUDDCBRT(RIICB),B'00010000'                         0452
*           RDCB=DXUDCBAD;          /* RESTORE DCB PTR       @Y021130*/
         L     RDCB,DXUDCBAD(,RCORE)                               0453
*           DCBOFUEX=ON;            /* TURN OFF DCB LOCK BIT (1)   0454
*                                                            @Y021130*/
         OI    DCBOFUEX+40(RDCB),B'00000010'                       0454
*         END;                      /* NO DCB EXIT IN EXLST          */
*       IF EXLAST=ON THEN           /* IF LAST ENTRY                 */
@RF00427 TM    EXLAST(RWORK2),B'10000000'                          0456
         BNO   @RF00456                                            0456
*         IIFTEST=ON;               /* STOP SEARCHING                */
         OI    IIFTEST(RIICB),B'00000100'                          0457
*     END;                                                         0458
@RF00456 AL    RWORK2,@CF00199                                     0458
@DE00426 TM    IIFTEST(RIICB),B'00000100'                          0458
         BZ    @DL00426                                            0458
*   R14=RLKRETSV;                   /* RESTORE RETURN ADDRESS      0459
*                                                            @Y021130*/
@RF00425 LR    R14,RLKRETSV                                        0459
*   RESPECIFY                                                      0460
*    (R15,                                                         0460
*     RLKRETSV,                                                    0460
*     RWORK2) UNRESTRICTED;                                        0460
*   END DCBEXIT;                                                   0461
@EL00010 DS    0H                                                  0461
@EF00010 DS    0H                                                  0461
@ER00010 BR    @14                                                 0461
*                                                                  0462
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = VALIDITY CHECK                                        *
**       THIS ROUTINE VALIDITY CHECKS THE DATA SET ATTRIBUTES THAT    *
**       WERE SPECIFIED WHEN THE VSAM DATA SET WAS DEFINED BY AMS.    *
**       THIS CHECK WILL BE PERFORMED BY COMPARING THE VALUES FOR     *
**       KEYLE, RKP AND LRECL IN THE DCB WITH THE CORRESPONDING       *
**       FIELDS IN THE VSAM AMDSB.  THIS COMPARE IS UNABLE TO DETECT  *
**       AMS DEFINE ERRORS IF THESE FIELDS WERE SPECIFIED IN THE DD   *
**       CARD WHEN ISAM WAS USED.  THIS ROUTINE WILL ABEND IF THE     *
**       ABOVE VALIDITY CHECK FAILS OR IF DISP=OLD IS SPECIFIED WHEN  *
**       THE DCB IS OPENED FOR OUTPUT (LOAD MODE) SINCE THE ISAM      *
**       INTERFACE DOES NOT SUPPORT RELOADING THE DATA SET.           *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**       REGISTER 6 - RACB - ACB POINTER                              *
**                                                                    *
**   OUTPUT =                                                         *
**       SAME AS ABOVE                                                *
**       ABEND 03B - ERROR                                            *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0462
*VALIDCHK:                                                         0462
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0462
VALIDCHK DS    0H                                                  0463
*   RESPECIFY                                                      0463
*    (JFCBPTR) RESTRICTED;                                         0463
*   JFCBPTR=ADDR(DXJBF);            /* INIT JFCB POINTER     @Y021130*/
         LA    JFCBPTR,DXJBF(,RCORE)                               0464
*   IF JFCDISP='01'B&               /* IF DISP = OLD AND             */
*       AMDNLR>ZERO&                /* DATA SET CONTAINS RECORDS     */
*       (DCBMFMVP=ON×               /* AND LOAD MODE (PUT MOVE       */
*       DCBMFLCP=ON) THEN           /* OR PUT LOCATE)                */
         TM    JFCDISP(JFCBPTR),B'01000000'                        0465
         BNO   @RF00465                                            0465
         TM    JFCDISP(JFCBPTR),B'10000000'                        0465
         BNZ   @RF00465                                            0465
         L     @15,ACBAMBL(,RACB)                                  0465
         L     @15,AMBLDTA(,@15)                                   0465
         L     @15,AMBDSB(,@15)                                    0465
         L     @15,AMDNLR(,@15)                                    0465
         LTR   @15,@15                                             0465
         BNP   @RF00465                                            0465
         TM    DCBMFMVP+40(RDCB),B'00011000'                       0465
         BNZ   @RT00465                                            0465
*     GO TO EXIT3B;                 /* THEN ABEND BECAUSE RELOAD IS
*                                      NOT SUPPORTED                 */
*   RESPECIFY                                                      0467
*    (JFCBPTR) UNRESTRICTED;                                       0467
@RF00465 DS    0H                                                  0468
*   IF DCBKEYLE=AMDKEYLN&           /* IF KEY LENGTH IS OK CHECK   0468
*                                      RELATIVE KEY POSITN           */
*       ((DCBRECL='10'B&            /* RKP IS OK IF IT IS THE        */
*       AMDRKP=DCBRKP)×             /* SAME FOR FIXED LENGTH RCDS    */
*       (DCBRECL='01'B&             /* OR 4 HAS BEEN SUBTRACTED      */
*       AMDRKP=DCBRKP-4))&          /* FOR VARIABLE LENGTH RECDS   0468
*                                      CHECK LOGICAL RECORD LEN      */
*       (DCBRECL='10'B&             /* LRECL IS OK IF KEY LENGTH     */
*       (DCBRECBR=OFF&              /* HAS BEEN ADDED FOR FIXED,     */
*       (DCBRKP=ZERO&               /* UNBLKD, RKP=0 RECORDS OR      */
*       AMDLRECL=DCBLRECL+DCBKEYLE)×(DCBRKP>ZERO&/* LRECL IS THE SAME
*                                      FOR                           */
*       AMDLRECL=DCBLRECL))×        /* FIXED, UNBLKD, RKP>0 OR       */
*       (DCBRECBR=ON&               /* LRECL IS THE SAME FOR         */
*       AMDLRECL=DCBLRECL))×        /* FIXED, BLOCKED RECORDS OR     */
*       (DCBRECL='01'B&             /* 4 HAS BEEN SUBTRACTED FOR     */
*       AMDLRECL=DCBLRECL-4) THEN   /* VARIABLE LENGTH RECORDS       */
         L     @15,ACBAMBL(,RACB)                                  0468
         L     @15,AMBLDTA(,@15)                                   0468
         L     @15,AMBDSB(,@15)                                    0468
         SLR   @07,@07                                             0468
         IC    @07,DCBKEYLE+16(,RDCB)                              0468
         CH    @07,AMDKEYLN(,@15)                                  0468
         BNE   @GL00013                                            0468
         TM    DCBRECL+28(RDCB),B'10000000'                        0468
         BNO   @GL00016                                            0468
         TM    DCBRECL+28(RDCB),B'01000000'                        0468
         BNZ   @GL00016                                            0468
         CLC   AMDRKP(2,@15),DCBRKP+52(RDCB)                       0468
         BE    @GL00014                                            0468
@GL00016 TM    DCBRECL+28(RDCB),B'01000000'                        0468
         BNO   @GL00013                                            0468
         TM    DCBRECL+28(RDCB),B'10000000'                        0468
         BNZ   @GL00013                                            0468
         L     @15,ACBAMBL(,RACB)                                  0468
         L     @15,AMBLDTA(,@15)                                   0468
         L     @15,AMBDSB(,@15)                                    0468
         LH    @07,DCBRKP+52(,RDCB)                                0468
         SL    @07,@CF00199                                        0468
         CH    @07,AMDRKP(,@15)                                    0468
         BNE   @GL00013                                            0468
@GL00014 LA    @15,28                                              0468
         ALR   @15,RDCB                                            0468
         TM    DCBRECL(@15),B'10000000'                            0468
         BNO   @GL00019                                            0468
         TM    DCBRECL(@15),B'01000000'                            0468
         BNZ   @GL00019                                            0468
         TM    DCBRECBR(@15),B'00010000'                           0468
         BNZ   @GL00021                                            0468
         LA    @15,52                                              0468
         ALR   @15,RDCB                                            0468
         LH    @07,DCBRKP(,@15)                                    0468
         LTR   @07,@07                                             0468
         BNZ   @GL00021                                            0468
         L     @07,ACBAMBL(,RACB)                                  0468
         L     @07,AMBLDTA(,@07)                                   0468
         L     @07,AMBDSB(,@07)                                    0468
         SLR   @05,@05                                             0468
         IC    @05,DCBKEYLE+16(,RDCB)                              0468
         AH    @05,DCBLRECL(,@15)                                  0468
         C     @05,AMDLRECL(,@07)                                  0468
         BE    @RT00468                                            0468
@GL00021 LA    @15,52                                              0468
         ALR   @15,RDCB                                            0468
         LH    @07,DCBRKP(,@15)                                    0468
         LTR   @07,@07                                             0468
         BNP   @GL00019                                            0468
         L     @07,ACBAMBL(,RACB)                                  0468
         L     @07,AMBLDTA(,@07)                                   0468
         L     @07,AMBDSB(,@07)                                    0468
         L     @07,AMDLRECL(,@07)                                  0468
         CH    @07,DCBLRECL(,@15)                                  0468
         BE    @RT00468                                            0468
@GL00019 TM    DCBRECBR+28(RDCB),B'00010000'                       0468
         BNO   @GL00013                                            0468
         L     @15,ACBAMBL(,RACB)                                  0468
         L     @15,AMBLDTA(,@15)                                   0468
         L     @15,AMBDSB(,@15)                                    0468
         L     @15,AMDLRECL(,@15)                                  0468
         CH    @15,DCBLRECL+52(,RDCB)                              0468
         BE    @RT00468                                            0468
@GL00013 TM    DCBRECL+28(RDCB),B'01000000'                        0468
         BNO   @RF00468                                            0468
         TM    DCBRECL+28(RDCB),B'10000000'                        0468
         BNZ   @RF00468                                            0468
         L     @15,ACBAMBL(,RACB)                                  0468
         L     @15,AMBLDTA(,@15)                                   0468
         L     @15,AMBDSB(,@15)                                    0468
         LH    @07,DCBLRECL+52(,RDCB)                              0468
         SL    @07,@CF00199                                        0468
         C     @07,AMDLRECL(,@15)                                  0468
         BE    @RT00468                                            0468
*     GO TO ENDCHECK;               /* KEYLE, LRECL & RKP ARE OK     */
*   ELSE                                                           0470
*     DO;                                                          0470
@RF00468 DS    0H                                                  0471
*EXIT3B:                                                           0471
*       DO;                         /* ABEND(ABEND3B) DUMP SYSTEM    */
EXIT3B   DS    0H                                                  0472
*         RESPECIFY                                                0472
*          (GPR01F) RESTRICTED;                                    0472
*         GPR01F=((ABEND3B)&'00000FFF'X)*4096;/* COMP CODE IN BITS 0473
*                                      8-19                          */
         L     GPR01F,@CF02487                                     0473
*         GPR01F=GPR01F×'80000000'X;/* DUMP/STEP/DUMPOPTS FLAG       */
         O     GPR01F,@CF02428                                     0474
*         SVC(13);                  /* ISSUE ABEND SVC               */
         SVC   13                                                  0475
*         RESPECIFY                                                0476
*          (GPR01F) UNRESTRICTED;                                  0476
*       END;                        /* ABEND(ABEND3B) DUMP SYSTEM  0477
*                                      X'03B' - ISSUE ABEND  @Y021130*/
*     END;                                                         0478
*ENDCHECK:                                                         0479
*   END VALIDCHK;                                                  0479
@EL00011 DS    0H                                                  0479
@EF00011 DS    0H                                                  0479
@ER00011 BR    @14                                                 0479
*                                                                  0480
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = BUFFER MERGE                                          *
**       THIS ROUTINE MERGES THE BUFFER RELATED FIELDS TO THE DCB     *
**       FIELDS THAT ARE ZERO.                                        *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 6 - RACB - ACB POINTER                              *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         DCBFA                                                      *
**         DCBUFNO                                                    *
**         DCBBUFL                                                    *
**         AMBLNST   -->  DCBBUFNO                                    *
**         IIBUFL                                                     *
**         IIBUFNO                                                    *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0480
*BFRMERGE:                                                         0480
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0480
BFRMERGE DS    0H                                                  0481
*   IF DCBBFA=BZERO THEN            /* BFALN NOT IN DCB MACRO        */
         LA    @15,28                                              0481
         ALR   @15,RDCB                                            0481
         TM    DCBBFA(@15),B'00000011'                             0481
         BNZ   @RF00481                                            0481
*     DO;                                                          0482
*       MBFALN=ON;                  /* SET DEB MODIFICATION MASK     */
         OI    MBFALN(RIICB),B'10000000'                           0483
*       DCBBFA='10'B;               /* DEFAULT TO DOUBLEWORD         */
         NI    DCBBFA(@15),B'11111110'                             0484
         OI    DCBBFA(@15),B'00000010'                             0484
*     END;                                                         0485
*   IIBUFNO=CMBNST;                 /* II BUFFER NO = STRING NO    0486
*                                                            @Z30SSRL
*                                      IF QISAM                      */
@RF00481 L     @15,ACBAMBL(,RACB)                                  0486
         L     @15,AMBLCMB(,@15)                                   0486
         IC    @07,CMBNST(,@15)                                    0486
         STC   @07,IIBUFNO(,RIICB)                                 0486
*   IF DCBMFGET=ON×DCBMFPUT=ON THEN                                0487
         LA    @15,40                                              0487
         ALR   @15,RDCB                                            0487
         TM    DCBMFGET(@15),B'01000000'                           0487
         BO    @RT00487                                            0487
         TM    DCBMFPUT(@15),B'01000000'                           0487
         BNO   @RF00487                                            0487
@RT00487 DS    0H                                                  0488
*     DO;                                                          0488
*       IIBUFNO=ONE;                /* INIT II BUFFER NUMBER         */
         MVI   IIBUFNO(RIICB),X'01'                                0489
*       IF DCBBUFCA=ONE THEN        /* IF BUFFERS HAVE NOT BEEN      */
         LA    @15,16                                              0490
         ALR   @15,RDCB                                            0490
         CLC   DCBBUFCA(3,@15),@CF00087+1                          0490
         BNE   @RF00490                                            0490
*         DCBBUFNO=ONE;             /* SUPPLIED, SET BUFNO=ONE       */
         MVI   DCBBUFNO(@15),X'01'                                 0491
*     END;                                                         0492
@RF00490 DS    0H                                                  0493
*   IF DCBBUFNO=ZERO THEN           /* IF BUFNO NOT IN DCB MACRO     */
@RF00487 LA    @15,16                                              0493
         ALR   @15,RDCB                                            0493
         CLI   DCBBUFNO(@15),0                                     0493
         BNE   @RF00493                                            0493
*     DO;                                                          0494
*       DCBBUFNO=IIBUFNO;           /* SET BUFNO TO II BUFFER NO     */
         IC    @07,IIBUFNO(,RIICB)                                 0495
         STC   @07,DCBBUFNO(,@15)                                  0495
*       MBUFNO=ON;                  /* SET MODIFICATION MASK         */
         OI    MBUFNO(RIICB),B'00000010'                           0496
*     END;                                                         0497
*   IF AMDLRECL>DCBLRECL THEN       /* SET II BUFFER LENGTH TO       */
@RF00493 L     @15,ACBAMBL(,RACB)                                  0498
         L     @15,AMBLDTA(,@15)                                   0498
         L     @15,AMBDSB(,@15)                                    0498
         L     @15,AMDLRECL(,@15)                                  0498
         CH    @15,DCBLRECL+52(,RDCB)                              0498
         BNH   @RF00498                                            0498
*     IIBUFL=AMDLRECL;              /* THE GREATER VALUE OF          */
         STH   @15,IIBUFL(,RIICB)                                  0499
*   ELSE                                                           0500
*     IIBUFL=DCBLRECL;              /* AMDLRECL OR DCBLRECL          */
         B     @RC00498                                            0500
@RF00498 LH    @15,DCBLRECL+52(,RDCB)                              0500
         STH   @15,IIBUFL(,RIICB)                                  0500
*   IIMAXLRL=IIBUFL;                /* SAVE MAXIMUM LOGICAL REC    0501
*                                      LENGTH                @Y021130*/
@RC00498 LH    @15,IIBUFL(,RIICB)                                  0501
         STH   @15,IIMAXLRL(,RIICB)                                0501
*   RESPECIFY                                                      0502
*     RWORK2 RESTRICTED;                                           0502
*   RWORK2=IIBUFL;                  /* RECORD LENGTH         @YM05335*/
         LR    RWORK2,@15                                          0503
*   RWORK2=((RWORK2+7)/8)*8;        /* ROUND TO MULTIPLE OF 8      0504
*                                                            @YM05335*/
         LA    @15,8                                               0504
         LA    @08,7                                               0504
         ALR   @08,RWORK2                                          0504
         SRDA  @08,32                                              0504
         DR    @08,@15                                             0504
         SLA   @09,3                                               0504
         LR    RWORK2,@09                                          0504
*   IIBUFL=RWORK2;                  /* BUFFER LENGTH         @YM05335*/
         STH   RWORK2,IIBUFL(,RIICB)                               0505
*   RESPECIFY                                                      0506
*     RWORK2 UNRESTRICTED;                                         0506
*   IF DCBBFA='01'B THEN            /* IF FULLWORD ALIGNMENT         */
         TM    DCBBFA+28(RDCB),B'00000001'                         0507
         BNO   @RF00507                                            0507
         TM    DCBBFA+28(RDCB),B'00000010'                         0507
         BNZ   @RF00507                                            0507
*     IIBUFL=IIBUFL+8;              /* ADD 8 BYTES                   */
         LH    @07,IIBUFL(,RIICB)                                  0508
         ALR   @07,@15                                             0508
         STH   @07,IIBUFL(,RIICB)                                  0508
*   IF DCBMFRD=ON×                  /* IF BISAM              @YM05335*/
*       DCBMFWRT=ON THEN            /* THEN ADD 16 BYTES     @YM05335*/
@RF00507 LA    @15,40                                              0509
         ALR   @15,RDCB                                            0509
         TM    DCBMFRD(@15),B'00100000'                            0509
         BO    @RT00509                                            0509
         TM    DCBMFWRT(@15),B'00100000'                           0509
         BNO   @RF00509                                            0509
@RT00509 DS    0H                                                  0510
*     IIBUFL=IIBUFL+16;             /* FOR PAD AND LINK      @YM05335*/
         LA    @15,16                                              0510
         AH    @15,IIBUFL(,RIICB)                                  0510
         STH   @15,IIBUFL(,RIICB)                                  0510
*   IF DCBBUFL=ZERO THEN            /* IF BUFL NOT IN DCB MACRO      */
@RF00509 LA    @15,16                                              0511
         ALR   @15,RDCB                                            0511
         LH    @07,DCBBUFL(,@15)                                   0511
         LTR   @07,@07                                             0511
         BNZ   @RF00511                                            0511
*     DO;                                                          0512
*       DCBBUFL=IIBUFL;             /* CALCULATE BUFFER LENGTH       */
         LH    @07,IIBUFL(,RIICB)                                  0513
         STH   @07,DCBBUFL(,@15)                                   0513
*       MBUFL=ON;                   /* SET DEB MODIFICATION MASK     */
         OI    MBUFL(RIICB),B'00000100'                            0514
*     END;                                                         0515
*   END BFRMERGE;                                                  0516
@EL00012 DS    0H                                                  0516
@EF00012 DS    0H                                                  0516
@ER00012 BR    @14                                                 0516
*                                                                  0517
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = BUILD DEB                                             *
**       THIS ROUTINE BUILDS AN ISAM INTERFACE DUMMY DEB.  IT IS      *
**       CHAINED TO THE CURRENT TCB AND THE ISAM DCB.  THE DEB        *
**       POINTS TO THE IICB TO LINK THE ISAM AND VSAM BLOCKS.         *
**       THE PURPOSE OF THIS DEB IS TO SUPPORT ISAM PROBLEM PROGRAM   *
**       REFERENCES, CHECKPOINT/RESTART, SUPERVISOR TASK              *
**       TERMINATION AND ABEND.                                       *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**       REGISTER 11 - RDEB - DEB POINTER                             *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         DCBDEBAD (USER DCB)                                        *
**         DCBDEVT (USER DCB)                                         *
**         DCBDEBAD (COPY DCB - FIELD DXPDEBAD FORMERLY USED)         *
**         TCBDEB                                                     *
**                                                                    *
**         DEB                                                        *
**         SECTION               BYTES      FIELDS                    *
**         APPEND                 20                          @OX02677*
**         PREFIX                 16        LNGTH, XTNP               *
**         BASIC                  32        TCBAD, AMLNG, DEBAD, OFLGS*
**                                          OPATB, FLGS1, NMEXT, PRIOR*
**                                          PROTG, DEBID, DCBAD, EXSCL*
**                                          APPAD                     *
**         ISAM DEVICE            16        EXPTR, FPEAD              *
**         DIRECT ACCESS          16        UCBAD                     *
**         ISAM ACCESS METHOD     28        WKPT5                     *
**         EXTENSION                        XDBPR                     *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = DATA MANAGEMENT KEY 5                 *
**                              SYSTEM KEY 0                          *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0517
*BUILDDEB:                                                         0517
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0517
BUILDDEB DS    0H                                                  0518
*   RESPECIFY                                                      0518
*    (RWORK2,                                                      0518
*     JFCBPTR,                                                     0518
*     RLKRETSV,                                                    0518
*     RTIOT,                                                       0518
*     RPARC) RESTRICTED;                                           0518
*   RLKRETSV=R14;                   /* SAVE RETURN ADDR      @Y021130
*                                      LENGTH OF DUMMY DEB           */
         LR    RLKRETSV,R14                                        0519
*   RWORK2=(LENGTH(DEB)+            /* APPEND, PREFIX,       @OZ02677*/
*       LENGTH(DEBBASIC)+           /* BASIC,                @Y021130*/
*       LENGTH(DEBISAM)+            /* ISAM DEPENDENT SECTION,       */
*       LENGTH(DEBDASD)+            /* ONE DUMMY EXTENT,             */
*       LENGTH(DEBACS08));          /* AND BISAM EXTENSION           */
         LA    RWORK2,128                                          0520
*   RESPECIFY                                                      0521
*    (RGMLEN) RESTRICTED;           /*                       @Y021130*/
*   RGMLEN=((RWORK2+7)/8)*8;        /* ROUND TO A DOUBLE WORD      0522
*                                                            @Y021130*/
         LA    @08,7                                               0522
         ALR   @08,RWORK2                                          0522
         SRDA  @08,32                                              0522
         D     @08,@CF00045                                        0522
         SLA   RGMLEN,3                                            0522
*   RESPECIFY                                                      0523
*    (RGMSP) RESTRICTED;            /*                       @Y021130*/
*   RGMSP=DEBPOOL;                  /* INIT SUBPOOL NUMBER   @Y021130*/
         LA    RGMSP,230                                           0524
*   CALL GETMAIN;                   /* GET CORE FOR DEB AND ZERO     */
         BAL   @14,GETMAIN                                         0525
*   RESPECIFY                                                      0526
*    (RWORK2,                                                      0526
*     RGMSP,                                                       0526
*     RGMLEN,                                                      0526
*     RLKRETSV) UNRESTRICTED;                                      0526
*   RESPECIFY                                                      0527
*     RDEB RESTRICTED;                                             0527
*   RDEB=MAINADDR;                  /* POINTER TO OBTAINED CORE      */
*                                                                  0528
         L     RDEB,MAINADDR(,RCORE)                               0528
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE ISAM INTERFACE DUMMY DEB                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0529
*   RDEB=RDEB+LENGTH(DEB);          /* BASIC SECTION PTR     @OZ02677*/
         LA    @15,36                                              0529
         ALR   RDEB,@15                                            0529
*   DCBDEBA=RDEB;                   /* INIT DCB PTR TO DEB   @Y021130*/
         STCM  RDEB,7,DCBDEBA+40(RDCB)                             0530
*   RTIOT=DXTIOTAD;                 /* LOCATE DD ENTRY IN OPEN WORK
*                                      AREA                  @Y021130*/
         L     RTIOT,DXTIOTAD(,RCORE)                              0531
*   DCBDEVT=UCBTYP;                 /* DCB DEV TYPE          @Y021130*/
         LA    @10,16                                              0532
         LR    @08,RDCB                                            0532
         ALR   @08,@10                                             0532
         L     @05,TIOEFSRT-1(,RTIOT)                              0532
         LA    @05,0(,@05)                                         0532
         MVC   DCBDEVT(1,@08),UCBTYP(@05)                          0532
*   DO;                             /* MODESET EXTKEY(DATAMGT)       */
*     SPKA(80);                     /* SET PSW KEY                   */
         LA    @08,80                                              0534
         SPKA  0(@08)                                              0534
*   END;                            /* MODESET EXTKEY(DATAMGT) DATA
*                                      MANAGEMENT KEY FOR DEB      0535
*                                                            @Y021130*/
*   DEBUCBA=TIOEFSRT;               /* VSAM UCB PTR          @Y021130*/
         LA    @08,DEBISAM(,RDEB)                                  0536
         ALR   @08,@10                                             0536
         STCM  @05,7,DEBUCBA(@08)                                  0536
*   RESPECIFY                                                      0537
*    (RTIOT) UNRESTRICTED;          /*                       @Y021130*/
*   DEBAPPB=RDEB-LENGTH(DEB);       /* ADDRESSABILITY FOR LNGTH      */
         LCR   @15,@15                                             0538
         ALR   @15,RDEB                                            0538
         STCM  @15,7,DEBAPPB(RDEB)                                 0538
*   DEBLNGTH=MAINLEN/EIGHT;         /* DOUBLEWORK LENGTH IN DEB      */
         L     @10,MAINLEN-1(,RCORE)                               0539
         LA    @10,0(,@10)                                         0539
         SRL   @10,3                                               0539
         STC   @10,DEBLNGTH(,@15)                                  0539
*   DEBTCBB=TCBPTR;                 /* INIT DEB PTR TO TCB           */
         L     @15,CVTPTR                                          0540
         L     @15,CVTTCBDW(,@15)                                  0540
         MVC   DEBTCBB(3,RDEB),TCBPTR+1(@15)                       0540
*   DEBAMLNG=LENGTH(DEBACS08);      /* ACCESS METHOD SECTION LEN     */
         MVI   DEBAMLNG(RDEB),X'1C'                                0541
*   JFCBPTR=ADDR(DXJBF);            /* INIT JFCB POINTER     @Y021130*/
         LA    JFCBPTR,DXJBF(,RCORE)                               0542
*   DEBOFLGS='11'B;                 /* DISP BITS TO BE MAPPED        */
         MVI   DEBOFLGS(RDEB),B'11000000'                          0543
*   DEBOFLGS=DEBOFLGS&JFCBIND2;     /* INSERT DISP FROM THE JFCB     */
         NC    DEBOFLGS(1,RDEB),JFCBIND2(JFCBPTR)                  0544
*   RESPECIFY                                                      0545
*     JFCBPTR UNRESTRICTED;                                        0545
*   R13=IIRSAVE;                    /* RESTORE SAVE AREA POINTER     */
         L     R13,IIRSAVE(,RCORE)                                 0546
*   GEN(L RPARC,48(R13));           /* RESTORE RPARC                 */
         L RPARC,48(R13)
*   DEBOPATB=PARCOPT;               /* MOVE OPTION BYTE FROM I/O   0548
*                                      (REFERENCED BY COBOL)         */
         MVC   DEBOPATB(1,RDEB),PARCOPT(RPARC)                     0548
*   RESPECIFY                                                      0549
*     RPARC UNRESTRICTED;           /* PARM LIST-HIGH ORDER BIT      */
*   DEBABEND=OFF;                   /* TO 0 TO INIT DEBACCS          */
         NI    DEBABEND(RDEB),B'01111111'                          0550
*   DEBCINDI=ON;                    /* TURN ON INTERFACE (CI) BIT    */
*   DEBXTNIN=ON;                    /* SHOW EXTSN EXISTS     @OZ18535*/
         OI    DEBCINDI(RDEB),B'00001001'                          0552
*   DEBNMEXT=2;                     /* TWO EXTENTS PRESENT           */
         MVI   DEBNMEXT(RDEB),X'02'                                0553
*   DEBPRIOR=TCBDSP;                /* MOVE PRIORITY FROM TCB        */
         L     @15,CVTPTR                                          0554
         L     @15,CVTTCBDW(,@15)                                  0554
         L     @15,TCBPTR(,@15)                                    0554
         IC    @10,TCBDSP(,@15)                                    0554
         STC   @10,DEBPRIOR(,RDEB)                                 0554
*   DEBDCBB=RDCB;                   /* INIT DEB DCB POINTER          */
         STCM  RDCB,7,DEBDCBB(RDEB)                                0555
*   DEBPROTG=DXUKEY×DEBID;          /* TCB PROTECT KEY/DEB ID        */
         MVC   DEBPROTG(1,RDEB),DXUKEY(RCORE)                      0556
         OC    DEBPROTG(1,RDEB),DEBID                              0556
*   DEBEXSCL=4;                     /* DA EXTENT SCALE       @OZ02677*/
         MVI   DEBEXSCL(RDEB),X'04'                                0557
*   DEBEXPTA=RDEB+                  /* ADDR OF ISAM AM SECTION       */
*       (LENGTH(DEBBASIC))+LENGTH(DEBISAM)+LENGTH(DEBDASD);/*      0558
*                                                            @Y021130*/
         LA    @10,64                                              0558
         ALR   @10,RDEB                                            0558
         STCM  @10,7,DEBEXPTA(RDEB)                                0558
*   DEBFPEB=ADDR(DEBDASD);          /* SUPPORT PP TO GET TO UCB      */
         LA    @10,DEBISAM(,RDEB)                                  0559
         AL    @10,@CF00210                                        0559
         STCM  @10,7,DEBFPEB(RDEB)                                 0559
*   DEBWKPT5=RIICB;                 /* ADDR OF IICB                  */
         L     @10,DEBEXPT(,RDEB)                                  0560
         ST    RIICB,DEBWKPT5(,@10)                                0560
*   DEBXTNP=DXDEBXAD;               /* CONNECT DEB EXTENSION TO DEB
*                                      USING PTR IN OPEN WORK AREA 0561
*                                                            @Y021130*/
         L     @10,DXDEBXAD(,RCORE)                                0561
         LR    @07,RDEB                                            0561
         AL    @07,@CF02508                                        0561
         ST    @10,DEBXTNP-28(,@07)                                0561
*   DEBXDBPR=RDEB;                  /* POINT DEB EXTENSION TO BASIC
*                                      DEB                   @Y021130*/
*                                                                  0562
         ST    RDEB,DEBXDBPR(,@10)                                 0562
*   /*****************************************************************/
*   /*                                                               */
*   /* CHAIN THE DEB                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0563
*   DO;                             /* MODESET EXTKEY(ZERO)          */
*     SPKA(0);                      /* SET PSW KEY                   */
         SLR   @10,@10                                             0564
         SPKA  0(@10)                                              0564
*   END;                            /* MODESET EXTKEY(ZERO) ASSUME 0565
*                                      KEY ZERO FOR STORE IN TCB   0565
*                                                            @Y021130*/
*   RESPECIFY                                                      0566
*    (RWORK2,                                                      0566
*     R10) RESTRICTED;              /*                       @Y021130*/
*   R10=TCBPTR;                     /* GET ADDRESSIBILITY ON TCB   0567
*                                                            @Y021130*/
         LR    R10,@15                                             0567
*   RESPECIFY                                                      0568
*    (TCB) BASED(R10);              /* HOLD IN REG DOING DEB CHAINING
*                                                            @Y021130*/
*   RWORK2=TCBDEB;                  /* PREPARE FOR COMPARE AND SWAP
*                                      FOR ADDING DEB TO CHAIN     0569
*                                                            @Y021130*/
         L     RWORK2,TCBDEB(,R10)                                 0569
*CSRETRY:                                                          0570
*   DEBDEBB=RWORK2;                 /* UPDATE OUR DEB TO POINT TO  0570
*                                      PREVIOUS              @Y021130*/
CSRETRY  STCM  RWORK2,7,DEBDEBB(RDEB)                              0570
*   CS(RWORK2,RDEB,TCBDEB);         /* ADD OUR DEB TO CHAIN  @Y021130*/
         CS    RWORK2,@11,TCBDEB(R10)                              0571
*   BC(7,CSRETRY);                  /* LOOP UNTIL SUCCESSFUL @Y021130*/
         BC    7,CSRETRY                                           0572
*   DO;                             /* MODESET EXTKEY(DATAMGT)       */
*     SPKA(80);                     /* SET PSW KEY                   */
         LA    @15,80                                              0574
         SPKA  0(@15)                                              0574
*   END;                            /* MODESET EXTKEY(DATAMGT) DATA
*                                      MANAGEMENT KEY FOR DEB      0575
*                                                            @Y021130*/
*   RESPECIFY                                                      0576
*    (RWORK2,                                                      0576
*     R10) UNRESTRICTED;            /*                       @Y021130*/
*   RESPECIFY                                                      0577
*    (TCB) BASED(TCBPTR);           /* RESET ORIGINAL        @Y021130*/
*   RDCB=DXUDCBAD;                  /* RESTORE DCB PTR       @Y021130
*                                      ISAM ACCESS METHOD TYPE NEEDED
*                                      FOR FREEDBUF DEBCHK           */
         L     RDCB,DXUDCBAD(,RCORE)                               0578
*   GEN(DEBCHK (1),TYPE=ADD,AM=ISAM);                              0579
         DEBCHK (1),TYPE=ADD,AM=ISAM
*   RESPECIFY                                                      0580
*     IHADCB BASED(DXPDCBAD);       /* USE COPY DCB          @Y021130*/
*   DCBDEBA=R1;                     /* SAVE DEB ADDR IN DCB COPY   0581
*                                                            @Y021130*/
         L     @15,DXPDCBAD(,RCORE)                                0581
         STCM  R1,7,DCBDEBA+40(@15)                                0581
*   RESPECIFY                                                      0582
*     IHADCB BASED(RDCB);           /* USER DCB              @Y021130*/
*   RDCB=DXUDCBAD;                  /* RESTORE DCB PTR       @Y021130*/
         L     RDCB,DXUDCBAD(,RCORE)                               0583
*   R14=IIRSAVE1;                   /* RESTORE RETURN ADDRESS        */
         L     R14,IIRSAVE1(,RCORE)                                0584
*   DO;                             /* MODESET KEYADDR(DXUKEY)       */
*     SPKA(DXUKEY);                 /* SET PSW KEY                   */
         SLR   @15,@15                                             0586
         IC    @15,DXUKEY(,RCORE)                                  0586
         SPKA  0(@15)                                              0586
*   END;                            /* MODESET KEYADDR(DXUKEY)     0587
*                                      RESTORE USER KEY      @Y021130*/
*   END BUILDDEB;                                                  0588
@EL00013 DS    0H                                                  0588
@EF00013 DS    0H                                                  0588
@ER00013 BR    @14                                                 0588
*                                                                  0589
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = LOAD MODULE                                           *
**       DETERMINE THE ISAM INTERFACE MODULES THAT NEED TO BE         *
**       LOADED AND INITIALIZE THE APPROPRIATE DCB AND DEB VECTOR     *
**       POINTERS.  THE ISAM SYNAD ROUTINE IS LOADED IF IT WAS        *
**       SPECIFIED IN THE AMP PARAMETER.                              *
