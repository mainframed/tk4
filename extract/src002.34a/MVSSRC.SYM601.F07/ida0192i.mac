         NI    AUDPRMOD(RIICB),B'11111011'                         0621
         OI    AUDPRMOD(RIICB),B'00001000'                         0621
*       RESPECIFY                                                  0622
*         R8 RESTRICTED;            /* DCBGET + 4 - PUTX DCBGET + 8 -
*                                      RELSE                         */
*       R8=R0;                                                     0623
         LR    R8,R0                                               0623
