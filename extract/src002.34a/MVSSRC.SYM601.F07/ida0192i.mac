         OI    AUDURSYN(RIICB),B'00000001'                         0673
*   DCBSYNAD=R0;                    /* INIT SYNAD ENTRY PT - DCB   0674
*                                                            @YM04603*/
         ST    R0,DCBSYNAD+52(,RWORK2)                             0674
*   R14=R9;                         /* RETURN TO SVC         @YM04603*/
         LR    R14,R9                                              0675
