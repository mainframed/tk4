         NI    AUDPRMOD(RIICB),B'11110111'                         0639
         OI    AUDPRMOD(RIICB),B'00000100'                         0639
*         DCBIIPUT=R0;              /* II PUT ENTRY                  */
         STCM  R0,7,DCBIIPUT+48(RWORK2)                            0640
*       END;                        /* LOAD II SYNAD MODULE          */
*   R1=CVTSVDCB;                    /* PT TO DCB FOR SYS1.SVCLIB     */
@RF00635 DS    0H                                                  0642
@RC00617 L     @15,CVTPTR                                          0642
         L     R1,CVTSVDCB(,@15)                                   0642
*   GEN(LOAD EP=IDAIISM1,DCB=(1));                                 0643
*                                                                  0643
         LOAD EP=IDAIISM1,DCB=(1)
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT THAT MODULE IDAIISM1 HAS BEEN LOADED SUCCESSFULLY       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0644
*   AUDIISYN=ON;                    /* SET FLAG IN IICB      @Y021130*/
         OI    AUDIISYN(RIICB),B'00000010'                         0644
*   EXLSYNP=R0;                     /* SYNAD ENTRY-ACB EXLST         */
         L     @15,ACBEXLST(,RACB)                                 0645
         STCM  R0,15,EXLSYNP(@15)                                  0645
*   EXLSYNF(1)=ON;                  /* MARK SYNAD PRESENT            */
*   EXLSYNF(2)=ON;                  /* MARK SYNAD ACTIVE             */
         OI    EXLSYNF(@15),B'11000000'                            0647
*   EXLLERP=R0;                     /* LERAD ENTRY-ACB EXLST         */
         STCM  R0,15,EXLLERP(@15)                                  0648
*   EXLLERF(1)=ON;                  /* MARK LERAD PRESENT            */
*   EXLLERF(2)=ON;                  /* MARK LERAD ACTIVE             */
         OI    EXLLERF(@15),B'11000000'                            0650
*   IISVCLST=R0+4;                  /* SYNADAF ENTRY                 */
         LA    @15,4                                               0651
         ALR   @15,R0                                              0651
         ST    @15,IISVCLST(,RIICB)                                0651
*   DCBSVCXA=ADDR(IISVCLST);        /* PTR TO SVC EXIT LIST          */
*                                                                  0652
         LA    @15,IISVCLST(,RIICB)                                0652
         STCM  @15,7,DCBSVCXA+28(RWORK2)                           0652
*   /*****************************************************************/
*   /*                                                               */
*   /* LOAD THE ISAM SYNAD ROUTINE WHEN IT IS SPECIFIED IN THE AMP   */
*   /* PARAMETER                                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0653
*   RESPECIFY                                                      0653
*     JFCBPTR RESTRICTED;                                          0653
*   JFCBPTR=ADDR(DXJBF);            /* INIT PTR TO JFCB      @Y021130*/
         LA    JFCBPTR,DXJBF(,RCORE)                               0654
*   IF JFCAMSYN(1:4)^=ZERO THEN     /* IF SYNAD SPECIFIED IN AMP     */
         ICM   @15,15,JFCAMSYN(JFCBPTR)                            0655
         BZ    @RF00655                                            0655
*     DO;                           /* YES                           */
*       RESPECIFY                                                  0657
*        (R15) RESTRICTED;                                         0657
*       IISAMSYN=JFCAMSYN;          /* SAVE ISAM SYNAD NAME          */
         MVC   IISAMSYN(8,RIICB),JFCAMSYN(JFCBPTR)                 0658
*       R15=ADDR(SYNCHRTN);         /* ADDR TO SYNCH TO IN ORDER TO
*                                      LOAD FROM JOBLIB      @YM04603*/
         LA    R15,SYNCHRTN                                        0659
*       GEN(SYNCH (R15));           /* ENTER PROBLEM PROGRAM STATE 0660
*                                                            @YM04603*/
*                                                                  0660
         SYNCH (R15)
*       /*************************************************************/
*       /*                                                           */
*       /* RETURN FROM LOADING USER SPECIFIED SYNAD ROUTINE          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0661
*       DO;                         /* MODESET KEYADDR(DXUKEY)       */
*         SPKA(DXUKEY);             /* SET PSW KEY                   */
         SLR   @08,@08                                             0662
         IC    @08,DXUKEY(,RCORE)                                  0662
         SPKA  0(@08)                                              0662
*       END;                        /* MODESET KEYADDR(DXUKEY) INSURE
*                                      USER KEY              @YM04603*/
*       RESPECIFY                                                  0664
*        (R15) UNRESTRICTED;                                       0664
*     END;                                                         0665
*   RDCB=RWORK2;                    /* RESTORE DCB POINTER           */
@RF00655 LR    RDCB,RWORK2                                         0666
*   R14=RLKRETSV;                   /* RESTORE RET ADDR      @Y021130*/
         LR    R14,RLKRETSV                                        0667
*   RETURN;                         /* RETURN TO MAINLINE    @YM04603*/
*                                                                  0668
@EL00014 DS    0H                                                  0668
@EF00014 DS    0H                                                  0668
@ER00014 BR    @14                                                 0668
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FOLLOWING LABEL IS USED WHEN IT IS NECESSARY TO LOAD A    */
