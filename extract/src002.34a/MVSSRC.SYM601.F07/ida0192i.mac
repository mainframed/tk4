*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = BUILD RPL                                             *
**       THIS ROUTINE COMPUTES SIZES AND GETS CORE IN THE USERS       *
**       REGION FOR THE ISAM INTERFACE RPLS, RPL EXTENSIONS, ERROR    *
**       MESSAGE AREAS, KEYSAVE AREA AND ISAM INTERFACE BUFFERS.      *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 3 - RRPL - RPL POINTER                              *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         IIKEYSL                                                    *
**         IIRPLL                                                     *
**         IIRPLPTR                                                   *
**         IITBUFL                                                    *
**         IIMSGL                                                     *
**         IIMSGPTR                                                   *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0679
*BLDRPL:                                                           0679
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0679
BLDRPL   DS    0H                                                  0680
*   RESPECIFY                                                      0680
*     RRPL RESTRICTED;                                             0680
*                                                                  0680
*   /*****************************************************************/
*   /*                                                               */
*   /* IIBUFL AND IIBUFNO ARE SET IN THE BFRMERGE ROUTINE. IIBUFNO = */
*   /* STRING NUMBER FOR BISAM, AND IS = ONE FOR QISAM. FINALLY,     */
*   /* COMPUTE RPL AND RPLE SIZE.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0681
*   IIRPLL=(LENGTH(IFGRPL)+         /* RPL LENGTH                    */
*       LENGTH(IDARPLE))            /* RPL EXTENSION LENGTH          */
*       *IIBUFNO;                   /* TIMES BUFFER NO               */
*                                                                  0681
         SLR   @15,@15                                             0681
         IC    @15,IIBUFNO(,RIICB)                                 0681
         LR    @10,@15                                             0681
         MH    @10,@CH02106                                        0681
         STCM  @10,7,IIRPLL(RIICB)                                 0681
*   /*****************************************************************/
*   /*                                                               */
*   /* COMPUTE II BUFFER SIZE                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0682
*   RESPECIFY                                                      0682
*     RWORK2 RESTRICTED;                                           0682
*   RWORK2=IIBUFL;                  /* LRECL FOR BUFFERS TOTAL CORE
*                                      FOR BUFFERS                   */
         LH    RWORK2,IIBUFL(,RIICB)                               0683
*   RWORK2=(RWORK2*IIBUFNO)+8;      /* LRECL*BUFNO+BCB LENGTH        */
         LA    @10,8                                               0684
         LR    @09,@15                                             0684
         MR    @08,RWORK2                                          0684
         ALR   @09,@10                                             0684
         LR    RWORK2,@09                                          0684
*   IITBUFL=RWORK2;                 /* SAVE BUFFER LENGTH            */
*                                                                  0685
         STCM  RWORK2,7,IITBUFL(RIICB)                             0685
*   /*****************************************************************/
*   /*                                                               */
*   /* COMPUTE KEYSAVE AREA SIZE FOR QISAM PUT LOCATE REQUEST        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0686
*   IF DCBMFLCP=ON THEN             /* IF PUT LOCATE THEN KEYSAVE  0686
*                                      AREA NEEDED CALCULATE LENGTH
*                                      NEEDED                        */
         TM    DCBMFLCP+40(RDCB),B'00001000'                       0686
         BNO   @RF00686                                            0686
*     IIKEYSL=((DCBKEYLE+7)/8)*8;                                  0687
*                                                                  0687
         LA    @08,7                                               0687
         SLR   @15,@15                                             0687
         IC    @15,DCBKEYLE+16(,RDCB)                              0687
         ALR   @08,@15                                             0687
         SRDA  @08,32                                              0687
         DR    @08,@10                                             0687
         SLA   @09,3                                               0687
         STH   @09,IIKEYSL(,RIICB)                                 0687
*   /*****************************************************************/
*   /*                                                               */
*   /* GET CORE FOR RPL'S, BUFFERS AND KEYSAVE                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0688
*   RESPECIFY                                                      0688
*    (RGMLEN) RESTRICTED;           /*                       @Y021130*/
@RF00686 DS    0H                                                  0689
*   RGMLEN=IIKEYSL+                 /* KEYSAVE LENGTH PLUS           */
*       IITBUFL+                    /* BUFFER LENGTH PLUS            */
*       IIRPLL;                     /* RPL LENGTH FOR GETMAIN      0689
*                                                            @Y021130*/
         L     RGMLEN,IITBUFL-1(,RIICB)                            0689
         LA    RGMLEN,0(,RGMLEN)                                   0689
         AH    RGMLEN,IIKEYSL(,RIICB)                              0689
         L     @15,IIRPLL-1(,RIICB)                                0689
         LA    @15,0(,@15)                                         0689
         ALR   RGMLEN,@15                                          0689
*   RESPECIFY                                                      0690
*    (RGMSP) RESTRICTED;            /*                       @Y021130*/
*   RGMSP=USERPOOL;                 /* USER REGION SUBPOOL   @Y021130*/
         LA    RGMSP,250                                           0691
*   RESPECIFY                                                      0692
*    (RLKRETSV) RESTRICTED;         /*                       @Y021130*/
*   RLKRETSV=R14;                   /* SAVE LINK REG         @Y021130*/
         LR    RLKRETSV,R14                                        0693
*   CALL GETMAIN;                   /* GET CORE FOR RPL'S            */
         BAL   @14,GETMAIN                                         0694
*   RESPECIFY                                                      0695
*    (RGMSP,                                                       0695
*     RGMLEN,                                                      0695
*     RLKRETSV) UNRESTRICTED;       /*                       @Y021130*/
*   RRPL=MAINADDR;                  /* INIT RPL PTR                  */
         L     RRPL,MAINADDR(,RCORE)                               0696
*   IIRPLPTR=RRPL;                  /* SAVE PTR TO 1ST RPL           */
*                                                                  0697
         ST    RRPL,IIRPLPTR(,RIICB)                               0697
*   /*****************************************************************/
*   /*                                                               */
*   /* COMPUTE THE SIZE AND GET CORE FOR ERROR MESSAGE AREAS         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0698
*   RWORK2=LENGTH(IDAERMSG);        /* INIT ERROR MSG LENGTH FOR   0698
*                                      GETMAIN/FREEMAIN - ROUNDED TO
*                                      DOUBLE WORD TIMES BUFFER NO   */
         LA    RWORK2,128                                          0698
*   IIMSGL=((RWORK2+7)/8)*8*IIBUFNO;                               0699
         LA    @08,7                                               0699
         ALR   @08,RWORK2                                          0699
         SRDA  @08,32                                              0699
         D     @08,@CF00045                                        0699
         SLA   @09,3                                               0699
         SLR   @15,@15                                             0699
         IC    @15,IIBUFNO(,RIICB)                                 0699
         MR    @08,@15                                             0699
         STCM  @09,7,IIMSGL(RIICB)                                 0699
*   RESPECIFY                                                      0700
*     RWORK2 UNRESTRICTED;                                         0700
*   RESPECIFY                                                      0701
*    (RGMLEN) RESTRICTED;           /*                       @Y021130*/
*   RGMLEN=IIMSGL;                  /* INIT LENGTH FOR GETMAIN     0702
*                                                            @Y021130*/
         L     @15,IIMSGL-1(,RIICB)                                0702
         LA    @15,0(,@15)                                         0702
         LR    RGMLEN,@15                                          0702
*   RESPECIFY                                                      0703
*    (RGMSP) RESTRICTED;            /*                       @Y021130*/
*   RGMSP=USERPOOL;                 /* USER REGION SUBPOOL   @Y021130*/
         LA    RGMSP,250                                           0704
*   RESPECIFY                                                      0705
*    (RLKRETSV) RESTRICTED;         /*                       @Y021130*/
*   RLKRETSV=IIRSAVE1;              /* PREVIOUSLY SAVED LINK REG   0706
*                                                            @Y021130*/
         L     RLKRETSV,IIRSAVE1(,RCORE)                           0706
*   CALL GETMAIN;                   /* GET CORE FOR ERROR MSG        */
         BAL   @14,GETMAIN                                         0707
*   RESPECIFY                                                      0708
*    (RGMSP,                                                       0708
*     RGMLEN,                                                      0708
*     RLKRETSV) UNRESTRICTED;       /*                       @Y021130*/
*   IIMSGPTR=MAINADDR;              /* SAVE MSG AREA PTR             */
         L     @15,MAINADDR(,RCORE)                                0709
         ST    @15,IIMSGPTR(,RIICB)                                0709
*   R14=IIRSAVE1;                   /* RESTORE RETURN ADDRESS        */
         L     R14,IIRSAVE1(,RCORE)                                0710
*   END BLDRPL;                                                    0711
@EL00015 DS    0H                                                  0711
@EF00015 DS    0H                                                  0711
@ER00015 BR    @14                                                 0711
*                                                                  0712
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = INITIALIZE RPL                                        *
**       INITIALIZE THE ISAM INTERFACE RPL, RPL EXTENSION AND         *
**       ERROR MESSAGE AREAS.  THE RPL EXTENSION IS LOCATED           *
**       IMMEDIATELY AFTER EACH RPL.  THE FIRST RPL IS POINTED        *
**       TO BY THE IICB.  THE REST ARE CHAINED TOGETHER IN THE        *
**       RPL EXTENSION.  THE RPL EXTENSION IS ALSO USED BY THE ISAM   *
**       INTERFACE TO POINT TO THE IICB, DECB AND THE II BUFFER.      *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 3 - RRPL - RPL POINTER                              *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**       REGISTER 6 - RACB - ACB POINTER                              *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         RPLBUFL                                                    *
**         RPLDACB                                                    *
**         RPLEMLEN                                                   *
**         RPLERMSA                                                   *
**         RPLID                                                      *
**         RPLIICB                                                    *
**         RPLKEY                                                     *
**         RPLKEYLE                                                   *
**         RPLLEN                                                     *
**         RPLOPTCD                                                   *
**         RPLRLEN                                                    *
**         RPLRPLPT                                                   *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0712
*INITRPL:                                                          0712
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0712
INITRPL  DS    0H                                                  0713
*   RESPECIFY                                                      0713
*     RWORK2 RESTRICTED;                                           0713
*   RWORK2=IIMSGPTR;                /* INIT MSG AREA PTR             */
         L     RWORK2,IIMSGPTR(,RIICB)                             0714
*   RESPECIFY                                                      0715
*    (R10) RESTRICTED;              /*                       @Y021130*/
*   DO R10=ONE BY ONE TO IIBUFNO;   /*                       @Y021130*/
         LA    R10,1                                               0716
         B     @DE00716                                            0716
@DL00716 DS    0H                                                  0717
*     RPLRPLPT=RRPL                 /* INIT PTR TO NEXT RPL          */
*         +LENGTH(IFGRPL)           /*                               */
*         +LENGTH(IDARPLE);         /*                               */
         LA    @15,104                                             0717
         ALR   @15,RRPL                                            0717
         ST    @15,RPLRPLPT+76(,RRPL)                              0717
*     RPLERMSA=RWORK2;              /* INIT RPL PTR TO MSG AREA      */
         ST    RWORK2,RPLERMSA(,RRPL)                              0718
*     RPLID='00'X;                  /* SET RPL IDENTIFIER            */
         MVI   RPLID(RRPL),X'00'                                   0719
*     RPLLEN=LENGTH(IFGRPL);        /* SET RPL LENGTH                */
         MVI   RPLLEN(RRPL),X'4C'                                  0720
*     RPLKEY=ON;                    /* KEYED ACCESS                  */
         OI    RPLKEY(RRPL),B'10000000'                            0721
*     IF ACBSEQ=ON THEN                                            0722
         TM    ACBSEQ(RACB),B'00010000'                            0722
         BNO   @RF00722                                            0722
*       RPLSEQ=ON;                  /* SEQUENTIAL                    */
         OI    RPLSEQ(RRPL),B'00100000'                            0723
*     IF ACBDIR=ON THEN                                            0724
@RF00722 TM    ACBDIR(RACB),B'00001000'                            0724
         BNO   @RF00724                                            0724
*       RPLDIR=ON;                  /* DIRECT                        */
         OI    RPLDIR(RRPL),B'01000000'                            0725
*     IF DCBMFUIP=ON THEN                                          0726
@RF00724 TM    DCBMFUIP+40(RDCB),B'00000100'                       0726
         BNO   @RF00726                                            0726
*       RPLUPD=ON;                  /* UPDATE - SCAN                 */
         OI    RPLUPD(RRPL),B'00000010'                            0727
*     RPLKEYLE=AMDKEYLN;            /* KEY LENGTH                    */
@RF00726 L     @15,ACBAMBL(,RACB)                                  0728
         L     @15,AMBLDTA(,@15)                                   0728
         L     @15,AMBDSB(,@15)                                    0728
         LH    @05,AMDKEYLN(,@15)                                  0728
         STH   @05,RPLKEYLE(,RRPL)                                 0728
*     RPLDACB=IIACBPTR;             /* ACB PTR                       */
         L     @05,IIACBPTR(,RIICB)                                0729
         ST    @05,RPLDACB(,RRPL)                                  0729
*     RPLRLEN=AMDLRECL;             /* RECORD LENGTH                 */
         L     @15,AMDLRECL(,@15)                                  0730
         ST    @15,RPLRLEN(,RRPL)                                  0730
*     RPLBUFL=IIBUFL;               /* AREA LENGTH ERROR MSG       0731
*                                      LEN-ROUND DWORD               */
         LH    @15,IIBUFL(,RIICB)                                  0731
         ST    @15,RPLBUFL(,RRPL)                                  0731
*     RPLEMLEN=((LENGTH(IDAERMSG)+7)/8)*8;                         0732
         LA    @15,128                                             0732
         STH   @15,RPLEMLEN(,RRPL)                                 0732
*     RPLIICB=RIICB;                /* IICB PTR                      */
         LA    @05,76                                              0733
         ALR   @05,RRPL                                            0733
         ST    RIICB,RPLIICB(,@05)                                 0733
*     RWORK2=RWORK2+RPLEMLEN;       /* ADD ERROR MSG LENGTH          */
         ALR   RWORK2,@15                                          0734
*     RRPL=RPLRPLPT;                /* INIT BASE FOR NEXT RPL        */
         L     @15,RPLRPLPT(,@05)                                  0735
         LR    RRPL,@15                                            0735
*   END;                                                           0736
         AL    R10,@CF00087                                        0736
@DE00716 SLR   @15,@15                                             0736
         IC    @15,IIBUFNO(,RIICB)                                 0736
         CR    R10,@15                                             0736
         BNH   @DL00716                                            0736
*   RESPECIFY                                                      0737
*    (R10) UNRESTRICTED;            /*                       @Y021130*/
*   RRPL=RRPL-(LENGTH(IFGRPL)       /* BACK UP RPL PTR               */
*       +LENGTH(IDARPLE));                                         0738
         SL    RRPL,@CF02106                                       0738
*   RPLRPLPT=ZERO;                  /* CLEAR RPL CHAIN               */
         SLR   @15,@15                                             0739
         ST    @15,RPLRPLPT+76(,RRPL)                              0739
*   RESPECIFY                                                      0740
*    (RWORK2) UNRESTRICTED;                                        0740
*   END INITRPL;                                                   0741
@EL00016 DS    0H                                                  0741
@EF00016 DS    0H                                                  0741
@ER00016 BR    @14                                                 0741
*                                                                  0742
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = BUILD BUFFERS                                         *
**                                                                    *
**       THIS ROUTINE INITIALIZES THE POINTERS TO THE ISAM            *
**       INTERFACE BUFFERS AND KEYSAVE AREA.  THE ISAM INTERFACE      *
**       BUILDS ONE BUFFER FOR QISAM AND USES STRING NUMBER TO        *
**       BUILD BUFFERS FOR BISAM.  THE ISAM INTERFACE BUFFERS ARE     *
**       NEEDED FOR LOCATE MODE, DYNAMIC BUFFERING, SETL WITH KEY,    *
**       STAND ALONE WRITE (ADD), AND FOR FIXED, UNVLOCKED RECORDS    *
**       WITH RKP = 0 (DATA-ONLY RETRIEVAL).                          *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 3 - RRPL - RPL POINTER                              *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         BCBBUFL                                                    *
**         BCBBUFNO                                                   *
**         BCBFSBUF                                                   *
**         BCBBUFLINK                                                 *
**         DCBBUFCA                                                   *
**         IIKEYPT                                                    *
**         IIQBFRS                                                    *
**         RPLIIBFR                                                   *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0742
*BLDBUFR:                                                          0742
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0742
BLDBUFR  DS    0H                                                  0743
*   RESPECIFY                                                      0743
*    (RBUFF) RESTRICTED;                                           0743
*   RBUFF=IIRPLPTR+IIRPLL;          /* INIT BUFFER POINTER           */
         L     RBUFF,IIRPLL-1(,RIICB)                              0744
         LA    RBUFF,0(,RBUFF)                                     0744
         AL    RBUFF,IIRPLPTR(,RIICB)                              0744
*   IF DCBMFLCP=ON THEN             /* IF PUT LOCATE                 */
         TM    DCBMFLCP+40(RDCB),B'00001000'                       0745
         BNO   @RF00745                                            0745
*     DO;                                                          0746
*       IIKEYPT=RBUFF;              /* INIT PTR TO KEYSAVE           */
         ST    RBUFF,IIKEYPT(,RIICB)                               0747
*       RBUFF=RBUFF+IIKEYSL;        /* ADD KEYSAVE LENGTH            */
         AH    RBUFF,IIKEYSL(,RIICB)                               0748
*     END;                                                         0749
*   RRPL=IIRPLPTR;                  /* INIT PTR TO 1ST RPL           */
*                                                                  0750
@RF00745 L     RRPL,IIRPLPTR(,RIICB)                               0750
*   /*****************************************************************/
*   /*                                                               */
*   /* WHEN A VALID BUFFER ADDRESS IS SPECIFIED IN THE DCB, OR A     */
*   /* BISAM DCB, ONLY THE RPL BUFFER POINTER IS INITIALIZED BY THE  */
*   /* ISAM INTERFACE.                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0751
*   RESPECIFY                                                      0751
*    (R10) RESTRICTED;              /*                       @Y021130*/
*   IF(DCBMFRD=ON×                  /* IF BISAM (READ        @YM05335*/
*       DCBMFWRT=ON) THEN           /* OR WRITE)             @YM05335*/
         LA    @15,40                                              0752
         ALR   @15,RDCB                                            0752
         TM    DCBMFRD(@15),B'00100000'                            0752
         BO    @RT00752                                            0752
         TM    DCBMFWRT(@15),B'00100000'                           0752
         BNO   @RF00752                                            0752
@RT00752 DS    0H                                                  0753
*     RBUFF=RBUFF+16;               /* ADD PAD AND LINK BYTES      0753
*                                                            @YM05335*/
         AL    RBUFF,@CF00210                                      0753
*   IF(DCBMFRD=ON×                  /* IF BISAM (READ OR WRITE)      */
*       DCBMFWRT=ON)×               /* OR                            */
*       BUFADDR(32)='0'B THEN       /* VALID BUFFER ADDR IN DCB      */
@RF00752 LA    @15,40                                              0754
         ALR   @15,RDCB                                            0754
         TM    DCBMFRD(@15),B'00100000'                            0754
         BO    @RT00754                                            0754
         TM    DCBMFWRT(@15),B'00100000'                           0754
         BO    @RT00754                                            0754
         TM    BUFADDR+19(RDCB),B'00000001'                        0754
         BNZ   @RF00754                                            0754
@RT00754 DS    0H                                                  0755
*     DO R10=ONE BY ONE TO IIBUFNO; /*                       @Y021130*/
         LA    R10,1                                               0755
         B     @DE00755                                            0755
@DL00755 DS    0H                                                  0756
*       RPLIIBFR=RBUFF;             /* INIT RPL BUFFER POINTER       */
         LA    @15,76                                              0756
         ALR   @15,RRPL                                            0756
         ST    RBUFF,RPLIIBFR(,@15)                                0756
*       RRPL=RPLRPLPT;              /* BUMP RPL PTR                  */
         L     @15,RPLRPLPT(,@15)                                  0757
         LR    RRPL,@15                                            0757
*       RBUFF=RBUFF+IIBUFL;         /* BUMP BUFFER POINTER           */
         AH    RBUFF,IIBUFL(,RIICB)                                0758
*     END;                          /* LOOP BACK                     */
         AL    R10,@CF00087                                        0759
@DE00755 SLR   @15,@15                                             0759
         IC    @15,IIBUFNO(,RIICB)                                 0759
         CR    R10,@15                                             0759
         BNH   @DL00755                                            0759
*   ELSE                                                           0760
*     DO;                           /* IF QISAM W/O BUFFERS THEN TURN
*                                      ON BIT TO PREVENT FREEMAIN OF
*                                      BUFFERS IN CLOSE - USER ISSUES
*                                      FREE-                         */
         B     @RC00754                                            0760
@RF00754 DS    0H                                                  0761
*       IIQBFRS=ON;                 /* POOL AFTER CLOSE              */
         OI    IIQBFRS(RIICB),B'00000001'                          0761
*       DCBBUFCA=RBUFF;             /* INIT BUFR CONTROL BLK PTR     */
         LA    @15,16                                              0762
         ALR   @15,RDCB                                            0762
         STCM  RBUFF,7,DCBBUFCA(@15)                               0762
*       BCBBUFNO=IIBUFNO;           /* INIT BUFR NO IN BCB           */
         L     @15,DCBBUFCB(,@15)                                  0763
         SLR   @07,@07                                             0763
         IC    @07,IIBUFNO(,RIICB)                                 0763
         STH   @07,BCBBUFNO(,@15)                                  0763
*       BCBBUFL=IIBUFL;             /* INIT BUFR LENGTH IN BCB       */
         LH    @07,IIBUFL(,RIICB)                                  0764
         STH   @07,BCBBUFL(,@15)                                   0764
*       RBUFF=RBUFF+8;              /* ADD LENGTH OF BCB             */
         LA    @15,8                                               0765
         ALR   RBUFF,@15                                           0765
*       IF DCBBFA='01'B THEN        /* IF FULL WORD ALIGN            */
         TM    DCBBFA+28(RDCB),B'00000001'                         0766
         BNO   @RF00766                                            0766
         TM    DCBBFA+28(RDCB),B'00000010'                         0766
         BNZ   @RF00766                                            0766
*         RBUFF=RBUFF+8;            /* ADJUST POINTER                */
         ALR   RBUFF,@15                                           0767
*       BCBFSBUF=RBUFF;             /* SET PTR TO 1ST BUFFER         */
@RF00766 L     @15,DCBBUFCB+16(,RDCB)                              0768
         ST    RBUFF,BCBFSBUF(,@15)                                0768
*       IF IIBUFNO>1 THEN                                          0769
         CLI   IIBUFNO(RIICB),1                                    0769
         BNH   @RF00769                                            0769
*         DO R10=ONE BY ONE TO IIBUFNO-1;/*                  @Y021130*/
         LA    R10,1                                               0770
         B     @DE00770                                            0770
@DL00770 DS    0H                                                  0771
*           BUFLINK=RBUFF+IIBUFL;   /* INIT BUFFER LINK FIELD        */
         LR    @15,RBUFF                                           0771
         AH    @15,IIBUFL(,RIICB)                                  0771
         ST    @15,BUFLINK(,RBUFF)                                 0771
*           RPLIIBFR=RBUFF;         /* INIT RPL PTR TO BUFFER        */
         LA    @07,76                                              0772
         ALR   @07,RRPL                                            0772
         ST    RBUFF,RPLIIBFR(,@07)                                0772
*           RBUFF=BUFLINK;          /* BUMP BUFFER PTR BY BUFL       */
         LR    RBUFF,@15                                           0773
*           RRPL=RPLRPLPT;          /* POINT TO NEXT RPL             */
         L     @15,RPLRPLPT(,@07)                                  0774
         LR    RRPL,@15                                            0774
*         END;                                                     0775
         AL    R10,@CF00087                                        0775
@DE00770 SLR   @15,@15                                             0775
         IC    @15,IIBUFNO(,RIICB)                                 0775
         BCTR  @15,0                                               0775
         CR    R10,@15                                             0775
         BNH   @DL00770                                            0775
*       RPLIIBFR=RBUFF;             /* INIT RPL PTR TO BUFFER        */
@RF00769 ST    RBUFF,RPLIIBFR+76(,RRPL)                            0776
*       BUFLINK=ZERO;               /* LAST BUFR LINK = ZERO         */
         SLR   @15,@15                                             0777
         ST    @15,BUFLINK(,RBUFF)                                 0777
*     END;                                                         0778
*   RESPECIFY                                                      0779
*    (RBUFF,                                                       0779
*     R10) UNRESTRICTED;                                           0779
@RC00754 DS    0H                                                  0780
*   RRPL=IIRPLPTR;                  /* RESTORE RRPL          @OX19980*/
         L     RRPL,IIRPLPTR(,RIICB)                               0780
*   END BLDBUFR;                                                   0781
@EL00017 DS    0H                                                  0781
@EF00017 DS    0H                                                  0781
@ER00017 BR    @14                                                 0781
*                                                                  0782
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = DCB INITIALIZATION                                    *
**       THIS ROUTINE PERFORMS FINAL INITIALIZATION OF FIELDS FOR     *
**       THE ISAM INTERFACE.                                          *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 3 - RRPL - RPL POINTER                              *
**       REGISTER 6 - RACB - ACB POINTER                              *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         DCBOFLGS                                                   *
**           OPEN BIT ON                                              *
**           LOCK BIT OFF                                             *
**           BUSY BIT OFF                                             *
**         DCBOPTCD                                                   *
**         DCBRORG1                                                   *
**         DCBRORG2                                                   *
**         DCBRORG3                                                   *
**         DCBWKPT6                                                   *
**         IISEQCHK                                                   *
**         IIW1CBF                                                    *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0782
*DCBINIT:                                                          0782
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0782
DCBINIT  DS    0H                                                  0783
*   RESPECIFY                                                      0783
*    (JFCBPTR) RESTRICTED;          /* II SUPPORT-DCB WORK AREA      */
*   IF DCBMFGET=ON THEN             /* IF SCAN MODE                  */
         TM    DCBMFGET+40(RDCB),B'01000000'                       0784
         BNO   @RF00784                                            0784
*     DO;                           /* REFERENCED BY COBOL           */
*       DCBWKPT1=ADDR(IIW1CBF)-112; /* QISAM SCAN - WA + 112 =     0786
*                                      W1CBF-CURRENT BUFFER PTR      */
         LA    @15,IIW1CBF(,RIICB)                                 0786
         SL    @15,@CF02483                                        0786
         ST    @15,DCBWKPT1+52(,RDCB)                              0786
*       IIW1CBF=RPLIIBFR;           /* USED BY COBOL FOR PUTX      0787
*                                                            @XM01562*/
         L     @15,RPLIIBFR+76(,RRPL)                              0787
         ST    @15,IIW1CBF(,RIICB)                                 0787
*     END;                          /* AFTER GET             @XM01562*/
*   DCBWKPT5=RIICB;                 /* INIT DCB TO POINT TO IICB   0789
*                                      REFERENCED BY PL/I            */
@RF00784 LA    @15,52                                              0789
         ALR   @15,RDCB                                            0789
         ST    RIICB,DCBWKPT5(,@15)                                0789
*   IF DCBMFLCP=ON THEN             /* IF PUT LOCATE                 */
         TM    DCBMFLCP+40(RDCB),B'00001000'                       0790
         BNO   @RF00790                                            0790
*     DCBWKPT6=ADDR(IIKEYPT)-4;     /* QISAM LOAD - ISLVPTR PTR    0791
*                                      ISLVPTRS + 4 = KEYSAVE        */
         LA    @10,IIKEYPT(,RIICB)                                 0791
         SL    @10,@CF00199                                        0791
         ST    @10,DCBWKPT6(,@15)                                  0791
*   IF(DCBMFMVP=ON×                 /* IF LOAD MODE          @XM01562
*                                      (PUT MOVE OR PUT      @XM01562*/
*       DCBMFLCP=ON)&               /* LOCATE) AND THE       @XM01562*/
*       AMDNLR>ZERO THEN            /* DATA SET CONTAINS     @XM01562*/
@RF00790 TM    DCBMFMVP+40(RDCB),B'00011000'                       0792
         BZ    @RF00792                                            0792
         L     @15,ACBAMBL(,RACB)                                  0792
         L     @15,AMBLDTA(,@15)                                   0792
         L     @15,AMBDSB(,@15)                                    0792
         L     @15,AMDNLR(,@15)                                    0792
         LTR   @15,@15                                             0792
         BNP   @RF00792                                            0792
*     DCBSTLOD=ON;                  /* TURN ON RESUME LOAD BIT     0793
*                                                            @XM01562*/
         OI    DCBSTLOD+52(RDCB),B'00100000'                       0793
*   ELSE                                                           0794
*     DCBSTLOD=OFF;                 /* ELSE TURN RL BIT OFF  @XM01562
*                                      MAP OPTCD FROM JFCB TO DCB TO
*                                      OVERRIDE DCB EXIT CHGS        */
         B     @RC00792                                            0794
@RF00792 NI    DCBSTLOD+52(RDCB),B'11011111'                       0794
*   JFCBPTR=ADDR(DXJBF);            /* INIT JFCB PTR         @Y021130*/
@RC00792 LA    JFCBPTR,DXJBF(,RCORE)                               0795
*   IF JFCDEL=ON×DCBOPTL=ON THEN    /* IF OPTCD=L IN AMP OR DCB      */
         TM    JFCDEL(JFCBPTR),B'00000010'                         0796
         BO    @RT00796                                            0796
         TM    DCBOPTL+52(RDCB),B'00000010'                        0796
         BNO   @RF00796                                            0796
@RT00796 DS    0H                                                  0797
*     DO;                                                          0797
*       DCBOPTI=OFF;                /* TURN OFF DCB OPTCD=I          */
*       DCBOPTL=ON;                 /* TURN ON DCB OPTCD=L           */
         LA    @15,52                                              0799
