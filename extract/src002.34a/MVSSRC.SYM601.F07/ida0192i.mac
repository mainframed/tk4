         LR    RLKRETSV,R14                                        0593
*   IF DCBMFRD=ON×DCBMFWRT=ON THEN                                 0594
         LA    @15,40                                              0594
         ALR   @15,RWORK2                                          0594
         TM    DCBMFRD(@15),B'00100000'                            0594
