*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = OPEN ACB                                              *
**       THIS ROUTINE INITIALIZES THE OPEN PARAMETER LIST AND         *
**       OPENS THE ACB.  IT CHECKS TO SEE IF THE ACB WAS SUCCESSFULLY *
**       OPENED AND SETS THE DCB OPEN BIT APPROPRIATELY.  FOR AN      *
**       UNSUCCESSFUL OPEN, THIS ROUTINE TURNS THE DCB LOCK           *
**       BIT OFF AND FREES THE CORE FOR THE IICB, AND AND EXLST.      *
**       IF THE OPEN WAS SUCCESSFUL BUT ERROR CODE 116 WAS RETURNED   *
**       THEN ABEND 03B WILL BE ISSUED.                               *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**       REGISTER 6 - RACB - ACB POINTER                              *
**                                                                    *
**   OUTPUT=                                                          *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         DCBOFLGS                                                   *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0297
*OPENACB:                                                          0297
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0297
OPENACB  DS    0H                                                  0298
*   RESPECIFY                                                      0298
*    (RWORK2,                                                      0298
*     RLKRETSV) RESTRICTED;         /*                       @Y021130*/
*   RFY                                                            0299
*    (R15,                                                         0299
*     R0) RSTD;                     /* RESTRICT REGS         @OY08151*/
*   RLKRETSV=R14;                   /* SAVE RETURN ADDR      @Y021130*/
         LR    RLKRETSV,R14                                        0300
*   RWORK2=ADDR(IIACBPTR);          /* OPEN PARM LIST PTR            */
         LA    RWORK2,IIACBPTR(,RIICB)                             0301
*   PARCOPT(1)=ON;                  /* BIT FOR LAST ACB IN LIST      */
*                                                                  0302
         MVI   PARCOPT(RPARC),B'10000000'                          0302
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT ACB OPEN HAS BEEN ISSUED                        @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0303
*   AUDACBOP=ON;                    /* SET FLAG IN IICB      @Y021130*/
         OI    AUDACBOP(RIICB),B'10000000'                         0303
*   R15=ADDR(SYNCHACB);             /* ADDR OF OPEN RTN      @OY08151*/
         LA    R15,SYNCHACB                                        0304
*   R0=RWORK2;                      /* PASS ACB ADDR         @OY08151*/
         LR    R0,RWORK2                                           0305
*   GEN(SYNCH (15));                /* OPEN THE VSAM ACB     @OY08151*/
*                                                                  0306
         SYNCH (15)
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT THAT ACB OPEN HAS RETURNED                      @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0307
*   AUDACBRO=ON;                    /* SET FLAG IN IICB      @Y021130*/
         OI    AUDACBRO(RIICB),B'01000000'                         0307
*   PARCOPT(1)=OFF;                 /* CLEAR ACB PTR IN IICB         */
         MVI   PARCOPT(RPARC),B'00000000'                          0308
*   RDCB=DXUDCBAD;                  /* RESTORE DCB PTR       @Y021130*/
         L     RDCB,DXUDCBAD(,RCORE)                               0309
*   DCBOFLGS=ZERO;                  /* CLEAR OPEN FLAGS              */
         LA    @11,40                                              0310
         ALR   @11,RDCB                                            0310
         MVI   DCBOFLGS(@11),X'00'                                 0310
*   IF ACBOPEN=ON&ACBERFLG^=OPERR116 THEN/* IF SUCCESSFUL OPEN AND 0311
*                                      DID NOT GET OPEN ERROR CODE 0311
*                                      116                   @ZA32474*/
         TM    ACBOPEN(RACB),B'00010000'                           0311
         BNO   @RF00311                                            0311
         CLI   ACBERFLG(RACB),116                                  0311
         BE    @RF00311                                            0311
*     DCBOFOPN=ON;                  /* SET DCB OPEN BIT ON           */
         OI    DCBOFOPN(@11),B'00010000'                           0312
*   ELSE                            /* UNSUCCESSFUL OPEN             */
*     DO;                           /* OPEN AND BUSY BITS OFF        */
         B     @RC00311                                            0313
@RF00311 DS    0H                                                  0314
*       IF ACBOPEN=ON&ACBERFLG=OPERR116 THEN/* IF ERROR CODE 116 THEN
*                                                            @ZA32474*/
         TM    ACBOPEN(RACB),B'00010000'                           0314
         BNO   @RF00314                                            0314
         CLI   ACBERFLG(RACB),116                                  0314
         BNE   @RF00314                                            0314
*         DO;                       /*                       @ZA32474*/
*           DO;                     /* ABEND(ABEND3B) DUMP SYSTEM    */
*             RESPECIFY                                            0317
*              (GPR01F) RESTRICTED;                                0317
*             GPR01F=((ABEND3B)&'00000FFF'X)*4096;/* COMP CODE IN  0318
*                                      BITS 8-19                     */
         L     GPR01F,@CF02487                                     0318
*             GPR01F=GPR01F×'80000000'X;/* DUMP/STEP/DUMPOPTS FLAG   */
         O     GPR01F,@CF02428                                     0319
*             SVC(13);              /* ISSUE ABEND SVC               */
         SVC   13                                                  0320
*             RESPECIFY                                            0321
*              (GPR01F) UNRESTRICTED;                              0321
*           END;                    /* ABEND(ABEND3B) DUMP SYSTEM  0322
*                                      X'03B' - ISSUE ABEND  @ZA32474*/
*         END;                      /*                       @ZA32474*/
*       DCBOFUEX=ON;                /* LOCK BIT OFF (=1) FREE CORE 0324
*                                      FOR IICB,ACB,EXL              */
@RF00314 OI    DCBOFUEX+40(RDCB),B'00000010'                       0324
*       RESPECIFY                                                  0325
*        (R0,                                                      0325
*         R1) RESTRICTED;                                          0325
*       DO;                         /* MODESET EXTKEY(DATAMGT)       */
*         SPKA(80);                 /* SET PSW KEY                   */
         LA    @11,80                                              0327
         SPKA  0(@11)                                              0327
*       END;                        /* MODESET EXTKEY(DATAMGT) DATA
*                                      MGT KEY               @ZA02267*/
*       IIRSAVE3=RDCB;              /* SAVE DCB PTR          @ZA02670*/
         ST    RDCB,IIRSAVE3(,RCORE)                               0329
*       DO;                         /* MODESET KEYADDR(DXUKEY)       */
*         SPKA(DXUKEY);             /* SET PSW KEY                   */
         SLR   @11,@11                                             0331
         IC    @11,DXUKEY(,RCORE)                                  0331
         SPKA  0(@11)                                              0331
*       END;                        /* MODESET KEYADDR(DXUKEY) RETURN
*                                      TO USER KEY           @ZA02267*/
*       R0=MAINR0;                  /* INIT LENGTH / SUBPOOL         */
         L     R0,MAINR0(,RCORE)                                   0333
*       R1=MAINADDR;                /* INIT PTR TO CORE              */
         L     R1,MAINADDR(,RCORE)                                 0334
*       GEN(FREEMAIN R,LV=(0),A=(1));/* FREEMAIN             @Y021130*/
         FREEMAIN R,LV=(0),A=(1)
*       RDCB=IIRSAVE3;              /* RESTORE DCB ADDR      @OZ02670*/
         L     RDCB,IIRSAVE3(,RCORE)                               0336
*       RESPECIFY                                                  0337
*        (R0,                                                      0337
*         R1) UNRESTRICTED;                                        0337
*       RESPECIFY                                                  0338
*        (RTIOT) RESTRICTED;        /*                       @Y021130
*                                      LOCATE DDNAME IN TIOT         */
*       RTIOT=DXTIOTAD;             /*                       @Y021130*/
         L     RTIOT,DXTIOTAD(,RCORE)                              0339
*       DCBDDNAM=TIOEDDNM;          /* GET DDNAME FROM TIOT          */
*                                                                  0340
         MVC   DCBDDNAM+40(8,RDCB),TIOEDDNM(RTIOT)                 0340
*       /*************************************************************/
*       /*                                                           */
*       /* AUDIT INDICATE IICB NO LONGER AVAILABLE           @Y021130*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0341
*       DO;                         /* MODESET EXTKEY(DATAMGT)       */
*         SPKA(80);                 /* SET PSW KEY                   */
         LA    @11,80                                              0342
         SPKA  0(@11)                                              0342
*       END;                        /* MODESET EXTKEY(DATAMGT) DATA
*                                      MGT KEY               @Y021130*/
*       IIAUDPTR=ZERO;              /* IICB NO LONGER AVAILABLE    0344
*                                                            @Y021130*/
         SLR   @11,@11                                             0344
         ST    @11,IIAUDPTR(,RCORE)                                0344
*       DO;                         /* MODESET KEYADDR(DXUKEY)       */
*         SPKA(DXUKEY);             /* SET PSW KEY                   */
         SLR   @11,@11                                             0346
         IC    @11,DXUKEY(,RCORE)                                  0346
         SPKA  0(@11)                                              0346
*       END;                        /* MODESET KEYADDR(DXUKEY) RETURN
*                                      TO USER KEY           @Y021130*/
*       RESPECIFY                                                  0348
*         RTIOT UNRESTRICTED;       /*                       @Y021130*/
*     END;                                                         0349
*   R14=RLKRETSV;                   /* RESTORE RETURN ADDRESS      0350
*                                                            @Y021130*/
@RC00311 LR    R14,RLKRETSV                                        0350
*   RESPECIFY                                                      0351
*    (RWORK2,                                                      0351
*     RLKRETSV) UNRESTRICTED;       /*                       @Y021130*/
*   RFY                                                            0352
*    (R15,                                                         0352
*     R0) UNRSTD;                   /* FREE REGS             @OY08151*/
*   END OPENACB;                                                   0353
@EL00006 DS    0H                                                  0353
@EF00006 DS    0H                                                  0353
@ER00006 BR    @14                                                 0353
*SYNCHACB:                          /*                       @OY08151*/
