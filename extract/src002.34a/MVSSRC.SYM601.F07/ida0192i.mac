*   R9=R14;                         /* SAVE RET TO SVC       @YM04603*/
         LR    R9,R14                                              0670
*   R8=ADDR(IISAMSYN);              /* INIT PTR TO SYNAD NAME      0671
*                                                            @YM04603*/
         LA    R8,IISAMSYN(,RIICB)                                 0671
*   GEN(LOAD EPLOC=(R8));           /* LOAD ISAM SYNAD MODULE      0672
*                                                            @YM04603*/
*                                                                  0672
         LOAD EPLOC=(R8)
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT THAT THE USER SYNAD MODULE WAS LOADED SUCESSFULLY       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0673
