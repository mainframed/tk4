*   ELSE                                                           0635
*     IF DCBMFPUT=ON THEN           /* IF LOAD MODE                  */
         B     @RC00617                                            0635
@RF00617 TM    DCBMFPUT+40(RWORK2),B'01000000'                     0635
         BNO   @RF00635                                            0635
*       DO;                         /* LOAD II LOAD MODE MODULE      */
*         R1=CVTSVDCB;              /* PT TO DCB FOR SYS1.SVCLIB     */
         L     @15,CVTPTR                                          0637
         L     R1,CVTSVDCB(,@15)                                   0637
