*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = DCB MERGE                                             *
**       THIS ROUTINE SIMULATES THE ISAM DCB MERGE FROM THE DD CARD   *
**       AND THE DSCB TO THE DCB.  INFORMATION THAT WAS SPECIFIED     *
**       IN THE AMP PARAMETER IS MERGED FROM THE JFCB.  II OPEN WAS   *
**       ENTERED WITH THE FOUNDATION SECTION OF THE DCB INITIALIZED   *
**       TO THE FORMAT AFTER OPEN.  ANOTHER FUNCTION IS TO SIMULATE   *
**       THE DS2NOBTY FIELD IN A FORMAT 2 DSCB TO SUPPORT ISAM        *
**       PROGRAMS THAT REFERENCE IT IN THE DCB EXIT ROUTINE.          *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**       REGISTER 6 - RACB - ACB POINTER                              *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         DCBNCP                                                     *
**         DCBNCRNI                                                   *
**         JFCOPTCD  -->  DCBOPTCD                                    *
**         AMDWCK    -->  DCBOPTCD                                    *
**         JFCRECFM  -->  DCBRECFM                                    *
**         DS2NOBYT                                                   *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0362
*DCBMERGE:                                                         0362
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0362
DCBMERGE DS    0H                                                  0363
*   RESPECIFY                                                      0363
*    (JFCBPTR) RESTRICTED;                                         0363
*   JFCBPTR=ADDR(DXJBF);            /* INIT JFCB PTR         @Y021130*/
         LA    JFCBPTR,DXJBF(,RCORE)                               0364
*   IF DCBRECL=BZERO THEN           /* IF RECFM NOT IN DCB MACRO     */
         LA    @08,28                                              0365
         ALR   @08,RDCB                                            0365
         TM    DCBRECL(@08),B'11000000'                            0365
         BNZ   @RF00365                                            0365
*     DO;                                                          0366
*       IF JFCRECFM=ZERO THEN       /* IF RECFM NOT IN AMP PARM      */
         CLI   JFCRECFM(JFCBPTR),0                                 0367
         BNE   @RF00367                                            0367
*         DCBRECL='01'B;            /* DEFAULT TO UNBLK VARIABLE     */
         NI    DCBRECL(@08),B'01111111'                            0368
         OI    DCBRECL(@08),B'01000000'                            0368
*       ELSE                                                       0369
*         DCBRECFM=JFCRECFM;        /* MERGE RECFM FROM AMP          */
         B     @RC00367                                            0369
@RF00367 MVC   DCBRECFM+28(1,RDCB),JFCRECFM(JFCBPTR)               0369
*       MRECFM=ON;                  /* SET MODIFICATION MASK         */
@RC00367 OI    MRECFM(RIICB),B'00001000'                           0370
*     END;                          /* MAP OPTCD FROM JFCB TO DCB    */
*   DCBOPTI=OFF;                    /* TURN OFF DCB OPTCD=I          */
@RF00365 LA    @08,52                                              0372
         ALR   @08,RDCB                                            0372
         NI    DCBOPTI(@08),B'11101111'                            0372
*   IF JFCDEL=ON×DCBOPTL=ON THEN    /* TURN OFF DCB OPTCD=I          */
         TM    JFCDEL(JFCBPTR),B'00000010'                         0373
         BO    @RT00373                                            0373
         TM    DCBOPTL(@08),B'00000010'                            0373
         BNO   @RF00373                                            0373
@RT00373 DS    0H                                                  0374
*     DO;                           /* THEN                          */
*       DCBOPTL=ON;                 /* TURN ON DCB OPTCD=L           */
         LA    @08,52                                              0375
         ALR   @08,RDCB                                            0375
         OI    DCBOPTL(@08),B'00000010'                            0375
*       IF JFCIND=ON THEN           /* IF OPTCD=I IN AMP PARM        */
         TM    JFCIND(JFCBPTR),B'00010000'                         0376
         BNO   @RF00376                                            0376
*         DCBOPTI=ON;               /* TURN ON DCB OPTCD=I           */
         OI    DCBOPTI(@08),B'00010000'                            0377
*       MOPTCD=ON;                  /* SET MODIFICATION MASK         */
@RF00376 OI    MOPTCD(RIICB),B'00010000'                           0378
*     END;                                                         0379
*   RESPECIFY                                                      0380
*     JFCBPTR UNRESTRICTED;                                        0380
@RF00373 DS    0H                                                  0381
*   IF DCBOPTW=OFF THEN             /* OPTCD=W NOT IN DCB MACRO      */
         LA    @07,52                                              0381
         ALR   @07,RDCB                                            0381
         TM    DCBOPTW(@07),B'10000000'                            0381
         BNZ   @RF00381                                            0381
*     DO;                                                          0382
*       IF AMDWCK=ON THEN           /* WRT CHK OPTION FROM DEFINE    */
         L     @05,ACBAMBL(,RACB)                                  0383
         L     @05,AMBLDTA(,@05)                                   0383
         L     @05,AMBDSB(,@05)                                    0383
         TM    AMDWCK(@05),B'01000000'                             0383
         BNO   @RF00383                                            0383
*         DCBOPTW=ON;                                              0384
         OI    DCBOPTW(@07),B'10000000'                            0384
*       MOPTCD=ON;                  /* SET MODIFICATION MASK         */
@RF00383 OI    MOPTCD(RIICB),B'00010000'                           0385
*     END;                                                         0386
*   IF(DCBMFRD=ON×                  /* IF BISAM (READ OR WRITE)      */
*       DCBMFWRT=ON)&               /* AND                           */
*       DCBNCP=ZERO THEN            /* NCP IS ZERO THEN SET          */
@RF00381 LA    @07,40                                              0387
         ALR   @07,RDCB                                            0387
         TM    DCBMFRD(@07),B'00100000'                            0387
         BO    @GL00006                                            0387
         TM    DCBMFWRT(@07),B'00100000'                           0387
         BNO   @RF00387                                            0387
@GL00006 LA    @07,52                                              0387
         ALR   @07,RDCB                                            0387
         CLI   DCBNCP(@07),0                                       0387
         BNE   @RF00387                                            0387
*     DCBNCP=ONE;                   /* NCP TO DEFAULT OF ONE         */
*                                                                  0388
         MVI   DCBNCP(@07),X'01'                                   0388
*   /*****************************************************************/
*   /*                                                               */
*   /* DCBDEBAD CONTAINS THE ADDRESS OF THE I/O SUPPORT WORK AREA TO */
*   /* ENABLE THE DCB EXIT ROUTINE TO REFERENCE THE F2 DSCB. DS2NOBYT*/
*   /* AND DCBNCRHI ARE BOTH DEFINED AS THE NUMBER OF BYTES NEEDED TO*/
*   /* HOLD THE HIGHEST LEVEL INDEX IN MAIN STORAGE. SINCE THIS FIELD*/
*   /* IS NOT USED BY THE ISAM INTER- FACE OR VSAM IT IS INITIALIZED */
*   /* TO SUPPORT PL/I AND OTHER PROGRAMS THAT REFERENCE IT AT DCB   */
*   /* EXIT TIME. THESE FIELDS ARE SET TO A LOW VALUE RATHER THAN    */
*   /* ZERO TO PREVENT A GETMAIN FAILURE IN THE ISAM PROBLEM PROGRAM */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0389
*   DCBNCRHI=EIGHT;                 /* INIT DCB                      */
@RF00387 MVC   DCBNCRHI+52(2,RDCB),@CH00045                        0389
*   END DCBMERGE;                                                  0390
@EL00008 DS    0H                                                  0390
@EF00008 DS    0H                                                  0390
@ER00008 BR    @14                                                 0390
*                                                                  0391
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = AMS MERGE                                             *
**       THIS ROUTINE MERGES THE DATA SET ATTRIBUTES FROM THE AMDSB   *
**       TO THE DCB WHEN THE CORRESPONDING DCB FIELD IS ZERO. THESE   *
**       ARE THE CHARACTERISTICS THAT WERE USED WHEN DEFINING THE     *
**       VSAM DATA SET WITH AMS.  WHEN PROCESSING WITH ISAM, THESE    *
**       FIELDS MAY BE SPECIFIED IN THE ISAM DCB EXIT ROUTINE FOR     *
**       LOAD MODE.  FOR SCAN MODE AND BISAM, THEY ARE MERGED FROM    *
**       THE DSCB TO THE DCB.  HOWEVER, THIS MERGE MUST TAKE PLACE    *
**       PRIOR TO THE DCB EXIT TO SUPPORT PL/I.                       *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 6 - RACB - ACB POINTER                              *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         DCBLRECL  -->  DCBBLKSI                                    *
**         AMDKEYLN  -->  DCBKEYLE                                    *
**         AMDLRECL  -->  DCBLRECL                                    *
**         AMDRKP    -->  DCBRKP                                      *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0391
*AMSMERGE:                                                         0391
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0391
*                                                                  0391
AMSMERGE DS    0H                                                  0392
*   /*****************************************************************/
*   /*                                                               */
*   /* PL/I REQUIRES THAT KEYLE AND RKP BE SPECIFIED IN THE DD. IF   */
*   /* THESE FIELDS ARE NOT MERGED PRIOR TO THE DCB EXIT THE KEYLE   */
*   /* GETS SET TO 1 IN THE DCB EXIT.                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0392
*   IF DCBKEYLE=ZERO THEN           /* IF KEYLE NOT IN DCB MACRO     */
         LA    @07,16                                              0392
         ALR   @07,RDCB                                            0392
         CLI   DCBKEYLE(@07),0                                     0392
         BNE   @RF00392                                            0392
*     DO;                                                          0393
*       DCBKEYLE=AMDKEYLN;          /* KEYLE FROM AMS DEFINE         */
         L     @05,ACBAMBL(,RACB)                                  0394
         L     @05,AMBLDTA(,@05)                                   0394
         L     @05,AMBDSB(,@05)                                    0394
         LH    @05,AMDKEYLN(,@05)                                  0394
         STC   @05,DCBKEYLE(,@07)                                  0394
*       MKEYLE=ON;                  /* SET MODIFICATION MASK         */
         OI    MKEYLE(RIICB),B'00000001'                           0395
*     END;                                                         0396
*   IF DCBRKP=ZERO THEN             /* IF RKP NOT IN DCB MACRO       */
@RF00392 LA    @07,52                                              0397
         ALR   @07,RDCB                                            0397
         LH    @05,DCBRKP(,@07)                                    0397
         LTR   @05,@05                                             0397
         BNZ   @RF00397                                            0397
*     DO;                                                          0398
*       DCBRKP=AMDRKP;              /* RKP FROM AMS DEFINE           */
         L     @05,ACBAMBL(,RACB)                                  0399
         L     @05,AMBLDTA(,@05)                                   0399
         L     @05,AMBDSB(,@05)                                    0399
         LH    @05,AMDRKP(,@05)                                    0399
         STH   @05,DCBRKP(,@07)                                    0399
*       MRKP=ON;                    /* SET MODIFICATION MASK         */
         OI    MRKP(RIICB),B'10000000'                             0400
*       IF DCBRECL='01'B THEN       /* IF VARIABLE LENGTH RECORDS    */
         TM    DCBRECL+28(RDCB),B'01000000'                        0401
         BNO   @RF00401                                            0401
         TM    DCBRECL+28(RDCB),B'10000000'                        0401
         BNZ   @RF00401                                            0401
*         DCBRKP=DCBRKP+4;          /* ADD 4 TO RKP FOR RDW          */
         AL    @05,@CF00199                                        0402
         STH   @05,DCBRKP(,@07)                                    0402
*     END;                                                         0403
*                                                                  0403
@RF00401 DS    0H                                                  0404
*   /*****************************************************************/
*   /*                                                               */
*   /* LRECL AND BLKSIZE ARE MERGED PRIOR TO TAKING THE DCB EXIT     */
*   /* BECAUSE PL/I REQUIRES BLKSIZE, LRECL (REQUIRED FOR BLOCKED    */
*   /* RECORDS) AND RECFM TO BE SPECIFIED TOGETHER IN EITHER THE DD  */
*   /* CARD OR THE ENVIRON ATTRIBUTE. IF THESE ATTRIBUTES ARE NOT    */
*   /* MERGED THEN PL/I DEFAULTS TO 125 BYTES                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0404
*   IF DCBLRECL=ZERO THEN           /* IF LRECL NOT IN DCB MACRO     */
@RF00397 SLR   @07,@07                                             0404
         LA    @05,52                                              0404
         ALR   @05,RDCB                                            0404
         CH    @07,DCBLRECL(,@05)                                  0404
         BNE   @RF00404                                            0404
*     DO;                                                          0405
*       DCBLRECL=AMDLRECL;          /* LRECL FROM AMS DEFINE         */
         L     @03,ACBAMBL(,RACB)                                  0406
         L     @03,AMBLDTA(,@03)                                   0406
         L     @03,AMBDSB(,@03)                                    0406
         L     @03,AMDLRECL(,@03)                                  0406
         STH   @03,DCBLRECL(,@05)                                  0406
*       MLRECL=ON;                  /* SET MODIFICATION MASK         */
         OI    MLRECL(RIICB),B'01000000'                           0407
*       IF DCBRECL='10'B&           /* IF FIXED LENGTH RECORDS       */
*           DCBRECBR=OFF&           /* UNBLOCKED AND                 */
*           DCBRKP=ZERO THEN        /* RKP = 0 THEN SUB KEYLENGTH TO
*                                      LRECL                         */
         LA    @11,28                                              0408
         ALR   @11,RDCB                                            0408
         TM    DCBRECL(@11),B'10000000'                            0408
         BNO   @RF00408                                            0408
         TM    DCBRECL(@11),B'01000000'                            0408
         BNZ   @RF00408                                            0408
         TM    DCBRECBR(@11),B'00010000'                           0408
         BNZ   @RF00408                                            0408
         CH    @07,DCBRKP(,@05)                                    0408
         BNE   @RF00408                                            0408
*         DCBLRECL=DCBLRECL-DCBKEYLE;                              0409
         SLR   @07,@07                                             0409
         IC    @07,DCBKEYLE+16(,RDCB)                              0409
         SLR   @03,@07                                             0409
         STH   @03,DCBLRECL(,@05)                                  0409
*       IF DCBRECL='01'B THEN       /* IF VARIABLE LENGTH RECORDS    */
@RF00408 TM    DCBRECL+28(RDCB),B'01000000'                        0410
         BNO   @RF00410                                            0410
         TM    DCBRECL+28(RDCB),B'10000000'                        0410
         BNZ   @RF00410                                            0410
*         DCBLRECL=DCBLRECL+4;      /* ADD 4 TO LRECL FOR RDW        */
         LA    @07,52                                              0411
         ALR   @07,RDCB                                            0411
         LA    @05,4                                               0411
         AH    @05,DCBLRECL(,@07)                                  0411
         STH   @05,DCBLRECL(,@07)                                  0411
*     END;                                                         0412
@RF00410 DS    0H                                                  0413
*   IF DCBBLKSI=ZERO THEN           /* IF BLKSI NOT IN DCB MACRO     */
@RF00404 LA    @07,52                                              0413
         ALR   @07,RDCB                                            0413
         LH    @05,DCBBLKSI(,@07)                                  0413
         LTR   @05,@05                                             0413
         BNZ   @RF00413                                            0413
*     DO;                                                          0414
*       DCBBLKSI=DCBLRECL;          /* SET BLKSI TO COMPUTED LEN     */
         LH    @05,DCBLRECL(,@07)                                  0415
         STH   @05,DCBBLKSI(,@07)                                  0415
*       MBLKSI=ON;                  /* SET MODIFICATION MASK         */
         OI    MBLKSI(RIICB),B'00100000'                           0416
*       IF DCBRECL='01'B THEN       /* IF VARIABLE LENGTH RECORDS    */
         TM    DCBRECL+28(RDCB),B'01000000'                        0417
         BNO   @RF00417                                            0417
         TM    DCBRECL+28(RDCB),B'10000000'                        0417
         BNZ   @RF00417                                            0417
*         DCBBLKSI=DCBBLKSI+4;      /* ADD 4 TO BLKSIZE FOR RDW      */
         AL    @05,@CF00199                                        0418
         STH   @05,DCBBLKSI(,@07)                                  0418
*     END;                                                         0419
*   END AMSMERGE;                                                  0420
@EL00009 DS    0H                                                  0420
@EF00009 DS    0H                                                  0420
@ER00009 BR    @14                                                 0420
