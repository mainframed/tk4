         TITLE 'IDA0192I - MVM  ISAM INTERFACE OPEN                    *
                        '
IDA0192I CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IDA0192I  78.144'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*   IIRSAVE=R13;                    /* SAVE PTR TO SAVE AREA         */
         ST    R13,IIRSAVE(,RCORE)                                 0172
*   RIICB=ZERO;                     /* INDICATE IICB NOT OBTAINED  0173
*                                                            @Y021130*/
         SLR   RIICB,RIICB                                         0173
*   IIAUDPTR=ZERO;                  /* INDICATE NO AUDITING HAS BEEN
*                                      DONE                  @Y021130*/
         SLR   @15,@15                                             0174
         ST    @15,IIAUDPTR(,RCORE)                                0174
*   DO;                             /* MODESET KEYADDR(DXUKEY)       */
*     SPKA(DXUKEY);                 /* SET PSW KEY                   */
         SLR   @15,@15                                             0176
         IC    @15,DXUKEY(,RCORE)                                  0176
         SPKA  0(@15)                                              0176
*   END;                            /* MODESET KEYADDR(DXUKEY) ASSUME
*                                      USER KEY              @Y021130*/
*   RDCB=DXUDCBAD;                  /* INIT DCB POINTER TO DCB COPY
*                                                            @Y021130*/
         L     RDCB,DXUDCBAD(,RCORE)                               0178
*   ISAMDCB=PCOPYDCB;               /* REFRESH USER DCB FROM       0179
*                                      PROTECTED COPY        @Y021130*/
         L     @15,DXPDCBAD(,RCORE)                                0179
         MVC   ISAMDCB+16(236,RDCB),PCOPYDCB(@15)                  0179
*   RESPECIFY                                                      0180
*    (RRPL) UNRESTRICTED;           /*                       @Y021130*/
*   CALL BLDIICB;                   /* BUILD THE IICB,ACB & EXLST    */
         BAL   @14,BLDIICB                                         0181
*   CALL INITIICB;                  /* INIT THE IICB, ACB & EXLST    */
         BAL   @14,INITIICB                                        0182
*   CALL ACBMERGE;                  /* MERGE FROM DCB TO ACB         */
         BAL   @14,ACBMERGE                                        0183
*   CALL OPENACB;                   /* OPEN THE ACB                  */
         BAL   @14,OPENACB                                         0184
*   IF DCBOFOPN=OFF THEN            /* IF UNSUCCESSFUL OPEN          */
         TM    DCBOFOPN+40(RDCB),B'00010000'                       0185
         BZ    @RT00185                                            0185
*     GO TO EXIT;                   /* RETURN TO USER                */
*   CALL DCBMERGE;                  /* MERGE FROM JFCB TO DCB        */
         BAL   @14,DCBMERGE                                        0187
*   CALL AMSMERGE;                  /* MERGE DATA SET ATTRIBUTES     */
         BAL   @14,AMSMERGE                                        0188
*   CALL DCBEXIT;                   /* ISAM DCB EXIT                 */
         BAL   @14,DCBEXIT                                         0189
*   CALL VALIDCHK;                  /* VALIDITY CHECK ATTRIBUTES     */
         BAL   @14,VALIDCHK                                        0190
*   CALL BFRMERGE;                  /* MERGE BUFFER RELATED INFO     */
         BAL   @14,BFRMERGE                                        0191
*   CALL BUILDDEB;                  /* BUILD DUMMY ISAM DEB          */
         BAL   @14,BUILDDEB                                        0192
*   CALL LOADMOD;                   /* LOAD II PROCESSING MODULES    */
         BAL   @14,LOADMOD                                         0193
*   CALL BLDRPL;                    /* COMPUTE RPL & BUFFER SIZE     */
         BAL   @14,BLDRPL                                          0194
*   CALL INITRPL;                   /* INIT RPL, RPLE & ERROR MSG    */
         BAL   @14,INITRPL                                         0195
*   CALL BLDBUFR;                   /* BUILD BUFFERS                 */
         BAL   @14,BLDBUFR                                         0196
*   CALL DCBINIT;                   /* FINAL DCB INITIALIZATION      */
         BAL   @14,DCBINIT                                         0197
*EXIT:                                                             0198
*   R13=IIRSAVE;                                                   0198
EXIT     L     R13,IIRSAVE(,RCORE)                                 0198
*   DO;                             /* MODESET EXTKEY(ZERO)          */
*     SPKA(0);                      /* SET PSW KEY                   */
         SLR   @15,@15                                             0200
         SPKA  0(@15)                                              0200
*   END;                            /* MODESET EXTKEY(ZERO) ASSUME 0201
*                                      SYSTEM KEY            @Y021130*/
*   PCOPYDCB=ISAMDCB;               /* REFRESH COPY DCB      @YM03981*/
         L     @15,DXPDCBAD(,RCORE)                                0202
         MVC   PCOPYDCB(236,@15),ISAMDCB+16(RDCB)                  0202
*   RETURN;                                                        0203
@EL00001 DS    0H                                                  0203
@EF00001 DS    0H                                                  0203
@ER00001 LM    @14,@12,12(@13)                                     0203
         BR    @14                                                 0203
*                                                                  0204
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = BUILD IICB                                            *
**       THIS ROUTINE CALCULATES THE AMOUNT OF CORE NEEDED AND        *
**       CALLS THE GETMAIN ROUTINE TO GET CORE IN THE USER'S          *
**       REGION FOR THE IICB, ACB AND EXLST.  THESE BLOCKS WILL BE    *
**       USED FOR THE DCB THAT IS BEING OPENED.                       *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         IIACBL                                                     *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0204
*BLDIICB:                                                          0204
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0204
BLDIICB  DS    0H                                                  0205
*   RESPECIFY                                                      0205
*    (RLKRETSV) RESTRICTED;         /*                       @Y021130*/
*   RLKRETSV=R14;                   /* SAVE RETURN ADDRESS   @Y021130*/
         LR    RLKRETSV,R14                                        0206
*   RESPECIFY                                                      0207
*    (RGMLEN) RESTRICTED;           /*                       @Y021130*/
*   RGMLEN=LENGTH(IDAIICB)+         /* INIT AMT OF CORE REQUIRED     */
*       LENGTH(IFGACB)+             /*                       @Y021130*/
*       20;                                                        0208
         LA    RGMLEN,264                                          0208
*   RESPECIFY                                                      0209
*    (RGMSP) RESTRICTED;            /*                       @Y021130*/
*   RGMSP=USERPOOL;                 /* SUBPOOL FOR USER REGION     0210
*                                                            @Y021130*/
         LA    RGMSP,250                                           0210
*   CALL GETMAIN;                   /* GET CORE                      */
         BAL   @14,GETMAIN                                         0211
*   RESPECIFY                                                      0212
*    (RGMSP,                                                       0212
*     RGMLEN,                                                      0212
*     RLKRETSV) UNRESTRICTED;       /*                       @Y021130*/
*   RIICB=MAINADDR;                 /* INIT IICB REG                 */
         L     RIICB,MAINADDR(,RCORE)                              0213
*   IIACBL=MAINLEN;                 /* SAVE LENGTH / SUBPOOL         */
         MVC   IIACBL(3,RIICB),MAINLEN(RCORE)                      0214
*   R14=IIRSAVE1;                   /* RESTORE RETURN ADDR           */
         L     R14,IIRSAVE1(,RCORE)                                0215
*   END BLDIICB;                                                   0216
@EL00002 DS    0H                                                  0216
@EF00002 DS    0H                                                  0216
@ER00002 BR    @14                                                 0216
*                                                                  0217
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = GETMAIN                                               *
**       THIS ROUTINE IS CALLED BY THE BLDIICB, BUILDDEB AND          *
**       BLDRPL ROUTINES IN THIS MODULE.  ALL ISAM INTERFACE          *
**       CORE IS OBTAINED BY THIS ROUTINE.                            *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**       REGISTER 8 - RGMSP = SUBPOOL NUMBER FOR GETMAIN              *
**       REGISTER 9 - RGMLEN = LENGTH FOR GETMAIN                     *
**       REGISTER 10 - RLKRETSV = CALLER'S SAVED LINKAGE              *
**                                                                    *
**   OUTPUT =                                                         *
**       SAME AS ABOVE                                                *
**       MAINADDR = ADDRESS OF CORE OBTAINED                          *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = DATA MANAGEMENT KEY 5                 *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0217
*GETMAIN:                                                          0217
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0217
GETMAIN  DS    0H                                                  0218
*   RESPECIFY                                                      0218
*    (RGMSP,                                                       0218
*     RGMLEN,                                                      0218
*     RLKRETSV,                                                    0218
*     RRPL) RESTRICTED;             /*                       @ZA02670*/
*   DO;                             /* MODESET EXTKEY(DATAMGT)       */
*     SPKA(80);                     /* SET PSW KEY                   */
         LA    @15,80                                              0220
         SPKA  0(@15)                                              0220
*   END;                            /* MODESET EXTKEY(DATAMGT) ASSUME
*                                      DATA MANAGEMENT KEY FOR     0221
*                                      STORING INTO THE OPEN WORK  0221
*                                      AREA                  @Y021130*/
*   IIRSAVE1=RLKRETSV;              /* SAVE CALLER'S LINKAGE @Y021130*/
         ST    RLKRETSV,IIRSAVE1(,RCORE)                           0222
*   MAINLEN=RGMLEN;                 /* SAVE LENGTH           @Y021130*/
         STCM  RGMLEN,7,MAINLEN(RCORE)                             0223
*   MAINPOOL=RGMSP;                 /* SAVE SUBPOOL          @Y021130*/
         STC   RGMSP,MAINPOOL(,RCORE)                              0224
*   RESPECIFY                                                      0225
*    (RGMSP,                                                       0225
*     RGMLEN,                                                      0225
*     RLKRETSV) UNRESTRICTED;       /*                       @Y021130*/
*   IIRSAVE2=R14;                   /* SAVE RETURN ADDR              */
         ST    R14,IIRSAVE2(,RCORE)                                0226
*   IIRSAVE3=RDCB;                  /* SAVE DCB POINTER              */
         ST    RDCB,IIRSAVE3(,RCORE)                               0227
*   RESPECIFY                                                      0228
*    (R0,                                                          0228
*     R1,                                                          0228
*     R15) RESTRICTED;                                             0228
*   R0=MAINR0;                      /* LOAD R0 FOR GETMAIN           */
         L     R0,MAINR0(,RCORE)                                   0229
*   GEN(GETMAIN R,LV=(0));          /*                       @Y021130*/
         GETMAIN R,LV=(0)
*   MAINADDR=R1;                    /* SAVE RESULTS                  */
         ST    R1,MAINADDR(,RCORE)                                 0231
*   RESPECIFY                                                      0232
*    (R8,                                                          0232
*     R9,                                                          0232
*     R10,                                                         0232
*     R11) RESTRICTED;                                             0232
*   R8=R1;                          /* ADDR OF CORE                  */
         LR    R8,R1                                               0233
*   R9=MAINLEN;                     /* SIZE OF CORE                  */
         L     R9,MAINLEN-1(,RCORE)                                0234
         LA    R9,0(,R9)                                           0234
*   R11=ZERO;                       /* ZERO PADDING AND SIZE         */
         SLR   R11,R11                                             0235
*   IF MAINPOOL=USERPOOL THEN                                      0236
         CLI   MAINPOOL(RCORE),250                                 0236
         BNE   @RF00236                                            0236
*     DO;                           /* MODESET KEYADDR(DXUKEY)       */
*       SPKA(DXUKEY);               /* SET PSW KEY                   */
         SLR   @07,@07                                             0238
         IC    @07,DXUKEY(,RCORE)                                  0238
         SPKA  0(@07)                                              0238
*     END;                          /* MODESET KEYADDR(DXUKEY) ASSUME
*                                      USER KEY              @Y021130*/
*   GEN(MVCL R8,R10);               /* CLEAR CORE                    */
*                                                                  0240
@RF00236 DS    0H                                                  0240
         MVCL R8,R10
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT SET UP AUDIT TRAIL IF FIRST GETMAIN             @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0241
*   IF IIAUDPTR=ZERO THEN           /* IF THIS IS FIRST GETMAIN WHICH
*                                      IS FOR IICB THEN              */
         L     @07,IIAUDPTR(,RCORE)                                0241
         LTR   @07,@07                                             0241
         BNZ   @RF00241                                            0241
*     DO;                           /* INIT AUDIT POINTER TO IICB    */
*       DO;                         /* MODESET EXTKEY(DATAMGT)       */
*         SPKA(80);                 /* SET PSW KEY                   */
         LA    @07,80                                              0244
         SPKA  0(@07)                                              0244
*       END;                        /* MODESET EXTKEY(DATAMGT) POINT
*                                      AUDIT TRAIL TO IICB ANCHORED
*                                      IN FORCORE                    */
*       RIICB=MAINADDR;             /* GET ADDR OF IICB      @Y021130*/
         L     RIICB,MAINADDR(,RCORE)                              0246
*       IIAUDPTR=RIICB;             /* SAVE IN OPEN CLOSE WORK AREA
*                                                            @Y021130*/
         ST    RIICB,IIAUDPTR(,RCORE)                              0247
*       DO;                         /* MODESET KEYADDR(DXUKEY)       */
*         SPKA(DXUKEY);             /* SET PSW KEY                   */
         SLR   @07,@07                                             0249
         IC    @07,DXUKEY(,RCORE)                                  0249
         SPKA  0(@07)                                              0249
*       END;                        /* MODESET KEYADDR(DXUKEY) KEY OF
*                                      IICB                  @Y021130*/
*       IIGMCNTR=LENGTH(IIAUDHDR);  /* SET OFFSET TO GETMAIN ENTRY 0251
*                                      PAST HEADER           @Y021130*/
         MVC   IIGMCNTR(2,RIICB),@CH00199                          0251
*     END;                          /*                       @Y021130*/
*                                                                  0252
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT GETMAIN                                         @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0253
*   DO;                             /* MODESET KEYADDR(DXUKEY)       */
@RF00241 DS    0H                                                  0254
*     SPKA(DXUKEY);                 /* SET PSW KEY                   */
         SLR   @07,@07                                             0254
         IC    @07,DXUKEY(,RCORE)                                  0254
         SPKA  0(@07)                                              0254
*   END;                            /* MODESET KEYADDR(DXUKEY) GET 0255
*                                      INTO USER KEY         @Y021130*/
*   IF IIGMCNTR<LENGTH(IIAUD) THEN                                 0256
         LH    @07,IIGMCNTR(,RIICB)                                0256
         C     @07,@CF02486                                        0256
         BNL   @RF00256                                            0256
*     DO;                           /* ONLY AUDIT IF WON'T OVERLAY 0257
*                                      LENGTH PROVIDED       @Y021130*/
*       IIGMADDR=MAINADDR;          /* SAVE ADDR OF CORE     @Y021130*/
         LA    @05,IIAUD(,RIICB)                                   0258
         ALR   @05,@07                                             0258
         MVC   IIGMADDR(4,@05),MAINADDR(RCORE)                     0258
*       IIGMSPLG=MAINR0;            /* SUBPOOL/LENGTH        @Y021130*/
         MVC   IIGMSPLG(4,@05),MAINR0(RCORE)                       0259
*       IIGMCNTR=IIGMCNTR+IIGMINCR; /* POINT TO NEXT SLOT AVAILABLE
*                                                            @Y021130*/
         AL    @07,@CF00045                                        0260
         STH   @07,IIGMCNTR(,RIICB)                                0260
*     END;                          /* END AUDIT             @Y021130*/
*   RDCB=IIRSAVE3;                  /* RESTORE DCB POINTER           */
@RF00256 L     RDCB,IIRSAVE3(,RCORE)                               0262
*   RESPECIFY                                                      0263
*    (R8,                                                          0263
*     R9,                                                          0263
*     R10,                                                         0263
*     R11) UNRESTRICTED;                                           0263
*   R14=IIRSAVE2;                   /* RESTORE RETURN ADDRESS        */
         L     R14,IIRSAVE2(,RCORE)                                0264
*   RESPECIFY                                                      0265
*    (R0,                                                          0265
*     R15,                                                         0265
*     RRPL) UNRESTRICTED;           /*                       @ZA02670*/
*   END GETMAIN;                                                   0266
@EL00003 DS    0H                                                  0266
@EF00003 DS    0H                                                  0266
@ER00003 BR    @14                                                 0266
*                                                                  0267
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = INITIALIZE IICB                                       *
**       THIS ROUTINE PERFORMS BASIC INITIALIZATION OF THE IICB       *
**       ACB AND EXIT LIST.                                           *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 6 - RACB - ACB POINTER                              *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         ACBDORGA                                                   *
**         ACBEXFG                                                    *
**         ACBEXLST                                                   *
**         ACBID                                                      *
**         ACBKEY                                                     *
**         ACBPASSW                                                   *
**         EXLID                                                      *
**         EXLLEN                                                     *
**         IIACBPTR                                                   *
**         IICBID                                                     *
**         IICBLEN                                                    *
**         IIDCBPTR                                                   *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0267
*INITIICB:                                                         0267
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* INITIALIZE THE IICB           */
INITIICB DS    0H                                                  0268
*   IICBID='80'X;                   /* IICB ID X'80'                 */
         MVI   IICBID(RIICB),X'80'                                 0268
*   IICBLEN=LENGTH(IDAIICB);        /* LENGTH OF IICB                */
         LA    @15,168                                             0269
         STH   @15,IICBLEN(,RIICB)                                 0269
*   IIDCBPTR=RDCB;                  /* DCB POINTER INITIALIZE ACB    */
         ST    RDCB,IIDCBPTR(,RIICB)                               0270
*   RACB=RIICB+LENGTH(IDAIICB);     /* INIT ACB PTR                  */
         ALR   @15,RIICB                                           0271
         LR    RACB,@15                                            0271
*   IIACBPTR=RACB;                  /* ACB POINTER                   */
         ST    RACB,IIACBPTR(,RIICB)                               0272
*   ACBID='A0'X;                    /* SET ACB IDENTIFIER            */
         MVI   ACBID(RACB),X'A0'                                   0273
*   ACBLENG=LENGTH(IFGACB);         /* SET ACB LENGTH                */
         LA    @15,76                                              0274
         STH   @15,ACBLENG(,RACB)                                  0274
*   ACBDORGA=ON;                    /* SET DSORG ACB BIT             */
         OI    ACBDORGA(RACB),B'00001000'                          0275
*   ACBPASSW='';                    /* SET PASSWORD TO BLANKS        */
         SLR   @07,@07                                             0276
         ST    @07,ACBPASSW(,RACB)                                 0276
*   ACBEXLST=RACB+LENGTH(IFGACB);   /* INIT EXLST PTR IN ACB OFLGS -
*                                      LOCK BIT                      */
         ALR   @15,RACB                                            0277
         ST    @15,ACBEXLST(,RACB)                                 0277
*   ACBEXFG=ON;                     /* SET NO USER EXITS FLAG        */
         OI    ACBEXFG(RACB),B'00000010'                           0278
*   ACBKEY=ON;                      /* MACRF - KEY INITIALIZE EXIT 0279
*                                      LIST                          */
         OI    ACBKEY(RACB),B'10000000'                            0279
*   EXLID='81'X;                    /* SET EXLST IDENTIFIER          */
         MVI   EXLID(@15),X'81'                                    0280
*   EXLLEN=20;                      /* LENGTH OF EXLST-3 ENTRIES EXIT
*                                      FLAGS AND POINTERS ARE      0281
*                                      INITIALIZED IN THE LOADMOD  0281
*                                      ROUTINE                       */
         MVC   EXLLEN(2,@15),@CH02175                              0281
*   END INITIICB;                                                  0282
@EL00004 DS    0H                                                  0282
@EF00004 DS    0H                                                  0282
@ER00004 BR    @14                                                 0282
*                                                                  0283
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = ACB MERGE                                             *
**       THIS ROUTINE INITIALIZES THE REMAINING FIELDS THAT ARE       *
**       NEEDED TO OPEN THE ACB.  THE DDNAME IS MERGED FROM THE       *
**       TIOT AND THE MACRO FORMAT IS MAPPED FROM THE DCB.  ALL       *
**       OTHER FIELDS DEFAULT TO THE VALUES SPECIFIED DURING AMS      *
**       DEFINE AND SPECIFIED ON THE DD CARD.  II OPEN WAS ENTERED    *
**       WITH THE FOUNDATION PORTION OF THE DCB INITIALIZED TO THE    *
**       FORMAT AFTER OPEN.                                           *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 6 - RACB - ACB POINTER                              *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         ACBDDNM                                                    *
**         ACBMACR1                                                   *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0283
*ACBMERGE:                                                         0283
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0283
ACBMERGE DS    0H                                                  0284
*   RESPECIFY                                                      0284
*    (RTIOT) RESTRICTED;            /* LOCATE DDNAME IN TIOT         */
*   RTIOT=DXTIOTAD;                 /*                       @Y021130*/
         L     RTIOT,DXTIOTAD(,RCORE)                              0285
*   ACBDDNM=TIOEDDNM;               /* GET DDNAME FROM TIOT          */
         MVC   ACBDDNM(8,RACB),TIOEDDNM(RTIOT)                     0286
*   RESPECIFY                                                      0287
*     RTIOT UNRESTRICTED;                                          0287
*                                                                  0287
*   /*****************************************************************/
*   /*                                                               */
*   /* MACRO FORMAT MAPPING FROM THE DCB TO THE ACB                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0288
*                                   /* IF GET OR PUT                 */
*   IF DCBMFGET=ON×DCBMFPUT=ON THEN                                0288
         LA    @15,40                                              0288
         ALR   @15,RDCB                                            0288
         TM    DCBMFGET(@15),B'01000000'                           0288
         BO    @RT00288                                            0288
         TM    DCBMFPUT(@15),B'01000000'                           0288
         BNO   @RF00288                                            0288
@RT00288 DS    0H                                                  0289
*     ACBSEQ=ON;                    /* THEN SEQUENTIAL IF BISAM (READ
*                                      OR WIRTE)                     */
         OI    ACBSEQ(RACB),B'00010000'                            0289
*   IF DCBMFRD=ON×DCBMFWRT=ON THEN                                 0290
@RF00288 LA    @15,40                                              0290
         ALR   @15,RDCB                                            0290
