         MVC   DCBIIGET+48(3,RWORK2),SCANPTR+1(R8)                 0624
*       R8=R8+4;                                                   0625
         LA    @15,4                                               0625
         ALR   R8,@15                                              0625
*       DCBSETL=SCANPTR;            /* II SETL ENTRY                 */
         LA    @09,52                                              0626
         ALR   @09,RWORK2                                          0626
         L     @05,SCANPTR(,R8)                                    0626
         ST    @05,DCBSETL(,@09)                                   0626
*       R8=R8+4;                                                   0627
         ALR   R8,@15                                              0627
*       DCBESETL=SCANPTR;           /* II ESETL ENTRY                */
         L     @05,SCANPTR(,R8)                                    0628
         ST    @05,DCBESETL(,@09)                                  0628
*       R8=R8+4;                                                   0629
         ALR   R8,@15                                              0629
*       EXLEODP=SCANPTR;            /* II EODAD ENTRY-ACB EXLST      */
         L     @15,ACBEXLST(,RACB)                                 0630
         MVC   EXLEODP(4,@15),SCANPTR(R8)                          0630
*       RESPECIFY                                                  0631
*         R8 UNRESTRICTED;                                         0631
*       EXLEODF(1)=ON;              /* MARK EODAD PRESENT            */
*       EXLEODF(2)=ON;              /* MARK EODAD ACTIVE             */
         OI    EXLEODF(@15),B'11000000'                            0633
