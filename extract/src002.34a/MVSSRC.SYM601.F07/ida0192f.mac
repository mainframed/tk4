         LA    @05,15                                              0521
         AL    @05,MWANEXTC                                        0521
         ST    @05,MWANEXTC                                        0521
*         MWAPTRA=ADDR(JFCBXVOL);   /* PT TO FIRST VOL SER NO. IN  0522
*                                      JFCB EXTENSION                */
         L     @05,MWAJFCB                                         0522
         LA    @05,JFCBXVOL(,@05)                                  0522
         ST    @05,MWAPTRA                                         0522
