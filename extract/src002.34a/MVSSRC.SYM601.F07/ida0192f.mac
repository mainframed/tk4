*/********************************************************************/
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/
*/********************************************************************/
*                                                                  0283
*   GEN;                                                           0283
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED
*                                       IF NO ERROR WAS ENCOUNTERED
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA
         ST    @01,88(,@15)             STORE LENGTH OF AREA
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE
         B     GMN00200                 BYPASS SETTING ERROR
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.
*   RFY                                                            0284
*    (R1,                                                          0284
*     RETCODE) RSTD;                                               0284
*   IF RETCODE=0 THEN               /* IF GETMAIN ERROR              */
         LTR   RETCODE,RETCODE                                     0285
         BNZ   @RF00285                                            0285
*     DO;                           /* THEN CALL PROB DET            */
*       ACBERFLG=OPERR136;          /* SET ACB ERROR FLAG            */
         MVI   ACBERFLG(RACB),X'88'                                0287
*       PDPCODE=PP20;               /* SET PROB DET ERROR CODE       */
         L     @14,OPWCOMWA(,RCORE)                                0288
         MVI   PDPCODE+280(@14),X'14'                              0288
*       R1=OPWCOMWA;                /* PT TO COMMON WORK AREA        */
         LR    R1,@14                                              0289
*       RFY                                                        0290
*        (R1,                                                      0290
*         RETCODE) UNRSTD;                                         0290
*       RSTATRTN=IDAR192P;          /* INDIC 192P CONTROL    @Y30DSDP*/
         MVC   RSTATRTN(4,@14),@CC04587                            0291
*       CALL IDA0192P;              /* WRITE PROB DET MESSAGE        */
         L     @15,@CV04818                                        0292
         BALR  @14,@15                                             0292
*       RSTATRTN=IDAR192F;          /* INDIC 192F CONTROL    @Y30DSDP*/
         L     @05,OPWCOMWA(,RCORE)                                0293
         MVC   RSTATRTN(4,@05),@CC04581                            0293
*       OPWNOWRK=ON;                /* IND NO MODULE WORK AREA       */
         OI    OPWNOWRK(RCORE),B'00010000'                         0294
*       GOTO TERM192F;              /* RESTORE ACB AND EXIT MODULE   */
         B     TERM192F                                            0295
*     END;                                                         0296
*                                                                  0296
*   /*****************************************************************/
*   /*                                                               */
*   /* ESTABLISH MODULE ADDRESSABILITY                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0297
*   RFY                                                            0297
*     R15 RSTD;                                                    0297
@RF00285 DS    0H                                                  0298
*   GEN(ST R13,4(R15))                                             0298
*   REFS(R13,R15) NOSETS;           /* SAVE LAST SAVE AREA PTR       */
         ST R13,4(R15)
*   R13=R15;                        /* SET BASE FOR MOD WORK AREA    */
         LR    R13,R15                                             0299
*   RFY                                                            0300
*     R15 UNRSTD;                                                  0300
*   GEN(USING @DATD,R13)                                           0301
*   REFS(R13) NOSETS;                                              0301
*                                                                  0301
         USING @DATD,R13
*   /*****************************************************************/
*   /*                                                               */
*   /* MAIN ROUTINE                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0302
*   IF OPWDUMMY=OFF THEN            /* IF NOT DUMMY DATA SET         */
         TM    OPWDUMMY(RCORE),B'00010000'                         0302
         BNZ   @RF00302                                            0302
*     CALL VOLMNT;                  /* MOUNT AND VERIFY VOLUMES      */
         BAL   @14,VOLMNT                                          0303
*   IF OPWRSTRT=OFF THEN            /* IF NOT RESTART PROCESSING     */
@RF00302 TM    OPWRSTRT(RCORE),B'00001000'                         0304
         BNZ   @RF00304                                            0304
*     DO;                           /* THEN DO NORMAL OPEN           */
*       CALL OPNBASE;               /* OPEN BASE DS                  */
         BAL   @14,OPNBASE                                         0306
*       IF OPWDUMMY=OFF THEN        /* IF NOT DUMMY DS               */
         TM    OPWDUMMY(RCORE),B'00010000'                         0307
         BNZ   @RF00307                                            0307
*         DO;                       /* THEN DO UPGRADE PROCESSING    */
*           IF AMBLWRIT=ON&AMBLCINV=OFF THEN/* IF BASE OPEN FOR    0309
*                                      OUTPUT AND NOT CINV PROCESSING*/
         L     @05,MWAXAMBL                                        0309
         TM    AMBLWRIT(@05),B'00100000'                           0309
         BNO   @RF00309                                            0309
         TM    AMBLCINV(@05),B'01000000'                           0309
         BNZ   @RF00309                                            0309
*             IF OPWNOUPG>0&BIBUPT=0 THEN/* THEN IF UPGR SET EXISTS
*                                      AND NOT ALREADY OPEN          */
         CLI   OPWNOUPG(RCORE),0                                   0310
         BNH   @RF00310                                            0310
         L     @05,OPWBIB(,RCORE)                                  0310
         L     @05,BIBUPT(,@05)                                    0310
         LTR   @05,@05                                             0310
         BNZ   @RF00310                                            0310
*               CALL OPNUPGR;       /* THEN OPEN UPGRADE SET         */
         BAL   @14,OPNUPGR                                         0311
*           IF OPWPDTCI^=0 THEN     /* IF PATH PROCESSING            */
@RF00310 DS    0H                                                  0312
@RF00309 L     @05,OPWPDTCI-1(,RCORE)                              0312
         LA    @05,0(,@05)                                         0312
         LTR   @05,@05                                             0312
         BZ    @RF00312                                            0312
*             CALL OPNPATH;         /* THEN OPEN PATH                */
         BAL   @14,OPNPATH                                         0313
*         END;                                                     0314
@RF00312 DS    0H                                                  0315
*     END;                                                         0315
@RF00307 DS    0H                                                  0316
*TERM192F:                                                         0316
*                                                                  0316
*/********************************************************************/
*/*                                                                  */
*/*   FUNCTION: MODULE TERMINATION.  FREES MODULE WORK AREA AND SETS */
*/*             A RETURN CODE IN REGISTER 15.                        */
*/*                                                                  */
*/*   INPUTS:   1. PTR TO OPW IN RCORE.                              */
*/*                                                                  */
*/*   OUTPUTS:  1. IF NO ERRORS ARE DETECTED, REGISTER 15 IS SET TO  */
*/*                ZERO.  IF AN ERROR IS DETECTED, REGISTER 15 IS    */
*/*                SET TO EIGHT.                                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0316
*   RFY                                                            0316
*    (R0,                                                          0316
*     R1,                                                          0316
*     R6) RSTD;                                                    0316
@RF00304 DS    0H                                                  0316
TERM192F DS    0H                                                  0317
*   IF OPWNOWRK=OFF THEN            /* IF MODULE WORK AREA EXISTS    */
         TM    OPWNOWRK(RCORE),B'00010000'                         0317
         BNZ   @RF00317                                            0317
*     DO;                           /* THEN FREE MODULE WORK AREA    */
*       MWAGFSP=WKPOOL;             /* INIT SUBPOOL OF WORK AREA     */
         MVI   MWAGFSP,X'FC'                                       0319
*       GEN(L R0,@SIZDATD)                                         0320
*       SETS(R0);                   /* INIT SIZE                     */
         L R0,@SIZDATD
*       MWAGFLEN=R0;                /* LD LENGTH OF MWA              */
         STCM  R0,7,MWAGFLEN                                       0321
*       R1=R13;                     /* PT TO MODULE WORK AREA        */
         LR    R1,R13                                              0322
*       R0=MWAGFR0;                 /* LD LENGTH AND SUBPPOL OF MWA  */
         L     R0,MWAGFR0                                          0323
*       IF MWAOFAIL=ON THEN         /* IF OPEN FAILURE               */
         TM    MWAOFAIL,B'10000000'                                0324
         BNO   @RF00324                                            0324
*         R6=8;                     /* THEN IND OPEN FAILURE         */
         LA    R6,8                                                0325
*       ELSE                        /* NO OPEN FAILURE               */
*         R6=0;                     /* ELSE IND OPEN SUCCESSFUL      */
         B     @RC00324                                            0326
@RF00324 SLR   R6,R6                                               0326
*       GEN(L R13,4(R13))                                          0327
*       SETS(R13);                  /* PT TO LAST SAVE AREA          */
@RC00324 DS    0H                                                  0327
         L R13,4(R13)
*       GEN(FREEMAIN R,LV=(0),A=(1));/* FREE MODULE WORK AREA        */
         FREEMAIN R,LV=(0),A=(1)
*     END;                                                         0329
*   ELSE                            /* MODULE WORK AREA DOES NOT   0330
*                                      EXIST                         */
*     R6=8;                         /* ELSE IND OPEN FAILURE         */
         B     @RC00317                                            0330
@RF00317 LA    R6,8                                                0330
*   GEN REFS(R6,R13) SETS(R12,RET,RETCODE);                        0331
@RC00317 DS    0H                                                  0331
         ST    R6,16(R13)               STORE RETURN CODE
         LM    RET,R12,12(R13)          RESTORE REGISTERS
         BR    RET                      RETURN TO
*   RFY                                                            0332
*    (R0,                                                          0332
*     R1,                                                          0332
*     R6) UNRSTD;                                                  0332
*VOLMNT:                                                           0333
*                                                                  0333
*/********************************************************************/
*/*                                                                  */
*/*   FUNCTION: VOLUME MOUNT.  THIS ROUTINE SCANS THE TIOT ENTRY     */
*/*             FOR THE SPHERE.  FOR EACH DEVICE TYPE IN THE TIOT    */
*/*             ENTRY, A VOLUME LIST (VVOLL) IS BUILT.  IF THIS IS   */
*/*             A CONNECT OPEN AND A VMT EXISTS FOR THE DEVICE       */
*/*             TYPE ASSOCIATED WITH THE TIOT ENTRY, THEN CALL       */
*/*             OLDDEV TO MOUNT VOLUMES, OTHERWISE CALL NEWDEV       */
*/*             TO MOUNT VOLUMES.  IF THIS IS A PRIMARY OPEN, THEN   */
*/*             CALL NEWDEV TO MOUNT VOLUMES.                        */
*/*                                                                  */
*/*   INPUTS:   1. PTR TO TIOT ENTRY IN OPWTIOT.                     */
*/*                                                                  */
*/*   OUTPUTS:  1. VOLUMES MOUNTED FOR SPHERE AND VMT'S BUILT.       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0333
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0333
VOLMNT   DS    0H                                                  0334
*   PDPFUNC=PDF85;                  /* SET FUNCTION CODE             */
         L     @05,OPWCOMWA(,RCORE)                                0334
         MVI   PDPFUNC+280(@05),X'55'                              0334
*   RFY                                                            0335
*     RET RSTD;                                                    0335
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */
         ST    RET,MWASAVEA                                        0336
*   RFY                                                            0337
*     RET UNRSTD;                                                  0337
*   MWADSABP=DXDSABAD;              /* SET UP DSAB ADDR      @Y30DSDP*/
         L     @15,DXDSABAD(,@05)                                  0338
         ST    @15,MWADSABP                                        0338
*   MWATIOT=OPWTIOT;                /* PT TO TIOT ENTRY FOR DDNAME   */
         L     @15,OPWTIOT(,RCORE)                                 0339
         ST    @15,MWATIOT                                         0339
*   MWANEWSW=ON;                    /* IND NEW VVOLL NEEDED          */
         OI    MWANEWSW,B'00001000'                                0340
*   MWAJFCB=ADDR(DXJBF);            /* PT TO JFCB IN COMMON WRK AREA */
         LA    @05,DXJBF(,@05)                                     0341
         ST    @05,MWAJFCB                                         0341
*   DO UNTIL(MWADSABP=0×TIOEDDNM^='        ');/* SCAN TIOT ENTRY FOR
*                                      ALL DEVICE TYPES      @Z30SSDP*/
@DL00342 DS    0H                                                  0343
*     IF MWANEWSW=ON THEN           /* IF NEW VVOLL NEEDED           */
         TM    MWANEWSW,B'00001000'                                0343
         BNO   @RF00343                                            0343
*       DO;                         /* THEN BUILD NEW VOLUME LIST  0344
*                                      (VVOLL) AND INITIALIZE FIXED
*                                      PART OF HEADER                */
*         MWAGFLEN=LENGTH(IDAVVOLL)+LENGTH(VVOLLENT)*JFCBNVOL;/* CALC
*                                      LENGTH OF VVOLL               */
         L     @05,MWAJFCB                                         0345
         SLR   @15,@15                                             0345
         IC    @15,JFCBNVOL(,@05)                                  0345
         MH    @15,@CH00033                                        0345
         LA    @15,36(,@15)                                        0345
         STCM  @15,7,MWAGFLEN                                      0345
*         MWAGFSP=WKPOOL;           /* INIT SUBPOOL                  */
         MVI   MWAGFSP,X'FC'                                       0346
*         CALL GETCORE;             /* OBTAIN STORAGE FOR VVOLL      */
         BAL   @14,GETCORE                                         0347
*         MWAVVOLL=MWAGFADR;        /* INIT PTR TO VVOLL             */
         L     @05,MWAGFADR                                        0348
         ST    @05,MWAVVOLL                                        0348
*         MWAVVLNG=MWAGFLEN;        /* SAVE LENGTH OF VVOLL          */
         MVC   MWAVVLNG(3),MWAGFLEN                                0349
*         MWAVVNBR=JFCBNVOL;        /* INIT MAX NO. OF ENTRIES IN  0350
*                                      VVOLL                         */
         L     @01,MWAJFCB                                         0350
         SLR   @00,@00                                             0350
         IC    @00,JFCBNVOL(,@01)                                  0350
         STH   @00,MWAVVNBR                                        0350
*         VVOLLID=ON;               /* IND VOLUME LIST               */
*         VVOLLOPE=ON;              /* IND CALLER IS OPEN            */
         OI    VVOLLID(@05),B'01010000'                            0352
*         IF OPWSCRA^=ON&OPWUCRA^=ON&OPWCAT^=ON THEN/* IF NOT      0353
*                                      CATALOG, SCRA OR UCRA @XM02259*/
         TM    OPWSCRA(RCORE),B'01000000'                          0353
         BO    @RF00353                                            0353
         TM    OPWUCRA(RCORE),B'10000000'                          0353
         BO    @RF00353                                            0353
         TM    OPWCAT(RCORE),B'10000000'                           0353
         BO    @RF00353                                            0353
*           VVOLLACB=OPWCACB;       /* INIT PTR TO CATALOG ACB       */
         L     @15,OPWCACB(,RCORE)                                 0354
         ST    @15,VVOLLACB(,@05)                                  0354
*         MWANEWSW=OFF;             /* RESET NEW VVOLL NEEDED INDR   */
@RF00353 NI    MWANEWSW,B'11110111'                                0355
*       END;                                                       0356
*                                                                  0356
*     /***************************************************************/
*     /*                                                             */
*     /* INITIALIZE VARIABLE PART OF VVOLL HEADER                    */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0357
*     VVOLLNBR=JFCBNVOL;            /* INIT NO. OF VOLUME ENTRIES    */
@RF00343 L     @05,MWAVVOLL                                        0357
         L     @01,MWAJFCB                                         0357
         SLR   @00,@00                                             0357
         IC    @00,JFCBNVOL(,@01)                                  0357
         STH   @00,VVOLLNBR(,@05)                                  0357
*     VVOLLTAB=MWATIOT;             /* INIT PTR TO TIOT ENTRY        */
         L     @15,MWATIOT                                         0358
         ST    @15,VVOLLTAB(,@05)                                  0358
*     VVOLLERR=0;                   /* ZERO RETURN CODE              */
         SLR   @14,@14                                             0359
         ST    @14,VVOLLERR(,@05)                                  0359
*     VVOLLSFI=VVOLLSFI&&VVOLLSFI;  /* ZERO SUB-FUNCTION INFORMATION */
         XC    VVOLLSFI(12,@05),VVOLLSFI(@05)                      0360
*     MWADEV=UCBTYP(2:4);           /* LD DEVICE TYPE FROM UCB       */
         L     @05,TIOEFSRT-1(,@15)                                0361
         LA    @05,0(,@05)                                         0361
         MVC   MWADEV(3),UCBTYP+1(@05)                             0361
*     MWADVOP=MWADVOP&DVOPMASK;     /* RESET UNNECESSARY BITS IN   0362
*                                      OPTION BYTE                   */
         NI    MWADVOP,B'00001000'                                 0362
*     IF UCB2OPT4=ON THEN           /* IF VIRTUAL UCB                */
         TM    UCB2OPT4(@05),B'00001000'                           0363
         BNO   @RF00363                                            0363
*       BIBVIRT=ON;                 /* THEN IND VIRT UCB'S IN SPHERE */
         L     @05,OPWBIB(,RCORE)                                  0364
         OI    BIBVIRT(@05),B'10000000'                            0364
*     IF OPWBCON^=0 THEN            /* IF CONNECTING                 */
@RF00363 L     @05,OPWBCON(,RCORE)                                 0365
         LTR   @05,@05                                             0365
         BZ    @RF00365                                            0365
*       DO;                         /* THEN SEARCH VMT CHAIN FOR LIKE
*                                      DEVICE TYPE                   */
*         MWAVMT=BIBVMT;            /* PT TO FIRST VMT               */
         L     @05,OPWBIB(,RCORE)                                  0367
         L     @05,BIBVMT(,@05)                                    0367
         ST    @05,MWAVMT                                          0367
*         DO WHILE(MWAVMT^=0&MWADEV^=VMTDEV);/* SCAN VMT'S FOR LIKE
*                                      DEVICE TYPE                   */
         B     @DE00368                                            0368
@DL00368 DS    0H                                                  0369
*           MWAVMT=VMTNXT;          /* PT TO NEXT VMT ON CHAIN       */
         L     @05,MWAVMT                                          0369
         L     @05,VMTNXT(,@05)                                    0369
         ST    @05,MWAVMT                                          0369
*         END;                                                     0370
@DE00368 L     @05,MWAVMT                                          0370
         LTR   @05,@05                                             0370
         BZ    @DC00368                                            0370
         CLC   MWADEV(3),VMTDEV(@05)                               0370
         BNE   @DL00368                                            0370
@DC00368 DS    0H                                                  0371
*         IF MWAVMT^=0 THEN         /* IF LIKE DEVICE TYPE FOUND     */
         L     @05,MWAVMT                                          0371
         LTR   @05,@05                                             0371
         BZ    @RF00371                                            0371
*           CALL OLDDEV;            /* MOUNT VOLUMES FOR EXISTING  0372
*                                      DEVICE TYPE                   */
         BAL   @14,OLDDEV                                          0372
*         ELSE                      /* LIKE DEVICE TYPE NOT FOUND    */
*           CALL NEWDEV;            /* MOUNT VOLUMES FOR NEW DEVICE
*                                      TYPE                          */
         B     @RC00371                                            0373
@RF00371 BAL   @14,NEWDEV                                          0373
*       END;                                                       0374
*     ELSE                          /* NOT CONNECTING                */
*       CALL NEWDEV;                /* NOT CONNECTING. MOUNT VOLUMES
*                                      FOR NEW DEVICE TYPE           */
         B     @RC00365                                            0375
@RF00365 BAL   @14,NEWDEV                                          0375
*     IF MWADSABP^=0 THEN           /* IF THERE IS A DSAB POINTER    */
@RC00365 L     @05,MWADSABP                                        0376
         SLR   @15,@15                                             0376
         CR    @05,@15                                             0376
         BE    @RF00376                                            0376
*       DO;                         /* CHECK FOR CONCATENATION       */
*         MWADSABP=DSABFCHN;        /* UPDATE DSAB PTR       @Z30SSDP*/
         L     @05,DSABFCHN(,@05)                                  0378
         ST    @05,MWADSABP                                        0378
*         MWATIOT=DSABTIOT;         /* UPDATE TIOT ENTRY PTR @Z30SSDP*/
         L     @14,DSABTIOT(,@05)                                  0379
         ST    @14,MWATIOT                                         0379
*         IF MWADSABP^=0&           /* CHK FOR CONCATENATION @Y30DSDP*/
*             TIOEDDNM='        ' THEN/* TIOT DD NAME IS BLANK     0380
*                                                            @Z30SSRL*/
         CR    @05,@15                                             0380
         BE    @RF00380                                            0380
         CLC   TIOEDDNM(8,@14),@CC04828                            0380
         BNE   @RF00380                                            0380
*           DO;                     /* YES,                  @Z30SSDP*/
*             MWAJFTTR(1:3)=TIOEJFCB;/* LD TTR OF NEXT JFCB          */
         MVC   MWAJFTTR(3),TIOEJFCB(@14)                           0382
*             CALL READJFCB;        /* READ NEXT JFCB                */
         BAL   @14,READJFCB                                        0383
*             IF MWAVVNBR<JFCBNVOL THEN/* IF OLD VVOLL NOT LARGE   0384
*                                      ENOUGH                        */
         LH    @05,MWAVVNBR                                        0384
         L     @01,MWAJFCB                                         0384
         SLR   @00,@00                                             0384
         IC    @00,JFCBNVOL(,@01)                                  0384
         CR    @05,@00                                             0384
         BNL   @RF00384                                            0384
*               DO;                 /* THEN FREE OLD VVOLL           */
*                 MWAGFADR=MWAVVOLL;/* POINT TO VVOLL                */
         L     @05,MWAVVOLL                                        0386
         ST    @05,MWAGFADR                                        0386
*                 MWAGFSP=WKPOOL;   /* INIT SUBPOOL                  */
         MVI   MWAGFSP,X'FC'                                       0387
*                 MWAGFLEN=MWAVVLNG;/* INIT LENGTH                   */
         MVC   MWAGFLEN(3),MWAVVLNG                                0388
*                 CALL FREECORE;    /* FREE OLD VVOLL                */
         BAL   @14,FREECORE                                        0389
*                 MWANEWSW=ON;      /* IND NEW VVOLL NEEDED          */
         OI    MWANEWSW,B'00001000'                                0390
*               END;                                               0391
*           END;                                                   0392
@RF00384 DS    0H                                                  0393
*       END;                                                       0393
@RF00380 DS    0H                                                  0394
*   END;                                                           0394
@RF00376 DS    0H                                                  0394
@DE00342 L     @05,MWADSABP                                        0394
         LTR   @05,@05                                             0394
         BZ    @DC00342                                            0394
         L     @05,MWATIOT                                         0394
         CLC   TIOEDDNM(8,@05),@CC04828                            0394
         BE    @DL00342                                            0394
@DC00342 DS    0H                                                  0395
*   MWAGFADR=MWAVVOLL;              /* PT TO VVOLL                   */
         L     @05,MWAVVOLL                                        0395
         ST    @05,MWAGFADR                                        0395
*   MWAGFSP=WKPOOL;                 /* INIT SUBPOOL OF CURRENT VVOLL */
         MVI   MWAGFSP,X'FC'                                       0396
*   MWAGFLEN=MWAVVLNG;              /* INIT LENGTH OF CURRENT VVOLL  */
         MVC   MWAGFLEN(3),MWAVVLNG                                0397
*   CALL FREECORE;                  /* FREE CURRENT VVOLL            */
         BAL   @14,FREECORE                                        0398
*   IF MWAJFBSW=ON THEN             /* IF JFCB WORK AREA OBTAINED    */
         TM    MWAJFBSW,B'00000010'                                0399
         BNO   @RF00399                                            0399
*     DO;                           /* THEN FREE IT                  */
