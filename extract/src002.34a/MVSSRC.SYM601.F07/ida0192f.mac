@RF00515 LA    MWAJ,1                                              0524
         B     @DE00524                                            0524
@DL00524 LR    @15,MWAJ                                            0524
         SLA   @15,4                                               0524
         L     @01,MWAPTRA                                         0524
         L     @14,MWAVMT                                          0524
         ALR   @14,@15                                             0524
         CLC   VOLSER(6,@01),VMTVLSER-16(@14)                      0524
         BE    @DC00524                                            0524
