*/********************************************************************/
*/*                                                                  */
*/*   FUNCTION: UPDATE SWAP SAVE LIST (SSL).  DETERMINES IF THERE IS */
*/*             AVAILABLE ENTRY IN SSL.  IF THERE IS AN ENTRY, THE   */
*/*             CONTENTS OF MWASWSAV ARE PLACED IN THE ENTRY AND THE */
*/*             SSLACEN FIELD IS UPDATED.  IF THER IS NOT AN         */
*/*             AVAILABLE ENTRY, A NEW SSL IS BUILT AND INITIALIZED, */
*/*             THE CONTENTS OF MWASWSAV ARE LOADED INTO THE FIRST   */
*/*             ENTRY, AND THE NEW SSL IS ADDED TO THE SSL CHAIN.    */
*/*                                                                  */
*/*   INPUTS:   1. CONTENTS OF NEW SSL ENTRY IN MWASWSAV.            */
*/*             2. PTR TO SSL CHAIN IN OPWSSL.                       */
*/*                                                                  */
*/*   OUTPUTS:  1. NEW ENTRY ADDED TO SSL.                           */
*/*                                                                  */
*/********************************************************************/
*                                                                  1004
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1004
UPDSSL   DS    0H                                                  1005
*   RFY                                                            1005
*     RET RSTD;                                                    1005
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */
         ST    RET,MWASAVEB                                        1006
*   RFY                                                            1007
*     RET UNRSTD;                                                  1007
*   IF SSLACEN>=DIM(SSLENTRY) THEN  /* IF SSL FULL                   */
         L     @05,OPWSSL(,RCORE)                                  1008
         LH    @05,SSLACEN(,@05)                                   1008
         C     @05,@CF00050                                        1008
         BL    @RF01008                                            1008
*     DO;                           /* THEN BUILD A NEW SSL          */
*       MWAGFSP=WKPOOL;             /* INIT SUBPOOL                  */
         MVI   MWAGFSP,X'FC'                                       1010
*       MWAGFLEN=LENGTH(IDASSL);    /* INIT LENGTH                   */
         MVC   MWAGFLEN(3),@CF01418+1                              1011
*       CALL GETCORE;               /* OBTAIN STORAGE FOR SSL        */
         BAL   @14,GETCORE                                         1012
*       RFY                                                        1013
*         IDASSL BASED(MWAGFADR);                                  1013
*       SSLNXPTR=OPWSSL;            /* PT TO FIRST SSL ON CHAIN      */
         L     @05,MWAGFADR                                        1014
         L     @15,OPWSSL(,RCORE)                                  1014
         ST    @15,SSLNXPTR(,@05)                                  1014
*       SSLSUBPL=WKPOOL;            /* INIT SUBPOOL IN SSL           */
         MVI   SSLSUBPL(@05),X'FC'                                 1015
*       SSLLENTH=MWAGFLEN;          /* INIT LENGTH OF SSL            */
         MVC   SSLLENTH(3,@05),MWAGFLEN                            1016
*       SSLID=IDASSLID;             /* INIT EBCDIC ID                */
         MVC   SSLID(8,@05),@CC02838                               1017
*       OPWSSL=MWAGFADR;            /* ADD SSL TO CHAIN              */
         ST    @05,OPWSSL(,RCORE)                                  1018
*       RFY                                                        1019
*         IDASSL BASED(OPWSSL);                                    1019
*     END;                                                         1020
*   SSLACEN=SSLACEN+1;              /* INCR NO. OF ACTIVE ENTRIES    */
@RF01008 L     @05,OPWSSL(,RCORE)                                  1021
         LA    @15,1                                               1021
         AH    @15,SSLACEN(,@05)                                   1021
         STH   @15,SSLACEN(,@05)                                   1021
*   SSLENTRY(SSLACEN)=MWASWSAV;     /* ADD NEW ENTRY TO SSL          */
         SLA   @15,3                                               1022
         ALR   @05,@15                                             1022
         MVC   SSLENTRY-8(8,@05),MWASWSAV                          1022
*   RFY                                                            1023
*     RET RSTD;                                                    1023
*   RET=MWASAVEB;                   /* RESTORE RETURN ADDRESS        */
         L     RET,MWASAVEB                                        1024
*   RFY                                                            1025
*     RET UNRSTD;                                                  1025
*   END UPDSSL;                                                    1026
@EL00009 DS    0H                                                  1026
