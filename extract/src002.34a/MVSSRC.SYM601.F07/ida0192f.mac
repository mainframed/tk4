@RF00550 LA    @05,280                                             0552
         AL    @05,OPWCOMWA(,RCORE)                                0552
         CLI   PDPSFC(@05),0                                       0552
         BNE   @RF00552                                            0552
         L     @15,MWAVVOLL                                        0552
         L     @14,SFIRETCD+16(,@15)                               0552
         LTR   @14,@14                                             0552
         BZ    @RF00552                                            0552
