@DE00825 L     @15,MWAPTRA                                         0830
         LTR   @15,@15                                             0830
         BNZ   @DL00825                                            0830
*     END;                                                         0831
@RF00824 LA    @15,1                                               0831
         AL    @15,MWAI                                            0831
@DE00822 ST    @15,MWAI                                            0831
         L     @14,MWAXAMBL                                        0831
         L     @14,AMBLCMB(,@14)                                   0831
         CH    @15,CMBCNT(,@14)                                    0831
         BNH   @DL00822                                            0831
*   MWAK=MWAK+1;                    /* INCR PSL ENTRY INIT CTR       */
@RF00821 AL    MWAK,@CF00053                                       0832
*   PSLSTAD(MWAK)=MWAXAMBL;         /* LD STARTING ADDR OF AMBL      */
         L     @15,MWAXAMBL                                        0833
         LR    @14,MWAK                                            0833
         SLA   @14,3                                               0833
         L     @11,OPWPSL(,RCORE)                                  0833
