         L     @05,MWAJFCB                                         0518
         MVC   MWAJFTTR(3),JFCBEXAD(@05)                           0518
*         ELSE                      /* ELSE PROCESSING EXTENSION     */
*           MWAJFTTR(1:3)=JFCBXTTR; /* SET DISK ADDR FOR NEXT JFCB 0519
