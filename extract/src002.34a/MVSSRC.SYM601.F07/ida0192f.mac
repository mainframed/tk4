*/*                                                                  */
*/*   FUNCTION: PROBLEM DETERMINATION INTERFACE.  INITIALIZES THE    */
*/*             CLUSTER NAME IN THE PROBLEM DETERMINATION PARAMETER  */
*/*             LIST (IDAPDPRM) TO THE DSNAME SPECIFIED ON THE DD    */
*/*             STATEMENT FOR THE SPHERE.  THE ACBERFLG FIELD IN THE */
*/*             USER ACB IS INITIALIZED TO THE CODE CORRESPONDING    */
*/*             TO THE PROBLEM DETERMINATION ERROR CODE (PPCODE).    */
*/*             THE ACBERFLG IS SET BY REFERENCING THE ARRAY         */
*/*             OPWERMAP THAT MAPS PPCODE TO ACBERFLG.  THIS ARRAY   */
*/*             IS  DEFINED BY IDAERMAP.  IDA0192P IS CALLED         */
*/*             TO WRITE THE PROBLEM DETERMINATION MESSAGE.          */
*/*                                                                  */
*/*   INPUTS:   1. PTR TO FORCORE WHICH CONTAINS THE PROBLEM         */
*/*               DETERMINATION PARAMETER LIST IN OPWCOMWA.          */
*/*             2. PTR TO USER ACB IN OPWUACB.                       */
*/*                                                                  */
*/*   OUTPUTS:  1. IF ERROR (ACBERFLG>=128), THEN MWAOFAIL IS SET    */
*/*                ON AND CONTROL IS GIVEN TO TERM192A.              */
*/*             2. IF WARNING (ACBERFLG<128), THEN CONTROL IS GIVEN  */
*/*                TO CALLER.                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  1408
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1408
PROBDT   DS    0H                                                  1409
*   RFY                                                            1409
*     RET RSTD;                                                    1409
*   MWASAVEF=RET;                   /* SAVE RETURN ADDRESS           */
         ST    RET,MWASAVEF                                        1410
*   RFY                                                            1411
*     RET UNRSTD;                                                  1411
*   RACB=OPWUACB;                   /* PT TO USER ACB                */
         L     RACB,OPWUACB(,RCORE)                                1412
*   ACBERFLG=OPWERMAP(PDPCODE);     /* SET ACBERFLG IN USER ACB      */
         L     @05,OPWCOMWA(,RCORE)                                1413
         SLR   @01,@01                                             1413
         IC    @01,PDPCODE+280(,@05)                               1413
         ALR   @01,RCORE                                           1413
         MVC   ACBERFLG(1,RACB),OPWERMAP-1(@01)                    1413
*   RFY                                                            1414
*     R1 RSTD;                                                     1414
*   R1=OPWCOMWA;                    /* POINT TO FORCORE              */
         LR    R1,@05                                              1415
*   RFY                                                            1416
*     R1 UNRSTD;                                                   1416
*   RSTATRTN=IDAR192P;              /* INDIC 192P CONTROL    @Y30DSDP*/
         MVC   RSTATRTN(4,@05),@CC04587                            1417
*   CALL IDA0192P;                  /* WRITE PROB DET MESSAGE        */
         L     @15,@CV04818                                        1418
         BALR  @14,@15                                             1418
*   RSTATRTN=IDAR192F;              /* INDIC 192F CONTROL    @Y30DSDP*/
         L     @05,OPWCOMWA(,RCORE)                                1419
         MVC   RSTATRTN(4,@05),@CC04581                            1419
*   IF ACBERFLG>=128 THEN           /* IF TERMINATING ERROR          */
         CLI   ACBERFLG(RACB),128                                  1420
         BL    @RF01420                                            1420
*     DO;                           /* THEN TERMINATE OPEN           */
*       MWAOFAIL=ON;                /* IND OPEN FAILURE              */
         OI    MWAOFAIL,B'10000000'                                1422
*       RETURN TO TERM192F;         /* GOTO TERMINATE OPEN           */
         LA    @14,TERM192F                                        1423
@EL00018 DS    0H                                                  1423
@EF00018 DS    0H                                                  1423
@ER00018 BR    @14                                                 1423
