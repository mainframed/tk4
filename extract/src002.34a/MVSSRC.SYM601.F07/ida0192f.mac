*/*   INPUTS:   1. PTR TO AMBL IN MWAXAMBL.                          */
*/*                                                                  */
*/*   OUTPUTS:  1. NEW ENTRY ADDED TO VAT FOR AMBL AND AMBL CHECK    */
*/*                INFORMATION INITIALIZED IN AMBL.                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  1163
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1163
VATUPD   DS    0H                                                  1164
*   RFY                                                            1164
*     RET RSTD;                                                    1164
*   MWASAVEC=RET;                   /* SAVE RETURN ADDRESS           */
         ST    RET,MWASAVEC                                        1165
*   RFY                                                            1166
*     RET UNRSTD;                                                  1166
*   RFY                                                            1167
*     IDAVAT BASED(MWAPTRA);                                       1167
*   RFY                                                            1168
*    (R6,                                                          1168
*     R7) RSTD;                                                    1168
*   R6=JSCBSHR;                     /* SAVE JSCBSHR                  */
         L     @05,OPWJSTCB(,RCORE)                                1169
         L     @05,TCBJSCB(,@05)                                   1169
         L     @15,JSCBSHR(,@05)                                   1169
         LR    R6,@15                                              1169
*   IF(JSCBSHR=0×VATCBSTP^=JSCBSTEP) THEN/* IF VAT DOESN'T EXIST     */
         LTR   @15,@15                                             1170
         BZ    @RT01170                                            1170
         CLC   VATCBSTP+196(1,@05),JSCBSTEP(@05)                   1170
         BE    @RF01170                                            1170
@RT01170 DS    0H                                                  1171
*     DO;                           /* THEN BUILD FIRST VAT          */
*       MWAGFSP=VATPOOL;            /* INIT SUBPOOL OF VAT           */
         MVI   MWAGFSP,X'FC'                                       1172
*       MWAGFLEN=LENGTH(IDAVAT);    /* INIT LENGTH OF VAT            */
         MVC   MWAGFLEN(3),@CF00034+1                              1173
*       CALL GETCORE;               /* OBTAIN STORAGE FOR VAT        */
         BAL   @14,GETCORE                                         1174
*       MWAGFADR->VATID=IDXVAT;     /* INIT CB ID            @OZ03937*/
         L     @05,MWAGFADR                                        1175
         MVI   VATID(@05),B'00010001'                              1175
*       MWAGFADR->VATLEN=MWAGFLEN;  /* INIT LENGTH           @OZ03937*/
         L     @15,MWAGFLEN-1                                      1176
         LA    @15,0(,@15)                                         1176
         STH   @15,VATLEN(,@05)                                    1176
*       MWAGFADR->VATVRT=0;         /* IND FIRST VAT         @OZ03937*/
         MVI   VATVRT(@05),X'00'                                   1177
*       MWAGFADR->VATENO=DIM(VATAMBL);/* INIT NO. OF ENTRIES @OZ03937*/
         MVI   VATENO(@05),X'10'                                   1178
*       ADDR(MWAGFADR)->VATCBSTP=JSCBSTEP;/* CURRENT STEP NO @OZ03937*/
         LA    @05,MWAGFADR                                        1179
         L     @01,OPWJSTCB(,RCORE)                                1179
         L     @15,TCBJSCB(,@01)                                   1179
         IC    @14,JSCBSTEP(,@15)                                  1179
         STC   @14,VATCBSTP(,@05)                                  1179
*       R7=ADDR(MWAGFADR)->IDAVATCB;/* INIT SWAP REG         @ZA07697*/
         L     R7,IDAVATCB(,@05)                                   1180
*       CS(R6,R7,JSCBSHR);          /* COMPARE AND SWAP VAT PTR      */
         CS    R6,@07,JSCBSHR(@15)                                 1181
*       IF R7^=JSCBSHR THEN         /* IF COMPARE AND SWAP FAILED    */
         C     R7,JSCBSHR(,@15)                                    1182
         BE    @RF01182                                            1182
*         CALL FREECORE;            /* THEN FREE NEW VAT             */
         BAL   @14,FREECORE                                        1183
*     END;                                                         1184
@RF01182 DS    0H                                                  1185
*   MWAVATSW=OFF;                   /* RESET VAT ENTRY FOUND SW      */
@RF01170 NI    MWAVATSW,B'11101111'                                1185
*   DO UNTIL(MWAVATSW=ON);                                         1186
@DL01186 DS    0H                                                  1187
*     MWAPTRA=JSCBSHR;              /* INIT PTR TO FIRST VAT         */
         L     @05,OPWJSTCB(,RCORE)                                1187
         L     @05,TCBJSCB(,@05)                                   1187
         L     @05,JSCBSHR(,@05)                                   1187
         ST    @05,MWAPTRA                                         1187
*     MWAI=1;                       /* INIT PTR TO FIRST ENTRY       */
         MVC   MWAI(4),@CF00053                                    1188
*     DO UNTIL(MWAPTRA=0×VATAMBL(MWAI)=0);/* SEARCH VAT CHAIN FOR  1189
*                                      INACTIVE ENTRY                */
@DL01189 DS    0H                                                  1190
*       IF VATNAE<VATENO THEN       /* IF THIS VAT NOT FULL          */
         L     @05,MWAPTRA                                         1190
         L     @15,VATNAE(,@05)                                    1190
         SLR   @14,@14                                             1190
         IC    @14,VATENO(,@05)                                    1190
         CR    @15,@14                                             1190
         BNL   @RF01190                                            1190
*         DO;                       /* THEN SEARCH ENTRIES           */
*           DO MWAI=1 TO VATENO WHILE(VATAMBL(MWAI)^=0);/* SEARCH FOR
*                                      INACTIVE ENTRY                */
         LA    @05,1                                               1192
         B     @DE01192                                            1192
@DL01192 LR    @15,@05                                             1192
         SLA   @15,2                                               1192
         L     @01,MWAPTRA                                         1192
         L     @15,VATAMBL-4(@15,@01)                              1192
         LTR   @15,@15                                             1192
         BZ    @DC01192                                            1192
*           END;                                                   1193
         AL    @05,@CF00053                                        1193
@DE01192 ST    @05,MWAI                                            1193
         L     @01,MWAPTRA                                         1193
         SLR   @00,@00                                             1193
         IC    @00,VATENO(,@01)                                    1193
         CR    @05,@00                                             1193
         BNH   @DL01192                                            1193
@DC01192 DS    0H                                                  1194
*           IF MWAI>VATENO THEN     /* IF INACTIVE ENTRY NOT FOUND   */
         L     @05,MWAPTRA                                         1194
         L     @15,MWAI                                            1194
         SLR   @14,@14                                             1194
         IC    @14,VATENO(,@05)                                    1194
         CR    @15,@14                                             1194
         BNH   @RF01194                                            1194
*             DO;                                                  1195
*               MWAPTRB=MWAPTRA;    /* SAVE PTR TO LAST VAT          */
         ST    @05,MWAPTRB                                         1196
*               MWAPTRA=VATNEXT;    /* POINT TO NEXT VAT             */
         L     @05,VATNEXT(,@05)                                   1197
         ST    @05,MWAPTRA                                         1197
*               MWAI=1;             /* INIT INDEX TO FIRST ENTRY     */
         MVC   MWAI(4),@CF00053                                    1198
*             END;                                                 1199
*         END;                                                     1200
*       ELSE                        /* ELSE VAT IS FULL              */
*         DO;                                                      1201
         B     @RC01190                                            1201
@RF01190 DS    0H                                                  1202
*           MWAPTRB=MWAPTRA;        /* SAVE PTR TO LAST VAT          */
         L     @05,MWAPTRA                                         1202
         ST    @05,MWAPTRB                                         1202
*           MWAPTRA=VATNEXT;        /* POINT TO NEXT VAT             */
         L     @05,VATNEXT(,@05)                                   1203
         ST    @05,MWAPTRA                                         1203
*           MWAI=1;                 /* INIT INDEX TO FIRST ENTRY     */
         MVC   MWAI(4),@CF00053                                    1204
*         END;                                                     1205
*     END;                                                         1206
@RC01190 DS    0H                                                  1206
@DE01189 L     @05,MWAPTRA                                         1206
         SLR   @15,@15                                             1206
         CR    @05,@15                                             1206
         BE    @DC01189                                            1206
         L     @14,MWAI                                            1206
         SLA   @14,2                                               1206
         C     @15,VATAMBL-4(@14,@05)                              1206
         BNE   @DL01189                                            1206
@DC01189 DS    0H                                                  1207
*     IF MWAPTRA^=0 THEN            /* IF INACTIVE ENTRY FOUND       */
         L     @05,MWAPTRA                                         1207
         SLR   @15,@15                                             1207
         CR    @05,@15                                             1207
         BE    @RF01207                                            1207
*       DO;                         /* THEN ADD ENTRY TO VAT         */
*         R6=0;                     /* ZERO COMPARE WORD             */
         SLR   R6,R6                                               1209
*         CS(R6,MWAXAMBL,VATAMBL(MWAI));/* COMPARE AND SWAP AMBL PTR */
         L     @14,MWAXAMBL                                        1210
         LR    @11,@14                                             1210
         L     @10,MWAI                                            1210
         LR    @09,@10                                             1210
         SLA   @09,2                                               1210
         ALR   @09,@05                                             1210
         CS    R6,@11,VATAMBL-4(@09)                               1210
*         IF R6=0 THEN              /* IF CS SUCCESSFUL              */
         CR    R6,@15                                              1211
         BNE   @RF01211                                            1211
*           DO;                     /* THEN INIT VAT INFO IN AMBL    */
*             AMBLVRT=VATVRT;       /* INIT TABLE NUMBER             */
         IC    @15,VATVRT(,@05)                                    1213
         STC   @15,AMBLVRT(,@14)                                   1213
*             AMBLENO=MWAI;         /* INIT ENTRY NUMBER             */
         STC   @10,AMBLENO(,@14)                                   1214
*             R6=VATNAE;            /* INIT COMPARE WORD TO NUMBER OF
*                                      ACTIVE ENTRIES                */
         L     R6,VATNAE(,@05)                                     1215
*             DO UNTIL(R7=R6+1);    /* UPDATE NO. OF ENTRIES IN VAT  */
@DL01216 DS    0H                                                  1217
*               R7=R6+1;            /* INIT SWAP WORD WITH         1217
*                                      INCREMENTED COUNTER           */
         LA    R7,1                                                1217
         ALR   R7,R6                                               1217
*               CS(R6,R7,VATNAE);   /* COMPARE AND SWAP COUNTER      */
         L     @05,MWAPTRA                                         1218
         CS    R6,@07,VATNAE(@05)                                  1218
*             END;                                                 1219
@DE01216 LA    @05,1                                               1219
         ALR   @05,R6                                              1219
         CR    R7,@05                                              1219
         BNE   @DL01216                                            1219
*             MWAVATSW=ON;          /* IND AMBL ENTRY ADDED TO VAT   */
         OI    MWAVATSW,B'00010000'                                1220
*           END;                                                   1221
*       END;                                                       1222
*     ELSE                          /* INACTIVE ENTRY NOT FOUND      */
*       DO;                         /* BUILD NEW VAT                 */
         B     @RC01207                                            1223
@RF01207 DS    0H                                                  1224
*         MWAPTRA=MWAPTRB;          /* PT TO LAST VAT ON CHAIN       */
         L     @05,MWAPTRB                                         1224
         ST    @05,MWAPTRA                                         1224
*         MWAGFSP=VATPOOL;          /* INIT SUBPOOL OF VAT           */
         MVI   MWAGFSP,X'FC'                                       1225
*         MWAGFLEN=LENGTH(IDAVAT);  /* INIT LENGTH OF VAT            */
         MVC   MWAGFLEN(3),@CF00034+1                              1226
*         CALL GETCORE;             /* OBTAIN STORAGE FOR VAT        */
         BAL   @14,GETCORE                                         1227
*         R6=0;                     /* INIT COMPARE REG TO 0         */
         SLR   R6,R6                                               1228
*         R7=MWAGFADR;              /* INIT SWAP REG                 */
         L     R7,MWAGFADR                                         1229
*         R7->VATID=IDXVAT;         /* INIT CB ID            @OZ03937*/
         MVI   VATID(R7),B'00010001'                               1230
*         R7->VATLEN=MWAGFLEN;      /* INIT LENGTH           @OZ03937*/
         L     @05,MWAGFLEN-1                                      1231
         LA    @05,0(,@05)                                         1231
         STH   @05,VATLEN(,R7)                                     1231
*         R7->VATVRT=MWAPTRA->VATVRT+1;/* INIT TABLE NUMBER  @OZ03937*/
         L     @05,MWAPTRA                                         1232
         LA    @15,1                                               1232
         SLR   @14,@14                                             1232
         IC    @14,VATVRT(,@05)                                    1232
         ALR   @15,@14                                             1232
         STC   @15,VATVRT(,R7)                                     1232
*         R7->VATENO=DIM(VATAMBL);  /* INIT NO. OF ENTRIES   @OZ03937*/
         MVI   VATENO(R7),X'10'                                    1233
*         CS(R6,R7,MWAPTRA->VATNEXT);/* ADD NEW VAT TO END OF CHAIN  */
         CS    R6,@07,VATNEXT(@05)                                 1234
*         IF R6^=0 THEN             /* IF NOT SUCCESSFUL             */
         LTR   R6,R6                                               1235
         BZ    @RF01235                                            1235
*           CALL FREECORE;          /* THEN FREE NEW VAT             */
         BAL   @14,FREECORE                                        1236
*       END;                                                       1237
@RF01235 DS    0H                                                  1238
*   END;                                                           1238
@RC01207 DS    0H                                                  1238
@DE01186 TM    MWAVATSW,B'00010000'                                1238
         BNO   @DL01186                                            1238
*   MWAPTRA=JSCBSHR;                /* RESTORE THE VAT POINTER TO THE
*                                      ADDR OF THE FIRST VAT @ZA07991*/
         L     @05,OPWJSTCB(,RCORE)                                1239
         L     @05,TCBJSCB(,@05)                                   1239
         L     @05,JSCBSHR(,@05)                                   1239
         ST    @05,MWAPTRA                                         1239
*   RFY                                                            1240
*     RET RSTD;                                                    1240
*   RET=MWASAVEC;                   /* RESTORE RETURN ADDRESS        */
         L     RET,MWASAVEC                                        1241
*   RFY                                                            1242
*     RET UNRSTD;                                                  1242
*   RFY                                                            1243
*    (R6,                                                          1243
*     R7) UNRSTD;                                                  1243
*   END VATUPD;                                                    1244
@EL00013 DS    0H                                                  1244
@EF00013 DS    0H                                                  1244
@ER00013 BR    @14                                                 1244
*CHNAMBL:                                                          1245
*                                                                  1245
*/********************************************************************/
*/*                                                                  */
*/*   FUNCTION: CALLED BY BLDAMBL.  INITIALIZES THE TYPE FIELDS IN   */
*/*             THE AMBL.  IF AMBL IS NOT FOR A DUMMY, CATALOG       */
*/*             OR SCRA DATA SET, THEN ADD AMBL TO APPROPRIATE       */
*/*             AMBL CHAIN.                                          */
*/*                                                                  */
*/*   INPUTS:   1. PTR TO AMBL IN MWAXAMBL.                          */
*/*                                                                  */
*/*   OUTPUTS:  1. PTR TO PATH AND BASE AMBL'S INITIALIZED IN OPW.   */
*/*                                                                  */
*/********************************************************************/
*                                                                  1245
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1245
CHNAMBL  DS    0H                                                  1246
*   RFY                                                            1246
*     RET RSTD;                                                    1246
*   MWASAVEC=RET;                   /* SAVE RETURN ADDRESS           */
         ST    RET,MWASAVEC                                        1247
*   RFY                                                            1248
*     RET UNRSTD;                                                  1248
*   IF OPWUPGR=ON THEN              /* IF UPGRADE AMBL               */
         L     @05,OPWCURPT(,RCORE)                                1249
         TM    OPWUPGR(@05),B'00100000'                            1249
         BNO   @RF01249                                            1249
*     DO;                           /* PROCESS UPGRADE AMBL          */
*       AMBLUPGR=ON;                /* FLAG AMBL AS UPGRADE          */
         L     @05,MWAXAMBL                                        1251
         OI    AMBLUPGR(@05),B'01000000'                           1251
*       IF OPWCAMBL^=0 THEN         /* IF CONNECTING                 */
         L     @15,OPWCAMBL(,RCORE)                                1252
         LTR   @15,@15                                             1252
         BZ    @RF01252                                            1252
*         AMBLCMB=OPWCAMBL->AMBLCMB;/* THEN POINT AMBL TO EXISTING 1253
*                                      CMB                           */
         L     @15,AMBLCMB(,@15)                                   1253
         ST    @15,AMBLCMB(,@05)                                   1253
*       ELSE                        /* NOT CONNECTING                */
*         DO;                                                      1254
         B     @RC01252                                            1254
@RF01252 DS    0H                                                  1255
*           CALL BLDCMB;            /* BUILD CMB                     */
         BAL   @14,BLDCMB                                          1255
*           CMBOUT=ON;              /* IND CLUSTER OPEN FOR OUTPUT   */
         L     @05,MWAXAMBL                                        1256
         L     @05,AMBLCMB(,@05)                                   1256
         OI    CMBOUT(@05),B'10000000'                             1256
*         END;                                                     1257
*     END;                                                         1258
*   ELSE                            /* NOT UPGRADE AMBL              */
*     DO;                                                          1259
         B     @RC01249                                            1259
@RF01249 DS    0H                                                  1260
*       IF OPWPATH=ON THEN          /* IF PATH AMBL                  */
         L     @05,OPWCURPT(,RCORE)                                1260
         TM    OPWPATH(@05),B'01000000'                            1260
         BNO   @RF01260                                            1260
*         DO;                       /* THEN PROCESS PATH AMBL        */
*           AMBLPATH=ON;            /* FLAG AMBL AS PATH             */
         L     @15,MWAXAMBL                                        1262
         OI    AMBLPATH(@15),B'10000000'                           1262
*           OPWPAMBL=MWAXAMBL;      /* INIT PATH AMBL PTR IN ACBWORK */
         ST    @15,OPWPAMBL(,RCORE)                                1263
*           IF OPWCAMBL^=0 THEN     /* IF CONNECTING                 */
         L     @14,OPWCAMBL(,RCORE)                                1264
         LTR   @14,@14                                             1264
         BZ    @RF01264                                            1264
*             DO;                                                  1265
*               AMBLCMB=OPWCAMBL->AMBLCMB;/* PT AMBL TO EXISTING CMB */
         L     @14,AMBLCMB(,@14)                                   1266
         ST    @14,AMBLCMB(,@15)                                   1266
*               IF OPWPRTBL=ON THEN /* IF PARTIAL CB BUILD REQUIRED  */
         TM    OPWPRTBL(@05),B'00001000'                           1267
         BNO   @RF01267                                            1267
*                 DO;                                              1268
*                   AMBLPRIM=ON;    /* THEN IND PRIMARY AMBL         */
         OI    AMBLPRIM(@15),B'10000000'                           1269
*                   CMBNST=OPWCSTRN;/* SET NO. OF STRINGS FOR PATH   */
         IC    @05,OPWCSTRN(,RCORE)                                1270
         STC   @05,CMBNST(,@14)                                    1270
*                 END;                                             1271
*               ELSE                /* PARTIAL BUILD NOT REQUIRED    */
*                 DO;               /* ADD AMBL TO SECONDARY AMBL  1272
*                                      CHAIN FOR PATH                */
         B     @RC01267                                            1272
@RF01267 DS    0H                                                  1273
*                   AMBLSCHN=OPWCAMBL->AMBLSCHN;/* PT CURRENT AMBL TO
*                                      FIRST SECONDARY AMBL          */
         L     @05,MWAXAMBL                                        1273
         L     @15,OPWCAMBL(,RCORE)                                1273
         L     @14,AMBLSCHN(,@15)                                  1273
         ST    @14,AMBLSCHN(,@05)                                  1273
*                   OPWCAMBL->AMBLSCHN=MWAXAMBL;/* CHAIN AMBL        */
         ST    @05,AMBLSCHN(,@15)                                  1274
*                 END;                                             1275
*             END;                                                 1276
*           ELSE                    /* NOT CONNECTING                */
*             DO;                                                  1277
         B     @RC01264                                            1277
@RF01264 DS    0H                                                  1278
*               AMBLPRIM=ON;        /* FLAG AMBL AS PRIMARY          */
         L     @05,MWAXAMBL                                        1278
         OI    AMBLPRIM(@05),B'10000000'                           1278
*               CALL BLDCMB;        /* BUILD CMB                     */
         BAL   @14,BLDCMB                                          1279
*             END;                                                 1280
*         END;                                                     1281
*       ELSE                        /* NOT PATH AMBL                 */
*         DO;                       /* PROCESS BASE AMBL             */
         B     @RC01260                                            1282
@RF01260 DS    0H                                                  1283
*           AMBLBASE=ON;            /* FLAG AMBL AS BASE             */
         L     @05,MWAXAMBL                                        1283
         OI    AMBLBASE(@05),B'00010000'                           1283
*           OPWBAMBL=MWAXAMBL;      /* INIT BASE AMBL PTR IN ACBWORK */
         ST    @05,OPWBAMBL(,RCORE)                                1284
*           IF OPWDUMMY=ON THEN     /* IF DUMMY DATA SET             */
         TM    OPWDUMMY(RCORE),B'00010000'                         1285
         BNO   @RF01285                                            1285
*             DO;                   /* THEN SET DUMMY INDICATORS     */
*               AMBLDUMY=ON;        /* FLAG AMBL AS DUMMY            */
         OI    AMBLDUMY(@05),B'00000001'                           1287
*               AMBLNUM=0;          /* INDICATE NO AMB'S             */
         SLR   @15,@15                                             1288
         STH   @15,AMBLNUM(,@05)                                   1288
*             END;                                                 1289
*           ELSE                    /* NOT DUMMY DATA SET            */
*             DO;                   /* PROCESS BASE AMBL             */
         B     @RC01285                                            1290
@RF01285 DS    0H                                                  1291
*               IF OPWFLGS1^=0 THEN /* IF OPEN FOR SYSTEM DATA SET   */
         CLI   OPWFLGS1(RCORE),0                                   1291
         BE    @RF01291                                            1291
*                 DO;               /* THEN SET AMBL FLAGS           */
*                   AMBLPRIM=ON;    /* IND PRIMARY AMBL              */
         L     @05,MWAXAMBL                                        1293
         OI    AMBLPRIM(@05),B'10000000'                           1293
*                   AMBLCAT=ON;     /* IND AMBL FOR CATALOG          */
         OI    AMBLCAT(@05),B'00000010'                            1294
*                   CALL BLDCMB;    /* BUILD CMB                     */
         BAL   @14,BLDCMB                                          1295
*                   IF ACBOUT=ON×ACBRST=ON THEN/* IF OUTPUT SPECIFIED
*                                                            @XM01165*/
         TM    ACBOUT(RACB),B'00000010'                            1296
         BO    @RT01296                                            1296
         TM    ACBRST(RACB),B'00000100'                            1296
         BNO   @RF01296                                            1296
@RT01296 DS    0H                                                  1297
*                     CMBOUT=ON;    /* IND CLUSTER OPEN FOR OUTPUT   */
         L     @05,MWAXAMBL                                        1297
         L     @05,AMBLCMB(,@05)                                   1297
         OI    CMBOUT(@05),B'10000000'                             1297
*                   IF OPWSCRA=ON THEN/* ELSE IF SCRA OPEN           */
@RF01296 TM    OPWSCRA(RCORE),B'01000000'                          1298
         BNO   @RF01298                                            1298
*                     AMBLSCRA=ON;  /* THEN IND SCRA AMBL            */
         L     @05,MWAXAMBL                                        1299
         OI    AMBLSCRA(@05),B'00001000'                           1299
*                   ELSE            /* NOT SCRA OPEN                 */
*                     IF OPWVVIC=ON THEN/* ELSE IF VVIC              */
         B     @RC01298                                            1300
@RF01298 TM    OPWVVIC(RCORE),B'00100000'                          1300
         BNO   @RF01300                                            1300
*                       AMBLVVIC=ON;/* THEN IND VVIC AMBL            */
         L     @05,MWAXAMBL                                        1301
         OI    AMBLVVIC(@05),B'00010000'                           1301
*                 END;                                             1302
*               ELSE                /* NOT OPEN FOR SYSTEM DATA SET  */
*                 DO;               /* COMPLETE AMBL CHAINING        */
         B     @RC01291                                            1303
@RF01291 DS    0H                                                  1304
*                   IF OPWUCRA=ON THEN/* IF OPEN FOR UCRA            */
         TM    OPWUCRA(RCORE),B'10000000'                          1304
         BNO   @RF01304                                            1304
*                     AMBLUCRA=ON;  /* THEN IND AMBL FOR UCRA        */
         L     @05,MWAXAMBL                                        1305
         OI    AMBLUCRA(@05),B'00000100'                           1305
*                   IF OPWCAMBL^=0 THEN/* IF CONNECTING              */
@RF01304 L     @05,OPWCAMBL(,RCORE)                                1306
         LTR   @05,@05                                             1306
         BZ    @RF01306                                            1306
*                     DO;           /* THEN ADD AMBL TO BASE       1307
*                                      SECONDARY CHAIN               */
*                       AMBLCMB=OPWCAMBL->AMBLCMB;/* PT AMBL TO    1308
*                                      EXISTING CMB                  */
         L     @15,MWAXAMBL                                        1308
         L     @14,AMBLCMB(,@05)                                   1308
         ST    @14,AMBLCMB(,@15)                                   1308
*                       AMBLSCHN=OPWCAMBL->AMBLSCHN;/* PT CURRENT  1309
*                                      AMBL TO FIRST SECONDARY AMBL  */
         L     @14,AMBLSCHN(,@05)                                  1309
         ST    @14,AMBLSCHN(,@15)                                  1309
*                       OPWCAMBL->AMBLSCHN=MWAXAMBL;/* CHAIN AMBL    */
