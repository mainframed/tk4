*               IF PDPCODE=PP116×   /* IF I/O ERROR READING DSCB     */
*                   PDPCODE=PP59 THEN/* OR VOL TIME STAMP ERROR      */
         CLI   PDPCODE(@05),116                                    0550
         BE    @RT00550                                            0550
         CLI   PDPCODE(@05),59                                     0550
         BNE   @RF00550                                            0550
@RT00550 DS    0H                                                  0551
*                 PDPVLSER=ADDR(VVOLLVSR(VVOLLCNT));/* PT TO BAD   0551
*                                      VOLUME                        */
         L     @05,OPWCOMWA(,RCORE)                                0551
         L     @15,MWAVVOLL                                        0551
