@RC00552 L     @05,OPWCOMWA(,RCORE)                                0555
         CLI   PDPCODE+280(@05),59                                 0555
         BNE   @RF00555                                            0555
         TM    ACBOUT(RACB),B'00000010'                            0555
         BO    @RT00555                                            0555
         TM    ACBRST(RACB),B'00000100'                            0555
         BNO   @RF00555                                            0555
@RT00555 DS    0H                                                  0556
*                 PDPCODE=PP60;     /* THEN IND TERMINATING ERROR    */
         L     @05,OPWCOMWA(,RCORE)                                0556
