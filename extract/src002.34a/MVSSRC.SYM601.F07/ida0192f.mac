         AL    MWAJ,@CF00053                                       0525
@DE00524 L     @01,MWAVMT                                          0525
         CH    MWAJ,VMTNOVOL(,@01)                                 0525
         BNH   @DL00524                                            0525
@DC00524 DS    0H                                                  0526
*     IF MWAJ>VMTNOVOL THEN         /* IF VOL SER NO. NOT IN VMT     */
         L     @01,MWAVMT                                          0526
         CH    MWAJ,VMTNOVOL(,@01)                                 0526
         BNH   @RF00526                                            0526
