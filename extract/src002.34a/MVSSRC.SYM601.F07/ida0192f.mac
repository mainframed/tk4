         LA    @05,1                                               0528
         AL    @05,MWAVOLCT                                        0528
         ST    @05,MWAVOLCT                                        0528
*         VVOLLVSR(MWAVOLCT)=VOLSER;/* MOVE VOL SER NO. FROM JFCB TO
