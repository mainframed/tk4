*         ELSE                                                     0372
*           ;                                                      0372
@RF00366 DS    0H                                                  0373
*         RBUFC=BUFCNXT2;           /* NEXT BUFC                     */
         L     @10,BUFCNXT2-1(,RBUFC)                              0373
         LA    @10,0(,@10)                                         0373
         LR    RBUFC,@10                                           0373
*       END;                        /* END OF DO-UNTIL LOOP          */
*                                                                  0374
@DE00365 C     RBUFC,PLHDBUFC(,RPLH)                               0374
