         TITLE 'IDA019SN - TERMINATE RPL                               *
                        '
IDA019SN CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IDA019SN  78.188'                                 0001
         DROP  @15
@PROLOG  BALR  @11,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@11                                         0001
*                                                                  0157
*   /*****************************************************************/
*   /*                                                               */
*   /* TERMRPL MAIN ROUTINE                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0157
*   RPLFDBWD=ZERO;                  /* CLEAR RPL FEEDBACK WORD       */
         SLR   @15,@15                                             0157
         ST    @15,RPLFDBWD(,RRPL)                                 0157
*   RBUFC=R14;                      /* SAVE RETURN ADDRESS           */
         LR    RBUFC,R14                                           0158
*   CALL VALCHK;                    /* CALL VALIDITY CHECK ROUTINE   */
         BAL   @14,VALCHK                                          0159
*   R14=RBUFC;                      /* RESTORE RETURN ADDRESS        */
         LR    R14,RBUFC                                           0160
*   IF RPLERREG=ZERO THEN           /* ANY ATTRIBUTE OR OPTION ERROR?*/
         CLI   RPLERREG(RRPL),0                                    0161
         BNE   @RF00161                                            0161
*     DO;                           /* NO ERROR, CONTINUE THE PROCESS*/
*                                                                  0162
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE PLH PUSH DOWN STACK OFFSET AND SAVE RETURN     */
*       /* ADDRESS BEFORE RELEASING THE RESOURCES.                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0163
*       PLHTERM=ON;                 /* SET TERMRPL IN PROCESS BIT    */
         OI    PLHTERM(RPLH),B'10000000'                           0163
*       PLHRETO=PLHRETOS;           /* CLEAR OFFSET OF PUSH DOWN   0164
*                                      STACK                         */
         LH    @15,PLHRETOS(,RPLH)                                 0164
         STH   @15,PLHRETO(,RPLH)                                  0164
*       GEN(IDASVR14);              /* SAVE RETURN ADDRESS IN PLH    */
*                                                                  0165
         IDASVR14
*       /*************************************************************/
*       /*                                                           */
*       /* RELEASE BUFC HEADER IF OWNED                              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0166
*       IF PLHBUFHD=ON THEN         /* DOES PLH OWN THE BUFC HDR?    */
         TM    PLHBUFHD(RPLH),B'01000000'                          0166
         BNO   @RF00166                                            0166
*         DO;                       /* YES, RELEASE IT               */
*           BUFDRTSB=CZERO;         /* RELEASE BUFC HEADER           */
         L     @15,AMBBUFC(,RAMB)                                  0168
         MVI   BUFDRTSB(@15),B'00000000'                           0168
*           PLHTMBUF=ON;            /* SET PROCESS BIT IN PLH        */
         OI    PLHTMBUF(RPLH),B'01000000'                          0169
*         END;                      /* END OF BUFC HDR CASE          */
*       ELSE                                                       0171
*         ;                         /* ELSE NOT OWN BUFC HDR         */
@RF00166 DS    0H                                                  0172
*       RAMB=AMBLIX;                /* LOAD INDEX AMB                */
         L     @15,RPLDACB(,RRPL)                                  0172
         L     @15,ACBAMBL(,@15)                                   0172
         L     RAMB,AMBLIX(,@15)                                   0172
*       IF RAMB^=ZERO THEN          /* IS IT A KSDS ?                */
         LTR   RAMB,RAMB                                           0173
         BZ    @RF00173                                            0173
*         CALL RELINX;              /* CALL RELINX TO RELEASE INDEX
*                                      BUFC                          */
         BAL   @14,RELINX                                          0174
*       ELSE                                                       0175
*         ;                         /* ELSE NOT A KSDS               */
@RF00173 DS    0H                                                  0176
*       RAMB=AMBLDTA;               /* RESET AMB BASE                */
         L     @15,RPLDACB(,RRPL)                                  0176
         L     @15,ACBAMBL(,@15)                                   0176
         L     RAMB,AMBLDTA(,@15)                                  0176
*       CALL RELDIWA;               /* CALL RELDIWA TO RELEASE DIWA  */
         BAL   @14,RELDIWA                                         0177
*       CALL RELDTA;                /* CALL RELDTA TO RELEASE DATA 0178
*                                      BUFC                          */
         BAL   @14,RELDTA                                          0178
*                                                                  0179
*       /*************************************************************/
*       /*                                                           */
*       /* TERMRPL WILL ATTEMPT TO RESTART DEFERRED REQUESTS BY      */
*       /* CALLING IDARSTRT ROUTINE. IDARSTRT WILL NOT RESTART       */
*       /* ASYNCHRONOUS DEFERRED REQUEST AND WILL RETURN AN          */
*       /* INFORMATION CODE TO THE USER TO INDICATE THE SITUATION.   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0179
*       IF PLHDRREQ^=ZERO THEN      /* ANY DEFERRED REQUEST ?        */
         L     @15,AMBPH(,RAMB)                                    0179
         L     @15,PLHDRREQ(,@15)                                  0179
         LTR   @15,@15                                             0179
         BZ    @RF00179                                            0179
*I01:                                                              0180
*         DO;                       /* IDACALL(IDARSTRT)             */
I01      DS    0H                                                  0181
*           CALL IDARSTRT;          /* EXIT TO IDARSTRT              */
         L     @15,@CV00992                                        0181
         BALR  @14,@15                                             0181
*           GEN(BALR @11,0);        /* LOAD ADDR OF REENTRY          */
         BALR @11,0
*           GEN(USING *,@11);       /* ESTABLISH ADDRESSING          */
         USING *,@11
*           GEN(L     @11,=A(@PSTART));/* RELOAD BASE                */
         L     @11,=A(@PSTART)
*           GEN(USING @PSTART,@11); /* ESTABLISH ADDRESSING          */
         USING @PSTART,@11
*         END I01;                  /* TRY TO RESTART THEM           */
*       ELSE                                                       0187
*         ;                         /* NO DEFERRED REQUEST           */
@RF00179 DS    0H                                                  0188
*       GEN REFS(HWORD4)(IDARST14); /* RESTORE RETURN ADDRESS        */
         IDARST14
*       RFY                                                        0189
*         R14 RSTD;                                                0189
*                                                                  0189
*       /*************************************************************/
*       /*                                                           */
*       /* DISCONNECT THE PLH                                        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0190
*       IF PLHDBPER=OFF THEN        /* ANY ERROR IN THE BUFC PTR?    */
         TM    PLHDBPER(RPLH),B'00000010'                          0190
         BNZ   @RF00190                                            0190
*         DO;                       /* NO ERROR, DISCONNECT PLH      */
*           PLHFLG1=CZERO;          /* CLEAR PROCESS FLAGS IN PLH    */
         MVI   PLHFLG1(RPLH),B'00000000'                           0192
*           PLHFLG2=CZERO;                                         0193
         MVI   PLHFLG2(RPLH),B'00000000'                           0193
*           PLHFLG3=CZERO;                                         0194
         MVI   PLHFLG3(RPLH),B'00000000'                           0194
*           PLHEFLGS=C2ZERO;                                       0195
         MVC   PLHEFLGS(2,RPLH),C2ZERO                             0195
*           PLHINVAL=ON;            /* SET PLH INVALID               */
         OI    PLHINVAL(RPLH),B'00000010'                          0196
*           PLHARDB=ZERO;           /* ZERO ARDB FIELD               */
         SLR   @15,@15                                             0197
         ST    @15,PLHARDB(,RPLH)                                  0197
*           PLHMRPL=ZERO;           /* DISCONNECT PLH                */
         ST    @15,PLHMRPL(,RPLH)                                  0198
*           PLHDRCUR=PLHDRCUR-ONE;  /* DECREMENT COUNT               */
         L     @15,AMBPH(,RAMB)                                    0199
         LH    @10,PLHDRCUR(,@15)                                  0199
         BCTR  @10,0                                               0199
         STH   @10,PLHDRCUR(,@15)                                  0199
*           PLHAVL=CZERO;           /* SET PLH AVAILABLE             */
         MVI   PLHAVL(RPLH),B'00000000'                            0200
*           IF RPLECBSW=ON THEN     /* EXTERNAL ECB ?                */
         TM    RPLECBSW(RRPL),B'00000001'                          0201
         BNO   @RF00201                                            0201
*             USERECB=POSTED;       /* SET POST FLAG IN USER ECB     */
         L     @15,RPLECB(,RRPL)                                   0202
         MVC   USERECB(4,@15),POSTED                               0202
*           ELSE                                                   0203
*             RPLECB=POSTED;        /* ELSE SET POST FLAG IN RPLECB  */
         B     @RC00201                                            0203
@RF00201 L     @15,POSTED                                          0203
         ST    @15,RPLECB(,RRPL)                                   0203
*           RPLACTIV=CZERO;         /* SET RPL INACTIVE              */
@RC00201 MVI   RPLACTIV(RRPL),B'00000000'                          0204
*         END;                      /* END OF NO BUFC PTR ERROR      */
*       ELSE                                                       0206
*         ;                         /* ELSE, DATA BUFC PTR ERROR     */
@RF00190 DS    0H                                                  0207
*     END;                          /* END OF VALID REQUEST CASE     */
*   ELSE                                                           0208
*     ;                             /* ELSE ERROR, RETURN            */
@RF00161 DS    0H                                                  0209
*   RETURN;                         /* RETURN TO CALLER              */
@EL00001 DS    0H                                                  0209
@EF00001 DS    0H                                                  0209
@ER00001 BR    @14                                                 0209
*                                                                  0210
*/********************************************************************
**  VALCHK: ROUTINE USED TO VALIDITY CHECK THE DATA SET ATTRIBUTES,  *
**          RPL OPTIONS, AND PROCESSING CONDITIONS OF THE TERMRPL    *
**          REQUEST.  IF AN ERROR IS FOUND, SET RPLERREG AND         *
**          RPLERRCD AND RETURN.  R14 IS RESTRICTED IN THIS ROUTINE. *
******************************************************************** */
*                                                                  0210
*VALCHK:                                                           0210
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0210
VALCHK   DS    0H                                                  0211
*   RFY                                                            0211
*     RWORK1 RSTD;                                                 0211
*   RWORK1=ZERO;                    /* CLEAR WORK REGISTER           */
*                                                                  0212
         SLR   RWORK1,RWORK1                                       0212
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF THE PREVIOUS REQUEST WAS TERMRPL, IF YES, SET ERROR  */
*   /* CODE.                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0213
*   IF PLHTERM=ON THEN              /* IS TERMRPL IN PROCESS BIT ON? */
         TM    PLHTERM(RPLH),B'10000000'                           0213
         BNO   @RF00213                                            0213
*     RWORK1=RPLPTERM;              /* YES, THE PREVIOUS REQUEST IS
*                                      TERMRPL                       */
         LA    RWORK1,52                                           0214
*   ELSE                                                           0215
*     ;                             /* ELSE CONTINUE                 */
*                                                                  0215
@RF00213 DS    0H                                                  0216
*   /*****************************************************************/
*   /*                                                               */
*   /* VALIDITY CHECK DATA SET ATTRIBUTES, RPL OPTIONS, AND          */
*   /* PROCESSING OPTIONS.                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0216
*   IF RPLCNV=ON×RPLASY=ON×RPLCHAIN^=ZERO×/* INVALID RPL OPTIONS     */
*       AMBLPATH=ON×AMBCREAT=ON×    /* INVALID PROCESSING OPTIONS    */
*       AMBGSR=ON×AMBLSR=ON×        /* INVALID PROCESSING OPTIONS    */
*       AMDRRDS=ON×AMDSPAN=ON       /* INVALID DATASET ATTRIBUTES    */
*     THEN                                                         0216
         TM    RPLCNV(RRPL),B'00100000'                            0216
         BO    @RT00216                                            0216
         TM    RPLASY(RRPL),B'00001000'                            0216
         BO    @RT00216                                            0216
         L     @15,RPLCHAIN(,RRPL)                                 0216
         LTR   @15,@15                                             0216
         BNZ   @RT00216                                            0216
         L     @15,RPLDACB(,RRPL)                                  0216
         L     @15,ACBAMBL(,@15)                                   0216
         TM    AMBLPATH(@15),B'10000000'                           0216
         BO    @RT00216                                            0216
         TM    AMBCREAT(RAMB),B'10000000'                          0216
         BO    @RT00216                                            0216
         TM    AMBGSR(RAMB),B'01100000'                            0216
         BNZ   @RT00216                                            0216
         L     @15,AMBDSB(,RAMB)                                   0216
         TM    AMDRRDS(@15),B'00000011'                            0216
         BZ    @RF00216                                            0216
@RT00216 DS    0H                                                  0217
*     RWORK1=RPLINTRM;              /* SET ERROR CODE                */
         LA    RWORK1,48                                           0217
*   ELSE                                                           0218
*     DO;                           /* ELSE VALID REQUEST            */
*                                                                  0218
         B     @RC00216                                            0218
@RF00216 DS    0H                                                  0219
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK IF USER IS IN SUPERVISOR STATE AND KEY 0            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0219
*       RFY                                                        0219
*        (RWORK2,                                                  0219
*         R15) RSTD;                                               0219
*       RWORK2=RRPL;                /* SAVE RPL ADDRESS              */
         LR    RWORK2,RRPL                                         0220
*       GEN(TESTAUTH STATE=YES,RBLEVEL=1);/* TEST FOR SUPV. STATE    */
         TESTAUTH STATE=YES,RBLEVEL=1
*       RRPL=RWORK2;                /* RESTORE RPL ADDRESS           */
         LR    RRPL,RWORK2                                         0222
*       IF R15^=ZERO THEN           /* NOT SUPV. STATE               */
         LTR   R15,R15                                             0223
         BZ    @RF00223                                            0223
*         RWORK1=RPLINTRM;          /* SET ERROR CODE                */
         LA    RWORK1,48                                           0224
*       ELSE                                                       0225
*         DO;                       /* ELSE TEST FOR KEY 0           */
         B     @RC00223                                            0225
@RF00223 DS    0H                                                  0226
*           RWORK2=RPLH;            /* SAVE REGISTER 2               */
         LR    RWORK2,RPLH                                         0226
*           RPLH=ZERO;              /* CLEAR WORK REGISTER           */
         SLR   RPLH,RPLH                                           0227
*           GEN(IPK);               /* OBTAIN KEY                    */
         IPK
*           IF RPLH^=ZERO THEN      /* KEY 0 ?                       */
         LTR   RPLH,RPLH                                           0229
         BZ    @RF00229                                            0229
*             DO;                   /* NO                            */
*               RPLH=RWORK2;        /* RESTORE REGISTER 2            */
         LR    RPLH,RWORK2                                         0231
*               RWORK1=RPLINTRM;    /* SET ERROR CODE                */
         LA    RWORK1,48                                           0232
*             END;                  /* END OF ERROR CASE             */
*           ELSE                                                   0234
*             DO;                   /* SUPV. STATE AND KEY 0         */
         B     @RC00229                                            0234
@RF00229 DS    0H                                                  0235
*               RPLH=RWORK2;        /* RESTORE REG2                  */
*                                                                  0235
         LR    RPLH,RWORK2                                         0235
*               /*****************************************************/
*               /*                                                   */
*               /* IF EOV WAS IN PROCESS (PLHINEOV = ON) WHEN THE    */
*               /* REQUEST WAS TERMINATED, THE REQUEST WILL BE       */
*               /* REJECTED AND AN ERROR CODE WILL BE RETURNED TO THE*/
*               /* USER.                                             */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0236
*               IF PLHINEOV=ON THEN /* WAS EOV IN PROCESS?           */
         TM    PLHINEOV(RPLH),B'10000000'                          0236
         BNO   @RF00236                                            0236
*                 RWORK1=RPLINTRM;  /* SET ERROR CODE                */
*                                                                  0237
         LA    RWORK1,48                                           0237
*               /*****************************************************/
*               /*                                                   */
*               /* IF THE IOMB IS LOCKED BY I/O MANAGEMENT ROUTINES, */
*               /* THE REQUEST WAS TERMINATED DURING ITS I/O         */
*               /* OPERATION, POSSIBLE DATA SET ERROR, SET           */
*               /* INFORMATION CODE TO WARN THE USER.                */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0238
*               ELSE                                               0238
*                 IF IOMLOCK^=ZERO&IOMEOVTS=OFF THEN               0238
         B     @RC00236                                            0238
@RF00236 L     @10,PLHDIOB(,RPLH)                                  0238
         CLI   IOMLOCK(@10),0                                      0238
         BE    @RF00238                                            0238
         TM    IOMEOVTS(@10),B'00100000'                           0238
         BNZ   @RF00238                                            0238
*                   DO;             /* IOMB LOCKED BY I/O MANAGER    */
*                     RPLERRCD=RPLDSERR;/* SET INFORMATION CODE      */
         MVI   RPLERRCD(RRPL),X'24'                                0240
*                     PLHDSERR=ON;  /* SET FDBK INDICATOR IN PLH     */
         OI    PLHDSERR(RPLH),B'00000100'                          0241
*                     IOECB=POSTED; /* SET POST FLAG                 */
         L     @07,IOMECBPT(,@10)                                  0242
         MVC   IOECB(4,@07),POSTED                                 0242
*                     IOMLOCK=OFF;  /* RELEASE IOMB                  */
         MVI   IOMLOCK(@10),B'00000000'                            0243
*                     PLHTMIOB=ON;  /* SET PROCESS BIT IN PLH        */
         OI    PLHTMIOB(RPLH),B'00000100'                          0244
*                   END;            /* END OF IOMB LOCKED CASE       */
*             END;                  /* END OF KEY 0 CASE             */
@RF00238 DS    0H                                                  0246
@RC00236 DS    0H                                                  0247
*         END;                      /* END OF SUPV. STATE CASE       */
@RC00229 DS    0H                                                  0248
*     END;                          /* END OF VALIDITY CHECK         */
@RC00223 DS    0H                                                  0249
*   RFY                                                            0249
*    (RWORK2,                                                      0249
*     R15) UNRSTD;                                                 0249
@RC00216 DS    0H                                                  0250
*   IF RWORK1^=ZERO THEN            /* ANY ERROR ?                   */
         LTR   RWORK1,RWORK1                                       0250
         BZ    @RF00250                                            0250
*     DO;                           /* YES                           */
*       RPLERREG='08'X;             /* SET LOGICAL ERROR             */
         MVI   RPLERREG(RRPL),X'08'                                0252
*       RPLERRCD=RWORK1;            /* SET FEEDBACK CODE             */
         STC   RWORK1,RPLERRCD(,RRPL)                              0253
*     END;                          /* END OF ERROR CASE             */
*   ELSE                                                           0255
*     ;                             /* NO ERROR                      */
@RF00250 DS    0H                                                  0256
*   RFY                                                            0256
*     RWORK1 UNRSTD;                                               0256
*   RETURN;                         /* RETURN TO MAIN ROUTINE        */
@EL00002 DS    0H                                                  0257
@EF00002 DS    0H                                                  0257
@ER00002 BR    @14                                                 0257
*   END VALCHK;                     /* END OF VALCHK PROC.           */
*                                                                  0259
*/* ******************************************************************
*  * RELINX: ROUTINE USED TO RELEASE THE INDEX BUFC(S) THAT IS OWNED *
*  *         BY THE SPECIFIED PLH.                                   *
*  *******************************************************************/
*                                                                  0259
*RELINX:                                                           0259
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0259
RELINX   DS    0H                                                  0260
*   GEN(IDASVR14);                  /* SAVE RETURN ADDRESS           */
         IDASVR14
*   RFY                                                            0261
*     R14 UNRSTD;                                                  0261
*                                                                  0261
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK INDEX BUFC CHAIN AND RELEASE ALL OWNED INDEX BUFC'S     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0262
*   RBUFC=BUFDBUFC;                 /* LOAD FIRST INDEX BUFC         */
         L     @10,AMBBUFC(,RAMB)                                  0262
         L     RBUFC,BUFDBUFC(,@10)                                0262
*   DO UNTIL RBUFC=ZERO;            /* LOOP THROUGH BUFC CHAIN       */
@DL00263 DS    0H                                                  0264
*     IF BUFCPLH=RPLH THEN          /* DOES PLH OWN BUFC?            */
         C     RPLH,BUFCPLH(,RBUFC)                                0264
         BNE   @RF00264                                            0264
*       DO;                         /* YES                           */
*         IF BUFCMW=ON THEN         /* MUST WRITE BIT ON?            */
         TM    BUFCMW(RBUFC),B'10000000'                           0266
         BNO   @RF00266                                            0266
*           DO;                     /* YES                           */
*             RPLERRCD=RPLDSERR;    /* SET FEEDBACK CODE             */
         MVI   RPLERRCD(RRPL),X'24'                                0268
*             PLHDSERR=ON;          /* SET FDBK INDICATOR IN PLH     */
         OI    PLHDSERR(RPLH),B'00000100'                          0269
*             BUFCMW=OFF;           /* SET MUST WRITE BIT OFF        */
         NI    BUFCMW(RBUFC),B'01111111'                           0270
*           END;                    /* END OF MUST WRITE CASE        */
*         ELSE                                                     0272
*           ;                       /* ELSE NO MUST WRITE DATA       */
@RF00266 DS    0H                                                  0273
*         BUFCVAL=OFF;              /* INVALIDATE BUFC               */
         NI    BUFCVAL(RBUFC),B'11111011'                          0273
*I02:                                                              0274
*         DO;                       /* IDACALL(IDAFREEB)             */
I02      DS    0H                                                  0275
*           CALL IDAFREEB;          /* EXIT TO IDAFREEB              */
         L     @15,@CV00991                                        0275
         BALR  @14,@15                                             0275
*           GEN(BALR @11,0);        /* LOAD ADDR OF REENTRY          */
         BALR @11,0
*           GEN(USING *,@11);       /* ESTABLISH ADDRESSING          */
         USING *,@11
*           GEN(L     @11,=A(@PSTART));/* RELOAD BASE                */
         L     @11,=A(@PSTART)
*           GEN(USING @PSTART,@11); /* ESTABLISH ADDRESSING          */
         USING @PSTART,@11
*         END I02;                  /* FREE BUFC                     */
*         PLHTMINX=ON;              /* TURN PROCESS BIT ON           */
         OI    PLHTMINX(RPLH),B'00001000'                          0281
*       END;                        /* END OF OWNED CASE             */
*     ELSE                                                         0283
*       ;                           /* ELSE NOT OWNED BY PLH         */
@RF00264 DS    0H                                                  0284
*     RBUFC=BUFCHAIN;               /* GET NEXT BUFC IN CHAIN        */
         L     RBUFC,BUFCHAIN(,RBUFC)                              0284
*   END;                            /* END OF DO-UNTIL               */
@DE00263 LTR   RBUFC,RBUFC                                         0285
         BNZ   @DL00263                                            0285
*   GEN REFS(HWORD4)(IDARST14);     /* RESTORE RETURN ADDRESS        */
         IDARST14
*   RETURN;                         /* RETURN TO MAIN ROUTINE        */
@EL00003 DS    0H                                                  0287
@EF00003 DS    0H                                                  0287
@ER00003 BR    @14                                                 0287
*   END RELINX;                     /* END OF RELINX PROC.           */
*                                                                  0289
*/********************************************************************/
*/*  RELDIWA: ROUTINE USED TO RELEASE THE DIWA IF OWNED.             */
*/*                                                                  */
*/*  FUNCTION: CHECK IF THIS REQUEST OWNS THE DIWA, FREE THE INSERT  */
*/*            BUFFER BEFORE RELEASING THE DIWA.  THE INSERT BUFFER  */
*/*            CAN BE RELEASED IN ONE OF TWO WAYS:                   */
*/*            (1) IF THE INSERT BUFFER WAS SWAPPED WITH THE DATA    */
*/*                BUFC BY THE CI SPLIT ROUTINE, IT HAS TO BE        */
*/*                SWAPPED BACK AND FREED BY IDAFREEB.               */
*/*            (2) IF THE INSERT BUFFER WAS ADDED TO THE DATA BUFFER */
*/*                CHAIN, IT HAS TO BE SUBTRACTED FROM THE CHAIN BY  */
*/*                IDASBF.                                           */
*/********************************************************************/
*                                                                  0289
*RELDIWA:                                                          0289
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0289
RELDIWA  DS    0H                                                  0290
*   GEN(IDASVR14);                  /* SAVE RETURN ADDRESS           */
         IDASVR14
*   IF DIWATV^=ZERO THEN            /* IS DIWA BUSY?                 */
         L     @10,AMBIWA(,RAMB)                                   0291
         CLI   DIWATV(@10),0                                       0291
         BE    @RF00291                                            0291
*     IF DIWPLH=RPLH THEN           /* DOES PLH OWN DIWA ?           */
         C     RPLH,DIWPLH(,@10)                                   0292
         BNE   @RF00292                                            0292
*       DO;                         /* YES                           */
*                                                                  0293
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK IF A SWAP WAS DONE BETWEEN THE INSERT BUFFER      */
*         /* POINTER (DIWBUFC) AND THE DATA BUFC POINTER (PLHDBUFC)  */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0294
*         IF DIWBUFC^=BUFDBUFC THEN /* DOES DIWBUFC PT TO INSERT BUF?*/
         L     @05,DIWBUFC(,@10)                                   0294
         L     @15,AMBBUFC(,RAMB)                                  0294
         L     @15,BUFDBUFC(,@15)                                  0294
         CR    @05,@15                                             0294
         BE    @RF00294                                            0294
*           IF PLHDBUFC=BUFDBUFC THEN/* PLH PTS TO INSERT BUFC?      */
