*             BUFCMW=OFF;           /* SET BIT OFF                   */
         NI    BUFCMW(RBUFC),B'01111111'                           0368
*             RPLERRCD=RPLDSERR;    /* SET FEEDBACK CODE             */
         MVI   RPLERRCD(RRPL),X'24'                                0369
