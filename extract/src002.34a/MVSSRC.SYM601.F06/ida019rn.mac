         TITLE 'IDA019RN  INDEXING SUBROUTINES                         *
                        '
IDA019RN CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'IDA019RN  78.172'                                 0001
IDAAQR   DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00097-IDAAQR)
         ENTRY IDAAQR
IDAER    DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00226-IDAER)
         ENTRY IDAER
         DROP  @15
@PROLOG  BALR  @11,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@11                                         0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*                                                                  0097
*/*    IDAAQR SUBROUTINE                                             */
*/*       FUNCTION                                                   */
*/*        OBTAINS AN RBA FOR AN INDEX RECORD                        */
*/*        PREFORMATS THE RECORD IF REQUIRED                         */
*/*       INPUT -                                                    */
*/*        RWORK1 = POINTER TO CURRENT WORK AREA                     */
*/*       OUTPUT -                                                   */
*/*        RBA WILL BE PLACED INTO ICWCRBA FIELD OF WORK AREA        */
*/*        AFTER ICWCRBA HAS BEEN MOVED TO ICWPRBA                   */
*/*       RETURN -                                                   */
*/*        TO CALLER IF NO ERRORS WERE ENCOUNTERED                   */
*/*        TO CALLER OF CALLER IN CASE OF I/O ERROR                  */
*/*                                                                  */
*                                                                  0097
*IDAAQR:                                                           0097
*   ENTRY;                          /*                               */
@EP00097 DS    0H                                                  0098
*   GEN SETS(PLHRETO,PLHRET1)(IDASVR14);/* SAVE REG 14               */
         IDASVR14
*   RESPECIFY                                                      0099
*    (RWORK2) RSTD;                 /*                               */
*   RWORK2=AMDPARDB;                /* POINT TO FIRST ARDB           */
         L     @10,AMBDSB(,RAMB)                                   0100
         L     RWORK2,AMDPARDB(,@10)                               0100
*   IF AMDSDT=ON THEN               /* IS SEQ SET WITH DATA          */
         TM    AMDSDT(@10),B'00100000'                             0101
         BNO   @RF00101                                            0101
*     DO;                           /* YES                           */
*       IF ICWLN=ICWSS THEN         /* IS THIS SEQ SET LEVEL         */
         CLC   ICWLN(2,RWORK1),@CH00053                            0103
         BNE   @RF00103                                            0103
*         DO;                       /* YES                           */
*           IF ICWKRDS=ON THEN      /* IS THIS A KEY RANGE DS        */
         TM    ICWKRDS(RWORK1),B'00000100'                         0105
         BNO   @RF00105                                            0105
*             DO WHILE RWORK2->ARDNPTR/* YES, START ROUTINE TO       */
*                   ^=ZERO;         /* LOCATE ARDB WITH ARDKR        */
         B     @DE00106                                            0106
@DL00106 DS    0H                                                  0107
*               IF RWORK2->ARDKR=ON THEN/* = ON AND LO AND HI KEY    */
         TM    ARDKR(RWORK2),B'10000000'                           0107
         BNO   @RF00107                                            0107
*                 DO;               /* BRACKETING PLH KEY            */
*                   IF RWORK2->ARDKEYS(1:AMDKEYLN)/* IS LO KEY LESS  */
*                       ^>PLHKEY(1:AMDKEYLN) THEN/* THAN OR = PLHKEY */
         L     @10,PLHKEYPT(,RPLH)                                 0109
         L     @07,AMBDSB(,RAMB)                                   0109
         LH    @07,AMDKEYLN(,@07)                                  0109
         LR    @15,@07                                             0109
         BCTR  @15,0                                               0109
         EX    @15,@SC00895                                        0109
         BH    @RF00109                                            0109
*                     DO;           /* YES, POINT TO HI KEY          */
*                       RPARM1=RWORK2+AMDKEYLN;/* OF THE ARDB        */
         ALR   @07,RWORK2                                          0111
         LR    RPARM1,@07                                          0111
*                       IF RPARM1->ARDKEYS(1:AMDKEYLN)/* IS HI KEY > */
*                           ^<PLHKEY(1:AMDKEYLN) THEN/* OR = TO    0112
*                                      PLHKEY                        */
         EX    @15,@SC00897                                        0112
         BNL   @RT00112                                            0112
*                         GO TO PROCEED;/* YES, ARDB FOUND           */
*                     END;          /*                               */
*                 END;              /*                               */
@RF00109 DS    0H                                                  0116
*               RWORK2=RWORK2->ARDNPTR;/* POINT TO NEXT ARDB         */
@RF00107 L     RWORK2,ARDNPTR(,RWORK2)                             0116
*             END;                  /*                               */
@DE00106 L     @10,ARDNPTR(,RWORK2)                                0117
         LTR   @10,@10                                             0117
         BNZ   @DL00106                                            0117
*           ELSE                    /* NOT KEY RANGE DATA SET        */
*             DO WHILE RWORK2->ARDNPTR/* LOOP THRU ARDB STRING       */
*                   ^=ZERO;         /* LOOKING FOR THE ONE           */
         B     @RC00105                                            0118
@RF00105 B     @DE00118                                            0118
@DL00118 DS    0H                                                  0119
*               IF RWORK2->ARDSS=ON THEN/* WHICH REPRESENTS THE      */
         TM    ARDSS(RWORK2),B'00100000'                           0119
         BO    @RT00119                                            0119
*                 GO TO PROCEED;    /* SEQUENCE                      */
*               ELSE                /*                               */
*                 RWORK2=RWORK2->ARDNPTR;/* MOVE TO NEXT ARDB        */
         L     RWORK2,ARDNPTR(,RWORK2)                             0121
*             END;                  /*                               */
@DE00118 L     @10,ARDNPTR(,RWORK2)                                0122
         LTR   @10,@10                                             0122
         BNZ   @DL00118                                            0122
*         END;                      /*                               */
*       ELSE                        /* NOT SEQUENCE SET LVL          */
*         DO WHILE RWORK2->ARDNPTR  /* SO LOOP THRU ARDB             */
*               ^=ZERO;             /* STRING TO LOCATE ARDB         */
         B     @RC00103                                            0124
@RF00103 B     @DE00124                                            0124
@DL00124 DS    0H                                                  0125
*           IF RWORK2->ARDHLI=ON THEN/* REPRESENTING THE HI          */
         TM    ARDHLI(RWORK2),B'01000000'                          0125
         BO    @RT00125                                            0125
*             GO TO PROCEED;        /* LEVELS OF INDEX               */
*           RWORK2=RWORK2->ARDNPTR; /* MOVE TO NEXT ARDB             */
         L     RWORK2,ARDNPTR(,RWORK2)                             0127
*         END;                      /*                               */
@DE00124 L     @10,ARDNPTR(,RWORK2)                                0128
         LTR   @10,@10                                             0128
         BNZ   @DL00124                                            0128
*     END;                          /*                               */
@RC00103 DS    0H                                                  0130
*PROCEED:                                                          0130
*   IF RWORK2->ARDHRBA^<            /* IS SPACE AVAILABLE IN         */
*       RWORK2->ARDERBA THEN        /* THIS ARDB                     */
@RF00101 DS    0H                                                  0130
PROCEED  CLC   ARDHRBA(4,RWORK2),ARDERBA(RWORK2)                   0130
         BL    @RF00130                                            0130
*     DO;                           /* NO                            */
*       PLHEOVR=AMBEOVAR;           /* REQUEST ALLOC. BY RBA         */
         MVI   PLHEOVR(RPLH),B'10000010'                           0132
*       PLHEOVPT=ADDR(RWORK2->ARDERBA);/* SET RBA TO EXTEND          */
         LA    @10,ARDERBA(,RWORK2)                                0133
         ST    @10,PLHEOVPT(,RPLH)                                 0133
*       RPARM1=RWORK2;              /* SAVE ARDB ADDRESS             */
         LR    RPARM1,RWORK2                                       0134
*I01:                                                              0135
*       DO;                         /* IDACALL(IDAEOVIF)             */
I01      DS    0H                                                  0136
*         CALL IDAEOVIF;            /* EXIT TO IDAEOVIF              */
         L     @15,@CV00854                                        0136
         BALR  @14,@15                                             0136
*         GEN(BALR @11,0);          /* LOAD ADDR OF REENTRY          */
         BALR @11,0
*         GEN(USING *,@11);         /* ESTABLISH ADDRESSING          */
         USING *,@11
*         GEN(L     @11,=A(@PSTART));/* RELOAD BASE                  */
         L     @11,=A(@PSTART)
*         GEN(USING @PSTART,@11);   /* ESTABLISH ADDRESSING          */
         USING @PSTART,@11
*       END I01;                    /* GO TO EOV ROUTINE             */
*       RESPECIFY                                                  0142
*        (RWORK1,                                                  0142
*         RWORK2) RSTD;             /*                               */
*       RESPECIFY                                                  0143
*        (R15) RSTD;                /*                               */
*       IF RPLERREG^=ZERO THEN      /* WAS SPACE AVAILABLE           */
         CLI   RPLERREG(RRPL),0                                    0144
         BE    @RF00144                                            0144
*         DO;                       /* NO                            */
*           IF AMBCREAT=ON THEN     /* IS THIS CREATE MODE           */
         TM    AMBCREAT(RAMB),B'10000000'                          0146
         BNO   @RF00146                                            0146
*             PLHKEYMD=OFF;         /* YES, FORCE ERRORS IF USER   0147
*                                      CONTINUES TO ADD RECORDS TO 0147
*                                      DATA SET                      */
         NI    PLHKEYMD(RPLH),B'11011111'                          0147
*           RAMB=AMBLDTA;           /* RESTORE RAMB                  */
@RF00146 L     @14,RPLDACB(,RRPL)                                  0148
         L     @14,ACBAMBL(,@14)                                   0148
         L     RAMB,AMBLDTA(,@14)                                  0148
*           R15=IOERR;              /* SET CATASTROPHIC ERROR        */
         LA    R15,4                                               0149
*           GEN(IDARST14);          /* POINT TO CALLER               */
         IDARST14
*           CALL EXIT;              /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0151
*           RESPECIFY                                              0152
*            (R15) UNRSTD;          /*                               */
*         END;                      /*                               */
*       DO;                         /*                               */
@RF00144 DS    0H                                                  0155
*         RWORK1=AMBIWA;            /* POINT TO FIRST WA             */
         L     RWORK1,AMBIWA(,RAMB)                                0155
*         RWORK2=ICWADD;            /* GET ADDRESS OF CURRENT        */
         L     RWORK2,ICWADD(,RWORK1)                              0156
*         RWORK1=RWORK2;            /* WA IN RWORK1                  */
         LR    RWORK1,RWORK2                                       0157
*         RWORK2=RPARM1;            /* POINT TO ARDB                 */
         LR    RWORK2,RPARM1                                       0158
*         RWORK2->ARDUSED=OFF;      /* INSURE PREFORMAT              */
         NI    ARDUSED(RWORK2),B'11111011'                         0159
*       END;                        /*                               */
*     END;                          /*                               */
*   DO;                             /*                               */
@RF00130 DS    0H                                                  0163
*     ICWPRBA=ICWCRBA;              /* MOVE CURR. TO PREV. RBA       */
         L     @10,ICWCRBA(,RWORK1)                                0163
         ST    @10,ICWPRBA(,RWORK1)                                0163
*     ICWCRBA=RWORK2->ARDHRBA;      /* MOVE NEW RBA TO WA            */
         L     @10,ARDHRBA(,RWORK2)                                0164
         ST    @10,ICWCRBA(,RWORK1)                                0164
*     AMDNLR=AMDNLR+ONE;            /* BUMP STATISTICS COUNT         */
         L     @07,AMBDSB(,RAMB)                                   0165
         LA    @00,1                                               0165
         AL    @00,AMDNLR(,@07)                                    0165
         ST    @00,AMDNLR(,@07)                                    0165
*     AMDASPA=AMDASPA-AMDCINV;      /* REDUCE TOTAL NO. BYTES        */
         L     @15,AMDCINV(,@07)                                   0166
         L     @00,AMDASPA(,@07)                                   0166
         SLR   @00,@15                                             0166
         ST    @00,AMDASPA(,@07)                                   0166
*     RWORK2->ARDHRBA=RWORK2->ARDHRBA/* MOVE HIGH USED               */
*         +AMDCINV;                 /* RBA UP BY CI LENGTH           */
         ALR   @15,@10                                             0167
         ST    @15,ARDHRBA(,RWORK2)                                0167
*     IF RWORK2->ARDUSED=OFF THEN   /* HAS ANY SPACE BEEN USED       */
         TM    ARDUSED(RWORK2),B'00000100'                         0168
         BNZ   @RF00168                                            0168
*       DO;                         /* NO, PREFORMAT IS REQD.        */
*         ICWARDBP=RWORK2;          /* SAVE ARDB PTR IN WA           */
         ST    RWORK2,ICWARDBP(,RWORK1)                            0170
*         RPARM1=ICWCRBA;           /* GET INITIAL RBA AND           */
         LR    RPARM1,@10                                          0171
*I02:                                                              0172
*         DO;                       /* IDACALL(IDA019RK)             */
I02      DS    0H                                                  0173
*           CALL IDA019RK;          /* EXIT TO IDA019RK              */
         L     @15,@CV00855                                        0173
         BALR  @14,@15                                             0173
*           GEN(BALR @11,0);        /* LOAD ADDR OF REENTRY          */
         BALR @11,0
*           GEN(USING *,@11);       /* ESTABLISH ADDRESSING          */
         USING *,@11
*           GEN(L     @11,=A(@PSTART));/* RELOAD BASE                */
         L     @11,=A(@PSTART)
*           GEN(USING @PSTART,@11); /* ESTABLISH ADDRESSING          */
         USING @PSTART,@11
*         END I02;                  /* GO PREFORMAT                  */
*         RESPECIFY                                                0179
*          (RWORK1,                                                0179
*           RWORK2) RSTD;           /*                               */
*         IF RPLERREG^=ZERO THEN    /* WAS THERE AN I/O ERROR        */
         CLI   RPLERREG(RRPL),0                                    0180
         BNE   @RT00180                                            0180
*           GO TO FMTERR;           /* GO RETURN WITH ERROR          */
*         RWORK1=AMBIWA;            /* REESTABLISH RWORK1            */
         L     RWORK1,AMBIWA(,RAMB)                                0182
*         RWORK2=ICWADD;            /* AND RWORK2 REGS               */
         L     RWORK2,ICWADD(,RWORK1)                              0183
*         RWORK1=RWORK2;            /* BECAUSE THEY WERE             */
         LR    RWORK1,RWORK2                                       0184
*         RWORK2=ICWARDBP;          /* SUBJECT TO DESTRUCTION        */
         L     RWORK2,ICWARDBP(,RWORK1)                            0185
*         RWORK2->ARDUSED=ON;       /* MARK ARDB INITIALIZED         */
         OI    ARDUSED(RWORK2),B'00000100'                         0186
*       END;                        /*                               */
*     IF AMDREPL=ON×(AMDSDT=ON&     /* IS REPLICATION                */
*         ICWLN=ICWSS) THEN         /* SPECIFIED OR IMPLIED          */
@RF00168 L     @10,AMBDSB(,RAMB)                                   0188
         TM    AMDREPL(@10),B'00010000'                            0188
         BO    @RT00188                                            0188
         TM    AMDSDT(@10),B'00100000'                             0188
         BNO   @RF00188                                            0188
         CLC   ICWLN(2,RWORK1),@CH00053                            0188
         BE    @RT00188                                            0188
*       GO TO PREFMT;               /* YES                           */
*     DO;                           /* ARE ANY PREFORMATTED          */
@RF00188 DS    0H                                                  0191
*       RPARM1=((ICWCRBA/AMDCINV)//(AMDCIPCA))+ONE;/* RECORDS        */
         L     @10,AMBDSB(,RAMB)                                   0191
         L     @09,ICWCRBA(,RWORK1)                                0191
         SLR   @08,@08                                             0191
         D     @08,AMDCINV(,@10)                                   0191
         LH    @10,AMDCIPCA(,@10)                                  0191
         SLR   @08,@08                                             0191
         DR    @08,@10                                             0191
         AL    @08,@CF00053                                        0191
         LR    RPARM1,@08                                          0191
*       IF RPARM1=AMDCIPCA&RWORK2->ARDHRBA^=/* REMAINING             */
*           RWORK2->ARDERBA THEN    /*                               */
         CR    RPARM1,@10                                          0192
         BNE   @RF00192                                            0192
         CLC   ARDHRBA(4,RWORK2),ARDERBA(RWORK2)                   0192
         BE    @RF00192                                            0192
*PREFMT:                                                           0193
*         DO;                       /* FINAL CHK BEFORE PREFMT       */
PREFMT   DS    0H                                                  0194
*           IF RWORK2->ARDHRBA<     /* IS ANY SPACE AVAILABLE        */
*               RWORK2->ARDERBA THEN/* IN ARDB TO PREFORMAT          */
         L     @10,ARDHRBA(,RWORK2)                                0194
         CL    @10,ARDERBA(,RWORK2)                                0194
         BNL   @RF00194                                            0194
*             DO;                   /* YES                           */
*               RPARM1=RWORK2->ARDHRBA;/* GET RBA TO PREFORMAT       */
         LR    RPARM1,@10                                          0196
*I03:                                                              0197
*               DO;                 /* IDACALL(IDA019RK)             */
I03      DS    0H                                                  0198
*                 CALL IDA019RK;    /* EXIT TO IDA019RK              */
         L     @15,@CV00855                                        0198
         BALR  @14,@15                                             0198
*                 GEN(BALR @11,0);  /* LOAD ADDR OF REENTRY          */
         BALR @11,0
*                 GEN(USING *,@11); /* ESTABLISH ADDRESSING          */
         USING *,@11
*                 GEN(L     @11,=A(@PSTART));/* RELOAD BASE          */
         L     @11,=A(@PSTART)
*                 GEN(USING @PSTART,@11);/* ESTABLISH ADDRESSING     */
         USING @PSTART,@11
*               END I03;            /* GO TO PREFORMAT ROUTINE       */
*               RESPECIFY                                          0204
*                (RWORK1) RSTD;     /*                               */
*               IF RPLERREG^=ZERO THEN/* WAS THERE AN ERROR          */
         CLI   RPLERREG(RRPL),0                                    0205
         BE    @RF00205                                            0205
*FMTERR:                                                           0206
*                 DO;               /* YES                           */
FMTERR   DS    0H                                                  0207
*                   RESPECIFY                                      0207
*                    (R15) RSTD;    /*                               */
*                   R15=IOERR;      /* SHOW ERROR IN REG 15          */
         LA    R15,4                                               0208
*                   RAMB=AMBLDTA;   /* RESTORE RAMB FOR CALLER       */
         L     @14,RPLDACB(,RRPL)                                  0209
         L     @14,ACBAMBL(,@14)                                   0209
         L     RAMB,AMBLDTA(,@14)                                  0209
*                   GEN(IDARST14);  /* POINT BACK TO CALLER          */
         IDARST14
*                   CALL EXIT;      /* RETURN                        */
         BAL   @14,EXIT                                            0211
*                   RESPECIFY                                      0212
*                    (R15) UNRSTD;  /*                               */
*                 END;              /*                               */
*               DO;                 /* REESTABLISH RWORK1            */
@RF00205 DS    0H                                                  0215
*                 RESPECIFY                                        0215
*                  (RWORK2) RSTD;   /*                               */
*                 RWORK1=AMBIWA;    /* BECAUSE OF EXPOSURE           */
         L     RWORK1,AMBIWA(,RAMB)                                0216
*                 RWORK2=ICWADD;    /* TO LOSS IN PREFORMAT          */
         L     RWORK2,ICWADD(,RWORK1)                              0217
*                 RWORK1=RWORK2;    /* PROCESS                       */
         LR    RWORK1,RWORK2                                       0218
*               END;                /*                               */
*             END;                  /*                               */
*         END;                      /*                               */
@RF00194 DS    0H                                                  0222
*     END;                          /*                               */
@RF00192 DS    0H                                                  0223
*ENDPRFT:                                                          0223
*   END;                            /*                               */
ENDPRFT  DS    0H                                                  0224
*   RESPECIFY                                                      0224
*    (RWORK2) UNRSTD;               /*                               */
*   CALL EXIT;                      /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0225
*                                                                  0226
*/*                                                                  */
*/*    IDAER SUBROUTINE                                              */
*/*                                                                  */
*/*       FUNCTION -                                                 */
*/*        REMOVES THE DUMMY ENTRY AT THE HIGH KEY END               */
*/*        OF THE INDEX RECORD IN THE CURRENT BUFFER IN              */
*/*        CORE.  CONVERTS THE ENTRY PRECEEDING THE DUMMY            */
*/*        TO A SECTION ENTRY, IF IT IS NOT ALREADY ONE.             */
*/*       INPUT -                                                    */
*/*        RWORK1 = POINTER TO CURRENT WORK AREA.                    */
*/*       OUTPUT -                                                   */
*/*        NONE                                                      */
*/*       RETURN -                                                   */
*/*        ALWAYS TO CALLER.  NO ERRORS ARE DETECTED.                */
*/*                                                                  */
*                                                                  0226
*IDAER:                                                            0226
*   ENTRY;                          /*                               */
@EP00226 DS    0H                                                  0227
*   GEN SETS(PLHRETO,PLHRET1)(IDASVR14);/* SAVE REG 14               */
         IDASVR14
*   RESPECIFY                                                      0228
*    (RWORK2,                                                      0228
*     RWORK3,                                                      0228
*     RWORK4,                                                      0228
*     RWORK0) RSTD;                 /*                               */
*   RWORK2=ICWBUFC->BUFCBAD;        /* POINT TO INDEX BUFFER         */
         L     @10,ICWBUFC(,RWORK1)                                0229
         L     RWORK2,BUFCBAD(,@10)                                0229
*   RWORK3=RWORK2+RWORK2->IXHLEO+   /* POINT TO THE DUMMY            */
*       LENGTH(IBFLPF)+             /* ENTRYS POINTER                */
*       LENGTH(IBFLPL);             /* FIELD                         */
         LR    RWORK3,RWORK2                                       0230
         LH    @10,IXHLEO(,RWORK2)                                 0230
         ALR   RWORK3,@10                                          0230
         AL    RWORK3,@CF00108                                     0230
*   RWORK4=RWORK2->IXHPTLS;         /* PICK UP MASK FROM HDR.        */
         SLR   RWORK4,RWORK4                                       0231
         IC    RWORK4,IXHPTLS(,RWORK2)                             0231
*   IF ICWLN=ICWSS×ICWSPLIT=ON THEN /* SEQ SET OR SPLIT              */
         CLC   ICWLN(2,RWORK1),@CH00053                            0232
         BE    @RT00232                                            0232
         TM    ICWSPLIT(RWORK1),B'00000010'                        0232
         BNO   @RF00232                                            0232
@RT00232 DS    0H                                                  0233
*     DO;                           /* YES                           */
*       RWORK0=RWORK3;              /* SAVE DUMMYS PTR. ADD.         */
         LR    RWORK0,RWORK3                                       0234
*       GEN(EX  RWORK4,PICKPT);     /* EXTRACT DUMMYS PTR            */
         EX  RWORK4,PICKPT
*       RWORK3=RWORK2+RWORK2->IXHFSO;/* POINT TO FREE SPACE          */
         LH    @10,IXHFSO(,RWORK2)                                 0236
         LR    RWORK3,RWORK2                                       0236
         ALR   RWORK3,@10                                          0236
*       IF RWORK2->IXHFSO^<LENGTH(IDAIXH) THEN/*                     */
         C     @10,@CF00145                                        0237
         BL    @RF00237                                            0237
*         GEN(EX  RWORK4,STOREPT);  /* SAVE PTR IN FREE SPACE        */
         EX  RWORK4,STOREPT
*       RWORK3=RWORK0;              /* POINT BACK TO DUMMY PTR       */
@RF00237 LR    RWORK3,RWORK0                                       0239
*       RWORK2->IXHFSO=RWORK2->IXHFSO+/* ADJUST FREE SPACE           */
*           ICWPTL;                 /* DISPLACEMENT IN HEADER        */
         SLR   @10,@10                                             0240
         IC    @10,ICWPTL(,RWORK1)                                 0240
         AH    @10,IXHFSO(,RWORK2)                                 0240
         STH   @10,IXHFSO(,RWORK2)                                 0240
*     END;                          /*                               */
*   RPARM1=ZERO;                    /* PREPARE TO STORE ZEROS        */
@RF00232 SLR   RPARM1,RPARM1                                       0242
*   GEN(EX  RWORK4,STOREPT);        /* CLEAR DUMMYS PTR FIELD        */
         EX  RWORK4,STOREPT
*   RESPECIFY                                                      0244
*    (RWORK0) UNRSTD;               /*                               */
*   RWORK3=RWORK2+RWORK2->IXHSEO;   /* POINT TO FIRST SECTION        */
         LR    RWORK3,RWORK2                                       0245
         LH    @10,IXHSEO(,RWORK2)                                 0245
         ALR   RWORK3,@10                                          0245
*   DO WHILE RWORK3-RPARM1^=RWORK2+ /* LOOP TO WORK                  */
*         RWORK2->IXHLEO;           /* THROUGH INDEX RECORD TO LOCATE
*                                      THE LAST SECTION WRITTEN    0246
*                                      BEFORE THE DUMMY              */
         B     @DE00246                                            0246
@DL00246 DS    0H                                                  0247
*     RWORK3=RWORK3-RPARM1;         /* MOVE TO NEXT SECTION          */
         SLR   RWORK3,RPARM1                                       0247
*     RWORK4=RWORK3-                /* ESTABLISH POINTER TO          */
*         RWORK3->IBFLPL-           /* SECTION CHAINING              */
*         LENGTH(IXSELL);           /* FIELD                         */
         LR    RWORK4,RWORK3                                       0248
         SLR   @10,@10                                             0248
         IC    @10,IBFLPL(,RWORK3)                                 0248
         SLR   RWORK4,@10                                          0248
         BCTR  RWORK4,0                                            0248
         BCTR  RWORK4,0                                            0248
