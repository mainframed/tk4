         TITLE 'IGG0199W    SAM-SI OPEN EXECUTOR                       *
                        '
IGG0199W CSECT ,                                                   0001
@PROLOG  BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         EJECT
         EJECT
         EJECT
*PROCESS:                                                          0058
*                                                                  0058
*        /***********************************************************/
*        /*          INITILIZE MODULE TO RUN IN USER KEY            */
*        /***********************************************************/
*                                                                  0058
*        RUKEY = DXUKEY;                /* SAVE USER KY IN R2 Y02082*/
PROCESS  L     @11,WTGCWA-1(,RWTGC)                                0058
         SLR   RUKEY,RUKEY                                         0058
         IC    RUKEY,DXUKEY(,@11)                                  0058
*        GEN (MODESET KEYADDR=(2));     /*                    Y02082*/
*                                                                  0059
         MODESET KEYADDR=(2)
*        RDCB = PARCDCB;                /* SET BASE FOR DCB   XM6042*/
         L     RDCB,PARCDCB(,RPARC)                                0060
*        RDCB = RDCB + 0;               /* ZERO HI-ORDER BYTE XM6042*/
*        RCICB = DCBCICB;               /* SET BASE FOR CICB  XM6042*/
         L     RCICB,DCBCICB(,RDCB)                                0062
*        RRPL = ADDR(CIRPL);            /* SET BASE FOR RPL   XM6042*/
*                                                                  0063
         LA    RRPL,CIRPL(,RCICB)                                  0063
*        /***********************************************************/
*        /*                                                         */
*        /* DETERMINE BUFFER REQUIREMENTS                           */
*        /*                                                         */
*        /***********************************************************/
*                                                                  0064
*                                                                  0064
*CHECBUFS:/* TEST FOR A VALID BUFFER CONTROL BLOCK ADDRESS. IF LOW   */
*         /* ORDER BIT OF DCBBUFCB IS ZERO THEN ADDRESS IS VALID.    */
*                                                                  0064
*         IF BUFADDR(32) = '0'B                                    0064
*            THEN DO;                       /* YES                   */
CHECBUFS TM    BUFADDR+3(RDCB),B'00000001'                         0064
         BNZ   @RF00064                                            0064
*              IF BCBBLDR(1) = '0'B         /* WAS BIULDRCD ISSUED   */
*                & DCBRECFM(1:2) = '01'B    /*  FOR A VARIABLE       */
*                & DCBRECFM(5)   = '1'B     /*  SPANNED DCB?         */
*                THEN DO;                   /* NOT ISSUED            */
         L     @11,DCBBUFCB(,RDCB)                                 0066
         TM    BCBBLDR(@11),B'10000000'                            0066
         BNZ   @RF00066                                            0066
         TM    DCBRECFM(RDCB),B'01000000'                          0066
         BNO   @RF00066                                            0066
         TM    DCBRECFM(RDCB),B'10000000'                          0066
         BNZ   @RF00066                                            0066
         TM    DCBRECFM(RDCB),B'00001000'                          0066
         BNO   @RF00066                                            0066
*                  RSIZE = DCBLRECL;        /* LOGICAL RECORD LENGTH */
         LH    RSIZE,DCBLRECL(,RDCB)                               0068
         N     RSIZE,@CF00434                                      0068
*                  GEN (GETMAIN R,LV=(RSIZE),SP=250);              0069
         GETMAIN R,LV=(RSIZE),SP=250
*                  BCBRECDA = R1;           /* INIT RECDAREA PTR     */
         L     @11,DCBBUFCB(,RDCB)                                 0070
         ST    R1,BCBRECDA(,@11)                                   0070
*                     END;                  /* END OF NOT BUILDRCD   */
*                                                                  0071
*              IF DCBRECFM(1:2) = '01'B /* THIS DATA SET FOR   XM6180*/
*                & DCBRECFM(5)   = '1'B /* VS RECORDS ?        XM6180*/
*                  THEN                 /* YES, SET UP TO USE  XM6180*/
@RF00066 TM    DCBRECFM(RDCB),B'01000000'                          0072
         BNO   @RF00072                                            0072
         TM    DCBRECFM(RDCB),B'10000000'                          0072
         BNZ   @RF00072                                            0072
         TM    DCBRECFM(RDCB),B'00001000'                          0072
         BNO   @RF00072                                            0072
*                    DO;                /* THE RECORD AREA     XM6180*/
*                      RPLAREA = BCBRECDA; /* PUT PTR IN RPL   XM6180*/
         L     @11,DCBBUFCB(,RDCB)                                 0074
         L     @11,BCBRECDA(,@11)                                  0074
         ST    @11,RPLAREA(,RRPL)                                  0074
*                      CIRAREA = BCBRECDA; /* PUT PTR IN CICB  XM6180*/
         ST    @11,CIRAREA(,RCICB)                                 0075
*                    END;               /* END OF VS SETUP     XM6180*/
*                  ELSE                 /* NOT VS, SO USE THE  XM6180*/
*                    RPLAREA = BCBFSBUF;/* 1ST BUFFER IN POOL  XM6180*/
*                                                                  0077
         B     @RC00072                                            0077
@RF00072 L     @11,DCBBUFCB(,RDCB)                                 0077
         L     @11,BCBFSBUF(,@11)                                  0077
         ST    @11,RPLAREA(,RRPL)                                  0077
*              IF DCBBUFNO = 0          /* IF BUFFER COUNT NOT XM5700*/
*                THEN                   /* REFLECTED  IN DCB   XM5700*/
@RC00072 CLI   DCBBUFNO(RDCB),0                                    0078
         BNE   @RF00078                                            0078
*                  DO;                  /* MERGE IN VALUE FROM XM5700*/
*                                                                  0079
*                    /* SUPERVISOR KEY REQ TO STORE IN DCB (DM KEY)
*                       AND REFERENCE BCB (UK)                 Y02082*/
*                    GEN (MODESET EXTKEY=ZERO);                    0080
*                                                                  0080
         MODESET EXTKEY=ZERO
*                    DCBBUFNO = BCBBUFNO;   /* BUF POOL BLOCK  XM5700*/
*                                                                  0081
         L     @11,DCBBUFCB(,RDCB)                                 0081
         LH    @11,BCBBUFNO(,@11)                                  0081
         STC   @11,DCBBUFNO(,RDCB)                                 0081
*                    /* REESTABLISH USER KEY FOR INITRPL ROUTINE     */
*                    RDCBSAVE = RDCB;   /* SAVE PTR TO DCB     Y02082*/
         LR    RDCBSAVE,RDCB                                       0082
*                    RUKEY = DXUKEY;    /* SAVE USER KY IN R2  Y02082*/
         L     @11,WTGCWA-1(,RWTGC)                                0083
         SLR   RUKEY,RUKEY                                         0083
         IC    RUKEY,DXUKEY(,@11)                                  0083
*                    GEN (MODESET KEYADDR=(2));                    0084
         MODESET KEYADDR=(2)
*                    RDCB = RDCBSAVE;   /* RESTORE DCB PTR     Y02082*/
*                                                                  0085
         LR    RDCB,RDCBSAVE                                       0085
*                  END;                 /*                     XM5700*/
*              GO TO INITRPL;             /* INITIALIZE THE RPL    */
         B     INITRPL                                             0087
*               END;                      /* END OF VALIDBCB GROUP */
*                                                                  0088
*        GEN   (EJECT);                 /* SKIP TO A NEW PAGE       */
@RF00064 DS    0H                                                  0089
         EJECT
*         /* SET BUFFER LENGTH USING VALUE FROM DCB            XA3128*/
*         RBUFL = DCBBUFL;                  /* BUFL            XA3128*/
*                                                                  0090
         LH    RBUFL,DCBBUFL(,RDCB)                                0090
         N     RBUFL,@CF00434                                      0090
*         IF RBUFL = 0 THEN                 /* IF DCBBUFL IS @ZA12902*/
         LTR   RBUFL,RBUFL                                         0091
         BNZ   @RF00091                                            0091
*            RBUFL = DCBBLKSI;              /* ZERO,USE BLKSI@ZA12902*/
*                                                                  0092
         LH    RBUFL,DCBBLKSI(,RDCB)                               0092
         N     RBUFL,@CF00434                                      0092
*         /***********************************************************/
*         /* IS THIS A BSAM DCB ?                                    */
*         /***********************************************************/
*                                                                  0093
*         IF DCBCIND2(8) = '0'B                                    0093
*            THEN                           /* YES                   */
@RF00091 TM    DCBCIND2(RDCB),B'00000001'                          0093
         BNZ   @RF00093                                            0093
*              DO ;                                                0094
*                                                                  0094
*                /* HAS USER SUPPLIED BUFNO ?                        */
*                IF DCBBUFNO = 0                                   0095
*                   THEN                    /* NO                    */
*                                                                  0095
         CLI   DCBBUFNO(RDCB),0                                    0095
         BE    @RT00095                                            0095
*                     GO TO INITRPL;        /* NEXT ENTRY IN WTG     */
*                                                                  0096
*                   ELSE                    /* USER SUPPLIED BUFNO   */
*                     DO;                                          0097
*                                                                  0097
*                       RBUFNO = DCBBUFNO;  /* PICK UP BUFFER NUMBER */
         SLR   RBUFNO,RBUFNO                                       0098
         IC    RBUFNO,DCBBUFNO(,RDCB)                              0098
*                       GO TO GETBUFRS;     /* GO GET BUFFERS        */
*                                                                  0099
         B     GETBUFRS                                            0099
*                     END;                  /* END OF DO GROUP       */
*              END;                                                0101
*                                                                  0101
*            ELSE                           /* NOT BSAM DCB          */
*                                                                  0102
*            /********************************************************/
*            /* ASSUME QSAM DCB                                      */
*            /********************************************************/
*                                                                  0102
*            /********************************************************/
*            /* SET BUFFER LENGTH                                    */
*            /********************************************************/
*                                                                  0102
*            /* LOCATE MODE SPECIFIED ?                              */
*              IF DCBMACRF(5)   = '1'B × DCBMACRF(13)  = '1'B THEN/*
*                                              LOCATE MODE ?   YM5890*/
@RF00093 TM    DCBMACRF(RDCB),B'00001000'                          0102
         BO    @RT00102                                            0102
         TM    DCBMACRF+1(RDCB),B'00001000'                        0102
         BNO   @RF00102                                            0102
@RT00102 DS    0H                                                  0103
*                 IF DCBRECFM(1:2) = '01'B  /* VARIABLE        YM5890*/
*                   & DCBRECFM(5)='1'B THEN /* SPANNED RECS    YM5890*/
         TM    DCBRECFM(RDCB),B'01000000'                          0103
         BNO   @RF00103                                            0103
         TM    DCBRECFM(RDCB),B'10000000'                          0103
         BNZ   @RF00103                                            0103
         TM    DCBRECFM(RDCB),B'00001000'                          0103
         BNO   @RF00103                                            0103
*                   RBUFL=DCBLRECL;         /* YES, USE LRECL  YM5890*/
         LH    RBUFL,DCBLRECL(,RDCB)                               0104
         N     RBUFL,@CF00434                                      0104
*                 ELSE;                     /* NOT VS          YM5890*/
@RF00103 DS    0H                                                  0106
*              ELSE;                        /* NOT LOCATE      YM5890*/
*                                                                  0106
@RF00102 DS    0H                                                  0107
*              /* GET BUFNO FROM DCBBUFNO                      XA3128*/
*              RBUFNO = DCBBUFNO;           /* NO. OF BUFFERS  XA3128*/
*                                                                  0107
         SLR   RBUFNO,RBUFNO                                       0107
         IC    RBUFNO,DCBBUFNO(,RDCB)                              0107
*GETBUFRS:/***********************************************************/
*         /* ACQUIRE BUFFERS                                         */
*         /***********************************************************/
*                                                                  0108
*         /* BUFFER LENGTH MUST BE MULTIPLE OF 8 FOR MULTIPLE BUFFERS*/
*         IF RBUFNO > 1                     /* MULTIPLE BUFFERS ?    */
*           THEN                            /* YES                   */
GETBUFRS LA    @11,1                                               0108
         CR    RBUFNO,@11                                          0108
         BNH   @RF00108                                            0108
*             DO;                                                  0109
*                                                                  0109
*               CIWK1 = RBUFL//8;           /* STORE IN WORK AREA    */
*                                                                  0110
         LR    @14,RBUFL                                           0110
         SRDA  @14,32                                              0110
         D     @14,@CF00056                                        0110
         ST    @14,CIWK1(,RCICB)                                   0110
*               IF CIWK1 ^= 0               /* IS IT MULTIPLE OF 8 ? */
*                 THEN                      /* BUFL NOT MULTIPLE OF 8*/
*                                                                  0111
         LTR   @14,@14                                             0111
         BZ    @RF00111                                            0111
*                   /* ROUND UP TO NEAREST MULTIPLE OF 8             */
*                   RBUFL = (RBUFL/8 + 1) * 8;                     0112
         LR    @14,RBUFL                                           0112
         SRDA  @14,32                                              0112
         D     @14,@CF00056                                        0112
         ALR   @15,@11                                             0112
         SLA   @15,3                                               0112
         LR    RBUFL,@15                                           0112
*                 ELSE;                     /* BUFL IS MULTIPLE OF 8 */
@RF00111 DS    0H                                                  0114
*             END;                                                 0114
*                                                                  0114
*         /* CALCULATE CORE REQUIREMENT FOR BUFFERS                  */
*         RSIZE = (RBUFNO * RBUFL) + 8;                            0115
*                                                                  0115
@RF00108 LA    @11,8                                               0115
         LR    @15,RBUFL                                           0115
         MR    @14,RBUFNO                                          0115
         ALR   @15,@11                                             0115
         LR    RSIZE,@15                                           0115
*         /* IS FULLWORD ALIGNMENT SPECIFIED ?                       */
*         IF DCBBFALN (8) = '1'B                                   0116
*            THEN                           /* YES                   */
*                                                                  0116
         TM    DCBBFALN(RDCB),B'00000001'                          0116
         BNO   @RF00116                                            0116
*              /* ADD EIGHT BYTES FOR ALIGNMENT                      */
*              RSIZE = RSIZE + 8;                                  0117
*                                                                  0117
         ALR   RSIZE,@11                                           0117
*GETCORE: /***********************************************************/
*         /* GET CORE FOR BUFFERS, INCLUDING BUFCB, IN SUBPOOL 0.    */
*         /* NOTE: IF USER KEY IS 0, THIS WILL RESULT IN SUBPOOL 252.*/
*         /***********************************************************/
*                                                                  0118
*         IF DCBRECFM(1:2) = '01'B & DCBRECFM(5) = '1'B /*   @ZA19137*/
*         & DCBMACRF(13) = '1'B THEN    /* IF IN LOCATE MODE @ZA19137*/
@RF00116 DS    0H                                                  0118
GETCORE  TM    DCBRECFM(RDCB),B'01000000'                          0118
         BNO   @RF00118                                            0118
         TM    DCBRECFM(RDCB),B'10000000'                          0118
         BNZ   @RF00118                                            0118
         TM    DCBRECFM(RDCB),B'00001000'                          0118
         BNO   @RF00118                                            0118
         TM    DCBMACRF+1(RDCB),B'00001000'                        0118
         BNO   @RF00118                                            0118
*              RSIZE = RSIZE + 8;  /* ADD 8 BYTES TO BCB FOR @ZA19137*/
         AL    RSIZE,@CF00056                                      0119
*                                  /* SPANNED RECORDS        @ZA19137*/
*                                                                  0120
*         GEN (GETMAIN R,LV=(RSIZE),SP=0);/* GET BUFFER CORE   YM4641*/
*                                                                  0120
*                                                                  0120
@RF00118 DS    0H                                                  0120
         GETMAIN R,LV=(RSIZE),SP=0
*         IF CIFRAGM = '1'B                 /* SPANREC FLAG SET ?    */
*           THEN DO;                        /* YES                   */
         TM    CIFRAGM(RCICB),B'00100000'                          0121
         BNO   @RF00121                                            0121
*             CIRAREA = R1;                 /* FOR SPANNED RECORDS   */
         ST    R1,CIRAREA(,RCICB)                                  0123
*             GO TO ZROCHAIN;               /* PREPARE FOR XCTL      */
         B     ZROCHAIN                                            0124
*                END;                       /* END OF SPANREC DO GRP */
*           ELSE;                           /* SPANREC FLAG NOT SET  */
*                                                                  0126
@RF00121 DS    0H                                                  0127
*         /*  DATA MANAGEMENT KEY REQUIRED TO REFERENCE DCB FIELD    */
*         GEN (MODESET EXTKEY=DATAMGT); /*                     Y02082*/
*                                                                  0127
         MODESET EXTKEY=DATAMGT
*         DCBBUFCB = R1;                    /* SET BUFCB ADDR IN DCB */
         ST    R1,DCBBUFCB(,RDCB)                                  0128
*         DCBCIND2(5) = '1'B;               /* DENOTE OPEN     XM6068*/
         OI    DCBCIND2(RDCB),B'00001000'                          0129
*                                           /* GOT BFR POOL    XM6068*/
*         DCBBUFNO = RBUFNO;                /* SET BUFNO IN DCB      */
*                                                                  0130
         STC   RBUFNO,DCBBUFNO(,RDCB)                              0130
*         /*          REESTABLISH USER KEY                          */
*         RDCBSAVE = RDCB;              /* SAVE PTR TO DCB    Y02082*/
         LR    RDCBSAVE,RDCB                                       0131
*         RUKEY = DXUKEY;               /* SAVE USER KY IN R2 Y02082*/
         L     @11,WTGCWA-1(,RWTGC)                                0132
         SLR   RUKEY,RUKEY                                         0132
         IC    RUKEY,DXUKEY(,@11)                                  0132
*         GEN (MODESET KEYADDR=(2));    /*                    Y02082*/
         MODESET KEYADDR=(2)
*         RDCB = RDCBSAVE;              /* RESTORE DCB PTR    Y02082*/
         LR    RDCB,RDCBSAVE                                       0134
*         BCBBUFL = RBUFL;                  /* SET BUFL IN BCB       */
         L     @11,DCBBUFCB(,RDCB)                                 0135
         STH   RBUFL,BCBBUFL(,@11)                                 0135
*         BCBBUFNO = RBUFNO;                /* SET BUFNO IN BCB      */
         STH   RBUFNO,BCBBUFNO(,@11)                               0136
*         IF DCBRECFM(1:2) = '01'B & DCBRECFM(5) = '1'B /*   @ZA19137*/
*         & DCBMACRF(13) = '1'B THEN /* IF VARIABLE SPANNED  @ZA19137*/
         TM    DCBRECFM(RDCB),B'01000000'                          0137
         BNO   @RF00137                                            0137
         TM    DCBRECFM(RDCB),B'10000000'                          0137
         BNZ   @RF00137                                            0137
         TM    DCBRECFM(RDCB),B'00001000'                          0137
         BNO   @RF00137                                            0137
         TM    DCBMACRF+1(RDCB),B'00001000'                        0137
         BNO   @RF00137                                            0137
*         BCBBLDR(2) = '1'B;            /* TURN ON LRI FLG   @ZA19137*/
         OI    BCBBLDR(@11),B'01000000'                            0138
*         GEN   (EJECT);                    /* SKIP TO A NEW PAGE    */
@RF00137 DS    0H                                                  0139
         EJECT
*                                                                  0140
*                                                                  0140
*         /***********************************************************/
*         /*                                                         */
*         /* STRUCTURE THE BUFFER CONTROL BLOCKS AND LINK BUFFERS    */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0140
*         /* SET POINTER PAST BUFFER CONTROL BLOCK                   */
*         R1 = R1 + 8;                                             0140
*                                                                  0140
         LA    R1,8(,R1)                                           0140
*         /* IS FULL WORD ALIGNMENT SPECIFIED ?                      */
*                                                                  0141
*         IF DCBBFALN (8) = '1'B                                   0141
*            THEN                           /* YES                   */
*                                                                  0141
         TM    DCBBFALN(RDCB),B'00000001'                          0141
         BNO   @RF00141                                            0141
*              /* ADJUST POINTER TO FULL WORD                        */
*              R1 = R1 + 4;                                        0142
*                                                                  0142
         LA    R1,4(,R1)                                           0142
*            ELSE;                          /* NO                    */
*                                                                  0143
@RF00141 DS    0H                                                  0144
*         /* POINT TO FIRST BUFFER                                   */
*         BCBFSBUF = R1;                                           0144
*                                                                  0144
         L     @11,DCBBUFCB(,RDCB)                                 0144
         ST    R1,BCBFSBUF(,@11)                                   0144
*         /* LINK ALL BUFFERS                                        */
*         DO RBUFNO = RBUFNO TO 1 BY -1;                           0145
*                                                                  0145
         B     @DE00145                                            0145
@DL00145 DS    0H                                                  0146
*            /* IS THIS THE LAST BUFFER IN THE CHAIN ?               */
*                                                                  0146
*            IF RBUFNO = 1                                         0146
*               THEN                                               0146
*                                                                  0146
         C     RBUFNO,@CF00082                                     0146
         BNE   @RF00146                                            0146
*                 /* ZERO LAST BUFFER LINK                           */
*                 BUFLINK = 0;                                     0147
*                                                                  0147
         SLR   @11,@11                                             0147
         ST    @11,BUFLINK(,R1)                                    0147
*               ELSE                        /* NOT LAST BUFFER       */
*                 BUFLINK = R1 + RBUFL;     /* ADDR OF NEXT BUFFER 0148
*                                              IN LINK FIELD         */
         B     @RC00146                                            0148
@RF00146 LR    @11,R1                                              0148
         ALR   @11,RBUFL                                           0148
         ST    @11,BUFLINK(,R1)                                    0148
*            R1 = R1 + RBUFL;               /* POINT TO NEXT BUFFER  */
*                                                                  0149
@RC00146 ALR   R1,RBUFL                                            0149
*         END;                              /* THIS LOOP IS EXECUTED
*                                              UNTIL COUNT OF BUFFERS
*                                              IS ZERO               */
*                                                                  0150
         BCTR  RBUFNO,0                                            0150
@DE00145 LTR   RBUFNO,RBUFNO                                       0150
         BP    @DL00145                                            0150
*         /* PLACE POINTER TO BUFFER IN RPL                          */
*         RPLAREA = BCBFSBUF;                                      0151
         L     @11,DCBBUFCB(,RDCB)                                 0151
         L     @11,BCBFSBUF(,@11)                                  0151
         ST    @11,RPLAREA(,RRPL)                                  0151
*         CIRAREA = BCBFSBUF;               /* FOR SPANNED RECORDS   */
         ST    @11,CIRAREA(,RCICB)                                 0152
*        GEN   (EJECT);                 /* SKIP TO A NEW PAGE       */
         EJECT
*                                                                  0154
*         /***********************************************************/
*         /*                                                         */
*         /* INITIALIZE THE RPL                                      */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0154
*INITRPL:      ;                                                   0154
INITRPL  DS    0H                                                  0155
*         IF DCBCIND2(8) = '1'B THEN    /* TEST FOR QSAM       Y02123*/
         TM    DCBCIND2(RDCB),B'00000001'                          0155
         BNO   @RF00155                                            0155
*           DO;                         /*                     Y02123*/
*             /* SUPERVISOR KEY REQUIRED TO REFERENCE RPL (UK)     0157
*                AND STORE IN DCB FIELDS (DMK)                       */
*             GEN (MODESET EXTKEY=ZERO);/* KEY ZERO            Y02123*/
*                                                                  0157
         MODESET EXTKEY=ZERO
*             IF DCBMACRF(2)   = '1'B THEN/*                       0158
*                                          QSAM=GET            Y02123*/
         TM    DCBMACRF(RDCB),B'01000000'                          0158
         BNO   @RF00158                                            0158
*               DO;                     /* SETUP DCB FOR PGET  Y02123*/
*                 DCBRECAD = RPLAREA;   /* START OF BUF        Y02123*/
         L     @11,RPLAREA(,RRPL)                                  0160
         ST    @11,DCBRECAD(,RDCB)                                 0160
*                 DCBEOBAD = DCBRECAD + DCBLRECL; /* ENDOFBUF  Y02123*/
         LH    @15,DCBLRECL(,RDCB)                                 0161
         N     @15,@CF00434                                        0161
         ALR   @11,@15                                             0161
         ST    @11,DCBEOBAD(,RDCB)                                 0161
*               END;                    /*                     Y02123*/
*             ELSE                      /* NOT GET ASSUME PUT  Y02123*/
*               DO;                     /* SET DCB FOR COBOL   Y02123*/
         B     @RC00158                                            0163
@RF00158 DS    0H                                                  0164
*                 DCBRECAD = RPLAREA + DCBLRECL; /* ENDOFBUF   Y02123*/
         LH    @11,DCBLRECL(,RDCB)                                 0164
         N     @11,@CF00434                                        0164
         AL    @11,RPLAREA(,RRPL)                                  0164
         ST    @11,DCBRECAD(,RDCB)                                 0164
*                 DCBEOBAD = DCBRECAD;  /* SIMULATE EOB        Y02123*/
         ST    @11,DCBEOBAD(,RDCB)                                 0165
*               END;                    /*                     Y02123*/
*                                                                  0166
*             /*  REESTABLISH USER KEY FOR INITRPL ROUTINE           */
*             RDCBSAVE = RDCB;          /* SAVE PTR TO DCB     Y02123*/
@RC00158 LR    RDCBSAVE,RDCB                                       0167
*             RUKEY = DXUKEY;           /* SAVE USER KY IN R2  Y02123*/
         L     @11,WTGCWA-1(,RWTGC)                                0168
         SLR   RUKEY,RUKEY                                         0168
         IC    RUKEY,DXUKEY(,@11)                                  0168
*             GEN (MODESET KEYADDR=(2));  /*                   Y02123*/
         MODESET KEYADDR=(2)
*             RDCB = RDCBSAVE;          /* RESTORE DCB PTR     Y02123*/
*                                                                  0170
         LR    RDCB,RDCBSAVE                                       0170
*             IF DCBMACRF(5)   = '1'B × DCBMACRF(13)  = '1'B THEN/*
*                                          LOCATE MODE         YM5890*/
         TM    DCBMACRF(RDCB),B'00001000'                          0171
         BO    @RT00171                                            0171
