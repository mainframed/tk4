         TITLE 'IGG019AH SAM SUBSYSTEM INTERFACE SYNAD MODULE          *
                        '
IGG019AH CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
INTSYDAF DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00181-INTSYDAF)
         ENTRY INTSYDAF
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*                                                                  0073
*        /* AT ENTRY REGISTER 1 POINTS TO THE RPL                  */
*                                                                  0074
*        /***********************************************************/
*        /* ESTABLISH CONTROL BLOCK BASES                           */
*        /***********************************************************/
*                                                                  0074
*        RRPL= R1;                      /* RPL BASE                 */
         LR    RRPL,R1                                             0074
*        RESPECIFY IFGRPL BASED(RRPL);                             0075
*                                                                  0075
*        RCICB = RPLCICB;               /* CICB BASE                */
         L     RCICB,RPLCICB(,RRPL)                                0076
*        RESPECIFY IFGJCICB BASED (RCICB);                         0077
*                                                                  0077
*        RDCB = CIDCBPTR;               /* DCB BASE                 */
         L     RDCB,CIDCBPTR(,RCICB)                               0078
*        RESPECIFY IFGSDCB  BASED (RDCB);                          0079
*                                                                  0079
*        RDECB = RPLDECB;               /* DECB BASE                */
         L     RDECB,RPLDECB(,RRPL)                                0080
*        RESPECIFY DECB BASED(RDECB);                              0081
*                                                                  0081
*                                       /****************************/
*                                       /* IF NO SYNAD SUPPLIED     */
*        IF DCBSYNAD = 1                /* BYPASS SYNAD EXIT SECTION*/
*          THEN                         /****************************/
         CLC   DCBSYNAD(3,RDCB),@CF00088+1                         0082
         BE    @RT00082                                            0082
*            GOTO EROPT;                                           0083
*                                                                  0083
*                                                                  0083
*/*******************************************************************/
*/*      GET AND CHAIN A SAVE AREA FOR USE ACROSS USER EXIT         */
*/*******************************************************************/
*                                                                  0084
*        RCSAVE = R13;                  /* SAVE PTR TO CALLER SAVE  */
         LR    RCSAVE,R13                                          0084
*        R13 = CIREGBC;                 /* POINT TO USER SAVE       */
         L     R13,CIREGBC(,RCICB)                                 0085
*        R0 = LENGTH(REGSAVE);          /* GET A NEW SAVE AREA      */
         LA    R0,72                                               0086
*        GEN (GETMAIN R,LV=(0));                                   0087
         GETMAIN R,LV=(0)
*        IF DCBMACRF(3)   = '1'B        /* IF CALLER IS IGG019BB    */
*          THEN                         /* YES                      */
         TM    DCBMACRF(RDCB),B'00100000'                          0088
         BNO   @RF00088                                            0088
*            DO;                        /* COMPENSATE - NONSTD SAVE */
*              R1->SAVE141 = R13->SAVE141; /* COPY USER X, X, 14, 15*/
         MVC   SAVE141(16,R1),SAVE141(R13)                         0090
*              R1->SAVE28 = R13->SAVE410; /* COPY USER 2 - 8        */
         MVC   SAVE28(28,R1),SAVE410(R13)                          0091
*              R1->SAVE912 = RCSAVE->SAVE912; /* COPY USER 9 - 12   */
         MVC   SAVE912(16,R1),SAVE912(RCSAVE)                      0092
*              /* NOTE THAT 9 - 12 ARE COMING FROM CALLERS SAVE AREA*/
*              /* IGG019BB DOES NOT SAVE OR USE THESE REGISTERS     */
*            END;                                                  0093
*                                                                  0093
*          ELSE                         /* NO, FOR ALL OTHER CALLERS*/
*            R1->SAVE1412 = R13->SAVE1412;/* SAVE USER REGS         */
*                                                                  0094
         B     @RC00088                                            0094
@RF00088 MVC   SAVE1412(60,R1),SAVE1412(R13)                       0094
*        R1->SAVEPREV = RCSAVE;         /* BACKCHAIN NEW TO CALLER  */
@RC00088 ST    RCSAVE,SAVEPREV(,R1)                                0095
*        RCSAVE->SAVENEXT = R1;         /* FORCHAIN CALLER TO NEW   */
         ST    R1,SAVENEXT(,RCSAVE)                                0096
*        R1->SAVENEXT = R13;            /* FORCHAIN NEW TO USER     */
         ST    R13,SAVENEXT(,R1)                                   0097
*        RCSAVE = R1;                   /* KEEP POINTER TO NEW      */
         LR    RCSAVE,R1                                           0098
*        R13->SAVE1 = R1;                                          0099
*                                                                  0099
         ST    R1,SAVE1(,R13)                                      0099
*/*******************************************************************/
*/*      SET UP PARAMETERS TO PASS TO USER EXIT                     */
*/*******************************************************************/
*                                                                  0100
*        CISYNADA = ADDR(INTSYDAF);     /* PLACE SYNADAF ENTRY POINT
*                                            ADDRESS IN CICB FOR   0100
*                                            SVC EXIT               */
*                                                                  0100
         L     @14,@CA01129                                        0100
         ST    @14,CISYNADA(,RCICB)                                0100
*        DCBCIND2(4) = '1'B;            /* INDICATE PERMANENT I/O  0101
*                                            ERROR IN DCB            */
         OI    DCBCIND2(RDCB),B'00010000'                          0101
*        IF DCBCIND2(8) = '1'B THEN     /* IS THIS A QSAM DCB? Y02144*/
*                                                                  0102
         TM    DCBCIND2(RDCB),B'00000001'                          0102
         BNO   @RF00102                                            0102
*          DO;                          /* YES SET UP FOR QSAM Y02144*/
*            R0 = ADDR(CIIOB);          /* POINT REG 0 TO IOB  Y02144*/
         LA    R0,CIIOB(,RCICB)                                    0104
*            CIRESID = RPLBUFL - RPLRLEN;/* SET 'CSW' COUNT    Y02144*/
         L     @14,RPLBUFL(,RRPL)                                  0105
         SL    @14,RPLRLEN(,RRPL)                                  0105
         ST    @14,CIRESID(,RCICB)                                 0105
*            IF CIRESID < 1 THEN        /* DID WE GO NEGATIVE? Y02144*/
         LTR   @14,@14                                             0106
         BP    @RF00106                                            0106
*              CIRESID = 0;             /* THEN RESET TO ZERO  Y02144*/
         SLR   @14,@14                                             0107
         ST    @14,CIRESID(,RCICB)                                 0107
*          END;                         /* END OF QSAM SETUP   Y02144*/
*                                                                  0108
*        ELSE                           /* NOT QSAM            Y02144*/
*          DO;                          /* SET UP FOR BSAM     Y02144*/
         B     @RC00102                                            0109
@RF00102 DS    0H                                                  0110
*            CIFLAG1 = DECBCOND;        /* ERR INDEX FROM DECB Y02144*/
         MVC   CIFLAG1(1,RCICB),DECBCOND(RDECB)                    0110
*            IF CIFLAG1 = 0 THEN        /* IF INDEX IS ZERO,   Y02144*/
         CLI   CIFLAG1(RCICB),0                                    0111
         BNE   @RF00111                                            0111
*              CIFLAG1 = ERR7;          /* ITS INVALID CHECK   Y02144*/
         MVI   CIFLAG1(RCICB),X'07'                                0112
*            R0 = RDECB;                /* POINT REG 0 TO DECB Y02144*/
@RF00111 LR    R0,RDECB                                            0113
*            CIRESID = DECRESDU;        /* RESID COUNT TO IOB  Y02144*/
         LH    @14,DECRESDU(,RDECB)                                0114
         N     @14,@CF01196                                        0114
         ST    @14,CIRESID(,RCICB)                                 0114
*            DECIOBPT = ADDR(CIIOB);    /* SET UP IOB PTR      Y02144*/
         LA    @14,CIIOB(,RCICB)                                   0115
         ST    @14,DECIOBPT(,RDECB)                                0115
*          END;                         /* END OF BSAM SETUP   Y02144*/
*                                                                  0116
*        /* DO SETUP COMMON TO BSAM AND QSAM                         */
*                                                                  0117
*        R1 = RDCB;                     /* REG 1 IS DCB PTR    Y02144*/
@RC00102 LR    R1,RDCB                                             0117
*        RWORK = ADDR(SYNSTAT(CIFLAG1));/* GET SYNAD CODE FROM Y02144
*                                          TBL USING ERR INDEX       */
         SLR   RWORK,RWORK                                         0118
         IC    RWORK,CIFLAG1(,RCICB)                               0118
         LA    RWORK,SYNSTAT-1(RWORK)                              0118
*        GEN (ICM R1,8,0(RWORK));       /* INSERT CODE TO REG1 Y02144*/
*                                                                  0119
*                                                                  0119
         ICM R1,8,0(RWORK)
*                                                                  0120
*        /***********************************************************/
*        /*                                                         */
*        /* GO TO USER SYNAD                                        */
*        /*                                                         */
*        /***********************************************************/
*                                                                  0120
*        R15 = DCBSYNAD;                /* GET EP TO USER ROUTINE   */
         L     R15,DCBSYNAD-1(,RDCB)                               0120
         LA    R15,0(,R15)                                         0120
*        GEN;                           /* GEN ASSEMBLER CODE       */
*                                                                  0121
         STM   @2,@C,SAVE2(R13)         SAVE MY REGS IN USER AREA
         LM    @2,@C,SAVE2(RCSAVE)      LOAD USER REGISTERS
         BALR  @E,@F                    TAKE USER EXIT
         L     R1,SAVE1(R13)            FIND NEW SAVE AREA
         STM   @2,@C,SAVE2(R1)          SAVE USER REGS THERE
         LM    @2,@C,SAVE2(R13)         RESTORE MY REGISTERS
*        RCSAVE = RCSAVE->SAVEPREV;     /* BACK UP TO CALLER SAVE   */
         L     RCSAVE,SAVEPREV(,RCSAVE)                            0122
*        IF DCBMACRF(3)   = '1'B        /* IS CALLER IGG019BB       */
*          THEN                                                    0123
         TM    DCBMACRF(RDCB),B'00100000'                          0123
         BNO   @RF00123                                            0123
*            DO;                        /* COMPENSATE - NONSTD SAVE */
*              R13->SAVE141 = R1->SAVE141; /*RESTORE X, X, 14, 15   */
         MVC   SAVE141(16,R13),SAVE141(R1)                         0125
*              R13->SAVE410 = R1->SAVE28;  /* RESTORE REG 2 - 8     */
         MVC   SAVE410(28,R13),SAVE28(R1)                          0126
*              RCSAVE->SAVE912 = R1->SAVE912; /* AND REG 9 - 12     */
         MVC   SAVE912(16,RCSAVE),SAVE912(R1)                      0127
*           END;                                                   0128
*                                                                  0128
*          ELSE                         /* FOR ALL OTHER CALLERS    */
*             R13->SAVE1412 = R1->SAVE1412; /* RESTORE USER SAVE    */
*                                                                  0129
         B     @RC00123                                            0129
@RF00123 MVC   SAVE1412(60,R13),SAVE1412(R1)                       0129
*        R13 = RCSAVE;                  /* RESTORE ORIG R13 VALUE   */
@RC00123 LR    R13,RCSAVE                                          0130
*        R0 = LENGTH(REGSAVE);          /* FREE NEW SAVE AREA       */
         LA    R0,72                                               0131
*        GEN (FREEMAIN R,LV=(0),A=(1)); /* ADDR IS STILL IN R1      */
*                                                                  0132
         FREEMAIN R,LV=(0),A=(1)
*        CISYNADA = 0;                  /* CLEAR SYNADAF EP ADDRESS */
         SLR   @14,@14                                             0133
         ST    @14,CISYNADA(,RCICB)                                0133
*        CISYNRC = '08'X;               /* RESTORE RETURN CODE      */
         MVI   CISYNRC(RCICB),X'08'                                0134
*        /***********************************************************/
*        GEN   (EJECT);                 /* SKIP TO A NEW PAGE       */
         EJECT
*                                                                  0136
*EROPT:  /***********************************************************/
*        /*    TEST DCBEROPT SPECIFIED                              */
*        /***********************************************************/
*                                                                  0136
*        IF DCBMACRF(3)   = '1'B × DCBMACRF(11)  = '1'B/*          0136
*                                          IF BSAM                  */
*          THEN IF DCBSYNAD ^= 1        /* AND NO SYNAD SPECIFIED   */
EROPT    TM    DCBMACRF(RDCB),B'00100000'                          0136
         BO    @RT00136                                            0136
         TM    DCBMACRF+1(RDCB),B'00100000'                        0136
         BNO   @RF00136                                            0136
@RT00136 DS    0H                                                  0137
*                 THEN RETURN;                                     0137
         CLC   DCBSYNAD(3,RDCB),@CF00088+1                         0137
         BNE   @RT00137                                            0137
*                 ELSE;                                            0139
*          ELSE IF DCBMACRF(2)   = '1'B × DCBMACRF(10)  = '1'B/*   0140
*                                          IF QSAM                  */
*                 THEN IF DCBEROPT(1) = '1'B ×  /* AND IF ERROR     */
         B     @RC00136                                            0140
@RF00136 TM    DCBMACRF(RDCB),B'01000000'                          0140
         BO    @RT00140                                            0140
         TM    DCBMACRF+1(RDCB),B'01000000'                        0140
         BNO   @RF00140                                            0140
@RT00140 DS    0H                                                  0141
*                         DCBEROPT(2) = '1'B    /* OPTIONS SPECIFIED*/
*                        THEN RETURN;   /* THEN RETURN TO USER      */
         TM    DCBEROPT(RDCB),B'11000000'                          0141
         BNZ   @RT00141                                            0141
*                        ELSE;                                     0143
*                 ELSE;                                            0144
*                                                                  0144
@RF00140 DS    0H                                                  0145
*        /***********************************************************/
*        /*   BEFORE ABEND, ISSUE PROBLEM DETERMINATION MESSAGES    */
*        /***********************************************************/
*                                                                  0145
*        R0 = LENGTH(WTOMSG);           /* MESSAGE BUFFER SIZE      */
@RC00136 LA    R0,52                                               0145
*        GEN (GETMAIN R,LV=(0));        /* REQUEST CORE FOR BUFFER  */
         GETMAIN R,LV=(0)
*        R6 = R1;                       /* SAVE BUFFER ADDRESS      */
         LR    R6,R1                                               0147
*        RESPECIFY WTOMSG BASED(R6);                               0148
*                                                                  0148
*        RTIODD = DCBTIOT + ADDR(IEFTIOT); /* ADDR OF TIOT ENTRY    */
         L     @14,DCBDEBAD-1(,RDCB)                               0149
         L     @14,DEBTCBAD(,@14)                                  0149
         LH    RTIODD,DCBTIOT(,RDCB)                               0149
         AL    RTIODD,TCBTIO(,@14)                                 0149
*        RESPECIFY TIODDENT BASED(RTIODD);                         0150
*                                                                  0150
*        WTOMSGLN = LENGTH(WTOPRFIX)    /* SET MSG LENGTH FIELD     */
*                  + LENGTH(WTOTEXT);   /*     FOR WTO SVC          */
         MVC   WTOMSGLN(2,R6),@CH01192                             0151
*        WTOMCSFL = '8000'X;            /* DENOTE ROUTE INFO PRESENT*/
*                                                                  0152
         MVC   WTOMCSFL(2,R6),@CB01169                             0152
*        WTOTEXT = IEC020I;             /* MOVE MSG INTO BUFFER     */
         MVC   WTOTEXT(44,R6),IEC020I                              0153
*        IF DCBCIND2(8) = '1'B          /* IF QSAM - RESET AM CODE  */
*          THEN WTOSAMID = '4';         /* CHANGE FROM BSAM TO QSAM */
         TM    DCBCIND2(RDCB),B'00000001'                          0154
         BNO   @RF00154                                            0154
         MVI   WTOSAMID(R6),C'4'                                   0155
*        IF CIFLAG1 = '05'X             /* IS USER TRYING TO @ZA25657*/
*        THEN WTOSAMID = '5';           /* READ BEYOND EOD ? @ZA25657*/
@RF00154 CLI   CIFLAG1(RCICB),X'05'                                0156
         BNE   @RF00156                                            0156
         MVI   WTOSAMID(R6),C'5'                                   0157
*        WTOJOB = TIOCNJOB;             /* INSERT JOB NAME          */
@RF00156 L     @14,DCBDEBAD-1(,RDCB)                               0158
         L     @14,DEBTCBAD(,@14)                                  0158
         L     @14,TCBTIO(,@14)                                    0158
         MVC   WTOJOB(8,R6),TIOCNJOB(@14)                          0158
*        WTOSTEP = TIOCSTP;             /* INSERT STEP NAME         */
         MVC   WTOSTEP(8,R6),TIOCSTP(@14)                          0159
*        WTODDN = TIOEDDNM;             /* INSERT DD NAME           */
*                                                                  0160
         MVC   WTODDN(8,R6),TIOEDDNM(RTIODD)                       0160
*        WTOSUFIX = WTOSUFIX && WTOSUFIX; /* CLEAR MSG TYPE FLAGS   */
         XC    WTOSUFIX(4,R6),WTOSUFIX(R6)                         0161
*        WTODESC(6) = '1'B;             /* JOB STATUS TYPE MSG      */
         OI    WTODESC(R6),B'00000100'                             0162
*        WTOROUT(11) = '1'B;            /* WRITE TO PROGRAMMER TYPE */
         OI    WTOROUT+1(R6),B'00100000'                           0163
*        R1 = R6;                       /* MSG ADDRESS FOR WTO      */
         LR    R1,R6                                               0164
*        GEN (WTO MF=(E,(1)));          /* WRITE MESSAGE            */
*                                                                  0165
         WTO MF=(E,(1))
*        R0 = LENGTH(WTOMSG);           /* MSG BUFFER SIZE          */
         LA    R0,52                                               0166
*        R1 = R6;                       /* MSG BUFFER ADDRESS       */
         LR    R1,R6                                               0167
*        GEN (FREEMAIN R,LV=(0),A=(1)); /* RELEASE MSG BUFFER       */
*                                                                  0168
         FREEMAIN R,LV=(0),A=(1)
*        IF DCBCIND2(8) = '1'B          /* TEST FOR QSAM            */
*          THEN R1 = ADDR(QSAMNOTE);    /* YES- ADDR OF QSAM REMARK */
         TM    DCBCIND2(RDCB),B'00000001'                          0169
         BNO   @RF00169                                            0169
         LA    R1,QSAMNOTE                                         0170
*          ELSE R1 = ADDR(BSAMNOTE);    /* NO - ADDR OF BSAM REMARK */
         B     @RC00169                                            0171
@RF00169 LA    R1,BSAMNOTE                                         0171
*        GEN (WTO MF=(E,(1)));          /* WRITE SAM REMARK         */
*                                                                  0172
@RC00169 DS    0H                                                  0172
         WTO MF=(E,(1))
*        R1 = 1;                        /* REG1 W/ ABEND CODE  Y02120*/
         LA    R1,1                                                0173
*        GEN  (ABEND (1),DUMP,STEP,SYSTEM);  /* ISSUE ABEND    Y02120*/
*                                                                  0174
         ABEND (1),DUMP,STEP,SYSTEM
*        GEN;                                                      0175
BSAMNOTE WTO   'IEC020I NO SYNAD EXIT SPECIFIED',                      *
               ROUTCDE=(11),DESC=6,MF=L
QSAMNOTE WTO   'IEC020I EROPT IS ''ABE'' OR NOT SPECIFIED',            *
               ROUTCDE=(11),DESC=6,MF=L
*        RESPECIFY IFGJCICB BASED(NOBASE);                         0176
*        RESPECIFY IFGSDCB BASED(NOBASE);                          0177
*        RESPECIFY IFGRPL BASED(NOBASE);                           0178
*        RESPECIFY DECB BASED(NOBASE);                             0179
*/*******************************************************************/
*        GEN   (EJECT);                 /* SKIP TO A NEW PAGE       */
         EJECT
*        /***********************************************************/
*        /* ENTRY POINT FOR SYNADAF PROCESSING                      */
*        /***********************************************************/
*                                                                  0181
*INTSYDAF: ENTRY;                                                  0181
@EP00181 DS    0H                                                  0182
*/*******************************************************************/
*/* ON ENTRY, REG 1 POINTS TO SYNADAF MESSAGE AREA, WHICH HAS BEEN  */
*/* PARTIALLY FORMATTED BY SYNADAF.                                 */
*/*      -LENGTH FIELDS HAVE BEEN INITIALIZED                       */
*/*      -STANDARD VARIABLE FIELDS HAVE HAD ASTERISKS INSERTED      */
*/*      -COMMAS HAVE BEEN INSERTED BETWEEN FIELDS                  */
*/* IMMEDIATELY BEYOND THE MESSAGE AREA ARE TWO WORDS, WHICH ARE    */
*/* REG 0 AND REG 1 AS AT ENTRY TO SYNADAF.                         */
*/*      -THE VALUE IN REG 1 IS THE DCB ADDRESS, AND CAN BE USED TO */
*/*       FIND ALL NECESSARY CONTROL BLOCKS.                        */
*/* REG 13 AT ENTRY POINTS TO A SAVE AREA WHICH CAN BE USED HERE    */
*/*                                                                 */
*/* ON EXIT, REG 0 MUST CONTAIN THE RETURN CODE WHICH SYNADAF WILL  */
*/* PASS BACK TO THE USER, AND THE MESSAGE AREA MUST HAVE BEEN      */
*/* COMPLETED.                                                      */
*/*******************************************************************/
*                                                                  0182
*        RESPECIFY SYNADMSG BASED(R1);  /* AREA PASSED BY SYNADAF   */
*        RDCB = ADDR(SYNADMSG)+SYNBLKSI;/* TEMP PTR TO SAVED R0,R1  */
         LR    RDCB,R1                                             0183
         AH    RDCB,SYNBLKSI(,R1)                                  0183
*        RESPECIFY SYNREGSV BASED(RDCB);                           0184
*        RDCB = SYNPARM1;               /* GET ADDR OF DCB          */
         L     RDCB,SYNPARM1(,RDCB)                                0185
*        RESPECIFY IFGSDCB BASED(RDCB);                            0186
*        RESPECIFY SYNREGSV BASED(NOBASE);/* NO LONGER ADDRESSABLE  */
*                                                                  0187
*        RCICB = DCBCICB;               /* ADDR OF JCICB            */
         L     RCICB,DCBCICB(,RDCB)                                0188
*        RESPECIFY IFGJCICB BASED (RCICB);                         0189
*                                                                  0189
*        RRPL = ADDR(CIRPL);            /* ADDR OF RPL              */
         LA    RRPL,CIRPL(,RCICB)                                  0190
*        RESPECIFY IFGRPL BASED (RRPL);                            0191
*                                                                  0191
*        IF DCBMACRF(2)   = '1'B × DCBMACRF(3)   = '1'B THEN/*     0192
*                                          ONLY FOR INPUT ERR  Y02144*/
         TM    DCBMACRF(RDCB),B'01100000'                          0192
         BZ    @RF00192                                            0192
*          DO;                          /* DO WE FILL THESE    Y02144*/
*            SYNBUFAD = RPLAREA;        /* BUFFER ADDRESS      Y02144*/
         L     @14,RPLAREA(,RRPL)                                  0194
         ST    @14,SYNBUFAD(,R1)                                   0194
*            SYNBYTES = DCBBLKSI - CIRESID;/* NO. BYTES READ   Y02144*/
         LH    @14,DCBBLKSI(,RDCB)                                 0195
         N     @14,@CF01196                                        0195
         SL    @14,CIRESID(,RCICB)                                 0195
         STH   @14,SYNBYTES(,R1)                                   0195
*            SYNDEVTY = 'SI';           /* DENOTE 'SYSIN'      Y02144*/
         MVC   SYNDEVTY(2,R1),@CC01186                             0196
*          END;                         /* END OF INPUT SETUP  Y02144*/
*        ELSE                           /* IT IS OUTPUT        Y02144*/
*          SYNDEVTY = 'SO';             /* DENOTE 'SYSOUT'     Y02144*/
*                                                                  0198
         B     @RC00192                                            0198
@RF00192 MVC   SYNDEVTY(2,R1),@CC01187                             0198
*        SYNBLANK = ' ';                /* CLEAR UNUSED AREA        */
@RC00192 MVI   SYNBLANK+1(R1),C' '                                 0199
         MVC   SYNBLANK+2(33,R1),SYNBLANK+1(R1)                    0199
         MVI   SYNBLANK(R1),C' '                                   0199
*        SYNJOBNM = TIOCNJOB;           /* FILL IN JOB NAME         */
         L     @14,DCBDEBAD-1(,RDCB)                               0200
         L     @14,DEBTCBAD(,@14)                                  0200
         L     @14,TCBTIO(,@14)                                    0200
         MVC   SYNJOBNM(8,R1),TIOCNJOB(@14)                        0200
*        SYNSTPNM = TIOCSTP;            /* FILL IN STEP NAME        */
         MVC   SYNSTPNM(8,R1),TIOCSTP(@14)                         0201
*        RTIODD = DCBTIOT + ADDR(IEFTIOT); /* ADD TIOT OFFSET TO    */
         AH    @14,DCBTIOT(,RDCB)                                  0202
         LR    RTIODD,@14                                          0202
*                                       /* GET TO DD ENTRY          */
*        RESPECIFY TIODDENT BASED(RTIODD);                         0203
*        SYNUNIT = 'JES';               /* UNIT = JES               */
         MVC   SYNUNIT(3,R1),@CC01189                              0204
*        SYNDDNAM = TIOEDDNM;           /* DDNAME FROM TIOT ENTRY   */
*                                                                  0205
         MVC   SYNDDNAM(8,R1),TIOEDDNM(RTIODD)                     0205
*        RWORK = CIFLAG1;               /* GET ERROR INDEX     Y02144*/
         SLR   RWORK,RWORK                                         0206
         IC    RWORK,CIFLAG1(,RCICB)                               0206
*        SYNERDES = MESSAGE(RWORK);     /* MOVE IN ERR TEXT    Y02144*/
         LR    @14,RWORK                                           0207
