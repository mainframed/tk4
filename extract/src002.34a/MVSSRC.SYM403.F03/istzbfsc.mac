         TITLE 'ISTZBFSC/ASC-VS1/MSC-VS2-SESSION CONTROL OUTBOUND      *
                        '
ISTZBFSC CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ISTZBFSC  78.052'                                 0001
         DROP  @15
@PROLOG  DS    0H                                                  0002
*                                                                  0508
*/********************************************************************/
*/*                                                                  */
*/* SECTION-                                                         */
*/*      MAINLINE                                                    */
*/*                                                                  */
*/* FUNCTION-                                                        */
*/*      SESSION CONTROL OUTBOUND PROCESSOR.                         */
*/*                                                                  */
*/*      THIS ROUTINE HANDLES CONTROL ROUTING FOR LCCWS AND          */
*/*      OUTBOUND FSBS.                                              */
*/*                                                                  */
*/*      THE FOLLOWING LCCW OPERATION CODES ARE HANDLED:             */
*/*            SEND                                                  */
*/*            CLEAR (,UNBIND (,NOTIFY))                             */
*/*            NOTIFY                                                */
*/*                                                                  */
*/*      THIS ROUTINE ALSO HANDLES CONTROL ROUTING FOR A             */
*/*      CLEAR OR SESSION TERMINATION FSB.                           */
*/*                                                                  */
*/********************************************************************/
*/* TPZSAVE (7,12,14) AC(BCSC)BASE                                   */
*                                                                  0508
*   DO;                                                            0508
*     DCL                                                          0509
*       GPR01F FIXED(31) REG(01);                                  0509
*     DCL                                                          0510
*       GPR01P PTR(31) REG(01);                                    0510
*     DCL                                                          0511
*       GPR12F FIXED(31) REG(12);                                  0511
*     DCL                                                          0512
*       GPR12P PTR(31) REG(12);                                    0512
*     DCL                                                          0513
*       GPR13F FIXED(31) REG(13);                                  0513
*     DCL                                                          0514
*       GPR13P PTR(31) REG(13);                                    0514
*     DCL                                                          0515
*       GPR14F FIXED(31) REG(14);                                  0515
*     DCL                                                          0516
*       GPR14P PTR(31) REG(14);                                    0516
*     RFY                                                          0517
*      (GPR01P,                                                    0517
*       GPR12P,                                                    0517
*       GPR13P,                                                    0517
*       GPR14P) RSTD;                                              0517
*     GEN(PRINT OFF);                                              0518
*                                                                  0518
         PRINT OFF
*/* ATBUILD (ISTZSAVE)                                               */
*/* END OF ATBUILD                                                   */
*                                                                  0519
*     GEN(PRINT ON);                                               0519
         PRINT ON
*     RFY                                                          0520
*       ISTZSAVE BASED(GPR13P);                                    0520
*     GEN SETS(ZSAZSVVR)(STM   7,12,ZSAZSVVR+0(13));               0521
         STM   7,12,ZSAZSVVR+0(13)
*     GEN SETS(GPR12P);                                            0522
         BALR  GPR12P,0
@TSTART  DS    0H
         USING @TSTART,GPR12P
*     GEN(PRINT OFF);                                              0523
*                                                                  0523
         PRINT OFF
*/* ATBUILD (ISTZCRR,ISTRPH,ISTCRA) EJECT(NO)                        */
*/* END OF ATBUILD                                                   */
*                                                                  0524
*     GEN(PRINT ON);                                               0524
         PRINT ON
*     GPR01P->RPHCRR->CRACRR->ZCREBCSC='1'B;                       0525
         L     @15,RPHCRR(,GPR01P)                                 0525
         L     @15,CRACRR(,@15)                                    0525
         OI    ZCREBCSC(@15),B'00000001'                           0525
*     ZSAZACTV='1'B;                                               0526
         OI    ZSAZACTV(GPR13P),B'10000000'                        0526
*     ZSAZTRAC='BCSC';                                             0527
         MVC   ZSAZTRAC(4,GPR13P),@CC04064                         0527
*     ZSAZSVRR=GPR14P;                                             0528
         ST    GPR14P,ZSAZSVRR(,GPR13P)                            0528
*     GPR13P=ZSAZSVFC;                                             0529
         L     GPR13P,ZSAZSVFC(,GPR13P)                            0529
*     RFY                                                          0530
*      (GPR01P) UNRSTD;                                            0530
*     RFY                                                          0531
*      (GPR14P) UNRSTD;                                            0531
*   END;                                                           0532
*                                                                  0532
*/* END TPZSAVE                                                      */
*                                                                  0533
*                                   /* TPIOS ENTRY LINKAGE           */
*   ZCRRPTR=CRACRR;                 /* ADDRESS ZCRR                  */
         L     @15,RPHCRR(,RPHPTR)                                 0533
         L     ZCRRPTR,CRACRR(,@15)                                0533
*   ZCRSCPSW=0;                     /* CLEAR BYTE OF INTERNAL FLAGS  */
         MVI   ZCRSCPSW(ZCRRPTR),X'00'                             0534
*   IF R7->FSBTYPE=FSTYPE THEN                                     0535
         CLI   FSBTYPE(R7),X'02'                                   0535
         BNE   @RF00535                                            0535
*     DO;                           /* IF ELEMENT IS A FSB           */
*       FSBPTR=R7;                  /* INITIALIZE PTR TO FSB         */
         ST    R7,FSBPTR(,ZCRRPTR)                                 0537
*       LCPBPTR=FSBLCPB;            /* INITIALIZE PTR TO LCPB        */
         L     @09,FSBPTR(,ZCRRPTR)                                0538
         L     @09,FSBLCPB(,@09)                                   0538
         ST    @09,LCPBPTR(,ZCRRPTR)                               0538
*       ZCRFSBWE=ON;                /* SET SWITCH INDICATING FSB     */
         OI    ZCRFSBWE(ZCRRPTR),B'00000100'                       0539
*     END;                          /* END FSB PROCESSING            */
*   ELSE                                                           0541
*     DO;                           /* NOT FSB, MUST BE LCPB         */
         B     @RC00535                                            0541
@RF00535 DS    0H                                                  0542
*       LCPBPTR=R7;                 /* INITIALIZE LCPB PTR           */
         ST    R7,LCPBPTR(,ZCRRPTR)                                0542
*       RFY                                                        0543
*        (R7) UNRSTD;               /* NOT REQUIRED                  */
*       LCCWPTR=LCPNXLCW;           /* INITIALIZE LCCW POINTER       */
         L     @09,LCPBPTR(,ZCRRPTR)                               0544
         L     @09,LCPNXLCW(,@09)                                  0544
         ST    @09,LCCWPTR(,ZCRRPTR)                               0544
         EJECT
*                                                                  0545
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT-                                                         */
*/*      ISTZNSES                                                    */
*/*                                                                  */
*/* FUNCTION-                                                        */
*/*      IF A SESSION REJECT REQUEST IS DETECTED,                    */
*/*            ALLOCATE A FSB,                                       */
*/*            SET MAJOR AND MINOR RETURN CODES TO                   */
*/*              LOGIN ERROR - NO SESSION,                           */
*/*            POST THE FSB BACK TO THE REQUESTOR,                   */
*/*            SET UP FLAGS FOR EXIT.                                */
*/*                                                                  */
*/********************************************************************/
*                                                                  0545
*ISTZNSES:                                                         0545
*       DO;                         /* SEGMENT ISTZNSES              */
ISTZNSES DS    0H                                                  0546
*         IF FMCNSALU=ON THEN                                      0546
         TM    FMCNSALU(FMCBPTR),B'00000100'                       0546
         BNO   @RF00546                                            0546
*           DO;                     /* IF SESSION REJECT REQUEST     */
*             ZCRPCOD1=0;           /* ZDFA0 PARAMETER               */
         MVI   ZCRPCOD1(ZCRRPTR),X'00'                             0548
*             ZCRPFSB=ON;           /* ZDFA0 PARAMETER               */
         OI    ZCRPFSB(ZCRRPTR),B'00100000'                        0549
*             CALL ISTZDFA0;        /* BUFFER ALLOCATION UTILITY     */
         L     @09,ATCZPSVT(,ATCVTPTR)                             0550
         L     @15,ZPSZDFA0(,@09)                                  0550
         BALR  @14,@15                                             0550
*             FSBPTR=RPHSRPRM;      /* ADDRESS NEWLY OBTAINED FSB    */
         L     @09,RPHSRPRM(,RPHPTR)                               0551
         ST    @09,FSBPTR(,ZCRRPTR)                                0551
*             FSBMJRCD=FRLOGICE;    /* MAJOR = LOGIC ERROR           */
         MVI   FSBMJRCD(@09),X'08'                                 0552
*             FSBMNRCD=FRMUSELE;    /* MINOR = NO SESSION            */
         MVI   FSBMNRCD(@09),X'A3'                                 0553
*             FSBLCPB=LCPBPTR;      /* SET LCPB POINTER IN FSB       */
         L     @15,LCPBPTR(,ZCRRPTR)                               0554
         ST    @15,FSBLCPB(,@09)                                   0554
*             FSBLCCWA=LCPNXLCW;    /* SET LCCW POINTER IN FSB       */
         L     @00,LCPNXLCW(,@15)                                  0555
         ST    @00,FSBLCCWA(,@09)                                  0555
*             CALL ISTZFDB;         /* FEEDBACK FSB TO REQUESTOR     */
         BAL   @14,ISTZFDB                                         0556
*             ZCRTPEX='1'B;         /* SET TPEXIT FLAG FOR CALLER    */
         OI    ZCRTPEX(ZCRRPTR),B'10000000'                        0557
*           END;                    /* END SESSION REJECT            */
*         ELSE                                                     0559
*           ;                       /* NOT SESSION REJECT            */
@RF00546 DS    0H                                                  0560
*       END ISTZNSES;               /* END OF SEGMENT                */
*/********************************************************************/
*/*      SEGMENT ISTZNSES END                                        */
*/********************************************************************/
*                                                                  0561
*     END;                          /* END LCPB PROCESSING   @XM00921*/
*   IF ZCRTPEX=ON THEN                                             0562
@RC00535 TM    ZCRTPEX(ZCRRPTR),B'10000000'                        0562
         BO    @RT00562                                            0562
*     ;                             /* IF TPEXIT REQUIRED            */
*   ELSE                            /* TPEXIT NOT REQUIRED           */
*     IF ZCRFSBWE=OFF               /* IF INPUT WAS NOT FSB  @XM00921*/
*         &LCCWOP=LSEND THEN        /* OPERATION CODE = SEND         */
         TM    ZCRFSBWE(ZCRRPTR),B'00000100'                       0564
         BNZ   @RF00564                                            0564
         L     @09,LCCWPTR(,ZCRRPTR)                               0564
         CLI   LCCWOP(@09),X'F1'                                   0564
         BNE   @RF00564                                            0564
         EJECT
*                                                                  0565
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT-                                                         */
*/*      ISTZSND                                                     */
*/*                                                                  */
*/* FUNCTION-                                                        */
*/*      HANDLE SESSION CONTROL SEND LCCW OPERATION CODE.            */
*/*                                                                  */
*/*      FOR RESPONSES, NO ACTION IS TAKEN BY THIS SEGMENT.          */
*/*                                                                  */
*/*      SESSION INITIATION REQUESTS, (BIND, ACTPU, OR ACTLU)        */
*/*            CAUSE THE PABINHBT FLAG SETTING TO BE MADE            */
*/*            ACCORDING TO THE DATA FLOW CONTROL LEVEL SPECIFIED.   */
*/*                                                                  */
*/*      FOR SET AND TEST SEQUENCE NUMBERS,                          */
*/*            THE FMCB SEQUENCE NUMBERS ARE UPDATED.                */
*/*                                                                  */
*/*      IF THE SESSION IS NOT BOUND,                                */
*/*      OR IF IN 'PROCESSING CLEAR' STATE,                          */
*/*      OR IF IN 'PROCESSING UNBIND' STATE,                         */
*/*            THE REQUEST IS REJECTED.                              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0565
*ISTZSND:                                                          0565
*       DO;                         /* SEGMENT ISTZSND               */
ISTZSND  DS    0H                                                  0566
*         IF LCCWIDAT=ON THEN       /* IF IMMEDIATE DATA FLAG ON     */
         L     @09,LCCWPTR(,ZCRRPTR)                               0566
         TM    LCCWIDAT(@09),B'00001000'                           0566
         BNO   @RF00566                                            0566
*           RHPTR=ADDR(LCCWDATA);   /* LCCW CONTAINS RH DATA         */
         LA    @09,LCCWDATA(,@09)                                  0567
         ST    @09,RHPTR(,ZCRRPTR)                                 0567
*         ELSE                      /* IF IMMEDIATE DATA FLAG OFF    */
*           RHPTR=LCCWDATA;         /* LCCW ADDRESSES RH DATA        */
         B     @RC00566                                            0568
@RF00566 L     @09,LCCWPTR(,ZCRRPTR)                               0568
         L     @09,LCCWDATA(,@09)                                  0568
         ST    @09,RHPTR(,ZCRRPTR)                                 0568
*         IF LCCWCD=OFF THEN        /* IF NOT DATA CHAINED           */
@RC00566 L     @09,LCCWPTR(,ZCRRPTR)                               0569
         TM    LCCWCD(@09),B'10000000'                             0569
         BNZ   @RF00569                                            0569
*           SRUPTR=RHPTR+3;         /* SRU IS IMMEDIATELY ADDRESSABLE*/
         LA    @09,3                                               0570
         AL    @09,RHPTR(,ZCRRPTR)                                 0570
         ST    @09,SRUPTR(,ZCRRPTR)                                0570
*         ELSE                                                     0571
*           DO;                     /* IF DATA CHAINED               */
         B     @RC00569                                            0571
@RF00569 DS    0H                                                  0572
*             K=1;                  /* START AT BEGIN OF SRUCOPY     */
         LA    @09,1                                               0572
         LR    K,@09                                               0572
*             SRUPTR=ADDR(SRUCOPY); /* SET RUPOINTER TO POINT TO   0573
*                                      COPIED RU                     */
         LA    @00,SRUCOPY(,ZCRRPTR)                               0573
         ST    @00,SRUPTR(,ZCRRPTR)                                0573
*             IF LCCWIDAT=ON        /* IF IMMEDIATE DATA             */
*                 &LCCWCNT=4 THEN                                  0574
         L     @15,LCCWPTR(,ZCRRPTR)                               0574
         TM    LCCWIDAT(@15),B'00001000'                           0574
         BNO   @RF00574                                            0574
         CLC   LCCWCNT(2,@15),@CH00052                             0574
         BNE   @RF00574                                            0574
*               DO;                 /* & LENGTH = 4                  */
*                 RUPTRC=ADDR(LCCWDATA);/* SET RUPTR = FIRST RU BYTE */
         LA    @00,LCCWDATA(,@15)                                  0576
         ST    @00,RUPTRC(,ZCRRPTR)                                0576
*                 SRUCOPY(K)=DATALCCW(4);/* COPY 1 BYTE OF RU        */
         LR    @15,ZCRRPTR                                         0577
         ALR   @15,K                                               0577
         LR    @14,@00                                             0577
         MVC   SRUCOPY-1(1,@15),DATALCCW+3(@14)                    0577
*                 K=K+1;            /* UPDATE INDEX INTO SRUCOPY     */
         ALR   K,@09                                               0578
*               END;                /* END IMMEDIATE 4 BYTE DATA     */
*             ELSE                  /* NOT IMMEDIATE 4 BYTE DATA     */
*               IF LCCWCNT>3 THEN                                  0580
         B     @RC00574                                            0580
@RF00574 L     @09,LCCWPTR(,ZCRRPTR)                               0580
         LH    @15,LCCWCNT(,@09)                                   0580
         N     @15,@CF04247                                        0580
         C     @15,@CF00287                                        0580
         BNH   @RF00580                                            0580
*                 DO;               /* IF LENGTH .GT. 3              */
*                   RUPTRC=ADDR(LCCWDATA)+3;/* POINT TO PART OF RU IN
*                                      DATA AREA             @XM08890*/
         LA    @14,3                                               0582
         LA    @09,LCCWDATA(,@09)                                  0582
         ALR   @09,@14                                             0582
         ST    @09,RUPTRC(,ZCRRPTR)                                0582
*                   SRUCOPY(1:MIN((LCCWCNT-3),6))=DATALCCW(1:(LCCWCNT-3
*                       ));         /* COPY LCCW DATA TO SRU         */
         SLR   @15,@14                                             0583
         LA    @14,6                                               0583
         CR    @14,@15                                             0583
         BNH   *+6
         LR    @14,@15                                             0583
         BCTR  @14,0                                               0583
         EX    @14,@SM04248                                        0583
*                   K=K+(LCCWCNT-3);/* DECREMENT DATA COUNTER        */
         ALR   K,@15                                               0584
*                 END;              /* END LENGTH .GT. 3             */
*               ELSE                                               0586
*                 ;                 /* IF LENGTH NOT .GT. 3          */
@RF00580 DS    0H                                                  0587
*             DO WHILE(LCCWPTR^=0&K<7);/* COPY 6 BYTES OF RU DATA    */
@RC00574 B     @DE00587                                            0587
@DL00587 DS    0H                                                  0588
*               LCCWPTR=LCCWPTR+LENGTH(ISTLCCW);/* INCREMENT TO NEXT
*                                      LCCW IN CHAIN                 */
         LA    @09,8                                               0588
         AL    @09,LCCWPTR(,ZCRRPTR)                               0588
         ST    @09,LCCWPTR(,ZCRRPTR)                               0588
*               IF LCCWOP=LTIC THEN /* IF OPCODE = LTIC              */
         CLI   LCCWOP(@09),X'08'                                   0589
         BNE   @RF00589                                            0589
*                 LCCWPTR=LCCWDATA; /* GET NEXT LCCW ADDRESS         */
         L     @09,LCCWDATA(,@09)                                  0590
         ST    @09,LCCWPTR(,ZCRRPTR)                               0590
*               ELSE                                               0591
*                 ;                 /* OPCODE .NE. LTIC              */
@RF00589 DS    0H                                                  0592
*               IF LCCWIDAT=ON THEN /* IF IMMEDIATE DATA             */
         L     @09,LCCWPTR(,ZCRRPTR)                               0592
         TM    LCCWIDAT(@09),B'00001000'                           0592
         BNO   @RF00592                                            0592
*                 RUPTRC=ADDR(LCCWDATA);/* ADDRESS DATA IN LCCW      */
         LA    @09,LCCWDATA(,@09)                                  0593
         ST    @09,RUPTRC(,ZCRRPTR)                                0593
*               ELSE                /* IF NOT IMMEDIATE DATA         */
*                 RUPTRC=LCCWDATA;  /* ADDRESS DATA FROM LCCW        */
         B     @RC00592                                            0594
@RF00592 L     @09,LCCWPTR(,ZCRRPTR)                               0594
         L     @09,LCCWDATA(,@09)                                  0594
         ST    @09,RUPTRC(,ZCRRPTR)                                0594
*               IF LCCWCNT>0        /* IF ANY ASSOCIATED DATA        */
*                   &K<7 THEN                                      0595
@RC00592 L     @09,LCCWPTR(,ZCRRPTR)                               0595
         LH    @09,LCCWCNT(,@09)                                   0595
         N     @09,@CF04247                                        0595
         LTR   @09,@09                                             0595
         BNP   @RF00595                                            0595
         C     K,@CF00714                                          0595
         BNL   @RF00595                                            0595
*                 DO;               /* & STILL MOVING DATA           */
*                   SRUCOPY(K:MIN((K+(LCCWCNT-1)),6))=DATALCCW(1:  0597
*                       LCCWCNT);   /* COPY NUMBER OF RU BYTES     0597
*                                      INDICATED BY COUNT            */
         LR    @15,ZCRRPTR                                         0597
         ALR   @15,K                                               0597
         LR    @14,@09                                             0597
         BCTR  @14,0                                               0597
         ALR   @14,K                                               0597
         LA    @11,6                                               0597
         CR    @14,@11                                             0597
         BNH   *+6
         LR    @14,@11                                             0597
         SLR   @14,K                                               0597
         L     @11,RUPTRC(,ZCRRPTR)                                0597
         EX    @14,@SM04250                                        0597
*                   K=K+LCCWCNT;    /* UPDATE INDEX INTO COPY FIELD  */
         ALR   K,@09                                               0598
*                 END;              /* END DATA MOVE                 */
*               ELSE                                               0600
*                 ;                 /* IF NOT MOVING DATA            */
@RF00595 DS    0H                                                  0601
*               IF LCCWCD=OFF THEN  /* IF NOT DATA CHAINED           */
         L     @09,LCCWPTR(,ZCRRPTR)                               0601
         TM    LCCWCD(@09),B'10000000'                             0601
         BNZ   @RF00601                                            0601
*                 LCCWPTR=0;        /* CLEAR LCCW POINTER            */
         SLR   @09,@09                                             0602
         ST    @09,LCCWPTR(,ZCRRPTR)                               0602
*               ELSE                                               0603
*                 ;                 /* IF DATA CHAINED               */
@RF00601 DS    0H                                                  0604
*             END;                  /* END DATA COPY LOOP            */
@DE00587 L     @09,LCCWPTR(,ZCRRPTR)                               0604
         LTR   @09,@09                                             0604
         BZ    @DC00587                                            0604
         C     K,@CF00714                                          0604
         BL    @DL00587                                            0604
@DC00587 DS    0H                                                  0605
*             LCCWPTR=LCPNXLCW;     /* RESTORE LCCW POINTER          */
         L     @09,LCPBPTR(,ZCRRPTR)                               0605
         L     @09,LCPNXLCW(,@09)                                  0605
         ST    @09,LCCWPTR(,ZCRRPTR)                               0605
*           END;                    /* END DATA CHAINING             */
*         RFY                                                      0607
*           K UNRSTD;                                              0607
@RC00569 DS    0H                                                  0608
*         IF RHQS=OFF THEN          /* IF REQUEST                    */
         L     @09,RHPTR(,ZCRRPTR)                                 0608
         TM    RHQS(@09),B'10000000'                               0608
         BNZ   @RF00608                                            0608
*           ZCRLREQ=ON;             /* SET REQUEST PROCESS FLAG      */
         OI    ZCRLREQ(ZCRRPTR),B'00010000'                        0609
*         ELSE                                                     0610
*           ;                       /* IF RESPONSE                   */
@RF00608 DS    0H                                                  0611
*         IF(SRUQSREQ=SRACTPU       /* IF ACTIVATE PHYSICAL          */
*             ×SRUQSREQ=SRBIND)     /* × BIND                        */
*             &ZCRLREQ=ON THEN                                     0611
         L     @09,SRUPTR(,ZCRRPTR)                                0611
         CLI   SRUQSREQ(@09),X'11'                                 0611
         BE    @GL00005                                            0611
         CLI   SRUQSREQ(@09),X'31'                                 0611
         BNE   @RF00611                                            0611
@GL00005 TM    ZCRLREQ(ZCRRPTR),B'00010000'                        0611
         BNO   @RF00611                                            0611
*           DO;                     /* REQUEST                       */
*             FMCOSSN=0;            /* ZERO OUTBOUND SYNCH COUNTER   */
         SLR   @09,@09                                             0613
         STH   @09,FMCOSSN(,FMCBPTR)                               0613
*             FMCISSN=0;            /* ZERO INBOUND SYNCH COUNTER    */
         STH   @09,FMCISSN(,FMCBPTR)                               0614
*             FMCOASN=0;            /* ZERO ASYNCHRONOUS COUNTER     */
         STH   @09,FMCOASN(,FMCBPTR)                               0615
*           END;                    /* END ACTPU OR BIND REQUEST     */
*         ELSE                                                     0617
*           ;                       /* NOT ACTPU OR BIND REQUEST     */
@RF00611 DS    0H                                                  0618
*         IF SRUQSREQ=SRBIND        /* IF BIND REQUEST       @XM08711*/
*             &ZCRLREQ=ON THEN                                     0618
         L     @09,SRUPTR(,ZCRRPTR)                                0618
         CLI   SRUQSREQ(@09),X'31'                                 0618
         BNE   @RF00618                                            0618
         TM    ZCRLREQ(ZCRRPTR),B'00010000'                        0618
         BNO   @RF00618                                            0618
*           DO;                                                    0619
*             IF FMCSNBI=OFF        /* IF SESSION BOUND              */
*                 ×FMCBIP=ON THEN                                  0620
         TM    FMCBIP(FMCBPTR),B'00001000'                         0620
         BNZ   @RT00620                                            0620
         TM    FMCSNBI(FMCBPTR),B'10000000'                        0620
         BO    @RF00620                                            0620
@RT00620 DS    0H                                                  0621
*               DO;                 /* × PROCESSING BIND             */
*                 ZCRPCOD1=0;       /* ZDFBO PARAMETER               */
         MVI   ZCRPCOD1(ZCRRPTR),X'00'                             0622
*                 ZCRPFSB=ON;       /* ZDFBO PARAMETER               */
         OI    ZCRPFSB(ZCRRPTR),B'00100000'                        0623
*                 CALL ISTZDFA0;    /* BUFFER ALLOCATION UTILITY     */
         L     @09,ATCZPSVT(,ATCVTPTR)                             0624
         L     @15,ZPSZDFA0(,@09)                                  0624
         BALR  @14,@15                                             0624
*                 FSBPTR=RPHSRPRM;  /* ADDRESS NEWLY OBTAINED FSB    */
         L     @09,RPHSRPRM(,RPHPTR)                               0625
         ST    @09,FSBPTR(,ZCRRPTR)                                0625
*                 FSBMJRCD=FRLOGICE;/* LOGIC ERROR                   */
         MVI   FSBMJRCD(@09),X'08'                                 0626
*                 FSBMNRCD=FRMUSELE;/* NO SESSION            @XM02224*/
         MVI   FSBMNRCD(@09),X'A3'                                 0627
*                 FSBLCPB=LCPBPTR;  /* SET LCPB ADDRESS IN FSB       */
         L     @07,LCPBPTR(,ZCRRPTR)                               0628
         ST    @07,FSBLCPB(,@09)                                   0628
*                 FSBLCCWA=LCPNXLCW;/* SET LCCW ADDRESS IN FSB       */
         L     @07,LCPNXLCW(,@07)                                  0629
         ST    @07,FSBLCCWA(,@09)                                  0629
*                 CALL ISTZFDB;     /* FEEDBACK STATUS               */
