SPCIAL   DS    0H                                               01S1408
*   RFY                                                         01S1408
*    (RDTEPTR,                                                  01S1408
*     RRTEPTR) RSTD;                                            01S1408
*   IF RPRENTRY=RPRENTLN&RLNPORT=ON /* IF ENTRY IS A                 */
*     THEN                          /* PORT, ON THE FIRST            */
         CLI   RPRENTRY(RDTEPTR),X'50'                          01S1409
         BNE   @RF01409                                         01S1409
         TM    RLNPORT(RDTEPTR),B'00010000'                     01S1409
         BNO   @RF01409                                         01S1409
*     IF NAMESAVE=BLANKS            /* PASS SAVE THE NAME,           */
*       THEN                                                    01S1410
         CLC   NAMESAVE(8),@CC03177                             01S1410
         BNE   @RF01410                                         01S1410
*       DO;                         /* FILL IN THE PORT              */
*         NAMESAVE=RPRNAME;         /* COM ID DAF AND USE            */
         MVC   NAMESAVE(8),RPRNAME(RDTEPTR)                     01S1412
*         RPRNAME=RPTCTNM;          /* THE PORT NAME FOR A           */
         MVC   RPRNAME(8,RDTEPTR),RPTCTNM(RDTEPTR)              01S1413
*         RPTLNDAF=RRTRSID;         /* SECOND PASS AT THIS           */
         MVC   RPTLNDAF(2,RDTEPTR),RRTRSID(RRTEPTR)             01S1414
*         RDTEPTR=RDTEPTR-RPRPELEN; /* ENTRY. BACK UP TO             */
         SH    RDTEPTR,RPRPELEN(,RDTEPTR)                       01S1415
*       END;                        /* PRIOR RDTE TO LOOP.           */
*     ELSE                                                      01S1417
*       DO;                         /* ON SECOND PASS,               */
         B     @RC01410                                         01S1417
@RF01410 DS    0H                                               01S1418
*         RPRNAME=NAMESAVE;         /* RESTORE ENTRY NAME,           */
         MVC   RPRNAME(8,RDTEPTR),NAMESAVE                      01S1418
*         NAMESAVE=BLANKS;          /* SET THE ENTRY DAF,            */
         MVC   NAMESAVE(8),@CC03177                             01S1419
*         RPRDAF=RRTRSID;           /* AND SAVE PORT ADDR            */
         MVC   RPRDAF(2,RDTEPTR),RRTRSID(RRTEPTR)               01S1420
*         PORTPTR=RDTEPTR;          /* FOR RELATED VTERMS            */
         LR    PORTPTR,RDTEPTR                                  01S1421
*       END;                        /* AND UTERMS.                   */
*/********************************************************************/
*/* IF NOT A PORT THEN ITS A 2980 SO MUST HANDLE IT                  */
*/********************************************************************/
*                                                               01S1423
*   ELSE                                                        01S1423
*     DO;                           /* IF NOT A PORT                 */
