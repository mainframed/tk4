         LA    @02,PROTODCB                                     01S0530
         SLR   @10,@02                                          01S0530
         BCTR  @10,0                                            01S0530
         EX    @10,@SM03547                                     01S0530
*   RFY                                                         01S0531
*     IHADCB BASED(ADDR(RNCLMDCB));                             01S0531
*   DCBDDNAM=RRNNMNCP;              /* USE NCP LOAD MODULE NAME AS
*                                      DDNAME                        */
