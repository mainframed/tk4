*/* THE DEFAULT IS TAKEN. IF THE SUBPARMS ARE VALID THE APPROPRIATE  */
*/* BITS ARE TURNED ON IN THE TERMINAL MODEL IF OTHER THAN THE       */
*/* DEFAULT VALUE IS SPECIFIED.                                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0624
*LINITBMD:                                                         0624
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0624
LINITBMD DS    0H                                                  0625
*   RTRRCIT1=OFF;                                                  0625
*   RTRRCIT2=OFF;                                                  0626
         L     @08,MLCPTR3(,MLCAREG)                               0626
         NI    RTRRCIT1(@08),B'00111111'                           0626
*   IF PHRF2VAL(1)=1                /* IF 1ST SUBPARM OK AND WAS     */
*     THEN                          /* 'YES' TURN ON FLAG IN         */
         CLC   PHRF2VAL(4,PHR1REG),@CF00039                        0627
         BNE   @RF00627                                            0627
*     RTRRCIT1=ON;                  /* TERM ENTRY. IF 'NO'           */
         OI    RTRRCIT1(@08),B'10000000'                           0628
*   ELSE                                                           0629
*     ;                                                            0629
@RF00627 DS    0H                                                  0630
*   IF PHRF2NVL>1                   /* MORE THAN ONE SUBPARM         */
*     THEN                          /* IF YES                        */
         LA    @08,1                                               0630
         C     @08,PHRF2NVL(,PHR1REG)                              0630
         BNL   @RF00630                                            0630
*     IF PHRF2VAL(2)=1              /* IF 2ND SUBPARM OK AND WAS     */
*       THEN                        /* 'YES' TURN ON FLAG IN ????    */
         C     @08,PHRF2VAL+4(,PHR1REG)                            0631
         BNE   @RF00631                                            0631
*       RTRRCIT2=ON;                /* ENTRY. IF 'NO' LEAVE FLAG     */
         L     @08,MLCPTR3(,MLCAREG)                               0632
         OI    RTRRCIT2(@08),B'01000000'                           0632
*     ELSE                                                         0633
*       ;                           /* OFF.                          */
@RF00631 DS    0H                                                  0634
*   ELSE                                                           0634
*     ;                             /* ONLY ONE PARM                 */
@RF00630 DS    0H                                                  0635
*   GPR15=0;                        /* SET RTN CODE TO 0             */
         SLR   GPR15,GPR15                                         0635
*   GPR14=ADDR(MAINSUB);                                           0636
         LA    GPR14,MAINSUB                                       0636
*   END LINITBMD;                                                  0637
*                                                                  0637
@EL00021 DS    0H                                                  0637
@EF00021 DS    0H                                                  0637
@ER00021 BR    @14                                                 0637
