@RC00217 TM    RNCGNOW(RNCAREG),B'00100000'                        0220
         BNO   @RF00220                                            0220
*         RNCLNOW='1'B;                                            0221
         OI    RNCLNOW(RNCAREG),B'00010000'                        0221
*       ELSE                                                       0222
*         RNCLNOW='0'B;                                            0222
         B     @RC00220                                            0222
@RF00220 NI    RNCLNOW(RNCAREG),B'11101111'                        0222
*       DO WHILE(RC=0);                                            0223
@RC00220 B     @DE00223                                            0223
@DL00223 DS    0H                                                  0224
