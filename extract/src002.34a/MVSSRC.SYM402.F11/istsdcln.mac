         L     @04,RNCGP(,RNCAREG)                                 0260
         AH    @03,RGPCTELN(,@04)                                  0260
         STH   @03,RGPCTELN(,@04)                                  0260
*             IF RLNANSWR='0'B      /* WAS ANSWER=YES SPECIFED?    0261
*                                                            @X50BHMX*/
