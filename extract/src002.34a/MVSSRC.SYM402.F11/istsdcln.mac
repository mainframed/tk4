         ST    GPR13,MLCSAV3+8(,MLCAREG)                           0205
*   DATDREG=ADDR(RNCDAT1);          /* SET UP OUR DATA REG           */
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0206
*   GENERATE;                                                      0207
           USING @DATD,11                 ADDRESSABILITY FOR DATA
*   IF RNCGGRP='1'B                 /* CHECK GROUP MACRO FP          */
