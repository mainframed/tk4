*       RTRTRMAX=CVBNUM2;           /* IF OK - STORE IT IN           */
         L     @08,MLCPTR3(,MLCAREG)                               0619
         MVC   RTRTRMAX(1,@08),CVBNUM2                             0619
*     END;                                                         0620
*   GPR15=0;                        /* SET RTN CODE TO 0             */
@RC00614 SLR   GPR15,GPR15                                         0621
*   GPR14=ADDR(MAINSUB);                                           0622
         LA    GPR14,MAINSUB                                       0622
*   END LINXMTLM;                                                  0623
*                                                                  0623
@EL00020 DS    0H                                                  0623
@EF00020 DS    0H                                                  0623
