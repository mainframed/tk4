*                                                                  0469
GETALCA  DS    0H                                                  0470
*/********************************************************************/
*/*                GETALCA ROUTINE                                   */
*/* IN-LINE SUB-ROUTINE TO OBTAIN THE AREA FOR APPL/LOCAL            */
*/* COMMUNICATION AREA (ALCA) AND INITIALIZE IT. THE ALCA IS DYNAMIC */
*/* SPACE FOR APPL OR LOCAL. SEE DSECT ISTALCA FOR DESCRIPTION       */
*/********************************************************************/
*                                                                  0470
*   RFY                                                            0470
*    (RPHPTR,                                                      0470
*     BLOCKPTR) RSTD;                                              0470
*   RLENGTH=LENALCA;                /* SET UP LENGTH FOR GETSTOR     */
         L     @11,LENALCA                                         0471
         ST    @11,RLENGTH                                         0471
*   RPHPTR=MLCRPH;                  /* SET UP RPH@ FOR GETSTOR ISSUE
*                                      GETSTOR MACRO FOR ISTALCA     */
         L     RPHPTR,MLCRPH(,MLCAPTR)                             0472
*GETSTR1:                                                          0473
*   ;                                                              0473
*                                                                  0473
GETSTR1  DS    0H                                                  0474
*/*  GETSTOR SIZE(RLENGTH)CLEAR(YES)TYPE(PAGEABLE)ADSPACE(PRIVATE)   */
*                                                                  0474
*   DO;                                                            0474
*                                                                  0474
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)                        */
*/* END OF ATBUILD                                                   */
*                                                                  0475
*     RFY                                                          0475
*      (GPR00P,                                                    0475
*       GPR01P,                                                    0475
*       GPR02P,                                                    0475
*       GPR03P) RSTD;                                              0475
*     RFY                                                          0476
*       ISTRPH BASED(GPR01P);                                      0476
*     GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,GPR09P,
*         GPR10P,GPR11P,GPR12P,GPR13P) SETS(RPHWORK);              0477
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS
*     GPR00F=RLENGTH;               /* SIZE IN REG0                  */
         L     GPR00F,RLENGTH                                      0478
*     DCL                                                          0479
*       1 CMZA0008 FIXED(31) STATIC LOCAL,                         0479
*        2 * FIXED(8) INIT(96),     /* OPTION FLAGS                  */
*        2 * FIXED(8) INIT(96),     /* STORAGE KEY                   */
*        2 * FIXED(8) INIT(229),    /* SUBPOOL NUMBER                */
*        2 * FIXED(8) INIT('00'X);  /* REQUEST TYPE                  */
*     GPR03F=CMZA0008;                                             0480
         L     GPR03F,CMZA0008                                     0480
*     DO;                           /* LISATCVT                    0481
*                                      CVTBASE(PAGEABLE,(GPR02P))    */
*       GPR02P=ATCLCPTR;            /* SET UP ATCVT POINTER          */
         L     GPR02P,ATCLCPTR                                     0482
*     END;                          /* LISATCVT                      */
*     CALL GPR02P->ATCGTSTR;        /* CALL ISTORFSR                 */
         L     @15,ATCGTSTR(,GPR02P)                               0484
         BALR  @14,@15                                             0484
*     RFY                                                          0485
*      (GPR00P,                                                    0485
*       GPR01P,                                                    0485
*       GPR02P,                                                    0485
*       GPR03P) UNRSTD;                                            0485
*   END;                            /* END OF GETSTOR                */
*                                                                  0486
*   /*****************************************************************/
*   /*                                                               */
*   /*                                                       @X04BHQH*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0487
*   IF RC=0 THEN                    /* IF GETSTOR WAS SUCCESS        */
         LTR   RC,RC                                               0487
         BNZ   @RF00487                                            0487
*     DO;                           /* DO SET UP ALCA                */
*       MLCSECCA=RPHSRPRM;          /* GET ADDR OF STORAGE           */
         L     @11,RPHSRPRM(,GPR01P)                               0489
         ST    @11,MLCSECCA(,MLCAPTR)                              0489
*       ALCAPTR=RPHSRPRM;           /* SET UP ALCA ADDRESABIL        */
         LR    ALCAPTR,@11                                         0490
*/*  GETSTOR SIZE(MLCGETSZ)SAVE(YES)TYPE(PAGEABLE)ADSPACE(PRIVATE)   */
*                                                                  0491
*       DO;                                                        0491
*                                                                  0491
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)                        */
*/* END OF ATBUILD                                                   */
*                                                                  0492
*         RFY                                                      0492
*          (GPR00P,                                                0492
*           GPR01P,                                                0492
*           GPR02P,                                                0492
*           GPR03P) RSTD;                                          0492
*         RFY                                                      0493
*           ISTRPH BASED(GPR01P);                                  0493
*         GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,
*             GPR09P,GPR10P,GPR11P,GPR12P,GPR13P) SETS(RPHWORK);   0494
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS
*         GPR00F=MLCGETSZ;          /* SIZE IN REG0                  */
         L     GPR00F,MLCGETSZ(,MLCAPTR)                           0495
*         DCL                                                      0496
*           1 CMZA0026 FIXED(31) STATIC LOCAL,                     0496
*            2 * FIXED(8) INIT(64), /* OPTION FLAGS                  */
*            2 * FIXED(8) INIT(96), /* STORAGE KEY                   */
*            2 * FIXED(8) INIT(229),/* SUBPOOL NUMBER                */
*            2 * FIXED(8) INIT('00'X);/* REQUEST TYPE                */
*         GPR03F=CMZA0026;                                         0497
         L     GPR03F,CMZA0026                                     0497
*         DO;                       /* LISATCVT                    0498
*                                      CVTBASE(PAGEABLE,(GPR02P))    */
*           GPR02P=ATCLCPTR;        /* SET UP ATCVT POINTER          */
         L     GPR02P,ATCLCPTR                                     0499
*         END;                      /* LISATCVT                      */
*         CALL GPR02P->ATCGTSTR;    /* CALL ISTORFSR                 */
         L     @15,ATCGTSTR(,GPR02P)                               0501
         BALR  @14,@15                                             0501
*         RFY                                                      0502
*          (GPR00P,                                                0502
*           GPR01P,                                                0502
*           GPR02P,                                                0502
*           GPR03P) UNRSTD;                                        0502
*       END;                        /* END OF GETSTOR                */
*                                                                  0503
*       /*************************************************************/
*       /*                                                           */
*       /*                                                   @X04BHQH*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0504
*       IF RC=0 THEN                /* IF GETSTOR WAS SUCCESS        */
         SLR   @11,@11                                             0504
         CR    RC,@11                                              0504
         BNE   @RF00504                                            0504
*         DO;                       /* BUILD AREA HAS BEEN OBTAIND   */
*           BLOCKPTR=RPHSRPRM;      /* SET UP INTER DSECT INALCB     */
         L     @02,RPHSRPRM(,GPR01P)                               0506
         LR    BLOCKPTR,@02                                        0506
*           INALEN=0;               /* SET LENGTH AND                */
         ST    @11,INALEN(,BLOCKPTR)                               0507
*           INAFWD=0;               /* FORWARD POINTER TO ZERO       */
         ST    @11,INAFWD(,BLOCKPTR)                               0508
*           ALCFAREA=RPHSRPRM;      /* SET FIRST AND LAST ALCA       */
         LR    @11,ALCAPTR                                         0509
         AL    @11,@CF03472                                        0509
         ST    @02,ALCFAREA-4868(,@11)                             0509
*           ALCLAREA=RPHSRPRM;      /* POINTERS TO FIRST BLOCK       */
*                                                                  0510
         LR    @11,ALCAPTR                                         0510
         AL    @11,@CF03473                                        0510
         ST    @02,ALCLAREA-4872(,@11)                             0510
*           /*********************************************************/
*           /*                                                       */
*           /* SEGMENT(MDLINIT)REQUEST                               */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0511
*           DO;                                                    0511
*             GOTO MDLINIT;                                        0512
         B     MDLINIT                                             0512
*MDLINIT#:                                                         0513
*           END;                                                   0513
*                                                                  0513
MDLINIT# DS    0H                                                  0514
*/* GO INIT REST OF ALCA                                             */
*                                                                  0514
*                                                                  0514
*/* SEGMENT(MDLINIT)START                                            */
*                                                                  0514
*           DO;                                                    0514
*             GOTO MDLINIT@;                                       0515
         B     MDLINIT@                                            0515
*MDLINIT:                                                          0516
*           END;                                                   0516
*                                                                  0516
MDLINIT  DS    0H                                                  0517
*/********************************************************************/
*/*              MDLINIT START INTERNAL IN-LINE SUB                  */
*/*       THIS ROUTINE SETS THE DEFAULTS IN THE APPL LOCAL AND       */
*/*       TERMINAL MODEL ENTRIES IN THE ALCA. THESE MODELS ARE USED  */
*/*       TO BE MOVED IN TO EACH ENTRY BEFOR THE NON DEFAULT         */
*/*       PARAMETERS ARE SET SO THE DEFAULTS DO NOT HAVE TO BE SET   */
*/*       EACH TIME                                                  */
*/********************************************************************/
*                                                                  0517
*           RFY                                                    0517
*             RDTEPTR RSTD;                                        0517
*           RFY                                                    0518
*             ISTRPRE BASED(RDTEPTR);                              0518
*           RFY                                                    0519
*             ISTRCPRE BASED(RDTEPTR);                             0519
*           RFY                                                    0520
*             ISTRAP BASED(RDTEPTR);                               0520
*           RFY                                                    0521
*             ISTRLC BASED(RDTEPTR);                               0521
*           RFY                                                    0522
*             ISTRTR BASED(RDTEPTR);                               0522
*           RFY                                                    0523
*             ISTDEVCH BASED(RDTEPTR);/* RDTEPTR LOADED DYNAMICALLY  */
*           RDTEPTR=ADDR(ALCHDMDL); /* PICK UP MODEL ADDRESS         */
         LA    RDTEPTR,ALCHDMDL(,ALCAPTR)                          0524
*           RPRENTRY=HDRTYPE;       /* SET UP TYPE                   */
         MVC   RPRENTRY(1,RDTEPTR),HDRTYPE                         0525
*           RPRELEN=LENGTH(ISTRDT); /* SET UP THE LENGTH             */
         MVC   RPRELEN(2,RDTEPTR),@CH03463                         0526
*           RPRHDTYP=HDRTYPE;       /* SET UP HEADER TYPE    @D32CKCK*/
         MVC   RPRHDTYP(1,RDTEPTR),HDRTYPE                         0527
*           RPRNAME=MLCCONFG;       /* ENTRY NAME                    */
         MVC   RPRNAME(8,RDTEPTR),MLCCONFG(MLCAPTR)                0528
*           IF HDRTYPE=RPRENTAH THEN/* IF A APPL ENTRY               */
         CLI   HDRTYPE,X'02'                                       0529
         BNE   @RF00529                                            0529
*             DO;                   /* SET IT UP                     */
*               RPRNOSRT='1'B;      /* SET NO SRT BIT IN APPL HEADER
*                                                            @XA08112*/
         OI    RPRNOSRT(RDTEPTR),B'10000000'                       0531
*               RDTEPTR=ADDR(ALCAPMDL);/* SET UP APPL MODEL          */
         LA    RDTEPTR,ALCAPMDL(,ALCAPTR)                          0532
*               RPRELEN=LENGTH(ISTRAP);/* ENTRY LENGTH               */
         MVC   RPRELEN(2,RDTEPTR),@CH03464                         0533
*               RPRENTRY=RPRENTAP;  /* APPL ENTRY TYPE               */
         MVI   RPRENTRY(RDTEPTR),X'55'                             0534
*               RPRNAME='';         /* APPL NAME                     */
         MVI   RPRNAME(RDTEPTR),C' '                               0535
         MVC   RPRNAME+1(7,RDTEPTR),RPRNAME(RDTEPTR)               0535
*               RPRNOSRT='1'B;      /* SET NO SRT BIT IN ALL APPLS 0536
*                                                            @XA08112*/
         OI    RPRNOSRT(RDTEPTR),B'10000000'                       0536
*               RPRHDTYP=RPRENTAH;  /* SET UP HEADER TYPE    @D32CKCK*/
         MVI   RPRHDTYP(RDTEPTR),X'02'                             0537
*               RAPAPPWD='';        /* PASSWORD                      */
         MVI   RAPAPPWD(RDTEPTR),C' '                              0538
         MVC   RAPAPPWD+1(7,RDTEPTR),RAPAPPWD(RDTEPTR)             0538
*               RAPBFACT=1;         /* BUFFER LIMIT FACTOR TO      0539
*                                      MULTIPLY TERMINAL BUFFER LIMIT*/
         MVI   RAPBFACT(RDTEPTR),X'01'                             0539
*/********************************************************************/
*/* THE REMAINDER OF THE APPL ENTRY MODEL IS ZERO BECAUSE THE ALCA   */
*/* IS ZEROED BY GETSTOR                                             */
*/********************************************************************/
*                                                                  0540
*             END;                                                 0540
*           ELSE                                                   0541
*             DO;                   /* THE ENTRY IS A LOCAL          */
         B     @RC00529                                            0541
@RF00529 DS    0H                                                  0542
*               RDTEPTR->RDTNODID=3;/* SET 3 NODES AS DEFAULT D31KKQH*/
         MVI   RDTNODID(RDTEPTR),X'03'                             0542
*               RDTEPTR->RDTCRSDS=BLANK8;/*                  @X50BHMX*/
         MVC   RDTCRSDS(8,RDTEPTR),@CC02956                        0543
*               RDTEPTR->RDTCRSPW=BLANK8;/*                  @X50BHMX*/
         MVC   RDTCRSPW(8,RDTEPTR),@CC02956                        0544
*               RDTEPTR=ADDR(ALCLCMDL);/* GET ADR OF DUMMY LOCAL     */
         LA    RDTEPTR,ALCLCMDL(,ALCAPTR)                          0545
*               RPRENTRY=RPRENTLC;  /* LOCAL ENTRY TYPE              */
         MVI   RPRENTRY(RDTEPTR),X'45'                             0546
*               RPRNOSRT='1'B;      /* SET NO SRT BIT IN ALL LOCALS
*                                                            @XA08112*/
         OI    RPRNOSRT(RDTEPTR),B'10000000'                       0547
*               RPRHDTYP=RPRENTLH;  /* SET UP HEADER TYPE    @D32CKCK*/
         MVI   RPRHDTYP(RDTEPTR),X'03'                             0548
*               RPRELEN=ADDR(ALCPHR1)-ADDR(ALCLCMDL);/* SO AREA    0549
*                                      HANDLER GET TWO ENTRIES AT  0549
*                                      ONCE                          */
         MVC   RPRELEN(2,RDTEPTR),@CH03465                         0549
*               RDTEPTR=ADDR(RPRDEVCH);                            0550
         LA    @11,RPRDEVCH(,RDTEPTR)                              0550
         LR    RDTEPTR,@11                                         0550
*               DEVINPUT='1'B;      /* LOCAL IS FOR INPUT            */
*               DEVOTPUT='1'B;      /* LOCAL IS FOR OUTPUT BECAUSE 0552
*                                      ALL LOCAL DEVICES ARE       0552
*                                      INPUT/OUTPUT (EVEN THE      0552
*                                      PRINTERS), THE USER IS NOT  0552
*                                      GIVEN THE OPTION OF 'DIRECTN'
*                                      ON LOCAL                      */
*               DEVSUBND='1'B;      /* LOCAL HAS SUBNODES            */
         OI    DEVINPUT(RDTEPTR),B'11010000'                       0553
*               DEVTCODE=DEV3272;   /* CLUST CONT                    */
         MVI   DEVTCODE(RDTEPTR),X'36'                             0554
*/********************************************************************/
*/*              NOW SET UP THE TERMINAL MODEL                       */
*/********************************************************************/
*                                                                  0555
*               RDTEPTR=ADDR(ALCTRMDL);                            0555
         LA    RDTEPTR,ALCTRMDL(,ALCAPTR)                          0555
*               RPRNAME='';         /* TERMINAL NAME                 */
         MVI   RPRNAME(RDTEPTR),C' '                               0556
         MVC   RPRNAME+1(7,RDTEPTR),RPRNAME(RDTEPTR)               0556
*               RPRENTRY=RPRENTTR;  /* TERMINAL ENTRY TYPE           */
         MVI   RPRENTRY(RDTEPTR),X'80'                             0557
*               RPRHDTYP=RPRENTLH;  /* SET UP HEADER TYPE    @D32CKCK*/
         MVI   RPRHDTYP(RDTEPTR),X'03'                             0558
*               RPRELEN=ADDR(ALCPHR1)-ADDR(ALCTRMDL);/* ENTRY LENGTH */
         MVC   RPRELEN(2,RDTEPTR),@CH03466                         0559
*               RPRPELEN=ADDR(ALCTRMDL)-ADDR(ALCLCMDL);            0560
         MVC   RPRPELEN(2,RDTEPTR),@CH03467                        0560
*               RPRAOPN='1'B;       /* NODE CAN BE OPENED BY       0561
*                                      APPLICATION                   */
         OI    RPRAOPN(RDTEPTR),B'00100000'                        0561
*               RPRNOSRT='1'B;      /* NO SYMBOLIC RESOLUTION TABLE
*                                      ENTRY (FOR POSSIBLE LATER USE
*                                      BY SRT DELETE)                */
         OI    RPRNOSRT(RDTEPTR),B'10000000'                       0562
*               RTRRBISA='1'B;      /* INITIAL STATUS - ACTIVE. VALUE
*                                      FOR LOCAL WILL BE SET LATER 0563
*                                      (IN LOCALMH) TO EQUAL THE   0563
*                                      VALUE FOR ASSOCIATED (THIS) 0563
*                                      TERMINAL ENTRY AFTER ALL    0563
*                                      PARAMETERS HAVE BEEN PROCESSED*/
         OI    RTRRBISA(RDTEPTR),B'01000000'                       0563
*               RCPLGNAP='';        /* NO DEFAULT APPL FOR SIM LOGON */
         MVI   RCPLGNAP(RDTEPTR),C' '                              0564
         MVC   RCPLGNAP+1(7,RDTEPTR),RCPLGNAP(RDTEPTR)             0564
*               RCPLGNCT='';        /* LOGON CHAR. TABLE             */
         MVI   RCPLGNCT(RDTEPTR),C' '                              0565
         MVC   RCPLGNCT+1(7,RDTEPTR),RCPLGNCT(RDTEPTR)             0565
*               RCPBFLIM=2;         /* BUFFER LIMIT                  */
