         TITLE 'ISTSDCCL - CLUSTER MACRO HNDLR                         *
                        '
ISTSDCCL CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(8)                                              0001
         DC    C'ISTSDCCL'                                         0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*   RNCSAV1(2)=GPR13;               /* BACKWARD POINT                */
         ST    GPR13,RNCSAV1+4(,RNCAREG)                           0185
*   GPR13=ADDR(RNCSAV1);            /* SET UP OUR SAVE REG           */
         LA    GPR13,RNCSAV1(,RNCAREG)                             0186
*   MLCSAV3(3)=GPR13;               /* FORWARD POINT                 */
         ST    GPR13,MLCSAV3+8(,MLCAREG)                           0187
*   DATDREG=ADDR(RNCDAT1);          /* SET UP OUR DATA REG           */
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0188
*   GENERATE;                                                      0189
*                                                                  0189
           USING @DATD,11                 ADDRESSABILITY FOR DATA
*/********************************************************************/
*/* WHILE THE RETURN CODE = 0, THIS CODE PREPARES THE PHR PARAMETERS */
*/* AND CALLS PHR.  UPON RETURN FROM PHR, THE RETURN CODE AND THE @  */
*/* OF THE NEXT MACRO KEYWORD ARE SAVED.                             */
*/********************************************************************/
*                                                                  0190
*   RFY                                                            0190
*    (GPR0,                                                        0190
*     GPR1,                                                        0190
*     GPR15) RSTD;                  /* RESTRICT REGS                 */
*                                                                  0190
*   /*****************************************************************/
*   /*                                                               */
*   /* SEGMENT(CLPRSCN)REQUEST                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0191
*   DO;                                                            0191
*     GOTO CLPRSCN;                                                0192
         B     CLPRSCN                                             0192
*CLPRSCN#:                                                         0193
*   END;                                                           0193
*                                                                  0193
CLPRSCN# DS    0H                                                  0194
*   /*****************************************************************/
*   /*                                                               */
*   /*                                                       @D32CKMX*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0194
*   IF RNCGCLPU^='1'B               /* IS PU FOOTPRINT OFF?  @D32CKMX*/
*     THEN                                                         0194
         TM    RNCGCLPU(RNCAREG),B'00001000'                       0194
         BO    @RF00194                                            0194
*     DO;                           /*                       @D32CKMX*/
*       GPR1=RPRENTCL;              /* INDICATE CALLR IS CLSTR       */
         LA    GPR1,112                                            0196
*       CALL ISTSDCRS;              /* CALL SIFTDEF                  */
         L     @15,@CV01241                                        0197
         BALR  @14,@15                                             0197
*       ENTRYPTR=GPR1;              /* SAVE ENTRY POINTER            */
         LR    ENTRYPTR,GPR1                                       0198
*       GPR1=MLCKWD;                /* SETUP REG1 -> KEYWORD         */
         L     GPR1,MLCKWD(,MLCAREG)                               0199
*       RC=0;                       /* INITIALIZE RETURN CODE        */
         SLR   RC,RC                                               0200
*       DO WHILE(RC=0);             /* GO PROCESS KEYWORDS           */
         B     @DE00201                                            0201
@DL00201 DS    0H                                                  0202
*         CALL DOWHILE;                                            0202
         BAL   @14,DOWHILE                                         0202
*DOLOOP:                                                           0203
*         ;                         /* USED TO GEN RETURN ADDR       */
DOLOOP   DS    0H                                                  0204
*/********************************************************************/
*/* AT THIS POINT A RETURN CODE OF 0 MEANS CONTINUE THE DO WHILE     */
*/* LOOP.  OTHERWISE, THE LOOP IS DISCONTINUED WITH A RETURN CODE OF */
*/* 8 OR 4.                                                          */
*/********************************************************************/
*                                                                  0204
*       END;                                                       0204
*                                                                  0204
@DE00201 SLR   @14,@14                                             0204
         CR    RC,@14                                              0204
         BE    @DL00201                                            0204
*/********************************************************************/
*/* AT THIS POINT IF RC=8, INDICATING END OF STRING, WE CALL AREA    */
*/* HANDLER TO MOVE THE MODEL AREA INTO THE BUILD AREA.  UPON RETURN */
*/* WE UPDATE THE CURRENT ENTRY POINTER TO THIS ENTRY. ALSO, ANY     */
*/* COUNTS THAT NEED BE UPDATED, ARE DONE AT THIS POINT.             */
*/********************************************************************/
*                                                                  0205
*       IF RC=8 THEN                                               0205
         CH    RC,@CH00036                                         0205
         BNE   @RF00205                                            0205
*         DO;                                                      0206
*           GPR0=0;                 /* INDICATE ADDR OF MODEL PASSED */
         LR    GPR0,@14                                            0207
*           GPR1=ENTRYPTR;          /* PASS ADDR CLUSTER ENTRY MODEL
*                                                            @D32CKMX*/
         LR    GPR1,ENTRYPTR                                       0208
*           CALL ISTSDCC1;          /* CALL AREA HANDLER             */
         L     @15,@CV01237                                        0209
         BALR  @14,@15                                             0209
*           RC=GPR15;               /* SAVE AREAHANDLER RET. CODE    */
         LR    RC,GPR15                                            0210
*           IF RC=0 THEN                                           0211
         LTR   RC,RC                                               0211
         BNZ   @RF00211                                            0211
*             DO;                   /*                       @D32CKMX*/
*/********************************************************************/
*/* PLACE CLUSTER BUILD AREA ADDR IN RNCA AND                        */
*/* SET CLUSTER ISTATUS BIT THE SAME AS THE TERMINAL ISTATUS BIT     */
*/********************************************************************/
*                                                                  0213
*               RNCCL=GPR1;                                        0213
         ST    GPR1,RNCCL(,RNCAREG)                                0213
*               IF MLCPTR3->RTRRBISA='1'B THEN                     0214
         L     @14,MLCPTR3(,MLCAREG)                               0214
         TM    RTRRBISA(@14),B'01000000'                           0214
         BNO   @RF00214                                            0214
*                 GPR1->RCLRBISA='1'B;                             0215
         OI    RCLRBISA(GPR1),B'01000000'                          0215
*               ELSE                                               0216
*                 GPR1->RCLRBISA='0'B;                             0216
*                                                                  0216
         B     @RC00214                                            0216
@RF00214 NI    RCLRBISA(GPR1),B'10111111'                          0216
*/********************************************************************/
*/*                     BIT HAS BEEN SET                             */
*/********************************************************************/
*/********************************************************************/
*/* UPDATE COUNT OF CLSTR RDT ENTRIES IN RN,GROUP, AND LINE ENTRIES  */
*/********************************************************************/
*                                                                  0217
*               RNCRN->RRNCTECL=RNCRN->RRNCTECL+1;/* INCR. RN ETY    */
@RC00214 L     @14,RNCRN(,RNCAREG)                                 0217
         LA    @10,1                                               0217
         LH    @08,RRNCTECL(,@14)                                  0217
         ALR   @08,@10                                             0217
         STH   @08,RRNCTECL(,@14)                                  0217
*               RNCGP->RGPCTECL=RNCGP->RGPCTECL+1;/* INCR. GP ETY    */
         L     @14,RNCGP(,RNCAREG)                                 0218
         LH    @08,RGPCTECL(,@14)                                  0218
         ALR   @08,@10                                             0218
         STH   @08,RGPCTECL(,@14)                                  0218
*               RNCLN->RLNCTECL=RNCLN->RLNCTECL+1;/* INCR. LN ETY    */
         L     @14,RNCLN(,RNCAREG)                                 0219
         AH    @10,RLNCTECL(,@14)                                  0219
         STH   @10,RLNCTECL(,@14)                                  0219
*             END;                                                 0220
*           ELSE                                                   0221
*             ;                                                    0221
@RF00211 DS    0H                                                  0222
*         END;                                                     0222
*       ELSE                                                       0223
*         ;                         /* AREAHANDLER RAN OUT OF CORE   */
@RF00205 DS    0H                                                  0224
*     END;                                                         0224
*   ELSE                                                           0225
*     ;                             /* RTN CODE NOT 8                */
@RF00194 DS    0H                                                  0226
*   RNCGCLPU='0'B;                  /* CLEAR PU CALLED BY CLUSTER  0226
*                                      FTPT                          */
         NI    RNCGCLPU(RNCAREG),B'11110111'                       0226
*   RNCGCLUS='1'B;                  /* SET CLUSTER FOOTPRINT         */
         OI    RNCGCLUS(RNCAREG),B'00000010'                       0227
*   GPR13=RNCSAV1(2);               /* RESTORE CALLERS REGS          */
         L     GPR13,RNCSAV1+4(,RNCAREG)                           0228
*/********************************************************************/
*/* AT THIS POINT RC CONTAINS EITHER A 0 OR A 4 INDICATING A GOOD    */
*/* RETURN CODE OR AN ERROR RETURN CODE WHICH WILL KILL THE BUILD.   */
*/********************************************************************/
*                                                                  0229
*   RETURN CODE(RC);                /* RETURN TO CALLER              */
         LR    @15,RC                                              0229
         L     @14,12(,@13)                                        0229
         LM    @00,@12,20(@13)                                     0229
         BR    @14                                                 0229
*DOWHILE:                                                          0230
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0230
DOWHILE  DS    0H                                                  0231
*   GPR0=ADDR(CLSKWTBL);            /* POINT TO KYWD TABLES          */
         LA    GPR0,CLSKWTBL                                       0231
*   PHR1REG=ADDR(RNCPHR1);          /* POINT TO PHR OUTPUT AREA      */
         LA    PHR1REG,RNCPHR1(,RNCAREG)                           0232
*   RFY                                                            0233
*     ISTPHROA BASED(PHR1REG);      /* SET UP STRUCTURE BASE         */
*   CALL ISTINCPH;                  /* CALL PHR                      */
         L     @15,@CV01240                                        0234
         BALR  @14,@15                                             0234
*   RC=GPR15;                       /* SAV RETURN CODE FROM PHR      */
         LR    RC,GPR15                                            0235
*   KWDPTR=GPR1;                    /* SAVE UPDATED KEYWORD PTR      */
         LR    KWDPTR,GPR1                                         0236
*   IF RC=0 THEN                                                   0237
         LTR   RC,RC                                               0237
         BNZ   @RF00237                                            0237
*     DO;                                                          0238
*                                                                  0238
*/********************************************************************/
*/* IF THE RETURN CODE FROM PHR = 0, CALL THE PARAMETER HANDLER      */
*/* WHOSE ADDRESS WAS RETURNED IN GPR0 BY PHR.                       */
*/* NOTE: THE EXTERNAL PARAMETER HANDLERS WILL RETURN TO THIS        */
*/*       ROUTINE VIA A RETURN.  ALL INTERNAL PARAMETER HANDLERS     */
*/*       WILL RETURN TO THIS ROUTINE VIA A GOTO 'MAINSUB'.          */
*/* NOTE: ALL PARAMETER HANDLERS MUST HAVE A 0 OR 4 RETURN CODE IN   */
*/*       GPR15 TO INDICATE CONTINUE PROCESSING OR KILL THE BUILD.   */
*/********************************************************************/
*                                                                  0239
*       GPR15=GPR0;                 /* SET UP PARM HANDLER SBRTN ADR */
         LR    GPR15,GPR0                                          0239
*       CALL PARMHDLR;              /* GO SETUP RDT ENTRY            */
         BALR  @14,@15                                             0240
*MAINSUB:                                                          0241
*       ;                           /* RTN LABEL FOR INTERNAL PROCS  */
MAINSUB  DS    0H                                                  0242
*       RC=GPR15;                   /* SAVE RETURN CODE              */
         LR    RC,GPR15                                            0242
*     END;                                                         0243
*   ELSE                                                           0244
*     DO;                                                          0244
*                                                                  0244
         B     @RC00237                                            0244
@RF00237 DS    0H                                                  0245
*/********************************************************************/
*/* IF THE RETURN CODE FROM PHR = 4 INDICATING THE KEYWORD WAS NOT   */
*/* FOUND IN THE PARAMETER TABLE, WRITE THE APPROPRIATE ERROR        */
*/* MESSAGE, SET THE RETURN CODE = 0 AND CONTINUE THE DO WHILE LOOP. */
*/********************************************************************/
*                                                                  0245
*       IF RC=4 THEN                                               0245
         CH    RC,@CH00052                                         0245
         BNE   @RF00245                                            0245
*         DO;                       /* KEYWORD NOT FOUND             */
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */
*                                                                  0247
*           DO;                                                    0247
*             DCL                                                  0248
*               1 CMZP0004 STATIC LOCAL,/* PARAMETER LIST PASSED TO
*                                      ERROR FORMAT ROUTINE          */
*                2 CMZL0004 FIXED(8) INIT(4),/* LEVEL PARAMETER    0248
*                                      (CLEV)                        */
*                2 CMZE0004 FIXED(8) INIT(4),/* ERROR PARAMETER    0248
*                                      (CERR)                        */
*                2 CMZA0004 CHAR(1) INIT('1'),/* ACTION PARAMETER  0248
*                                      (CACT)                        */
*                2 CMZS0004 FIXED(8) INIT(0);/* LENGTH OF STRING   0248
*                                      PARAM                         */
*             DCL                                                  0249
*               CMZR0004 REGISTER(1);/* ESTABLISH PARAMeter          */
*             RESPECIFY                                            0250
*               CMZR0004 RSTD;      /* REGISTER FOR                  */
*             CMZR0004=ADDR(CMZP0004);/* ADDRESS OF PARAMETER LIST   */
         LA    CMZR0004,CMZP0004                                   0251
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01275                                        0252
         BALR  @14,@15                                             0252
*             RESPECIFY                                            0253
*               CMZR0004 UNRSTD;                                   0253
*           END;                                                   0254
*           RC=0;                   /* WONT KILL BUILD               */
         SLR   RC,RC                                               0255
*         END;                                                     0256
*       ELSE                                                       0257
*         DO;                                                      0257
*                                                                  0257
         B     @RC00245                                            0257
@RF00245 DS    0H                                                  0258
*/********************************************************************/
*/* IF THE RETURN CODE FROM PHR = 8 OR 12 OR 16 INDICATING AN END OF */
*/* STRING OR SYNTAX ERROR OR FUNCTION ERROR, RESPECTIVELY,          */
*/* THE FOLLOWING ROUTINE IS EXECUTED.                               */
*/********************************************************************/
*                                                                  0258
*           IF RC=8                 /* END OF STRING ?               */
*             THEN                                                 0258
         CH    RC,@CH00036                                         0258
         BNE   @RF00258                                            0258
*             IF RCLCTNGP>0         /* GPOLL?                @D32CKMX*/
*               THEN                                               0259
         ICM   @14,3,RCLCTNGP(ENTRYPTR)                            0259
         BP    @RT00259                                            0259
*               ;                   /* YES, DO NOTHING               */
*             ELSE                                                 0261
*               DO;                 /* NO, ERROR                     */
*/* ERRFMT    LEVEL(PARM)  ERROR(MISS)  ACTION(KILLBLD)  STRING('GPOLL'
*                                                                    */
*                                                                  0262
*                 DO;                                              0262
*                   DCL                                            0263
*                     1 CMZP0005 STATIC LOCAL,/* PARAMETER LIST    0263
*                                      PASSED TO ERROR FORMAT ROUTINE*/
*                      2 CMZL0005 FIXED(8) INIT(4),/* LEVEL PARAMETER
*                                      (CLEV)                        */
*                      2 CMZE0005 FIXED(8) INIT(8),/* ERROR PARAMETER
*                                      (CERR)                        */
*                      2 CMZA0005 CHAR(1) INIT('6'),/* ACTION      0263
*                                      PARAMETER (CACT)              */
*                      2 CMZS0005 FIXED(8) INIT(5),/* LENGTH OF    0263
*                                      STRING PARAM                  */
*                      2 CMZM0005 CHAR(5) INIT('GPOLL');/* PARAMETER
*                                      STRING                        */
*                   DCL                                            0264
*                     CMZR0005 REGISTER(1);/* ESTABLISH PARAMeter    */
*                   RESPECIFY                                      0265
*                     CMZR0005 RSTD;/* REGISTER FOR                  */
*                   CMZR0005=ADDR(CMZP0005);/* ADDRESS OF PARAMETER
*                                      LIST                          */
         LA    CMZR0005,CMZP0005                                   0266
*                   CALL ISTSDCC2;  /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01275                                        0267
         BALR  @14,@15                                             0267
*                   RESPECIFY                                      0268
*                     CMZR0005 UNRSTD;                             0268
*                 END;                                             0269
*                 RC=4;                                            0270
         LA    RC,4                                                0270
*               END;                /* END RTN CODE = 8              */
*           ELSE                                                   0272
*             DO;                   /* NO, THEN SOME TYPE OF ERROR   */
         B     @RC00258                                            0272
@RF00258 DS    0H                                                  0273
*               IF RC=12            /* SYNTAX ERROR ?                */
*                 THEN              /* FUNCTION ERROR                */
         CH    RC,@CH00032                                         0273
         BNE   @RF00273                                            0273
*/* ERRFMT    LEVEL(PARM)  ERROR(SYN)  ACTION(KILLBLD)               */
*                                                                  0274
*                 DO;                                              0274
*                   DCL                                            0275
*                     1 CMZP0006 STATIC LOCAL,/* PARAMETER LIST    0275
*                                      PASSED TO ERROR FORMAT ROUTINE*/
*                      2 CMZL0006 FIXED(8) INIT(4),/* LEVEL PARAMETER
*                                      (CLEV)                        */
*                      2 CMZE0006 FIXED(8) INIT(6),/* ERROR PARAMETER
*                                      (CERR)                        */
*                      2 CMZA0006 CHAR(1) INIT('6'),/* ACTION      0275
*                                      PARAMETER (CACT)              */
*                      2 CMZS0006 FIXED(8) INIT(0);/* LENGTH OF    0275
*                                      STRING PARAM                  */
*                   DCL                                            0276
*                     CMZR0006 REGISTER(1);/* ESTABLISH PARAMeter    */
*                   RESPECIFY                                      0277
*                     CMZR0006 RSTD;/* REGISTER FOR                  */
*                   CMZR0006=ADDR(CMZP0006);/* ADDRESS OF PARAMETER
*                                      LIST                          */
         LA    CMZR0006,CMZP0006                                   0278
*                   CALL ISTSDCC2;  /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01275                                        0279
         BALR  @14,@15                                             0279
*                   RESPECIFY                                      0280
*                     CMZR0006 UNRSTD;                             0280
*                 END;                                             0281
*               ELSE                                               0282
*                                                                  0282
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(KILLBLD)             */
*                                                                  0282
*                 DO;                                              0282
         B     @RC00273                                            0282
@RF00273 DS    0H                                                  0283
*                   DCL                                            0283
*                     1 CMZP0007 STATIC LOCAL,/* PARAMETER LIST    0283
*                                      PASSED TO ERROR FORMAT ROUTINE*/
*                      2 CMZL0007 FIXED(8) INIT(4),/* LEVEL PARAMETER
*                                      (CLEV)                        */
*                      2 CMZE0007 FIXED(8) INIT(4),/* ERROR PARAMETER
*                                      (CERR)                        */
*                      2 CMZA0007 CHAR(1) INIT('6'),/* ACTION      0283
*                                      PARAMETER (CACT)              */
*                      2 CMZS0007 FIXED(8) INIT(0);/* LENGTH OF    0283
*                                      STRING PARAM                  */
*                   DCL                                            0284
*                     CMZR0007 REGISTER(1);/* ESTABLISH PARAMeter    */
*                   RESPECIFY                                      0285
*                     CMZR0007 RSTD;/* REGISTER FOR                  */
*                   CMZR0007=ADDR(CMZP0007);/* ADDRESS OF PARAMETER
*                                      LIST                          */
         LA    CMZR0007,CMZP0007                                   0286
*                   CALL ISTSDCC2;  /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01275                                        0287
         BALR  @14,@15                                             0287
*                   RESPECIFY                                      0288
*                     CMZR0007 UNRSTD;                             0288
*                 END;                                             0289
*               RC=4;               /* RET CODE IS FOUR FOR 12,16, &
*                                      20                            */
@RC00273 LA    RC,4                                                0290
*             END;                  /* END RTN CODE ^= 8             */
*         END;                      /* END RTN CODE ^= 4             */
@RC00258 DS    0H                                                  0293
*     END;                          /* END RNT CODE ^= 0             */
@RC00245 DS    0H                                                  0294
*   GPR1=KWDPTR;                    /* SETUP KEYWORD PTR             */
@RC00237 LR    GPR1,KWDPTR                                         0294
*   MLCKWD=GPR1;                    /* UPDATE MLCA KEYWORD PTR       */
         ST    GPR1,MLCKWD(,MLCAREG)                               0295
*   RFY                                                            0296
*     GPR14 RSTD;                                                  0296
*   GPR14=ADDR(DOLOOP);             /* SETUP RETURN ADDRESS          */
         LA    GPR14,DOLOOP                                        0297
*   RFY                                                            0298
*    (GPR0,                                                        0298
*     GPR1,                                                        0298
*     GPR14,                                                       0298
*     GPR15) UNRSTD;                /* UNRESTRICT REGS               */
*CLSBHSET:                                                         0299
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0299
*                                                                  0299
         B     @PB00003                                            0299
CLSBHSET DS    0H                                                  0300
*/********************************************************************/
*/* THIS ROUTINE WILL PROCESS THE BHSET PARAMETER OF THE CLUSTER     */
*/* MACRO IF 'NONE' OR 'DYNAMIC' HAS BEEN SPECIFIED THE APPROPRIATE  */
*/* FLAG IS SET IN THE RDT. IF A SET NAME WAS SPECIFIED IT IS MOVED  */
*/* TO THE RDT. IF A VALIDITY OR INVALID COMBINATION ERROR OCCURS,   */
*/* ERRFMT IS ISSUED AND THE PARAMETER IS IGNORED.                   */
*/********************************************************************/
*                                                                  0300
*   RFY                                                            0300
*     ISTRTR BASED(MLCPTR3);        /* COVER TERMINAL MODEL          */
*   IF PHRF34SP(1)='NONE    '       /* CHECK FOR 'NONE'              */
*       ×PHRF34SP(1)='        '     /* OR BLANKS                     */
*     THEN                                                         0301
         CLC   PHRF34SP(8,PHR1REG),@CC01301                        0301
         BE    @RT00301                                            0301
         CLC   PHRF34SP(8,PHR1REG),@CC01302                        0301
         BNE   @RF00301                                            0301
@RT00301 DS    0H                                                  0302
*     DO;                           /* IF 'NONE' SPECIFIED           */
*       RTRRDBHN=ON;                /* TURN ON 'NONE' FLG IN RDT     */
*       RTRRDBHD=OFF;               /* INSURE DYNAMIC FLAG OFF       */
         L     @04,MLCPTR3(,MLCAREG)                               0304
         OI    RTRRDBHN(@04),B'10000000'                           0304
         NI    RTRRDBHD(@04),B'10111111'                           0304
*     END;                                                         0305
*   ELSE                                                           0306
*     IF PHRF34SP(1)='DYNAMIC ' THEN                               0306
*                                                                  0306
         B     @RC00301                                            0306
@RF00301 CLC   PHRF34SP(8,PHR1REG),@CC01303                        0306
         BNE   @RF00306                                            0306
*/********************************************************************/
*/* IF DYNAMIC IS SPECIFIED THE SYSCNTRL MACRO MUST HAVE HAD THE     */
*/* MODIFY BLOCK HANDLER SET ASSOCIATION (BHSASSC) SUBPARM LISTED    */
*/* AS AN OPTION ENTRY.                                              */
*/********************************************************************/
*                                                                  0307
*       IF RNCRN->RRNCAMBH=ON       /* BHSASSC SET BY SYSCNTRL       */
*         THEN                                                     0307
         L     @04,RNCRN(,RNCAREG)                                 0307
         TM    RRNCAMBH(@04),B'00001000'                           0307
         BNO   @RF00307                                            0307
*         DO;                                                      0308
*                                                                  0308
*/********************************************************************/
*/* SET 'DYNAMIC' FLG IN MODEL AND TURN OFF 'NONE' FLAG              */
*/********************************************************************/
*                                                                  0309
*           RTRRDBHD=ON;                                           0309
*           RTRRDBHN=OFF;                                          0310
         L     @04,MLCPTR3(,MLCAREG)                               0310
         OI    RTRRDBHD(@04),B'01000000'                           0310
         NI    RTRRDBHN(@04),B'01111111'                           0310
*         END;                                                     0311
*       ELSE                                                       0312
*         DO;                                                      0312
*                                                                  0312
         B     @RC00307                                            0312
@RF00307 DS    0H                                                  0313
*/* ERRFMT    LEVEL(VALUE)  ERROR(COMB)  ACTION(IGNORE)              */
*                                                                  0313
*           DO;                                                    0313
*             DCL                                                  0314
*               1 CMZP0008 STATIC LOCAL,/* PARAMETER LIST PASSED TO
*                                      ERROR FORMAT ROUTINE          */
*                2 CMZL0008 FIXED(8) INIT(5),/* LEVEL PARAMETER    0314
*                                      (CLEV)                        */
*                2 CMZE0008 FIXED(8) INIT(1),/* ERROR PARAMETER    0314
*                                      (CERR)                        */
*                2 CMZA0008 CHAR(1) INIT('1'),/* ACTION PARAMETER  0314
*                                      (CACT)                        */
*                2 CMZS0008 FIXED(8) INIT(0);/* LENGTH OF STRING   0314
*                                      PARAM                         */
*             DCL                                                  0315
*               CMZR0008 REGISTER(1);/* ESTABLISH PARAMeter          */
*             RESPECIFY                                            0316
*               CMZR0008 RSTD;      /* REGISTER FOR                  */
*             CMZR0008=ADDR(CMZP0008);/* ADDRESS OF PARAMETER LIST   */
         LA    CMZR0008,CMZP0008                                   0317
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01275                                        0318
         BALR  @14,@15                                             0318
*             RESPECIFY                                            0319
*               CMZR0008 UNRSTD;                                   0319
*           END;                    /*                               */
*/* TURN OFF 'DYNAMIC' FLG IN RDT AND SET THE 'NONE' ON              */
*/********************************************************************/
*                                                                  0321
*           RTRRDBHD=OFF;                                          0321
