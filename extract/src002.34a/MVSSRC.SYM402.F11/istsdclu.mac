*                   RLNCTETR=RLNCTETR+1;                           0226
         L     @04,RNCLN(,RNCAREG)                                 0226
         AH    @03,RLNCTETR(,@04)                                  0226
         STH   @03,RLNCTETR(,@04)                                  0226
*                 END;              /*                       @D32CKCK*/
*               ELSE                                               0228
*                 ;                 /*                       @D32CKCK*/
@RF00222 DS    0H                                                  0229
*               RCCCTELU=RCCCTELU+1;                               0229
         L     @04,RNCCC(,RNCAREG)                                 0229
         LA    @03,1                                               0229
         AH    @03,RCCCTELU(,@04)                                  0229
         STH   @03,RCCCTELU(,@04)                                  0229
*             END;                                                 0230
*           ELSE                                                   0231
