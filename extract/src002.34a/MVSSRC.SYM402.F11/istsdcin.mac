*                                          /*REGISTER FOR    */    0267
*                       CMZR0004=ADDR(CMZP0004);                   0267
         LA    CMZR0004,CMZP0004                                   0267
*                                          /*ADDRESS OF PARAMETER LIST*
*                       CALL ISTSDCC2;     /*CALL ERROR FORMAT ROUTINE*
         L     @15,@CV01234                                        0268
         BALR  @14,@15                                             0268
*                       RESPECIFY CMZR0004 UNRSTD;                 0269
*                     END;                                         0270
*               ELSE                                               0271
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(KILLBLD)  */       0271
