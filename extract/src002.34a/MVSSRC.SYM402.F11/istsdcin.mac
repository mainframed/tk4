         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*/********************************************************************/
*/*                          START PROCESSING                        */
*/********************************************************************/
*        RNCSAV1(2)=GPR13;              /* BACK PTR                  */
         ST    GPR13,RNCSAV1+4(,RNCAREG)                           0183
*        GPR13=ADDR(RNCSAV1);           /* SAVE REG                  */
         LA    GPR13,RNCSAV1(,RNCAREG)                             0184
*        SAVIN(3)=GPR13;              /* FRWD PTR                  */
         ST    GPR13,SAVIN+8(,GPR13)                               0185
*        DATREG=ADDR(RNCDAT1);          /* DATA REG                  */
         LA    DATREG,RNCDAT1(,RNCAREG)                            0186
*        GENERATE NOREFS NOSETS;                                   0187
          USING @DATD,11                ADDR FOR DATA
*        IF RRNRBREM=ON                 /* REMOTE RN?                */
*         THEN DO;                                                 0188
         L     @10,RNCRN(,RNCAREG)                                 0188
         TM    RRNRBREM(@10),B'00000100'                           0188
         BNO   @RF00188                                            0188
*/********************************************************************/
*/* IN REMOTE 370X NO INNODE ENTRY IS BUILT AND THE PREVIOUS LINE    */
*/* ENTRY IS DELETED IF THIS IS THE FIRST INNODE MACRO.              */
*/********************************************************************/
*          RC=0;                                                   0190
         SLR   RC,RC                                               0190
*          IF RNCGDLLN=ON                 /* LINE DELETED?            *
*           THEN;                                                  0191
         TM    RNCGDLLN(RNCAREG),B'00000001'                       0191
         BO    @RT00191                                            0191
*           ELSE DO;                      /* DELETE IT               */
*            RRNCTELI=RRNCTELI-1;         /* ADJUST LINE COUNTS      */
         L     @10,RNCRN(,RNCAREG)                                 0194
         LH    @09,RRNCTELI(,@10)                                  0194
         BCTR  @09,0                                               0194
         STH   @09,RRNCTELI(,@10)                                  0194
*            RGPCTELN=RGPCTELN-1;         /*  IN RN AND GP ENTRIES   */
         L     @10,RNCGP(,RNCAREG)                                 0195
         LH    @09,RGPCTELN(,@10)                                  0195
         BCTR  @09,0                                               0195
         STH   @09,RGPCTELN(,@10)                                  0195
*            RNCLN=RNCLLN;                /* BACK UP ACTIVE LINE PTR */
         MVC   RNCLN(4,RNCAREG),RNCLLN(RNCAREG)                    0196
*            MLCENT=MLCLENT;              /*  AND ENTRY PTR  @X03KKQJ*/
         MVC   MLCENT(4,MLCAREG),MLCLENT(MLCAREG)                  0197
*            RNCLENT->LENUSED=RNCLENT->LENUSED-LENGTH(ISTRLN);     0198
         L     @10,RNCLENT(,RNCAREG)                               0198
         L     @09,LENUSED(,@10)                                   0198
         SH    @09,@CH01324                                        0198
         ST    @09,LENUSED(,@10)                                   0198
*            RNCGDLLN=ON;                 /* SET LINE DELETED FLAG   */
         OI    RNCGDLLN(RNCAREG),B'00000001'                       0199
*            END;                                                  0200
*          END;                                                    0201
*         ELSE DO;                                                 0202
         B     @RC00188                                            0202
@RF00188 DS    0H                                                  0203
*/********************************************************************/
*/* IN LOCAL 370X AN INNODE ENTRY MUST BE BUILT. SIFTDEF IS CALLED   */
*/* TO SET UP THE ENTRY MODEL. THEN A LOOP IS USED TO PROCESS        */
*/* THE PARMS. FINALLY A CALL TO AREA HANDLER TO MOVE THE MODEL TO   */
*/* THE BUILD AREA.                                                  */
*/********************************************************************/
*          GPR1=RPRENTIN;                                          0203
         LA    GPR1,130                                            0203
*          CALL ISTSDCRS;                  /* SIFTDEF                */
         L     @15,@CV01233                                        0204
         BALR  @14,@15                                             0204
*          RINPTR=GPR1;                    /* SET RIN ENTRY PTR      */
         LR    RINPTR,GPR1                                         0205
*          GPR1=MLCKWD;                                            0206
         L     GPR1,MLCKWD(,MLCAREG)                               0206
*          RC=0;                                                   0207
         SLR   RC,RC                                               0207
*          DO WHILE(RC=0);                 /* LOOP ON PARMS          */
         B     @DE00208                                            0208
@DL00208 DS    0H                                                  0209
*           CALL DOWHILE;                                          0209
         BAL   @14,DOWHILE                                         0209
*           DOLOOP: ;                                              0210
DOLOOP   DS    0H                                                  0211
*           END;                                                   0211
@DE00208 SLR   @10,@10                                             0211
         CR    RC,@10                                              0211
         BE    @DL00208                                            0211
*          IF RC=8                         /* END OF STRING?         */
*           THEN DO;                                               0212
         CH    RC,@CH00046                                         0212
         BNE   @RF00212                                            0212
*            GPR0=0;                                               0214
         LR    GPR0,@10                                            0214
*            GPR1=RINPTR;                                          0215
         LR    GPR1,RINPTR                                         0215
*            CALL ISTSDCC1;                /* AREA HANDLER           */
         L     @15,@CV01232                                        0216
         BALR  @14,@15                                             0216
*            RC=GPR15;                                             0217
         LR    RC,GPR15                                            0217
*            IF RC=0                                               0218
*             THEN DO;                                             0218
         LTR   RC,RC                                               0218
         BNZ   @RF00218                                            0218
*              RNCIN=GPR1;                                         0220
         ST    GPR1,RNCIN(,RNCAREG)                                0220
*              RRNCTEIN=RRNCTEIN+1;        /* RN INNODE COUNT        */
         L     @10,RNCRN(,RNCAREG)                                 0221
         LA    @09,1                                               0221
         LH    @07,RRNCTEIN(,@10)                                  0221
         ALR   @07,@09                                             0221
         STH   @07,RRNCTEIN(,@10)                                  0221
*              RGPCTEIN=RGPCTEIN+1;        /* GP INNODE COUNT        */
         L     @10,RNCGP(,RNCAREG)                                 0222
         LH    @07,RGPCTEIN(,@10)                                  0222
         ALR   @07,@09                                             0222
         STH   @07,RGPCTEIN(,@10)                                  0222
*              RLNCTEIN=RLNCTEIN+1;        /* LN INNODE COUNT        */
         L     @10,RNCLN(,RNCAREG)                                 0223
         AH    @09,RLNCTEIN(,@10)                                  0223
         STH   @09,RLNCTEIN(,@10)                                  0223
*              RNCGINOD=ON;                /* INNODE FOOTPRINT       */
         OI    RNCGINOD(RNCAREG),B'00100000'                       0224
*              END;                                                0225
*             ELSE RC=4;                                           0226
         B     @RC00218                                            0226
@RF00218 LA    RC,4                                                0226
*            END;                                                  0227
*           ELSE;                                                  0228
@RF00212 DS    0H                                                  0229
*          END;                                                    0229
@RC00212 DS    0H                                                  0230
*        GPR13=RNCSAV1(2);                                         0230
@RC00188 L     GPR13,RNCSAV1+4(,RNCAREG)                           0230
*/********************************************************************/
*/*               RC IS 0 FOR GOOD PASS OR 4 FOR ERROR               */
*/********************************************************************/
*        RETURN CODE(RC);                                          0231
         LR    @15,RC                                              0231
         L     @14,12(,@13)                                        0231
         LM    @00,@12,20(@13)                                     0231
         BR    @14                                                 0231
*DOWHILE: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0232
DOWHILE  DS    0H                                                  0233
*        GPR0=ADDR(INNKWTBL);                                      0233
         LA    GPR0,INNKWTBL                                       0233
*        PHR1REG=ADDR(RNCPHR1);                                    0234
         LA    PHR1REG,RNCPHR1(,RNCAREG)                           0234
*        RFY ISTPHROA BASED(PHR1REG);                              0235
*        CALL ISTINCPH;                    /* PARM HANDLER (PHR)     */
         L     @15,@CV01231                                        0236
         BALR  @14,@15                                             0236
*        RC=GPR15;                                                 0237
         LR    RC,GPR15                                            0237
*        KWDPTR=GPR1;                                              0238
         LR    KWDPTR,GPR1                                         0238
*        IF RC=0                                                   0239
*         THEN DO;                                                 0239
         LTR   RC,RC                                               0239
         BNZ   @RF00239                                            0239
*/********************************************************************/
*/* THIS IS A CALL TO THE ROUTINE NEEDED TO PROCESS THE KEYWORD AS   */
*/* DECODED BY PHR. THE ADDR OF THE ROUTINE NEEDED IS RETURNED BY    */
*/* PHR IN REG 0.                                                    */
*/********************************************************************/
*          GPR15=GPR0;                                             0241
         LR    GPR15,GPR0                                          0241
*          CALL PARMHDLR;                                          0242
         BALR  @14,@15                                             0242
*          RC=GPR15;                                               0243
         LR    RC,GPR15                                            0243
*          END;                                                    0244
*         ELSE DO;                                                 0245
         B     @RC00239                                            0245
@RF00239 DS    0H                                                  0246
*          IF RC=4                                                 0246
*           THEN DO;                                               0246
         CH    RC,@CH00130                                         0246
         BNE   @RF00246                                            0246
*/********************************************************************/
*/* RC OF 4 INDICATES KEYWORD NOT FOUND. WRITE ERROR MSG AND THEN    */
*/* CONTINUE TO PROCESS OTHER KEYWORDS.                              */
*/********************************************************************/
*                                                                  0248
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)  */        0248
*             DO;                                                  0248
*               DCL 1 CMZP0003 STATIC LOCAL,                       0249
*                                          /*PARAMETER LIST PASSED TO*/
*                                          /*ERROR FORMAT ROUTINE*/
*                     2 CMZL0003 FIXED (8) INIT (4),               0249
*                                          /*LEVEL PARAMETER (CLEV)*/
*                     2 CMZE0003 FIXED (8) INIT (4),               0249
*                                          /*ERROR PARAMETER (CERR)*/
*                     2 CMZA0003 CHAR (1) INIT ('1'),              0249
*                                          /*ACTION PARAMETER (CACT)*/
*                     2 CMZS0003 FIXED(8) INIT(0);                 0249
*                                          /*LENGTH OF STRING PARAM*/
*               DCL CMZR0003 REGISTER (1); /*ESTABLISH PARAMeter*/ 0250
*               RESPECIFY CMZR0003 RSTD;   /*REGISTER FOR    */    0251
*               CMZR0003=ADDR(CMZP0003);   /*ADDRESS OF PARAMETER LIST*
         LA    CMZR0003,CMZP0003                                   0252
*               CALL ISTSDCC2;             /*CALL ERROR FORMAT ROUTINE*
         L     @15,@CV01234                                        0253
         BALR  @14,@15                                             0253
*               RESPECIFY CMZR0003 UNRSTD;                         0254
*             END;                                                 0255
*            RC=0;                                                 0256
         SLR   RC,RC                                               0256
*            END;                                                  0257
*           ELSE DO;                                               0258
         B     @RC00246                                            0258
@RF00246 DS    0H                                                  0259
*            IF RC=8                                               0259
*             THEN;                                                0259
         CH    RC,@CH00046                                         0259
         BE    @RT00259                                            0259
*/********************************************************************/
*/* RC OF 8 IS END OF STRING. RC OF 12 IS SYSTAX. ANY OTHER INVALID. */
*/********************************************************************/
*             ELSE DO;                                             0261
*              IF RC=12                                            0262
*               THEN                                               0262
         CH    RC,@CH00032                                         0262
         BNE   @RF00262                                            0262
*/* ERRFMT    LEVEL(PARM)  ERROR(SYN)  ACTION(KILLBLD)  */         0263
