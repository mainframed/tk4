*/*                                                                  */
*/* ROUTINE NAME: SRTSUB                                             */
*/*                                                                  */
*/* FUNCTION: CALLED BY RDTLOOP TO ADD A NAME TO THE SRT. ANALYZES   */
*/*           RETURN CODE FROM SRTADD AND TAKES ERROR ACTIONS.       */
*/*           IF A NODE NAME (EXCEPT GROUP AND LINE) IS A DUPLICATE, */
*/*           THE NODE IS IGNORED AND ITS TYPE IS SAVED SO ALL NODES */
*/*           BELOW IT CAN BE IGNORED. (FOR EXAMPLE, IF A CLUSTER IS */
*/*           A DUPLICATE, ALL TERMS, ETC., ARE SKIPPED UNTIL A      */
*/*           GROUP OR A LINE OR ANOTHER CLUSTER IS ENCOUNTERED.     */
*/*           ANY OTHER ERROR RETURN FROM SRTADD, AS WELL AS A       */
*/*           DUPLICATE GROUP OR LINE NAME, IS UNRECOVERABLE.        */
*/*                                                                  */
*/*           NOTE: BHSET NAMES PROCESSED BY BHSETSRT, NOT SRTSUB.   */
*/*                                                                  */
*/* INPUT: REG 0 - ADDRESS OF SYMBOLIC NAME                          */
*/*        REG 2 - ADDRESS OF RDT ENTRY                              */
*/*        ERNODTYP - ENTRY TYPE OF FAILING NODE. IF THIS ENTRY IS   */
*/*                   GREATER, IT WILL NOT BE PROCESSED. OTHERWISE,  */
*/*                   NODE WILL BE PROCESSED AND ERNODTYP SET TO     */
*/*                   INITIAL SETTING OF 255. (ORIGINALLY SET BEFORE */
*/*                   FIRST ENTRY IN RDTLOOP INITIALIZATION.)        */
*/*                                                                  */
*/* OUTPUT: REG 15 - RETURN CODE FROM SRTADD - ZERO/NON-ZERO         */
*/*         TPMSGID - ADDR OF TERMINATING ERROR MESSAGE ID, IF ANY   */
*/*         ERNODTYP - SEE INPUT                                     */
*/*         RPRNOSRT - OFF FOR SUCCESSFUL SRTADD                     */
*/*         RPRINV - ON FOR NODE SKIPPED FOR DUPLICATE NAME          */
*/*                                                                  */
*/* ROUTINES CALLED: SRTADD (ISTOCCSA) - ADD NAMES TO SRT            */
*/*                                                                  */
*/* MACROS USED: TPMSG                                               */
*/*                                                                  */
*/********************************************************************/
*SRTSUB: PROC OPTIONS(NOSAVE,NOSAVEAREA);                       02S0570
SRTSUB   DS    0H                                               02S0571
*DCL SRTSAV PTR;                   /* TO SAVE RETURN ADDRESS         */
*RFY (GPR00P,GPR02P,GPR14P,GPR15F) RSTD;                        02S0572
*RFY ISTRPRE BASED(GPR02P);                                     02S0573
*RFY ISTRCC  BASED(GPR02P);                                /*@DM13842*/
*RFY ISTNCSPL BASED(MLCNCSPL);                             /*@D32CKZA*/
*   SRTSAV=GPR14P;                                              02S0576
         ST    GPR14P,SRTSAV                                    02S0576
*   IF RPRENTRY>ERNODTYP THEN      /* CHECK IF THIS NODE IS BELOW
*                                  AN ERROR NODE.                    */
         CLC   RPRENTRY(1,GPR02P),ERNODTYP                      02S0577
         BNH   @RF00577                                         02S0577
*     DO;                          /* ERROR. SET NODE INVALID, BUT
*                                  GIVE RETURN CODE TO CONTINUE.     */
*       RPRINV=ON;                                              02S0579
         OI    RPRINV(GPR02P),B'01000000'                       02S0579
*       GPR15F=0;                                               02S0580
         SLR   GPR15F,GPR15F                                    02S0580
*     END;                                                      02S0581
*   ELSE                           /* NO BAD UPPER NODE. ADD NAME.   */
*     DO;                                                       02S0582
         B     @RC00577                                         02S0582
@RF00577 DS    0H                                               02S0583
*       IF RPRINV = ON  /*TEST RDTE ALREADY INVALID          @ZA11978*/
*          THEN GPR15F = 0; /*YES INVALID SKIP SRT ADD PROCSS@ZA11978*/
         TM    RPRINV(GPR02P),B'01000000'                       02S0583
         BNO   @RF00583                                         02S0583
         SLR   GPR15F,GPR15F                                    02S0584
*          ELSE DO;  /*RDTE OK CONTINUE PROCESSING SRTADD    @ZA11978*/
         B     @RC00583                                         02S0585
@RF00583 DS    0H                                               02S0586
*RFY RPHPTR RSTD;                                               02S0586
*       IF RPRNAME = '0000000000000000'X                   /*@D32CKQH*/
*         THEN DO;                                         /*D32CKQH*/
         CLC   RPRNAME(8,GPR02P),@CB01070                       02S0587
         BNE   @RF00587                                         02S0587
*           ATCNMCTR = ATCNMCTR + 1;   /*INCREMENT NAME CNTR @D32CKQH*/
         LA    @09,1                                            02S0589
         AH    @09,ATCNMCTR(,ATCVTPTR)                          02S0589
         STH   @09,ATCNMCTR(,ATCVTPTR)                          02S0589
*           RPRNAME(1:2)=ATCNMCTR;     /*AND USE IT FOR NAME @D32CKQH*/
         STH   @09,RPRNAME(,GPR02P)                             02S0590
*         END;                                             /*@D32CKQH*/
*         ELSE;                                            /*@D32CKQH*/
@RF00587 DS    0H                                               02S0593
*       RPHPTR=MLCRPH;                                          02S0593
         L     RPHPTR,MLCRPH(,MLCAPTR)                          02S0593
*       GPR15P=ATCSRTAD;                                        02S0594
         L     GPR15P,ATCSRTAD(,ATCVTPTR)                       02S0594
*       CALL SRTADD;                                            02S0595
         BALR  @14,@15                                          02S0595
*RFY RPHPTR UNRSTD;                                             02S0596
*       IF GPR15F=0 THEN          /* ADD SUCCESSFUL. SET ERROR NODE
*                                  TYPE TO ALLOW ALL NODES. SHOW SRT
*                                  OK FOR THIS ENTRY.                */
         LTR   GPR15F,GPR15F                                    02S0597
         BNZ   @RF00597                                         02S0597
