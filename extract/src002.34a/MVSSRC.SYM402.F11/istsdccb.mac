*/*                                                                  */
*/* ROUTINE NAME: SNTSUB                                             */
*/*                                                                  */
*/* FUNCTION: CALLED BY RDTLOOP TO INITIALIZE AN SNT ENTRY FOR A     */
*/*           PARTICULAR CID. THE DAF IS USED TO INDEX TO THE ENTRY, */
*/*           THE RDT ENTRY ADDRESS IS INSERTED, AND THE LOCKWORD IS */
*/*           CLEARED, EXCEPT THE PRIORITY BIT WHICH IS LEFT ON.     */
*/*           THE DUMMY ENTRY FLAG IS SET FOR AN LUX OR PUX.         */
*/*                                                                  */
*/* INPUT: MNTSNT - ADDRESS OF SNT                                   */
*/*        COMPCIDM - COMPLEMENT OF ATCCIDM                          */
*/*        CIDSNT - CID TO BE USED                                   */
*/*        RDTEPTR - ADDRESS OF RDT ENTRY                            */
*/*                                                                  */
*/* OUTPUT: INITIALIZED SNT ENTRY                                    */
*/*                                                                  */
*/* ROUTINES CALLED: NONE                                            */
*/*                                                                  */
*/* MACROS USED: NONE                                                */
*/*                                                                  */
*/********************************************************************/
*SNTSUB: PROC OPTIONS(NOSAVE,NOSAVEAREA);                       02S0555
SNTSUB   DS    0H                                               02S0556
*DCL SNTEPTR PTR;                                               02S0556
*RFY ISTRPRE BASED(RDTEPTR);                               /*@X50CKZA*/
*RFY GPR14P RSTD;                                               02S0558
*RFY ISTSNT BASED(SNTEPTR);                                     02S0559
*   SNTEPTR=(CIDSNT&COMPCIDM)*LENGTH(SNTENTRY)+MNTSNT;   /* REMOVE
*                                  MAJOR NODEID FROM CID, INDEX INTO
*                                  SNT. HEADER IS COVER BY BASING.   */
         LR    SNTEPTR,CIDSNT                                   02S0560
         N     SNTEPTR,COMPCIDM                                 02S0560
         SLA   SNTEPTR,3                                        02S0560
         L     @15,MNTEPTR                                      02S0560
         AL    SNTEPTR,MNTSNT(,@15)                             02S0560
*   SNTRDTFG = ON;                  /*TURN ON HIGH ORDER BIT @DM12404*/
         OI    SNTRDTFG(SNTEPTR),B'10000000'                    02S0561
*   SNTCBPTR=RDTEPTR;                                      /*@D31JKMQ*/
         STCM  RDTEPTR,7,SNTCBPTR(SNTEPTR)                      02S0562
*   SNTLOCK=LOKPRY1;               /* CLEAR LOCK LEAVING LOKPRY ON   */
         MVC   SNTLOCK(4,SNTEPTR),@CB01665                      02S0563
*IF RPRENTRY = RPRENTPX ×  /* IS THIS AN LUX OR PUX          @X50CKZA*/
*   RPRENTRY = RPRENTLX                                    /*@X50CKZA*/
*  THEN SNTDUMFG = '1'B;  /* YES  SET DUMMY ENTRY FLAG       @X50CKZA*/
         CLI   RPRENTRY(RDTEPTR),X'72'                          02S0564
         BE    @RT00564                                         02S0564
         CLI   RPRENTRY(RDTEPTR),X'10'                          02S0564
         BNE   @RF00564                                         02S0564
@RT00564 DS    0H                                               02S0565
         OI    SNTDUMFG(SNTEPTR),B'00100000'                    02S0565
*  ELSE;                                                        02S0566
@RF00564 DS    0H                                               02S0567
*RFY GPR14P UNRSTD;                                             02S0567
*RFY ISTSNT;                                                    02S0568
*END SNTSUB;                                                    02S0569
