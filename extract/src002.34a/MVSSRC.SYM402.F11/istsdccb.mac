         ST    @02,RCPICEAQ(,RDTEPTR)                           04S0329
*       RCPACETR=ADDR(RCPACETR);                                04S0330
         LA    @02,RCPACETR(,RDTEPTR)                           04S0330
         ST    @02,RCPACETR(,RDTEPTR)                           04S0330
*       IF RPRENTRY=RPRENTLU THEN  /* LU'S NEXT SCHEDUABLE NODE IS   */
         CLI   RPRENTRY(RDTEPTR),X'81'                          04S0331
         BNE   @RF00331                                         04S0331
*         RPRNSNEA=MLCPTR5;        /*   THE CLUSTER CONTROLLER.      */
         MVC   RPRNSNEA(4,RDTEPTR),MLCPTR5(MLCAPTR)             04S0332
*       ELSE                                                    04S0333
*/*********************************************************************
*   PROCESSING FOR TERMS, UTERMS AND COMPONENTS                 04S0333
*++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++*/
*         DO;                                                   04S0333
         B     @RC00331                                         04S0333
@RF00331 DS    0H                                               04S0334
*           IF RTRDIAA^=0 THEN     /* CHANGE THE OFFSETS TO DIAL AND
*                                  POLL CHARACTERS, IF ANY, TO ACTUAL
*                                  ADDRESSES. (NEVER FOR LOCAL TRS.) */
         L     @02,RTRDIAA(,RDTEPTR)                            04S0334
         LTR   @02,@02                                          04S0334
         BZ    @RF00334                                         04S0334
*             RTRDIAA=RTRDIAA+MLCSKEL;                          04S0335
         AL    @02,MLCSKEL(,MLCAPTR)                            04S0335
         ST    @02,RTRDIAA(,RDTEPTR)                            04S0335
*              ELSE;                                       /*@D32CKMX*/
@RF00334 DS    0H                                               04S0337
*           IF RTRPOLA^=0 THEN                                  04S0337
         L     @02,RTRPOLA(,RDTEPTR)                            04S0337
         LTR   @02,@02                                          04S0337
         BZ    @RF00337                                         04S0337
*             RTRPOLA=RTRPOLA+MLCSKEL;                          04S0338
         AL    @02,MLCSKEL(,MLCAPTR)                            04S0338
         ST    @02,RTRPOLA(,RDTEPTR)                            04S0338
*             ELSE;                                        /*@D32CKMX*/
@RF00337 DS    0H                                               04S0340
*/*********************************************************************
*   NEXT SCHEDUABLE NODE ADDRESS PROCESSING - COMP, THEN VARIOUS TERMS
*++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++*/
*           IF RPRENTRY=RPRENTCM THEN                           04S0340
         CLI   RPRENTRY(RDTEPTR),X'90'                          04S0340
         BNE   @RF00340                                         04S0340
*             RPRNSNEA=MLCPTR3;    /* COMP NSNEA IS THE DUMMY TERM.  */
         MVC   RPRNSNEA(4,RDTEPTR),MLCPTR3(MLCAPTR)             04S0341
*           ELSE                                                04S0342
*             DO;                                               04S0342
         B     @RC00340                                         04S0342
@RF00340 DS    0H                                               04S0343
*               MLCPTR3=RDTEPTR;   /* SAVE TERM ADDR FOR COMP NSNEA. */
         ST    RDTEPTR,MLCPTR3(,MLCAPTR)                        04S0343
*               IF DEVTCODE >= DEV2980 /* FOR CLUS TERMS, NSNEA IS THE
*                                  CLUSTER. DEVICE TYPE CODES FOR CLUS
*                                  TERMS ARE GROUPED TOGETHER, I.E.,
*                                  2980, 3277, 3284, 3286, 3275.     */
*                & DEVTCODE <= DEV3275 THEN                     04S0344
         LA    @02,44                                           04S0344
         ALR   @02,RDTEPTR                                      04S0344
         CLI   DEVTCODE(@02),X'18'                              04S0344
         BL    @RF00344                                         04S0344
         CLI   DEVTCODE(@02),X'1C'                              04S0344
         BH    @RF00344                                         04S0344
*                 RPRNSNEA=MLCPTR2;                             04S0345
         MVC   RPRNSNEA(4,RDTEPTR),MLCPTR2(MLCAPTR)             04S0345
*               ELSE                                            04S0346
*                 IF RPRNSNEA^=0 THEN  /* DIAL-OUT TERM HAS OFFSET TO A
*                                  DIALSET IN NSNEA. CHANGE IT TO A
*                                  POINTER. POINT THE DIALSET'S NEXT
*                                  DOT SLOT TO THE TERM AND BUMP THE
*                                  COUNT OF TERM ADRS IN THE DOT.    */
         B     @RC00344                                         04S0346
@RF00344 L     @02,RPRNSNEA(,RDTEPTR)                           04S0346
         LTR   @02,@02                                          04S0346
         BZ    @RF00346                                         04S0346
*                   DO;                                         04S0347
*                     RPRNSNEA=RPRNSNEA+MLCSKEL;                04S0348
         AL    @02,MLCSKEL(,MLCAPTR)                            04S0348
         ST    @02,RPRNSNEA(,RDTEPTR)                           04S0348
*RFY ISTRDS BASED(RPRNSNEA);                                    04S0349
*                       EVAL(RDSADDOT+RDSCTTRM*LENGTH(ISTDOT)) ->
*                     DOTADTRM=RDTEPTR;                         04S0350
         LH    @15,RDSCTTRM(,@02)                               04S0350
         LR    @14,@15                                          04S0350
         SLA   @14,2                                            04S0350
         AL    @14,RDSADDOT(,@02)                               04S0350
         ST    RDTEPTR,DOTADTRM(,@14)                           04S0350
*                     RDSCTTRM=RDSCTTRM+1;                      04S0351
         AH    @15,@CH00104                                     04S0351
         STH   @15,RDSCTTRM(,@02)                               04S0351
*RFY ISTRDS BASED(RDTEPTR);                                     04S0352
*                   END;                                        04S0353
*                 ELSE             /* FOR DIAL-IN TERM, NSNEA IS THE
*                                  GROUP. FOR NON-DIAL, THE LINE.    */
*                   IF MLCPTR0 -> RGPADIL=ON THEN               04S0354
         B     @RC00346                                         04S0354
@RF00346 L     @02,MLCPTR0(,MLCAPTR)                            04S0354
         TM    RGPADIL(@02),B'10000000'                         04S0354
         BNO   @RF00354                                         04S0354
*                     RPRNSNEA=MLCPTR0;                         04S0355
         ST    @02,RPRNSNEA(,RDTEPTR)                           04S0355
*                   ELSE RPRNSNEA=MLCPTR1;                      04S0356
         B     @RC00354                                         04S0356
@RF00354 MVC   RPRNSNEA(4,RDTEPTR),MLCPTR1(MLCAPTR)             04S0356
*             END;                                              04S0357
*         END;                                                  04S0358
*     END;                                                      04S0359
*/********************************************************************/
*/*          END OF TERMS SUBSEGMENT                                 */
*/********************************************************************/
*         END;                                                  03S0360
*       ELSE                                                    03S0361
*         IF RPRENTRY>=RPRENTLN    /* LINE,APPL,CLUS,CC,DAN,PU, OR PUX
*                                                 PROCESSED @D32CKMX */
*            THEN                  /*   IN APLCLSLN SUBSEGMENT       */
         B     @RC00310                                         03S0361
@RF00310 CLI   RPRENTRY(RDTEPTR),X'50'                          03S0361
         BL    @RF00361                                         03S0361
*            DO;                   /*   INCLUDING PORT SUBSEGMENT    */
*/********************************************************************/
*/*                                                                  */
*/* SUBSEGMENT NAME: APLCLSLN                                        */
*/*                                                                  */
*/* FUNCTION: PROCESS APPLICATION, CLUSTER, CLUSTER CONTROLLER AND   */
*/*           LINE RDT ENTRIES ENCOUNTERED BY SEGMENT RDTLOOP.       */
*/*                                                                  */
*/* SUBSEGMENT USED: PORTS - DOES PORT-UNIQUE PROCESSING FOR LINES   */
*/*                  THAT ARE PORTS.                                 */
*/*                                                                  */
*/*           FOR ALL ADDITIONAL INFORMATION, SEE HEADER OF RDTLOOP. */
*/*                                                                  */
*/********************************************************************/
*   IF RPRENTRY=RPRENTAP THEN                                   04S0363
         CLI   RPRENTRY(RDTEPTR),X'55'                          04S0363
         BNE   @RF00363                                         04S0363
*     DO;                          /* FOR APPLS, NEXT SCHEDUABLE NODE
*                                  IS THE SKELETON, SIM. LOGON CHAINS
*                                  POINT TO THE ENTRY, AND APPL ACE
*                                  AND ICE QUEUES ARE WRAPPED.       */
*       RPRNSNEA=MLCSKEL;                                       04S0365
         MVC   RPRNSNEA(4,RDTEPTR),MLCSKEL(MLCAPTR)             04S0365
*       RPRLGONF=RDTEPTR;                                       04S0366
         ST    RDTEPTR,RPRLGONF(,RDTEPTR)                       04S0366
*       RPRLGONB=RDTEPTR;                                       04S0367
         ST    RDTEPTR,RPRLGONB(,RDTEPTR)                       04S0367
*       RAPICELG=ADDR(RAPICELG);                                04S0368
         LA    @02,RAPICELG(,RDTEPTR)                           04S0368
         ST    @02,RAPICELG(,RDTEPTR)                           04S0368
*       RAPICEAC=ADDR(RAPICEAC);                                04S0369
         LA    @02,RAPICEAC(,RDTEPTR)                           04S0369
         ST    @02,RAPICEAC(,RDTEPTR)                           04S0369
*       RAPICEAQ=ADDR(RAPICEAQ);                                04S0370
         LA    @02,RAPICEAQ(,RDTEPTR)                           04S0370
         ST    @02,RAPICEAQ(,RDTEPTR)                           04S0370
*       RAPACEAP=ADDR(RAPACEAP);                                04S0371
         LA    @02,RAPACEAP(,RDTEPTR)                           04S0371
         ST    @02,RAPACEAP(,RDTEPTR)                           04S0371
*     END;                                                      04S0372
*   ELSE                                                        04S0373
*     DO;                                                       04S0373
         B     @RC00363                                         04S0373
@RF00363 DS    0H                                               04S0374
*       CIDSNT=RPRCOMID;           /* LN, CL, CC: INITIALIZE SNT.    */
         L     CIDSNT,RPRCOMID(,RDTEPTR)                        04S0374
*       CALL SNTSUB;                                            04S0375
         BAL   @14,SNTSUB                                       04S0375
*       IF RPRENTRY=RPRENTLN THEN                               04S0376
         CLI   RPRENTRY(RDTEPTR),X'50'                          04S0376
         BNE   @RF00376                                         04S0376
*         DO;                                                   04S0377
*           MLCPTR1=RDTEPTR;       /* SAVE LN ADDR FOR LOWER NODES.  */
         ST    RDTEPTR,MLCPTR1(,MLCAPTR)                        04S0378
*           MLCPTR2=RDTEPTR;                                    04S0379
         ST    RDTEPTR,MLCPTR2(,MLCAPTR)                        04S0379
*           IF RLNRACAN=ON                                      04S0380
*            & RPRNSNEA^=0 THEN    /* FOR CALL-OUT LINE WITH NSNEA A
*                                  DIALSET OFFSET, CHANGE TO ADDRESS.
*                                  FOR OTHER LN, NSNEA IS THE GROUP. */
         TM    RLNRACAN(RDTEPTR),B'01000000'                    04S0380
         BNO   @RF00380                                         04S0380
         L     @02,RPRNSNEA(,RDTEPTR)                           04S0380
         LTR   @02,@02                                          04S0380
         BZ    @RF00380                                         04S0380
*             RPRNSNEA=RPRNSNEA+MLCSKEL;                        04S0381
         AL    @02,MLCSKEL(,MLCAPTR)                            04S0381
         ST    @02,RPRNSNEA(,RDTEPTR)                           04S0381
*           ELSE RPRNSNEA=MLCPTR0;                              04S0382
         B     @RC00380                                         04S0382
@RF00380 MVC   RPRNSNEA(4,RDTEPTR),MLCPTR0(MLCAPTR)             04S0382
*           IF RLNPORT=OFF THEN;                                04S0383
@RC00380 TM    RLNPORT(RDTEPTR),B'00010000'                     04S0383
         BZ    @RT00383                                         04S0383
*           ELSE                                                04S0385
*             DO;                  /* DO DIAL PROCESSING FOR A PORT. */
*/********************************************************************/
*/*                                                                  */
*/* SUBSEGMENT NAME: PORTS                                           */
*/*                                                                  */
*/* FUNCTION: INCLUDED IN SUBSEGMENT APLCLSLN TO:                    */
*/*           (1) CHANGE OFFSETS TO POINTERS FOR THE POINTER TO THE  */
*/*               UTERM ENTRY, THE DITT/DIVT POINTER AND THE TERM    */
*/*               ENTRY POINTERS IN THE DITTS AND DIVTS.             */
*/*           (2) INITIALIZE THE SNT ENTRY FOR THE CTERM             */
*/*           (3) ADD THE CTERM NAME TO THE SRT.                     */
*/*                                                                  */
*/*           FOR ALL ADDITIONAL INFORMATION, SEE HEADER OF RDTLOOP. */
*/*                                                                  */
*/********************************************************************/
*RFY (ISTDTE,ISTDIVTH,ISTDVE) BASED(DITTDIVT);                  05S0386
*   IF RPTDITT=0 THEN;             /* NOTE: RPTDIVT IS RPTDITT.      */
         ICM   @02,15,RPTDITT(RDTEPTR)                          05S0387
         BZ    @RT00387                                         05S0387
*   ELSE                                                        05S0389
*     DO;                                                       05S0389
*       DITTDIVT=RPTDITT+MLCSKEL;  /* CONV DITT/DIVT OFFSET TO PTR.  */
         L     DITTDIVT,RPTDITT(,RDTEPTR)                       05S0390
         AL    DITTDIVT,MLCSKEL(,MLCAPTR)                       05S0390
*       RPTDITT=DITTDIVT;                                       05S0391
         ST    DITTDIVT,RPTDITT(,RDTEPTR)                       05S0391
*       IF RLNTTV=ON THEN                                       05S0392
         TM    RLNTTV(RDTEPTR),B'00001000'                      05S0392
         BNO   @RF00392                                         05S0392
*/*********************************************************************
*   FOR TERM TYPE VERIFICATION PORT, CHANGE RDT ENTRY ADDRESSES IN
*   THE DITT FROM POINTERS TO OFFSETS.                          05S0393
*++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++*/
*         DO DITTDIVT=DITTDIVT BY LENGTH(ISTDTE) WHILE DTELCTI^=DTEND;
         B     @DE00393                                         05S0393
@DL00393 DS    0H                                               05S0394
*           IF DTERDTEA^=0 THEN                                 05S0394
         L     @15,DTERDTEA(,DITTDIVT)                          05S0394
         LTR   @15,@15                                          05S0394
         BZ    @RF00394                                         05S0394
*             DTERDTEA=DTERDTEA+MLCSKEL;                        05S0395
         AL    @15,MLCSKEL(,MLCAPTR)                            05S0395
         ST    @15,DTERDTEA(,DITTDIVT)                          05S0395
*         END;                                                  05S0396
@RF00394 AH    DITTDIVT,@CH00873                                05S0396
@DE00393 CLI   DTELCTI(DITTDIVT),X'FF'                          05S0396
         BNE   @DL00393                                         05S0396
*       ELSE                                                    05S0397
*         DO;                                                   05S0397
         B     @RC00392                                         05S0397
@RF00392 DS    0H                                               05S0398
*/*********************************************************************
*   NOT A TTV, BUT DITT/DIVT EXISTS, SO MUST BE ID VERIFIED PORT.
*   THE DIVT OFFSETS ARE TO BE CHANGED TO POINTERS IF THIS HAS NOT
*   ALREADY BEEN DONE DURING PROCESSING OF ANOTHER PORT WHICH   05S0398
*   SHARES THESE DIVTS.                                         05S0398
*++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++*/
*           CTDIVT=DIVNUME;        /* COUNT OF ENTRIES.              */
         LH    CTDIVT,DIVNUME(,DITTDIVT)                        05S0398
*           LENDIVT=DIVIDLEN+(ADDR(DVEBSCAR)-ADDR(ISTDVE));   /*
*                                  ENTRY LENGTH IS MAXIMUM LENGTH OF
*                                  ID + REST OF ENTRY.               */
         LA    LENDIVT,DVEBSCAR(,DITTDIVT)                      05S0399
         SLR   LENDIVT,DITTDIVT                                 05S0399
         AH    LENDIVT,DIVIDLEN(,DITTDIVT)                      05S0399
*           DITTDIVT=DITTDIVT+LENGTH(ISTDIVTH);   /* FIRST ENTRY.    */
         AH    DITTDIVT,@CH00102                                05S0400
*           IF DVEACIND=OFF THEN;                          /*@DL31HCK*/
         TM    DVEACIND(DITTDIVT),B'10000000'                   05S0401
         BZ    @RT00401                                         05S0401
*           ELSE                                                05S0403
*             DO;                  /* CONVERSIONS NOT DONE. DO THEM. */
*               DVEACIND = OFF;           /*NOW TURN IT OFF  @DM12193*/
         NI    DVEACIND(DITTDIVT),B'01111111'                   05S0404
*               DO CTDIVT=CTDIVT BY -1 TO 1;                    05S0405
         B     @DE00405                                         05S0405
@DL00405 DS    0H                                               05S0406
*                 DVERDTEA=DVERDTEA+MLCSKEL;                    05S0406
         L     @00,DVERDTEA(,DITTDIVT)                          05S0406
         AL    @00,MLCSKEL(,MLCAPTR)                            05S0406
         ST    @00,DVERDTEA(,DITTDIVT)                          05S0406
*                 DITTDIVT=DITTDIVT+LENDIVT;                    05S0407
         ALR   DITTDIVT,LENDIVT                                 05S0407
*               END;                                            05S0408
         BCTR  CTDIVT,0                                         05S0408
@DE00405 LTR   CTDIVT,CTDIVT                                    05S0408
         BP    @DL00405                                         05S0408
*             END;                                              05S0409
*         END;                                                  05S0410
@RT00401 DS    0H                                               05S0411
*     END;                                                      05S0411
@RC00392 DS    0H                                               05S0412
*RFY (ISTDTE,ISTDIVTH,ISTDVE);                                  05S0412
@RT00387 DS    0H                                               05S0413
*   IF RPTUTERM^=0 THEN            /* CONVERT UTERM POINTER.         */
         L     @02,RPTUTERM(,RDTEPTR)                           05S0413
         LTR   @02,@02                                          05S0413
         BZ    @RF00413                                         05S0413
*     RPTUTERM=RPTUTERM+MLCSKEL;                                05S0414
         AL    @02,MLCSKEL(,MLCAPTR)                            05S0414
         ST    @02,RPTUTERM(,RDTEPTR)                           05S0414
*     ELSE;                                                /*@D32CKMX*/
@RF00413 DS    0H                                               05S0416
*/*********************************************************************
*   CTERM PROCESSING: INITIALIZE THE SNT ENTRY FOR THE CTERM. ADD THE
*   CTERM NAME TO THE SRT. THIS IS THE LAST PROCESSING THIS TIME
*   THROUGH RDTLOOP, SO IF SRTSUB FAILS, IT WILL BE CHECKED AT THE
*   TOP OF RDTLOOP IMMEDIATELY.                                 05S0416
*++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++*/
*   CIDSNT=RPTLNCID;               /* CTERM CID                      */
         L     CIDSNT,RPTLNCID(,RDTEPTR)                        05S0416
*   CALL SNTSUB;                                                05S0417
         BAL   @14,SNTSUB                                       05S0417
*   SRTSEC=ON; /* 2ND SRTADD FOR THIS ENTRY                  @ZA30444*/
         OI    SRTSEC,B'01000000'                               05S0418
*RFY (GPR00P,GPR02P) RSTD;                                      05S0419
*   GPR00P=ADDR(RPTCTNM);          /* CTERM NAME                     */
         LA    GPR00P,RPTCTNM(,RDTEPTR)                         05S0420
*   GPR02P=RDTEPTR;                /* RDT ENTRY                      */
         LR    GPR02P,RDTEPTR                                   05S0421
*   CALL SRTSUB;                                                05S0422
         BAL   @14,SRTSUB                                       05S0422
*RFY (GPR00P,GPR02P) UNRSTD;                                    05S0423
*SRTSEC=OFF; /* RESET 2ND SRTADD FOR THIS ENTRY              @ZA30444*/
         NI    SRTSEC,B'10111111'                               05S0424
*/********************************************************************/
*/*          END OF PORTS SUBSEGMENT                                 */
*/********************************************************************/
*             END;                                              04S0425
*         END;                                                  04S0426
*         ELSE IF RPRENTRY = RPRENTDA       /*IS ENTRY A DAN @D32CKMX*/
*              THEN DO;  /*YES. PROCESS PROINTERS ETC.       @D32CKMX*/
         B     @RC00376                                         04S0427
@RF00376 CLI   RPRENTRY(RDTEPTR),X'51'                          04S0427
         BNE   @RF00427                                         04S0427
*                 RPRNSNEA = MLCSKEL;  /*SET NSNEA TO ADDR OF HDR
*                                                            @D32CKMX*/
         MVC   RPRNSNEA(4,RDTEPTR),MLCSKEL(MLCAPTR)             04S0429
*                 MLCPTR1 = RDTEPTR;  /*SET UP SAVE ADDR OF DAN ENTRY
*                                                            @D32CKMX*/
         ST    RDTEPTR,MLCPTR1(,MLCAPTR)                        04S0430
*                   END;                                   /*@D32CKMX*/
*              ELSE DO;                                    /*@D32CKMX*/
         B     @RC00427                                         04S0432
@RF00427 DS    0H                                               04S0433
*                 IF RPRENTRY = RPRENTCC &                      04S0433
*                    RPRHDTYP = RPRENTSW           /*IS ENTRY A PU IN
*                                               SWITCHED SEG @D32CKMX*/
*                    THEN DO;            /*YES. SET PTRS ETC @D32CKMX*/
         CLI   RPRENTRY(RDTEPTR),X'71'                          04S0433
         BNE   @RF00433                                         04S0433
         CLI   RPRHDTYP(RDTEPTR),X'04'                          04S0433
         BNE   @RF00433                                         04S0433
*                       RPRNSNEA = MLCSKEL;   /*SET UP NSNEA TO ADDR
*                                                    RDT HDR @D32CKMX*/
         L     @02,MLCSKEL(,MLCAPTR)                            04S0435
         ST    @02,RPRNSNEA(,RDTEPTR)                           04S0435
*                       IF RCCPATHT ^= 0    /*CHECK FOR PATH TABLE
*                                                            @D32CKMX*/
*                          THEN RCCPATHT = RCCPATHT + MLCSKEL; /*SET
*                                           PATH TABLE  ADDR @D32CKMX*/
         L     @15,RCCPATHT(,RDTEPTR)                           04S0436
         LTR   @15,@15                                          04S0436
         BZ    @RF00436                                         04S0436
         ALR   @15,@02                                          04S0437
         ST    @15,RCCPATHT(,RDTEPTR)                           04S0437
*                            ELSE;          /*NO. DO NOTHING @D32CKMX*/
@RF00436 DS    0H                                               04S0439
*                        SRTSEC=ON; /* 2ND SRTADD FOR THIS ENTRY
*                                                            @ZA30444*/
         OI    SRTSEC,B'01000000'                               04S0439
*                         RFY (GPR00P,GPR02P,GPR15F) RSTD; /*@D32CKMX*/
*                         GPR02P = RDTEPTR; /*POINT TO RDTE  @D32CKMX*/
         LR    GPR02P,RDTEPTR                                   04S0441
*                         GPR00P = ADDR(RCCSTAID); /*POINT TO ENTRY
*                                                       NAME @D32CKMX*/
         LA    GPR00P,RCCSTAID(,RDTEPTR)                        04S0442
*                         CALL SRTSUB;          /*GO ADD SRT @D32CKMX*/
         BAL   @14,SRTSUB                                       04S0443
*                         RFY (GPR00P,GPR02P,GPR15F) UNRSTD;     /*
*                         SRTSEC=OFF; /*RESET 2ND SRTADD FOR    04S0444
*                                       THIS ENTRY           @ZA30444*/
*                           END;                           /*@D32CKMX*/
*                        ELSE                              /*@D32CKMX*/
*                           RPRNSNEA = MLCPTR1; /*SET NSNEA TO POINT TO
*                                                  LINE/DAN  @D32CKMX*/
         B     @RC00433                                         04S0446
@RF00433 MVC   RPRNSNEA(4,RDTEPTR),MLCPTR1(MLCAPTR)             04S0446
*                           IF RPRENTRY = RPRENTPX  /*IS ENTRY PUX?
*                                                            @D32CKMX*/
*                              THEN;  /*YES. BUT NO MORE PROCESSING
*                                                            @D32CKMX*/
@RC00433 CLI   RPRENTRY(RDTEPTR),X'72'                          04S0447
         BE    @RT00447                                         04S0447
*                              ELSE IF RPRENTRY = RPRENTCC /*IS ENTRY
*                                                         PU @D32CKMX*/
*                                      THEN MLCPTR5=RDTEPTR; /*SAVE
*                                              ADDR OF ENTRY @D32CKMX*/
         CLI   RPRENTRY(RDTEPTR),X'71'                          04S0449
         BNE   @RF00449                                         04S0449
         ST    RDTEPTR,MLCPTR5(,MLCAPTR)                        04S0450
*                                      ELSE DO;  /*NO. SET PTR FOR
*                                         CLUSTER AND GPOLL  @D32CKMX*/
         B     @RC00449                                         04S0451
@RF00449 DS    0H                                               04S0452
*                                         MLCPTR2 = RDTEPTR;  /*
*                                                            @D32CKMX*/
         ST    RDTEPTR,MLCPTR2(,MLCAPTR)                        04S0452
*                                         IF RCLGPOLA = 0 /*GPOLL
*                                                 SPECIFIED? @D32CKMX*/
*                                            THEN;  /*NO. DO NOTHING
*                                                            @D32CKMX*/
         ICM   @02,15,RCLGPOLA(RDTEPTR)                         04S0453
         BZ    @RT00453                                         04S0453
*                      ELSE RCLGPOLA = RCLGPOLA + MLCSKEL; /*@D32CKMX*/
         L     @02,RCLGPOLA(,RDTEPTR)                           04S0455
         AL    @02,MLCSKEL(,MLCAPTR)                            04S0455
         ST    @02,RCLGPOLA(,RDTEPTR)                           04S0455
*                                             END;         /*@D32CKMX*/
*                          END;                            /*@D32CKMX*/
*        END;                                              /*@D32CKMX*/
*/********************************************************************/
*/*          END OF APLCLSLN SUBSEGMENT                              */
