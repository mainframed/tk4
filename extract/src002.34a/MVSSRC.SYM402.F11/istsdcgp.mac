*/********************************************************************/
*/*                                                                  */
*/*              BHSET PARAMETER SUBROUTINE                          */
*/*                                                                  */
*/* THIS ROUTINE WILL PROCESS THE BHSET PARAMETER OF THE GROUP MACRO.*/
*/* IF 'NONE' OR 'DYNAMIC' HAS BEEN SPECIFIED THE APPROPRIATE FLAG   */
*/* IS SET IN THE RDT. IF A SET NAME WAS SPECIFIED IT IS MOVED TO    */
*/* THE RDT. IF A VALIDITY OR INVALID COMBINATION ERROR OCCURS,      */
*/* ERRFMT IS ISSUED AND THE PARAMETER IS IGNORED.                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0371
*GRPBHSET:                                                         0371
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0371
GRPBHSET DS    0H                                                  0372
*   IF PHRF34SP(1)='NONE    '×PHRF34SP(1)=BLANK8 THEN/* IF 'NONE'  0372
*                                      SPECIFIED                     */
         CLC   PHRF34SP(8,PHRREG),@CC01352                         0372
         BE    @RT00372                                            0372
         CLC   PHRF34SP(8,PHRREG),@CC01232                         0372
         BNE   @RF00372                                            0372
@RT00372 DS    0H                                                  0373
*     RTRRDBHN=ON;                  /* TURN ON 'NONE' FLG IN RDT     */
         L     @09,MLCPTR3(,MLCAREG)                               0373
         OI    RTRRDBHN(@09),B'10000000'                           0373
*   ELSE                                                           0374
*     IF PHRF34SP(1)='DYNAMIC ' THEN                               0374
*                                                                  0374
         B     @RC00372                                            0374
@RF00372 CLC   PHRF34SP(8,PHRREG),@CC01353                         0374
         BNE   @RF00374                                            0374
*/********************************************************************/
*/* IF DYNAMIC IS SPECIFIED THE SYSCNTRL MACRO MUST HAVE HAD THE     */
*/* MODIFY BLOCK HANDLER SET ASSOCIATION (BHSASSC) SUBPARM LISTED    */
*/* AS AN OPTION ENTRY.                                              */
*/********************************************************************/
*                                                                  0375
*       IF RRNCAMBH='1'B            /* IF BHSASSC SET BY SYSCNTRL    */
*         THEN                                                     0375
         L     @09,RNCRN(,RNCAREG)                                 0375
         TM    RRNCAMBH(@09),B'00001000'                           0375
         BNO   @RF00375                                            0375
*         DO;                                                      0376
*           RTRRDBHD=ON;            /* SET 'DYNAMIC' FLG IN RDT      */
*           RTRRDBHN=OFF;           /* AND TURN OFF 'NONE' FLAG      */
         L     @09,MLCPTR3(,MLCAREG)                               0378
         OI    RTRRDBHD(@09),B'01000000'                           0378
         NI    RTRRDBHN(@09),B'01111111'                           0378
*         END;                                                     0379
*       ELSE                                                       0380
*         DO;                                                      0380
*                                                                  0380
         B     @RC00375                                            0380
@RF00375 DS    0H                                                  0381
*/* ERRFMT    LEVEL(VALUE)  ERROR(COMB)  ACTION(IGNORE)              */
*                                                                  0381
*           DO;                                                    0381
*             DCL                                                  0382
*               1 CMZP0007 STATIC LOCAL,/* PARAMETER LIST PASSED TO
*                                      ERROR FORMAT ROUTINE          */
*                2 CMZL0007 FIXED(8) INIT(5),/* LEVEL PARAMETER    0382
*                                      (CLEV)                        */
*                2 CMZE0007 FIXED(8) INIT(1),/* ERROR PARAMETER    0382
*                                      (CERR)                        */
*                2 CMZA0007 CHAR(1) INIT('1'),/* ACTION PARAMETER  0382
*                                      (CACT)                        */
*                2 CMZS0007 FIXED(8) INIT(0);/* LENGTH OF STRING   0382
*                                      PARAM                         */
*             DCL                                                  0383
*               CMZR0007 REGISTER(1);/* ESTABLISH PARAMeter          */
*             RESPECIFY                                            0384
*               CMZR0007 RSTD;      /* REGISTER FOR                  */
*             CMZR0007=ADDR(CMZP0007);/* ADDRESS OF PARAMETER LIST   */
         LA    CMZR0007,CMZP0007                                   0385
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */
         L     @15,@CV01244                                        0386
         BALR  @14,@15                                             0386
*             RESPECIFY                                            0387
*               CMZR0007 UNRSTD;                                   0387
*           END;                                                   0388
*           RTRRDBHD=OFF;           /* TURN OFF 'DYNAMIC' FLG IN     */
*           RTRRDBHN=ON;            /* RDT AND SET THE 'NONE' ON     */
         L     @09,MLCPTR3(,MLCAREG)                               0390
         OI    RTRRDBHN(@09),B'10000000'                           0390
         NI    RTRRDBHD(@09),B'10111111'                           0390
*         END;                                                     0391
*     ELSE                                                         0392
*       DO;                                                        0392
         B     @RC00374                                            0392
@RF00374 DS    0H                                                  0393
*         RTRBHSN1=PHRF34SP(1);     /* MOVE SET NAME TO              */
         L     @09,MLCPTR3(,MLCAREG)                               0393
         MVC   RTRBHSN1(8,@09),PHRF34SP(PHRREG)                    0393
*         RTRRDBHN=OFF;             /* RDT AND TURN OFF 'NONE' FLG   */
         NI    RTRRDBHN(@09),B'01111111'                           0394
*       END;                                                       0395
*   GPR15=0;                                                       0396
@RC00374 DS    0H                                                  0396
@RC00372 SLR   GPR15,GPR15                                         0396
*   GPR14=ADDR(MAINSUB);                                           0397
         LA    GPR14,MAINSUB                                       0397
*   END GRPBHSET;                                                  0398
@EL00007 DS    0H                                                  0398
@EF00007 DS    0H                                                  0398
@ER00007 BR    @14                                                 0398
*                                                                  0399
*/********************************************************************/
*/*                                                                  */
*/*            XMITLIM PARAMETER SUBROUTINE                          */
*/*                                                                  */
*/* THIS ROUTINE WILL MOVE IF SPECIFIED, THE NUMBER OF TRANSMISSION  */
*/* THE NCP WILL RECEIVE FROM OR SEND TO THIS STATION OR BOTH. IF NO */
*/* NUMBER IS SPECIFIED A CHECK IS MADE TO SEE IF 'NO' WAS THE PARM. */
*/* IF SO THER IS NO LIMIT TRANSMISSIONS. IF NEITHER A COUNT OR      */
*/* 'NO' WERE GIVEN, AN ERROR MSG IS PRINTED AND THE DEFAULT OF 'NO' */
*/* IS TAKEN. IF A COUNT IS SPECIFIED IT MUST BE >0<256.             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0399
*GRPXMTLM:                                                         0399
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0399
GRPXMTLM DS    0H                                                  0400
*   IF PHRF34SP(1)='000000NO'×PHRF34SL(1)=0 THEN                   0400
         CLC   PHRF34SP(8,PHRREG),@CC01363                         0400
         BE    @RT00400                                            0400
         ICM   @09,3,PHRF34SL(PHRREG)                              0400
         BNZ   @RF00400                                            0400
@RT00400 DS    0H                                                  0401
*     RTRTRMAX=0;                   /* IF 'NO' SET XMISSION CNT=0    */
         L     @09,MLCPTR3(,MLCAREG)                               0401
         MVI   RTRTRMAX(@09),X'00'                                 0401
*   ELSE                                                           0402
*     DO;                           /* A NUM WAS SPECIFIED           */
         B     @RC00400                                            0402
@RF00400 DS    0H                                                  0403
*       PACK(PHRF34SP(1),PHRF34SP(1));/* CONVERT IT TO BINARY        */
         PACK  PHRF34SP(8,PHRREG),PHRF34SP(8,PHRREG)               0403
*       CVB(CVBNUM,PHRF34SP(1));    /* AND CK FOR VALID              */
         CVB   @09,PHRF34SP(,PHRREG)                               0404
         ST    @09,CVBNUM                                          0404
*       RTRTRMAX=CVBNUM2;           /* IF OK - STORE IT IN           */
         L     @09,MLCPTR3(,MLCAREG)                               0405
         MVC   RTRTRMAX(1,@09),CVBNUM2                             0405
*     END;                                                         0406
*   GPR15=0;                        /* SET RTN CODE TO 0             */
@RC00400 SLR   GPR15,GPR15                                         0407
*   GPR14=ADDR(MAINSUB);                                           0408
         LA    GPR14,MAINSUB                                       0408
*   END GRPXMTLM;                                                  0409
@EL00008 DS    0H                                                  0409
@EF00008 DS    0H                                                  0409
@ER00008 BR    @14                                                 0409
*                                                                  0410
