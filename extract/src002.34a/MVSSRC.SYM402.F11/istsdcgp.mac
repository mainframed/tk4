*/*                                                                  */
*/*            ITBMODE PARAMETER SUBROUTINE                          */
*/*                                                                  */
*/* THIS ROUTINE WILL PROCESS THE ITBMODE PARAMETER OF THE GROUP     */
*/* MACRO. IF EITHER SUBPARM IS INVALID AN ERROR MSG IS PUT OUT AND  */
*/* THE DEFAULT IS TAKEN. IF THE SUBPARMS ARE VALID THE APPROPRIATE  */
*/* BITS ARE TURNED ON IN THE TERM SIFT AREA IF OTHER THAN THE       */
*/* DEFAULT VALUE IS SPECIFIED.                                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0410
*GRPITBMD:                                                         0410
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0410
GRPITBMD DS    0H                                                  0411
*   IF PHRF2VAL(1)=ITBMDYES         /* WAS 'YES' TURN ON FLAG IN     */
*     THEN                                                         0411
         CLC   PHRF2VAL(4,PHRREG),@CF00039                         0411
         BNE   @RF00411                                            0411
*     RTRRCIT1=ON;                  /* TERM SIFT AREA. IF 'NO'       */
         L     @09,MLCPTR3(,MLCAREG)                               0412
         OI    RTRRCIT1(@09),B'10000000'                           0412
*   ELSE                                                           0413
*     RTRRCIT1=OFF;                 /* TURN FLAG OFF.                */
         B     @RC00411                                            0413
@RF00411 L     @09,MLCPTR3(,MLCAREG)                               0413
         NI    RTRRCIT1(@09),B'01111111'                           0413
*   IF PHRF2NVL>1                   /* MORE THAN ONE SUBPARM?        */
*     THEN                          /* YES                           */
@RC00411 LA    @09,1                                               0414
         C     @09,PHRF2NVL(,PHRREG)                               0414
         BNL   @RF00414                                            0414
*     IF PHRF2VAL(2)=ITBMDYES       /* IF 2ND SUBPARM OK AND WAS     */
*       THEN                                                       0415
         C     @09,PHRF2VAL+4(,PHRREG)                             0415
         BNE   @RF00415                                            0415
*       RTRRCIT2=ON;                /* 'YES' TURN ON FLAG IN TERM    */
         L     @09,MLCPTR3(,MLCAREG)                               0416
         OI    RTRRCIT2(@09),B'01000000'                           0416
*     ELSE                                                         0417
*       RTRRCIT2=OFF;                                              0417
         B     @RC00415                                            0417
@RF00415 L     @09,MLCPTR3(,MLCAREG)                               0417
         NI    RTRRCIT2(@09),B'10111111'                           0417
*   ELSE                                                           0418
*     ;                             /* ONLY ONE SUBPARM              */
@RF00414 DS    0H                                                  0419
*   GPR15=0;                        /* SET RTN CODE TO 0             */
@RC00414 SLR   GPR15,GPR15                                         0419
*   GPR14=ADDR(MAINSUB);                                           0420
         LA    GPR14,MAINSUB                                       0420
*   END GRPITBMD;                                                  0421
@EL00009 DS    0H                                                  0421
@EF00009 DS    0H                                                  0421
@ER00009 BR    @14                                                 0421
