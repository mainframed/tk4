*                   TRHRUCAT=TRHSC& /* SESSION CONTROL AND...        */
*                   CMDBYTE=CD1CLEAR THEN/* CLEAR COMMAND            */
         LA    @09,10                                              0480
         ALR   @09,PIUSTART                                        0480
         TM    TRHTYPE(@09),B'10000000'                            0480
         BNO   @RF00480                                            0480
         TM    TRHRUCAT(@09),B'01100000'                           0480
         BNO   @RF00480                                            0480
         CLI   CMDBYTE,X'A1'                                       0480
         BNE   @RF00480                                            0480
*                 CALL POSTSCLU;    /* POST PIU TO LU-LU SESSION   0481
*                                      CONTROL (IEDSCLU) QCB         */
         BAL   @14,POSTSCLU                                        0481
*               ELSE                                               0482
