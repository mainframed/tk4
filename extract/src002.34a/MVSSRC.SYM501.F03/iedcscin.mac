         B     @RC00413                                            0415
@RF00413 DS    0H                                                  0416
*                       SNC=SNSNOSES;/* SENSE=SESSION NOT ACTIVE     */
         MVC   SNC(4),@CB01525                                     0416
*                       CALL SNDERRSP;/* SEND ERROR RESPONSE         */
         BAL   @14,SNDERRSP                                        0417
*                     END;                                         0418
*                 END;                                             0419
*             END;                                                 0420
*         END;                                                     0421
*       ELSE                        /* DAF IS AN LU                  */
