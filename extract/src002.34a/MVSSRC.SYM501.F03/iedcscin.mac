         L     @09,SIBPTR                                          0470
         TM    SIBDSTAT(@09),B'00000001'                           0470
         BNO   @RF00470                                            0470
         TM    SIBDSTAT(@09),B'00000010'                           0470
         BNZ   @RF00470                                            0470
*             DO;                   /* YES                           */
*               IF TRHTYPE=ON&      /* RESPONSE AND...               */
*                   TRHRUCAT=TRHSC& /* SESSION CONTROL...            */
*                   CMDBYTE=CD1SDT THEN/* START DATA TRAFFIC COMMAND */
         LA    @09,10                                              0472
         ALR   @09,PIUSTART                                        0472
         TM    TRHTYPE(@09),B'10000000'                            0472
         BNO   @RF00472                                            0472
         TM    TRHRUCAT(@09),B'01100000'                           0472
         BNO   @RF00472                                            0472
         CLI   CMDBYTE,X'A0'                                       0472
