*             DO;                                                  0425
         B     @RC00423                                            0425
@RF00423 DS    0H                                                  0426
*               SNC=SNSNOSES;       /* SENSE=SESSION NOT ACTIVE      */
         MVC   SNC(4),@CB01525                                     0426
*               CALL SNDERRSP;      /* SEND ERROR RESPONSE           */
         BAL   @14,SNDERRSP                                        0427
*             END;                                                 0428
*         END;                                                     0429
*     END;                                                         0430
*   ELSE                            /* NO SIB PRESENT                */
