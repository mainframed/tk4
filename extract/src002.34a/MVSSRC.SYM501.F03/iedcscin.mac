         ST    R14,R14SV01                                         0448
*/*                                                                  */
*                                                                  0449
*   RPARM->PRFNBUNT=0;              /* SET NUMBER OF UNITS = 0 (THIS
*                                      MEANS IEDQGB WILL CALCULATE 0449
*                                      THE NUMBER OF UNITS TO BE   0449
*                                      FREED)                        */
         MVI   PRFNBUNT(RPARM),X'00'                               0449
