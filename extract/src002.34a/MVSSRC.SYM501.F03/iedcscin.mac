         TM    TRHRUCAT(@09),B'01100000'                           0459
         BZ    @RT00459                                            0459
         TM    TRHRUCAT(@09),B'01000000'                           0459
         BNO   @RF00459                                            0459
         TM    TRHRUCAT(@09),B'00100000'                           0459
         BNZ   @RF00459                                            0459
@RT00459 DS    0H                                                  0460
