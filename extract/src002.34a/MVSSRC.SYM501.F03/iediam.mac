POSTQCA  DS    0H                                                  0738
*   RFY                                                            0738
*     RRETURN RSTD;                                                0738
*   RPARM=RLCB;                     /* PLCB ADDRESS                  */
         LR    RPARM,RLCB                                          0739
*   LCBQCBA=ADDR(AVTOPCOB);         /* QCB                           */
         LA    @15,AVTOPCOB(,RAVT)                                 0740
         STCM  @15,7,LCBQCBA(RLCB)                                 0740
*   LCBPRI=PRIPLCB;                 /* PRIORITY                      */
         MVI   LCBPRI(RLCB),X'E8'                                  0741
*   TRMINPTP=OFF;                   /* RESET INPUT PENDING           */
         NI    TRMINPTP(RTRM),B'10111111'                          0742
*   RSTATE=POSTST;                  /* REQUEST POST STATE            */
         LA    RSTATE,1                                            0743
*   RFY                                                            0744
*     RRETURN UNRSTD;                                              0744
*   END POSTQCA;                                                   0745
@EL00006 DS    0H                                                  0745
@EF00006 DS    0H                                                  0745
@ER00006 BR    @14                                                 0745
*                                                                  0746
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP TO CALL IEDQHG01                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0746
*CALLQHG:                                                          0746
*   PROC OPTIONS(RETREG,SAVE(14));                                 0746
CALLQHG  ST    @14,@SA00007                                        0746
*   RFY                                                            0747
*     RTIME RSTD;                                                  0747
*   IF QCBDELAY=OFF THEN            /* QCB IS NOT ON TIME DELAY      */
         TM    QCBDELAY(RQCB),B'00000010'                          0748
         BNZ   @RF00748                                            0748
*     DO;                                                          0749
*                                                                  0749
*       /*************************************************************/
*       /*                                                           */
*       /* SET UP QCB FOR DELAY                                      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0750
*       QCBEOLDT=RTIME;             /* HOW LONG TO DELAY             */
         STH   RTIME,QCBEOLDT(,RQCB)                               0750
*       QCBLKRLN=0;                 /* ZERO LOCK RELATED LINE NUMBER */
         MVI   QCBLKRLN(RQCB),X'00'                                0751
*       QCBELCHN=RQCB;              /* ADDRESS OF QCB                */
         STCM  RQCB,7,QCBELCHN(RQCB)                               0752
*       QCBPRI=PRIMHUNT;            /* PRIORITY                      */
         MVI   QCBPRI(RQCB),X'E8'                                  0753
*       IF LCBSTCBA->STCBVTO=DSPSEND THEN/* SEND SCHEDULER IS IN PLCB*/
         L     @06,LCBSTCBA-1(,RLCB)                               0754
         LA    @06,0(,@06)                                         0754
         CLI   STCBVTO(@06),X'22'                                  0754
         BNE   @RF00754                                            0754
*         DO;                                                      0755
*           LCBSTCBA=LCBSTCBA->STCBLINK;/* REMOVE SEND SCHEDULER ... */
         L     @06,STCBLINK-1(,@06)                                0756
         LA    @06,0(,@06)                                         0756
         STCM  @06,7,LCBSTCBA(RLCB)                                0756
*           QCBSLINK=QCBSTCHN;      /* ... FROM PLCB                 */
         MVC   QCBSLINK(3,RQCB),QCBSTCHN(RQCB)                     0757
*         END;                                                     0758
*       ELSE                                                       0759
*         QCBSTCHN=QCBSLINK;        /* REMOVE SEND SCHEDULER FROM QCB*/
         B     @RC00754                                            0759
@RF00754 MVC   QCBSTCHN(3,RQCB),QCBSLINK(RQCB)                     0759
*       IF QCBTSSES=ON THEN         /* IF IN TSO SESSION THEN      0760
*                                                            @YM09109*/
@RC00754 TM    QCBTSSES(RQCB),B'10000000'                          0760
         BNO   @RF00760                                            0760
*         QCBSTCHN=ADDR(QCBSTVTO);  /* INSURE SS IN QCB      @YM09109*/
*                                                                  0761
         LA    @06,QCBSTVTO(,RQCB)                                 0761
         STCM  @06,7,QCBSTCHN(RQCB)                                0761
*       /*************************************************************/
*       /*                                                           */
*       /* SET UP TO CALL TIME DELAY ROUTINE                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0762
*       RPARM=RQCB;                 /* QCB ADDRESS                   */
@RF00760 LR    RPARM,RQCB                                          0762
*       CALL IEDQHG01;                                             0763
         L     @15,AVTHG01(,RAVT)                                  0763
         BALR  @14,@15                                             0763
*     END;                                                         0764
*   RSTATE=NOST;                                                   0765
@RF00748 SLR   RSTATE,RSTATE                                       0765
*   RFY                                                            0766
*     RTIME UNRSTD;                                                0766
*   END CALLQHG;                                                   0767
@EL00007 DS    0H                                                  0767
@EF00007 DS    0H                                                  0767
@ER00007 L     @14,@SA00007                                        0767
         BR    @14                                                 0767
