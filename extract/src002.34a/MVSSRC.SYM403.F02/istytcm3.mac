         FUNCT PROC=ISTZBFX0,EXCEPT=32XX                      @ZA29837
         FUNCT PROC=ISTZBFXA,ATTACH=L,DVC=32XX
         FUNCT PROC=ISTZBFX0,ATTACH=R,DVC=32XX                @ZA29837
