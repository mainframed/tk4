* /* START OF SPECIFICATIONS ****
*     PROLOGUE
*
*01*  MINI-MODULE-NAME = ISTYSSCP
*
*01*  FUNCTIONS = TO CREATE A SKELETEL DVT FOR VARY (SNA DEVICES)
*                 CONTAINING THE PROCESS MODULES NEEDED TO BE USED
*                 ALONG WITH THE NECESSARY QUALIFIERS SO AS THE PROPER
*                 FUNCTIONS ARE SELECTED AS PER DEVICE TYPE USED
*                NOTE -- SEE FUNCT MACRO FOR QUALIFYING ARGUMENTS
*
*01*  MODULE-SIZE = APPROX 1260-BYTES
*
*01*  PROCESSOR = ASSEMBLER
*
*01*  SYSTEMS-WHERE-USED = OS/VS1, OS/VS2
*
*01*  HOW-USED = AS-FOLLOWS:
*
*                THE SKELETEL DVT WILL BE SCANNED BY ISTOCCMD AND
*                THE CORRECT PROCESSES RETAINED BY MATCHING FUNCTION
*                QUALIFIERS WITH RESOURCE DEFINITION TABLE ENTRY INFO
*
*01*  INPUT-OUTPUT = NONE
*
*01*  CHANGE-ACTIVITY = NONE
*
**** END OF SPECIFICATIONS ***/
ISTYSSCP STRTMODE   CONTROL SUPPORT FOR SNA DEVICES  (VS2)
*A-000000-999999                                                XL03HBS
*
ISTCLOBP DVT
         FUNCT PROC=ISTDCC00
*
IDPURGEO DVT
         FUNCT PROC=ISTDCC80
*
ISTCLIBP DVT
         FUNCT PROC=ISTINCIP
*
***********************************************************************
*
ISTTPOBP DVT  ,     OUTBOUND- CONTROL MODE
         SPACE
*        REMOTE DEVICE SCHEDULER
         SPACE
         FUNCT PROC=ISTZAF1B
         SPACE
*        LCCW TO BTU TRANSLATOR
         SPACE
         FUNCT PROC=ISTZBF0B
         SPACE
*        DIALOG SCHEDULER
         SPACE
         FUNCT PROC=ISTZDFL0
         SPACE
*        SESSION SCHEDULER
         SPACE
         FUNCT PROC=ISTZDFL0
         SPACE
*        READ
         SPACE
         FUNCT PROC=ISTZBFX0
         SPACE
*        WRITE
         SPACE
         FUNCT PROC=ISTZBFX0
         SPACE
*        WRITE SOH
         SPACE
         FUNCT PROC=ISTZBFX0
         SPACE
*        COPY
         SPACE
         FUNCT PROC=ISTZBFX0
         SPACE
*        WRITE CONTROL
         SPACE
         FUNCT PROC=ISTZBFX0
         SPACE
*        RESET
         SPACE
         FUNCT PROC=ISTZBFX0
         SPACE
*        INVALID CONTROL
         SPACE
         FUNCT PROC=ISTZBFX0
         SPACE
*        CONTACT
         SPACE
         FUNCT PROC=ISTZBFX0
         SPACE
*        DISCONNECT
         SPACE
         FUNCT PROC=ISTZBFX0
         SPACE
*        QUIESCE LCCW                                          @X50BHDA
         SPACE
         FUNCT PROC=ISTZBFID,DVC=370X                          @X50BHDA
         FUNCT PROC=ISTZBFID,DVC=LU01,ATTACH=R                 @X50BHDA
         FUNCT PROC=ISTZBFID,DVC=CC01,ATTACH=R                 @X50BHDA
         FUNCT PROC=ISTZBFX0,DVC=LU01,ATTACH=L                 @X50BHDA
         FUNCT PROC=ISTZBFX0,DVC=CC01,ATTACH=L                 @X50BHDA
         SPACE
*        PREEMPT
         SPACE
         FUNCT PROC=ISTZBFX0
         SPACE
*        RESTORE
         SPACE
         FUNCT PROC=ISTZBFX0
         SPACE
*        INVALID COMMAND
         SPACE
         FUNCT PROC=ISTZBFX0
         SPACE
*        SEND LCCW
         SPACE
         FUNCT PROC=ISTZBFID,DVC=370X                          @X50BHDA
         FUNCT PROC=ISTZBFID,DVC=LU01,ATTACH=R                 @X50BHDA
         FUNCT PROC=ISTZBFID,DVC=CC01,ATTACH=R                 @X50BHDA
         FUNCT PROC=ISTZBFIF,DVC=LU01,ATTACH=L                 @X50BHDA
         FUNCT PROC=ISTZBFIF,DVC=CC01,ATTACH=L                 @X50BHDA
         SPACE
*        PREEMPT COMPLETION
         SPACE
         FUNCT PROC=ISTZDFL0
         SPACE
*        RESET CLEANUP (PHASE 1)
         SPACE
         FUNCT PROC=ISTZDFL0
         SPACE
*        RESET CLEANUP (PHASE 3)
         SPACE
         FUNCT PROC=ISTZDFL0
         SPACE
*        RESET CLEANUP (PHASE 4)
         SPACE
         FUNCT PROC=ISTZDFL0
         SPACE
*        SESSION CONTROL
         SPACE
         FUNCT PROC=ISTZBFSC
         SPACE
ISTTPIBP DVT  ,    INBOUND- CONTROL MODE
         SPACE
*        PIU ROUTER
         SPACE
         FUNCT PROC=ISTZIF1B
         SPACE
*        FID1 PROCESSOR
         SPACE
         FUNCT PROC=ISTZIFID
         SPACE
*        SESSION CONTROL
         SPACE
         FUNCT PROC=ISTZIFSC
         SPACE
*        SEGMENTING PROCESSOR                                  @X50BHDA
         SPACE
         FUNCT PROC=ISTZIFSG                                   @X50BHDA
*
ISTATOBP DVT
         SPACE
*        REMOTE DEVICE SCHEDULER
         SPACE
         FUNCT PROC=ISTZAF1B
         SPACE
ISTYSSCP ENDMODE
         END
