 TITLE 'ISTZBFBI-WRITE LCCW PROCESSOR FOR GENERAL POLL DEVICE-FORMBTU'
*/********************************************************************/
*/*                                                                  */
*/* FORMBTU: THIS ROUTINE COMPLETES FORMATTING FOR THE BTU BEING     */
*/*          BUILT. ALSO FMCB AND DNCB SESSION STATUS FLAGS ARE SET  */
*/*          TO REFLECT ANY CONTACT OR DISCONNECT OUTSTANDING STATUS */
*/*                                                                  */
*/********************************************************************/
*                                                                  0499
*   /*****************************************************************/
*   /*                                                               */
*   /* SEGMENT(FORMBTU)START                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0499
*   DO;                                                            0499
*     GOTO FORMBTU@;                                               0500
         B     FORMBTU@                                            0500
*FORMBTU:                                                          0501
*   END;                                                           0501
FORMBTU  DS    0H                                                  0502
*FBIIF010:                                                         0502
*   IF(FMCZFA06=ON×FMCZFB06=ON×FMCZFC06=ON)&FMCZFD06=OFF THEN/* IS 0502
*                                      (DEVICE IN SESSION, INVITE  0502
*                                      OUTSTANDING, OR CONTACT     0502
*                                      OUTSTANDING) AND DISCONNECT 0502
*                                      NOT OUTSTANDING?              */
FBIIF010 TM    FMCZFA06(FMCBREG),B'00000111'                       0502
         BZ    @RF00502                                            0502
         TM    FMCZFD06(FMCBREG),B'10000000'                       0502
         BNZ   @RF00502                                            0502
*FBIDO160:                                                         0503
*     DO;                                                          0503
FBIDO160 DS    0H                                                  0504
*       IF LCCWSI=ON THEN           /* IS START DIALOG FLAG ON?      */
         TM    LCCWSI(LCCWPTR),B'10000000'                         0504
         BNO   @RF00504                                            0504
*FBIDO170:                                                         0505
*         DO;                       /* YES,                          */
FBIDO170 DS    0H                                                  0506
*           LSCZRT00=ON;            /* SET ON LOGICAL ERROR FLAG     */
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0506
*           LSCZMNRC=FRMCDITS;      /* SET MINOR RC: CONFLICTING   0507
*                                      DIALOG INIT/TERM              */
         MVI   LSCZMNRC(LSCBPTR),X'A6'                             0507
*         END FBIDO170;                                            0508
*       ELSE                                                       0509
*         ;                         /* NO,(LCCWSI=OFF) (NULL)        */
@RF00504 DS    0H                                                  0510
*     END FBIDO160;                                                0510
*   ELSE                            /* NO, (SEE FBIIF010)            */
*FBIDO180:                                                         0511
*     DO;                                                          0511
         B     @RC00502                                            0511
@RF00502 DS    0H                                                  0511
FBIDO180 DS    0H                                                  0512
*       BDUMODIF=BDUMODIF×BMODWC;   /* SET WRT WITH CONTACT MODIF    */
         LA    @08,42                                              0512
         ALR   @08,ZOBPTR                                          0512
         OI    BDUMODIF(@08),X'08'                                 0512
*       BDURTOE=ON;                 /* SET BIT TO RETURN TEXT ON   0513
*                                      ERROR                 @X50BHXF*/
         OI    BDURTOE(@08),B'00000010'                            0513
*       FMCZFC06=ON;                /* SET ON CONTACT OUTSTANDING    */
         OI    FMCZFC06(FMCBREG),B'00000001'                       0514
*       FMCZX001=FMCZX001+1;        /* INCREMENT APPL SESS COUNT     */
         LA    @08,1                                               0515
         SLR   @00,@00                                             0515
         IC    @00,FMCZX001(,FMCBREG)                              0515
         ALR   @08,@00                                             0515
         STC   @08,FMCZX001(,FMCBREG)                              0515
*       FMCZF002=ON;                /* SET I/O SCHED SUSP    @XM09540*/
         OI    FMCZF002(FMCBREG),B'01000000'                       0516
*     END FBIDO180;                                                0517
*   IF LSCZRT00=ON THEN             /* LOGICAL ERROR FLAG SET ON?    */
@RC00502 TM    LSCZRT00(LSCBPTR),B'10000000'                       0518
         BO    @RT00518                                            0518
*     ;                             /* YES, SKIP FURTHER PROCESSG    */
*   ELSE                            /* NO,                           */
*FBIDO190:                                                         0520
*     DO;                                                          0520
FBIDO190 DS    0H                                                  0521
*       IF LCCWSD=ON THEN           /* IS END DIALOG FLAG ON?        */
         TM    LCCWSD(LCCWPTR),B'01000000'                         0521
         BNO   @RF00521                                            0521
*FBIDO200:                                                         0522
*         DO;                       /* YES,                          */
FBIDO200 DS    0H                                                  0523
*           BDUMODIF=BDUMODIF×BMODWD;/* SET DISCONNECT MODIFR        */
         OI    BDUMODIF+42(ZOBPTR),X'03'                           0523
*           FMCZFD06=ON;            /* SET ON DISCONN OUTSTANDING    */
         OI    FMCZFD06(FMCBREG),B'10000000'                       0524
*           DNCDISC=ON;             /* SET ON DISCONN OUTSTANDING    */
         OI    DNCDISC(DNCBREG),B'00001000'                        0525
*         END FBIDO200;                                            0526
*       ELSE                        /* NO, (LCCWSD=OFF)              */
*FBIDO210:                                                         0527
*         DO;                                                      0527
         B     @RC00521                                            0527
@RF00521 DS    0H                                                  0527
FBIDO210 DS    0H                                                  0528
*           FMCZFD06=OFF;           /* SET OFF DISCONN OUTSTANDING   */
         NI    FMCZFD06(FMCBREG),B'01111111'                       0528
*           DNCDISC=OFF;            /* SET OFF DISCONN OUTSTANDING   */
         NI    DNCDISC(DNCBREG),B'11110111'                        0529
*         END FBIDO210;                                            0530
*                                                                  0530
*/* SET BTU FLAGS AND REQUEST TAG SEQUENCE, ADJUST FMCB REQTAG SEQ   */
*                                                                  0531
*       IF FMCZF007=ON THEN         /* BUFFER TRACE ACTIVE FLAG      */
@RC00521 TM    FMCZF007(FMCBREG),B'00000100'                       0531
         BNO   @RF00531                                            0531
*         BTUZRT07=ON;                                             0532
         OI    BTUZRT07+28(ZOBPTR),B'00000001'                     0532
*       ELSE                                                       0533
*         BTUZRT07=OFF;                                            0533
         B     @RC00531                                            0533
@RF00531 NI    BTUZRT07+28(ZOBPTR),B'11111110'                     0533
*       IF NCBFTRIO=ON THEN         /* I/O TRACE ACTIVE FLAG         */
@RC00531 CLI   NCBFTRIO(DNCBREG),1                                 0534
         BNE   @RF00534                                            0534
*         BTUZRT08=ON;                                             0535
         OI    BTUZRT08+28(ZOBPTR),B'10000000'                     0535
*       ELSE                                                       0536
*         BTUZRT08=OFF;                                            0536
         B     @RC00534                                            0536
@RF00534 NI    BTUZRT08+28(ZOBPTR),B'01111111'                     0536
*       IF PROCFTX=ON THEN          /* CONFIDENTIAL TEXT FLAG        */
@RC00534 TM    PROCFTX+48(FMCBREG),B'00000001'                     0537
         BNO   @RF00537                                            0537
*         BTUZRT06=ON;                                             0538
         OI    BTUZRT06+28(ZOBPTR),B'00000010'                     0538
*       ELSE                                                       0539
*         BTUZRT06=OFF;                                            0539
         B     @RC00537                                            0539
@RF00537 NI    BTUZRT06+28(ZOBPTR),B'11111101'                     0539
*       GEN NODEFS REFS(FMCSEQ1B,ZOBBTU,ISTZOBUF,ZOBPTR,FMCBREG) SETS(
