*                                                                  0401
 TITLE 'ISTZBFBI-WRITE LCCW PROCESSOR FOR GENERAL POLL DEVICE-MAINLINE'
*/********************************************************************/
*/*                                                                  */
*/*  ISTZBFBI - WRITE LCCW PROCESSOR FOR A GENERAL POLL DEVICE       */
*/*                                                                  */
*/********************************************************************/
*/* TPZSAVE (7,12,14) AC(BCBI)BASE                                   */
*                                                                  0402
*   DO;                                                            0402
*     RFY                                                          0403
*      (GPR01P,                                                    0403
*       GPR12P,                                                    0403
*       GPR13P,                                                    0403
*       GPR14P) RSTD;                                              0403
*     GEN(PRINT OFF);                                              0404
*                                                                  0404
         PRINT OFF
*/* ATBUILD (ISTZSAVE)                                               */
*/* END OF ATBUILD                                                   */
*                                                                  0405
*     GEN(PRINT ON);                                               0405
         PRINT ON
*     RFY                                                          0406
*       ISTZSAVE BASED(GPR13P);                                    0406
*     GEN SETS(ZSAZSVVR)(STM   7,12,ZSAZSVVR+0(13));               0407
         STM   7,12,ZSAZSVVR+0(13)
*     GEN SETS(GPR12P);                                            0408
         BALR  GPR12P,0
@TSTART  DS    0H
         USING @TSTART,GPR12P
*     GEN(PRINT OFF);                                              0409
*                                                                  0409
         PRINT OFF
*/* ATBUILD (ISTZCRR,ISTRPH,ISTCRA) EJECT(NO)                        */
*/* END OF ATBUILD                                                   */
*                                                                  0410
*     GEN(PRINT ON);                                               0410
         PRINT ON
*     GPR01P->RPHCRR->CRACRR->ZCREBCBI='1'B;                       0411
         L     @15,RPHCRR(,GPR01P)                                 0411
         L     @15,CRACRR(,@15)                                    0411
         OI    ZCREBCBI(@15),B'00001000'                           0411
*     ZSAZACTV='1'B;                                               0412
         OI    ZSAZACTV(GPR13P),B'10000000'                        0412
*     ZSAZTRAC='BCBI';                                             0413
         MVC   ZSAZTRAC(4,GPR13P),@CC03011                         0413
*     ZSAZSVRR=GPR14P;                                             0414
         ST    GPR14P,ZSAZSVRR(,GPR13P)                            0414
*     GPR13P=ZSAZSVFC;                                             0415
         L     GPR13P,ZSAZSVFC(,GPR13P)                            0415
*     RFY                                                          0416
*      (GPR01P) UNRSTD;                                            0416
*     RFY                                                          0417
*      (GPR14P) UNRSTD;                                            0417
*   END;                                                           0418
*                                                                  0418
*/* END TPZSAVE                                                      */
*                                                                  0419
*   FDBDATAA=ADDR(ZOBBTU);          /* SET BUFFER DATA ADDRESS       */
         LA    @08,ZOBBTU(,ZOBPTR)                                 0419
         ST    @08,FDBDATAA(,ZOBPTR)                               0419
*   BDUCOMND=BCMDW;                 /* MAKE BDU COMMAND A WRITE      */
         LA    @08,42                                              0420
         ALR   @08,ZOBPTR                                          0420
         MVI   BDUCOMND(@08),X'02'                                 0420
*   BTUDCF=LENGTH(BTURH)+LENGTH(BTUSYSTR)+LENGTH(BTUEXTR)+LENGTH(ISTBDU
*       )+2;                        /* INITIALIZE COUNT      @XL03HAH*/
         MVC   BTUDCF+28(2,ZOBPTR),@CH00442                        0421
*   BDUMODIF=BMODWT;                /* MAKE BDU MODIFIER: WRITE    0422
*                                      TRANSMISSION                  */
         MVI   BDUMODIF(@08),X'02'                                 0422
*   LCCWPTR=LSCLCCWA;               /* ADDRESS THE LCCW              */
         L     LCCWPTR,LSCLCCWA(,LSCBPTR)                          0423
*   IF LCCWMODE=LMT THEN            /* IS LCCW FOR TRANSMIT MODE?    */
         TM    LCCWMODE(LCCWPTR),B'00001100'                       0424
         BO    @RT00424                                            0424
*     ;                             /* YES, (NULL)                   */
*   ELSE                                                           0426
*FBIDO010:                                                         0426
*     DO;                           /* NO,                           */
FBIDO010 DS    0H                                                  0427
*       IF LCCWMODE=LMM×LCCWRWOP=LWR THEN/* IS LCCW CMD CODE MESSAG
*                                      MODE OR WRITE CONVERSATIONAL? */
         TM    LCCWMODE(LCCWPTR),B'00001000'                       0427
         BNO   @GL00001                                            0427
         TM    LCCWMODE(LCCWPTR),B'00000100'                       0427
         BZ    @RT00427                                            0427
@GL00001 TM    LCCWRWOP(LCCWPTR),B'00000001'                       0427
         BNO   @RF00427                                            0427
         TM    LCCWRWOP(LCCWPTR),B'00111110'                       0427
         BNZ   @RF00427                                            0427
@RT00427 DS    0H                                                  0428
*FBIDO020:                                                         0428
*         DO;                       /* YES,                          */
FBIDO020 DS    0H                                                  0429
*           IF LCCWSD=ON THEN       /* IS END DIALOG SWITCH ON?      */
         TM    LCCWSD(LCCWPTR),B'01000000'                         0429
         BNO   @RF00429                                            0429
*FBIDO030:                                                         0430
*             DO;                   /* YES,                          */
FBIDO030 DS    0H                                                  0431
*               LSCZRT00=ON;        /* SET ON LOGICAL ERROR FLAG     */
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0431
*               LSCZMNRC=FRMCDITS;  /* SET MINOR RC: CONFLICTING   0432
*                                      DIALOG INIT/TERM SEQUENCE     */
         MVI   LSCZMNRC(LSCBPTR),X'A6'                             0432
*             END FBIDO030;                                        0433
*           ELSE                    /* NO,(END DIALOG SWITCH OFF)    */
*FBIDO040:                                                         0434
*             DO;                                                  0434
         B     @RC00429                                            0434
@RF00429 DS    0H                                                  0434
FBIDO040 DS    0H                                                  0435
*               IF LCCWMODE=LMM THEN/* IS LCCW FOR MESSAGE MODE?     */
         TM    LCCWMODE(LCCWPTR),B'00001000'                       0435
         BNO   @RF00435                                            0435
         TM    LCCWMODE(LCCWPTR),B'00000100'                       0435
         BNZ   @RF00435                                            0435
*                 BDUMODIF=BMODWM;  /* YES, SET WRITE MSG MODIF      */
         MVI   BDUMODIF+42(ZOBPTR),X'01'                           0436
*               ELSE                /* NO, (NOT MSG MODE)            */
*FBIDO050:                                                         0437
*                 DO;               /* (LCCW IS FOR WRITE CONV)      */
         B     @RC00435                                            0437
@RF00435 DS    0H                                                  0437
FBIDO050 DS    0H                                                  0438
*                   BTUZRT09=ON;    /* SET DIALOG SCHED DISABLED   0438
*                                      SWITCH ON                     */
         OI    BTUZRT09+28(ZOBPTR),B'01000000'                     0438
*                   FMCZF004=ON;    /* SET ON READ OUTSTANDG FLAG    */
         OI    FMCZF004(FMCBREG),B'00010000'                       0439
*                   IF DEVTCODE=DEV3284×DEVTCODE=DEV3286 THEN/* ERROR
*                                      IF DEVICE A PRINTER           */
         LA    @08,36                                              0440
         ALR   @08,DNCBREG                                         0440
         CLI   DEVTCODE(@08),X'1A'                                 0440
         BE    @RT00440                                            0440
         CLI   DEVTCODE(@08),X'1B'                                 0440
         BNE   @RF00440                                            0440
@RT00440 DS    0H                                                  0441
*                     DO;                                          0441
*                       LSCZRT00=ON;                               0442
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0442
*                       LSCZMNRC=FRMUSELE;/* FLAG LOGICAL ERROR AND
*                                      SET MINOR RETURN CODE         */
         MVI   LSCZMNRC(LSCBPTR),X'A3'                             0443
*                     END;                                         0444
*                   ELSE                                           0445
*                     ;                                            0445
@RF00440 DS    0H                                                  0446
*                 END FBIDO050;                                    0446
*             END FBIDO040;                                        0447
*         END FBIDO020;                                            0448
*       ELSE                        /* NO, (NEITHER MSG MODE NOR   0449
*                                      WRITE CONVERSATIONAL)         */
*FBIDO060:                                                         0449
*         DO;                                                      0449
         B     @RC00427                                            0449
@RF00427 DS    0H                                                  0449
FBIDO060 DS    0H                                                  0450
*           IF LCCWRWOP=LEAU THEN   /* IS COMMAND ERASE ALL        0450
*                                      UNPROTECTED?                  */
         TM    LCCWRWOP(LCCWPTR),B'00010001'                       0450
         BNO   @RF00450                                            0450
         TM    LCCWRWOP(LCCWPTR),B'00101110'                       0450
         BNZ   @RF00450                                            0450
*FBIDO080:                                                         0451
*             DO;                   /* YES, (3270)                   */
FBIDO080 DS    0H                                                  0452
*               BTUTEXT(1)=ESC;     /* SET 3270 ESCAPE CHAR          */
         LA    @08,28                                              0452
         ALR   @08,ZOBPTR                                          0452
         MVI   BTUTEXT(@08),X'27'                                  0452
*               BTUTEXT(2)=EAU;     /* SET 3270 ERASE ALL UNPROTECTED
*                                      COMMAND                       */
         MVI   BTUTEXT+1(@08),X'6F'                                0453
*             END FBIDO080;                                        0454
*           ELSE                    /* NO,(NOT ERASE ALL UNPROT)     */
*FBIDO100:                                                         0455
*             DO;                                                  0455
         B     @RC00450                                            0455
@RF00450 DS    0H                                                  0455
FBIDO100 DS    0H                                                  0456
*               LSCZRT01=ON;        /* SET ON CTL BLK ERROR FLAG     */
         OI    LSCZRT01(LSCBPTR),B'01000000'                       0456
*               LSCZMNRC=FRMINVL;   /* SET MINOR RC: INVALID LCCW  0457
*                                      CHAINING FLAGS SET            */
         MVI   LSCZMNRC(LSCBPTR),X'A3'                             0457
*             END FBIDO100;                                        0458
*         END FBIDO060;                                            0459
@RC00450 DS    0H                                                  0460
*     END FBIDO010;                                                0460
@RC00427 DS    0H                                                  0461
*   IF LCCWSOP=ON THEN              /* IF ERASE TYPE COMMAND         */
@RT00424 TM    LCCWSOP(LCCWPTR),B'00010000'                        0461
         BNO   @RF00461                                            0461
*     IF DEVTCODE=DEV3275×DEVTCODE=DEV3277×DEVTCODE=DEV3284×DEVTCODE=
*         DEV3286 THEN              /* IS DEVICE A 3270 PRINTER OR 0462
*                                      DISPLAY                       */
         LA    @08,36                                              0462
         ALR   @08,DNCBREG                                         0462
         CLI   DEVTCODE(@08),X'1C'                                 0462
         BE    @RT00462                                            0462
         CLI   DEVTCODE(@08),X'19'                                 0462
         BE    @RT00462                                            0462
         CLI   DEVTCODE(@08),X'1A'                                 0462
         BE    @RT00462                                            0462
         CLI   DEVTCODE(@08),X'1B'                                 0462
         BNE   @RF00462                                            0462
@RT00462 DS    0H                                                  0463
*       IF LCCWRWOP=LEAU THEN                                      0463
         TM    LCCWRWOP(LCCWPTR),B'00010001'                       0463
         BNO   @RF00463                                            0463
         TM    LCCWRWOP(LCCWPTR),B'00101110'                       0463
         BZ    @RT00463                                            0463
*         ;                         /* IF ERASE ALL UNPROTECTED      */
*       ELSE                                                       0465
*FBIDO130:                                                         0465
*         DO;                                                      0465
@RF00463 DS    0H                                                  0465
FBIDO130 DS    0H                                                  0466
*           BTUTEXT(1)=ESC;         /* SET 3270 ESCAPE CHARACTER     */
         LA    @08,28                                              0466
         ALR   @08,ZOBPTR                                          0466
         MVI   BTUTEXT(@08),X'27'                                  0466
*           BTUTEXT(2)=EW;          /* SET ERASE/WRITE COMMAND       */
         MVI   BTUTEXT+1(@08),X'F5'                                0467
*         END FBIDO130;                                            0468
*     ELSE                          /* NO,(NOT 3270 DEVICE)          */
*FBIDO140:                                                         0469
*       DO;                                                        0469
         B     @RC00462                                            0469
@RF00462 DS    0H                                                  0469
FBIDO140 DS    0H                                                  0470
*         LSCZRT00=ON;              /* SET ON LOGICAL ERROR FLAG     */
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0470
*         LSCZMNRC=FRMUSELE;        /* SET MINOR RC: USER LOGICAL  0471
*                                      ERROR                         */
         MVI   LSCZMNRC(LSCBPTR),X'A3'                             0471
*       END FBIDO140;                                              0472
*   ELSE                            /* NOT ERASE TYPE COMMAND        */
*     IF DEVTCODE=DEV3275×DEVTCODE=DEV3277×DEVTCODE=DEV3284×DEVTCODE=
*         DEV3286 THEN              /* IS DEVICE A 3270 PRINTER OR 0473
*                                      DISPLAY                       */
         B     @RC00461                                            0473
@RF00461 LA    @08,36                                              0473
         ALR   @08,DNCBREG                                         0473
         CLI   DEVTCODE(@08),X'1C'                                 0473
         BE    @RT00473                                            0473
         CLI   DEVTCODE(@08),X'19'                                 0473
         BE    @RT00473                                            0473
         CLI   DEVTCODE(@08),X'1A'                                 0473
         BE    @RT00473                                            0473
         CLI   DEVTCODE(@08),X'1B'                                 0473
         BNE   @RF00473                                            0473
@RT00473 DS    0H                                                  0474
*       DO;                                                        0474
*         BTUTEXT(1)=ESC;           /* PUT WRITE COMMAND IN DATA     */
         LA    @08,28                                              0475
         ALR   @08,ZOBPTR                                          0475
         MVI   BTUTEXT(@08),X'27'                                  0475
*         BTUTEXT(2)=WR;                                           0476
         MVI   BTUTEXT+1(@08),X'F1'                                0476
*       END;                                                       0477
*   IF LSCZRT00=ON×LSCZRT01=ON THEN /* LOGICAL OR CTL BLK ERROR?     */
@RF00473 DS    0H                                                  0478
@RC00461 TM    LSCZRT00(LSCBPTR),B'11000000'                       0478
         BNZ   @RT00478                                            0478
*     ;                             /* YES, (NULL)                   */
*   ELSE                            /* NO,                           */
*FBIDO150:                                                         0480
*     DO;                           /* FORMAT BTU AND PROC DATA      */
*                                                                  0480
FBIDO150 DS    0H                                                  0481
*       /*************************************************************/
*       /*                                                           */
*       /* SEGMENT(FORMBTU)REQUEST                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0481
*       DO;                                                        0481
*         GOTO FORMBTU;                                            0482
         B     FORMBTU                                             0482
*FORMBTU#:                                                         0483
*       END;                                                       0483
FORMBTU# DS    0H                                                  0484
*     END FBIDO150;                                                0484
*   IF LSCZRT00=ON×LSCZRT01=ON THEN /* LOGICAL OR CTL BLK ERROR?     */
@RT00478 TM    LSCZRT00(LSCBPTR),B'11000000'                       0485
         BZ    @RF00485                                            0485
*     CALL ZPSZBFZ0;                /* YES, CALL INVALID LCP RECOVERY
*                                      ROUTINE                       */
         L     @08,ATCZPSVT(,VTCVTREG)                             0486
         L     @15,ZPSZBFZ0(,@08)                                  0486
         BALR  @14,@15                                             0486
*   ELSE                                                           0487
*     ;                             /* NO ERRORS (NULL)              */
@RF00485 DS    0H                                                  0488
*/* TPZRETN                                                          */
*                                                                  0488
*   DO;                                                            0488
*     RFY                                                          0489
*      (GPR07P,                                                    0489
*       GPR14P,                                                    0489
*       GPR15P) RSTD;                                              0489
*     GPR13P=ZSAZSVBC;                                             0490
         L     GPR13P,ZSAZSVBC(,GPR13P)                            0490
*     IF GPR13P=0 THEN                                             0491
         LTR   GPR13P,GPR13P                                       0491
         BZ    @RT00491                                            0491
*       ;                           /* TPDVTS ISTZFARR               */
*     ELSE                                                         0493
*       GPR14P=ZSAZSVRR;                                           0493
         L     GPR14P,ZSAZSVRR(,GPR13P)                            0493
*     GEN REFS(ZSAZSVVR)(LM    7,12,ZSAZSVVR+0(13));               0494
@RT00491 DS    0H                                                  0494
         LM    7,12,ZSAZSVVR+0(13)
*     GEN REFS(GPR14P) EXIT NOSEQFLOW(BR    GPR14P);               0495
         BR    GPR14P
*     RFY                                                          0496
*      (GPR07P,                                                    0496
*       GPR14P,                                                    0496
