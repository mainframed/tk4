*             DO;                   /* NO,(LCCWCNT^=3)               */
         B     @RC00424                                            0454
@RF00424 DS    0H                                                  0454
FDIDO150 DS    0H                                                  0455
*               LSCZRT01=ON;        /* SET ON CTL BLK ERROR FLAG     */
         OI    LSCZRT01(LSCBPTR),B'01000000'                       0455
*               LSCZMNRC=FRMCPCNT;  /* SET MINOR RC: COPY LCCW COUNT
