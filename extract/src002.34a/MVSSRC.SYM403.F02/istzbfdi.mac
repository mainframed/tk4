 TITLE 'ISTZBFDI-COPY LCCW PROCESSOR FOR GENERAL POLL DEVICE-SETFDAC'
*/********************************************************************/
*/*                                                                  */
*/* SETFDAC: THIS ROUTINE GETS THE DEVICE ADDRESS CHARACTER FOR THE  */
*/*          3270 DEVICE TO BE COPIED FROM, PLACES IT IN THE BTU     */
*/*          TEXT, AND REQUESTS FORMBTU TO FORMAT THE BTU WITH A     */
*/*          REQUEST TAG. NOTE THAT REG2, REG13, AND THE DAF IN THE  */
*/*          BTU ORIGINALLY MUST BE SAVED IN THE ZCR. ALSO, CIDCTL   */
*/*          FINISH IS ISSUED SUBSEQUENTLY TO CLEAR THE DAF LOCK     */
*/*          SET BY CIDCTL FIND.                                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0487
*   /*****************************************************************/
*   /*                                                               */
*   /* SEGMENT(SETFDAC)START                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0487
*   DO;                                                            0487
*     GOTO SETFDAC@;                                               0488
         B     SETFDAC@                                            0488
*SETFDAC:                                                          0489
*   END;                                                           0489
SETFDAC  DS    0H                                                  0490
*   ZCRR2SV=VTCVTREG;               /* SAVE CIDCTL-CORRUPTED REGS    */
         L     @08,RPHCRR(,RPHREG)                                 0490
         L     @15,CRACRR(,@08)                                    0490
         ST    VTCVTREG,ZCRR2SV(,@15)                              0490
*   ZCRR13SV=SAVAREG;                                              0491
         ST    SAVAREG,ZCRR13SV(,@15)                              0491
*   ZCRDAFSV=BTUTEXT(4:5);          /* SAVE FROM DEVICE DAF          */
         MVC   ZCRDAFSV(2,@15),BTUTEXT+31(ZOBPTR)                  0492
*   DAFREG=ZERO;                    /* CLEAR DAF HOLDER              */
         SLR   DAFREG,DAFREG                                       0493
*   DAFREG=ZCRDAFSV;                /* GET DAF OF COPY FROM DEV      */
         LH    DAFREG,ZCRDAFSV(,@15)                               0494
         N     DAFREG,@CF03760                                     0494
*   SAVAREG=ADDR(CRASVC);           /* SET SPECIAL SAVE AREA PTR     */
         LA    SAVAREG,CRASVC(,@08)                                0495
*   RFY                                                            0496
*    (RDNCBPTR,                                                    0496
*     RCDREG) RSTD;                 /* RESERVE CIDCTL PARAM REGS     */
*   DO;                             /* CIDCTL(FIND)                  */
*     RFY                                                          0498
*      (EMCR2,                                                     0498
*       EMCR3) RSTD;                                               0498
*     GEN SETS(RPHWORK)                                            0499
*         (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);     0499
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH
*     EMCR3='00'X;                  /* CLEAR EXTRA BITS              */
         SLR   EMCR3,EMCR3                                         0500
*     EMCR3='01'X;                  /* INDICATE SHARED LOCK          */
         LA    EMCR3,1                                             0501
*     CALL ATCLCPTR->ATCCDFND->ISTSDCCF;/* PROCESS REQUEST           */
         L     @08,ATCLCPTR                                        0502
         L     @15,ATCCDFND(,@08)                                  0502
         BALR  @14,@15                                             0502
*     RFY                                                          0503
*      (EMCR2,                                                     0503
*       EMCR3) UNRSTD;                                             0503
*   END;                            /* CIDCTL LOCATE DNCB OF FROM  0504
*                                      DEVICE                        */
*   IF RCDREG=GOODRC THEN           /* DID CIDCTL FIND DNCB?         */
         C     RCDREG,@CF00035                                     0505
         BNE   @RF00505                                            0505
*FDIDO240:                                                         0506
*     DO;                           /* YES,                          */
FDIDO240 DS    0H                                                  0507
*       RFY                                                        0507
*         FDNCBPTR RSTD;                                           0507
*       FDNCBPTR=RDNCBPTR;          /* SET FOUND DNCB POINTER        */
         LR    FDNCBPTR,RDNCBPTR                                   0508
*       IF ADDR(FDNCBPTR->DNCNCB)->NCBNSNA=NCBNSNA THEN/* IS NEXT  0509
*                                      SCHEDULEABLE NODE ADDRESS IN
*                                      FOUND DNCB SAME AS ONE IN DNCB
*                                      PASSED TO THIS ROUTINE?       */
         CLC   NCBNSNA(4,FDNCBPTR),NCBNSNA(DNCBREG)                0509
         BNE   @RF00509                                            0509
*         IF FDNCBPTR->DNCZV001^=EMPTY&/* IF ACTIVE FMCB IN          */
*             FDNCBPTR->DNCZV001->FMCDEBA=/* DNCB & BOTH FMCBS       */
*             FMCDEBA THEN          /* BELONG TO THE SAME APPLICATION
*                                                            @XL03HEK*/
         L     @14,DNCZV001-1(,FDNCBPTR)                           0510
         LA    @14,0(,@14)                                         0510
         LTR   @14,@14                                             0510
         BZ    @RF00510                                            0510
         CLC   FMCDEBA(4,@14),FMCDEBA(FMCBREG)                     0510
         BNE   @RF00510                                            0510
*FDIDO250:                                                         0511
*           DO;                     /* THEN,                         */
FDIDO250 DS    0H                                                  0512
*             BTUTEXT(4)=ADDR(ADDR(FDNCBPTR->DNCNCB)->NCBDEVCH)->  0512
*                 DEVPHYSA;         /* MOVE DEVICE ADDR CHARACTER  0512
*                                      FROM FOUND DNCB TO BTUTEXT    */
         MVC   BTUTEXT+31(1,ZOBPTR),DEVPHYSA+36(FDNCBPTR)          0512
*           END FDIDO250;                                          0513
*         ELSE                      /* NO ACTIVE FMCB PTR IN DNCB  0514
*                                                            @XL03HCH*/
*FDIDO255:                                                         0514
*           DO;                     /*                       @XL03HEK*/
         B     @RC00510                                            0514
@RF00510 DS    0H                                                  0514
FDIDO255 DS    0H                                                  0515
*             LSCZRT00=ON;          /* SET ON LOGIC ERROR@XL03HEK    */
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0515
