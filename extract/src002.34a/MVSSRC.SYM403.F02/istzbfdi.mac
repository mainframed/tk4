*FDIDO160:                                                         0460
*     DO;                                                          0460
         B     @RC00396                                            0460
@RF00396 DS    0H                                                  0460
FDIDO160 DS    0H                                                  0461
*       LSCZRT00=ON;                /* SET ON LOGICAL ERROR FLAG     */
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0461
*       LSCZMNRC=FRMUSELE;          /* SET MINOR RC: USER LOGIC ERROR*/
         MVI   LSCZMNRC(LSCBPTR),X'A3'                             0462
