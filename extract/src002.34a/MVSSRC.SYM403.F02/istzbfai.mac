*                                                                  0415
*/* END TPZSAVE                                                      */
*                                                                  0416
*   IF LCCWCD=ON THEN               /* IS LCCW MARKED DATA CHAIN?    */
         L     @15,LSCLCCWA(,LSCBPTR)                              0416
         TM    LCCWCD(@15),B'10000000'                             0416
         BNO   @RF00416                                            0416
*FAIDO010:                                                         0417
*     DO;                           /* YES,                          */
FAIDO010 DS    0H                                                  0418
*       LSCZRT01=ON;                /* SET ON CTL BLK ERROR FLAG     */
         OI    LSCZRT01(LSCBPTR),B'01000000'                       0418
*       LSCZMNRC=FRMINVCF;          /* SET ON INVALID LCCW CHAING    */
         MVI   LSCZMNRC(LSCBPTR),X'81'                             0419
*     END FAIDO010;                                                0420
*   ELSE                            /* NO, (NOT CHAINED DATA)        */
*FAIDO020:                                                         0421
*     DO;                                                          0421
         B     @RC00416                                            0421
@RF00416 DS    0H                                                  0421
FAIDO020 DS    0H                                                  0422
*       IF LCCWSI=ON&(FMCZFA06=ON×FMCZFB06=ON×FMCZFC06=ON)&FMCZFD06=OFF
*         THEN                      /* IS START DIALOG ON AND (ANY OF
*                                      DEV IN SESS, INVITE         0422
*                                      OUTSTANDING, OR CONTACT     0422
*                                      OUTSTANDING) AND DISCONNECT 0422
*                                      NOT OUTSTANDING?              */
         L     @15,LSCLCCWA(,LSCBPTR)                              0422
         TM    LCCWSI(@15),B'10000000'                             0422
         BNO   @RF00422                                            0422
         TM    FMCZFA06(FMCBREG),B'00000111'                       0422
         BZ    @RF00422                                            0422
         TM    FMCZFD06(FMCBREG),B'10000000'                       0422
         BNZ   @RF00422                                            0422
*FAIDO030:                                                         0423
*         DO;                       /* YES, SET ERROR INDICATORS SO
*                                      OP CODE WILL NOT BE PROCESSED */
FAIDO030 DS    0H                                                  0424
*           LSCZRT00=ON;            /* SET ON LOGICAL ERROR FLAG     */
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0424
*           LSCZMNRC=FRMCDITS;      /* SET ON CONFLICTING DIALOG   0425
*                                      INIT/TERM SEQUENCE FLAG       */
         MVI   LSCZMNRC(LSCBPTR),X'A6'                             0425
*         END FAIDO030;                                            0426
*       ELSE                                                       0427
*         ;                         /* NO, OP CODE WILL BE PROCD     */
@RF00422 DS    0H                                                  0428
*       IF LSCZRT00=ON THEN         /* WAS LOGICAL ERROR SET ON?     */
         TM    LSCZRT00(LSCBPTR),B'10000000'                       0428
         BO    @RT00428                                            0428
*         ;                         /* YES, DO NOT PROCESS OPCODE    */
*       ELSE                        /* NO, PROCESS LCCW OPCODE       */
*FAIDO040:                                                         0430
*         DO;                                                      0430
FAIDO040 DS    0H                                                  0431
*           RFY                                                    0431
*             OPCODREG RSTD;                                       0431
*           OPCODREG=LCCWOP;        /* GET LCCW READ OP CODE         */
         L     @15,LSCLCCWA(,LSCBPTR)                              0432
         SLR   OPCODREG,OPCODREG                                   0432
         IC    OPCODREG,LCCWOP(,@15)                               0432
*           OPCODREG=OPCODREG&RWOPMASK;                            0433
         N     OPCODREG,@CF02951                                   0433
*           GEN CODE DEFS(FAIBRTBL) FLOWS(FAIRMSG,FAIRINV,FAIRTRAN,
*               FAIRMOD,FAIRBUFF) NOSETS NOSEQFLOW REFS(FAIBRTBL,  0434
*               OPCODREG);                                         0434
         B     FAIBRTBL(OPCODREG)       GO TO A READ PROCESSING ROUTINE
FAIBRTBL EQU   *-6                      FIRST 6 BYTES ARE NEVER USED
         B     FAIRINV                  06 READ BLOCK IS INVALID
         B     FAIRMSG                  0A READ MESSAGE
         B     FAIRTRAN                 0E READ TRANSMISSION
         B     FAIRMOD                  12 READ MODIFIED
         B     FAIRBUFF                 16 READ BUFFER
         B     FAIRINV                  1A READ CONTINUOUS IS INVALID
*                                       ONLY THE ABOVE CODES WILL
*                                        OCCUR
*           RFY                                                    0435
*             OPCODREG UNRSTD;                                     0435
*           GEN NODEFS NOREFS NOSETS(EJECT);                       0436
*                                                                  0436
         EJECT
*/********************************************************************/
*/* PROCESS READ MESSAGE                                             */
*/********************************************************************/
*                                                                  0437
*FAIRMSG:                                                          0437
*           IF LCCWSD=ON THEN       /* IS END DIALOG SWITCH ON?      */
FAIRMSG  L     @15,LSCLCCWA(,LSCBPTR)                              0437
         TM    LCCWSD(@15),B'01000000'                             0437
         BNO   @RF00437                                            0437
*FAIDO050:                                                         0438
*             DO;                   /* YES,                          */
FAIDO050 DS    0H                                                  0439
*               LSCZRT00=ON;        /* SET ON LOGICAL ERROR FLAG     */
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0439
*               LSCZMNRC=FRMCDITS;  /* SET MINOR RC=CONFLICTING    0440
*                                      DIALOG INIT/TERM              */
         MVI   LSCZMNRC(LSCBPTR),X'A6'                             0440
*             END FAIDO050;                                        0441
*           ELSE                    /* NO, (END DIALOG SW IS OFF)    */
*             CALL PROCMT;          /* PROCESS READ MESSAGE          */
         B     @RC00437                                            0442
@RF00437 BAL   @14,PROCMT                                          0442
*           GO TO FAIERP;           /* END READ PROCESSING           */
         B     FAIERP                                              0443
*/********************************************************************/
*/* PROCESS READ TRANSMISSION                                        */
*/********************************************************************/
*                                                                  0444
*FAIRTRAN:                                                         0444
*           IF LCCWSD=ON THEN       /* IS END DIALOG SWITCH ON?      */
FAIRTRAN L     @15,LSCLCCWA(,LSCBPTR)                              0444
         TM    LCCWSD(@15),B'01000000'                             0444
         BNO   @RF00444                                            0444
*             DO;                   /* YES,                          */
*               FMCZF002=ON;        /* SET ON I/O SCHED SUSPENDED  0446
*                                                            @XL03HEK*/
         OI    FMCZF002(FMCBREG),B'01000000'                       0446
*               FMCZFD06=ON;        /* SET ON DISCONNECT     @XL03HEK*/
         OI    FMCZFD06(FMCBREG),B'10000000'                       0447
*               DNCDISC=ON;         /* OUTSTANDING           @XL03HEK*/
         OI    DNCDISC(DNCBREG),B'00001000'                        0448
*             END;                  /*                       @XL03HEK*/
*           ELSE                    /* NO,                   @XL03HEK*/
*             DO;                   /*                       @XL03HEK*/
         B     @RC00444                                            0450
@RF00444 DS    0H                                                  0451
*               FMCZFD06=OFF;       /* SET OFF DISCONNECT@XL03HEK    */
         NI    FMCZFD06(FMCBREG),B'01111111'                       0451
*               DNCDISC=OFF;        /* OUTSTANDING           @XL03HEK*/
         NI    DNCDISC(DNCBREG),B'11110111'                        0452
*             END;                  /*                       @XL03HEK*/
*           CALL PROCMT;            /* PROCESS READ TRANSMISSION     */
@RC00444 BAL   @14,PROCMT                                          0454
*           GO TO FAIERP;           /* END READ PROCESSING           */
         B     FAIERP                                              0455
*/********************************************************************/
*/* PROCESS READ MODIFIED                                            */
*/********************************************************************/
*                                                                  0456
*FAIRMOD:                                                          0456
*           BTUTEXT(1)=ESC;         /* SET ESCAPE CHARACTER          */
FAIRMOD  LA    @15,28                                              0456
         ALR   @15,ZOBPTR                                          0456
         MVI   BTUTEXT(@15),X'27'                                  0456
*           BTUTEXT(2)=RMOD;        /* SET READ MODIFIED COMMAND     */
         MVI   BTUTEXT+1(@15),X'F6'                                0457
*           GO TO FAIRBUF1;         /* GO PROCESS READ MODIFIED      */
         B     FAIRBUF1                                            0458
*/********************************************************************/
*/* PROCESS READ BUFFER                                              */
*/********************************************************************/
*                                                                  0459
*FAIRBUFF:                                                         0459
*           BTUTEXT(1)=ESC;         /* SET ESCAPE CHARACTER          */
FAIRBUFF LA    @15,28                                              0459
         ALR   @15,ZOBPTR                                          0459
         MVI   BTUTEXT(@15),X'27'                                  0459
*           BTUTEXT(2)=RBUFF;       /* SET READ BUFFER COMMAND       */
         MVI   BTUTEXT+1(@15),X'F2'                                0460
*FAIRBUF1:                                                         0461
*           ;                       /* PROCESS RMOD OR RBUFF         */
*                                                                  0461
FAIRBUF1 DS    0H                                                  0462
*           /*********************************************************/
*           /*                                                       */
*           /* SEGMENT(PROCBM)REQUEST                                */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0462
*           DO;                                                    0462
*             GOTO PROCBM;                                         0463
         B     PROCBM                                              0463
*PROCBM#:                                                          0464
*           END;                                                   0464
*           GO TO FAIERP;           /* END READ PROCESSING           */
         B     FAIERP                                              0465
*/********************************************************************/
*/* PROCESS INVALID LCCW OP CODE                                     */
*/********************************************************************/
*                                                                  0466
*FAIRINV:                                                          0466
*           LSCZRT01=ON;            /* SET ON CTL BLK ERROR FLAG     */
FAIRINV  OI    LSCZRT01(LSCBPTR),B'01000000'                       0466
*           LSCZMNRC=FRMINVL;       /* SET MINOR RC=INVALID CODE     */
         MVI   LSCZMNRC(LSCBPTR),X'A3'                             0467
*           GO TO FAIERP;           /* END READ PROCESSING           */
         B     FAIERP                                              0468
*/********************************************************************/
*/* END OF SPECIAL READ TYPE PROCESSING                              */
*/********************************************************************/
*                                                                  0469
*FAIERP:                                                           0469
*           ;                                                      0469
FAIERP   DS    0H                                                  0470
*         END FAIDO040;                                            0470
*     END FAIDO020;                                                0471
@RT00428 DS    0H                                                  0472
*   GEN NODEFS NOREFS NOSETS(EJECT);                               0472
@RC00416 DS    0H                                                  0472
         EJECT
*   IF LSCZRT00=ON×LSCZRT01=ON THEN /* WERE LOGICAL OR CONTROL BLOCK
*                                      ERROR FLAGS SET ON?           */
         TM    LSCZRT00(LSCBPTR),B'11000000'                       0473
         BZ    @RF00473                                            0473
*     CALL ZPSZBFZ0;                /* YES, CALL INVALID LCP RECOVERY*/
         L     @14,ATCZPSVT(,VTCVTREG)                             0474
         L     @15,ZPSZBFZ0(,@14)                                  0474
         BALR  @14,@15                                             0474
*   ELSE                            /* NO,(NEITHER ERROR FLAG ON)    */
*FAIDO060:                                                         0475
*     DO;                                                          0475
         B     @RC00473                                            0475
@RF00473 DS    0H                                                  0475
FAIDO060 DS    0H                                                  0476
*       FMCZX003=FMCZX003+1;        /* INCREMENT I/O SCHEDQ COUNT    */
         LA    @00,1                                               0476
         SLR   @15,@15                                             0476
         IC    @15,FMCZX003(,FMCBREG)                              0476
         ALR   @00,@15                                             0476
         STC   @00,FMCZX003(,FMCBREG)                              0476
*       IF LCCWLSI=ON THEN          /* IS LCCW SYNCH REQUESTED?      */
         L     @15,LSCLCCWA(,LSCBPTR)                              0477
         TM    LCCWLSI(@15),B'00000100'                            0477
         BNO   @RF00477                                            0477
*         CALL ZPSZBFY0;            /* YES, CALL LCCW SYNCH RTN      */
         L     @14,ATCZPSVT(,VTCVTREG)                             0478
         L     @15,ZPSZBFY0(,@14)                                  0478
         BALR  @14,@15                                             0478
*       ELSE                                                       0479
*         ;                         /* NO, (NULL)                    */
@RF00477 DS    0H                                                  0480
*     END FAIDO060;                                                0480
