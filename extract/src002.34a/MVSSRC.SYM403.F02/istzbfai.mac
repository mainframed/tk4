 TITLE 'ISTZBFAI - READ PROCESSOR FOR GENERAL POLL DEVICE - PROCMT'
*/********************************************************************/
*/*                                                                  */
*/* PROCMT: THIS ROUTINE COMPLETES PROCESSING OF READ MESSAGE AND    */
*/*         READ TRANSMISSION COMMANDS.                              */
*/*                                                                  */
*/*         NOTE: THE RETURN ADDRESS IS SAVED IN WORKREG2            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0492
*PROCMT:                                                           0492
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0492
         B     @PB00002                                            0492
PROCMT   DS    0H                                                  0493
*   RFY                                                            0493
*    (WORKREG2,                                                    0493
*     LNKREG) RSTD;                                                0493
*   WORKREG2=LNKREG;                /* SAVE RETURN ADDRESS           */
         LR    WORKREG2,LNKREG                                     0494
*   RFY                                                            0495
*     LNKREG UNRSTD;                                               0495
*   IF FMCZFA06=ON THEN             /* IS DEVICE IN SESSION?         */
         TM    FMCZFA06(FMCBREG),B'00000100'                       0496
         BO    @RT00496                                            0496
*     ;                             /* YES, (NULL)                   */
*   ELSE                            /* NO,                           */
*FAIDO070:                                                         0498
*     DO;                                                          0498
FAIDO070 DS    0H                                                  0499
*       FMCZFB06=ON;                /* SET ON INVITE OUTSTANDING FLAG*/
         OI    FMCZFB06(FMCBREG),B'00000010'                       0499
*       FMCZX001=FMCZX001+1;        /* INCREMENT APPLICATION SESSION
*                                      COUNT                         */
         LA    @08,1                                               0500
         SLR   @00,@00                                             0500
         IC    @00,FMCZX001(,FMCBREG)                              0500
         ALR   @08,@00                                             0500
         STC   @08,FMCZX001(,FMCBREG)                              0500
*     END FAIDO070;                                                0501
*   RFY                                                            0502
*     WORKREG RSTD;                                                0502
@RT00496 DS    0H                                                  0503
*   WORKREG=LSCBPTR;                /* SAVE WAITQ ELEMENT ADDRESS    */
         LR    WORKREG,LSCBPTR                                     0503
*   RLBFPTR=ZOBPTR;                 /* GET CURRENT BUFFER ADDRESS    */
         LR    RLBFPTR,ZOBPTR                                      0504
*   ZCRPCOD1='00'X;                 /* CLEAR UTILITY PARAM FLAGS     */
         L     @15,RPHCRR(,RPHREG)                                 0505
         L     @15,CRACRR(,@15)                                    0505
         MVI   ZCRPCOD1(@15),X'00'                                 0505
*   ZCRPBUF=ON;                     /* SET ON FLAG FOR RELEASE BUFFER
*                                      REQUEST                       */
         OI    ZCRPBUF(@15),B'00000100'                            0506
*   CALL ZPSZDFB0;                  /* CALL BUFFER DEALLOCATION      */
         L     @14,ATCZPSVT(,VTCVTREG)                             0507
         L     @15,ZPSZDFB0(,@14)                                  0507
         BALR  @14,@15                                             0507
*   RFY                                                            0508
*     RCDREG RSTD;                                                 0508
*   LSCBPTR=WORKREG;                /* RESTORE WAITQ ELEM ADDRESS    */
         LR    LSCBPTR,WORKREG                                     0509
*   RFY                                                            0510
*     WORKREG UNRSTD;                                              0510
*   IF RCDREG=GOODRC THEN           /* WAS BUFFER DEALLOCATION     0511
*                                      SUCCESSFUL?                   */
         C     RCDREG,@CF00583                                     0511
         BNE   @RF00511                                            0511
*FAIDO080:                                                         0512
*     DO;                           /* YES,                          */
FAIDO080 DS    0H                                                  0513
*       LSCFDBA=EMPTY;              /* NULLIFY LSCB BUFFER PTR       */
         SLR   @14,@14                                             0513
         ST    @14,LSCFDBA(,LSCBPTR)                               0513
*       LSCREQTG=ZERO;              /* SET GENERAL POLL REQ TAG      */
         STH   @14,LSCREQTG(,LSCBPTR)                              0514
*       FMCZF004=ON;                /* SET ON READ OUTSTANDING FL    */
*       FMCZF001=ON;                /* SET ON BTU ALLOC REQD FLAG    */
         OI    FMCZF004(FMCBREG),B'10010000'                       0516
*     END FAIDO080;                                                0517
*   ELSE                            /* NO, (NOT SUCCESSUL RC)        */
*FAIDO090:                                                         0518
*     DO;                                                          0518
         B     @RC00511                                            0518
@RF00511 DS    0H                                                  0518
FAIDO090 DS    0H                                                  0519
*       LSCZRT00=ON;                /* SET ON LOGICAL ERROR FLAG     */
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0519
*       LSCZMNRC=FRMVTAME;          /* SET MINOR RC AS VTAM ERROR    */
         MVI   LSCZMNRC(LSCBPTR),X'A0'                             0520
*     END FAIDO090;                                                0521
*   RFY                                                            0522
*     LNKREG RSTD;                                                 0522
@RC00511 DS    0H                                                  0523
*   LNKREG=WORKREG2;                /* RESTORE RETURN ADDRESS        */
         LR    LNKREG,WORKREG2                                     0523
*   RFY                                                            0524
*    (WORKREG2,                                                    0524
*     LNKREG) UNRSTD;                                              0524
*   END PROCMT;                                                    0525
@EL00002 DS    0H                                                  0525
@EF00002 DS    0H                                                  0525
@ER00002 BR    @14                                                 0525
@PB00002 DS    0H                                                  0526
*   GEN NODEFS NOREFS NOSETS;                                      0526
