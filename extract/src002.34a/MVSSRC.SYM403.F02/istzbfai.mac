 TITLE 'ISTZBFAI - READ PROCESSOR FOR GENERAL POLL DEVICE - PROCBM'
*/********************************************************************/
*/*                                                                  */
*/* PROCBM: THIS ROUTINE COMPLETES PROCESSING OF READ MODIFIED AND   */
*/*         READ BUFFER COMMANDS.                                    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0527
*   /*****************************************************************/
*   /*                                                               */
*   /* SEGMENT(PROCBM)START                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0527
*   DO;                                                            0527
*     GOTO PROCBM@;                                                0528
         B     PROCBM@                                             0528
*PROCBM:                                                           0529
*   END;                                                           0529
PROCBM   DS    0H                                                  0530
*   IF DEVTCODE=DEV3275×DEVTCODE=DEV3277×DEVTCODE=DEV3284×DEVTCODE=
*       DEV3286 THEN                /* IS DEVICE A 3270?             */
         LA    @14,36                                              0530
         ALR   @14,DNCBREG                                         0530
         CLI   DEVTCODE(@14),X'1C'                                 0530
         BE    @RT00530                                            0530
         CLI   DEVTCODE(@14),X'19'                                 0530
         BE    @RT00530                                            0530
         CLI   DEVTCODE(@14),X'1A'                                 0530
         BE    @RT00530                                            0530
         CLI   DEVTCODE(@14),X'1B'                                 0530
         BNE   @RF00530                                            0530
@RT00530 DS    0H                                                  0531
*FAIDO100:                                                         0531
*     DO;                           /* YES,                          */
FAIDO100 DS    0H                                                  0532
*       FDBDATAA=ADDR(ZOBBTU);      /* SET BUFFER DATA ADDRESS AS BTU
*                                      ADDRESS                       */
         LA    @14,ZOBBTU(,ZOBPTR)                                 0532
         ST    @14,FDBDATAA(,ZOBPTR)                               0532
*       BTUDCF=12;                  /* INITIALIZE COUNT      @XL03HAH*/
         LA    @14,12                                              0533
         STH   @14,BTUDCF+28(,ZOBPTR)                              0533
*       FDBDATCT=BTUDCF+LENGTH(ISTTH);/* SET BFR DATA CNT    @XM08930*/
         AL    @14,@CF02602                                        0534
         STH   @14,FDBDATCT(,ZOBPTR)                               0534
*       BDUCOMND=BCMDW;             /* BDU COMMAND IS WRITE          */
         MVI   BDUCOMND+42(ZOBPTR),X'02'                           0535
*FAIIF010:                                                         0536
*       IF(FMCZFA06=ON&FMCZFD06=ON)×(FMCZFA06=OFF&FMCZFB06=OFF&FMCZFC06
*           =OFF) THEN              /* IS EITHER (DEVICE IN SESSION
*                                      AND DISCONNECT OUTSTANDING) OR
*                                      (DEVICE NOT IN SESSION, INVITE
*                                      NOT OUTSTANDING, AND CONTACT
*                                      NOT OUTSTANDING)?             */
FAIIF010 TM    FMCZFA06(FMCBREG),B'00000100'                       0536
         BNO   @GL00006                                            0536
         TM    FMCZFD06(FMCBREG),B'10000000'                       0536
         BO    @RT00536                                            0536
