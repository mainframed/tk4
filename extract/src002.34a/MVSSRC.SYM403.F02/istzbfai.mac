         PRINT ON
*     GPR01P->RPHCRR->CRACRR->ZCREBCAI='1'B;                       0408
         L     @15,RPHCRR(,GPR01P)                                 0408
         L     @15,CRACRR(,@15)                                    0408
         OI    ZCREBCAI(@15),B'10000000'                           0408
*     ZSAZACTV='1'B;                                               0409
         OI    ZSAZACTV(GPR13P),B'10000000'                        0409
*     ZSAZTRAC='BCAI';                                             0410
         MVC   ZSAZTRAC(4,GPR13P),@CC02979                         0410
