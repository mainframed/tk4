*/********************************************************************/
*/*                                                                  */
*/*DNCBPRC:BGNSEGMENT (BLOCK).                               @AS33958*/
*/*                                                                  */
*/*FUNCTION IS TO DO ANY REQUIRED CLEANUP PROCESSING FOR THE DNCB.   */
*/*                                                                  */
*/*  IF THE DNCB IS AVAILABLE AND THE DEVICE IS NON-SNA              */
*/*    THEN                                                          */
*/*      CALL ISTZDFB0 (EXTERNAL), THE BUFFER DEALLOCATION           */
*/*          ROUTINE, TO PURGE THE FREE LSCB QUEUE, IF               */
*/*          NECESSARY.                                              */
*/*      IF IT IS A MULTI-COMPONENT DEVICE                           */
*/*        THEN                                                      */
*/*          DECREMENT THE SESSION COUNT IN THE DNCB BY ONE          */
*/*              IF MORE THAN ONE COMPONENT IS ACTIVE.               */
*/*          TURN OFF THE DISCONNECT OUTSTANDING FLAG IN THE DNCB.   */
*/*          CLEAR THE I/O SCHEDULED COUNT IN THE DNCB.              */
*/*      ENDIF (MULTI-COMPONENT DEVICE).                             */
*/*      IF THERE ARE MULTIPLE FMCB(S)                               */
*/*        THEN                                                      */
*/*          NOTE - THIS WILL BE TRUE ONLY FOR PORT SOLICITOR OR     */
*/*              CLUSTER SOLICITOR.                                  */
*/*          CLEAN UP THE SESSION FLAGS IN THE DNCB.                 */
*/*          ZERO THE POINTER IN THE DNCB TO THE ACTIVE FMCB.        */
*/*          SET THE TASK ID IN THE NCB TO THE VTAM TASK ID.         */
*/*      ENDIF (MULTIPLE FMCB(S).                                    */
*/*  ENDIF (DNCB IS AVAILABLE AND NON-SNA DEVICE).                   */
*/*ENDSEGMENT (DNCBPRC).                                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  1652
*   IF ICNCBPTR^=NCBPTR THEN        /* IF THE NCB IS AVAILABLE THEN  */
         CR    ICNCBPTR,NCBPTR                                     1652
         BE    @RF01652                                            1652
*     IF NCBTYPE=DNTYPE THEN        /* IF THE DEVICE IS NON-SNA THEN */
         CLI   NCBTYPE(NCBPTR),X'05'                               1653
         BNE   @RF01653                                            1653
*       DO;                         /* THEN CLEAN UP DNCB.           */
*         WKREG=ADDR(DNCZC001);     /*                       @ZA26159*/
         LA    WKREG,DNCZC001(,NCBPTR)                             1655
*         PABWQCHN=PABWQCHN&&PABWQCHN;/* ZERO WORK ELEMENT @ AND   1656
*                                      CHAIN POINTER.        @ZA26159*/
         XC    PABWQCHN(8,WKREG),PABWQCHN(WKREG)                   1656
*         PABERRLK=OFF;             /* CLEAR THE ERROR LOCK. @ZA26159*/
*         PABINHBT=OFF;             /* CLEAR THE INHIBIT FLG @ZA26159*/
         L     @00,PABINHBT(,WKREG)                                1658
@GL00022 LR    @15,@00                                             1658
         N     @15,@CF05608                                        1658
         CS    @00,@15,PABINHBT(WKREG)                             1658
         BNE   @GL00022                                            1658
*         PABRPHA=PABRPHA&&PABRPHA; /* ZERO RPH POINTER.     @ZA26159*/
         L     @15,PABRPHA-1(,WKREG)                               1659
         LA    @15,0(,@15)                                         1659
         LR    @00,@15                                             1659
         XR    @00,@15                                             1659
         STCM  @00,7,PABRPHA(WKREG)                                1659
*         IF DNCZX001^=0 THEN       /* IF THE FREE LSCB QUEUE IS TO  */
         CLI   DNCZX001(NCBPTR),0                                  1660
         BE    @RF01660                                            1660
*           DO;                     /* BE PURGED THEN                */
*             ZCRPCOD1=0;           /* CLEAR THE PARAMETER FLAGS.    */
         MVI   ZCRPCOD1(ZCRRPTR),X'00'                             1662
*             ZCRPLSBQ=ON;          /* INDICATE PURGE LSCB QUEUE.    */
         OI    ZCRPLSBQ(ZCRRPTR),B'00010000'                       1663
*             CALL ISTZDFB0;        /* CALL BUFFER DEALLOCATION RTN. */
         L     @14,ATCZPSVT(,VTCVTPTR)                             1664
         L     @15,ZPSZDFB0(,@14)                                  1664
         BALR  @14,@15                                             1664
*           END;                    /* END FREE LSCB QUEUE PURGED.   */
*         ELSE                                                     1666
*           ;                       /* FREE LSCB Q DOES NOT NEED TO
*                                      BE PURGED.                    */
@RF01660 DS    0H                                                  1667
*         IF NCBNSNA->NCBTYPE=DNTYPE THEN/* IF MULTI-COMPONENT       */
         L     @15,NCBNSNA(,NCBPTR)                                1667
         CLI   NCBTYPE(@15),X'05'                                  1667
         BNE   @RF01667                                            1667
*           DO;                     /* DEVICE THEN                   */
*             IF NCBNSNA->DNCZX003^=0 THEN/* IF MORE THAN ONE      1669
*                                      COMPONENT IS ACTIVE THEN      */
         CLI   DNCZX003(@15),0                                     1669
         BE    @RF01669                                            1669
*               NCBNSNA->DNCZX003=NCBNSNA->DNCZX003-1;/* DECREMENT 1670
*                                      THE SESSION COUNT BY ONE.     */
         SLR   @00,@00                                             1670
         IC    @00,DNCZX003(,@15)                                  1670
         BCTR  @00,0                                               1670
         STC   @00,DNCZX003(,@15)                                  1670
*             ELSE                                                 1671
*               ;                   /* NOT MORE THAN ONE COMPONENT 1671
*                                      ACTIVE.                       */
@RF01669 DS    0H                                                  1672
*             NCBNSNA->DNCDISC=OFF; /* TURN OFF DISCONNECT         1672
*                                      OUTSTANDING FLAG.             */
         L     @15,NCBNSNA(,NCBPTR)                                1672
         NI    DNCDISC(@15),B'11110111'                            1672
*             NCBNSNA->DNCACIP=0;   /* CLEAR THE SCHEDULE COUNT.     */
         MVI   DNCACIP(@15),X'00'                                  1673
*           END;                    /* END MULTI-COMP PROCESSING.    */
*         IF NCBFMCBA->FMCFMCBA^=0 THEN/* IF ANOTHER FMCB ACTIVE   1675
*                                      (PORT SOLICITOR OR CLUSTER    */
@RF01667 SLR   @15,@15                                             1675
         L     @14,NCBFMCBA(,NCBPTR)                               1675
         C     @15,FMCFMCBA(,@14)                                  1675
         BE    @RF01675                                            1675
*           DO;                     /* SOLICITOR) THEN               */
*             DNCACIP=0;            /* CLEAR THE SCHEDULE COUNT.     */
         MVI   DNCACIP(NCBPTR),X'00'                               1677
*             DNCZF001=OFF;         /* TURN OFF ERROR LCK INDICATOR. */
*             DNCZRSCH=ON;          /* INDICATE DEVICE RESCHEDULED.  */
*             DNCZF002=OFF;         /* TURN OFF DIALOGUE SCHEDULING
*                                      IS IN PROGRESS.               */
         OI    DNCZRSCH(NCBPTR),B'00000100'                        1680
         NI    DNCZF001(NCBPTR),B'01111101'                        1680
*             DNCZV001=0;           /* CLEAR ACTIVE FMCB POINTER.    */
         STCM  @15,7,DNCZV001(NCBPTR)                              1681
*             ADDR(DNCZC001)->PABWEQG=OFF;/* TURN OFF THE GATE BIT IN
*                                      THE TIP                       */
         LA    @15,96                                              1682
         ALR   @15,NCBPTR                                          1682
         NI    PABWEQG(@15),B'01111111'                            1682
*             ADDR(DNCZC001)->PABDVTA=DNCZV004;/* SET PAB DVT PTR TO
*                                      DIALOGUE SCHEDULER DVT.       */
         MVC   PABDVTA(3,@15),DNCZV004+1(NCBPTR)                   1683
*             NCBTSKID=ATCCDTID;    /* SET TASK ID TO VTAM TASK ID.  */
         L     @00,ATCCDTID(,VTCVTPTR)                             1684
         ST    @00,NCBTSKID(,NCBPTR)                               1684
*           END;                    /* END MULTIPLE FMCB(S).         */
*         ELSE                                                     1686
*           ;                       /* SINGLE FMCB.                  */
@RF01675 DS    0H                                                  1687
*       END;                        /* END NON-SNA DEVICE.           */
*     ELSE                                                         1688
*       DO;                         /* SNA DEVICE.                   */
         B     @RC01653                                            1688
@RF01653 DS    0H                                                  1689
*         IF NCBTYPE=LUTYPE THEN                                   1689
         CLI   NCBTYPE(NCBPTR),X'25'                               1689
         BNE   @RF01689                                            1689
*           DO;                     /* IS NCB LUNCB ?        @ZA26159*/
*             WKREG=ADDR(LUNZC001); /*                       @ZA26159*/
         LA    WKREG,LUNZC001(,NCBPTR)                             1691
*             PABWQCHN=PABWQCHN&&PABWQCHN;/* ZERO WORK ELEMENT @ AND
*                                      CHAIN POINTER.        @ZA26159*/
         XC    PABWQCHN(8,WKREG),PABWQCHN(WKREG)                   1692
*             PABERRLK=OFF;         /* CLEAR THE ERROR LOCK. @ZA26159*/
*             PABINHBT=OFF;         /* CLEAR THE INHIBIT FLG @ZA26159*/
         L     @00,PABINHBT(,WKREG)                                1694
@GL00023 LR    @15,@00                                             1694
         N     @15,@CF05608                                        1694
         CS    @00,@15,PABINHBT(WKREG)                             1694
         BNE   @GL00023                                            1694
*             PABRPHA=PABRPHA&&PABRPHA;/* ZERO RPH POINTER.  @ZA26159*/
         L     @15,PABRPHA-1(,WKREG)                               1695
         LA    @15,0(,@15)                                         1695
         LR    @00,@15                                             1695
         XR    @00,@15                                             1695
         STCM  @00,7,PABRPHA(WKREG)                                1695
*           END;                    /* NCB IS LUNCB          @ZA26159*/
*         FMCSNBI=OFF;              /* TURN OFF SESSION NOT BOUND  1697
*                                      FLAG.                 @AS33958*/
@RF01689 DS    0H                                                  1698
*         FMCCIPI=OFF;              /* TURN OFF CLEAR IN PROGRESS  1698
*                                      FLAG.                 @AS33958*/
*         FMCUIPI=OFF;              /* TURN OFF UNBIND IN PROGRESS 1699
*                                      FLAG.                 @AS33958*/
         NI    FMCSNBI(FMCBPTR),B'00011111'                        1699
*       END;                        /* END SNA DEVICE.               */
*   ELSE                                                           1701
*     ;                             /* NCB IS NOT AVAILABLE.         */
*                                                                  1701
@RF01652 DS    0H                                                  1702
