         TITLE 'IDA121A2--ACTUAL BLOCK PROCESSOR                       *
                        '
IDA121A2 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IDA121A2  78.179'                                 0001
         DROP  @15
@PROLOG  BALR  @11,0                                               0001
@PSTART  LA    @12,4095(,@11)                                      0001
         USING @PSTART,@11                                         0001
         USING @PSTART+4095,@12                                    0001
*   IOSBPTR=R1;                     /* SAVE IOSB POINTER             */
         LR    IOSBPTR,R1                                          0200
*   RESPECIFY                                                      0201
*     R1 UNRSTD;                                                   0201
*   IOMBPTR=IOSUSE;                 /* ESTABLISH IOMB POINTER        */
         L     IOMBPTR,IOSUSE(,IOSBPTR)                            0202
*   RESPECIFY                                                      0203
*    (IOSB) BASED(IOMIOSB);         /* ADDRESS OF IOSB               */
*   IOMSAVED=R13;                   /* SAVE REGISTER 13 CONTENTS     */
         ST    R13,IOMSAVED(,IOMBPTR)                              0204
*   IOMSAVE8=R14;                   /* SAVE RETURN REGISTER          */
         ST    R14,IOMSAVE8(,IOMBPTR)                              0205
*   RESPECIFY                                                      0206
*    (R13,                                                         0206
*     R14) UNRSTD;                                                 0206
*   IOMNBUF=ZERO;                   /* ZERO BUFC COUNT AREA          */
         SLR   @10,@10                                             0207
         STH   @10,IOMNBUF(,IOMBPTR)                               0207
*   IOMNSEG=ZERO;                   /* ZERO CP SEG COUNT AREA        */
         STH   @10,IOMNSEG(,IOMBPTR)                               0208
*   RBUFC=IOMBUFC;                  /* SET BUFC POINTER REGISTER     */
         L     RBUFC,IOMBUFC(,IOMBPTR)                             0209
*   RCPA=IOMCPA;                    /* SET CPA POINTER REGISTER      */
*                                                                  0210
         L     RCPA,IOMCPA(,IOMBPTR)                               0210
*   /*****************************************************************/
*   /*                                                               */
*   /* ESTABLISH EXISTENCE OF AN FRR                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0211
*   RESPECIFY                                                      0211
*    (RW1,                                                         0211
*     RW2,                                                         0211
*     RW3) RSTD;                                                   0211
*   RW1=ADDR(IDA121F2);                                            0212
         LA    RW1,IDA121F2                                        0212
*   GEN SETS(RW1,RW2,RW3,FRRSCURR,FRRSFRRA,FRRSPARM) REFS(PSACSTK, 0213
*       PSALSFCC,FRRS,FRRSENTR,FRRSLAST,FRRSELEN)                  0213
*       (SETFRR  A,FRRAD=(RW1),WRKREGS=(RW2,RW3),PARMAD=(RW1));    0213
         SETFRR  A,FRRAD=(RW1),WRKREGS=(RW2,RW3),PARMAD=(RW1)
*   IOMBADR=IOMBPTR;                /* IOMB ADDRESS INTO PARM FIELD  */
         ST    IOMBPTR,IOMBADR(,RW1)                               0214
*   BASER11=R11;                    /* FIRST BASE REG VALUE FOR FRR  */
         ST    R11,BASER11(,RW1)                                   0215
*   BASER12=R12;                    /* SECOND BASE REG VALUE FOR FRR */
         ST    R12,BASER12(,RW1)                                   0216
*   RESPECIFY                                                      0217
*    (RW2,                                                         0217
*     RW3) UNRSTD;                                                 0217
*                                                                  0217
*/*           BUILD CHANNEL PROGRAMS                                 */
*                                                                  0218
*CPGEN:                                                            0218
*                                                                  0218
*   /*****************************************************************/
*   /*                                                               */
*   /* IF BUFFER IS TO BE WRITTEN, BUILD CHANNEL PROGRAM SEGMENT FOR */
*   /* WRITE AND WRITE CHECK, IF REQUIRED                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0218
*   CPAFLAGS=ZERO;                                                 0218
CPGEN    SLR   @10,@10                                             0218
         STH   @10,CPAFLAGS(,RCPA)                                 0218
*   CPAWPHAD=CZERO;                 /* CLEAR WRITE ADDRESS AREA      */
         XC    CPAWPHAD+1(7,RCPA),CPAWPHAD+1(RCPA)                 0219
         MVI   CPAWPHAD(RCPA),B'00000000'                          0219
*   CPARPHAD=CZERO;                 /* CLEAR READ ADDRESS AREA       */
         XC    CPARPHAD+1(7,RCPA),CPARPHAD+1(RCPA)                 0220
         MVI   CPARPHAD(RCPA),B'00000000'                          0220
*   IOMNBUF=IOMNBUF+ONE;            /* INCREMENT BUFC COUNT          */
         LA    @10,1                                               0221
         LH    @06,IOMNBUF(,IOMBPTR)                               0221
         N     @06,@CF03366                                        0221
         ALR   @10,@06                                             0221
         STH   @10,IOMNBUF(,IOMBPTR)                               0221
*   RBABIT=ON;                      /* VARIABLE LGTH IND     @Z30SSLP*/
         OI    RBABIT(RCPA),B'10000000'                            0222
*   IF BUFCMW=ON THEN               /* WRITE REQUESTED               */
*                                                                  0223
         TM    BUFCMW(RBUFC),B'10000000'                           0223
         BNO   @RF00223                                            0223
*     /***************************************************************/
*     /*                                                             */
*     /* DO PRELIMINARY WRITE INITIALIZATION AND THEN BUILD FORMAT OR*/
*     /* UPDATE WRITE CHANNEL PROGRAM SEGMENT                        */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0224
*WRITE:                                                            0224
*     DO;                                                          0224
*                                                                  0224
WRITE    DS    0H                                                  0225
*       /*************************************************************/
*       /*                                                           */
*       /* SET MODE TO USER'S KEY WHILE SETTING BUFC BITS            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0225
*       SPKA(IOMCKEY);              /* SWITCH TO USER KEY    @Z40TSDT*/
         SLR   @10,@10                                             0225
         IC    @10,IOMCKEY(,IOMBPTR)                               0225
         SPKA  0(@10)                                              0225
*       BUFCEPT=OFF;                /* TURN OFF IF I/O TO DO         */
         NI    BUFCEPT(RBUFC),B'11111110'                          0226
*       GEN(MODESET EXTKEY=ZERO);   /* RESET KEY TO 0        @Z40TSDT*/
         MODESET EXTKEY=ZERO
*       RESPECIFY                                                  0228
*        (R0,                                                      0228
*         RLPMB,                                                   0228
*         R6,                                                      0228
*         RW2,                                                     0228
*         RW3,                                                     0228
*         R15) RSTD;                                               0228
*                                                                  0228
*       /*************************************************************/
*       /*                                                           */
*       /* CONVERT CHANGES RLPMB,R6,RW1,RW2,RW3,R14,R15,CPASSECT     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0229
*       R0=BUFCORBA;                /* OUTPUT RBA                    */
         L     R0,BUFCORBA(,RBUFC)                                 0229
*       CALL CONVERT;               /* GO CONVERT RBA TO MBBCCHHR    */
         BAL   @14,CONVERT                                         0230
*       CPAWSECT=CPASSECT;          /* SECTOR #              @Z30SSLP*/
         IC    @14,CPASSECT(,RCPA)                                 0231
         STC   @14,CPAWSECT(,RCPA)                                 0231
*       RESPECIFY                                                  0232
*        (R0,                                                      0232
*         R6,                                                      0232
*         RW1,                                                     0232
*         RW2,                                                     0232
*         RW3,                                                     0232
*         R15) UNRSTD;                                             0232
*       CPAWPHAD=CONVOUT;           /* SAVE SEEK ADDRESS             */
*                                                                  0233
         MVC   CPAWPHAD(8,RCPA),CONVOUT(IOMBPTR)                   0233
*       /*************************************************************/
*       /*                                                           */
*       /* TEST IF RBA CONVERTED                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0234
*       IF CONBB=ZERO THEN          /* EDB FOUND IN CONVERT ROUTINE  */
*                                                                  0234
         ICM   @10,3,CONBB(IOMBPTR)                                0234
         BNZ   @RF00234                                            0234
*         /***********************************************************/
*         /*                                                         */
*         /* PROCESS WRITE CHANNEL PROGRAM                           */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0235
*DOWRITE:                                                          0235
*         DO;                                                      0235
DOWRITE  DS    0H                                                  0236
*           RESPECIFY                                              0236
*            (SIDARG) BASED(RW4);                                  0236
*           RESPECIFY                                              0237
*            (RW4) RSTD;                                           0237
*           RW4=CPAWSID;            /* POINT TO SRCH ID ARG AREA     */
         L     RW4,CPAWSID(,RCPA)                                  0238
*           SIDARG=CONSID;          /* SAVE FIRST SRCH ID ARG        */
         MVC   SIDARG(5,RW4),CONSID(IOMBPTR)                       0239
*           IF LPMBTOFL=ON THEN     /* TRACK OVERFLOW                */
         TM    LPMBTOFL(RLPMB),B'00010000'                         0240
         BNO   @RF00240                                            0240
*             CPATKOFL=ON;          /* TRACK OVERFLOW BIT IN CPA     */
         OI    CPATKOFL(RCPA),B'00001000'                          0241
*           IF LPMREPL=ON&          /* REPLICATED WRITE      @Z30SSLC*/
*               BUFCFMT=OFF&        /* REPL UPDATE WRITE     @Z30SSLC*/
*               AMBLSR=OFF&AMBGSR=OFF THEN/* IF NO SHR RES   @Z30SSLC*/
@RF00240 TM    LPMREPL(RLPMB),B'01000000'                          0242
         BNO   @RF00242                                            0242
         TM    BUFCFMT(RBUFC),B'01000000'                          0242
         BNZ   @RF00242                                            0242
         L     @02,IOMAMB(,IOMBPTR)                                0242
         TM    AMBLSR(@02),B'01100000'                             0242
         BNZ   @RF00242                                            0242
*             DO;                                                  0243
*               CPAWREPL=ON;        /* REPL INDICATOR IN FIRST BUFC  */
         OI    CPAWREPL(RCPA),B'10000000'                          0244
*               RW4=BUFCBAD;        /* SAVE BUFFER ADDRESS           */
         L     RW4,BUFCBAD(,RBUFC)                                 0245
*               RBUFC=BUFDRPFB;     /* PREFORMATTED BUFC/CPA PTR     */
*                                                                  0246
         L     @02,AMBBUFC(,@02)                                   0246
         L     RBUFC,BUFDRPFB(,@02)                                0246
*               /*****************************************************/
*               /*                                                   */
*               /* SET MODE TO USER KEY WHILE SETTING BUFC BITS      */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0247
*               RESPECIFY                                          0247
*                (RW2) RSTD;        /*                       @Z40TSDT*/
*               SPKA(IOMCKEY);      /* SWITCH TO USER KEY    @Z40TSDT*/
         SLR   @09,@09                                             0248
         IC    @09,IOMCKEY(,IOMBPTR)                               0248
         SPKA  0(@09)                                              0248
*               BUFCBAD=RW4;        /* STORE BUFFER ADDR IN PF BUFC  */
         ST    RW4,BUFCBAD(,RBUFC)                                 0249
*               BUFCMW=ON;          /* WRITE REQUEST ON IN PF-BUFC   */
*               BUFCXEDB=OFF;                                      0251
         OI    BUFCMW(RBUFC),B'10000000'                           0251
         NI    BUFCXEDB(RBUFC),B'11111011'                         0251
*               BUFCEPT=OFF;        /* TURN OFF BIT IN THIS BUFC     */
         NI    BUFCEPT(RBUFC),B'11111110'                          0252
*               GEN(MODESET EXTKEY=ZERO);/* RESET KEY TO ZERO        */
         MODESET EXTKEY=ZERO
*               RESPECIFY                                          0254
*                (RW2) UNRSTD;      /*                       @Z40TSDT*/
*               CPABLKSZ=LPMBLKSZ;  /* BLOCKSIZE IN PF-CPA           */
         L     @02,LPMBLKSZ(,RLPMB)                                0255
         ST    @02,CPABLKSZ(,RCPA)                                 0255
*             END;                                                 0256
*           IF LPMBRPS=ON THEN      /* RPS DEV?              @Z30SSLP*/
@RF00242 TM    LPMBRPS(RLPMB),B'10000000'                          0257
         BNO   @RF00257                                            0257
*             CPAWRPS=ON;           /* INDICATE RPS DEVICE IN CPA    */
*                                                                  0258
         OI    CPAWRPS(RCPA),B'00010000'                           0258
*           /*********************************************************/
*           /*                                                       */
*           /* FORMAT WRITE                                          */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0259
*           IF BUFCFMT=ON×          /* FORMAT WRITE ON       @Z30SSLC*/
*               ((AMBLSR=ON×AMBGSR=ON)&LPMREPL=ON) THEN/* IF REPL  0259
*                                      WITH SHARED RESOURCES @Z30SSLC*/
@RF00257 TM    BUFCFMT(RBUFC),B'01000000'                          0259
         BO    @RT00259                                            0259
         L     @02,IOMAMB(,IOMBPTR)                                0259
         TM    AMBLSR(@02),B'01100000'                             0259
         BZ    @RF00259                                            0259
         TM    LPMREPL(RLPMB),B'01000000'                          0259
         BNO   @RF00259                                            0259
@RT00259 DS    0H                                                  0260
*             DO;                                                  0260
*                                                                  0260
*               /*****************************************************/
*               /*                                                   */
*               /* IF INDICATED, USE PREFORMATTED CP                 */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0261
*               IF BUFCPFCP=ON×     /*                       @Z30SSLC*/
*                   ((AMBLSR=ON×AMBGSR=ON)&LPMREPL=ON) THEN/* IF REPL
*                                      WITH SHARED RESOURCES @Z30SSLC*/
         TM    BUFCPFCP(RBUFC),B'00000010'                         0261
         BO    @RT00261                                            0261
         L     @02,IOMAMB(,IOMBPTR)                                0261
         TM    AMBLSR(@02),B'01100000'                             0261
         BZ    @RF00261                                            0261
         TM    LPMREPL(RLPMB),B'01000000'                          0261
         BNO   @RF00261                                            0261
@RT00261 DS    0H                                                  0262
*                 DO;                                              0262
*                                                                  0262
*                   /*************************************************/
*                   /*                                               */
*                   /* THE CHANNEL PROGRAM IS ASSUMED TO BE THE      */
*                   /* MAXIMUM SIZE REQUIRED TO WRITE ONE ENTIRE     */
*                   /* TRACK OF EITHER DATA OR INDEX. THE REMAINDER  */
*                   /* OF THE TRACK IS WRITTEN FROM THE BLOCK        */
*                   /* DETERMINED BY THE CONVERTED RBA.              */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0263
*                   RFY                                            0263
*                    (RW1) RSTD;                                   0263
*                   RFY                                            0264
*                     CPACCW BASED(CPAWCPS);                       0264
*                   IF CPAWRPS=ON THEN/* USE SET SECT IF RPS         */
         TM    CPAWRPS(RCPA),B'00010000'                           0265
         BNO   @RF00265                                            0265
*                     DO;                                          0266
*                       IOSVST=ADDR(CPACCWSS);/* ST ADDR     @Z30SSLP*/
         L     @06,IOMIOSB(,IOMBPTR)                               0267
         L     @02,CPAWCPS(,RCPA)                                  0267
         LA    @02,CPACCWSS(,@02)                                  0267
         ST    @02,IOSVST(,@06)                                    0267
*                       CPAWSECT=ZERO;/* SCTR IS 0           @Z30SSLP*/
         MVI   CPAWSECT(RCPA),X'00'                                0268
*                     END;                                         0269
*                   ELSE            /* NOT RPS                       */
*                     IOSVST=ADDR(CPACCWSI);/* ST ADDR       @Z30SSLP*/
         B     @RC00265                                            0270
@RF00265 L     @06,IOMIOSB(,IOMBPTR)                               0270
         L     @02,CPAWCPS(,RCPA)                                  0270
         LA    @02,CPACCWSI(,@02)                                  0270
         ST    @02,IOSVST(,@06)                                    0270
*                   IOSEEK=CPAWPHAD;/* START OF CP IN IOSB           */
@RC00265 L     @06,IOMIOSB(,IOMBPTR)                               0271
         MVC   IOSEEK(8,@06),CPAWPHAD(RCPA)                        0271
*                   CPAWPHR=CPAWPHR-ONE;/* SEARCH PREVIOUS R         */
         SLR   @06,@06                                             0272
         IC    @06,CPAWPHR(,RCPA)                                  0272
         BCTR  @06,0                                               0272
         STC   @06,CPAWPHR(,RCPA)                                  0272
*                   RW1=LPMBLKTR-CPAWPHR;/* NUMBER OF BLOCKS         */
         LCR   @06,@06                                             0273
         AH    @06,LPMBLKTR(,RLPMB)                                0273
         LR    RW1,@06                                             0273
*                   RW4=CPAWSID;    /* BASE FOR SIDARG               */
         L     RW4,CPAWSID(,RCPA)                                  0274
*                   SIDARG=CPAWCHR; /* BUILD SEARCH ID ARG           */
*                                                                  0275
         MVC   SIDARG(5,RW4),CPAWCHR(RCPA)                         0275
*                   /*************************************************/
*                   /*                                               */
*                   /* BUILD FORMAT WRITE WCKD FIELDS                */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0276
*                   RESPECIFY                                      0276
*                    (CNTFLD) BASED(CPAWORK1);                     0276
*                   CPAWORK1=CPAFWCNT;                             0277
         L     @06,CPAFWCNT(,RCPA)                                 0277
         ST    @06,CPAWORK1(,RCPA)                                 0277
*                   CNTCCHHR=CPAWCHR;                              0278
         MVC   CNTCCHHR(5,@06),CPAWCHR(RCPA)                       0278
*                   CNTR=CNTR+ONE;  /* INCREMENT RECORD              */
         LA    @02,1                                               0279
         SLR   @15,@15                                             0279
         IC    @15,CNTR(,@06)                                      0279
         ALR   @02,@15                                             0279
         STC   @02,CNTR(,@06)                                      0279
*                   CNTK=ZERO;      /* KEY IS ALWAYS ZERO            */
         MVI   CNTK(@06),X'00'                                     0280
*                   CNTDD=CPABLKSZ; /* DATA LENGTH                   */
         L     @02,CPABLKSZ(,RCPA)                                 0281
         STH   @02,CNTDD(,@06)                                     0281
*                   DO WHILE CNTR<LPMBLKTR;                        0282
         B     @DE00282                                            0282
@DL00282 DS    0H                                                  0283
*                     CPAWORK2=CPAWORK1+EIGHT;/* PNT NEXT ENTRY      */
         L     @06,CPAWORK1(,RCPA)                                 0283
         LA    @02,8                                               0283
         ALR   @02,@06                                             0283
         ST    @02,CPAWORK2(,RCPA)                                 0283
*                     CPAWORK2->CNTFLD=CNTFLD;                     0284
         MVC   CNTFLD(8,@02),CNTFLD(@06)                           0284
*                     CPAWORK1=CPAWORK2;/* PTR NEXT ENTRY            */
         ST    @02,CPAWORK1(,RCPA)                                 0285
*                     CNTR=CNTR+ONE;                               0286
         LA    @06,1                                               0286
         SLR   @15,@15                                             0286
         IC    @15,CNTR(,@02)                                      0286
         ALR   @06,@15                                             0286
         STC   @06,CNTR(,@02)                                      0286
*                   END;                                           0287
*                                                                  0287
@DE00282 L     @06,CPAWORK1(,RCPA)                                 0287
         SLR   @02,@02                                             0287
         IC    @02,CNTR(,@06)                                      0287
         CH    @02,LPMBLKTR(,RLPMB)                                0287
         BL    @DL00282                                            0287
*                   /*************************************************/
*                   /*                                               */
*                   /* ONLY ONE BUFC IF THIS IS A PREFORMATTED WRITE */
*                   /* CHANNEL PROGRAM                               */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0288
*                   RESPECIFY                                      0288
*                    (RW2) RSTD;                                   0288
*                   RW2=SIXTEEN*RW1;/* TWO CCW'S PER BLK-WRITE       */
         LR    RW2,RW1                                             0289
         SLA   RW2,4                                               0289
*                   RW4=CPAWCPS+THIRTY2+RW2;/* PTR LAST WRITE CCW  0290
*                                                            @Z30SSLP*/
         LA    @09,32                                              0290
         L     RW4,CPAWCPS(,RCPA)                                  0290
         ALR   RW4,@09                                             0290
         ALR   RW4,RW2                                             0290
*                   CPAWORK1=RW4;   /* PTR TO LAST WRITE CCW         */
         ST    RW4,CPAWORK1(,RCPA)                                 0291
*                   IOMNSEG=IOMNSEG+ONE;/* COUNT SEGMENT             */
         LA    @06,1                                               0292
         LH    @02,IOMNSEG(,IOMBPTR)                               0292
         N     @02,@CF03366                                        0292
         ALR   @02,@06                                             0292
         STH   @02,IOMNSEG(,IOMBPTR)                               0292
*                   CPAWV=ON;       /* SET WRITE CP VALID            */
         OI    CPAWV(RCPA),B'10000000'                             0293
*                   IF BUFCWC=ON THEN/* WRITE-CHECK CP EXISTS        */
         TM    BUFCWC(RBUFC),B'00001000'                           0294
         BNO   @RF00294                                            0294
*                     DO;                                          0295
*                       RESPECIFY                                  0296
*                        (CCW) BASED(RW4);                         0296
*                       RW4=CPAWCKS;/* WRITE CHECK START             */
*                                                                  0297
         L     @15,CPAWCKS(,RCPA)                                  0297
         LR    RW4,@15                                             0297
*                       /*********************************************/
*                       /*                                           */
*                       /* CHAIN WRITE CP TO WRITE-CHECK CP          */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0298
*                       RW4=CPAWORK1;/* PTR TO LAST WRITE CCW        */
         L     @14,CPAWORK1(,RCPA)                                 0298
         LR    RW4,@14                                             0298
*                       IOMNSEG=IOMNSEG+ONE;/* COUNT SEGMENT         */
         ALR   @02,@06                                             0299
         STH   @02,IOMNSEG(,IOMBPTR)                               0299
*                       CPAWCV=ON;  /* SET WRITE CHECK INDICATOR     */
         OI    CPAWCV(RCPA),B'01000000'                            0300
*                       CCWCC=TIC;                                 0301
         MVI   CCWCC(RW4),B'00001000'                              0301
*                       CCWDA=CPAWCKS+SIXTEEN;/*             @Z30SSMR*/
         LA    @06,16                                              0302
         ALR   @06,@15                                             0302
         STCM  @06,7,CCWDA(RW4)                                    0302
*                       CCWTICDA=CCWDA;/* SAVE VIRT ADDR             */
         STCM  @06,7,CCWTICDA(RW4)                                 0303
*                       RW2=EIGHT*RW1;/* CHECK HAS 1 CCW PER BLK     */
         LR    RW2,RW1                                             0304
         SLA   RW2,3                                               0304
*                       RW4=CPAWCKS+THIRTY2+RW2;/* LAST CHK CCW    0305
*                                                            @Z30SSLP*/
         ALR   @15,@09                                             0305
         ALR   @15,RW2                                             0305
         LR    RW4,@15                                             0305
*                       CPAWORK2=RW4;/* POINT LAST CCW IN CHAIN      */
         ST    RW4,CPAWORK2(,RCPA)                                 0306
*                     END;                                         0307
*                   CCW1=CCWNOP;    /* BUILD NOP CCW                 */
@RF00294 MVC   CCW1(8,RW4),@CB00137                                0308
*                   GO TO STARTIOX; /* CHANNEL PGM COMPLETED @Z30SSLP*/
         B     STARTIOX                                            0309
*                 END;              /* ONE BUFC FOR REPLICATED INDEX */
*               ELSE                /* BUFCPFCP=OFF                  */
*                                                                  0311
*                 /***************************************************/
*                 /*                                                 */
*                 /* SHOULD NOT BE A REPLICATED WRITE                */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0311
*                 IF LPMREPL=ON THEN/* ERROR CONDITION               */
@RF00261 TM    LPMREPL(RLPMB),B'01000000'                          0311
         BNO   @RF00311                                            0311
*                   DO;                                            0312
*                     RESPECIFY                                    0313
*                      (RW1,                                       0313
*                       RW2) UNRSTD;                               0313
*                     RBUFC=IOMBUFC;/* SET SWITCHES IN THIS BUFC     */
*                                                                  0314
         L     RBUFC,IOMBUFC(,IOMBPTR)                             0314
*                     /***********************************************/
*                     /*                                             */
*                     /* SET MODE TO USER KEY TO SET BUFC BITS       */
*                     /*                                             */
*                     /***********************************************/
*                                                                  0315
*                     SPKA(IOMCKEY);/* SWITCH TO USER KEY    @Z40TSDT*/
         SLR   @02,@02                                             0315
         IC    @02,IOMCKEY(,IOMBPTR)                               0315
         SPKA  0(@02)                                              0315
*                     BUFCEPT=ON;   /* SET COMPLETE BIT ON           */
         OI    BUFCEPT(RBUFC),B'00000001'                          0316
*                     BUFCMW=OFF;   /* WRITE REQUEST IN BUFC OFF     */
