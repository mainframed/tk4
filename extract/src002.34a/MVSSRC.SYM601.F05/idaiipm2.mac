         TITLE 'IDAIIPM2--ISAM INTERFACE SCAN                          *
                        '
IDAIIPM2 CSECT ,                                                   0001
@PROLOG  DS    0H                                                  0002
@PSTART  EQU   *                           LABEL FOR PATCH MACRO
         DC    A(IIGET)                    /* ENTRY ADDR FOR GET    */
         DC    A(IISETL)                   /* ENTRY ADDR FOR SETL   */
         DC    A(IIESETL)                  /* ENTRY ADDR FOR ESETL  */
         DC    A(IIEODAD)                  /* ENTRY ADDR FOR EODAD  */
* /******************************************************************/
* /*                                                                */
* /*               BRANCH TABLE                                     */
* /*                                                                */
* /******************************************************************/
         USING *,R15                       /* ENTRY ADDRESSABILITY  */
IIGET    B     GETRTNE                     /* ENTRY FOR GET         */
         B     IIPUTX                      /* BRANCH TO PUTX EP     */
         B     IIRELSE                     /* BRANCH TO RELSE EP    */
*        /* CHANGE ADDRESSABILITY REGISTER                          */
         DROP  R15                         /* STOP USING R15 AS BASE*/
         USING IIGET,R11                   /* USE R11 AS A BASE REG */
         EJECT                             /* SKIP TO NEW PAGE      */
* /******************************************************************/
* /*                                                                */
* /*                 SETL MACRO ENTRY POINT                         */
* /*                                                                */
* /******************************************************************/
IISETL   STM   R14,R12,12(R13)             /* SAVE USER'S REGS      */
*   R11=DCBGET;                     /* ESTABLISH BASE REG            */
         L     R11,DCBGET+48(,RDCB)                                0168
*   RIICB=DEBWKPT5;                 /* SET IICB BASE                 */
         L     @14,DCBDEBAD+40(,RDCB)                              0169
         L     @14,DEBEXPT(,@14)                                   0169
         L     RIICB,DEBWKPT5(,@14)                                0169
*   IIREGBC=R13;                    /* KEEP PTR TO USER SAVE         */
         ST    R13,IIREGBC(,RIICB)                                 0170
*   R15=ADDR(IIREGSAV);             /* HOLD II SAVE ADDR             */
         LA    R15,IIREGSAV(,RIICB)                                0171
*   GEN(ST R15,8(0,R13));           /* STORE IT IN USER SAVE         */
         ST R15,8(0,R13)
*   R13=R15;                        /* ESTABLISH SAVE PTR            */
         LR    R13,R15                                             0173
*   RRPL=IIRPLPTR;                  /* ESTABLISH RPL BASE            */
         L     RRPL,IIRPLPTR(,RIICB)                               0174
*   IIKEYPT=R0;                     /* SETL TYPE CODE                */
         ST    R0,IIKEYPT(,RIICB)                                  0175
*   RFY                                                            0176
*    (R0,                                                          0176
*     R15) UNRSTD;                  /* DROP REGISTERS                */
*   DCBEXCD1=CZERO;                 /* CLEAR ERROR FLAGS             */
         LA    @12,52                                              0177
         ALR   @12,RDCB                                            0177
         MVI   DCBEXCD1(@12),B'00000000'                           0177
*   DCBEXCD2=CZERO;                 /* CLEAR ERROR FLAGS CONSECUTIVE
*                                      SETL'S?                       */
         MVI   DCBEXCD2(@12),B'00000000'                           0178
*   IF IIFSCAN=ON THEN              /* SCAN BIT ON?                  */
         TM    IIFSCAN(RIICB),B'10000000'                          0179
         BO    @RT00179                                            0179
*     GOTO INVALID;                 /* YES, --- INVALID REQ          */
*/* ******************************************************************/
*/*                                                                  */
*/*         DETERMINE SEARCH LOGIC FOR VSAM POINT:                   */
*/*                                                                  */
*/*            ISAM SETL K = VSAM POINT KEQ, FKS                     */
*/*            ISAM SETL KH = VSAM POINT KGE, FKS                    */
*/*            ISAM SETL KC = VSAM POINT GEN, KEQ            @XA05682*/
*/*            ISAM SETL B  = VSAM POINT KGE, GEN WITH KEY = ZERO    */
*/*                           AND VSAM KEY LENGTH FOR GENERIC SETL   */
*/*                                                                  */
*/* ******************************************************************/
*                                                                  0181
*   RPLGEN=OFF;                     /* ASSUME A FULL KEY SCH         */
*   RPLKGE=OFF;                     /* AND FOR KEY EQUAL             */
         NI    RPLGEN(RRPL),B'11111001'                            0182
*   RPLARG=KEYPTR;                  /* PUT KEY POINTER IN RPL        */
         L     @12,IIREGBC(,RIICB)                                 0183
         L     @10,KEYPTR-1(,@12)                                  0183
         LA    @10,0(,@10)                                         0183
         ST    @10,RPLARG(,RRPL)                                   0183
*   IF SETLDAT=ON THEN              /* IF DATA ONLY RETRIEVAL,       */
         TM    SETLDAT(@12),B'00001000'                            0184
         BNO   @RF00184                                            0184
*     IIDATA=ON;                    /* SET FLAG IN THE               */
         OI    IIDATA(RIICB),B'00001000'                           0185
*   ELSE                                                           0186
*     IIDATA=OFF;                   /* IICB.                         */
         B     @RC00184                                            0186
@RF00184 NI    IIDATA(RIICB),B'11110111'                           0186
*   IF SETLKEY=ON THEN              /* IF SETL BY KEY, CHECK         */
@RC00184 L     @12,IIREGBC(,RIICB)                                 0187
         TM    SETLKEY(@12),B'10000000'                            0187
         BNO   @RF00187                                            0187
*     DO;                           /* SEARCH LOGIC DESIRED          */
*       IF SETLKGE=ON THEN          /* IF SEARCH HIGH / EQUAL        */
         TM    SETLKGE(@12),B'00000010'                            0189
         BNO   @RF00189                                            0189
*         RPLKGE=ON;                /* INDICATE GT/EQ SEARCH         */
         OI    RPLKGE(RRPL),B'00000100'                            0190
*                                                                  0191
*       /*************************************************************/
*       /*                                                           */
*       /* IF GENERIC, DETERMINE NUMBER OF KEY BYTES FOR SEARCH.     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0191
*       ELSE                        /* IF NOT SEARCH HIGH OR         */
*         IF SETLGEN=ON THEN        /* EQUAL, BUT GENERIC            */
         B     @RC00189                                            0191
@RF00189 L     @12,IIREGBC(,RIICB)                                 0191
         TM    SETLGEN(@12),B'01000000'                            0191
         BNO   @RF00191                                            0191
*           DO;                     /* SEARCH, INDICATE A            */
*             RPLGEN=ON;            /* GENERIC SEARCH TO VSAM SEARCH
*                                      KEQ TO ALLOW KEY NOT FOUND  0193
*                                                            @XA05682*/
         OI    RPLGEN(RRPL),B'00000010'                            0193
*             RFY                                                  0194
*               RWORK1 RSTD;        /* GET WORK REGISTER             */
*             RWORK1=KEYPTR+DCBKEYLE-ONE;/* A(LAST KEY BYTE)         */
         L     RWORK1,KEYPTR-1(,@12)                               0195
         LA    RWORK1,0(,RWORK1)                                   0195
         SLR   @12,@12                                             0195
         IC    @12,DCBKEYLE+16(,RDCB)                              0195
         ALR   RWORK1,@12                                          0195
         BCTR  RWORK1,0                                            0195
*             DO WHILE KEYBYTE=CZERO;/* LOOP UNTIL WE LOCATE A       */
         B     @DE00196                                            0196
@DL00196 DS    0H                                                  0197
*               RWORK1=RWORK1-ONE;  /* NON-ZERO KEY BYTE             */
         BCTR  RWORK1,0                                            0197
*             END;                  /* END OF SEARCH                 */
@DE00196 CLI   KEYBYTE(RWORK1),B'00000000'                         0198
         BE    @DL00196                                            0198
*             RPLKEYLE=RWORK1-KEYPTR+ONE;/* SET LENGTH IN RPL        */
         LA    @12,1                                               0199
         L     @10,IIREGBC(,RIICB)                                 0199
         LR    @07,RWORK1                                          0199
         L     @10,KEYPTR-1(,@10)                                  0199
         LA    @10,0(,@10)                                         0199
         SLR   @07,@10                                             0199
         ALR   @07,@12                                             0199
         STH   @07,RPLKEYLE(,RRPL)                                 0199
*             IF RPLKEYLE<ONE THEN  /* SEE IF KEY ALL 0              */
         CR    @07,@12                                             0200
         BL    @RT00200                                            0200
*               GO TO INVALID;      /* IF SO, INVALID REQ            */
*           END;                    /* END OF GENERIC CODE           */
*         ELSE                                                     0203
*           ;                       /* HERE IF NOT GEN/KH            */
@RF00191 DS    0H                                                  0204
*     END;                          /* END OF SETL K CODE            */
*                                                                  0204
*   /*****************************************************************/
*   /*                                                               */
*   /* IF SETL TO BEGINNING, SET DUMMY KEY OF ZERO FOR SEARCH.       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0205
*   ELSE                            /* IF NOT A SETL K, SEE IF       */
*     DO;                                                          0205
         B     @RC00187                                            0205
@RF00187 DS    0H                                                  0206
*       IF SETLBEG=ON THEN          /* IT IS A SETL B.               */
         L     @12,IIREGBC(,RIICB)                                 0206
         TM    SETLBEG(@12),B'00010000'                            0206
         BNO   @RF00206                                            0206
*         CALL SETLB;               /* IF SO, POINT TO ZERO          */
         BAL   @14,SETLB                                           0207
*       ELSE                        /* IF NONE OF THE ABOVE,         */
*         DO;                       /* IT IS AN UNKNOWN SETL         */
         B     @RC00206                                            0208
@RF00206 DS    0H                                                  0209
*INVALID:                                                          0209
*           DCBEXINV=ON;            /* INDICATE INVALID REQ          */
INVALID  OI    DCBEXINV+52(RDCB),B'00010000'                       0209
*           GO TO ERROR;            /* EXIT                          */
         B     ERROR                                               0210
*         END;                      /* END OF INVALID REQ CODE       */
*     END;                          /* END OF NON-SETL K CODE        */
@RC00206 DS    0H                                                  0213
*                                                                  0213
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE VSAM POINT. IF THE SETL WAS BY KEY, ISSUE A GET SO THAT */
*   /* IF THE RECORD IS MARKED FOR DELETION A NO RECORD FOUND        */
*   /* CONDITION CAN BE INDICATED.                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0213
*   RWORK1=RDCB;                    /* SAVE DCB ADDRESS              */
@RC00187 LR    RWORK1,RDCB                                         0213
*   RDCB=RRPL;                      /* GET RPL ADDRESS INTO R1 ISSUE
*                                      VSAM POINT                    */
         LR    RDCB,RRPL                                           0214
*   GEN(POINT RPL=(1));             /*                               */
         POINT RPL=(1)
*   RDCB=RWORK1;                    /* RESTORE DCB BASE              */
         LR    RDCB,RWORK1                                         0216
*   IF RPLERRCD=RPLNOPLH THEN       /* IF NO PLACEHOLDER,            */
         CLI   RPLERRCD(RRPL),64                                   0217
         BE    @RT00217                                            0217
*     GO TO INVALID;                /* SET INVALID REQUEST           */
*   IF RPLGEN=OFF&                  /* IF SETL BY KEY (NOT B         */
*       RPLKGE=OFF&                 /* H OR C) CHECK FOR A           */
*       DCBOPTL=ON THEN             /* SETL TO A DELETED REC.        */
         TM    RPLGEN(RRPL),B'00000110'                            0219
         BNZ   @RF00219                                            0219
         TM    DCBOPTL+52(RDCB),B'00000010'                        0219
         BNO   @RF00219                                            0219
*     DO;                           /* SET NO RECORD FOUND           */
*       RPLAREA=RPLIIBFR;           /* POINT TO II BUFFER            */
         L     @12,RPLIIBFR+76(,RRPL)                              0221
         ST    @12,RPLAREA(,RRPL)                                  0221
*       IF DCBRECL='01'B THEN       /* IF VLR, LEAVE RDW SPACE AT  0222
*                                      BEGINNING OF BUFFER           */
         TM    DCBRECL+28(RDCB),B'01000000'                        0222
         BNO   @RF00222                                            0222
         TM    DCBRECL+28(RDCB),B'10000000'                        0222
         BNZ   @RF00222                                            0222
*         RPLAREA=RPLAREA+LENGTH(ALLRDW);/* POINT PAST RDW           */
         AL    @12,@CF00183                                        0223
         ST    @12,RPLAREA(,RRPL)                                  0223
*       RWORK1=RDCB;                /* SAVE DCB ADDRESS              */
@RF00222 LR    RWORK1,RDCB                                         0224
*       RDCB=RRPL;                  /* GET A (RPL) IN R1 ISSUE VSAM
*                                      GET                           */
         LR    RDCB,RRPL                                           0225
*       GEN(GET   RPL=(1));         /*                               */
         GET   RPL=(1)
*       RDCB=RWORK1;                /* RESTORE DCB ADDRESS           */
         LR    RDCB,RWORK1                                         0227
*       IF DCBRECL='01'B THEN       /* SEE IF VLR                    */
         TM    DCBRECL+28(RDCB),B'01000000'                        0228
         BNO   @RF00228                                            0228
         TM    DCBRECL+28(RDCB),B'10000000'                        0228
         BNZ   @RF00228                                            0228
*         DO;                       /* IF SO, MAKE AN RDW            */
*           RPLIIBFR->RDW=RPLRLEN+LENGTH(ALLRDW);/* SET RDW LENGTH   */
         L     @12,RPLIIBFR+76(,RRPL)                              0230
         LA    @10,4                                               0230
         AL    @10,RPLRLEN(,RRPL)                                  0230
         STH   @10,RDW(,@12)                                       0230
*           RPLIIBFR->RDW00=ZERO;   /* SET UNUSED TO ZERO            */
         SLR   @10,@10                                             0231
         STH   @10,RDW00(,@12)                                     0231
*         END;                      /* END OF VLR RDW CODE           */
*       IF DCBRKP=ZERO THEN         /* FIND DELETE CODE BYTE         */
@RF00228 LH    @12,DCBRKP+52(,RDCB)                                0233
         LTR   @12,@12                                             0233
         BNZ   @RF00233                                            0233
*         RWORK1=RPLAREA+DCBKEYLE;  /* POINT TO 1ST DATA BYTE        */
         SLR   RWORK1,RWORK1                                       0234
         IC    RWORK1,DCBKEYLE+16(,RDCB)                           0234
         AL    RWORK1,RPLAREA(,RRPL)                               0234
*       ELSE                                                       0235
*         RWORK1=RPLAREA;           /* ALLOW FOR KEY IF RKP=0        */
         B     @RC00233                                            0235
@RF00233 L     RWORK1,RPLAREA(,RRPL)                               0235
*       IF DATABYT1=DELCODE THEN    /* IF DELETED RECORD,            */
@RC00233 CLI   DATABYT1(RWORK1),B'11111111'                        0236
         BNE   @RF00236                                            0236
*         DO;                       /* SET NO RECORD FOUND           */
*           DCBEXNKY=ON;            /* INDICATE KEY NOT FOUND        */
         OI    DCBEXNKY+52(RDCB),B'10000000'                       0238
*           GO TO ERROR;            /* EXIT                          */
         B     ERROR                                               0239
*         END;                      /* END OF NO RECORD FOUND        */
*       IIFGET=ON;                  /* TELL 1ST GET REC HERE         */
@RF00236 OI    IIFGET(RIICB),B'01000000'                           0241
*     END;                          /* END OF DELETE REC CHECK       */
*   DCBEXOFL=ON;                    /* INDICATE OVERFLOW             */
@RF00219 OI    DCBEXOFL+52(RDCB),B'00010000'                       0243
*   IIFSCAN=ON;                     /* INDICATE GOOD SETL            */
         OI    IIFSCAN(RIICB),B'10000000'                          0244
*   GO TO PM2EXIT;                  /* RETURN TO USER                */
         B     PM2EXIT                                             0245
*                                                                  0246
*/* **************************************************************** */
*/*                                                                  */
*/*        GET MACRO ENTRY POINT FROM BRANCH TABLE                   */
*/*                                                                  */
*/* **************************************************************** */
*                                                                  0246
*GETRTNE:                           /* TARGET FROM BRANCH TBL        */
*   GEN(STM R14,R12,12(R13));       /* SAVE USER'S REGS              */
GETRTNE  STM R14,R12,12(R13)
*   RFY                                                            0247
*    (R0,                                                          0247
*     R15) RSTD;                    /* GET REGISTERS                 */
*   R11=R15;                        /* LOAD BASE ADDR                */
         LR    R11,R15                                             0248
*   RIICB=DEBWKPT5;                 /* SET IICB BASE                 */
         L     @14,DCBDEBAD+40(,RDCB)                              0249
         L     @14,DEBEXPT(,@14)                                   0249
         L     RIICB,DEBWKPT5(,@14)                                0249
*   IIREGBC=R13;                    /* SAVE BACKCHAIN PTR            */
         ST    R13,IIREGBC(,RIICB)                                 0250
*   R15=ADDR(IIREGSAV);             /* HOLD II SAVE ADDR             */
         LA    R15,IIREGSAV(,RIICB)                                0251
*   GEN(ST  R15,8(0,R13));          /* STORE IT IN USER SAVE         */
         ST  R15,8(0,R13)
*   R13=R15;                        /* II SAVE PTR IN R13            */
         LR    R13,R15                                             0253
*   RRPL=IIRPLPTR;                  /* ESTABLISH RPL BASE            */
         L     RRPL,IIRPLPTR(,RIICB)                               0254
*   RFY                                                            0255
*     R8 RSTD;                      /* GET A WORK REGISTER           */
*   R8=R0;                          /* SAVE USER BFR ADDRESS         */
         LR    R8,R0                                               0256
*   RFY                                                            0257
*    (R0,                                                          0257
*     R15) UNRSTD;                  /* DROP REGISTERS                */
*   DCBEXCD1=CZERO;                 /* CLEAR THE ERROR FLAGS         */
         LA    @12,52                                              0258
         ALR   @12,RDCB                                            0258
         MVI   DCBEXCD1(@12),B'00000000'                           0258
*   DCBEXCD2=CZERO;                 /* IN THE DCB                    */
*                                                                  0259
         MVI   DCBEXCD2(@12),B'00000000'                           0259
*   /*****************************************************************/
*   /*                                                               */
*   /* IF NOT YET IN SCAN MODE, A SETL BD IS IMPLIED.                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0260
*   IF IIFSCAN=OFF THEN             /* IF GET WITHOUT SETL,          */
         TM    IIFSCAN(RIICB),B'10000000'                          0260
         BNZ   @RF00260                                            0260
*     DO;                           /* A SETL B IS IMPLIED.          */
*       CALL SETLB;                 /* SET UP RPL FOR POINT          */
         BAL   @14,SETLB                                           0262
*       RWORK1=RDCB;                /* SAVE DCB ADDRESS              */
         LR    RWORK1,RDCB                                         0263
*       RDCB=RRPL;                  /* GET RPL ADDRESS INTO R1 ISSUE
*                                      VSAM POINT                    */
         LR    RDCB,RRPL                                           0264
*       GEN(POINT RPL=(1));         /*                               */
         POINT RPL=(1)
*       RDCB=RWORK1;                /* RESTORE DCB BASE              */
         LR    RDCB,RWORK1                                         0266
*       KEYADDR=R8;                 /* RESTORE USER REG 0            */
         L     @12,IIREGBC(,RIICB)                                 0267
         ST    R8,KEYADDR(,@12)                                    0267
*       IF RPLERRCD=RPLNOPLH THEN   /* IF NO PLACEHOLDER,            */
         CLI   RPLERRCD(RRPL),64                                   0268
         BE    @RT00268                                            0268
*         GO TO INVALID;            /* SET INVALID REQUEST           */
*       IIFSCAN=ON;                 /* INDICATE SCAN                 */
*       IIDATA=OFF;                 /* SET KEY/DATA RETRIEVAL        */
         OI    IIFSCAN(RIICB),B'10000000'                          0271
         NI    IIDATA(RIICB),B'11110111'                           0271
*     END;                          /* END OF IMPLIED SETL B         */
*                                                                  0273
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THIS IS THE FIRST GET AFTER A SETL (KEQ AND FKS) THEN THE  */
*   /* FIRST VSAM GET HAS ALREADY BEEN ISSUED. ALL THAT IS NECESSARY */
*   /* IS TO MOVE THE RECORD FROM THE ISAM INTERFACE BUFFER TO THE   */
*   /* USER AREA                                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0273
*   IF IIFGET=ON THEN               /* WAS FIRST RECORD READ DURING  */
@RF00260 TM    IIFGET(RIICB),B'01000000'                           0273
         BNO   @RF00273                                            0273
*     DO;                           /* SETL? IF SO, BYPASS VSAM      */
*       IIFGET=OFF;                 /* GET & RESET FIRST GET BIT     */
         NI    IIFGET(RIICB),B'10111111'                           0275
*       IF DCBMFLCG=OFF&            /* IF RECORD WOULD HAVE BEEN     */
*           IIDATA=OFF THEN         /* READ INTO THE USER BUFFER,    */
         TM    DCBMFLCG+40(RDCB),B'00001000'                       0276
         BNZ   @RF00276                                            0276
         TM    IIDATA(RIICB),B'00001000'                           0276
         BNZ   @RF00276                                            0276
*         DO;                       /* THEN MOVE IT THERE.           */
*           RFY                                                    0278
*            (R4,                                                  0278
*             R5,                                                  0278
*             R6,                                                  0278
*             R7) RSTD;             /* GET REGS FOR MVCL             */
*           IF DCBRECBR=OFF&        /* IF RECORDS ARE UNBLOCKED      */
*               DCBRKP^=ZERO THEN   /* AND KEYS ARE EMBEDDED,        */
         TM    DCBRECBR+28(RDCB),B'00010000'                       0279
         BNZ   @RF00279                                            0279
         LH    @12,DCBRKP+52(,RDCB)                                0279
         LTR   @12,@12                                             0279
         BZ    @RF00279                                            0279
*             R4=R8+DCBKEYLE;       /* ALLOW FOR KEY MOVE            */
         LR    R4,R8                                               0280
         SLR   @12,@12                                             0280
         IC    @12,DCBKEYLE+16(,RDCB)                              0280
         ALR   R4,@12                                              0280
*           ELSE                                                   0281
*             R4=R8;                /* ELSE, MOVE TO BUFFER START    */
         B     @RC00279                                            0281
@RF00279 LR    R4,R8                                               0281
*           R6=RPLIIBFR;            /* GET FROM ADDRESS              */
@RC00279 L     R6,RPLIIBFR+76(,RRPL)                               0282
*           IF DCBRECL='01'B THEN   /* IF VLR, LENGTH FROM RDW       */
         TM    DCBRECL+28(RDCB),B'01000000'                        0283
         BNO   @RF00283                                            0283
         TM    DCBRECL+28(RDCB),B'10000000'                        0283
         BNZ   @RF00283                                            0283
*             R5=R6->RDW;           /* AND FOR FLR THE LENGTH        */
         LH    R5,RDW(,R6)                                         0284
*           ELSE                    /* IS DCB LRECL                  */
*             IF DCBRKP=ZERO&       /* IF NON-EMBEDDED               */
*                 DCBRECBR=OFF THEN /* KEY, THEN LRECL               */
         B     @RC00283                                            0285
@RF00283 LA    @12,52                                              0285
         ALR   @12,RDCB                                            0285
         LH    @10,DCBRKP(,@12)                                    0285
         LTR   @10,@10                                             0285
         BNZ   @RF00285                                            0285
         TM    DCBRECBR+28(RDCB),B'00010000'                       0285
         BNZ   @RF00285                                            0285
*               R5=DCBLRECL+DCBKEYLE;/* DOES NOT                     */
         SLR   R5,R5                                               0286
         IC    R5,DCBKEYLE+16(,RDCB)                               0286
         AH    R5,DCBLRECL(,@12)                                   0286
*             ELSE                                                 0287
*               R5=DCBLRECL;        /* INCLUDE THE KEY LENGTH        */
         B     @RC00285                                            0287
@RF00285 LH    R5,DCBLRECL+52(,RDCB)                               0287
*           R7=R5;                  /* LENGTH IN BOTH ODD REGS       */
@RC00285 DS    0H                                                  0288
@RC00283 LR    R7,R5                                               0288
*           GEN(MVCL  R4,R6);       /* MOVE RECORD TO USER BFR       */
         MVCL  R4,R6
*           RFY                                                    0290
*            (R4,                                                  0290
*             R5,                                                  0290
*             R6,                                                  0290
*             R7) UNRSTD;           /* RELEASE WORK REGISTERS        */
*         END;                      /* END OF 1ST GET DATA MVC       */
*     END;                          /* END OF 1ST GET CODE           */
*                                                                  0293
*   /*****************************************************************/
*   /*                                                               */
*   /* FOR NON-FIRST ISAM GET SET UP AREA ADDRESS AND ISSUE VSAM GET.*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0293
*   ELSE                            /* HERE IF NOT THE FIRST         */
*     DO;                           /* GET AFTER A SETL K            */
         B     @RC00273                                            0293
@RF00273 DS    0H                                                  0294
*       IF DCBMFLCG=ON×             /* IF LOCATE MODE OR IF          */
*           IIDATA=ON THEN          /* A DATA ONLY RETRIEVAL,        */
         TM    DCBMFLCG+40(RDCB),B'00001000'                       0294
         BO    @RT00294                                            0294
         TM    IIDATA(RIICB),B'00001000'                           0294
         BNO   @RF00294                                            0294
@RT00294 DS    0H                                                  0295
*         RPLAREA=RPLIIBFR;         /* USE THE II BUFFER.            */
         L     @12,RPLIIBFR+76(,RRPL)                              0295
         ST    @12,RPLAREA(,RRPL)                                  0295
*       ELSE                        /* READ INTO USER BUFFER.        */
*         IF DCBRECBR=OFF&          /* IF KEY NOT EMBEDDED AND       */
*             DCBRKP^=ZERO THEN     /* RECORD UNBLOCKED, ALLOW       */
