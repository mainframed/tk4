         DC  Y(STE1101B-STE1101A)         STATIC TEXT LENGTH
         DC  Y(STE1101A-TXT110A)          STATIC TEXT OFFSET
         DC  H'1'                         PRINT COLUMN
         DC  Y(STE1101B-STE1101A)         PRINT FIELD LENGTH
         DC  H'0'                         CONVERSION FLAGS (CHAR)
* STATIC TEXT ENTRIES
STE1101A DC  C'RECORD'
STE1101B EQU *
TXT110B  EQU *
         EJECT
*
*                                                                    *
*  FORMAT 4 AND 5 AND 6 RECORD HEX LINE
*                                                                    *
*
TXT111   DC  Y(TXT111B-TXT111A)           ENTRY LENGTH
         DC  X'0000'                      MESSAGE FLAG - NO TSO
* SPACING CONTROL SUB-STRUCTURE
TXT111A  DC  X'4000'                      SPACE CONTROL FLAG
         DC  H'2'                         SKIP A
         DC  C'R '                               LINE FROM PREVIOUS LIN
* BLOCK DATA  SUB-STRUCTURE
         DC  X'9000'                      BLOCK AND EOF FLAG
         DC  H'0'                         INPUT LENGTH-USE DARGILP
         DC  H'0'                         INPUT OFFSET
         DC  H'1'                         PRINT COLUMN
         DC  H'0'                         PRINT FIELD LENGTH
         DC  X'8000'                      CONVERSION FLAGS(BTE-PR HEX)
* NO STATIC TEXT ENTRIES
TXT111B  EQU *
         EJECT
*
*                                                                     *
*  FORMAT 4 AND 5 AND 6 RECORD CHAR LINE
*                                                                     *
*
TXT112   DC  Y(TXT112B-TXT112A)           ENTRY LENGTH
         DC  X'0000'                      MESSAGE FLAG - NO TSO
* SPACING CONTROL SUB-STRUCTURE
TXT112A  DC  X'4000'                      SPACE CONTROL FLAG
         DC  H'2'                         SKIP A
         DC  C'R '                               LINE FROM PREVIOUS LIN
* BLOCK DATA SUB-STRUCTURE
         DC  X'9000'                      BLOCK AND EOF FLAG
         DC  H'0'                         INPUT LENGTH-USE DARGILP
         DC  H'0'                         INPUT OFFSET
         DC  H'1'                         PRINT COLUMN
         DC  H'0'                         PRINT FIELD LENGTH
         DC  H'0'                         CONVERSION FLAGS (CHAR)
* NO STATIC TEXT ENTRIES
TXT112B  EQU *
         EJECT
*
*                                                                     *
*  FORMAT 4 AND 5 AND 6 RECORD HEX AND CHAR LINE
*                                                                     *
*
TXT113   DC  Y(TXT113B-TXT113A)           ENTRY LENGTH
         DC  X'0000'                      MESSAGE FLAG - NO TSO
* SPACING CONTROL SUB-STRUCTURE
TXT113A  DC  X'4000'                      SPACE CONTROL FLAG
         DC  H'2'                         SKIP A
         DC  C'R '                             LINE FROM APREVIOUS LINE
* BLOCK  DATA SUB-STRUCTURE
         DC  X'9000'                      BLOCK AND EOF FLAG
         DC  H'0'                         INPUT LENGTH-USE DARGILP
         DC  H'0'                         INPUT OFFSET
         DC  H'1'                         PRINT COLUMN
         DC  H'0'                         PRINT FIELD LENGTH
         DC  X'2000'                      CONVERSION FLAGS(STAND DUMP)
* NO STATIC TEXT ENTRIES
TXT113B  EQU *
         END
