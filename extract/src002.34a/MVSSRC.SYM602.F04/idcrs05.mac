         L     @07,RSGRABCU(,REG6)                                 0931
         L     @04,RSIOTBL(,REG6)                                  0931
         LA    @15,GRABSCA(,@04)                                   0931
         CR    @07,@15                                             0931
         BE    @RT00931                                            0931
         LA    @04,GRABSCR(,@04)                                   0931
