@RF00950 L     @07,RSGRABCU(,REG6)                                 0952
         MVC   GRABFLGS(3,@07),RSVIAGRB(REG6)                      0952
*              END;                                                0953
*            RSVIAFND = ON;            /*END OF GETVIA PROC          */
@RT00933 OI    RSVIAFND(REG6),B'00100000'                          0954
*          END;                                                    0955
*                                                                  0955
