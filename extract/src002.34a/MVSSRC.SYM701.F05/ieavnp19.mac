*                                                            @ZM31050*/
         L     @15,CVTPTR                                          0585
         L     @15,CVTEXT2(,@15)                                   0585
         L     @15,CVTICB(,@15)                                    0585
         L     R1,ICBIATNA(,@15)                                   0585
*   R2=MSCATNAS;                    /* ASID OF MSC ATTENTION ROUTINE
*                                                            @ZM31050*/
         LA    R2,1                                                0586
*   R3=ICBPUCB;                     /* ADDR OF UCB FOR PRI MSC     0587
*                                                            @ZM31050*/
         L     R3,ICBPUCB(,@15)                                    0587
*   R5=MSCATNTX;                    /* NEW ATTENTION TABLE INDEX FOR
*                                      MSC                   @ZM31050*/
         LA    R5,56                                               0588
*IIOSGEN1:                          /* FIRST IOSGEN MACRO USE HERE 0589
