    LOAD EP=IEAVNPC2,DCB=(1),ERRET=LOADERR,RELATED=(IEAVNPC2,IEAVNP19(**
               VVOLUMES))                                     @ZM31050
*   R15=R0;                         /* GET ADDRESS OF IEAVNPC2     0607
