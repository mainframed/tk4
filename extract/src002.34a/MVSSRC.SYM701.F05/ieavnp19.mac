         TITLE 'IEAVNP19 -- THE MSS RIM                                *
                        '
IEAVNP19 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVNP19  78.166'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @04,0                                               0001
@PSTART  LA    @12,4095(,@04)                                      0001
         USING @PSTART,@04                                         0001
         USING @PSTART+4095,@12                                    0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
*
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
* WILL ASSEMBLE PROPERLY.
*   NVTPTR=RNVT;                    /* FIND THE NVT          @G24LB02*/
         ST    RNVT,NVTPTR                                         0218
*   TERMERR=OFF;                    /* CLEAR ERR SWITCH      @G24LB02*/
*   MINERR=OFF;                     /* CLEAR MINOR ERROR SWITCH    0220
*                                                            @G24LB02*/
         NI    TERMERR,B'00111111'                                 0220
*   FAILCALL=OFF;                   /* CLEAR SWITCH          @ZDR2048*/
         NI    FAILCALL,B'10111111'                                0221
*   RESPECIFY                                                      0222
*     NVT BASED(NVTPTR);            /*                       @G24LB02*/
*   RFY                                                            0223
*    (RCVT,                                                        0223
*     RNVT) UNRSTD;                 /* INPUT REGISTERS       @G24LB02*/
*   RESPECIFY                                                      0224
*     PARMTAB BASED(NVTPTAB);       /* & FOR PARAMETERS      @G24LB02*/
*   RESPECIFY                                                      0225
*     NIPPAREA BASED(NVTPAREA);     /*                       @G24LB02*/
*   RESPECIFY                                                      0226
*     NIPPTE BASED(PURGE);          /*                       @G24LB02*/
*   IF PURGE^=NOPURG THEN                                          0227
         L     @05,NVTPTR                                          0227
         L     @05,NVTPTAB(,@05)                                   0227
         L     @05,PURGE(,@05)                                     0227
         LTR   @05,@05                                             0227
         BZ    @RF00227                                            0227
*     DO;                                                          0228
*       PURGBIT=ON;                 /* SET THE PURGE INDICATOR ON  0229
*                                                            @G24LB02*/
         OI    PURGBIT,B'10000000'                                 0229
*     END;                                                         0230
*                                                                  0230
*/********************************************************** @G24LB02*/
*                                                                  0231
*   CALL MSCUCBS;                   /* FIND OUT IF THERE ARE ANY MSCS
*                                                            @G24LB02*/
@RF00227 BAL   @14,MSCUCBS                                         0231
*/********************************************************** @G24LB02*/
*                                                                  0232
*   IF MSCUCBA1^=NOMSCS             /* ARE THERE ANY MSCS ?  @G24LB02*/
*     THEN                          /* AT LEAST 1 MSC SYSGENED     0232
*                                                            @G24LB02*/
         L     @05,MSCUCBA1                                        0232
         LTR   @05,@05                                             0232
         BZ    @RF00232                                            0232
*     DO;                           /* THIS PATH FOR MSS USE @G24LB02*/
*/********************************************************** @G24LB02*/
*                                                                  0234
*       CALL CBSETUP;               /* SET UP MSSC CONTROL BLOCK   0234
*                                                            @G24LB02*/
         BAL   @14,CBSETUP                                         0234
*/********************************************************** @G24LB02*/
*                                                                  0235
*       IF CLEANCOD=ALLCLEAN THEN   /* ANY ERROR SO FAR ?    @G24LB02*/
         L     @05,CLEANCOD                                        0235
         LTR   @05,@05                                             0235
         BNZ   @RF00235                                            0235
*         DO;                       /*                       @G24LB02*/
*/********************************************************** @G24LB02*/
*                                                                  0237
*           CALL ADDRESSS;          /* FILL IN ADDRESSES FOR MSSC  0237
*                                                            @G24LB02*/
         BAL   @14,ADDRESSS                                        0237
*/********************************************************** @G24LB02*/
*                                                                  0238
*         END;                      /*                       @G24LB02*/
*       ELSE                        /* SET TERMINATION ERROR SWICTH
*                                      ON                    @G24LB02*/
*         TERMERR=ON;               /* SET TERMINAL ERR ON   @G24LB02*/
         B     @RC00235                                            0239
@RF00235 OI    TERMERR,B'01000000'                                 0239
*       IF CLEANCOD=ALLCLEAN THEN   /* ANY ERROR SO FAR ?    @G24LB02*/
@RC00235 L     @05,CLEANCOD                                        0240
         LTR   @05,@05                                             0240
         BNZ   @RF00240                                            0240
*         DO;                       /* SET UP THE IOS ATTENTION INDEX
*                                                            @G24LB02*/
*           CALL ATTENTNX;          /*                       @G24LB02*/
         BAL   @14,ATTENTNX                                        0242
*         END;                      /*                       @G24LB02*/
*       ELSE                        /* CONTINUE WITH MSSC          0244
*                                      INITIALIZATION        @G24LB02*/
*         TERMERR=ON;               /* SET TERMINAL ERR ON   @G24LB02*/
         B     @RC00240                                            0244
@RF00240 OI    TERMERR,B'01000000'                                 0244
*       IF CLEANCOD=ALLCLEAN THEN   /* ANY ERROR SO FAR ?    @G24LB02*/
@RC00240 L     @05,CLEANCOD                                        0245
         LTR   @05,@05                                             0245
         BNZ   @RF00245                                            0245
*         DO;                       /* SET UP THE IOS ATTENTION INDEX
*                                                            @G24LB02*/
*/********************************************************** @G24LB02*/
*                                                                  0247
*           CALL MSG05LCD;          /* INITIALIZE THE MSC(S) @G24LB02*/
         BAL   @14,MSG05LCD                                        0247
*           IF MINERR=ON THEN                                      0248
         TM    MINERR,B'10000000'                                  0248
         BNO   @RF00248                                            0248
*             ICBB=ON;              /*                       @G24LB02*/
         L     @05,CVTPTR                                          0249
         L     @05,CVTEXT2(,@05)                                   0249
         L     @05,CVTICB(,@05)                                    0249
         OI    ICBB(@05),B'00010000'                               0249
*/********************************************************** @G24LB02*/
*                                                                  0250
*         END;                      /*                       @G24LB02*/
@RF00248 DS    0H                                                  0251
*       IF TERMERR=OFF THEN         /* WAS TERMINAL ERR SW SET?    0251
*                                                            @G24LB02*/
@RF00245 TM    TERMERR,B'01000000'                                 0251
         BNZ   @RF00251                                            0251
*         DO;                       /*                       @G24LB02*/
*           CALL VVOLUMES;          /*                       @G24LB02*/
*                                                                  0253
         BAL   @14,VVOLUMES                                        0253
*           /*********************************************************/
*           /*                                                       */
*           /* VOLZERS ISSUES DEMOUNT VUA FOR OFFLINE VUCB(S) AND    */
*           /*                                               @G24LB02*/
*           /* SHOULD ONLY DO SO IF WE HAD A NORMAL INITIALIZATION   */
*           /* AND WE DID NOT PURGE                          @G24LB02*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0254
*           IF ICBB=OFF&PURGBIT=OFF THEN                           0254
         L     @05,CVTPTR                                          0254
         L     @05,CVTEXT2(,@05)                                   0254
         L     @05,CVTICB(,@05)                                    0254
         TM    ICBB(@05),B'00010000'                               0254
         BNZ   @RF00254                                            0254
         TM    PURGBIT,B'10000000'                                 0254
         BNZ   @RF00254                                            0254
*             DO;                   /*                       @G24LB02*/
*               CALL VOLZERS;       /*                       @G24LB02*/
         BAL   @14,VOLZERS                                         0256
*             END;                  /*                       @G24LB02*/
*         END;                      /*                       @G24LB02*/
@RF00254 DS    0H                                                  0259
*NOMSSC:                            /* CHECK IF NO MSSC      @G24LB02*/
*       IF CLEANCOD^=ALLCLEAN       /* ANY MSSC OR MSC PROBLEM ?   0259
*                                                            @G24LB02*/
*         THEN                      /* UNABLE TO USE MSSC OR 3850  0259
*                                                            @G24LB02*/
@RF00251 DS    0H                                                  0259
NOMSSC   L     @05,CLEANCOD                                        0259
         LTR   @05,@05                                             0259
         BZ    @RF00259                                            0259
*         CALL NP19FAIL;            /* INVOKE FAILURE PROCESSING   0260
*                                                            @G24LB02*/
         BAL   @14,NP19FAIL                                        0260
*       ELSE                                                       0261
*         ;                         /* 3850 IS READY FOR VIRTUAL   0261
*                                      VOLUME MOUNTS         @G24LB02*/
@RF00259 DS    0H                                                  0262
*     END;                          /*                       @G24LB02*/
*   ELSE                                                           0263
*     ;                             /* NO MSC UCBS -- JUST RETURN  0263
*                                                            @G24LB02*/
@RF00232 DS    0H                                                  0264
*                                                                  0264
*/********************************************************** @G24LB02*/
*/*                                                          @G24LB02*/
*/*  PROCEDURE NAME - MSG05LCD                               @G24LB02*/
*/*                                                          @G24LB02*/
*/*  FUNCTION - THIS ROUTINE SETS UP ARGUMENTS FOR AND       @G24LB02*/
*/*             INVOKES THE LOAD MACRO.  THE LOAD MACRO      @G24LB02*/
*/*             WILL CAUSE ICBMSG05 TO BE LOADED INTO THE    @G24LB02*/
*/*             SYSTEM. AFTER THE LOAD IS COMPLETED,         @G24LB02*/
*/*             IS CALLED TO DO MSC INITIALIZATION.          @G24LB02*/
*/*             THE DELETE MACRO IS USED TO DELETE ICBMSG05. @G24LB02*/
*/*                                                          @G24LB02*/
*/*  INPUT - PARAMETERS ARE SET IN THIS ROUTINE              @G24LB02*/
*/*                                                          @G24LB02*/
*/*  OUTPUT - R15 IS SET TO THE ADDRESS OF ICBMSG05          @G24LB02*/
*/*           PASS THE PURGE PARAMETER TO ICBMSG05           @G24LB02*/
*/********************************************************   @G24LB02*/
*                                                                  0264
*MSG05LCD:                                                         0264
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @G24LB02*/
@EL00001 L     @13,4(,@13)                                         0264
@EF00001 DS    0H                                                  0264
@ER00001 LM    @14,@12,12(@13)                                     0264
         BR    @14                                                 0264
MSG05LCD DS    0H                                                  0265
*   RESPECIFY                                                      0265
*     R14 RESTRICTED;               /*                       @G24LB02*/
*   M5LCDR14=R14;                   /* SAVE REG 14           @G24LB02*/
         ST    R14,M5LCDR14                                        0266
*   RESPECIFY                                                      0267
*     R14 UNRESTRICTED;             /*                       @G24LB02*/
*   RFY                                                            0268
*    (R0,                                                          0268
*     R1,                                                          0268
*     R15) RSTD;                    /*                       @G24LB02*/
*   R1=NVTDCBSN;                    /*                       @G24LB02*/
         L     @14,NVTPTR                                          0269
         L     R1,NVTDCBSN(,@14)                                   0269
*   GEN REFS(LOADER1,R1) SETS(R0);                                 0270
*                                                         /*@G24LB02*/
LOAD05  LOAD EP=ICBMSG05,DCB=(1),ERRET=LOADER1,RELATED=(ICBMSG05,IEAVN**
               P19(MSG05LCD))                                 @G24LB02
*   GOTO LOADGD;                    /* BRANCH, LOAD GOOD     @G24LB02*/
         B     LOADGD                                              0271
*LOADER1:                           /*                       @G24LB02*/
*   FAILCODE=R1;                    /* SAVE FAIL CODE        @G24LB02*/
LOADER1  ST    R1,FAILCODE                                         0272
*   CLEANCOD=LNM5FAIL;              /* SETUP CLEANUP CODE    @G24LB02*/
         MVC   CLEANCOD(4),@CF02461                                0273
*   MINERR=ON;                      /* MINOR ERROR INIT WILL CONT  0274
*                                                            @G24LB02*/
         OI    MINERR,B'10000000'                                  0274
*   R14=M5LCDR14;                   /*                       @G24LB02*/
         L     R14,M5LCDR14                                        0275
*   RETURN;                         /*                        @24LB02*/
@EL00002 DS    0H                                                  0276
@EF00002 DS    0H                                                  0276
@ER00002 BR    @14                                                 0276
*LOADGD:                            /*                       @G24LB02*/
*   R15=R0;                         /* SAVE ADDRESS OF ICBMSG05 IN 0277
*                                      R15                   @G24LB02*/
LOADGD   LR    R15,R0                                              0277
*   R1=ADDR(PURGINDC);              /* SETUP PARM PTR FOR ICBMSG05 0278
*                                                            @G24LB02*/
         LA    R1,PURGINDC                                         0278
*   GEN REFS(R14,R15);                                             0279
*                                                         /*@G24LB02*/
       BALR     R14,R15     BR AND LINK TO ICBMSG05           @G24LB02
DEL05  DELETE EP=ICBMSG05,RELATED=(ICBMSG05,IEAVNP19(MSG05LCD))
*   RESPECIFY                                                      0280
*     R14 RESTRICTED;               /*                       @G24LB02*/
*   R14=M5LCDR14;                   /* RESTORE REGISTER 14   @G24LB02*/
         L     R14,M5LCDR14                                        0281
*   RESPECIFY                                                      0282
*     R14 UNRESTRICTED;             /*                       @G24LB02*/
*   RETURN;                         /*                       @G24LB02*/
         B     @EL00002                                            0283
*   END MSG05LCD;                   /*                       @G24LB02*/
*/********************************************************** @G24LB02*/
*/*                                                          @G24LB02*/
*/*   MSCUCBS      SEGMENT OF IEAVNP19 WHICH IDENTIFIES      @G24LB02*/
*/*                 UCBS OF THE MSC DEVICE TYPE              @G24LB02*/
*/*                                                          @G24LB02*/
*/*    NOTE THAT IN THIS VERSION THERE MAY BE ONLY TWO MSCS, @G24LB02*/
*/*    AND THE FIRST TWO UCBS IN THE UCB ADDRESS LIST ARE    @G24LB02*/
*/*    THOSE FOUND AND REMEMBERED FOR MSSC USE               @G24LB02*/
*/*                                                          @G24LB02*/
*/*   INPUT:       A POINTER TO LIST OF UCBS VIA THE CVT     @G24LB02*/
*/*                                                          @G24LB02*/
*/*  PROCESSING:   SEARCH ALL UCBS UNTIL TWO ARE FOUND FOR   @G24LB02*/
*/*                THE MSC DEVICE TYPE, OR UNTIL THERE ARE   @G24LB02*/
*/*                NO MORE UCBS IN THE SYSTEM                @G24LB02*/
*/*                TURNS ON THE UCBALOC BIT FOR EACH MSC UCB @G24LB02*/
*/*                FOUND                                     @G24LB02*/
*/*                TURNS OFF THE UCBQISCE BIT FOR EACH MSC   @ZM31123*/
*/*                UCB, TO ALLOW I/O TO BE SENT TO THE MSC   @ZM31123*/
*/*                                                          @G24LB02*/
*/*  OUTPUT:       THE ADDRESS OF THE FIRST MSC UCB FOUND,   @G24LB02*/
*/*                IF ANY                                    @G24LB02*/
*/*                THE ADDRESS OF THE SECOND MSC UCB FOUND,  @G24LB02*/
*/*                IF ANY                                    @G24LB02*/
*/*                THE CALLING MODULE CHECKS TO SEE IF THE   @G24LB02*/
*/*                ADDRESS OF THE FIRST MSC FOUND IS ZERO.   @G24LB02*/
*/*                IF SO, IEAVNP19 ASSUMES THE IPL WILL NOT  @G24LB02*/
*/*                BE USING MSS, AS NO SYSGEN SUPPORT FOR    @G24LB02*/
*/*                3850 WAS PROVIDED.                        @G24LB02*/
*/*                                                          @G24LB02*/
*/*      MESSAGES:  NONE ARE WRITTEN OR CAUSED TO BE WRITTEN @G24LB02*/
*/*                                                          @G24LB02*/
*/********************************************************** @G24LB02*/
*                                                                  0285
*MSCUCBS:                                                          0285
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB02*/
MSCUCBS  DS    0H                                                  0286
*   DCL                                                            0286
*     MSCRET POINTER(31);           /* FOR RETURN ADDRESS    @G24LB02*/
*   DCL                                                            0287
*     MSCDEVC FIXED(8) CONSTANT(66);/* HEX 42 IN MSC UCB     @G24LB02*/
*   RFY                                                            0288
*     R14 RSTD;                     /* FOR RETURN ADDRESS    @G24LB02*/
*   MSCRET=R14;                     /* SAVE RETURN ADDRESS   @G24LB02*/
         ST    R14,MSCRET                                          0289
*   RFY                                                            0290
*     R14 UNRSTD;                   /* FREE UP REGISTER 14   @G24LB02*/
*   LISTA=CVTILK2;                  /* ADDR OF UCB ADDRESS LIST    0291
*                                                            @G24LB02*/
         L     @14,CVTPTR                                          0291
         L     @14,CVTILK2(,@14)                                   0291
         ST    @14,LISTA                                           0291
*   DO WHILE UCBPTR^=FOXES&MSCUCBA2=LT2MSCS;/* FIND MSCS     @G24LB02*/
         B     @DE00292                                            0292
@DL00292 DS    0H                                                  0293
*     UCBPTR=LISTA->BEGUPTR;        /* ADDRESS OF ONE UCB    @G24LB02*/
         L     @14,LISTA                                           0293
         LH    @14,BEGUPTR(,@14)                                   0293
         N     @14,@CF02747                                        0293
         STH   @14,UCBPTR                                          0293
*     IF UCBPTR^=ZERO&UCBPTR^=FOXES /* IS IT VALID?          @G24LB02*/
*       THEN                        /* THIS IS A VALID UCB ADDRESS 0294
*                                                            @G24LB02*/
         LH    @14,UCBPTR                                          0294
         N     @14,@CF02747                                        0294
         LTR   @14,@14                                             0294
         BZ    @RF00294                                            0294
         SLR   @05,@05                                             0294
         ICM   @05,3,FOXES                                         0294
         CR    @14,@05                                             0294
         BE    @RF00294                                            0294
*       DO;                         /* SEE IF IT IS AN MSC UCB,    0295
*                                                            @G24LB02*/
*         IF UCB3UREC=ON&UCBTBYT4=MSCDEVC/* AN MSC UCB ?     @G24LB02*/
*           THEN                    /* YES, AN MSC UCB       @G24LB02*/
         TM    UCB3UREC(@14),B'00001000'                           0296
         BNO   @RF00296                                            0296
         CLI   UCBTBYT4(@14),66                                    0296
         BNE   @RF00296                                            0296
*           DO;                     /* NEED TO PROCESS AND REMEMBER
*                                      IT                    @Y30LB04*/
*             UCBALOC=ON;           /* (MSSC CONVENTION)     @Y30LB04*/
         OI    UCBALOC(@14),B'00001000'                            0298
*             UCBQISCE=OFF;         /* TURN OFF QUIESCE BIT  @ZM31123*/
         NI    UCBQISCE(@14),B'11111110'                           0299
*             IF MSCUCBA1=NOMSCS    /* IS THIS THE FIRST MSC UCB ? 0300
*                                                            @Y30LB04*/
*               THEN                /* THIS IS FIRST MSC UCB @Y30LB04*/
         L     @05,MSCUCBA1                                        0300
         LTR   @05,@05                                             0300
         BNZ   @RF00300                                            0300
*               MSCUCBA1=UCBPTR;    /* YES, SAVE ITS ADDRESS @Y30LB04*/
         ST    @14,MSCUCBA1                                        0301
*             ELSE                  /* NOT FIRST MSC UCB, MUST BE 2ND
*                                                            @ZM31096*/
*               DO;                 /* SEE IF ANOTHER MSC UCB      0302
*                                                            @ZM31096*/
         B     @RC00300                                            0302
@RF00300 DS    0H                                                  0303
*                 IF UCBPTR=MSCUCBA1/* HAVE FOUND SAME MSC?  @ZM31096*/
*                   THEN                                           0303
         LH    @14,UCBPTR                                          0303
         N     @14,@CF02747                                        0303
         C     @14,MSCUCBA1                                        0303
         BE    @RT00303                                            0303
*                   ;               /* YES, NOT A DIFFERENT MSC UCB
*                                                            @ZM31096*/
*                 ELSE              /* NO, THIS IS ANOTHER MSC UCB 0305
*                                                            @ZM31096*/
*                   MSCUCBA2=UCBPTR;/* SAVE ADDR AS SECOND   @ZM31096*/
         LH    @14,UCBPTR                                          0305
         N     @14,@CF02747                                        0305
         ST    @14,MSCUCBA2                                        0305
*               END;                /*                       @ZM31096*/
*           END;                    /* PROCESSING FOR AN MSC UCB   0307
*                                                            @G24LB02*/
*         ELSE                      /* THIS IS NOT AN MSC UCB      0308
*                                                            @G24LB02*/
*           ;                       /* SKIP IT               @G24LB02*/
@RF00296 DS    0H                                                  0309
*       END;                        /* FINDING OUT IF THIS IS AN MSC
*                                      UCB                   @G24LB02*/
*     ELSE                          /* NO UCB FOR THIS LIST ENTRY  0310
*                                                            @G24LB02*/
*       ;                           /* ON TO THE NEXT LIST ENTRY   0310
*                                                            @G24LB02*/
@RF00294 DS    0H                                                  0311
*     LISTA=LISTA+ALINC;            /* INCREMENT THROUGH UCB LIST  0311
*                                                            @G24LB02*/
@RC00294 LA    @14,2                                               0311
         AL    @14,LISTA                                           0311
         ST    @14,LISTA                                           0311
*   END;                            /* OF THE SEARCH FOR MSC UCBS  0312
*                                                            @G24LB02*/
@DE00292 CLC   UCBPTR(2),FOXES                                     0312
         BE    @DC00292                                            0312
         L     @14,MSCUCBA2                                        0312
         LTR   @14,@14                                             0312
         BZ    @DL00292                                            0312
@DC00292 DS    0H                                                  0313
*   RFY                                                            0313
*     R14 RSTD;                     /* FOR RETURN ADDRESS    @G24LB02*/
*   R14=MSCRET;                     /* RESTORE RETURN ADDRESS      0314
*                                                            @G24LB02*/
         L     R14,MSCRET                                          0314
*   RFY                                                            0315
*     R14 UNRSTD;                   /* FOR RETURN TO CALLER  @G24LB02*/
*   END MSCUCBS;                    /* HAVE FOUND ALL MSC UCBS     0316
*                                      EXPECTED              @G24LB02*/
@EL00003 DS    0H                                                  0316
@EF00003 DS    0H                                                  0316
@ER00003 BR    @14                                                 0316
*                                                                  0317
*/********************************************************** @G24LB02*/
*/*                                                          @G24LB02*/
*/*   CBSETUP      SEGMENT OF IEAVNP19 WHICH SETS UP THE     @G24LB02*/
*/*                MSSC CONTROL BLOCK, ICB2SSCB              @G24LB02*/
*/*                                                          @G24LB02*/
*/*   INPUT:       POINTER TO THE CVT IN RCVT                @G24LB02*/
*/*                                                          @G24LB02*/
*/*  PROCESSING:  OBTAINS FIXED STORAGE FOR THE MSSC         @Y30LB04*/
*/*               CONTROL BLOCK, ICB2SSCB,                   @Y30LB04*/
*/*               CLEARS THAT STORAGE, AND                   @Y30LB04*/
*/*               PLACES ITS ADDRESS INTO CVTICB, WHICH      @Y30LB04*/
*/*               IS BOTH THE ADDRESS OF THE MSSC CONTROL    @Y30LB04*/
*/*               BLOCK AND AN INDICATION TO OTHER MODULES   @Y30LB04*/
*/*               THAT THERE IS AN MSSC IN THIS SYSTEM.      @Y30LB04*/
*/*               OBTAINS FIXED STORAGE FOR THE MSSC SRB,    @Y30LB04*/
*/*               CLEARS THE SRB STORAGE,                    @Y30LB04*/
*/*               AND PLACES A POINTER TO THE MSSC SRB INTO  @Y30LB04*/
*/*                THE MSSC CONTROL BLOCK                    @Y30LB04*/
*/*                                                          @Y30LB04*/
*/*   OUTPUT:      PARTIALLY INITIALIZED MSSC CONTROL BLOCK  @Y30LB04*/
*/*                A POINTER IN THE CVT WHICH IS USED TO     @Y30LB04*/
*/*                NOTIFY OTHER COMPONENTS THAT MSSC IS IN   @Y30LB04*/
*/*                THIS SYSTEM, AND TO FIND THE LOCATION OF  @Y30LB04*/
*/*                THE MSSC CONTROL BLOCK, ICB2SSCB          @Y30LB04*/
*/*                ON ERROR, FAILURE AND CLEANUP CODES ARE   @Y30LB04*/
*/*                PASSED TO THE MAIN SEGMENT                @Y30LB04*/
*/*                                                          @Y30LB04*/
*/********************************************************** @Y30LB04*/
*                                                                  0317
*CBSETUP:                                                          0317
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @Y30LB04*/
CBSETUP  DS    0H                                                  0318
*   DCL                                                            0318
*     CBRET POINTER(31);            /* FOR RETURN ADDRESS    @Y30LB04*/
*   RFY                                                            0319
*     R14 RSTD;                     /* FOR RETURN ADDRESS    @Y30LB04*/
*   CBRET=R14;                      /* SAVE RETURN ADDRESS   @Y30LB04*/
         ST    R14,CBRET                                           0320
*   RFY                                                            0321
*     R14 UNRSTD;                   /* FREE UP REGISTER FOURTEEN   0321
*                                                            @Y30LB04*/
*/*  GET STORAGE FOR MSSC CONTRO BLOCK                       @Y30LB04*/
*                                                                  0322
*   RFY                                                            0322
*    (R0,                                                          0322
*     R1,                                                          0322
*     R15) RSTD;                    /* FOR GETMAIN           @Y30LB04*/
*   R0=LENGTH(ICB2SSCB);            /* AMOUNT OF STORAGE     @Y30LB04*/
         LA    R0,532                                              0323
*   R1=ZERO;                        /*                       @Y30LB04*/
         SLR   R1,R1                                               0324
*   R15=SP245;                      /* SP 245, CONDITIONAL GET     0325
*                                                            @Y30LB04*/
         LA    R15,245                                             0325
*IGETMAI1:                          /* GET CONTROL BLOCK STORAGE   0326
*                                                            @Y30LB04*/
*   GEN REFS(R0,R1,R15) SETS(R0,R1,R15);                           0326
IGETMAI1 DS    0H                                                  0326
