*       IF CLEANCOD<GETTHRSH        /* WAS IT A GETMAIN FAILURE?   0634
*                                                            @G24LB02*/
*         THEN                      /* THIS WAS A GETMAIN FAILURE  0634
*                                                            @G24LB02*/
         L     @14,CLEANCOD                                        0634
         C     @14,@CF02423                                        0634
         BNL   @RF00634                                            0634
*         DO;                       /* UPDATE THE NVT        @G24LB02*/
*                                                                  0635
*           /*********************************************************/
*           /*                                                       */
