         CLC   SDGNN-3(2,@10),MTDNN                                0233
         BNE   @RF00233                                            0233
*       DO;                         /* KEEP TRACK OF IT      #Y30LB04*/
*         MATCH=YES;                /* HAVE FOUND THE LIST ENTRY   0235
*                                                            #Y30LB04*/
         OI    MATCH,B'00100000'                                   0235
*       END;                        /* WILL QUIT THE SEARCH  #Y30LB04*/
