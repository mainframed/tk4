*       ;                           /* CONTINUE SEARCH       #Y30LB04*/
@RF00233 DS    0H                                                  0238
*   END;                            /* LOOKING FOR THE MATCH #Y30LB04*/
         LA    @10,1                                               0238
         AL    @10,MATCHDX                                         0238
         ST    @10,MATCHDX                                         0238
         C     @10,@CF01056                                        0238
         BNH   @DL00232                                            0238
@DC00232 DS    0H                                                  0239
*   MATCHDX=MATCHDX-ONE;            /* CORRECT SDG INDEX     @ZA08838*/
