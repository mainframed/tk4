         TITLE 'ICBME--MSSC MOUNT EQUALIZATION                         *
                        '
ICBME    CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ICBME     76.338'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
*   ALLOPTR=R1;                     /* ADDRESS ALLOCATION INPUT    0053
*                                                            #Y30LB04*/
         LR    ALLOPTR,R1                                          0053
*   SDGPTR=ADDR(ALOBUF);            /* FIRST OF STAGING DRIVE GRP  0054
*                                      LIST                  @Y30LB04*/
         L     SDGPTR,ALOWKPTR(,ALLOPTR)                           0054
         LA    SDGPTR,ALOBUF(,SDGPTR)                              0054
*   MOUNTED=NO;                     /* INDICATE VOL NOT FOUND MOUNTED
*                                      YET                   #Y30LB04*/
         NI    MOUNTED,B'01111111'                                 0055
*   IF ICBOPT=ZERO THEN             /* SEE IF I HAVE SDG LIST      0056
*                                                            @ZA11005*/
         SLR   @10,@10                                             0056
         L     @07,CVTPTR                                          0056
         L     @07,CVTEXT2(,@07)                                   0056
         L     @07,CVTICB(,@07)                                    0056
         C     @10,ICBOPT(,@07)                                    0056
         BNE   @RF00056                                            0056
*     DO;                           /* NO LIST               @ZA11005*/
*       ALLOCPTR=ZERO;              /* ZERO PTR FOR ALLOC.   @ZA13481*/
         L     @07,ALOULPTR(,ALLOPTR)                              0058
         ST    @10,ALLOCPTR(,@07)                                  0058
*       GO TO MERET;                /* TAKE QUICK EXIT       @ZA11005*/
         B     MERET                                               0059
*     END;                          /* TO AVOID LOW CORE.    @ZA11005*/
*   IF ACALLCNT=FRSTCAL             /* FIRST CALL OF ICBME FOR THIS
*                                      REQ?                  #Y30LB04*/
*     THEN                          /* THIS IS FIRST ENTRY   #Y30LB04*/
@RF00056 L     @10,ALOWKPTR(,ALLOPTR)                              0061
         L     @10,ACALLCNT(,@10)                                  0061
         LTR   @10,@10                                             0061
         BNZ   @RF00061                                            0061
*     DO;                           /* SPECIAL FIRST TIME PROCESSING
*                                                            #Y30LB04*/
*       CALL ICBMEDR;               /* GET STAGING DRIVE GROUP LIST.
*                                      R1 IS SET             @Y30LB04*/
         BAL   @14,ICBMEDR                                         0063
*       RESPECIFY                                                  0064
*         R15 RESTRICTED;           /* RESTRICT REGISTER 15  @Y30LB04*/
*       IF R15^=GOODCODE            /* ICBMEDR ENCOUNTER ERROR?    0065
*                                                            #Y30LB04*/
*         THEN                      /* YES, ERROR IN ICBMEDR #Y30LB04*/
         SLR   @14,@14                                             0065
         CR    R15,@14                                             0065
         BE    @RF00065                                            0065
*         DO;                       /* ERROR CAUSES EXIT     #Y30LB04*/
*           ALLOCPTR=ZERO;          /* ZERO POINTER TO UNIT LIST   0067
*                                                            @ZA13481*/
         L     @10,ALOULPTR(,ALLOPTR)                              0067
         ST    @14,ALLOCPTR(,@10)                                  0067
*           GOTO MERET;             /* MAKE A HASTY EXIT     #Y30LB04*/
         B     MERET                                               0068
*         END;                      /* BACK TO CALLER WITHOUT LIST 0069
*                                                            #Y30LB04*/
*       ELSE                                                       0070
*         ;                         /* ALL WENT WELL FOR ICBMEDR   0070
*                                                            #Y30LB04*/
@RF00065 DS    0H                                                  0071
*       ACALLCNT=ONE;               /* COUNT THIS FIRST ENTRY      0071
*                                                            #Y30LB04*/
         L     @14,ALOWKPTR(,ALLOPTR)                              0071
         MVC   ACALLCNT(4,@14),@CF00063                            0071
*     END;                          /* FIRST TIME PROCESSING #Y30LB04*/
*   ELSE                                                           0073
*     ;                             /* NOT FIRST TIME, NOTHING     0073
*                                      SPECIAL               #Y30LB04*/
@RF00061 DS    0H                                                  0074
*   FABFLAGP=ADDR(FABFLAGS);        /* SET FLAGS FOR IEFAB4UV      0074
*                                                            #Y30LB04*/
         LA    @14,FABFLAGS                                        0074
         ST    @14,FABFLAGP                                        0074
*   FABFLAGS=MEFLAGS;               /* TELL IEFAB4UV THIS IS MSSC  0075
*                                                            @Y30LB04*/
         MVC   FABFLAGS(2),MEFLAGS                                 0075
*   FABUNITP=ADDR(UNITSTUF);        /* LOCATE UNIT INFO FOR IT     0076
*                                                            #Y30LB04*/
         LA    @14,UNITSTUF                                        0076
         ST    @14,FABUNITP                                        0076
*   FABULIST=ZERO;                  /* ZERO FOR IEFAB4UV TO FILL IN
*                                                            #Y30LB04*/
         SLR   @14,@14                                             0077
         ST    @14,FABULIST                                        0077
*   ACALL=ACALLCNT;                 /* NUMBER OF TIMES CALLED      0078
*                                                            #Y30LB04*/
         L     @14,ALOWKPTR(,ALLOPTR)                              0078
         L     @14,ACALLCNT(,@14)                                  0078
         ST    @14,ACALL                                           0078
*   CONTINUE=YES;                   /* STILL LOOKING FOR STAGING   0079
*                                      DRIVE GROUP           @Y30LB04*/
         OI    CONTINUE,B'01000000'                                0079
*   DO ACALLCNT=ACALL TO LISTLEN WHILE CONTINUE=YES;/*       #Y30LB04*/
         B     @DE00080                                            0080
@DL00080 TM    CONTINUE,B'01000000'                                0080
         BNO   @DC00080                                            0080
*     R1=ADDR(FABPARMS);            /* SET UP PARAMETERS     #Y30LB04*/
         LA    R1,FABPARMS                                         0081
*     FABUNAME=SDGPTR->SDGNNBBB(ACALLCNT);/* UNIT NAME       @ZA08838*/
         L     @14,ALOWKPTR(,ALLOPTR)                              0082
         L     @14,ACALLCNT(,@14)                                  0082
         SLA   @14,3                                               0082
         ALR   @14,SDGPTR                                          0082
         AL    @14,@CF01230                                        0082
         MVC   FABUNAME(8),SDGNNBBB(@14)                           0082
*     R15=ICBFAB;                   /* ADDRESS OF IEFAB4UV   #Y30LB04*/
         L     @10,CVTPTR                                          0083
         L     @10,CVTEXT2(,@10)                                   0083
         L     @10,CVTICB(,@10)                                    0083
         L     R15,ICBFAB(,@10)                                    0083
*     RESPECIFY                                                    0084
*       R14 RESTRICTED;             /* RESTRICT REGISTER 14  @Y30LB04*/
*IIEFAB4U:                          /* USE OF IEFAB4UV       @YD00151*/
*     GEN(BALR 14,15);              /* GET UNIT LIST         #Y30LB04*/
IIEFAB4U BALR 14,15
*     RFY                                                          0086
*       R14 UNRSTD;                 /* FREE UP REGISTER 14   #Y30LB04*/
*                                                                  0086
*     /***************************************************************/
*     /*                                                             */
*     /* NOTE -- IEFAB4UV BUILDS THE UNIT LIST AND SETS A    #Y30LB04*/
*     /* POINTER TO IT INTO THE ICBME COMMUNICATION AREA     #Y30LB04*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0087
*     IF R15^=GOODCODE              /* DID IEFAB4UV BUILD LIST ?   0087
*                                                            #Y30LB04*/
*       THEN                        /* THE LIST WAS NOT BUILT      0087
*                                                            #Y30LB04*/
         LTR   R15,R15                                             0087
         BZ    @RF00087                                            0087
*       DO;                         /* FIND OUT WHY & WHETHER TO   0088
*                                      CONTINUE              #Y30LB04*/
*         IF R15=NOTAVAIL           /* CANNOT THIS HOST USE THIS   0089
*                                      UNIT?                 #Y30LB04*/
*           THEN                    /* THIS HOST CANNOT USE THIS   0089
*                                      UNIT,WILL LOOP        #Y30LB04*/
         C     R15,@CF00055                                        0089
         BE    @RT00089                                            0089
*           ;                       /* THRU AGAIN FOR NEXT STAGING 0090
*                                      DRIVE GROUP           #Y30LB04*/
*         ELSE                      /* ANOTHER FAILURE TO BUILD THE
*                                      LIST                  #Y30LB04*/
*           DO;                     /* WILL NOT CALL IEFAB4UV AGAIN
*                                      THIS TIME             #Y30LB04*/
*             ALLOCPTR=ZERO;        /* ZERO PTR TO UNIT LIST @ZA13481*/
         L     @14,ALOULPTR(,ALLOPTR)                              0092
         SLR   @10,@10                                             0092
         ST    @10,ALLOCPTR(,@14)                                  0092
*             CONTINUE=NO;          /* TIME TO QUIT CALLING IEFAB4UV
*                                                            #Y30LB04*/
         NI    CONTINUE,B'10111111'                                0093
*           END;                    /* IEFAB4UV RETURN WITHOUT LIST
*                                                            #Y30LB04*/
*       END;                        /* LOGIC FOR NO LIST BUILT     0095
*                                                            #Y30LB04*/
*     ELSE                          /* REG 15 WAS 0, HAVE LIST NOW 0096
*                                                            #Y30LB04*/
*       DO;                         /* PROVIDE ADDR FOR CALLER OF  0096
*                                      ICBME                 #Y30LB04*/
         B     @RC00087                                            0096
@RF00087 DS    0H                                                  0097
*         ALLOCPTR=FABULIST;        /* UNIT LIST ADDR INTO ALLOC AREA
*                                                            @ZA13481*/
         L     @14,ALOULPTR(,ALLOPTR)                              0097
         L     @10,FABULIST                                        0097
         ST    @10,ALLOCPTR(,@14)                                  0097
*         CONTINUE=NO;              /* TIME TO QUIT CALLING IEFAB4UV
*                                                            #Y30LB04*/
         NI    CONTINUE,B'10111111'                                0098
*       END;                        /* LIST TO CALLER COMMUNICATION
*                                                            #Y30LB04*/
*   END;                            /* OF THE DO WHILE       #Y30LB04*/
@RC00087 L     @10,ALOWKPTR(,ALLOPTR)                              0100
         LA    @14,1                                               0100
         AL    @14,ACALLCNT(,@10)                                  0100
@DE00080 L     @10,ALOWKPTR(,ALLOPTR)                              0100
         ST    @14,ACALLCNT(,@10)                                  0100
         C     @14,@CF01056                                        0100
         BNH   @DL00080                                            0100
@DC00080 DS    0H                                                  0101
*MERET:                             /*                       #Y30LB04*/
*   RETURN;                         /* BACK TO ICBME CALLER  #Y30LB04*/
@EL00001 L     @13,4(,@13)                                         0101
@EF00001 L     @00,@SIZDATD                                        0101
         LR    @01,@11                                             0101
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0101
         BR    @14                                                 0101
*                                                                  0102
*/********************************************************** #Y30LB04*/
*/*                                                          #Y30LB04*/
*/*   ICBMEDR: MSSC MOUNT EQUALIZATION AND DATA REUSE        #Y30LB04*/
*/*                                                          #Y30LB04*/
*/*   INPUT: POINTER TO ALLOCATION PARAMETER LIST            #Y30LB04*/
*/*            MSSC ACTIVITY BUFFER (FOUND VIA CVT AND MSSC  #Y30LB04*/
*/*                        CONTROL BLOCK)                    #Y30LB04*/
*/*                                                          #Y30LB04*/
*/* PROCESSING: COPIES MSSC ACTIVITY BUFFER INTO ALLOCATION  #Y30LB04*/
*/*               BUFFER                                     #Y30LB04*/
*/*            ENTERS KEY ZERO FOR LOCK ENTRIES AND MSSC     #Y30LB04*/
*/*              I/O DRIVER USE                              #Y30LB04*/
*/*            FOR SPECIFIC VOLUME, DOES DIAGNOSTIC READ TO  #Y30LB04*/
*/*              MSC FOR MOUNTED VOLUME TABLE ENTRY          #Y30LB04*/
*/*            IF THAT VOLUME WAS MOUNTED, FINDS THE         #Y30LB04*/
*/*              CORRESPONDING STAGING DRIVE GROUP IN THE    #Y30LB04*/
*/*              MSSC ACTIVITY BUFFER LIST AND MOVES IT TO   #Y30LB04*/
*/*              THE TOP OF THE LIST IN ALLOCATION COPY      #Y30LB04*/
*/*            RETURNS TO KEY OF SCHEDULER                   #Y30LB04*/
*/*                                                          #Y30LB04*/
*/*   OUTPUT:  BUFFER FOR ALLOCATION TO USE IN CHOOSING      #Y30LB04*/
*/*              VOLUME                                      #Y30LB04*/
*/*          R15 IS ZERO IF ALL WENT WELL                    #Y30LB04*/
*/*          R15 CONTAINS ERROR CODE IF ANY PROBLEM          #Y30LB04*/
*/*                                                          #Y30LB04*/
*/********************************************************** #Y30LB04*/
*                                                                  0102
*ICBMEDR:                                                          0102
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/* DATA REUSE       #Y30LB04*/
         B     @EL00001                                            0102
ICBMEDR  DS    0H                                                  0103
*   DCL                                                            0103
*     MEDR14 PTR(31) AUTOMATIC;     /* FOR RETURN ADDRESS    @YD00151*/
*   DCL                                                            0104
*     MELOCSAV(5) POINTER(31) AUTOMATIC;/* FOR SETLOCK       #Y30LB04*/
*   RFY                                                            0105
*     R14 RSTD;                     /* RESTRICT REGISTER 14  #Y30LB04*/
*   MEDR14=R14;                     /* SAVE RETURN ADDRESS   #Y30LB04*/
         ST    R14,MEDR14                                          0106
*   RFY                                                            0107
*     R14 UNRSTD;                   /* FREE UP REGISTER 14   #Y30LB04*/
*   RFY                                                            0108
*     R13 RSTD;                     /* RESTRICT REGISTER 13  #Y30LB04*/
*   R13SAVE=R13;                    /* SAVE OWN REGISTER 13  #Y30LB04*/
         ST    R13,R13SAVE                                         0109
*   R13=ADDR(MELOCSAV);             /* PROVIDE REGSAVE TO SETLOCK  0110
*                                                            #Y30LB04*/
         LA    R13,MELOCSAV                                        0110
*IMODESE1:                          /* FIRST ICBME MODESET USE     0111
*                                                            #Y30LB04*/
*   DO;                             /* MODESET KEYADDR(ZERO)         */
IMODESE1 DS    0H                                                  0112
*     SPKA(ZERO);                   /* SET PSW KEY                   */
         SLR   @14,@14                                             0112
         SPKA  0(@14)                                              0112
*   END;                            /* MODESET KEYADDR(ZERO) GET INTO
*                                      KEY 0                 #Y30LB04*/
*ISTAE1:                            /* FIRST STAE MACRO DEPENDENCY IN
*                                      ICBME                 @ZA08838*/
*/* ******************************************************** @ZA08838*/
*/*  STAE REMOTE LIST FOR AUTOMATIC STORAGE                  @ZA08838*/
*/*  NOTE (DEPENDCY) -- THE LENGTH OF STAERL MUST BE         @ZA08838*/
*/*  THE NUMBER OF BYTES GENERATED BY THE EXPANSION OF       @ZA08838*/
*/*  THE STATEMENT STAE MF=L, FOR USE AS A REQUEST LIST      @ZA08838*/
*/*  AT THE STATEMENT LABELED ISTAE2 IN THIS SEGMENT         @ZA08838*/
*/* ******************************************************** @ZA08838*/
*                                                                  0114
*   DCL                                                            0114
*     STAERL CHAR(12) BDY(WORD);    /* STAE REMOTE LIST      @ZA08838*/
ISTAE1   DS    0H                                                  0115
*   BASEVALU=R12;                   /* SAVE BASE ADDRESS FOR MEREXIT
*                                                            @ZA08838*/
         ST    R12,BASEVALU                                        0115
*   STAERL=''B;                     /* ZERO STAE REMOTE PARM LIST  0116
*                                                            @ZA08838*/
         XC    STAERL(12),STAERL                                   0116
*   RFY                                                            0117
*     R1 RSTD;                      /* RESTRICT REGISTER 1   @ZA08838*/
*   R1=ADDR(STAERL);                /* ADDRESS OF STAE LIST  @ZA08838*/
         LA    R1,STAERL                                           0118
*ISTAE2:                            /* ESTABLISH STAE ENVIRONMENT  0119
*                                                            @ZA08838*/
*   GEN(STAE MEREXIT,PARAM=(R11),MF=(E,(1)))/* RECOVERY EXIT @ZA08838*/
*   REFS(MEREXIT,R11,R1) SETS(R15); /* MEREXIT ROUTINE       @ZA08838*/
ISTAE2   STAE MEREXIT,PARAM=(R11),MF=(E,(1))
*ISETLOC1:                          /*                       #Y30LB04*/
*   GEN REFS(R0,R1,R15,PSALITA)     /*                       #Y30LB04*/
*       SETS(R15);                                                 0120
ISETLOC1 DS    0H                                                  0120
*                                                         /*#Y30LB04*/
       SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=SAVE,RELATED=(LOCALL**
               OCK,ICBME(ISETLOC3,ISETLOC5))                  @YD00151
*ISETLOC2:                          /*                       #Y30LB04*/
*   GEN REFS(R0,R1,R15,PSALITA)     /*                       #Y30LB04*/
*       SETS(R15);                                                 0121
*                                                                  0121
ISETLOC2 DS    0H                                                  0121
*                                                       /*  #Y30LB04*/
       SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=SAVE,RELATED=(LOCALLOC**
               K,ICBME(ISETLOC4))                             @YD00151
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW COPY THE ACTIVITY BUFFER INTO ALLOCATION          #Y30LB04*/
*   /* PARAMETER LIST, WITH PROTECTION OF LOCAL & CMS LOCKS  #Y30LB04*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0122
*   ALOBUF=ICBOPT->SDGLS;           /* FROM ACTIVITY BUFFER INTO   0122
*                                      ALLOCATION BUFFER     @ZA08838*/
*                                                                  0122
         L     @14,ALOWKPTR(,ALLOPTR)                              0122
         L     @10,CVTPTR                                          0122
         L     @10,CVTEXT2(,@10)                                   0122
         L     @10,CVTICB(,@10)                                    0122
         L     @10,ICBOPT(,@10)                                    0122
         MVC   ALOBUF(240,@14),SDGLS(@10)                          0122
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE THE CMS AND LOCAL LOCKS, NO LONGER REQUIRED      #Y30LB04*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0123
*ISETLOC3:                          /*                       @YD00151*/
*   GEN REFS(R0,R1,R15,PSALITA)     /*                       #Y30LB04*/
*       SETS(R15);                                                 0123
ISETLOC3 DS    0H                                                  0123
*                                                        /* #Y30LB04*/
       SETLOCK RELEASE,TYPE=CMS,REGS=SAVE,RELATED=(CMSLOCK,ICBME(ISETLOO
               C2))                                           @Y30LB04
*ISETLOC4:                          /*                       @YD00151*/
*   GEN REFS(R0,R1,R15,PSALITA)     /*                       #Y30LB04*/
*       SETS(R15);                                                 0124
ISETLOC4 DS    0H                                                  0124
*                                                         /*#Y30LB04*/
       SETLOCK RELEASE,TYPE=LOCAL,REGS=SAVE,RELATED=(LOCALLOCK,ICBME(I**
               SETLOC1))                                      @Y30LB04
*   IF AVOLWORD=NOVOLID             /* WAS NO VOLUME SPECIFIED     0125
*                                                            #Y30LB04*/
*     THEN                          /* NOT SPECIFIC REQUEST  #Y30LB04*/
         L     @14,ALOVLIPT(,ALLOPTR)                              0125
         ICM   @10,15,AVOLWORD(@14)                                0125
         BZ    @RT00125                                            0125
*     ;                             /* SO JUST RETURN        #Y30LB04*/
*   ELSE                            /* HAVE SPECIFIC VOLUME REQUEST
*                                                            #Y30LB04*/
*     DO;                           /* SEE IF MOUNTED TO MSC #Y30LB04*/
*       CALL VOLENTR;               /* GETS MOUNTED VOLUME TABLE   0128
*                                      ENTRY                 #Y30LB04*/
*                                                                  0128
         BAL   @14,VOLENTR                                         0128
*       /*************************************************************/
*       /*                                                           */
*       /* THERE MAY OR NOT BE A MOUNTED VOLUME TABLE ENTRY  #Y30LB04*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0129
*       IF R15^=GOODCODE            /* WAS THERE AN ERROR ?  #Y30LB04*/
*         THEN                      /* SOME VOLENTR (ETC) PROBLEM  0129
*                                                            #Y30LB04*/
         LTR   R15,R15                                             0129
         BNZ   @RT00129                                            0129
*         ;                         /* NO MORE FOR THIS TIME #Y30LB04*/
*       ELSE                        /* NO ERROR FROM VOLENTR #Y30LB04*/
*         DO;                       /* CONTINUE PROCESSING   #Y30LB04*/
*           IF MOUNTED=NO           /* WAS VOL NOT MOUNTED ? #Y30LB04*/
*             THEN                  /* NO MOUNTED VOLUME TABLE ENTRY
*                                      IN MSC                #Y30LB04*/
         TM    MOUNTED,B'10000000'                                 0132
         BZ    @RT00132                                            0132
*             ;                     /* ACTIVITY BUFFER RETURNS AS IS
*                                                            #Y30LB04*/
*           ELSE                    /* VOLUME WAS MOUNTED    #Y30LB04*/
*             DO;                   /* MAY REARRANGE BUFFER  #Y30LB04*/
*               CALL MATCHSRT;      /* MATCH & PUT ON TOP OF BUFFER
*                                                            @Y30LB04*/
         BAL   @14,MATCHSRT                                        0135
*             END;                  /* PROCESSING WHEN VOLUME MOUNTED
*                                                            #Y30LB04*/
*         END;                      /* SPECIFIC VOLUME REQUEST,NO  0137
*                                      ERROR                 #Y30LB04*/
@RT00132 DS    0H                                                  0138
*     END;                          /* SPECIFIC VOLUME PROCESSING  0138
*                                                            #Y30LB04*/
@RT00129 DS    0H                                                  0139
*   RFY                                                            0139
*     R14 RSTD;                     /* RESTRICT REGISTER 14  @Y30LB04*/
@RT00125 DS    0H                                                  0140
*MEDRRET:                           /*                       @Y30LB04*/
*IMODESE2:                          /* 2ND USE OF MODESET MACRO IN 0140
*                                      ICBME                 @Y30LB04*/
*   DO;                             /* MODESET EXTKEY(SCHED)         */
MEDRRET  DS    0H                                                  0140
IMODESE2 DS    0H                                                  0141
*     SPKA(16);                     /* SET PSW KEY                   */
         LA    @10,16                                              0141
         SPKA  0(@10)                                              0141
*   END;                            /* MODESET EXTKEY(SCHED) BACK TO
*                                      SCHEDULER KEY         @Y30LB04*/
*ISTAE3:                            /* INVOKE STAE MACRO     @ZA08838*/
*   GEN(STAE 0);                    /* DELETE STAE PROTECTION      0143
*                                                            @ZA08838*/
ISTAE3   STAE 0
*   R14=MEDR14;                     /* RESTORE RETURN TO CALLER    0144
*                                                            @Y30LB04*/
         L     R14,MEDR14                                          0144
*   RFY                                                            0145
*     R14 UNRSTD;                   /* FREE UP REGISTER 14   @Y30LB04*/
*   R13=R13SAVE;                    /* RESTORE OWN REGISTER 13     0146
*                                                            @Y30LB04*/
         L     R13,R13SAVE                                         0146
*   RFY                                                            0147
*     R13 UNRSTD;                   /* FREE UP REGISTER 13   @Y30LB04*/
*/********************************************************** @ZA08838*/
*/*                                                          @ZA08838*/
*/*     MEREXIT -- RECOVERY EXIT ROUTINE FOR ICBME,ICBMEDR   @ZA08838*/
*/*                                                          @ZA08838*/
*/*  INPUT:                                                  @ZA08838*/
*/*    R14 HAS THE RETURN ADDRESS                            @ZA08838*/
*/*    R12 TELLS WHETHER A STAE WORK AREA WAS ALLOCATED      @ZA08838*/
*/*   IF THERE WAS NONE,                                     @ZA08838*/
*/*     R2 HAS ADDRESS OF USER PARAMETER LIST                @ZA08838*/
*/*   IF THERE IS ONE                                        @ZA08838*/
*/*     R0 HAS INFORMATION ABOUT I/O ACTIVITY (NOT USED)     @ZA08838*/
*/*     R1 HAS ADDRESS OF STAE WORK AREA, IN WHICH IS        @ZA08838*/
*/*        A POINTER TO USER PARAMETER LIST                  @ZA08838*/
*/*                                                          @ZA08838*/
*/* PROCESSING:  MEREXIT ESTABLISHES ADDRESSABILITY AND      @ZA08838*/
*/*     REASSUMES THE KEY OF THE SCHEDULER, SO THE           @ZA08838*/
*/*     RECOVERY TERMINATION ROUTINES OF ITS CALLERS WILL    @ZA08838*/
*/*     BE ENTERED IN THE APPROPRIATE KEY                    @ZA08838*/
*/*  MEREXIT SETS R15 TO ZERO TO CONTINUE WITH               @ZA08838*/
*/*  TERMINATION                                             @ZA08838*/
*/*  MEREXIT RETURNS AS DIRECTED BY R14 ON ENTRY             @ZA08838*/
*/*                                                          @ZA08838*/
*/*  OUTPUT: NONE                                            @ZA08838*/
*/*                                                          @ZA08838*/
*/*********************************************************  @ZA08838*/
*                                                                  0148
*MEREXIT:                                                          0148
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @ZA08838*/
         B     @PB00003                                            0148
MEREXIT  DS    0H                                                  0149
*   GEN(USING *,R15);               /* ESTABLISH INITIAL           0149
*                                      ADDRESSABILITY        @ZA08838*/
         USING *,R15
*   RESPECIFY                                                      0150
*    (R0,                                                          0150
*     R2,                                                          0150
*     R14) RSTD;                    /*                       @ZA08838*/
*   IF R0=HVNOSDWA THEN             /* IS THERE NO STORAGE WORK AREA
*                                                            @ZA08838*/
         C     R0,@CF00033                                         0151
         BNE   @RF00151                                            0151
*     DO;                           /*                       @ZA08838*/
