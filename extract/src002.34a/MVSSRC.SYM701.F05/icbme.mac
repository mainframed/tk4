         LR    @07,@10                                             0245
         SLA   @07,3                                               0245
         ST    @07,@TF00001                                        0245
         ALR   @07,SDGPTR                                          0245
         AL    @07,@CF01230                                        0245
         LR    @02,SDGPTR                                          0245
         AL    @02,@TF00001                                        0245
         AL    @02,@CF01236                                        0245
         MVC   SDGNNBBB(8,@07),SDGNNBBB(@02)                       0245
*       END;                        /* NOW HAVE ROOM AT TOP FOR REUSE
*                                      SDG                   @ZA08838*/
*                                                                  0246
         BCTR  @10,0                                               0246
         ST    @10,IMOVE                                           0246
@DE00244 C     @10,@CF00075                                        0246
         BNL   @DL00244                                            0246
*       /*************************************************************/
*       /*                                                           */
*       /* NOW MOVE THE REUSE SDG INTO TOP POSITION          @ZA08838*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0247
*       SDGPTR->SDGNNBBB(1)=REUSSDG;/*                       @ZA08838*/
         MVC   SDGNNBBB(8,SDGPTR),REUSSDG                          0247
*     END;                          /* OF SHIFTING PROCESS   @Y30LB04*/
*   RFY                                                            0249
*     R14 RSTD;                     /* RESTRICT REGISTER FOURTEEN  0249
*                                                            @Y30LB04*/
@RT00240 DS    0H                                                  0250
*   R14=MAT14;                      /* RESTORE RETURN ADDRESS      0250
*                                                            @Y30LB04*/
         L     R14,MAT14                                           0250
*   RFY                                                            0251
*     R14 UNRSTD;                   /* FREE UP REGISTER FOURTEEN   0251
*                                                            @Y30LB04*/
*   RESPECIFY                                                      0252
