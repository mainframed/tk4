         L     @10,CCACPE2(,@10)                                   0442
         MVC   EXTGOCI(3,@10),CCACHAIN(R11)                        0442
*     END;                          /* END SET                       */
*   ELSE                                                           0444
*     ;                             /* NO RCD FOLLOWS                */
@RF00440 DS    0H                                                  0445
*   R2=RCDSIZ+CCAVARLN-MXRCD;       /* GET LENGTH OF OVERFLOW        */
