         TITLE 'IGG0CLBW, DELETE/INSERT (MODIFY)                       *
                        '
IGG0CLBW CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGG0CLBW  75.012'                                 0001
         DROP  @15
@PROLOG  DS    0H                                                  0002
         USING @MAINENT,@15
         USING @DATD-(CCATEMPS-IGGCCA),R11
         DS 0F
         DC CL8'IGGPDEIN'
         EJECT
         B     @PB00002                                            0083
IGGPDEIN DS    0H                                                  0084
         ENTRY IGGPDEIN
         DROP @15
Z003     DS    0H                                                  0086
Z003DEIN DS    0H                                                  0087
         STM 12,14,12(13)
         LA 13,12(13)
         BALR 12,0
IGGSEGST DS 0H
         USING *,12
*   CCAREPCT=CNTVAR;                /* SAVE COUNT OF VAR FIELDS IN 0094
*                                      OCCURRENCE                    */
         L     @10,CCACPE6(,R11)                                   0094
         SLR   @05,@05                                             0094
         IC    @05,CNTVAR(,@10)                                    0094
         STH   @05,CCAREPCT(,R11)                                  0094
*   CCAVARLN=MXRCD-RCDSIZ-LENGTH(RUNLNCDE)+1;/* SET TOTAL AVAIL SPACE
*                                      IN LOGICAL RCD                */
         L     @10,CCAURAB(,R11)                                   0095
         L     @10,CCACPE1(,@10)                                   0095
         LA    @05,505                                             0095
         ICM   @15,12,RCDSIZ(@10)                                  0095
         SRA   @15,16                                              0095
         SLR   @05,@15                                             0095
         BCTR  @05,0                                               0095
         BCTR  @05,0                                               0095
         STH   @05,CCAVARLN(,R11)                                  0095
*   CCATLEN=CCAILNG;                /* SET CURRENT TOTAL DISPLACEMENT*/
         L     @05,CCAILNG(,R11)                                   0096
         STH   @05,CCATLEN(,R11)                                   0096
*   CCACI1=RCDCINO;                 /* SAVE CI NO. OF CURRENT RCD  0097
*                                      WITH FIXED INFO               */
         MVC   CCACI1(3,R11),RCDCINO(@10)                          0097
*   CCACI2=0;                       /* INDICATE NO CI                */
         SLR   @10,@10                                             0098
         STCM  @10,7,CCACI2(R11)                                   0098
*   CCACI3=0;                       /* INDICATE NO CI                */
         STCM  @10,7,CCACI3(R11)                                   0099
*   CCAMEX1=OFF;                    /* ENTER SEARCH LOOP             */
         NI    CCAMEX1(R11),B'11011111'                            0100
*   DO WHILE(CCAMEX1=OFF&CCACD1=RCS);/* SEARCH FOR RECORD WITH     0101
*                                      DELETE/INSERT START           */
         B     @DE00101                                            0101
@DL00101 DS    0H                                                  0102
*     IF CCADISP<=CCATLEN THEN      /* IF USER DISPLACEMENT WITHIN 0102
*                                      CURRENT TOTAL,HAVE FOUND    0102
*                                      DELETE/INSERT POINT           */
         LH    @10,CCADISP(,R11)                                   0102
         LH    @05,CCATLEN(,R11)                                   0102
         CR    @10,@05                                             0102
         BH    @RF00102                                            0102
*       DO;                         /* PROCESS DELETE/INSERT         */
*         CCADISP=CCAILNG-CCATLEN+CCADISP;/* ADJUST DISPLACEMENT FOR
*                                      THIS RECORD                   */
         LCR   @05,@05                                             0104
         AL    @05,CCAILNG(,R11)                                   0104
         ALR   @05,@10                                             0104
         STH   @05,CCADISP(,R11)                                   0104
*         CCARBU1=ON;               /* DONT USE RCD AREA             */
         L     @10,CCAURAB(,R11)                                   0105
         OI    CCARBU1(@10),B'01000000'                            0105
*         IF CCAVARLN>=CCAILNG2 THEN/* IF AVAILABLE SPACE IN PREVIOUS
*                                      RECORDS                       */
         LH    @10,CCAVARLN(,R11)                                  0106
         C     @10,CCAILNG2(,R11)                                  0106
         BL    @RF00106                                            0106
*           DO;                     /* USE PREVIOUS RCDS             */
*Z004RISE:                                                         0108
*             CALL IGGPRISE;        /* INSERT INFO UPWARD            */
Z004RISE BAL   @14,IGGPRISE                                        0108
*           END;                    /* END USE                       */
*         ELSE                      /* NOT ENOUGH SPACE              */
*           DO;                     /* USE NEXT RCDS                 */
         B     @RC00106                                            0110
@RF00106 DS    0H                                                  0111
*Z005SINK:                                                         0111
*             CALL IGGPSINK;        /* INSERT INFO DOWNWARD          */
Z005SINK BAL   @14,IGGPSINK                                        0111
*Z006SNK2:                                                         0112
*             CALL IGGPSNK2;        /* UPDATE RECORDS                */
Z006SNK2 BAL   @14,IGGPSNK2                                        0112
*           END;                    /* END USE                       */
*         CCAMEX1=ON;               /* PROCESSING DONE               */
@RC00106 OI    CCAMEX1(R11),B'00100000'                            0114
*       END;                        /* END PROCESS                   */
*     ELSE                          /* DELETE/INSERT POINT FARTHER ON*/
*       IF CCAGVEXT^=0 THEN         /* IF CHAINED RCD                */
         B     @RC00102                                            0116
@RF00102 ICM   @10,7,CCAGVEXT(R11)                                 0116
         BZ    @RF00116                                            0116
*         DO;                       /* GET IT                        */
*                                                                  0117
*           /*********************************************************/
*           /*                                                       */
*           /* CALL TO MODULE IGG0CLBA                               */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0118
*Z007GVAL:                                                         0118
*           CALL IGGPGVAL;          /* GET RECORD                    */
Z007GVAL L     @15,@CV00044                                        0118
         BALR  @14,@15                                             0118
*           CCACI3=CCACI2;          /* PUSH DOWN LIST OF CURRENT CIS */
         MVC   CCACI3(3,R11),CCACI2(R11)                           0119
*           CCACI2=CCACI1;          /* PUSH                          */
         MVC   CCACI2(3,R11),CCACI1(R11)                           0120
*           CCACI1=RCDCINO;         /* SAVE CURRENT CI NO.           */
         L     @10,CCAURAB(,R11)                                   0121
         L     @10,CCACPE1(,@10)                                   0121
         MVC   CCACI1(3,R11),RCDCINO(@10)                          0121
*           CCAVARLN=CCAVARLN+MXRCD-RCDSIZ-/* INCR TOTAL AVAILABLE   */
*               LENGTH(RUNLNCDE)+1; /* SPACE                         */
         LA    @05,505                                             0122
         AH    @05,CCAVARLN(,R11)                                  0122
         ICM   @10,12,RCDSIZ(@10)                                  0122
         SRA   @10,16                                              0122
         SLR   @05,@10                                             0122
         BCTR  @05,0                                               0122
         BCTR  @05,0                                               0122
         STH   @05,CCAVARLN(,R11)                                  0122
*           CCATLEN=CCATLEN+CCAILNG-LENGTH(VARLEN);/* INCR DISPL     */
         LH    @10,CCATLEN(,R11)                                   0123
         AL    @10,CCAILNG(,R11)                                   0123
         BCTR  @10,0                                               0123
         BCTR  @10,0                                               0123
         STH   @10,CCATLEN(,R11)                                   0123
*         END;                      /* END GET                       */
*       ELSE                        /* NO CHAINED RCD                */
*         DO;                       /* STOP IT                       */
         B     @RC00116                                            0125
@RF00116 DS    0H                                                  0126
*           CCADISP=CCATLEN;        /* ASSUME ADD AT END             */
         LH    @10,CCATLEN(,R11)                                   0126
         STH   @10,CCADISP(,R11)                                   0126
*         END;                      /* END STOP                      */
*   END;                            /* END SEARCH                    */
@RC00116 DS    0H                                                  0128
@RC00102 DS    0H                                                  0128
@DE00101 TM    CCAMEX1(R11),B'00100000'                            0128
         BNZ   @DC00101                                            0128
         CLI   CCACD1(R11),0                                       0128
         BE    @DL00101                                            0128
@DC00101 DS    0H                                                  0129
*   CALL EXIT;                      /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0129
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */
         DS 0F
*   GEN(DC CL8'IGGPSINK');          /* NAME OF FOLLOWING PROCEDURE   */
         DC CL8'IGGPSINK'
*   END;                            /* END OF INNER PROCEDURE        */
@EL00002 DS    0H                                                  0132
@EF00002 DS    0H                                                  0132
@ER00002 BR    @14                                                 0132
@PB00002 DS    0H                                                  0133
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */
         EJECT
*                                                                  0134
*/********************************************************************/
*/*                                                                  */
*/*        IGGPSINK  -  PROCEDURE  INTERNALLY  CALLED  NESTED        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0134
*IGGPSINK:                                                         0134
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */
         B     @PB00003                                            0134
IGGPSINK DS    0H                                                  0135
*Z008:                              /* LABEL FOR PROCEDURE TRACKING  */
*Z008SINK:                          /* LABEL FOR PROCEDURE TRACKING  */
*   RFY                                                            0135
*    (R5,                                                          0135
*     R6,                                                          0135
*     R7,                                                          0135
*     R8,                                                          0135
*     R9,                                                          0135
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */
Z008     DS    0H                                                  0135
Z008SINK DS    0H                                                  0136
*   RFY                                                            0136
*    (R2,                                                          0136
*     R3,                                                          0136
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */
         STM 12,14,12(13)
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA     0138
*                                      PROCEDURE                     */
         LA 13,12(13)
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**   MODULE NAME =  IGGPSINK(IGG0CLBW)                                *
**                                                                    *
**   DESCRIPTIVE NAME =  SINK FIELD DATA DOWN                         *
**                                                                    *
**   FUNCTION =                                                       *
**      THIS SUB-FUNCTION PERFORMS THE NECESSARY INITIAL PROCESSING   *
**      FOR THE INSERTION OF USER FIELD DATA INTO A VARIABLE LENGTH   *
**      FIELD BY SHIFTING THE FIELD INFORMATION IN THE VARIOUS        *
**      PHYSICAL RECORDS DOWNWARD.  BASICALLY THE PROCESSING CONSISTS *
**      OF DELETING (IGGPSHNK) THE SPECIFIED FIELD DATA FROM THE      *
**      VARIABLE LENGTH FIELD AND THEN INSERTING (IGGPEXPD)           *
**      THE NEW USER FIELD DATA.  COMPLICATIONS ARISE BECAUSE THE     *
**      FIELD DATA FOR DELETION MAY SPAN TWO PHYSICAL RECORDS AND     *
**      BECAUSE THE INSERTION MAY CAUSE AN OVERFLOW INTO THE NEXT     *
**      PHYSICAL RECORD WHICH IN TURN MAY OVERFLOW TO THE NEXT RECORD.*
**      THIS POSSIBLE CASCADING DOWNWARD OF FIELD DATA IS HANDLED     *
**      BY IGGPSNK2.                                                  *
**                                                                    *
**   INPUT =                                                          *
**      IGGCCA INPUT:                                                 *
**         CCADISP - DISPLACEMENT WITHIN THIS PHYSICAL RECORD TO      *
**                   DELETE/INSERT LOCATION.                          *
**         CCADLEN - USER PROVIDED DELETE LENGTH                      *
**         CCAILNG2 - USER PROVIDED INSERT LENGTH                     *
**         CCAURAB - ADDRESS OF RAB WITH PHYSICAL RECORD CONTAINING   *
**                   DELETE/INSERT LOCATION.                          *
**                                                                    *
**   OUTPUT =                                                         *
**      IGGCCA OUTPUT:                                                *
**         CCADELN - LENGTH OF DATA TO BE DELETED IN NEXT RECORD.     *
**         CCAVARLN - LENGTH OF DATA TO BE INSERTED IN NEXT RECORD    *
**         CCAVARPT - ADDRESS OF DATA TO BE INSERTED IN NEXT RECORD.  *
**         CCAVAR - DATA TO BE INSERTED IN NEXT RECORD.               *
**                                                                    *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0139
*   IF CCADISP+CCADLEN-CCAILNG>0    /* IF DELETE LENGTH              */
*     THEN                          /* CROSSES PHYSICAL RECORD       */
         LH    @10,CCADLEN(,R11)                                   0139
         LH    @05,CCADISP(,R11)                                   0139
         ALR   @05,@10                                             0139
         SL    @05,CCAILNG(,R11)                                   0139
         LTR   @05,@05                                             0139
         BNP   @RF00139                                            0139
*     DO;                           /* ADJUST DELETE LENGTH          */
*       CCADELN=CCADISP+CCADLEN-CCAILNG;/* SET COMPUTED DELETE LENGTH
*                                      FOR NEXT RECORD               */
         STH   @05,CCADELN(,R11)                                   0141
*       CCADLEN=CCADLEN-CCADELN;    /* ADJUST DELETE LENGTH FOR THIS
*                                      RECORD                        */
         SLR   @10,@05                                             0142
         STH   @10,CCADLEN(,R11)                                   0142
*     END;                          /* END ADJUST                    */
*   ELSE                            /* NOT CROSS PHYSICAL RCD        */
*     CCADELN=0;                    /* NO DELETION IN NEXT RCD       */
         B     @RC00139                                            0144
@RF00139 SLR   @10,@10                                             0144
         STH   @10,CCADELN(,R11)                                   0144
*   CCADIFF=-CCADLEN;               /* SET DELETE LENGTH             */
@RC00139 LH    @10,CCADLEN(,R11)                                   0145
         LCR   @10,@10                                             0145
         STH   @10,CCADIFF(,R11)                                   0145
*   CCAILNG3=CCAILNG2;              /* SAVE USER INSERT LENGTH       */
         L     @10,CCAILNG2(,R11)                                  0146
         ST    @10,CCAILNG3(,R11)                                  0146
*   CCAILNG2=0;                     /* ZERO INSERT FOR SHRINK        */
*                                                                  0147
         SLR   @10,@10                                             0147
         ST    @10,CCAILNG2(,R11)                                  0147
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL TO MODULE IGG0CLAX                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0148
*Z009SHNK:                                                         0148
*   CALL IGGPSHNK;                  /* DO SPECIFIED USER DELETE      */
Z009SHNK L     @15,@CV00040                                        0148
         BALR  @14,@15                                             0148
*   CCAILNG2=CCAILNG3;              /* RESTORE USER INSERT LEN       */
         L     @10,CCAILNG3(,R11)                                  0149
         ST    @10,CCAILNG2(,R11)                                  0149
*   CCADLEN=0;                      /* USER DELETE LENGTH NOW ZERO   */
         SLR   @05,@05                                             0150
         STH   @05,CCADLEN(,R11)                                   0150
*   R2=RCDSIZ+CCAILNG2-MXRCD;       /* GET AMOUNT OF OVERFLOW        */
         L     @01,CCAURAB(,R11)                                   0151
         L     @01,CCACPE1(,@01)                                   0151
         ICM   R2,12,RCDSIZ(@01)                                   0151
         SRA   R2,16                                               0151
         ALR   R2,@10                                              0151
         SL    R2,@CF00049                                         0151
*   R3=CCAILNG-CCADISP;             /* AMOUNT OF INFO FOLLOWING    0152
*                                      DELETE/INSERT POINT           */
         L     R3,CCAILNG(,R11)                                    0152
         SH    R3,CCADISP(,R11)                                    0152
*   IF DIC2GC=DSDPGC&R2>0 THEN      /* IF SPACE MAP, ADJUST R2       */
         CLI   DIC2GC+372(R11),7                                   0153
         BNE   @RF00153                                            0153
         CR    R2,@05                                              0153
         BNH   @RF00153                                            0153
*     DO;                           /* ADJUST R2 TO RUN LENGTH CODE
*                                      BOUNDARY                      */
*       RFY                                                        0155
*         R5 RSTD;                  /* USE TO GET ADJUSTMENT         */
*       R4=R3+CCAILNG2;             /* SET TOTAL LENGTH TO SCAN      */
         ALR   @10,R3                                              0156
         LR    R4,@10                                              0156
*       CCAMEX1=OFF;                /* ENTER ADJUSTMENT LOOP         */
         NI    CCAMEX1(R11),B'11011111'                            0157
*       DO WHILE(CCAMEX1=OFF);      /* SCAN RUN LENGTH CODES OF USER
*                                      INSERT INFO                   */
         B     @DE00158                                            0158
@DL00158 DS    0H                                                  0159
*         IF R4<R2 THEN             /* IF ADJUSTED BOUNDARY FOUND    */
         CR    R4,R2                                               0159
         BNL   @RF00159                                            0159
*           DO;                     /* GET OUT                       */
*             CCAMEX1=ON;           /* EXIT                          */
         OI    CCAMEX1(R11),B'00100000'                            0161
*             R2=R5;                /* SET ADJUSTED OVERFLOW LENGTH  */
         LR    R2,R5                                               0162
*           END;                    /* END GET OUT                   */
*         ELSE                      /* NOT FOUND                     */
*           IF R4<=R3 THEN          /* TEST TO SEE IF SCAN DONE      */
         B     @RC00159                                            0164
@RF00159 CR    R4,R3                                               0164
         BH    @RF00164                                            0164
*             DO WHILE(CCAMEX1=OFF);/* SCAN RUN LENGTH CODES OF    0165
*                                      CATALOG INFO                  */
         B     @DE00165                                            0165
@DL00165 DS    0H                                                  0166
*               IF R4<R2 THEN       /* IF ADJUSTED BOUNDARY FOUND    */
         CR    R4,R2                                               0166
         BNL   @RF00166                                            0166
*                 DO;               /* GET OUT                       */
*                   CCAMEX1=ON;     /* EXIT                          */
         OI    CCAMEX1(R11),B'00100000'                            0168
*                   R2=R5;          /* RESET ADJUSTED OVERFLOW LENGTH*/
         LR    R2,R5                                               0169
*                 END;              /* END GET OUT                   */
*               ELSE                                               0171
*                 ;                 /* BOUNDARY NOT FOUND            */
@RF00166 DS    0H                                                  0172
*               R5=R4;              /* SAVE CURRENT ADJUSTED OVERFLOW
*                                      LENGTH                        */
         LR    R5,R4                                               0172
*               RFY                                                0173
*                 RUNLNCDE BASED(CCACPE7+CCADISP+R3-R4);/* MAP INFO  */
*               IF RUNCODE<OBRLC THEN/* IF ONE BYTE RUN CODE         */
         LH    @10,CCADISP(,R11)                                   0174
         AL    @10,CCACPE7(,R11)                                   0174
         ALR   @10,R3                                              0174
         SLR   @10,R4                                              0174
         CLI   RUNCODE(@10),250                                    0174
         BNL   @RF00174                                            0174
*                 R4=R4-LENGTH(RUNCODE);/* DECREMENT OVERFLOW        */
         BCTR  R4,0                                                0175
*               ELSE                /* NOT ONE BYTE RUN CODE         */
*                 R4=R4-LENGTH(RUNLNCDE);/* DECREMENT OVERFLOW       */
         B     @RC00174                                            0176
@RF00174 SL    R4,@CF00438                                         0176
*             END;                  /* END SCAN                      */
@RC00174 DS    0H                                                  0177
@DE00165 TM    CCAMEX1(R11),B'00100000'                            0177
         BZ    @DL00165                                            0177
*           ELSE                                                   0178
*             ;                     /* NOT DONE WITH USER INSERT INFO
*                                      SCAN                          */
@RF00164 DS    0H                                                  0179
*         R5=R4;                    /* SAVE CURRENT ADJUSTED OVERFLOW*/
@RC00159 LR    R5,R4                                               0179
*         RFY                                                      0180
*           RUNLNCDE BASED(CCAIDPT2+R3+CCAILNG2-R4);/* INSERT INFO   */
*         IF RUNCODE<OBRLC THEN     /* IF ONE BYTE RUN CODE          */
         LR    @10,R3                                              0181
         AL    @10,CCAIDPT2(,R11)                                  0181
         AL    @10,CCAILNG2(,R11)                                  0181
         SLR   @10,R4                                              0181
         CLI   RUNCODE(@10),250                                    0181
         BNL   @RF00181                                            0181
*           R4=R4-LENGTH(RUNCODE);  /* DECREMENT OVERFLOW            */
         BCTR  R4,0                                                0182
*         ELSE                      /* NOT ONE BYTE RUN CODE         */
*           R4=R4-LENGTH(RUNLNCDE); /* DECREMENT OVERFLOW            */
         B     @RC00181                                            0183
@RF00181 SL    R4,@CF00438                                         0183
*       END;                        /* END SCAN                      */
@RC00181 DS    0H                                                  0184
@DE00158 TM    CCAMEX1(R11),B'00100000'                            0184
         BZ    @DL00158                                            0184
*     END;                          /* END ADJUST                    */
*   ELSE                                                           0186
*     ;                             /* NOT SPACE MAP                 */
@RF00153 DS    0H                                                  0187
*   R4=CCACPE7+CCADISP;             /* POINT TO INSERT/DELETE      0187
*                                      POSITION                      */
         LH    R4,CCADISP(,R11)                                    0187
         AL    R4,CCACPE7(,R11)                                    0187
*   IF R2<=0 THEN                   /* IF NO OVERFLOW, NOTHING TO  0188
*                                      GIVE TO NEXT RCD              */
         SLR   @10,@10                                             0188
         CR    R2,@10                                              0188
         BH    @RF00188                                            0188
*     CCAVARLN=0;                   /* ZERO INSERT LENGTH            */
         STH   @10,CCAVARLN(,R11)                                  0189
*   ELSE                            /* OVERFLOW, SO MUST PASS SOME 0190
*                                      INFO TO NEXT RCD              */
*     DO;                           /* SET UP INSERT OF INFO INTO  0190
*                                      NEXT RCD                      */
         B     @RC00188                                            0190
@RF00188 DS    0H                                                  0191
*       CCAVARLN=R2;                /* SET OVERFLOW INSERT LEN       */
         STH   R2,CCAVARLN(,R11)                                   0191
*       IF R2<=R3 THEN              /* IF NO USER INSERT INFO      0192
*                                      OVERFLOWS                     */
         CR    R2,R3                                               0192
         BH    @RF00192                                            0192
*         DO;                       /* PASS INFO FOLLOWING         0193
*                                      DELETE/INSERT POINT           */
*           RFY                                                    0194
*             RCDMV1 BASED(R4+R3-R2);/* MAP INFO FOR NEXT RCD        */
*           CCAVARPT=ADDR(CCAVAR)-LENGTH(VARLEN);/* ADDR OF INFO     */
         LA    @10,CCAVAR(,R11)                                    0195
         BCTR  @10,0                                               0195
         BCTR  @10,0                                               0195
         ST    @10,CCAVARPT(,R11)                                  0195
*           CCAVAR(1:R2)=RCDMV1(1:R2);/* INSERT INFO                 */
         LR    @10,R2                                              0196
         BCTR  @10,0                                               0196
         LR    @15,R4                                              0196
         ALR   @15,R3                                              0196
         SLR   @15,R2                                              0196
         EX    @10,@SM02801                                        0196
*         END;                      /* END PASS                      */
*       ELSE                        /* USER INSERT INFO CROSSES    0198
*                                      PHYSICAL BOUNDARY             */
*         DO;                       /* PROCESS USER CROSS            */
         B     @RC00192                                            0198
@RF00192 DS    0H                                                  0199
*           IF R3^=0 THEN           /* IF CATALOG RCD INFO FOLLOWS 0199
*                                      DELETE/INSERT POINT           */
         LTR   R3,R3                                               0199
         BZ    @RF00199                                            0199
*             DO;                   /* USE CCA TO SAVE INFO FOR NEXT
*                                      RECORD                        */
*               CCAVARPT=ADDR(CCAVAR)-LENGTH(VARLEN);/* ADDR OF INFO */
         LA    @10,CCAVAR(,R11)                                    0201
         BCTR  @10,0                                               0201
         BCTR  @10,0                                               0201
         ST    @10,CCAVARPT(,R11)                                  0201
*               RFY                                                0202
*                 RCDMV1 BASED(CCAIDPT2+CCAILNG2-R2+R3);/* MAP EXCESS
*                                      USER INFO                     */
*               CCAVAR(1:R2-R3)=    /* SAVE EXCESS USER INSERT       */
*                   RCDMV1(1:R2-R3);/* INFO                          */
         LR    @10,R2                                              0203
         SLR   @10,R3                                              0203
         BCTR  @10,0                                               0203
         L     @15,CCAIDPT2(,R11)                                  0203
         AL    @15,CCAILNG2(,R11)                                  0203
         SLR   @15,R2                                              0203
         ALR   @15,R3                                              0203
         EX    @10,@SM02801                                        0203
*               RFY                                                0204
*                 RCDMV1 BASED(R4); /* MAP INFO FOLLOWING DELETE   0204
*                                      INSERT POINT                  */
*               CCAVAR(1+R2-R3:R2)=RCDMV1(1:R3);/* SAVE INSERT INFO  */
         LA    @10,1                                               0205
         ALR   @10,R2                                              0205
         SLR   @10,R3                                              0205
         ST    @10,@TF00001                                        0205
