*         DO;                       /* ADJUST OVERFLOW TO RUN LENGTH
*                                      CODE BOUNDARY                 */
*           RFY                                                    0450
*             RUNLNCDE BASED(ADDR(VARINFO)+R3);/* OVERLAY SPACE MAP  */
*           R3=0;                   /* DISPLACEMENT INTO MAP       0451
*                                      INITIALLY ZERO                */
         LR    R3,@10                                              0451
*           DO WHILE(R3<=R2);       /* SCAN FOR RUN CODE JUST AFTER
*                                      OVERFLOW POINT                */
         B     @DE00452                                            0452
@DL00452 DS    0H                                                  0453
*             R4=R3;                /* SAVE REFERENCE TO CURRENT RUN
*                                      CODE                          */
