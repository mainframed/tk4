*Z022GNEX:                                                         0294
*           CALL IGGPGNEX;          /* FORMAT RECORD                 */
Z022GNEX L     @15,@CV00037                                        0294
         BALR  @14,@15                                             0294
*           CCACPE5=ADDR(REPGOPT);  /* POINT TO GOP                  */
         L     @10,CCAURAB(,R11)                                   0295
         L     @05,CCACPE2(,@10)                                   0295
         LA    @15,REPGOPT(,@05)                                   0295
         ST    @15,CCACPE5(,R11)                                   0295
*           CATGOGC1=DIC2GC;        /* SET GROUP CODE                */
*                                                                  0296
         MVC   CATGOGC1(1,@15),DIC2GC+372(R11)                     0296
*           /*********************************************************/
*           /*                                                       */
*           /* SET SEQUENCE NUMBER OF ONE OR TWO BYTES               */
