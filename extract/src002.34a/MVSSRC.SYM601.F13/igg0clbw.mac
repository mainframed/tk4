*           /*********************************************************/
*                                                                  0297
*           CATSN(1:CCASL)=CCALREL1(LENGTH(CCASN)+1-CCASL:LENGTH(CCASN)
*               );                                                 0297
         LH    @14,CCASL(,R11)                                     0297
         LR    @09,@14                                             0297
         BCTR  @09,0                                               0297
         LCR   @14,@14                                             0297
         AL    @14,@CF00438                                        0297
         ALR   @14,R11                                             0297
         EX    @09,@SM02805                                        0297
*           CCACPE3=CCACPE3+CCAGOPLN;/* UPDATE CPE3                  */
         LH    @15,CCAGOPLN(,R11)                                  0298
         L     @14,CCACPE3(,@10)                                   0298
         ALR   @14,@15                                             0298
         ST    @14,CCACPE3(,@10)                                   0298
*           CCACPE6=CCACPE3;        /* POINT TO OCCURRENCE           */
         ST    @14,CCACPE6(,R11)                                   0299
*           LENFIX=LENGTH(CNTRLINF);/* SET LENGTH OF FIXED INFO      */
         MVI   LENFIX(@14),X'02'                                   0300
*           CNTVAR=CCAREPCT;        /* SET COUNT OF VARIABLE FIELDS  */
