*               R3=R3+LENGTH(RUNCODE);/* ADJUST DISPLACEMENT         */
         AL    R3,@CF00065                                         0455
*             ELSE                  /* NOT ONE BYTE RUN CODE         */
*               R3=R3+LENGTH(RUNLNCDE);/* ADJUST DISPLACEMENT TO NEXT
*                                      RUN LENGTH CODE               */
         B     @RC00454                                            0456
@RF00454 AL    R3,@CF00438                                         0456
*           END;                    /* END SCAN                      */
@RC00454 DS    0H                                                  0457
@DE00452 CR    R3,R2                                               0457
         BNH   @DL00452                                            0457
*           R2=R4;                  /* SAVE NEW OVERFLOW LENGTH      */
