         L     @14,CVTPTR                                          0404
         L     @14,CVTTCBP(,@14)                                   0404
         L     @14,TCBPTR(,@14)                                    0404
         LA    @10,96                                              0404
         AL    @10,SVRBPTR(,@14)                                   0404
         L     R13,XRBXSAV(,@10)                                   0404
