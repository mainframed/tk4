*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**   MODULE NAME =  IGGPCKCC                                          *
**                                                                    *
**   DESCRIPTIVE NAME =  GET AND CHECK CATALOG RECORD FOR SECURITY    *
**                                                                    *
**   COPYRIGHT =  NONE                                                *
**                                                                    *
**   STATUS =  RELEASE 1, LEVEL 0                                     *
**                                                                    *
**   FUNCTION =                                                       *
**      THIS INTERNAL PROCEDURE READS THE CATALOG CLUSTER RECORD AND  *
**      CHECKS TO SEE IF IT IS SECURITY PROTECTED.  IF NOT PROTECTED, *
**      THE ORIGINAL BASE RECORD IS RE-READ.                          *
**      1. CLEAR THE CATALOG SECURITY CHECK INDICATOR AND SAVE THE    *
**         ORIGINAL BASE RECORD CI.                                   *
**      2. GET THE CATALOG RECORD THAT CONTAINS SECURITY DATA IF IT   *
**         EXISTS.                                                    *
**      3. CALL INTERNAL PROCEDURE IGGPCKEX TO EXTRACT SECURITY DATA. *
**      4. IF THE CATALOG IS NOT SECURITY PROTECTED:                  *
**         A. THE ORIGINAL RECORD IS RE-READ.                         *
**         B. IF ALTER OR DELETE FUNCTIONS AND THE ORIGINAL BASE WAS  *
**            NOT SECURITY PROTECTED, SET NO VERIFICATION REQUIRED    *
**            CODE.                                                   *
**      5. RETURN                                                     *
**                                                                    *
**   INPUT =                                                          *
**      CCA                                                           *
**        CCAR5REC                                                    *
**          CKAUWKA                                                   *
**            CKAUNPAL                                                *
**          CCAURAB                                                   *
**            CCACPE1                                                 *
**              RCDCINO                                               *
**                                                                    *
**   OUTPUT =                                                         *
**      CCA                                                           *
**        CCAR5REC                                                    *
**          CKAUWKA                                                   *
**            REGETCI                                                 *
**            CKAUBASE                                                *
**            CKAUSTOP                                                *
**          CCACD1                                                    *
**          CCACD2                                                    *
**          CCASRCIN                                                  *
**                                                                    *
**     MESSAGES =  NONE                                               *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0440
*   /*****************************************************************/
*   /*                                                               */
*   /* IGGPCKCC - THIS ROUTINE GETS THE CATALOG RECORD AND CHECKS TO */
*   /* SEE IF IT IS SECURITY PROTECTED BY CALLING IGGPCKEX. IF NOT   */
*   /* PROTECTED AND USED FOR AN UNPROTECTED ALTER REQUEST, THE MAIN */
*   /* RECORD IS RE-READ.                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0440
*   CCACD2=OKAY;                    /* CLEAR THE INDICATOR BYTE      */
         MVI   CCACD2(R11),X'00'                                   0440
*   REGETCI=RCDCINO;                /* SAVE BASE RECORD CI NO.       */
         L     @10,CCAR5REC(,R11)                                  0441
         L     @05,CCAURAB(,R11)                                   0441
         L     @15,CCACPE1(,@05)                                   0441
         MVC   REGETCI(3,@10),RCDCINO(@15)                         0441
*   CKAUBASE=ON;                    /* SET FLAG TO RE-GET BASE       */
         OI    CKAUBASE(@10),B'00010000'                           0442
*   CCASRCIN=CATSECRD;              /* SET CATALOG RECORD CI         */
         MVC   CCASRCIN(3,R11),@CF00056+1                          0443
*   CKAUNOSV=OFF;                   /* INDICATE REC SAVED      Y02020*/
         NI    CKAUNOSV(@10),B'10111111'                           0444
*   CCAR4REC->CATRECRD(1)=CATRECRD(1);/* SAVE THE RECORD             */
         L     @14,CCAR4REC(,R11)                                  0445
         MVC   CATRECRD(256,@14),CATRECRD(@15)                     0445
*   CCAR4REC->CATRECRD(2)=CATRECRD(2);/* SAVE THE RECORD             */
         MVC   CATRECRD+256(256,@14),CATRECRD+256(@15)             0446
*   CCARAB4=CCARAB;                 /* SAVE THE CPE POINTERS         */
         MVC   CCARAB4(20,R11),CCARAB(@05)                         0447
*   CKAUNOSV=OFF;                   /* INDICATE RECORD SAVED         */
         NI    CKAUNOSV(@10),B'10111111'                           0448
*   DO;                                                            0449
*     CCAF1KEY=OFF;                 /* GET BY CI NO.                 */
*     CCAF1KGE=OFF;                 /* NOT GET NEXT                  */
*     CCAF1UP=OFF;                  /* NOT GET UPD                   */
*                                                                  0452
         NI    CCAF1KEY(R11),B'11100101'                           0452
*     /***************************************************************/
*     /*                                                             */
*     /* CALL TO MODULE IGG0CLBI                                     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0453
*Z024GET:                                                          0453
*     CALL IGGPGET;                 /* GET RECORD                    */
Z024GET  L     @15,@CV03736                                        0453
         BALR  @14,@15                                             0453
*   END;                                                           0454
*   IF CCACD1=OKAY THEN             /* NO ERRORS                     */
         CLI   CCACD1(R11),0                                       0455
         BNE   @RF00455                                            0455
*     IF CAXRACP=NONE THEN          /* IF RAC STATUS NOT CHECKED   0456
*                                      ALREADY               @Z40RSRC*/
         L     @10,CCAACB(,R11)                                    0456
         L     @10,ACBUAPTR(,@10)                                  0456
         ICM   @05,15,CAXRACP(@10)                                 0456
         BNZ   @RF00456                                            0456
*       DO;                         /*                       @Z40RSRC*/
*         IF RCDEXSEC=ON THEN       /* IF CATALOG IS RAC-INDICATED 0458
*                                                            @Z40RSRC*/
         L     @05,CCAURAB(,R11)                                   0458
         L     @05,CCACPE1(,@05)                                   0458
         TM    RCDEXSEC(@05),B'10000000'                           0458
         BNO   @RF00458                                            0458
*           CAXRAC=ON;              /* SHOW IS RAC CAT       @Z40RSRC*/
         OI    CAXRAC(@10),B'00010000'                             0459
*         ELSE                                                     0460
*           ;                       /* NOT A RAC CATLG       @Z40RSRC*/
*                                                                  0460
@RF00458 DS    0H                                                  0461
*         /***********************************************************/
*         /*                                                         */
*         /* CALL TO MODULE IGG0CLBM                                 */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0461
*Z025BSGT:                                                         0461
*         CALL IGGPBSGT;            /* REGET BASE REC        @Z40RSRC*/
Z025BSGT L     @15,@CV03808                                        0461
         BALR  @14,@15                                             0461
*       END;                        /*                       @Z40RSRC*/
*     ELSE                          /* ALREADY CHKED RAC     @Z40RSRC*/
*       DO;                         /* GET CAT RCRD SECURITY         */
*                                                                  0463
         B     @RC00456                                            0463
@RF00456 DS    0H                                                  0464
*         /***********************************************************/
*         /*                                                         */
*         /* CALL TO MODULE IGG0CLBM                                 */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0464
*Z026CKEX:                                                         0464
*         CALL IGGPCKEX;            /* EXTRACT SECURITY DATA         */
Z026CKEX L     @15,@CV03737                                        0464
         BALR  @14,@15                                             0464
*         IF CCACD1=OKAY&           /* NO MAJOR ERRORS               */
*             CCACD2^=OKAY THEN     /* NOT SECURITY PROTECTED        */
         CLI   CCACD1(R11),0                                       0465
         BNE   @RF00465                                            0465
         CLI   CCACD2(R11),0                                       0465
         BE    @RF00465                                            0465
*           DO;                     /* RE-GET ORIGINAL RECORD        */
*             CKAUSTOP=ON;          /* SET TO STOP RECORD CHANGE     */
*                                                                  0467
         L     @10,CCAR5REC(,R11)                                  0467
         OI    CKAUSTOP(@10),B'00001000'                           0467
*             /*******************************************************/
*             /*                                                     */
*             /* CALL TO MODULE IGG0CLBM                             */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0468
*Z027BSGT:                                                         0468
*             CALL IGGPBSGT;        /* RE-GET RECORD                 */
Z027BSGT L     @15,@CV03808                                        0468
         BALR  @14,@15                                             0468
*             CKAUVFMT=ON;          /* INDICATE ALL ACCESSABLE       */
*             CKAUVFST=ON;          /* STOP VERIFY PROCESS           */
         L     @10,CCAR5REC(,R11)                                  0470
         OI    CKAUVFMT(@10),B'10100000'                           0470
*           END;                                                   0471
*         ELSE                                                     0472
*           ;                                                      0472
@RF00465 DS    0H                                                  0473
*       END;                                                       0473
*   ELSE                                                           0474
*     ;                             /* ERRORS IN THE PROCESSING      */
@RF00455 DS    0H                                                  0475
*   CALL EXIT;                      /* RETURN TO CALLER              */
@RC00455 BAL   @14,EXIT                                            0475
*   END;                            /* END OF INNER PROCEDURE        */
@EL00006 DS    0H                                                  0476
@EF00006 DS    0H                                                  0476
@ER00006 BR    @14                                                 0476
@PB00006 DS    0H                                                  0477
