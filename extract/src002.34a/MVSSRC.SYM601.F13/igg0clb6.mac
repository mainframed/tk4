         L     R2,CCAR5REC(,R11)                                   0411
         LA    R2,CKAUMDUL+216(,R2)                                0411
*   GEN(DELETE EPLOC=(R2));                                        0412
         DELETE EPLOC=(R2)
*   CCASRCIN=CKBASECI;              /* SET PROPER CI VALUE           */
         L     @14,CCAR5REC(,R11)                                  0413
         MVC   CCASRCIN(3,R11),CKBASECI(@14)                       0413
*   DO;                                                            0414
*     CCAF1KEY=OFF;                 /* GET BY CI NO.                 */
*     CCAF1KGE=OFF;                 /* NOT GET NEXT                  */
