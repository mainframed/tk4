         L     R2,CCAR5REC(,R11)                                   0392
         LA    R2,CKAUMDUL+216(,R2)                                0392
*   R3=CVTLINK;                     /* GET SYS1.LINKLIB DCB ADDR     */
         L     @10,CVTPTR                                          0393
         L     R3,CVTLINK(,@10)                                    0393
*   GEN(LOAD EPLOC=(R2),DCB=(R3));  /* LOAD THE USVR MODULE          */
*                                                                  0394
         LOAD EPLOC=(R2),DCB=(R3)
*   /*****************************************************************/
*   /*                                                               */
*   /* PASS CONTROL TO THE USVR MODULE                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0395
*   RFY                                                            0395
*    (R0,                                                          0395
*     R1,                                                          0395
*     R15) RSTD;                                                   0395
*   R1=CKUSVRPT;                    /* SET USVR INPUT ADDRESS        */
         L     @14,CCAR5REC(,R11)                                  0396
         L     R1,CKUSVRPT(,@14)                                   0396
*   R15=R0;                         /* GET USVR ENTRY POINT ADDR     */
         LR    R15,R0                                              0397
*   RFY                                                            0398
*     RBBASIC BASED(SVRBPTR);                                      0398
*   GEN(STM  11,13,12(13));         /* SAVE REGS 11,12,13 IN CCA     */
         STM  11,13,12(13)
*   SAVEREGL=XRBXSAV;               /* GET FIRST WORD OF EXT S A     */
         L     @14,CVTPTR                                          0400
         L     @14,CVTTCBP(,@14)                                   0400
         L     @14,TCBPTR(,@14)                                    0400
         LA    @10,96                                              0400
         AL    @10,SVRBPTR(,@14)                                   0400
         MVC   SAVEREGL(4,R13),XRBXSAV(@10)                        0400
