*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**   MODULE NAME =  IGGPINMD                                          *
**                                                                    *
**   DESCRIPTIVE NAME =  USVR INTERFACE BUILDER AND PROCESSOR         *
**                                                                    *
**   COPYRIGHT =  NONE                                                *
**                                                                    *
**   STATUS =  RELEASE 1, LEVEL 0                                     *
**                                                                    *
**   FUNCTION =                                                       *
**      THIS EXTERNAL PROCEDURE INTERFACES WITH THE USVR.  IF A NON-  *
**      MASTER LEVEL OF PASSWORD WAS VERIFIED AND A USVR EXISTS, THIS *
**      PROCEDURE RECEIVES CONTROL TO INTERFACE WITH THE USVR.  THE   *
**      FOLLOWING STEPS OUTLINE THE GENERAL PROCEDURE:                *
**      1. GET STORAGE TO CONTAIN THE INFORMATION TO BE PASSED TO THE *
**         USVR.                                                      *
**      2. CONSTRUCT THE USVR INPUT PARAMETER LIST                    *
**      3. CLEAR THE IN CORE RECORDS.                                 *
**      4. LOAD THE USER SPECIFIED VERIFICATION ROUTINE FROM          *
**         SYS1.LINKLIB.                                              *
**      5. SYNCH TO THE LOADED MODULE.                                *
**      6. EXAMINE THE RETURN CODE                                    *
**      7. FREE THE INPUT PARAMETER LIST STORAGE                      *
**      8. RETURN TO THE INVOKER OF THIS PROCEDURE.                   *
**                                                                    *
**   INPUT =                                                          *
**      CCA                                                           *
**        CCAR5REC                                                    *
**          CKAUWKA                                                   *
**            CKAUINFO                                                *
**            CKAUPRMT                                                *
**        CCAR1REC                                                    *
**          RCDCINO                                                   *
**          CNAME                                                     *
**          COWNID                                                    *
**                                                                    *
**   OUTPUT =                                                         *
**      CCA                                                           *
**        CCAR5REC                                                    *
**          CKAUWKA                                                   *
**            CKUSVRPT                                                *
**            CKBASECI                                                *
**            CKAUINFO                                                *
**        CCASRCIN                                                    *
**      USVRPWKA                                                      *
**                                                                    *
**     MESSAGES =  NONE                                               *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0358
*   /*****************************************************************/
*   /*                                                               */
*   /* IGGPINMD - THIS IS AN EXTERNAL ROUTINE THAT BUILDS THE LIST OF*/
*   /* INFORMATION TO PASS TO THE USVR MODULE, LOADS THE USVR MODULE */
*   /* FROM SYS1.LINKLIB AND TRANSFERS CONTROL TO THE USVR VIA A     */
*   /* SYNCH MACRO. WHEN CONTROL IS RETURNED FROM THE USVR, A RETURN */
*   /* CODE IS SET IN REGISTER 15 INDICATING THE CONTINUATION STATUS.*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0358
*   CCAMNLEN=LENGTH(USVRPWKA)+LENUSAR+SAVEASIZ;/* CORE SIZE FOR USVR
*                                      PARM L                        */
         MVC   CCAMNLEN(3,R11),@CF03830+1                          0358
*   RFY                                                            0359
*    (R1) RSTD;                     /* AND USVR REG SAVE AREA        */
*                                                                  0359
*   /*****************************************************************/
*   /*                                                               */
*   /* GET CORE FOR PARM LIST                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0360
*   R1=ADDR(CCAMSSPL);              /* POINT TO MSS PL               */
         LA    R1,CCAMSSPL(,R11)                                   0360
*   CCAMNSPL=USERSTG;               /* USER STG SUBPOOL NO.          */
         MVI   CCAMNSPL(R11),X'FB'                                 0361
*   GEN(GETMAIN LC,MF=(E,(1)));     /* GET USER KEY STORAGE          */
         GETMAIN LC,MF=(E,(1))
*   GEN(LR R1,R15);                 /* PICK UP RETURN CODE           */
         LR R1,R15
*   IF R1^=NONE THEN                /* GET STORAGE OKAY?             */
         LTR   R1,R1                                               0364
         BZ    @RF00364                                            0364
*     DO;                           /* ERROR GETTING STORAGE         */
*                                                                  0365
*       /*************************************************************/
*       /*                                                           */
*       /* SET CCAMODID='B6', CCAREASN=RSN2, CCARETRN=RCINSSWA       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0366
*       CCAPROB=IGGSEGID+(RSN2*K256)+RCINSSWA;/* SET ERROR CODE    0366
*                                                            @Y30SSSB*/
         MVC   CCAPROB(4,R11),@CF03832                             0366
*       CALL EXIT;                  /* ERROR EXIT                    */
         BAL   @14,EXIT                                            0367
*     END;                                                         0368
*   ELSE                                                           0369
*     ;                             /* NO GET STORAGE ERROR          */
@RF00364 DS    0H                                                  0370
*   RFY                                                            0370
*    (R1) UNRSTD;                                                  0370
*   CKUSVRPT=CCAMNADR;              /* SAVE PARM LIST ADDRESS        */
         L     @01,CCAR5REC(,R11)                                  0371
         L     @10,CCAMNADR(,R11)                                  0371
         ST    @10,CKUSVRPT(,@01)                                  0371
*   USVRPDSN=CNAME;                 /* SET OBJECT NAME               */
         L     @01,CCAURAB(,R11)                                   0372
         L     @05,CCACPE1(,@01)                                   0372
         MVC   USVRPDSN(44,@10),CNAME(@05)                         0372
*   IF COWNID=NULLPRMT THEN         /* NULL OWNER ID                 */
         CLC   COWNID(8,@05),NULLPRMT                              0373
         BNE   @RF00373                                            0373
*     USVRPOWN=USVRPOWN&&USVRPOWN;  /* ZERO FIELD                    */
         XC    USVRPOWN(8,@10),USVRPOWN(@10)                       0374
*   ELSE                            /* VALID OWNER ID                */
*     USVRPOWN=COWNID;              /* PASS VALID OWNER ID           */
         B     @RC00373                                            0375
@RF00373 L     @01,CCAR5REC(,R11)                                  0375
         L     @01,CKUSVRPT(,@01)                                  0375
         L     @10,CCAURAB(,R11)                                   0375
         L     @10,CCACPE1(,@10)                                   0375
         MVC   USVRPOWN(8,@01),COWNID(@10)                         0375
*   IF CKAUPRMT=NULLPRMT THEN       /* NULL PROMPTING NAME           */
@RC00373 L     @10,CCAR5REC(,R11)                                  0376
         CLC   CKAUPRMT+216(8,@10),NULLPRMT                        0376
         BNE   @RF00376                                            0376
*     USVRPRMT=USVRPRMT&&USVRPRMT;  /* ZERO PROMPT FIELD             */
         L     @01,CKUSVRPT(,@10)                                  0377
         XC    USVRPRMT(8,@01),USVRPRMT(@01)                       0377
*   ELSE                            /* VALID PROMPTING NAME          */
*     USVRPRMT=CKAUPRMT;            /* PASS PROMPTING NAME           */
         B     @RC00376                                            0378
@RF00376 L     @10,CCAR5REC(,R11)                                  0378
         L     @01,CKUSVRPT(,@10)                                  0378
         MVC   USVRPRMT(8,@01),CKAUPRMT+216(@10)                   0378
*   USVRPSWD=CKAUPSWK;              /* PASS PASSWORD VERIFIED        */
@RC00376 L     @10,CCAR5REC(,R11)                                  0379
         L     @05,CKUSVRPT(,@10)                                  0379
         MVC   USVRPSWD(8,@05),CKAUPSWK(@10)                       0379
*   USVRPLEN=CKAUVRLN;              /* SET USAR INFO LENGTH          */
         LA    @01,216                                             0380
         ALR   @01,@10                                             0380
         LH    @15,CKAUVRLN(,@01)                                  0380
         STH   @15,USVRPLEN(,@05)                                  0380
*   USVRPDTA(1:CKAUVRLN)=CKAUAREC(1:CKAUVRLN);/* PASS USAR           */
         BCTR  @15,0                                               0381
         EX    @15,@SM03850                                        0381
*   CKAUPSWD=CKAUPSWD&&CKAUPSWD;    /* CLEAR IN CORE PASSWORDS       */
         XC    CKAUPSWD(32,@01),CKAUPSWD(@01)                      0382
*   CKBASECI=RCDCINO;               /* SAVE BASE RECORD CI VALUE     */
         L     @01,CCAURAB(,R11)                                   0383
         L     @01,CCACPE1(,@01)                                   0383
         MVC   CKBASECI(3,@10),RCDCINO(@01)                        0383
*   CKAUNOSV=ON;                    /* SET RECORD NOT SAVED          */
         OI    CKAUNOSV(@10),B'01000000'                           0384
*   CCAR4REC=CCAR3REC+CTRECSIZ;     /* SET RECORD AREA POINTER       */
*                                                                  0385
         LA    @10,512                                             0385
         AL    @10,CCAR3REC(,R11)                                  0385
         ST    @10,CCAR4REC(,R11)                                  0385
*   /*****************************************************************/
*   /*                                                               */
*   /* CLEAR ALL IN CORE RECORDS                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0386
*   R2=CCAR1REC;                    /* FIRST RECORD AREA             */
         L     R2,CCAR1REC(,R11)                                   0386
*   R3=NORABS*CTRECSIZ;             /* TOTAL AREA LENGTH             */
         LA    R3,2048                                             0387
*   RFY                                                            0388
*    (R5) RSTD;                                                    0388
*   R5=NONE;                        /* SET PADDING CHARACTER         */
         SLR   R5,R5                                               0389
*   GEN(MVCL R2,R4);                /* CLEAR THE RECORD AREAS        */
         MVCL R2,R4
*   RFY                                                            0391
*    (R5) UNRSTD;                                                  0391
*                                                                  0391
