**        TO UPGRADE MANAGEMENT.                                     *
**      CUMBCI - CALLER'S ORIGINAL BASE RECORD CI NUMBER AT ENTRY    *
**        TO UPGRADE MANAGEMENT.                                     *
**      CUMNRG - IF SET ON, THE CALLER'S ORGINAL BASE RECORD IS NOT  *
**        TO BE RE-GOTTEN.                                           *
**                                                                   *
**   OUTPUT =                                                        *
**      CCAURAB - RESTORED WITH CONTENTS OF CUMURAB                  *
**      CCARABB - RESTORED WITH CONTENTS OF CUMRABB                  *
**      CCAREUSE - SET OFF                                           *
**      CCACD1 - ANY ERROR CODE RETURNED BY GET OR ANY ERROR         *
**        INITIALLY PASSED TO THIS SUB-FUNCTION.                     *
**      CCACD2 - ANY ERROR CODE RETURNED BY GET WHEN ATTEMPTING      *
**        TO RE-GET THE CALLER'S BASE RECORD                         *
**                                                                   *
******************************************************************** */
*                                                                  0482
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FOLLOWING CODE RESTORES THE CALLER'S CCAURAB, CCARABB AND */
*   /* THE CALLER'S ORGINAL BASE RECORD.                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0482
*   RFY                                                            0482
*     CCARAB BASED(CUMRABB);        /* MAP RECORD AREA OF CALLER'S 0482
*                                      BASE RECORD                   */
*   CCACD2=RCS;                     /* CLEAR SECOND ERROR CODE       */
         MVI   CCACD2(R11),X'00'                                   0483
*   CCARABB=CUMRABB;                /* RESTORE CCARABB               */
         L     @10,CCACUMPL(,R11)                                  0484
         L     @05,CUMRABB(,@10)                                   0484
         ST    @05,CCARABB(,R11)                                   0484
*   CCAREUSE=OFF;                   /* INDICATE EXTRACT/MODIFY RECORD
*                                      AREAS ARE NOT TO BE REUSED    */
         NI    CCAREUSE(R11),B'11111011'                           0485
*   IF RCDCINO^=CUMBCI              /* IF THE CALLER'S BASE          */
*       &CUMNRG=OFF THEN            /* RECORD IS NOT IN-CORE AND THE
*                                      CALLER WISHES THE BASE RECORD
*                                      TO BE RE-GOTTEN               */
         L     @01,CCACPE1(,@05)                                   0486
         CLC   RCDCINO(3,@01),CUMBCI(@10)                          0486
         BE    @RF00486                                            0486
         TM    CUMNRG(@10),B'01000000'                             0486
         BNZ   @RF00486                                            0486
*     DO;                           /* READ BASE RECORD              */
*       CUMRC=CCAPROB;              /* SAVE MAJOR ERROR CODE         */
*                                                                  0488
         MVC   CUMRC(4,@10),CCAPROB(R11)                           0488
*       /*************************************************************/
*       /*                                                           */
*       /* CLEAR CCAMODID, CCAREASN, AND CCARETRN                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0489
*       CCAPROB=IGNOREIT;           /* CLEAR MAJOR ERROR CODE FIELD  */
         SLR   @15,@15                                             0489
         ST    @15,CCAPROB(,R11)                                   0489
*       CCAURAB=CUMRABB;            /* SET CCAURAB                   */
         ST    @05,CCAURAB(,R11)                                   0490
*       CCASRID=CUMBCI;             /* SET BASE RECORD CI NUMBER     */
         MVC   CCASRID(3,R11),CUMBCI(@10)                          0491
*       DO;                                                        0492
*         CCAF1KEY=OFF;             /* GET BY CI NO.                 */
*         CCAF1KGE=OFF;             /* NOT GET NEXT                  */
*         CCAF1UP=ON;               /* GET FOR UPD                   */
*                                                                  0495
         OI    CCAF1UP(R11),B'00000010'                            0495
         NI    CCAF1KEY(R11),B'11100111'                           0495
*         /***********************************************************/
*         /*                                                         */
*         /* CALL TO MODULE IGG0CLBI                                 */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0496
*Z065GET:                                                          0496
*         CALL IGGPGET;             /* GET RECORD                    */
Z065GET  L     @15,@CV00036                                        0496
         BALR  @14,@15                                             0496
*       END;                        /* GET BASE RECORD               */
*       CCACD2=CCACD1;              /* SAVE BASE RCD RE-GET ERROR    */
         MVC   CCACD2(1,R11),CCACD1(R11)                           0498
*       IF CUMRC=ZRC THEN                                          0499
         L     @10,CCACUMPL(,R11)                                  0499
         CLC   CUMRC(4,@10),@CB00044                               0499
         BE    @RT00499                                            0499
*         ;                         /* IF NO PREVIOUS MAJOR ERR      */
*       ELSE                        /* PREVIOUS MAJOR ERROR          */
*         CCAPROB=CUMRC;            /* RESTORE IT                    */
         L     @10,CCACUMPL(,R11)                                  0501
         MVC   CCAPROB(4,R11),CUMRC(@10)                           0501
*     END;                          /* END READ BASE RECORD          */
*   ELSE                                                           0503
*     ;                             /* BASE RECORD IN-CORE           */
@RF00486 DS    0H                                                  0504
*   CCAURAB=CUMURAB;                /* RESTORE CCAURAB               */
@RC00486 L     @10,CCACUMPL(,R11)                                  0504
         MVC   CCAURAB(4,R11),CUMURAB(@10)                         0504
*   CALL EXIT;                      /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0505
*   END;                            /* END OF INNER PROCEDURE        */
@EL00005 DS    0H                                                  0506
@EF00005 DS    0H                                                  0506
@ER00005 BR    @14                                                 0506
