         TITLE 'IGG0CLB0 - CMS DEFINE: DATA AND INDEX CHARACTERISTICS  *
                        '
IGG0CLB0 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGG0CLB0  78.167'                                 0001
         DROP  @15
@PROLOG  DS    0H                                                  0002
         USING @MAINENT,@15
         USING @DATD-(CCATEMPS-IGGCCA),R11
         DS 0F
         DC CL8'IGGPDCCO'
         EJECT
         B     @PB00002                                            0174
IGGPDCCO DS    0H                                                  0175
         ENTRY IGGPDCCO
         DROP @15
Z003     DS    0H                                                  0177
Z003DCCO DS    0H                                                  0178
         STM 12,14,12(13)
         LA 13,12(13)
         BALR 12,0
IGGSEGST DS 0H
         USING *,12
*                                      DEV CHARACTERISTICS MAPPING 0184
*                                      STRUCTURE                     */
*   AMDSBDAB=CTGFVDCH->CTGFVAMD->CTGFLPT(1);/* SET BASE OF DATA AMDSB*/
         L     @10,CCACPL(,R11)                                    0185
         L     @10,CTGFVT(,@10)                                    0185
         L     @10,CTGFVDCH(,@10)                                  0185
         L     @10,CTGFVAMD(,@10)                                  0185
         L     AMDSBDAB,CTGFLPT(,@10)                              0185
*   RFY                                                            0186
*     IDAAMDSB BASED(AMDSBDAB);     /* ESTABLISH ADDR OF AMDSB     0186
*                                      MAPPING STRUCTURE FOR DATA  0186
*                                      AMDSB                         */
*   CMPROCM=ON;                     /* ALLOW PROCESSING TO BEGIN TO
*                                      SETTLE ON SIZE OF CA AND INDEX
*                                      CI SIZE (IF INDEXED)          */
         L     @10,CCACMSWA(,R11)                                  0187
         OI    CMPROCM(@10),B'01000000'                            0187
*   DO WHILE CMPROCM=ON;                                           0188
         B     @DE00188                                            0188
@DL00188 DS    0H                                                  0189
*     AMDSBDAB=CTGFVDCH->CTGFVAMD->CTGFLPT(1);/* SET BASE ADDRESS OF
*                                      DATA AMDSB                    */
         L     @10,CCACPL(,R11)                                    0189
         L     @10,CTGFVT(,@10)                                    0189
         L     @10,CTGFVDCH(,@10)                                  0189
         L     @10,CTGFVAMD(,@10)                                  0189
         L     AMDSBDAB,CTGFLPT(,@10)                              0189
*     DCWABASE=ADDR(CMSDSPTB(1));   /* SET ADDRESS OF DATA ELEMENT OF
*                                      DEVICE CHARACTERICTICS ARRAY  */
         L     @10,CCACMSWA(,R11)                                  0190
         LA    DCWABASE,CMSDSPTB(,@10)                             0190
*     CMDCONV=AMDCINV;                                             0191
*                                                                  0191
         MVC   CMDCONV(4,@10),AMDCINV(AMDSBDAB)                    0191
*     /***************************************************************/
*     /*                                                             */
*     /* CALL SEGMENT TO DETERMINE PHYSICAL BLOCK SIZE AND NUMBER OF */
*     /* BLOCKS PER TRACK INDEX VALUE FOR DATA CISIZE                */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0192
*Z004DPBI:                                                         0192
*     CALL IGGPDPBI;                                               0192
Z004DPBI BAL   @14,IGGPDPBI                                        0192
*     IF CTGTRKOV=ON THEN           /* TRACK OVERFLOW ?      @ZA11077*/
         L     @10,CCAFLPT(,R11)                                   0193
         L     @10,CTGFLPT(,@10)                                   0193
         TM    CTGTRKOV(@10),B'00000001'                           0193
         BNO   @RF00193                                            0193
*       CMPBI=FOUR;                 /* SET INDX TO MAX       @ZA11077*/
         L     @10,CCACMSWA(,R11)                                  0194
         MVC   CMPBI(4,@10),@CF00062                               0194
*     ELSE                                                         0195
*       ;                           /* NOT TRACK OVERFLOW@ZA11077    */
@RF00193 DS    0H                                                  0196
*     CMPHYBLK(1)=PHYBLKS(CMPBI);   /* SET PHYSICAL BLOCK SIZE FOR 0196
*                                      DATA                          */
         L     @10,CCACMSWA(,R11)                                  0196
         L     @05,CMPBI(,@10)                                     0196
         ALR   @05,@05                                             0196
         LH    @15,PHYBLKS-2(@05)                                  0196
         ST    @15,CMPHYBLK(,@10)                                  0196
*     CMBLKTRK(1)=DSPHYBPT(CMPBI);  /* SET NUMBER OF PHYSICAL BLOCKS
*                                      PER TRACK FOR DATA            */
         LH    @15,DSPHYBPT-2(@05,DCWABASE)                        0197
         STH   @15,CMBLKTRK(,@10)                                  0197
*     CMTRKCYL(1)=RTNOTRKS;         /* SET NUMBER OF TRACKS PER    0198
*                                      CYLINDER FOR DATA DEVICE      */
         MVC   CMTRKCYL(2,@10),RTNOTRKS(DCWABASE)                  0198
*     CMSWADTY(1)=RTDEVUCB;         /* SET DEVICE TYPE UCB CODE FOR
*                                      DATA DEVICE                   */
         MVC   CMSWADTY(4,@10),RTDEVUCB(DCWABASE)                  0199
*     RFY                                                          0200
*      (BA,                                                        0200
*       BM,                                                        0200
*       I) RSTD;                    /*                         Y02020*/
*     CCAURAB=ADDR(CCARAB1);        /* POINT TO TRKREM       @ZA10251*/
         ST    @05,@TF00001                                        0201
         LA    @15,CCARAB1(,R11)                                   0201
         ST    @15,CCAURAB(,R11)                                   0201
*     IF CTGTRKOV=ON THEN           /* TRACK OVERFLOW          Y02020*/
         L     @01,CCAFLPT(,R11)                                   0202
         L     @01,CTGFLPT(,@01)                                   0202
         TM    CTGTRKOV(@01),B'00000001'                           0202
         BNO   @RF00202                                            0202
*       DO;                         /*                         Y02020*/
*                                                                  0203
*         /***********************************************************/
*         /*                                                         */
*         /* INITIALIZE FIELDS AS THOUGH NO                    Y02020*/
*         /* EXTRA BLOCKS WILL FIT ON A CYL                    Y02020*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0204
*         CMTRKPCA(1)=CMTRKCYL(1);  /* TRKS PER CONTROL AREA   Y02020*/
         LH    @14,CMTRKCYL(,@10)                                  0204
         STH   @14,CMTRKPCA(,@10)                                  0204
*         CMBYTTRK=CMPHYBLK(1)*CMBLKTRK(1);/* BYTES PER TRK    Y02020*/
         L     @09,CMPHYBLK(,@10)                                  0205
         LR    @08,@09                                             0205
         MH    @08,CMBLKTRK(,@10)                                  0205
         ST    @08,CMBYTTRK(,@10)                                  0205
*         CMBYTAU=CMBYTTRK*CMTRKPCA(1);/* BYTES PER ALLOC UNIT Y02020*/
         LR    @01,@08                                             0206
         MR    @00,@14                                             0206
         ST    @01,CMBYTAU(,@10)                                   0206
*         BM=(CMTRKCYL(1)*TRKREM(CMPBI))/CMPHYBLK(1);/* BLKS PER CYL
*                                                              Y02020*/
         L     @01,CCACPE1(,@15)                                   0207
         LH    BM,CMTRKCYL(,@10)                                   0207
         L     @10,@TF00001                                        0207
         BCTR  @01,0                                               0207
         BCTR  @01,0                                               0207
         MH    BM,TRKREM(@10,@01)                                  0207
         LR    @00,BM                                              0207
         SRDA  @00,32                                              0207
         DR    @00,@09                                             0207
         LR    BM,@01                                              0207
*         IF BM^=N THEN             /* AT LEAST 1 EXTRA BLK WILL FIT
*                                                              Y02020*/
         LTR   BM,BM                                               0208
         BZ    @RF00208                                            0208
*           DO;                     /*                         Y02020*/
*                                                                  0209
*             /*******************************************************/
*             /*                                                     */
*             /* BM IS MAX NBR BLOCKS THAT WILL FIT ON         Y02020*/
*             /* A CYL. BA IS THE ACTUAL NBR TO PUT.           Y02020*/
*             /* BA MUST DIVIDE EVENLY INTO THE NBR OF         Y02020*/
*             /* TRACKS PER CYL. BA WILL ALSO BE THE NBR       Y02020*/
*             /* OF CONTROL AREAS PER CYL.                     Y02020*/
*             /*                                                     */
*             /*******************************************************/
*                                                                  0210
*             BA=N;                 /*                         Y02020*/
         SLR   BA,BA                                               0210
*             DO I=BM TO 1 BY-1 WHILE BA=N;/*                  Y02020*/
         LR    I,BM                                                0211
         B     @DE00211                                            0211
@DL00211 SLR   @10,@10                                             0211
         CR    BA,@10                                              0211
         BNE   @DC00211                                            0211
*               IF CMTRKCYL(1)//I=N THEN/*                     Y02020*/
         L     @01,CCACMSWA(,R11)                                  0212
         LH    @00,CMTRKCYL(,@01)                                  0212
         SRDA  @00,32                                              0212
         DR    @00,I                                               0212
         CR    @00,@10                                             0212
         BNE   @RF00212                                            0212
*                 BA=I;             /*                         Y02020*/
         LR    BA,I                                                0213
*             END;                  /*                         Y02020*/
@RF00212 BCTR  I,0                                                 0214
@DE00211 LTR   I,I                                                 0214
         BP    @DL00211                                            0214
@DC00211 DS    0H                                                  0215
*             CMTRKPCA(1)=CMTRKCYL(1)/BA;/*                    Y02020*/
         L     @10,CCACMSWA(,R11)                                  0215
         LH    @00,CMTRKCYL(,@10)                                  0215
         SRDA  @00,32                                              0215
         DR    @00,BA                                              0215
         STH   @01,CMTRKPCA(,@10)                                  0215
*             CMBYTAU=CMBYTTRK*CMTRKPCA(1)+CMPHYBLK(1);/*      Y02020*/
         L     @15,CMBYTTRK(,@10)                                  0216
         LR    @09,@15                                             0216
         MR    @08,@01                                             0216
         L     @08,CMPHYBLK(,@10)                                  0216
         ALR   @09,@08                                             0216
         ST    @09,CMBYTAU(,@10)                                   0216
*             CMBYTTRK=CMBYTTRK+(CMPHYBLK(1)+CMTRKPCA(1)-ONE)/CMTRKPCA(
*                 1);               /*                         Y02020*/
         ALR   @08,@01                                             0217
         BCTR  @08,0                                               0217
         ST    @01,@TF00001                                        0217
         LR    @01,@08                                             0217
         SLR   @00,@00                                             0217
         D     @00,@TF00001                                        0217
         ALR   @15,@01                                             0217
         ST    @15,CMBYTTRK(,@10)                                  0217
*           END;                    /*                         Y02020*/
*         ELSE                                                     0219
*           CTGTRKOV=OFF;           /* SET OFF WHEN NO EXTRA BLKS  0219
*                                      EXIST                 @YM05952*/
         B     @RC00208                                            0219
@RF00208 L     @10,CCAFLPT(,R11)                                   0219
         L     @10,CTGFLPT(,@10)                                   0219
         NI    CTGTRKOV(@10),B'11111110'                           0219
*         AMDCIPCA=CMBYTAU/AMDCINV; /* NBR OF CI PER CA        Y02020*/
@RC00208 L     @10,CCACMSWA(,R11)                                  0220
         L     @00,CMBYTAU(,@10)                                   0220
         SRDA  @00,32                                              0220
         D     @00,AMDCINV(,AMDSBDAB)                              0220
         STH   @01,AMDCIPCA(,AMDSBDAB)                             0220
*       END;                        /*                         Y02020*/
*     ELSE                          /* TRACK OVERFLOW NOT SPECIFIED
*                                                              Y02020*/
*       DO;                         /*                         Y02020*/
         B     @RC00202                                            0222
@RF00202 DS    0H                                                  0223
*         RFY                                                      0223
*          (BA,                                                    0223
*           BM,                                                    0223
*           I) UNRSTD;              /*                         Y02020*/
*         IF CTGSWAP=ON THEN        /* IF SWAPSPACE THEN     @Z40WSSG*/
         L     @10,CCACPL(,R11)                                    0224
         TM    CTGSWAP(@10),B'00100000'                            0224
         BNO   @RF00224                                            0224
*           DO;                     /* YES                   @Z40WSSG*/
*             CMBYTTRK=CMPHYBLK(1)*CMBLKTRK(1);/* BYTES PER TRK    0226
*                                                            @Z40WSSG*/
         L     @10,CCACMSWA(,R11)                                  0226
         L     @05,CMPHYBLK(,@10)                                  0226
         MH    @05,CMBLKTRK(,@10)                                  0226
         ST    @05,CMBYTTRK(,@10)                                  0226
*             CMTRKPCA(1)=CMTRKCYL(1);/* SET TRKS PER CA     @Z40WSSG*/
         MVC   CMTRKPCA(2,@10),CMTRKCYL(@10)                       0227
*           END;                    /*                       @Z40WSSG*/
*         ELSE                                                     0229
*           ;                       /* NOT SWAPSPACE         @Z40WSSG*/
*                                                                  0229
@RF00224 DS    0H                                                  0230
*         /***********************************************************/
*         /*                                                         */
*         /* SET UP SPCQTY TO DETERMINE NUMBER OF CI PER CA          */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0230
*         IF CTGFVICH^=N THEN       /* INDEXED CLUSTER               */
         L     @10,CCACPL(,R11)                                    0230
         L     @10,CTGFVT(,@10)                                    0230
         ICM   @05,15,CTGFVICH(@10)                                0230
         BZ    @RF00230                                            0230
*           IF AMDSDT=ON THEN       /* SEQUENCE SET WITH DATA        */
         TM    AMDSDT(AMDSBDAB),B'00100000'                        0231
         BNO   @RF00231                                            0231
*             SPCQTY=CMTRKPCA(1)-1;                                0232
         L     @10,CCACMSWA(,R11)                                  0232
         LH    SPCQTY,CMTRKPCA(,@10)                               0232
         BCTR  SPCQTY,0                                            0232
*           ELSE                                                   0233
*             SPCQTY=CMTRKPCA(1);                                  0233
         B     @RC00231                                            0233
@RF00231 L     @10,CCACMSWA(,R11)                                  0233
         LH    SPCQTY,CMTRKPCA(,@10)                               0233
*         ELSE                      /* NOT INDEXED CLUSTER           */
*           SPCQTY=CMTRKPCA(1);                                    0234
         B     @RC00230                                            0234
@RF00230 L     @10,CCACMSWA(,R11)                                  0234
         LH    SPCQTY,CMTRKPCA(,@10)                               0234
*         AMDCIPCA=(SPCQTY*CMBLKTRK(1))/(AMDCINV/CMPHYBLK(1));/* SET
*                                      NUMBER OF CI PER CA FOR DATA  */
@RC00230 L     @10,CCACMSWA(,R11)                                  0235
         LR    @05,SPCQTY                                          0235
         MH    @05,CMBLKTRK(,@10)                                  0235
         L     @00,AMDCINV(,AMDSBDAB)                              0235
         SRDA  @00,32                                              0235
         D     @00,CMPHYBLK(,@10)                                  0235
         LR    @06,@05                                             0235
         SRDA  @06,32                                              0235
         DR    @06,@01                                             0235
         STH   @07,AMDCIPCA(,AMDSBDAB)                             0235
*       END;                        /*                         Y02020*/
*     IF AMDCIPCA<=ONE THEN         /* ONLY ONE CI PER CA            */
@RC00202 LH    @10,AMDCIPCA(,AMDSBDAB)                             0237
         CH    @10,@CH00041                                        0237
         BH    @RF00237                                            0237
*       DO;                         /*                               */
*                                                                  0238
*         /***********************************************************/
*         /*                                                         */
*         /* DECREMENT DATA CI SIZE AND TRY ONCE MORE                */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0239
*         IF AMDSPAN=ON THEN        /* SPANNED RECORDS               */
         TM    AMDSPAN(AMDSBDAB),B'00000001'                       0239
         BNO   @RF00239                                            0239
*           DO;                     /* YES                           */
*             DCONV=CMPHYBLK(1)*(CMBLKTRK(1)/TWO);/* SET CI TO 1/2 0241
*                                      TRK                           */
         L     @10,CCACMSWA(,R11)                                  0241
         LH    @00,CMBLKTRK(,@10)                                  0241
         SRDA  @00,32                                              0241
         D     @00,@CF00059                                        0241
         M     @00,CMPHYBLK(,@10)                                  0241
         LR    DCONV,@01                                           0241
*           END;                    /*                               */
*         ELSE                      /* NONSPANNED                    */
*           DO;                     /* YES                           */
         B     @RC00239                                            0243
@RF00239 DS    0H                                                  0244
*             DCONV=AMDLRECL+DRECOVHD;/* SET DATA CI TO MAX RECORD 0244
*                                      SIZE                          */
         LA    DCONV,7                                             0244
         AL    DCONV,AMDLRECL(,AMDSBDAB)                           0244
*           END;                    /*                               */
*Z005DCIM:                                                         0246
*         CALL IGGPDCIM;            /* CONVERT TO VALID CI           */
@RC00239 DS    0H                                                  0246
Z005DCIM L     @15,@CV00037                                        0246
         BALR  @14,@15                                             0246
*         AMDCINV=DCONV;            /* SET CI SIZE                   */
         ST    DCONV,AMDCINV(,AMDSBDAB)                            0247
*         DCWABASE=ADDR(CMSDSPTB(1));/* SET ADDR OF DATA ELEMENT OF
*                                      DEVICE CHARACTERICTICS ARRAY  */
         L     @10,CCACMSWA(,R11)                                  0248
         LA    DCWABASE,CMSDSPTB(,@10)                             0248
*         CMDCONV=AMDCINV;          /* SET INPUT PARM                */
*                                                                  0249
         L     @05,AMDCINV(,AMDSBDAB)                              0249
         ST    @05,CMDCONV(,@10)                                   0249
*         /***********************************************************/
*         /*                                                         */
*         /* CALL SEGMENT TO DETERMINE NEW PHYSICAL BLK SIZE AND     */
*         /* NUMBER OF BLOCKS PER TRACK INDEX VALUE FOR DATA CISIZE  */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0250
*Z006DPBI:                                                         0250
*         CALL IGGPDPBI;            /* GET INDEX                     */
Z006DPBI BAL   @14,IGGPDPBI                                        0250
*         CMPHYBLK(1)=PHYBLKS(CMPBI);/* SET BLK SIZE                 */
         L     @10,CCACMSWA(,R11)                                  0251
         L     @05,CMPBI(,@10)                                     0251
         ALR   @05,@05                                             0251
         LH    @15,PHYBLKS-2(@05)                                  0251
         ST    @15,CMPHYBLK(,@10)                                  0251
*         CMBLKTRK(1)=DSPHYBPT(CMPBI);/* BLKS PER TRK                */
         LH    @05,DSPHYBPT-2(@05,DCWABASE)                        0252
         STH   @05,CMBLKTRK(,@10)                                  0252
*         AMDCIPCA=(SPCQTY*CMBLKTRK(1))/(AMDCINV/CMPHYBLK(1));/* SET
*                                      NUMBER OF CI PER CA FOR DATA  */
         LR    @01,@05                                             0253
         MR    @00,SPCQTY                                          0253
         L     @06,AMDCINV(,AMDSBDAB)                              0253
         SRDA  @06,32                                              0253
         D     @06,CMPHYBLK(,@10)                                  0253
         LR    @08,@01                                             0253
         SRDA  @08,32                                              0253
         DR    @08,@07                                             0253
         STH   @09,AMDCIPCA(,AMDSBDAB)                             0253
*         IF AMDCIPCA<=ONE THEN     /* ONLY ONE CI PER CA            */
         CH    @09,@CH00041                                        0254
         BH    @RF00254                                            0254
*           DO;                     /* YES                           */
*                                                                  0255
*             /*******************************************************/
*             /*                                                     */
*             /* SET CCAMODID='B0', CCAREASN=RSN4, CCARETRN=RCSIZCAL */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0256
*             CCAPROB=IGGSEGID+(RSN4*K256)+RCSIZCAL;/* SIZE CALC   0256
*                                      UNSOLVABLE WITH SPECIFIED   0256
*                                      PARMS, RECD MGT CANNOT PROCESS
*                                      CA SPLIT FOR THIS CASE.       */
         MVC   CCAPROB(4,R11),@CF03507                             0256
*             CALL EXIT;            /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0257
*           END;                    /*                               */
*         ELSE                                                     0259
*           ;                       /* NEW CISIZE OKAY               */
@RF00254 DS    0H                                                  0260
*       END;                        /*                               */
*     ELSE                                                         0261
*       ;                           /*                               */
*                                                                  0261
@RF00237 DS    0H                                                  0262
*     /***************************************************************/
*     /*                                                             */
*     /* IF RRDS THEN SET SLOTS PER CI AND MAX RECORD NO.            */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0262
*     IF AMDRRDS=ON THEN            /* RRDS                          */
         TM    AMDRRDS(AMDSBDAB),B'00000010'                       0262
         BNO   @RF00262                                            0262
*       DO;                         /* YES                           */
*                                                                  0263
*         /***********************************************************/
*         /*                                                         */
*         /* THE HIGHEST RELATIVE RECORD NUMBER IS EQUAL TO THE      */
*         /* ENTIER OF 2 TO THE 32ND TIMES NSLOT DIVIDED BY THE      */
*         /* CONTROL INTERVAL SIZE                                   */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0264
*         AMDNSLOT=(AMDCINV-FOUR)/(AMDLRECL+THREE);/* SET NO. SLOTS
*                                      PER CI                        */
         L     @10,AMDCINV(,AMDSBDAB)                              0264
         LR    @05,@10                                             0264
         SL    @05,@CF00062                                        0264
         LA    @15,3                                               0264
         AL    @15,AMDLRECL(,AMDSBDAB)                             0264
         LR    @00,@05                                             0264
         SRDA  @00,32                                              0264
         DR    @00,@15                                             0264
         ST    @01,AMDNSLOT(,AMDSBDAB)                             0264
*         AMDMAXRR=MAXREC/(AMDCINV/TWO56);/* SET MAX                 */
         LR    @06,@10                                             0265
         SRDA  @06,32                                              0265
         D     @06,@CF00064                                        0265
         L     @08,@CF03398                                        0265
         SRDA  @08,32                                              0265
         DR    @08,@07                                             0265
         ST    @09,AMDMAXRR(,AMDSBDAB)                             0265
*         AMDMAXRR=AMDMAXRR*AMDNSLOT;/* SET MAX RECORD NUMBER        */
         MR    @08,@01                                             0266
         ST    @09,AMDMAXRR(,AMDSBDAB)                             0266
*       END;                        /*                               */
*     ELSE                                                         0268
*       ;                           /* NOT RRDS                      */
@RF00262 DS    0H                                                  0269
*     IF AMDSPAN=ON THEN            /* SPANNED RECORDS               */
         TM    AMDSPAN(AMDSBDAB),B'00000001'                       0269
         BNO   @RF00269                                            0269
*       DO;                         /* YES                           */
*         IF AMDLRECL>AMDCIPCA*(AMDCINV-SRECOVHD) THEN/* LRECL EXCEED
*                                      CA SIZE                       */
*                                                                  0271
         L     @10,AMDCINV(,AMDSBDAB)                              0271
         SH    @10,@CH00260                                        0271
         MH    @10,AMDCIPCA(,AMDSBDAB)                             0271
         C     @10,AMDLRECL(,AMDSBDAB)                             0271
         BNL   @RF00271                                            0271
*           /*********************************************************/
*           /*                                                       */
*           /* SET CCAMODID='B0', CCAREASN=RSN4, CCARETRN=RCSPANCK   */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0272
*           CCAPROB=IGGSEGID+(RSN4*K256)+RCSPANCK;/* LRECL GT CA     */
         MVC   CCAPROB(4,R11),@CF03508                             0272
*         ELSE                                                     0273
*           ;                       /*                               */
@RF00271 DS    0H                                                  0274
*       END;                        /*                               */
*     ELSE                                                         0275
*       ;                           /* NONSPANNED                    */
@RF00269 DS    0H                                                  0276
*     IF CTGFVICH^=N THEN           /* INDEXED CLUSTER               */
         L     @10,CCACPL(,R11)                                    0276
         L     @10,CTGFVT(,@10)                                    0276
         ICM   @05,15,CTGFVICH(@10)                                0276
         BZ    @RF00276                                            0276
*       DO;                                                        0277
*                                                                  0277
*         /***********************************************************/
*         /*                                                         */
*         /* CALL CALCULATE INDEX OBJECT CHARACTERISTICS             */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0278
*Z009DCCI:                                                         0278
*         CALL IGGPDCCI;                                           0278
