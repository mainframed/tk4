*/********************************************************************/
*                                                                  0221
*   DCRACIPT(1)=ADDR(DCRARACI);     /* SET POINTER TO CI 0           */
         LA    @10,DCRARACI(,DCRAWAPR)                             0221
         ST    @10,DCRACIPT(,DCRAWAPR)                             0221
*   DO DCRARCTR=2 TO 6;             /* SET ADDR ARRAY SUBSCRIPT RANGE*/
         LA    DCRARCTR,2                                          0222
@DL00222 DS    0H                                                  0223
*     DCRACIPT(DCRARCTR)=           /* SET POINTER TO RECORD         */
*         DCRACIPT(DCRARCTR-INCR1)+DCRACISZ;                       0223
         LR    @10,DCRARCTR                                        0223
         SLA   @10,2                                               0223
         LA    @05,512                                             0223
         LR    @01,DCRAWAPR                                        0223
         AL    @01,@CF04659                                        0223
         AL    @05,DCRACIPT(@10,@01)                               0223
         AL    @10,@CF04660                                        0223
         ST    @05,DCRACIPT(@10,DCRAWAPR)                          0223
*   END;                                                           0224
         AH    DCRARCTR,@CH00046                                   0224
         CH    DCRARCTR,@CH00087                                   0224
         BNH   @DL00222                                            0224
*Z008CHIU:                                                         0225
