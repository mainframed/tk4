Z009CCI0 BAL   @14,IGGPCCI0                                        0226
*Z010CI15:                                                         0227
*   CALL IGGPCI15;                  /* CONSTRUCT CRA CI'S 1 TO 5     */
*                                                                  0227
Z010CI15 BAL   @14,IGGPCI15                                        0227
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL TO MODULE IGG0CLDA                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0228
*Z011MODI:                                                         0228
*   CALL IGGPMODI;                  /* INTERFACE TO MODIFY ROUTINE   */
Z011MODI L     @15,@CV04455                                        0228
         BALR  @14,@15                                             0228
*   IF CCACD1^=RCS THEN             /* CHECK IF MODIFY SUCCESSFUL    */
         CLI   CCACD1(R11),0                                       0229
         BE    @RF00229                                            0229
*Z012DCXT:                                                         0230
*     CALL IGGPDCXT;                /* IF NOT, CLEAN UP AND EXIT     */
Z012DCXT BAL   @14,IGGPDCXT                                        0230
*   ELSE                                                           0231
*     ;                             /* MODIFY PROCESSING SUCCESSFUL  */
@RF00229 DS    0H                                                  0232
*/********************************************************************/
*/*                CRA  'OPEN'-RELATED  PROCESSING                   */
*/********************************************************************/
*                                                                  0232
*Z013CACB:                                                         0232
*   CALL IGGPCACB;                  /* CONSTRUCT CRA ACB IN RA 1     */
Z013CACB BAL   @14,IGGPCACB                                        0232
*Z014CXWA:                                                         0233
*   CALL IGGPCXWA;                  /* CONSTRUCT CRA CAXWA IN RA 1   */
Z014CXWA BAL   @14,IGGPCXWA                                        0233
*Z015OCRA:                                                         0234
*   CALL IGGPOCRA;                  /* OPEN THE CRA ESDS             */
Z015OCRA BAL   @14,IGGPOCRA                                        0234
*   IF CCACD1^=RCS THEN             /* IF CRA OPEN FAILED,           */
         CLI   CCACD1(R11),0                                       0235
         BE    @RF00235                                            0235
*Z016DCXT:                                                         0236
*     CALL IGGPDCXT;                /* CLEAN UP AND EXIT             */
Z016DCXT BAL   @14,IGGPDCXT                                        0236
*   ELSE                                                           0237
*     ;                             /* CRA SUCCESSFULLY OPENED       */
@RF00235 DS    0H                                                  0238
*/********************************************************************/
*/*      WRITE THE CRA SELF-DESCRIBING RECORDS AND THE VOLUME        */
*/*      RECORDS TO THE CRA AND THE CATALOG.                         */
*/********************************************************************/
*                                                                  0238
*Z017CRPL:                                                         0238
*   CALL IGGPCRPL;                  /* CONSTRUCT CRA RPL IN RA 1     */
*                                                                  0238
Z017CRPL BAL   @14,IGGPCRPL                                        0238
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL TO MODULE IGG0CLDA                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0239
*Z018WCRA:                                                         0239
*   CALL IGGPWCRA;                  /* WRITE RECORDS TO THE CRA      */
Z018WCRA L     @15,@CV04458                                        0239
         BALR  @14,@15                                             0239
*   IF CCACD1^=RCS THEN             /* CHECK IF I/O SUCCESSFUL       */
         CLI   CCACD1(R11),0                                       0240
         BE    @RF00240                                            0240
*Z019DCXT:                                                         0241
*     CALL IGGPDCXT;                /* IF NOT, CLEAN UP AND EXIT     */
Z019DCXT BAL   @14,IGGPDCXT                                        0241
*   ELSE                                                           0242
*     ;                             /* CRA RECORDS WRITTEN OKAY      */
@RF00240 DS    0H                                                  0243
*   IF CCAF2CB=OFF THEN             /* CALLER IS DEFINE SPACE        */
*                                                                  0243
         TM    CCAF2CB(R11),B'00000010'                            0243
         BNZ   @RF00243                                            0243
*     /***************************************************************/
*     /*                                                             */
*     /* CALL TO MODULE IGG0CLDA                                     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0244
*Z020WCAT:                                                         0244
*     CALL IGGPWCAT;                /* WRITE VOLUME RECORDS TO CATLG */
Z020WCAT L     @15,@CV04457                                        0244
         BALR  @14,@15                                             0244
*   ELSE                                                           0245
*     ;                             /* CALLER IS DEFINE CATALOG      */
@RF00243 DS    0H                                                  0246
*Z021DCXT:                                                         0246
*   CALL IGGPDCXT;                  /* DO CLEANUP PROCESSING FOR EXIT*/
Z021DCXT BAL   @14,IGGPDCXT                                        0246
*   CALL EXIT;                      /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0247
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */
         DS 0F
*   GEN(DC CL8'IGGPSTRG');          /* NAME OF FOLLOWING PROCEDURE   */
         DC CL8'IGGPSTRG'
*   END;                            /* END OF INNER PROCEDURE        */
@EL00002 DS    0H                                                  0250
@EF00002 DS    0H                                                  0250
@ER00002 BR    @14                                                 0250
@PB00002 DS    0H                                                  0251
