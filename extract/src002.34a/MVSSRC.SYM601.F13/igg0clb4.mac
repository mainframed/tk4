IGGPSTRG DS    0H                                                  0253
*Z022:                              /* LABEL FOR PROCEDURE TRACKING  */
*Z022STRG:                          /* LABEL FOR PROCEDURE TRACKING  */
*   RFY                                                            0253
*    (R5,                                                          0253
*     R6,                                                          0253
*     R7,                                                          0253
*     R8,                                                          0253
*     R9,                                                          0253
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */
Z022     DS    0H                                                  0253
Z022STRG DS    0H                                                  0254
*   RFY                                                            0254
*    (R2,                                                          0254
*     R3,                                                          0254
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */
         STM 12,14,12(13)
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */
         LA 13,12(13)
*/*********************************************************************
**                                                                    *
**   PROCEDURE NAME = IGGPSTRG                                        *
**                                                                    *
**   PROCEDURE TYPE = INTERNALLY CALLED, NESTED                       *
**                                                                    *
**   DESCRIPTIVE NAME = OBTAIN MAIN STORAGE AND VOLUME SPACE          *
**                                                                    *
**   FUNCTION =                                                       *
**      OBTAIN MAIN STORAGE WORK SPACE FOR THE DEFINE CRA PROCESS     *
**      AND ALSO TO ALLOCATE SPACE ON THE VOLUME FOR THE CATALOG      *
**      RECOVERY AREA ITSELF:                                         *
**      1.  OBTAIN SPACE FOR RECORD CONSTRUCTION AND CONTROL BLOCKS   *
**      2.  CLEAR NEWLY OBTAINED MAIN STORAGE WORK AREA               *
**      3.  OBTAIN ONE CYLINDER OF SPACE ON THE VOLUME FOR THE CRA    *
**                                                                    *
**   INPUT =                                                          *
**      ADDRESS OF CCA IN REGISTER 11                                 *
**                                                                    *
**   OUTPUT =                                                         *
**      IF ERROR ENCOUNTERED, 'CCAPROB' CONTAINS PROBLEM              *
**         DETERMINATION INFORMATION.                                 *
**      ADDRESS OF DEFINE CRA WORKAREA IN DCRAWAPR (REGISTER 3)       *
**      ADDRESS OF DEFINE CRA WORKAREA IN CCACRACI                    *
**                                                                    *
**********************************************************************/
*/********************************************************************/
*/*     OBTAIN SPACE FOR RECORD CONSTRUCTION AND CONTROL BLOCKS      */
*/********************************************************************/
*                                                                  0257
*   CCAMNLEN=LENGTH(DCRAWKA);       /* SET REQUEST CORE SIZE         */
         LA    @10,3472                                            0257
         STCM  @10,7,CCAMNLEN(R11)                                 0257
*   IF CCAF2CB=ON THEN              /* CALLER IS DEFINE CATALOG      */
         TM    CCAF2CB(R11),B'00000010'                            0258
         BNO   @RF00258                                            0258
*     CCAMNLEN=CCAMNLEN             /* GETMAIN RAB 3 SAVE AREA ALSO  */
*         +LENGTH(DCRARABS);                                       0259
         AH    @10,@CH04638                                        0259
         STCM  @10,7,CCAMNLEN(R11)                                 0259
*   ELSE                                                           0260
*     ;                             /* CALLER IS DEFINE SPACE        */
@RF00258 DS    0H                                                  0261
*   RFY                                                            0261
*     R1 RSTD;                      /* RESTRICT R1 FOR GETMAIN MACRO */
*   R1=ADDR(CCAMSSPL);              /* POINT TO STRG MGMT P.L.       */
         LA    R1,CCAMSSPL(,R11)                                   0262
*   CCAMNSPL=USUBPOOL;              /* STORE SUBPOOL VALUE           */
*                                                                  0263
         MVI   CCAMNSPL(R11),X'FC'                                 0263
*   /*****************************************************************/
*   /*                                                               */
*   /* ALLOCATE MAIN STORAGE FROM USER SUBPOOL 252                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0264
*   GEN(GETMAIN LC,MF=(E,(1)));     /* GET MAIN STORAGE SPACE        */
         GETMAIN LC,MF=(E,(1))
*   GEN(LR R1,R15);                 /* PICK UP RETURN CODE           */
         LR R1,R15
*   IF R1^=NOGETMSE THEN            /* CHECK FOR ERROR RETURN        */
*                                                                  0266
         LTR   R1,R1                                               0266
         BZ    @RF00266                                            0266
*     /***************************************************************/
*     /*                                                             */
*     /* SET CCAMODID='B4', CCAREASN=RSN2, CCARETRN=RCINSSWA         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0267
*     CCAPROB=IGGSEGID+(RSN2*K256)+RCINSSWA;/* INSUFFICIENT CORE   0267
*                                      ERROR                         */
         MVC   CCAPROB(4,R11),@CF04640                             0267
*   ELSE                            /* GETMAIN SUCCESSFUL            */
*     DO;                                                          0268
         B     @RC00266                                            0268
@RF00266 DS    0H                                                  0269
*       DO;                                                        0269
*         IF MNATFULL=ON THEN       /* TRACKING TABLE FULL           */
*                                                                  0270
         TM    MNATFULL(R11),B'10000000'                           0270
         BNO   @RF00270                                            0270
*           /*********************************************************/
*           /*                                                       */
*           /* CALL TO MODULE IGG0CLAT                               */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0271
*Z026ARFS:                                                         0271
*           CALL IGGPARFS;          /* YES,CALL SPILL ROUTINE        */
Z026ARFS L     @15,@CV04451                                        0271
         BALR  @14,@15                                             0271
*         GEN REFS(CCAMNAT)(MVC CCAMNAT(248,11),CCAMNAT+8(11));/*  0272
*                                      PUSH AND TRACK                */
@RF00270 DS    0H                                                  0272
         MVC CCAMNAT(248,11),CCAMNAT+8(11)
*       END;                                                       0273
*     END;                          /* GET MAIN STORAGE              */
*   RFY                                                            0275
*     R1 UNRSTD;                    /* UNRESTRICT R1'S USE           */
@RC00266 DS    0H                                                  0276
*   IF CCACD1^=RCS THEN             /* GET CORE SUCCESSFUL?          */
         CLI   CCACD1(R11),0                                       0276
         BE    @RF00276                                            0276
*     DO;                           /* IF NOT SUCCESSFUL,            */
*       DCRAWAPR=BLANK;             /* CLEAR WORK AREA ADDRESS       */
         SLR   DCRAWAPR,DCRAWAPR                                   0278
*       CALL EXIT;                  /* RETURN TO CALLER: IGGPDCRA    */
         BAL   @14,EXIT                                            0279
*     END;                                                         0280
*   ELSE                                                           0281
*     ;                             /* GET CORE SUCCESSFUL           */
@RF00276 DS    0H                                                  0282
*/********************************************************************/
*/*         CLEAR  NEWLY  OBTAINED  MAIN  STORAGE  AREA              */
*/********************************************************************/
*                                                                  0282
*   R2=CCAMNADR;                    /* GET CORE ADDRESS              */
         L     R2,CCAMNADR(,R11)                                   0282
*   R3=CCAMNLEN;                    /* GET CORE LENGTH               */
         SLR   R3,R3                                               0283
         ICM   R3,7,CCAMNLEN(R11)                                  0283
*   RFY                                                            0284
*     R5 RSTD;                      /* RESTRICT R5 FOR MVCL INSTRUC. */
*   R5=BLANK;                       /* CLEAR R5 FOR MVCL INSTRUCTION */
         SLR   R5,R5                                               0285
*   GEN(MVCL  R2,R4);               /* CLEAR MAIN STORAGE WORKAREA   */
         MVCL  R2,R4
*   RFY                                                            0287
*     R5 UNRSTD;                    /* UNRESTRICT USE OF REGISTER 5  */
*   DCRAWAPR=CCAMNADR;              /* SET CRA WORKAREA BASE REGISTER*/
         L     DCRAWAPR,CCAMNADR(,R11)                             0288
*   CCACRACI=DCRAWAPR;              /* SET PTR TO CRA CI ADDR ARRAY  */
         ST    DCRAWAPR,CCACRACI(,R11)                             0289
*   DCRAMNLN=CCAMNLEN;              /* SAVE TOTAL LENGTH OF WORKAREA */
         SLR   @10,@10                                             0290
         ICM   @10,7,CCAMNLEN(R11)                                 0290
         ST    @10,DCRAMNLN(,DCRAWAPR)                             0290
*   DCRADESA=CCADESA;               /* SAVE DADSM EXTENT INFORMATION */
         MVC   DCRADESA(44,DCRAWAPR),CCADESA(R11)                  0291
*/********************************************************************/
*/*      OBTAIN ONE CYLINDER OF SPACE ON THE VOLUME FOR THE CRA      */
*/********************************************************************/
*                                                                  0292
*   CCACRABU=OFF;                   /* TURN OFF CRA BUILD INDICATOR  */
         NI    CCACRABU(R11),B'11111110'                           0292
*   IF CCAF2CB=OFF THEN             /* CALLER IS DEFINE SPACE        */
         TM    CCAF2CB(R11),B'00000010'                            0293
         BNZ   @RF00293                                            0293
*     DO;                           /* PERFORM DEFINE SPACE CHORES   */
*       DCRATRCY=CTVNOTRK;          /* SAVE TRACKS/CYLINDER VALUE    */
         L     @10,CCAURAB(,R11)                                   0295
         L     @10,CCACPE1(,@10)                                   0295
         MVC   DCRATRCY(2,DCRAWAPR),CTVNOTRK+48(@10)               0295
*       DCRABTRK=CCACRABT;          /* SAVE BLOCKS/TRACK VALUE       */
         MVC   DCRABTRK(2,DCRAWAPR),CCACRABT(R11)                  0296
*       IF((CTGFVATR^=BLANK)&       /* DATA SET ATTR FPL EXISTS      */
*           (CTGFVATR->CTGFLPT(1)->CTGDSUNQ=ON))×/* REQUEST IS FOR 0297
*                                      DEFINE UNIQUE                 */
*           (CTGFVSPC->CTGFLPT(1)->SPCRQTYP=CMSALCAN) THEN/* REQUEST
*                                      IS FOR DEF CANDIDATE          */
*                                                                  0297
         L     @10,CCACPL(,R11)                                    0297
         L     @10,CTGFVT(,@10)                                    0297
         L     @10,CTGFVATR(,@10)                                  0297
         LTR   @10,@10                                             0297
         BZ    @GL00001                                            0297
         L     @10,CTGFLPT(,@10)                                   0297
         TM    CTGDSUNQ(@10),B'01000000'                           0297
         BO    @RT00297                                            0297
@GL00001 L     @10,CCACPL(,R11)                                    0297
         L     @10,CTGFVT(,@10)                                    0297
         L     @10,CTGFVSPC(,@10)                                  0297
         L     @10,CTGFLPT(,@10)                                   0297
         TM    SPCRQTYP(@10),B'11000000'                           0297
         BNZ   @RF00297                                            0297
@RT00297 DS    0H                                                  0298
*         /***********************************************************/
*         /*                                                         */
*         /* CALL TO MODULE IGG0CLDD                                 */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0298
*Z027RDEF:                                                         0298
*         CALL IGGPRDEF;            /* INTERFACE TO DEFINE SPACE     */
Z027RDEF BAL   @14,IGGPRDEF                                        0298
*       ELSE                                                       0299
*         ;                         /* NOT CANDIDATE OR UNIQUE     0299
*                                      REQUEST                       */
@RF00297 DS    0H                                                  0300
*     END;                          /* END 'DO' FOR DEFINE SPACE     */
*   ELSE                                                           0301
*     ;                             /* CALLER IS DEFINE CATALOG      */
@RF00293 DS    0H                                                  0302
*   IF CCACD1=RCS THEN              /* SUCCESSFUL SO FAR             */
         CLI   CCACD1(R11),0                                       0302
         BNE   @RF00302                                            0302
*Z028SBAL:                                                         0303
*     CALL IGGPSBAL;                /* INTERFACE TO SUBALLOCATE RTN  */
Z028SBAL BAL   @14,IGGPSBAL                                        0303
*   ELSE                                                           0304
*     ;                             /* AN ERROR HAS OCCURRED         */
@RF00302 DS    0H                                                  0305
*   CCACRABU=ON;                    /* TURN ON CRA BUILD FLAG AGAIN  */
         OI    CCACRABU(R11),B'00000001'                           0305
*   CALL EXIT;                      /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0306
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */
         DS 0F
*   GEN(DC CL8'IGGPRDEF');          /* NAME OF FOLLOWING PROCEDURE   */
         DC CL8'IGGPRDEF'
*   END;                            /* END OF INNER PROCEDURE        */
@EL00003 DS    0H                                                  0309
@EF00003 DS    0H                                                  0309
@ER00003 BR    @14                                                 0309
@PB00003 DS    0H                                                  0310
