         LA    @15,DDSTRU                                          0426
         ST    @15,@AL00001+8                                      0426
         MVI   @AL00001+8,X'80'                                    0426
         L     @15,GDTPRT(,@02)                                    0426
         LA    @01,@AL00001                                        0426
         BALR  @14,@15                                             0426
*       CKCOND=4;                   /*                       @Y30LB32*/
         LA    CKCOND,4                                            0427
*     END;                          /*                       @Y30LB32*/
*                                                                  0428
*   /*****************************************************************/
*   /*                                                               */
*   /* RESTORE PRINT AREA POINTER AND RETURN ADDRESS AND     @Y30LB32*/
