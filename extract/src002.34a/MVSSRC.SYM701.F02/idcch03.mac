*   /*                                                       @Y30LB39*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0831
*                                   /*                       @Y30LB39*/
*   OLDERID2=NEWERID2;                                             0831
         L     @15,@PC00001                                        0831
         L     @14,GDTTR2(,@15)                                    0831
         MVC   @TS00001(95),NEWERID2(@14)                          0831
         MVC   OLDERID2(95,@14),@TS00001                           0831
*   NEWID2='SSAA';                  /*                       @Y30LB39
