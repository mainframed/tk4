         MVI   ENTNUM(@08),X'01'                                   0813
*     END;                          /*                       @Z40LB61*/
*                                                                  0814
*   /*****************************************************************/
*   /*                                                               */
*   /* COMPUTE THE RECORD OFFSET OF THIS RECORD FROM THE     @Z40LB61*/
*   /* BEGINNING OF THE PARTICULAR TABLE. DO THIS BY         @Z40LB61*/
*   /* OBTAINING THE CCHHR OF THE BEGINNING OF THE TABLE,    @Z40LB61*/
*   /* CONVERTING BOTH THE BEGINNING CCHHR AND CCHHR OF THE  @Z40LB61*/
*   /* CURRENT RECORD INTO RECORD OFFSETS, AND THEN TAKING   @Z40LB61*/
*   /* THEIR DIFFERENCE.                                     @Z40LB61*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0815
*   IF TABLEID=MVTBLID&MVTDB=ON THEN                               0815
@RF00810 L     @08,@PC00001+4                                      0815
         L     @04,EARGPTR(,@08)                                   0815
         CLC   TABLEID(1,@04),MVTBLID                              0815
         BNE   @RF00815                                            0815
         TM    MVTDB,B'10000000'                                   0815
         BNO   @RF00815                                            0815
*     COPYCHR=MVTDBCHR;             /*                       @Z40LB61*/
         L     @08,CCHHRPTR(,@08)                                  0816
         MVC   COPYCHR(5),MVTDBCHR(@08)                            0816
*   ELSE                                                           0817
*     COPYCHR=TBLCCHHR(TABLE#);     /*                       @Z40LB61*/
         B     @RC00815                                            0817
@RF00815 LR    @08,TABLE#                                          0817
         MH    @08,@CH00154                                        0817
         L     @04,@PC00001+4                                      0817
         L     @04,CCHHRPTR(,@04)                                  0817
         ALR   @04,@08                                             0817
         AL    @04,@CF02054                                        0817
         MVC   COPYCHR(5),TBLCCHHR-2(@04)                          0817
*   RECNUM=(RECCYL-COPYCC)*TRKSCYL*RECSTRK+(RECHEAD-COPYHH)*RECSTRK+(
*       RECREC-COPYR)+1;            /*                       @Z40LB61*/
@RC00815 L     @08,@PC00001+4                                      0818
         L     @08,EARGPTR(,@08)                                   0818
         LH    @04,RECCYL(,@08)                                    0818
         SH    @04,COPYCC                                          0818
         MH    @04,@CH02045                                        0818
         LH    @02,RECHEAD(,@08)                                   0818
         SH    @02,COPYHH                                          0818
         SLA   @02,5                                               0818
         ALR   @04,@02                                             0818
         SLR   @02,@02                                             0818
         IC    @02,RECREC(,@08)                                    0818
         SLR   @15,@15                                             0818
         IC    @15,COPYR                                           0818
         SLR   @02,@15                                             0818
         ALR   @04,@02                                             0818
         AL    @04,@CF00217                                        0818
         STH   @04,RECNUM(,@08)                                    0818
*   ORIGCHR=SAVECHR;                /*                       @Z40LB61*/
         MVC   ORIGCHR(5),SAVECHR                                  0819
