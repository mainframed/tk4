*   /*                                                               */
*   /*****************************************************************/
*                                                                  0779
*   SAVECHR=ORIGCHR;                /*                       @Z40LB61*/
         MVC   SAVECHR(5),ORIGCHR                                  0779
*   ORIGR=ORIGR+RECBUMP-1;          /*                       @Z40LB61*/
         SLR   @08,@08                                             0780
         IC    @08,ORIGR                                           0780
