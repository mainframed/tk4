*   CHRCARRY=ORIGR/RECSTRK;         /*                       @Z40LB61*/
         LR    CHRCARRY,@08                                        0781
         SRL   CHRCARRY,5                                          0781
*   ORIGR=ORIGR//RECSTRK+1;         /*                       @Z40LB61*/
         LR    @00,@08                                             0782
         SRDA  @00,32                                              0782
         D     @00,@CF00219                                        0782
         AL    @00,@CF00217                                        0782
         STC   @00,ORIGR                                           0782
*   ORIGHH=ORIGHH+CHRCARRY;         /*                       @Z40LB61*/
         LR    @08,CHRCARRY                                        0783
         AH    @08,ORIGHH                                          0783
         STH   @08,ORIGHH                                          0783
*   CHRCARRY=ORIGHH/TRKSCYL;        /*                       @Z40LB61*/
         LA    @04,19                                              0784
         LR    @00,@08                                             0784
         SRDA  @00,32                                              0784
         DR    @00,@04                                             0784
         LR    CHRCARRY,@01                                        0784
*   ORIGHH=ORIGHH//TRKSCYL;         /*                       @Z40LB61*/
         LR    @00,@08                                             0785
         SRDA  @00,32                                              0785
         DR    @00,@04                                             0785
         STH   @00,ORIGHH                                          0785
*   ORIGCC=ORIGCC+CHRCARRY;         /*                       @Z40LB61*/
         LR    @08,CHRCARRY                                        0786
         AH    @08,ORIGCC                                          0786
         STH   @08,ORIGCC                                          0786
*   COPYCHR=ORIGCHR;                /*                       @Z40LB61*/
         MVC   COPYCHR(5),ORIGCHR                                  0787
*   COPYCC=COPYCC+CCOFFSET;         /*                       @Z40LB61*/
*                                                                  0788
         L     @08,@PC00001+4                                      0788
         L     @04,CCHHRPTR(,@08)                                  0788
         LH    @02,COPYCC                                          0788
         AH    @02,CCOFFSET(,@04)                                  0788
         STH   @02,COPYCC                                          0788
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE RECORD TO BE OBTAINED IS NOT ALREADY IN THE    @Z40LB61*/
*   /* I/O BUFFER IN STORAGE, ISSUE THE UEXCP MACRO TO READ  @Z40LB61*/
*   /* THE RECORD. CHECK FOR AN ERROR READING IT.            @Z40LB61*/
