*       EXGDATAL=RECLNGTH;          /*                       @Z40LB61*/
         MVC   EXGDATAL(2,@08),@CH01842                            0794
*       EXGCCHH1=ADDR(COPYCHR);     /*                       @Z40LB61*/
         LA    @15,COPYCHR                                         0795
         ST    @15,EXGCCHH1(,@08)                                  0795
*       EXGCCHH2=ADDR(ORIGCHR);     /*                       @Z40LB61*/
         LA    @15,ORIGCHR                                         0796
         ST    @15,EXGCCHH2(,@08)                                  0796
*       CALL IDCIO050(GDTTBL,GET,EXGARG);/*                  @Z40LB61*/
         ST    @02,@AL00001                                        0797
         LA    @15,GET                                             0797
         ST    @15,@AL00001+4                                      0797
         ST    @08,@AL00001+8                                      0797
         MVI   @AL00001+8,X'80'                                    0797
         L     @15,GDTEXP(,@02)                                    0797
         LA    @01,@AL00001                                        0797
         BALR  @14,@15                                             0797
*       RESPECIFY                                                  0798
*         RTNREG RSTD;                                             0798
*       TESTRC=RTNREG;                                             0799
         LR    TESTRC,RTNREG                                       0799
*       RESPECIFY                                                  0800
*         RTNREG UNRSTD;            /*                       @Z40LB61*/
*       IF TESTRC^=0 THEN                                          0801
         LTR   TESTRC,TESTRC                                       0801
         BZ    @RF00801                                            0801
*         DO;                       /*                       @Z40LB61*/
*           GETRC=TABLERR;          /*                       @Z40LB61*/
         L     @08,@PC00001+4                                      0803
         L     @08,EARGPTR(,@08)                                   0803
         MVC   GETRC(2,@08),@CH00565                               0803
*           RETURN;                 /*                       @Z40LB61*/
@EL00003 DS    0H                                                  0804
@EF00003 DS    0H                                                  0804
@ER00003 LM    @14,@12,@SA00003                                    0804
         BR    @14                                                 0804
*         END;                      /*                       @Z40LB61*/
*       PREVCHR=ORIGCHR;            /*                       @Z40LB61*/
@RF00801 L     @08,@PC00001+4                                      0806
         L     @08,EARGPTR(,@08)                                   0806
         MVC   PREVCHR(5,@08),ORIGCHR                              0806
*       TBLIO=ON;                   /*                       @Z40LB61*/
         OI    TBLIO,B'10000000'                                   0807
*     END;                          /*                       @Z40LB61*/
*                                                                  0808
*   /*****************************************************************/
*   /*                                                               */
*   /* SET THE POINTER TO THE FIRST ENTRY OF THE RECORD.     @Z40LB61*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0809
*   TBLADDR=RECADDR+LENGTH(PREFIX); /*                       @Z40LB61*/
@RF00789 L     @08,@PC00001+4                                      0809
         L     @08,EARGPTR(,@08)                                   0809
         LA    @04,8                                               0809
         LR    @02,RECADDR                                         0809
         ALR   @02,@04                                             0809
         ST    @02,TBLADDR(,@08)                                   0809
*   IF FREEFORM=ON THEN                                            0810
         TM    FREEFORM,B'10000000'                                0810
         BNO   @RF00810                                            0810
*     DO;                           /*                       @Z40LB61*/
*       TBLADDR=TBLADDR+LENGTH(CBLOCK);/*                    @Z40LB61*/
         ALR   @02,@04                                             0812
         ST    @02,TBLADDR(,@08)                                   0812
