         L     @10,@PC00001+4                                      0501
         L     @10,FDTPTR+76(,@10)                                 0501
         LTR   @10,@10                                             0501
         BZ    @RF00501                                            0501
*             MODBIND=ON;           /*                       @G24LB22*/
         L     @10,MODCPTR                                         0502
         OI    MODBIND(@10),B'00000100'                            0502
