*/********************************************************************/
*/*                                                                  */
*/* INPUT TO UPDATO:                                                 */
*/*      REQUEST CODE = 4                                            */
*/*      BUFFER ADDRESS= ADDRESS OF FIRST UNUSED BYTE OF MESSAGE     */
*/* OUTPUT FROM UPDATO:                                              */
*/*      RETURN CODE = 1 => ELEMENT UPDATED                          */
*/*                  = 2 => NOTHING ON QUEUE TO UPDATE               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0306
*UPDATO:                                                           0306
*         DO;                       /* SUBCASE UPDATE QUEUE ELEMENT
*                                      BECAUSE A PARTIAL MESSAGE WAS
*                                      USED                          */
UPDATO   DS    0H                                                  0307
*           RFY                                                    0307
*             OQELE BASED(TVWANOM);                                0307
*           IF TVWABOQ^=TVWANOM THEN/* IF THERE IS SOMETHING TO    0308
*                                      UPDATE                        */
         L     @10,TVWANOM(,TVWAPTR)                               0308
         C     @10,TVWABOQ(,TVWAPTR)                               0308
         BE    @RF00308                                            0308
*             DO;                                                  0309
*                                                                  0309
*               /*****************************************************/
*               /*                                                   */
*               /* MOVE NEXT OUTPUT MESSAGE POINTER BACK TO THE      */
*               /* ELEMENT TO BE UPDATED                             */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0310
*               IF TVWANOM=0 THEN   /* IF NEXT POINTER IS PAST QUEUE */
         LTR   @10,@10                                             0310
         BNZ   @RF00310                                            0310
*                 DO;                                              0311
*                   TVWANOM=TVWAEOQ;/* SET NEXT MESSAGE PTR TO END 0312
*                                      QUEUE                         */
         L     @10,TVWAEOQ(,TVWAPTR)                               0312
         ST    @10,TVWANOM(,TVWAPTR)                               0312
*                   TVWADOOQ='1'B;  /* MARK DATA ON OUTPUT QUEUE     */
         OI    TVWADOOQ(TVWAPTR),B'01000000'                       0313
*                 END;                                             0314
*               ELSE                                               0315
*                 TVWANOM=TVWANOM->OQPREV;/* SET NEXT MESSAGE PTR TO
*                                      PREVIOUS                      */
         B     @RC00310                                            0315
@RF00310 L     @10,TVWANOM(,TVWAPTR)                               0315
         L     @10,OQPREV(,@10)                                    0315
         ST    @10,TVWANOM(,TVWAPTR)                               0315
*               OQ@CURR=PBUFADR-TVWANOM;/* CHANGE CURRENT OFFSET TO
*                                      REFLECT PARTIAL MESSAGE USED  */
@RC00310 L     @10,TVWANOM(,TVWAPTR)                               0316
         L     @07,PBUFADR                                         0316
         SLR   @07,@10                                             0316
         STH   @07,OQ@CURR(,@10)                                   0316
*               PRC=1;              /* INDICATE UPDATE SUCCESSFUL    */
         MVI   PRC,X'01'                                           0317
*             END;                                                 0318
*           ELSE                                                   0319
*             PRC=2;                /* INDICATE THERE WAS NOTHING ON
*                                      QUEUE TO UPDATE               */
         B     @RC00308                                            0319
@RF00308 MVI   PRC,X'02'                                           0319
*           GOTO CE0002@;           /* BRANCH TO END OF CASE         */
         B     CE0002@                                             0320
*         END UPDATO;               /* END OF SUBCASE                */
         EJECT
*                                                                  0322
*/********************************************************************/
*/*                                                                  */
*/* INPUT TO CLEARO:                                                 */
*/*      REQUEST CODE = 5                                            */
*/* OUTPUT FROM CLEARO:                                              */
*/*      NONE                                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0322
*CLEARO:                                                           0322
*         DO;                       /* SUBCASE REMOVE ALL BUT ASID 0322
*                                      MESSAGES FROM OUTPUT QUEUE    */
CLEARO   DS    0H                                                  0323
*           RFY                                                    0323
*             OQELE BASED(R1);                                     0323
*           RFY                                                    0324
*             R1 RSTD;                                             0324
*           R1=TVWAEOQ;             /* SET A TEMPORARY TO BEGINNING
*                                      OF Q                          */
         L     R1,TVWAEOQ(,TVWAPTR)                                0325
*           TVWADOOQ='0'B;          /* MARK NO DATA ON OUTPUT QUEUE  */
         NI    TVWADOOQ(TVWAPTR),B'10111111'                       0326
*           QPTR2=ADDR(TVWABOQ);    /* PASS @ OF OUTPUT PTRS TO DELEL*/
         LA    QPTR2,TVWABOQ(,TVWAPTR)                             0327
*           DO WHILE R1^=0;         /* WHILE THERE ARE ELEMENTS TO BE
*                                      DELETED                       */
         B     @DE00328                                            0328
@DL00328 DS    0H                                                  0329
*             TEMP5=OQPREV;         /* SAVE ADDRESS OF PREVIOUS    0329
*                                      ELEMENT                       */
         L     TEMP5,OQPREV(,R1)                                   0329
*             IF OQASID='0'B THEN   /* IF THIS IS NOT AN ASID MESSAGE*/
         TM    OQASID(R1),B'00001000'                              0330
         BNZ   @RF00330                                            0330
*               DO;                                                0331
*                 TVWAUSMN=TVWAUSMN-OQNBUF;/* REDUCE USE COUNT NO. OF
*                                      BYTES IN THE BUFFER           */
         L     @07,TVWAUSMN(,TVWAPTR)                              0332
         SH    @07,OQNBUF(,R1)                                     0332
         ST    @07,TVWAUSMN(,TVWAPTR)                              0332
*                 CALL DELEL;       /* TO UNCHAIN ELEMENT AND FREE 0333
*                                      STORAGE                       */
         BAL   @14,DELEL                                           0333
*               END;                                               0334
*             ELSE                  /* IF THERE IS AN ASID MESSAGE   */
*               TVWADOOQ='1'B;      /* THERES STILL DATA ON OUTPUT 0335
*                                      QUEUE                         */
         B     @RC00330                                            0335
@RF00330 OI    TVWADOOQ(TVWAPTR),B'01000000'                       0335
*             R1=TEMP5;             /* SET REG1TO PREVIOUS MESSAGE   */
@RC00330 LR    R1,TEMP5                                            0336
*           END;                                                   0337
@DE00328 LTR   R1,R1                                               0337
         BNZ   @DL00328                                            0337
*           RFY                                                    0338
*             R1 UNRSTD;                                           0338
*           TSBPTR=ASCBTSB;                                        0339
         L     @10,ASCBPTR                                         0339
         L     TSBPTR,ASCBTSB(,@10)                                0339
*           RFY                                                    0340
*             TSB BASED(TSBPTR);                                   0340
*           IF TSBNOBUF='1'B×TSBTJBF='1'B THEN                     0341
         TM    TSBNOBUF(TSBPTR),B'00010000'                        0341
         BO    @RT00341                                            0341
         TM    TSBTJBF(TSBPTR),B'00000001'                         0341
         BNO   @RF00341                                            0341
@RT00341 DS    0H                                                  0342
*             CALL MARKDISP;        /* TO DISPATCH TASKS SUSPENDED 0342
*                                      FOR BUFFER OVERUSE            */
         BAL   @14,MARKDISP                                        0342
*           RFY                                                    0343
*             TSB BASED(ASCBTSB);                                  0343
*           GOTO CE0002@;           /* BRANCH TO END OF CASE         */
         B     CE0002@                                             0344
*         END CLEARO;               /* END OF SUBCASE                */
         EJECT
*                                                                  0346
*/********************************************************************/
*/*                                                                  */
*/* INPUT TO REOBTAIN:                                               */
*/*      REQUEST CODE = 6                                            */
*/* OUTPUT FROM REOBTAIN:                                            */
*/*      RETURN CODE  = 1 => SUCCESSFUL, MORE MESSAGES ON QUEUE      */
*/*                   = 2 => SUCCESSFUL, NO MORE MESSAGES            */
*/*                   = 3 => UNSUCCESSFUL, NOTHING ON QUEUE          */
*/*      BUFFER SIZE  = SIZE (IN BYTES) OF MESSAGE                   */
*/*      BUFFER ADDRESS= ADDRESS OF FIRST BYTE OF MESSAGE            */
*/*      FLAGS         = QUEUE ELEMENT FLAGS                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0346
*REOBTAIN:                                                         0346
*         DO;                       /* SUBCASE                       */
REOBTAIN DS    0H                                                  0347
*           RFY                                                    0347
*             OQELE BASED(TEMP6);                                  0347
*           TEMP6=TVWABOQ;          /* SET A TEMPORARY TO TOP OF   0348
*                                      QUEUE                         */
         L     TEMP6,TVWABOQ(,TVWAPTR)                             0348
*           IF TEMP6=0 THEN         /* IF THERE IS NOTHING ON QUEUE  */
         LTR   TEMP6,TEMP6                                         0349
         BNZ   @RF00349                                            0349
*             PRC=3;                /* SET RC TO 'NOTHING ON QUEUE'  */
         MVI   PRC,X'03'                                           0350
*           ELSE                    /* IF THERE IS SOMETHING TO    0351
*                                      REOBTAIN                      */
*             DO;                                                  0351
         B     @RC00349                                            0351
@RF00349 DS    0H                                                  0352
*               DO WHILE(TEMP6^=0&  /* WHILE THERE ARE ELEMENTS LEFT
*                                      AND                           */
*                     TEMP6^=TVWANOM->OQNEXT);/* TEMPORARY ^= NEXT 0352
*                                      POINTER                       */
         B     @DE00352                                            0352
@DL00352 DS    0H                                                  0353
*                 OQ@CURR=OQ@STRT;  /* SET CURRENT OFFSET TO START 0353
*                                      OFFSET                        */
         LH    @10,OQ@STRT(,TEMP6)                                 0353
         STH   @10,OQ@CURR(,TEMP6)                                 0353
*                 TEMP6=OQNEXT;     /* SET TEMPORARY TO NEXT ELEMENT */
         L     TEMP6,OQNEXT(,TEMP6)                                0354
*               END;                                               0355
@DE00352 LTR   TEMP6,TEMP6                                         0355
         BZ    @DC00352                                            0355
         L     @10,TVWANOM(,TVWAPTR)                               0355
         C     TEMP6,OQNEXT(,@10)                                  0355
         BNE   @DL00352                                            0355
@DC00352 DS    0H                                                  0356
*               RFY                                                0356
*                 OQELE BASED(TVWABOQ);                            0356
*               PBUFADR=TVWABOQ+OQ@CURR;/* SET BUFFER ADDRESS PARM   */
         L     @10,TVWABOQ(,TVWAPTR)                               0357
         LH    @07,OQ@CURR(,@10)                                   0357
         LR    @15,@10                                             0357
         ALR   @15,@07                                             0357
         ST    @15,PBUFADR                                         0357
*               PBFSZ=OQNBQE-OQ@CURR;/* SET BUFFER SIZE PARM         */
         LCR   @07,@07                                             0358
         LH    @15,OQNBQE(,@10)                                    0358
         N     @15,@CF02260                                        0358
         ALR   @07,@15                                             0358
         STH   @07,PBFSZ                                           0358
*               PFLAGS=OQFLAGS;     /* SET BUFFER FLAGS PARM         */
         MVC   PFLAGS(4),OQFLAGS(@10)                              0359
*               TVWANOM=OQNEXT;     /* SET NEXT MESSAGE PTR TO NEXT
*                                      ELE                           */
         L     @10,OQNEXT(,@10)                                    0360
         ST    @10,TVWANOM(,TVWAPTR)                               0360
*               RFY                                                0361
*                 OQELE BASED(TVWANOM);                            0361
*               IF TVWANOM=0 THEN   /* IF NOTHING MORE ON QUEUE      */
         LTR   @10,@10                                             0362
         BNZ   @RF00362                                            0362
*                 DO;                                              0363
*                   TVWADOOQ='0'B;  /* MARK NO MORE DATA ON OUTPUT 0364
*                                      QUEUE                         */
         NI    TVWADOOQ(TVWAPTR),B'10111111'                       0364
*                   PRC=2;          /* SET RC TO 'LAST MESSAGE ON  0365
*                                      QUEUE'                        */
         MVI   PRC,X'02'                                           0365
*                 END;                                             0366
*               ELSE                /* IF MORE ON QUEUE              */
*                 DO;                                              0367
         B     @RC00362                                            0367
@RF00362 DS    0H                                                  0368
*                   TVWADOOQ='1'B;  /* MARK DATA ON OUTPUT QUEUE     */
         OI    TVWADOOQ(TVWAPTR),B'01000000'                       0368
*                   PRC=1;          /* SET RC TO 'MORE TO COME'      */
         MVI   PRC,X'01'                                           0369
*                   PNXBFSZ=OQNBQE-OQ@CURR;/* SET NEXT BUFFER SIZE 0370
*                                      PARM                          */
         L     @10,TVWANOM(,TVWAPTR)                               0370
         LH    @07,OQNBQE(,@10)                                    0370
         N     @07,@CF02260                                        0370
         SH    @07,OQ@CURR(,@10)                                   0370
         STH   @07,PNXBFSZ                                         0370
*                   PNXFLGS=OQFLAGS;/* SET NEXT BUFFER FLAGS PARM    */
         MVC   PNXFLGS(4),OQFLAGS(@10)                             0371
*                 END;                                             0372
*             END;                                                 0373
*           GOTO CE0002@;           /* BRANCH TO END OF CASE         */
         B     CE0002@                                             0374
*         END REOBTAIN;             /* END OF SUBCASE                */
         EJECT
*                                                                  0376
*/********************************************************************/
*/*                                                                  */
*/* INPUT TO RESET:                                                  */
*/*      REQUEST CODE = 7                                            */
*/* OUTPUT FROM RESET:                                               */
*/*      NONE                                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0376
*RESET:                                                            0376
*         DO;                       /* SUBCASE RETURN TO TOP OF QUEUE*/
RESET    DS    0H                                                  0377
*           RFY                                                    0377
*             OQELE BASED(TEMP7);                                  0377
*           TEMP7=TVWABOQ;          /* SET A TEMPORARY TO TOP OF   0378
*                                      QUEUE                         */
         L     TEMP7,TVWABOQ(,TVWAPTR)                             0378
*           DO WHILE(TEMP7^=0&      /* DO WHILE THERE IS SOMETHING 0379
*                                      AND                           */
*                 TEMP7^=TVWANOM->OQNEXT);/* THE POINTER ^= NEXT     */
         B     @DE00379                                            0379
@DL00379 DS    0H                                                  0380
*             OQ@CURR=OQ@STRT;      /* SET CURRENT OFFSET TO START 0380
*                                      OFFSET                        */
         LH    @10,OQ@STRT(,TEMP7)                                 0380
         STH   @10,OQ@CURR(,TEMP7)                                 0380
*             TEMP7=OQNEXT;         /* SET TEMPORARY TO NEXT ELEMENT */
         L     TEMP7,OQNEXT(,TEMP7)                                0381
*           END;                                                   0382
@DE00379 LTR   TEMP7,TEMP7                                         0382
         BZ    @DC00379                                            0382
         L     @10,TVWANOM(,TVWAPTR)                               0382
         C     TEMP7,OQNEXT(,@10)                                  0382
         BNE   @DL00379                                            0382
@DC00379 DS    0H                                                  0383
*           TVWANOM=TVWABOQ;        /* SET NEXT MESSAGE POINTER TO 0383
*                                      TOP                           */
         L     @10,TVWABOQ(,TVWAPTR)                               0383
         ST    @10,TVWANOM(,TVWAPTR)                               0383
*           IF TVWANOM=0 THEN       /* IF THERE IS NOTHING ON QUEUE  */
         LTR   @10,@10                                             0384
         BNZ   @RF00384                                            0384
*             TVWADOOQ='0'B;        /* MARK NO DATA ON OUTPUT QUEUE  */
         NI    TVWADOOQ(TVWAPTR),B'10111111'                       0385
*           ELSE                                                   0386
*             TVWADOOQ='1'B;        /* THERE IS DATA ON OUTPUT QUEUE */
         B     @RC00384                                            0386
@RF00384 OI    TVWADOOQ(TVWAPTR),B'01000000'                       0386
*           GOTO CE0002@;           /* BRANCH TO END OF CASE         */
         B     CE0002@                                             0387
*         END RESET;                /* END OF SUBCASE                */
         EJECT
*                                                                  0389
*/********************************************************************/
*/*                                                                  */
*/* INPUT TO ADDTOP:                                                 */
*/*      REQUEST CODE = 8                                            */
*/*      FLAGS        = THE FLAGS THE CALLER WANTS SET               */
*/*      BUFFER SIZE  = THE MESSAGE LENGTH                           */
*/* OUTPUT FROM ADDTOP:                                              */
*/*      RETURN CODE  = 1 => SUCCESSFUL                              */
*/*                   = 2 => UNSUCCESSFUL, NO MORE ROOM FOR ELEMENTS */
*/*      BUFFER ADDRESS= ADDRESS AT WHICH CALLER SHOULD PLACE THE    */
*/*                      MESSAGE                                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0389
*ADDTOP:                                                           0389
*         DO;                       /* SUBCASE ADD AN ELEMENT TO BE
*                                      NEXT OUTPUT                   */
*                                                                  0389
ADDTOP   DS    0H                                                  0390
*           /*********************************************************/
*           /*                                                       */
*           /* MESSAGES ARE TO BE ADDED TO THE MIDDLE OF THE OUTPUT  */
*           /* QUEUE IN FIFO ORDER. THIS LOOP FINDS THE POSITION     */
*           /* WHERE THIS NEW ELEMENT SHOULD GO                      */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0390
*           TEMP9=TVWANOM;          /* SET TEMPORARY TO NEXT MESSAGE
*                                      PTR                           */
         L     TEMP9,TVWANOM(,TVWAPTR)                             0390
*           DO WHILE(TEMP9^=0&      /* WHILE THERE ARE ELEMENTS ON 0391
*                                      QUEUE                         */
*                 TEMP9->OQADTOP='1'B);/* AND MESSAGES ARE MARKED AS
*                                      ADDED TO TOP OF QUEUE         */
         B     @DE00391                                            0391
@DL00391 DS    0H                                                  0392
*             TEMP9=TEMP9->OQNEXT;  /* SET TEMPORARY TO NEXT ELEMENT */
         L     TEMP9,OQNEXT(,TEMP9)                                0392
*           END;                                                   0393
@DE00391 LTR   TEMP9,TEMP9                                         0393
         BZ    @DC00391                                            0393
         TM    OQADTOP(TEMP9),B'01000000'                          0393
         BO    @DL00391                                            0393
@DC00391 DS    0H                                                  0394
*           QPTR1=ADDR(TVWABOQ);    /* PASS ADDRESS OF QUEUE PTRS TO
*                                      RTN                           */
         LA    QPTR1,TVWABOQ(,TVWAPTR)                             0394
*           CALL BLDELE;            /* TO BUILD ELEMENT AND CHAIN AT
*                                      END                           */
         BAL   @14,BLDELE                                          0395
*           IF R15=0 THEN           /* IF BLDELE WENT OK             */
         SLR   @14,@14                                             0396
         CR    R15,@14                                             0396
         BNE   @RF00396                                            0396
*             DO;                                                  0397
*               PRC=1;              /* SET RC TO SUCCESSFUL          */
         MVI   PRC,X'01'                                           0398
*               RFY                                                0399
*                 OQELE BASED(R1);                                 0399
*               RFY                                                0400
*                 R1 RSTD;          /* REG 1 CONTAINS ADDRESS OF NEW
*                                      ELE AFTER THE CALL TO BLDELE  */
*               TVWADOOQ='1'B;      /* MARK DATA ON OUTPUT QUEUE     */
         OI    TVWADOOQ(TVWAPTR),B'01000000'                       0401
*               OQADTOP='1'B;       /* MARK THIS ELEMENT AS ADD TO 0402
*                                      TOP                           */
         OI    OQADTOP(R1),B'01000000'                             0402
*               IF TEMP9^=0 THEN    /* IF NEW ELEMENT MUST GO IN   0403
*                                      MIDDLE OF QUEUE SOMEWHERE     */
*                                                                  0403
         CR    TEMP9,@14                                           0403
         BE    @RF00403                                            0403
*                 /***************************************************/
*                 /*                                                 */
*                 /* THIS CODE TAKES THE ELEMENT OFF THE END OF THE  */
*                 /* QUEUE AND PLACES IT (LOGICALLY) IN ITS PROPER   */
*                 /* PLACE, WHICH WAS DETERMINED EARLIER, IN THE     */
*                 /* MIDDLE OF THE QUEUE                             */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0404
*                 DO;                                              0404
*                   OQPREV->OQNEXT=OQNEXT;/* TAKE THIS ELEMENT OFF 0405
*                                      END OF QUEUE                  */
         L     @10,OQPREV(,R1)                                     0405
         L     @07,OQNEXT(,R1)                                     0405
         ST    @07,OQNEXT(,@10)                                    0405
*                   TVWAEOQ=OQPREV;                                0406
         ST    @10,TVWAEOQ(,TVWAPTR)                               0406
*                   OQNEXT=TEMP9;   /* AND CHAIN IT AHEAD OF FIRST 0407
*                                      NON ADDED TO TOP OF THE QUEUE
*                                      MESSAGE                       */
         ST    TEMP9,OQNEXT(,R1)                                   0407
*                   OQPREV=TEMP9->OQPREV;                          0408
         L     @10,OQPREV(,TEMP9)                                  0408
         ST    @10,OQPREV(,R1)                                     0408
*                   TEMP9->OQPREV=R1;/* SET PREVIOUS PTR TO NEW    0409
*                                      ELEMENT                       */
         ST    R1,OQPREV(,TEMP9)                                   0409
*                   IF TEMP9=TVWABOQ THEN/* IF NEW ELEMENT IS GOING
*                                      AT                            */
         C     TEMP9,TVWABOQ(,TVWAPTR)                             0410
         BNE   @RF00410                                            0410
*                     DO;           /* BEGINNING OF OUTPUT QUEUE     */
*                       TVWABOQ=R1; /* SET START OF QUEUE TO NEW   0412
*                                      ELEMENT                       */
         ST    R1,TVWABOQ(,TVWAPTR)                                0412
*                       TVWANOM=R1; /* SET NEXT MESSAGE PTR TO NEW 0413
*                                      ELEMENT                       */
         ST    R1,TVWANOM(,TVWAPTR)                                0413
*                     END;                                         0414
*                   ELSE                                           0415
*                     DO;                                          0415
         B     @RC00410                                            0415
@RF00410 DS    0H                                                  0416
*                       OQPREV->OQNEXT=R1;/* SET NEXT PTR OF PREVIOUS
*                                      ELEMENT TO POINT TO NEW     0416
*                                      ELEMENT                       */
         L     @10,OQPREV(,R1)                                     0416
         ST    R1,OQNEXT(,@10)                                     0416
*                       IF TEMP9=TVWANOM THEN/* IF NEW ELE IS GOING
*                                      AT NEXT PTR                   */
         C     TEMP9,TVWANOM(,TVWAPTR)                             0417
         BNE   @RF00417                                            0417
*                         TVWANOM=R1;/* SET NEXT MESSAGE PTR TO NEW
*                                      ELE                           */
         ST    R1,TVWANOM(,TVWAPTR)                                0418
*                     END;                                         0419
@RF00417 DS    0H                                                  0420
*                 END;                                             0420
@RC00410 DS    0H                                                  0421
*               RFY                                                0421
*                 R1 UNRSTD;                                       0421
@RF00403 DS    0H                                                  0422
*             END;                                                 0422
*           ELSE                    /* IF BLDELE FAILED              */
*             PRC=2;                /* SET RC TO 'OUT OF STORAGE'    */
         B     @RC00396                                            0423
@RF00396 MVI   PRC,X'02'                                           0423
*           GOTO CE0002@;           /* BRANCH TO END OF CASE         */
         B     CE0002@                                             0424
*         END ADDTOP;               /* END OF SUBCASE                */
         EJECT
*CE0002@:                                                          0426
*       END BRAN2;                  /* END OF CASE                   */
CE0002@  DS    0H                                                  0427
