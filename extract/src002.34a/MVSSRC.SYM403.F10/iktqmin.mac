MARKDISP ST    @14,@SA00004                                        0692
*/* THIS ROUTINE DISPATCHES TASKS SUSPENDED FOR BUFFER OVERUSE AND   */
*/* RESETS INDICATORS                                                */
*                                                                  0693
*   RFY                                                            0693
*     TSB BASED(TSBPTR);                                           0693
*   IF TSBNOBUF='1'B&TSBOWAIT='1'B THEN/* IF A TASK IN THIS ADDRESS
*                                      SPACE IS WAITING              */
         TM    TSBNOBUF(TSBPTR),B'00010000'                        0694
         BNO   @RF00694                                            0694
         TM    TSBOWAIT(TSBPTR),B'00100000'                        0694
         BNO   @RF00694                                            0694
*     DO;                                                          0695
*       TSBOWAIT='0'B;              /* CLEAR TASK IS WAITING FLAG    */
         NI    TSBOWAIT(TSBPTR),B'11011111'                        0696
*/*   CALL STATUS BRANCH ENTRY                                       */
*                                                                  0697
*       RFY                                                        0697
*        (R0,                                                      0697
*         R1,                                                      0697
*         R10,                                                     0697
*         R13) RSTD;                                               0697
*       R0=8;                       /* ENTRY CODE IN REG0    @YM03241*/
         LA    R0,8                                                0698
*       R1='80000000'X;             /* INDICATES DESET               */
         L     R1,@CF02250                                         0699
*       R10=R13;                    /* MOVE ADDRESS OF SAVE AREA TO
*                                      REG 10                        */
         LR    R10,R13                                             0700
*       R13='0400'X;                /* INDICATES TCBOWAIT IS BIT   0701
*                                      MANIPULATED                   */
         LA    R13,1024                                            0701
*       CALL STATUS;                                               0702
         L     @07,CVTPTR                                          0702
         L     @07,SCVTSPTR(,@07)                                  0702
         L     @15,SCVTSTAT(,@07)                                  0702
         BALR  @14,@15                                             0702
*       R13=R10;                    /* MOVE SAVE AREA @ BACK TO REG
*                                      13                            */
         LR    R13,R10                                             0703
*       RFY                                                        0704
*        (R0,                                                      0704
*         R1,                                                      0704
*         R10,                                                     0704
*         R13) UNRSTD;                                             0704
*     END;                                                         0705
*   TSBNOBUF='0'B;                  /* CLEAR TASK WAITING ON BUFFERS
*                                      FLAG                          */
@RF00694 NI    TSBNOBUF(TSBPTR),B'11101111'                        0706
*   IF TSBTJBF='1'B THEN            /* IF A TASK IS WAITING IN     0707
*                                      ANOTHER ADDRESS SPACE         */
*                                                                  0707
         TM    TSBTJBF(TSBPTR),B'00000001'                         0707
         BNO   @RF00707                                            0707
*     /***************************************************************/
*     /*                                                             */
*     /* RESCHEDULE IKTXMTPT IN THIS ADDRESS SAPCE                   */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0708
*     DO;                                                          0708
*       RFY                                                        0709
*         SRBPTR RSTD;                                             0709
*       SRBPTR=TVCSSRBA;                                           0710
         L     @10,TSBEXTNT(,TSBPTR)                               0710
         L     @10,TSBXCSAP(,@10)                                  0710
         L     SRBPTR,TVCSSRBA-1(,@10)                             0710
         LA    SRBPTR,0(,SRBPTR)                                   0710
*       GEN REFS(CVTLSMQ,CVTMAP,SRBFLNK,SRBSECT,SRBPTR) SETS(R0,R15);
         SCHEDULE SRB=(1),SCOPE=LOCAL
*       TSBTJBF='0'B;               /* CLEAR TSBTJBF                 */
         NI    TSBTJBF(TSBPTR),B'11111110'                         0712
*       RFY                                                        0713
*         SRBPTR UNRSTD;                                           0713
*     END;                                                         0714
*   RFY                                                            0715
*     TSB BASED(ASCBTSB);                                          0715
*   END MARKDISP;                                                  0716
@EL00004 DS    0H                                                  0716
@EF00004 DS    0H                                                  0716
@ER00004 L     @14,@SA00004                                        0716
         BR    @14                                                 0716
