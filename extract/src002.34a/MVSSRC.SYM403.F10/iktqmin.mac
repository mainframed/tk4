BLDELE   ST    @14,@SA00003                                        0612
*/*  THIS ROUTINE IS TO BUILD A QUEUE ELEMENT FOR EITHER QUEUE AND   */
*/*  CHAIN IT ON THE END OF THE QUEUE                                */
*/*  ADDRESS OF NEW ELEMENT IS RETURNED IN REG 1                     */
*                                                                  0613
*   RFY                                                            0613
*     QELE BASED(R1);                                              0613
*   RFY                                                            0614
*     QPTRS BASED(QPTR1);                                          0614
*   RFY                                                            0615
*     R1 RSTD;                                                     0615
*   IF PBFSZ+HEADER>TVWACLSZ THEN   /* IF BUFFER NEEDED IS BIGGER  0616
*                                      THAN A                        */
         LA    @10,20                                              0616
         AH    @10,PBFSZ                                           0616
         CH    @10,TVWACLSZ(,TVWAPTR)                              0616
         BNH   @RF00616                                            0616
*     DO;                           /* NORMAL BUFFER                 */
*       RFY                                                        0618
*        (R0,                                                      0618
*         R4,                                                      0618
*         R7,                                                      0618
*         R15) RSTD;                                               0618
*       R0=PBFSZ+HEADER;            /* SET SIZE OF GETMAIN           */
         LR    R0,@10                                              0619
*       R4=TVWATCB;                 /* SET TCB @ FOR BRANCH ENTRY  0620
*                                      GETMAIN                       */
         L     R4,TVWATCB(,TVWAPTR)                                0620
*       R7=PSAAOLD;                 /* SET ASCB@ FOR BRANCH ENTRY    */
*                                                                  0621
         L     R7,PSAAOLD                                          0621
*       /*************************************************************/
*       /*                                                           */
*       /* GETMAIN A BUFFER FOR USE                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0622
*       GEN REFS(R0,R4,R7,CVTCRMN,CVTPTR) SETS(R1,R2,R3,R14,R15);  0622
        MODESET EXTKEY=ZERO,SAVEKEY=(2),WORKREG=2    SAVE CALLERS KEY
        GETMAIN RC,LV=(0),SP=SP229,BRANCH=YES,KEY=TCASKEY
        MODESET KEYADDR=(2)
*       RFY                                                        0623
*        (R4,                                                      0623
*         R7) UNRSTD;                                              0623
*       IF R15=0 THEN               /* IF GETMAIN WAS SUCCESSFUL     */
         LTR   R15,R15                                             0624
         BNZ   @RF00624                                            0624
*         DO;                                                      0625
*           QNBUF=R0;               /* PUT NO. OF BYTES IN BUFFER    */
         STH   R0,QNBUF(,R1)                                       0626
*           QALLOC='1'B;            /* SET ALLOC BIT TO GETMAIN'D    */
         OI    QALLOC(R1),B'10000000'                              0627
*         END;                                                     0628
*     END;                                                         0629
*   ELSE                            /* IF BUFFER NEEDED IS A NORMAL  */
*     DO;                           /* SIZE OR LESS                  */
         B     @RC00616                                            0630
@RF00616 DS    0H                                                  0631
*       R0=TVWACPID;                /* SET REG 0 TO CELL POOL ID     */
         L     R0,TVWACPID(,TVWAPTR)                               0631
*       GEN REFS(R0,CVTGETCL,CVTMAP) SETS(R1,R14,R15);             0632
*                                                    /* GETCELL  */
       GETCELL CPID=(0),BRANCH=YES
*       RFY                                                        0633
*         R0 UNRSTD;                                               0633
*       IF R15=0 THEN               /* IF GETCELL WAS SUCCESSFUL     */
         LTR   R15,R15                                             0634
         BNZ   @RF00634                                            0634
*         DO;                                                      0635
*           QNBUF=TVWACLSZ;         /* SET SIZE OF BUFFER            */
         LH    @14,TVWACLSZ(,TVWAPTR)                              0636
         STH   @14,QNBUF(,R1)                                      0636
*           QALLOC='0'B;            /* MARK THIS BUFFER GETCELL'D    */
         NI    QALLOC(R1),B'01111111'                              0637
*         END;                                                     0638
*       ELSE                        /* IF GETCELL FAILED             */
*         IF R15>12 THEN                                           0639
         B     @RC00634                                            0639
@RF00634 C     R15,@CF02219                                        0639
         BNH   @RF00639                                            0639
*           GEN;                                                   0640
        ABEND X'0AD',,,SYSTEM
*         ELSE                                                     0641
*           DO;                                                    0641
         B     @RC00639                                            0641
@RF00639 DS    0H                                                  0642
*             RFY                                                  0642
*              (R0,                                                0642
*               R4,                                                0642
*               R7) RSTD;                                          0642
*             R0=4096;              /* LENGTH OF GETMAIN AREA        */
         L     R0,@CF02165                                         0643
*             R4=TVWATCB;                                          0644
         L     R4,TVWATCB(,TVWAPTR)                                0644
*             R7=PSAAOLD;                                          0645
         L     R7,PSAAOLD                                          0645
*             GEN REFS(R0,R4,R7,CVTCRMN,CVTMAP) SETS(R1,R2,R3,R14,R15);
        MODESET EXTKEY=ZERO,SAVEKEY=(2),WORKREG=2     SAVE CALLERS KEY
       GETMAIN RC,LV=(0),SP=SP229,BNDRY=PAGE,BRANCH=YES,KEY=TCASKEY
        MODESET KEYADDR=(2)                      RETURN TO CALLERS KEY
*             RFY                                                  0647
*              (R0,                                                0647
*               R4,                                                0647
*               R7) UNRSTD;                                        0647
*             IF R15=0 THEN         /* IF GTMN FOR POOL EXTNT      0648
*                                      SUCCESSFUL                    */
         LTR   R15,R15                                             0648
         BNZ   @RF00648                                            0648
*               DO;                                                0649
*                 RFY                                              0650
*                  (R0,                                            0650
*                   R3) RSTD;                                      0650
*                 R3=TVWACLSZ;      /* PUT SIZE OF A CELL IN REG 3   */
         LH    R3,TVWACLSZ(,TVWAPTR)                               0651
*                 R0=TVWACPID;      /* SET REG 0 TO CELL POOL ID     */
         L     R0,TVWACPID(,TVWAPTR)                               0652
*                 GEN REFS(R0,R1,R3,R13,CVTBLDCP,CVTGETCL,CVTMAP) SETS(
*                     R1,R14,R15);                                 0653
         BLDCPOOL CPID=(0),BRANCH=YES,AUTODEL=YES,CPADDR=(1),         ++
               CSIZE=(R3),POOLSIZ=4,SP=SP229
         GETCELL  CPID=(0),BRANCH=YES
*                 RFY                                              0654
*                  (R0,                                            0654
*                   R3) UNRSTD;                                    0654
*                 QALLOC='0'B;      /* MARK THIS BUFFER AS GETCELL'D */
         NI    QALLOC(R1),B'01111111'                              0655
*                 QNBUF=TVWACLSZ;   /* SET NO. OF BYTES TO CELL SIZE */
         LH    @14,TVWACLSZ(,TVWAPTR)                              0656
         STH   @14,QNBUF(,R1)                                      0656
*               END;                                               0657
*           END;                                                   0658
@RF00648 DS    0H                                                  0659
*     END;                                                         0659
@RC00639 DS    0H                                                  0659
@RC00634 DS    0H                                                  0660
*   IF R15>0&TVWAQMIO='1'B&TVWAQLBU='0'B THEN                      0660
@RC00616 LTR   R15,R15                                             0660
         BNP   @RF00660                                            0660
         TM    TVWAQMIO(TVWAPTR),B'00001000'                       0660
         BNO   @RF00660                                            0660
         TM    TVWAQLBU(TVWAPTR),B'00000100'                       0660
         BNZ   @RF00660                                            0660
*     DO;                                                          0661
*       TVWAQLBU='1'B;              /* MARK LOOK ASIDE BUFFER AS IN
*                                      USE                           */
         OI    TVWAQLBU(TVWAPTR),B'00000100'                       0662
*       R1=TVWAQMLB;                /* PUT @ OF LOOK ASIDE BUFFER IN
*                                      R1                            */
         L     R1,TVWAQMLB(,TVWAPTR)                               0663
*       R15=0;                      /* RESET REGISTER 15             */
         SLR   R15,R15                                             0664
*       QNBUF=TVWACLSZ;             /* SET SIZE OF THE BUFFER        */
         LH    @14,TVWACLSZ(,TVWAPTR)                              0665
         STH   @14,QNBUF(,R1)                                      0665
*       IF PBFSZ+HEADER>TVWACLSZ THEN/* IF SIZE OF BUFFER NEEDED IS
*                                      BIGGER THAN A CELL            */
         LA    @10,20                                              0666
         LH    @09,PBFSZ                                           0666
         ALR   @09,@10                                             0666
         CR    @09,@14                                             0666
         BNH   @RF00666                                            0666
*         PBFSZ=TVWACLSZ-HEADER;                                   0667
         SLR   @14,@10                                             0667
         STH   @14,PBFSZ                                           0667
*     END;                                                         0668
@RF00666 DS    0H                                                  0669
*   IF R15=0 THEN                                                  0669
@RF00660 SLR   @14,@14                                             0669
         CR    R15,@14                                             0669
         BNE   @RF00669                                            0669
*     DO;                                                          0670
*       QFLGMV=PFLGMV;              /* SET THE FLAGS IN THE QUEUE  0671
*                                      ELEMENT                       */
         MVC   QFLGMV(3,R1),PFLGMV                                 0671
*       QNBQE=PBFSZ+HEADER;         /* SET THE SIZE OF BUFFER        */
         LA    @10,20                                              0672
         LH    @09,PBFSZ                                           0672
         ALR   @09,@10                                             0672
         STH   @09,QNBQE(,R1)                                      0672
*       Q@STRT=HEADER;              /* SET START OFFSET PAST HEADER  */
         STH   @10,Q@STRT(,R1)                                     0673
*       Q@CURR=HEADER;              /* SET THE CURRENT OFFSET PAST 0674
*                                      HEADER                        */
         STH   @10,Q@CURR(,R1)                                     0674
*       QPREV=MTVWAEQ;              /* SET PREVIOUS PTR IN NEW QUEUE
*                                      ELE                           */
         L     @10,MTVWAEQ(,QPTR1)                                 0675
         ST    @10,QPREV(,R1)                                      0675
*       QNEXT=0;                    /* SET NEXT PTR IN NEW QUEUE   0676
*                                      ELEMENT                       */
         ST    @14,QNEXT(,R1)                                      0676
*       IF MTVWABQ=0 THEN           /* IF THERE IS NOTHING ON QUEUE  */
         C     @14,MTVWABQ(,QPTR1)                                 0677
         BNE   @RF00677                                            0677
*         DO;                                                      0678
*           MTVWANM=R1;             /* SET NEXT MESSAGE TO NEW     0679
*                                      ELEMENT                       */
         ST    R1,MTVWANM(,QPTR1)                                  0679
*           MTVWABQ=R1;             /* SET START OF QUEUE TO NEW   0680
*                                      ELEMENT                       */
         ST    R1,MTVWABQ(,QPTR1)                                  0680
*         END;                                                     0681
*       ELSE                        /* IF THERE IS ALREADY SOMETHING
*                                      ON THE QUEUE                  */
*         DO;                                                      0682
         B     @RC00677                                            0682
@RF00677 DS    0H                                                  0683
*           MTVWAEQ->QNEXT=R1;      /* CHAIN THIS MESSAGE ON END OF
*                                      QUEUE                         */
         L     @14,MTVWAEQ(,QPTR1)                                 0683
         ST    R1,QNEXT(,@14)                                      0683
*           IF MTVWANM=0 THEN       /* IF THERE IS NO NEXT MESSAGE   */
         L     @14,MTVWANM(,QPTR1)                                 0684
         LTR   @14,@14                                             0684
         BNZ   @RF00684                                            0684
*             MTVWANM=R1;           /* SET NEXT MESSAGE PTR TO NEW 0685
*                                      ELE                           */
         ST    R1,MTVWANM(,QPTR1)                                  0685
*         END;                                                     0686
@RF00684 DS    0H                                                  0687
*       MTVWAEQ=R1;                 /* SET END OF QUEUE PTR TO NEW 0687
*                                      ELE.                          */
@RC00677 ST    R1,MTVWAEQ(,QPTR1)                                  0687
*       PBUFADR=Q@CURR+R1;          /* PASS BUFFER @ BACK TO CALLER  */
         LR    @14,R1                                              0688
         AH    @14,Q@CURR(,R1)                                     0688
         ST    @14,PBUFADR                                         0688
*     END;                                                         0689
*   RFY                                                            0690
*    (R1,                                                          0690
*     R15) UNRSTD;                                                 0690
*   END BLDELE;                                                    0691
@EL00003 DS    0H                                                  0691
@EF00003 DS    0H                                                  0691
@ER00003 L     @14,@SA00003                                        0691
