*               OQ@STRT=OQ@CURR;                                   0301
         L     @10,TVWABOQ(,TVWAPTR)                               0301
         LH    @07,OQ@CURR(,@10)                                   0301
         STH   @07,OQ@STRT(,@10)                                   0301
*               PRC=1;              /* SET RETURN CODE TO MORE ON  0302
*                                      QUEUE                         */
         MVI   PRC,X'01'                                           0302
*             END;                                                 0303
*           GOTO CE0002@;           /* BRANCH TO END OF CASE         */
         B     CE0002@                                             0304
*         END DELETO;               /* END OF SUBCASE                */
         EJECT
