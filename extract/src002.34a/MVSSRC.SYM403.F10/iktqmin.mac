         TITLE 'IKTQMNGR: TSO/VTAM QUEUE MANAGER                       *
                        '
IKTQMIN  CSECT ,
MAINENT  DS    0H
         USING *,R15
         B     SAVEIT
         DC    AL2(EP0001-MAINENT)
         DC    AL1(16)
         DC    C'IKTQMNGR  76.026'
IKTQMOUT DS    0H
         USING *,R15
         B     SAVEIT
         DC    AL2(EP0002-IKTQMOUT)
         ENTRY IKTQMOUT
IKTQMFR1 DS    0H
         USING *,R15
         B     PROLOG
         DC    AL2(EP0003-IKTQMFR1)
         ENTRY IKTQMFR1
IKTQMEV  DS    0H
         STM   R0,R14,4(R1)
         BALR 15,0            ADJUST ADDRESS FOR ENTRY
         USING *,15
IKTQMEV1 B     PROLOG
         DC    AL2(EP0004-IKTQMEV1)
         ENTRY IKTQMEV
IKTQMFR2 DS    0H
         USING *,R15
         B     PROLOG
         DC    AL2(EP0005-IKTQMFR2)
         ENTRY IKTQMFR2
         DROP  R15
SAVEIT   STM   R14,R12,12(R13) SAVE CALLERS REGS
PROLOG   BALR  R12,0
PSTART   DS    0H
         USING PSTART,R12
         L     R10,ASCBPTR            SET UP ADDRESSABILITY TO
         L     R10,ASCBTSB(,R10)      THE TVWA
         L     R10,TSBEXTNT(,R10)
         L     TVWAPTR,TSBXTVWA(,R10)
         AH    R15,4(,R15)
         BR    R15
         EJECT
EP0001   DS    0H
*   DATARG=TVWAQMWA;                /* PUT @ OF WORKAREA IN THE    0059
*                                      DATAREG                       */
         L     DATARG,TVWAQMWA(,TVWAPTR)                           0059
*   GEN REFS(DATARG) SETS(R13);                                    0060
        USING @DATD,DATARG       ESTABLISH ADDRESSABILITY TO WORK AREA
        MVC   @PC00001(4),0(@01) SAVE POINTER TO PARAMETER LIST
        ST    @13,@SA00001+4     SAVE ADDRESS OF CALLERS SAVE AREA
        LA    @13,@SA00001       LOAD ADDRESS OF MY SAVE AREA
*   PARMS=IPARMS;                   /* MAKE A LOCAL COPY OF PARM LIST*/
         L     @10,@PC00001                                        0061
         MVC   PARMS(20),IPARMS(@10)                               0061
*   IF PREQ>0&PREQ<=INFUNC THEN     /* IS THIS A PROPER REQUEST CODE */
         CLI   PREQ,0                                              0062
         BNH   @RF00062                                            0062
         CLI   PREQ,7                                              0062
         BH    @RF00062                                            0062
*     DO;                                                          0063
*       RFY                                                        0064
*         R0 RSTD;                                                 0064
*       R0=ADDR(IKTQMFR1);                                         0065
         L     R0,@CA02138                                         0065
*       GEN REFS(R0,PSACSTK,PSA,FRRSCURR,FRRS,FRRSLAST,PSALSFCC,   0066
*           FRRSELEN,FRRSFRRA,FRRSENTR) SETS(R2,R14,R15);          0066
        MODESET EXTKEY=ZERO,SAVEKEY=(2),WORKREG=2    SAVE CALLERS KEY
        SETFRR  A,FRRAD=(R0),WRKREGS=(R14,R15)
        MODESET KEYADDR=(2)                     RETURN TO CALLERS KEY
*       RFY                                                        0067
*         R0 UNRSTD;                                               0067
*       TVWAQMIO='0'B;              /* INPUT SERVICES BEING USED     */
         NI    TVWAQMIO(TVWAPTR),B'11110111'                       0068
*/********************************************************************/
*/*          CASE MACRO USED TO BRANCH TO PROPER SERVICE ROUTINE     */
*/********************************************************************/
*                                                                  0069
*BRAN:                                                             0069
*       DO;                         /* BRAN:CASE(PREQ)NAMES(ADDEL,OBT
*                                      AIN,DELIN,UPDATE,CLEARI,DELBK,
*                                      MARKAVBL)                     */
BRAN     DS    0H                                                  0070
*         DCL                                                      0070
*           CL0001@ LABEL BASED(CP0001@(PREQ)) VALUERANGE(ADDEL,OBTAIN,
*               DELIN,UPDATE,CLEARI,DELBK,MARKAVBL);/* BRANCH TARGET */
*         DCL                                                      0071
*           CP0001@(7) PTR(31) INIT(ADDR(ADDEL),ADDR(OBTAIN),ADDR(DELIN
*               ),ADDR(UPDATE),ADDR(CLEARI),ADDR(DELBK),ADDR(MARKAVBL))
*             ;                                                    0071
*         GOTO CL0001@;             /* BRANCH TO SELECTED CASE       */
         SLR   @10,@10                                             0072
         IC    @10,PREQ                                            0072
         SLA   @10,2                                               0072
         L     @10,CP0001@-4(@10)                                  0072
         BR    @10                                                 0072
         EJECT
*                                                                  0073
*/********************************************************************/
*/*                                                                  */
*/* INPUT TO ADDEL:                                                  */
*/*      REQUEST CODE = 1                                            */
*/*      FLAGS        = THE FLAGS THE CALLER WANTS SET               */
*/*      BUFFER SIZE  = THE MESSAGE LENGTH                           */
*/* OUTPUT FROM ADDEL:                                               */
*/*      RETURN CODE  = 1 => SUCCESSFUL                              */
*/*                   = 2 => UNSUCCESSFUL, NO MORE ROOM FOR ELEMENTS */
*/*      BUFFER ADDRESS= ADDRESS AT WHICH CALLER SHOULD PLACE THE    */
*/*                      MESSAGE                                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0073
*ADDEL:                                                            0073
*         DO;                       /* SUBCASE ADD AN ELEMENT TO THE
*                                      INPUT QUEUE                   */
ADDEL    DS    0H                                                  0074
*           RFY                                                    0074
*             IQELE BASED(TVWAEIQ); /* USE TVWAEIQ AS QUEUE PTR      */
*           QPTR1=ADDR(TVWABIQ);    /* PASS @ OF INPUT PTRS. TO    0075
*                                      BLDELE                        */
         LA    QPTR1,TVWABIQ(,TVWAPTR)                             0075
*           CALL BLDELE;            /* CALL BLDELE TO BUILD AND CHAIN
*                                      ELE                           */
         BAL   @14,BLDELE                                          0076
*           RFY                                                    0077
*             R15 RSTD;                                            0077
*           IF R15=0 THEN           /* IF BLDELE WAS SUCCESSFUL      */
         LTR   R15,R15                                             0078
         BNZ   @RF00078                                            0078
*             DO;                                                  0079
*               IF IQAVBL='1'B THEN /* IF THIS LAST ELEMENT IS NOW 0080
*                                      MARKED AVAILABLE              */
         L     @14,TVWAEIQ(,TVWAPTR)                               0080
         TM    IQAVBL(@14),B'10000000'                             0080
         BNO   @RF00080                                            0080
*                 DO;               /* MARK PREVIOUS ELEMENTS      0081
*                                      AVAILABLE IF THEY ARE       0081
*                                      UNAVAILABLE                   */
*                   TEMP1=IQPREV;   /* SET TEMPORARY PTR TO PREVIOUS
*                                      ELE.                          */
         L     TEMP1,IQPREV(,@14)                                  0082
*                   DO WHILE(TEMP1^=0&/* DO WHILE MORE IS ON QUEUE 0083
*                                      AND                           */
*                         TEMP1->IQAVBL='0'B);/* ELEMENTS ARE MARKED
*                                      UNAVAILABLE                   */
         B     @DE00083                                            0083
@DL00083 DS    0H                                                  0084
*                     TEMP1->IQAVBL='1'B;/* MARK THIS MESSAGE      0084
*                                      AVAILABLE                     */
         OI    IQAVBL(TEMP1),B'10000000'                           0084
*                     TEMP1=TEMP1->IQPREV;/* SET TEMPORARY TO      0085
*                                      PREVIOUS ELEMENT              */
         L     TEMP1,IQPREV(,TEMP1)                                0085
*                   END;                                           0086
@DE00083 LTR   TEMP1,TEMP1                                         0086
         BZ    @DC00083                                            0086
         TM    IQAVBL(TEMP1),B'10000000'                           0086
         BZ    @DL00083                                            0086
@DC00083 DS    0H                                                  0087
*                 END;                                             0087
*               PRC=1;              /* SET RC TO 'SUCCESSFUL ADD'    */
@RF00080 MVI   PRC,X'01'                                           0088
*             END;                                                 0089
*           ELSE                    /* IF BLDELE NOT SUCCESSFUL      */
*             PRC=2;                /* SET RC TO 'OUT OF STORAGE'    */
         B     @RC00078                                            0090
@RF00078 MVI   PRC,X'02'                                           0090
*           RFY                                                    0091
*             R15 UNRSTD;                                          0091
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */
         B     CE0001@                                             0092
*         END ADDEL;                /* END OF SUBCASE                */
         EJECT
*                                                                  0094
*/********************************************************************/
*/*                                                                  */
*/* INPUT TO OBTAIN:                                                 */
*/*      REQUEST CODE = 2                                            */
*/* OUTPUT FROM OBTAIN:                                              */
*/*      RETURN CODE  = 1 => SUCCESSFUL, MORE MESSAGES ON QUEUE      */
*/*                   = 2 => SUCCESSFUL, NO MORE MESSAGES ON QUEUE   */
*/*                   = 3 => UNSUCCESSFUL, NOTHING ON QUEUE          */
*/*      BUFFER ADDRESS= ADDRESS OF FIRST BYTE OF MESSAGE            */
*/*      BUFFER SIZE  = SIZE (IN BYTES) OF MESSAGE                   */
*/*      FLAGS        = THE QUEUE ELEMENT FLAGS                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0094
*OBTAIN:                                                           0094
*         DO;                       /* SUBCASE GET THE NEXT MESSAGE
*                                      AND THE FOLLOWING MESSAGE SIZE
*                                      AND FLAGS                     */
OBTAIN   DS    0H                                                  0095
*           RFY                                                    0095
*             IQELE BASED(TVWANIM); /* USE TVWANIM AS QUEUE ELEMENT
*                                      PTR                           */
*           IF(TVWANIM=0×           /* IF THERE IS NOTHING TO OBTAIN
*                                      OR                            */
*               IQAVBL='0'B) THEN   /* IF THERE IS NOTHING AVAILABLE */
         L     @10,TVWANIM(,TVWAPTR)                               0096
         LTR   @10,@10                                             0096
         BZ    @RT00096                                            0096
         TM    IQAVBL(@10),B'10000000'                             0096
         BNZ   @RF00096                                            0096
@RT00096 DS    0H                                                  0097
*             PRC=3;                /* SET RC TO 'NOTHING TO BE    0097
*                                      OBTAINED'                     */
         MVI   PRC,X'03'                                           0097
*           ELSE                    /* IF THERE IS SOMETHING       0098
*                                      AVAILABLE                     */
*             DO;                                                  0098
         B     @RC00096                                            0098
@RF00096 DS    0H                                                  0099
*               PBUFADR=IQ@CURR+TVWANIM;/* SET BUFFER ADDRESS        */
         L     @10,TVWANIM(,TVWAPTR)                               0099
         LH    @07,IQ@CURR(,@10)                                   0099
         LR    @15,@07                                             0099
         ALR   @15,@10                                             0099
         ST    @15,PBUFADR                                         0099
*               PBFSZ=IQNBQE-IQ@CURR;/* SET BUFFER SIZE              */
         LCR   @07,@07                                             0100
         LH    @15,IQNBQE(,@10)                                    0100
         N     @15,@CF02260                                        0100
         ALR   @07,@15                                             0100
         STH   @07,PBFSZ                                           0100
*               PFLAGS=IQFLAGS;     /* SET BUFFER FLAGS              */
         MVC   PFLAGS(4),IQFLAGS(@10)                              0101
*               TVWANIM=IQNEXT;     /* SET POINTER TO NEXT MESSAGE   */
         L     @10,IQNEXT(,@10)                                    0102
         ST    @10,TVWANIM(,TVWAPTR)                               0102
*               IF(TVWANIM=0×IQAVBL='0'B) THEN/* IF ANYTHING LEFT FOR
*                                      OBTAINING                     */
         LTR   @10,@10                                             0103
         BZ    @RT00103                                            0103
         TM    IQAVBL(@10),B'10000000'                             0103
         BNZ   @RF00103                                            0103
@RT00103 DS    0H                                                  0104
*                 PRC=2;            /* SET RC TO 'THIS IS LAST     0104
*                                      ELEMENT'                      */
         MVI   PRC,X'02'                                           0104
*               ELSE                /* IF THERE IS SOMETHING LEFT    */
*                 DO;                                              0105
         B     @RC00103                                            0105
@RF00103 DS    0H                                                  0106
*                   PRC=1;          /* SET RC TO 'THERE IS MORE ON 0106
*                                      QUEUE'                        */
         MVI   PRC,X'01'                                           0106
*                   PNXBFSZ=IQNBQE-IQ@CURR;/* SET NEXT BUFFER SIZE   */
         L     @10,TVWANIM(,TVWAPTR)                               0107
         LH    @07,IQNBQE(,@10)                                    0107
         N     @07,@CF02260                                        0107
         SH    @07,IQ@CURR(,@10)                                   0107
         STH   @07,PNXBFSZ                                         0107
*                   PNXFLGS=IQFLAGS;/* SET NEXT FLAGS                */
         MVC   PNXFLGS(4),IQFLAGS(@10)                             0108
*                 END;                                             0109
*             END;                                                 0110
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */
         B     CE0001@                                             0111
*         END OBTAIN;               /* END OF SUBCASE                */
         EJECT
*                                                                  0113
*/********************************************************************/
*/*                                                                  */
*/* INPUT TO DELIN:                                                  */
*/*      REQUEST CODE = 3                                            */
*/* OUTPUT FROM DELIN:                                               */
*/*      RETURN CODE  = 1 => SUCCESSFUL DELETE, MORE MESSAGES ON     */
*/*                          QUEUE TO BE OBTAINED                    */
*/*                   = 2 => SUCCESSFUL DELETE, NO MORE MESSAGES ON  */
*/*                          QUEUE                                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0113
*DELIN:                                                            0113
*         DO;                       /* SUBCASE REMOVE ALL PREVIOUSLY
*                                      OBTAIN'D MESSAGES FROM THE  0113
*                                      QUEUE                         */
DELIN    DS    0H                                                  0114
*           RFY                                                    0114
*             R1 RSTD;                                             0114
*           QPTR2=ADDR(TVWABIQ);    /* PASS @ OF INPUT PTRS TO DELEL */
         LA    QPTR2,TVWABIQ(,TVWAPTR)                             0115
*           DO WHILE TVWABIQ^=TVWANIM;/* WHILE THERE ARE ELEMENTS TO
*                                      BE DELETED                    */
         B     @DE00116                                            0116
@DL00116 DS    0H                                                  0117
*             R1=TVWABIQ;           /* PUT ADDRESS OF FIRST IN REG 1 */
         L     R1,TVWABIQ(,TVWAPTR)                                0117
*/* ADDRESS OF ELEMENT TO BE DELETED IS PASSED TO DELEL IN REG 1     */
*                                                                  0118
*             CALL DELEL;           /* TO UNCHAIN THIS QUEUE ELEMENT
*                                      AND FREE ASSOCIATED STORAGE   */
         BAL   @14,DELEL                                           0118
*           END;                                                   0119
@DE00116 L     @10,TVWABIQ(,TVWAPTR)                               0119
         C     @10,TVWANIM(,TVWAPTR)                               0119
         BNE   @DL00116                                            0119
*           RFY                                                    0120
*             R1 UNRSTD;                                           0120
*           IF TVWABIQ=0 THEN       /* IF THERE IS NOTHING LEFT ON 0121
*                                      QUEUE                         */
         LTR   @10,@10                                             0121
         BNZ   @RF00121                                            0121
*             PRC=2;                /* SET RETURN CODE TO 2          */
         MVI   PRC,X'02'                                           0122
*           ELSE                                                   0123
*             DO;                                                  0123
         B     @RC00121                                            0123
@RF00121 DS    0H                                                  0124
*               PRC=1;              /* SET RETURN CODE TO 1          */
         MVI   PRC,X'01'                                           0124
*               TVWABIQ->IQ@STRT=TVWABIQ->IQ@CURR;/* DELETE A PORTION
*                                      OF A PREVIOUSLY OBTAINED QUEUE
*                                      ELEMENT                       */
         L     @10,TVWABIQ(,TVWAPTR)                               0125
         LH    @07,IQ@CURR(,@10)                                   0125
         STH   @07,IQ@STRT(,@10)                                   0125
*             END;                                                 0126
*           TSBPTR=ASCBTSB;         /* GET ADDRESS OF TSB            */
@RC00121 L     @10,ASCBPTR                                         0127
         L     TSBPTR,ASCBTSB(,@10)                                0127
*           RFY                                                    0128
*             TSB BASED(TSBPTR);                                   0128
*                                                                  0128
*           /*********************************************************/
*           /*                                                       */
*           /* TSBNOBUF INDICATES TASKS ARE WAITING IN THIS ADDRESS  */
*           /* SPACE TSBTJBF INDICATES TASKS ARE WAITING IN ANOTHER  */
*           /* ADDRESS SPACE                                         */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0129
*           IF(TSBNOBUF='1'B×TSBTJBF='1'B)&TVWAUSMN<TVWALBUF THEN/*
*                                      IF TASKS ARE WAITING AND    0129
*                                      STORAGE USAGE IS BELOW LOW  0129
*                                      THRESHOLD                     */
         TM    TSBNOBUF(TSBPTR),B'00010000'                        0129
         BO    @GL00005                                            0129
         TM    TSBTJBF(TSBPTR),B'00000001'                         0129
         BNO   @RF00129                                            0129
@GL00005 L     @10,TVWAUSMN(,TVWAPTR)                              0129
         C     @10,TVWALBUF(,TVWAPTR)                              0129
         BNL   @RF00129                                            0129
*             CALL MARKDISP;        /* TO DISPATCH TASKS SUSPENDED 0130
*                                      FOR BUFFER OVERUSE            */
         BAL   @14,MARKDISP                                        0130
*           RFY                                                    0131
*             TSB BASED(ASCBTSB);                                  0131
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */
         B     CE0001@                                             0132
*         END DELIN;                /* END OF SUBCASE                */
         EJECT
*                                                                  0134
*/********************************************************************/
*/*                                                                  */
*/* INPUT TO CLEARI:                                                 */
*/*      REQUEST CODE = 5                                            */
*/* OUTPUT FROM CLEARI:                                              */
*/*      NONE                                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0134
*CLEARI:                                                           0134
*         DO;                       /* SUBCASE REMOVE ALL MESSAGES 0134
*                                      FROM INPUT QUE                */
CLEARI   DS    0H                                                  0135
*           RFY                                                    0135
*             IQELE BASED(TEMP2);                                  0135
*           TEMP2=TVWAEIQ;          /* SET TEMPORARY TO END OF QUEUE */
         L     TEMP2,TVWAEIQ(,TVWAPTR)                             0136
*           COUNT1=0;               /* SET A TEMPORARY COUNTER TO 0  */
         SLR   COUNT1,COUNT1                                       0137
*           DO WHILE TEMP2^=0;      /* WHILE THERE ARE ELEMENTS TO 0138
*                                      CLEAR                         */
         B     @DE00138                                            0138
@DL00138 DS    0H                                                  0139
*             IF IQPRMPT='1'B THEN  /* IF THIS ELEMENT WAS PROMPTED
*                                      FOR                           */
         TM    IQPRMPT(TEMP2),B'01000000'                          0139
         BNO   @RF00139                                            0139
*               COUNT1=COUNT1+1;    /* INCREMENT COUNT OF ELEMENTS 0140
*                                      PROMPTED FOR                  */
         AL    COUNT1,@CF00047                                     0140
*             RFY                                                  0141
*              (R0,                                                0141
*               R1) RSTD;                                          0141
@RF00139 DS    0H                                                  0142
*             R1=TEMP2;             /* REMEMBER ADDRESS OF ELEMENT   */
         LR    R1,TEMP2                                            0142
*             TEMP2=IQPREV;         /* SET TEMPORARY TO PREVIOUS     */
         L     TEMP2,IQPREV(,TEMP2)                                0143
*             IF R1->IQALLOC='0'B THEN/* IF ELEMENT WAS GETCELL'D    */
         TM    IQALLOC(R1),B'10000000'                             0144
         BNZ   @RF00144                                            0144
*               IF TVWAQLBU='1'B THEN/* IF LOOK ASIDE BUFFER IS IN 0145
*                                      USE                           */
         TM    TVWAQLBU(TVWAPTR),B'00000100'                       0145
         BNO   @RF00145                                            0145
*                 DO;                                              0146
*                   TVWAQMLB=R1;    /* SET LOOK ASIDE BUFFER ADDRESS
*                                      TO ADDRESS OF ELEMENT TO BE 0147
*                                      DELETED                       */
         ST    R1,TVWAQMLB(,TVWAPTR)                               0147
*                   TVWAQLBU='0'B;  /* SET LOOK ASIDE BUFFER FLAG TO
*                                      'NOT IN USE'                  */
         NI    TVWAQLBU(TVWAPTR),B'11111011'                       0148
*                 END;                                             0149
*               ELSE                                               0150
*                 DO;                                              0150
         B     @RC00145                                            0150
@RF00145 DS    0H                                                  0151
*                   R0=TVWACPID;    /* PUT CELL POOL ID IN REG 0     */
         L     R0,TVWACPID(,TVWAPTR)                               0151
*                   RFY                                            0152
*                     R15 RSTD;                                    0152
*                   GEN REFS(R0,R1,CVTFRECL,CVTMAP) SETS(R14,R15); 0153
        FREECELL CPID=(0),CELL=(1),BRANCH=YES
*                   RFY                                            0154
*                    (R4,                                          0154
*                     R7) RSTD;                                    0154
*                   IF R15=AUTODEL THEN/* IF AN EXTENT IS NOW EMPTY  */
         C     R15,@CF00137                                        0155
         BNE   @RF00155                                            0155
*                     DO;                                          0156
*                       R0=4096;    /* SIZE OF AREA TO FREEMAIN      */
         L     R0,@CF02165                                         0157
*                       R4=TVWATCB; /* PUT ADDRESS OF TCB IN REG 4   */
         L     R4,TVWATCB(,TVWAPTR)                                0158
*                       R7=PSAAOLD; /* PUT ADDRESS OF ASCB IN REG 7  */
         L     R7,PSAAOLD                                          0159
*                       GEN REFS(R0,R1,CVTRMBR,CVTMAP,CVTPTR) SETS(R2,
*                           R3,R14,R15);                           0160
        MODESET EXTKEY=ZERO,SAVEKEY=(2),WORKREG=2    SAVE CALLERS KEY
        FREEMAIN RU,LV=(0),A=(1),BRANCH=YES,KEY=TCASKEY,SP=SP229
        MODESET KEYADDR=(2)                     RETURN TO CALLERS KEY
*                     END;                                         0161
*                   RFY                                            0162
*                     R15 UNRSTD;                                  0162
@RF00155 DS    0H                                                  0163
*                 END;                                             0163
*             ELSE                  /* IF THIS ELEMENT WAS GETMAIN'D */
*               DO;                                                0164
         B     @RC00144                                            0164
@RF00144 DS    0H                                                  0165
*                 R4=TVWATCB;       /* PUT TCB @ IN REG 4 FOR      0165
*                                      FREEMAIN                      */
         L     R4,TVWATCB(,TVWAPTR)                                0165
*                 R7=PSAAOLD;       /* PUT ASCB@ IN REG 7 FOR      0166
*                                      FREEMAIN                      */
         L     R7,PSAAOLD                                          0166
*                 R0=R1->IQNBUF;    /* PUT SIZE OF FREEMAIN IN REG 0 */
         LH    R0,IQNBUF(,R1)                                      0167
*                 GEN REFS(R0,R1,CVTRMBR,CVTMAP,CVTPTR) SETS(R2,R3,R14,
*                     R15);                                        0168
        MODESET EXTKEY=ZERO,SAVEKEY=(2),WORKREG=2    SAVE CALLERS KEY
        FREEMAIN RU,LV=(0),A=(1),BRANCH=YES,KEY=TCASKEY,SP=SP229
        MODESET KEYADDR=(2)                     RETURN TO CALLERS KEY
*               END;                                               0169
*             RFY                                                  0170
*              (R0,                                                0170
*               R1,                                                0170
*               R4,                                                0170
*               R7) UNRSTD;                                        0170
@RC00144 DS    0H                                                  0171
*           END;                                                   0171
@DE00138 SLR   @10,@10                                             0171
         CR    TEMP2,@10                                           0171
         BNE   @DL00138                                            0171
*           TVWABIQ=0;              /* SET BEGINNING OF QUEUE TO 0   */
         ST    @10,TVWABIQ(,TVWAPTR)                               0172
*           TVWANIM=0;              /* SET NEXT MESSAGE PTR TO 0     */
         ST    @10,TVWANIM(,TVWAPTR)                               0173
*           TVWAEIQ=0;              /* SET END OF QUEUE TO 0         */
         ST    @10,TVWAEIQ(,TVWAPTR)                               0174
*           TSBAUTOS=TSBAUTOS-COUNT1*TSBAUTOI;/* RESET LINE PROMPTING
*                                      BY COUNTER TIMES INCREMENT    */
         L     @10,ASCBPTR                                         0175
         L     @10,ASCBTSB(,@10)                                   0175
         LR    @01,COUNT1                                          0175
         M     @00,TSBAUTOI(,@10)                                  0175
         LCR   @01,@01                                             0175
         AL    @01,TSBAUTOS(,@10)                                  0175
         ST    @01,TSBAUTOS(,@10)                                  0175
*           GOTO CE0001@;           /* BRANCH TO END OF CASE         */
         B     CE0001@                                             0176
*         END CLEARI;               /* END OF SUBCASE                */
         EJECT
*                                                                  0178
*/********************************************************************/
*/*                                                                  */
*/* INPUT TO UPDATE:                                                 */
*/*      REQUEST CODE = 4                                            */
*/*      BUFFER ADDRESS= ADDRESS OF FIRST UNUSED BYTE OF MESSAGE     */
*/* OUTPUT FROM UPDATE:                                              */
*/*      RETURN CODE  = 1 => ELEMENT UPDATED                         */
*/*                   = 2 => UNSUCCESSFUL, NOTHING ON QUEUE TO UPDATE*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0178
*UPDATE:                                                           0178
*         DO;                       /* SUBCASE UPDATE THE PREVIOUSLY
*                                      OBTAINED MESSAGE BECAUSE A  0178
*                                      PARTIAL MESSAGE WAS USED      */
UPDATE   DS    0H                                                  0179
*           RFY                                                    0179
*             IQELE BASED(TVWANIM);                                0179
