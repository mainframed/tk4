         ST    @14,TVWAEOQ(,TVWAPTR)                               0500
*         END;                                                     0501
*       RFY                                                        0502
*         R15 UNRSTD;                                              0502
*                                                                  0502
@RF00481 DS    0H                                                  0503
*       /*************************************************************/
*       /*                                                           */
*       /* LOG TVWA AND SDWA                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0503
*       SDWAFRM1=ADDR(TVWA);        /* SET BEGINNING AND ENDING    0503
*                                      ADDRESS                       */
         ST    TVWAPTR,SDWAFRM1(,SDWAPTR)                          0503
*       SDWATO1=ADDR(TVWA)+LENGTH(TVWA);/* OF TVWA FOR RECORDING     */
*                                                                  0504
         LA    @07,216                                             0504
         ALR   @07,TVWAPTR                                         0504
         ST    @07,SDWATO1(,SDWAPTR)                               0504
*       /*************************************************************/
