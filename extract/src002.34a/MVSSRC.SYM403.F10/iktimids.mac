         TITLE '*** IKTIMIDS: INPUT MANAGER FOR THE 3270/SNA  TERMINALS*
               S ***    '
IKTIMIDS CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKTIMIDS  77.343'                                 0001
         DROP  @15
@PROLOG  BALR  @05,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@05                                         0001
         LR    WRKSPC,REG1         LOAD ADDR OF GETMAINED AREA
         USING @DATD,WRKSPC        SET UP ADDRESSABILITY
         ST    @13,TIMGETMN        SET UP ADDRESSABILITY
*TVWAPTR=TSBXTVWA;                     /* ADDRESSING TO TVWA         */
         L     @12,PSAAOLD                                         0134
         L     @12,ASCBTSB(,@12)                                   0134
         L     @12,TSBEXTNT(,@12)                                  0134
         L     @12,TSBXTVWA(,@12)                                  0134
         ST    @12,TVWAPTR                                         0134
*SRBRTN=REG14;                         /* SAVE RETURN ADDRESS        */
         L     @12,TIMGETMN                                        0135
         ST    REG14,SRBRTN(,@12)                                  0135
*STM(REG0,REG15,SAVEREGS);             /* FREEMAIN RECOVERY POINT    */
         STM   REG0,REG15,SAVEREGS(@12)                            0136
*RFY (REG1,REG14) UNRSTD;              /* FREE REGISTERS 1 AND 14    */
*/********************************************************************/
*/* THE FOLLOWING CODE WILL BE EXECUTED UPON INITIAL ENTRY TO        */
*/* IKTIMIDS.  THE SRB WILL BE FREED AND THE RPL FOR THE VTAM        */
*/* RECEIVE MACRO WILL BE INITIALIZED.                               */
*/********************************************************************/
* DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/        0138
*CKMC0002:                                                         0139
*   STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */    0139
CKMC0002 L     @14,TVWAPTR                                         0139
         L     @14,TVWAFRWI(,@14)                                  0139
         STM   REG0,REG15,FRWACKRG(@14)                            0139
*   FRWACKPT(2)=ADDR(CKMC0002); /* SAVE ADDRESS    @ZM20856 */     0140
         LA    @12,CKMC0002                                        0140
         ST    @12,FRWACKPT+4(,@14)                                0140
* END;                  /* CHECKPOINT         @ZM20856*/           0141
*REG7=ADDR(IKTIOFRR);                  /* FRR ADDRESS        @G58AK3B*/
         L     @14,CVTPTR                                          0142
         L     @01,CVTTCASP(,@14)                                  0142
         L     REG7,TCASFRR(,@01)                                  0142
*GEN   REFS(FRRSCURR,FRRS,FRRSLAST,PSALSFCC,PSA,FRRSELEN,PSACSTK,  0143
*          FRRSFRRA,FRRSENTR,FRRSPARM,REG7)                        0143
*          SETS(REG1,REG2,REG3,FRRSCURR);                          0143
   SETFRR A,FRRAD=(REG7),WRKREGS=(REG1,REG2),PARMAD=(REG3)
*PARMPTR = REG3;                       /* ADDRESS OF PARM AREA       */
         ST    REG3,PARMPTR                                        0144
*                                      /* DELETE OLD FRR CODE@ZM20856*/
*RFY (REG3,REG7) UNRSTD;               /* FREE REGS FOR COMPILER     */
*CALL INITFRR;                         /* CALL FRR INITIALIZE      0146
*                                         ROUTINE            @ZM20856*/
         BAL   @14,INITFRR                                         0146
*/********************************************************************/
*/* OBTAIN THE LOCAL LOCK AND FREE THE ADDRESS OF THE TIM SRB        */
*/* IN THE TSB EXTENSION.                                            */
*/********************************************************************/
*REG2=REG0;                            /* SAVE ADDRESS OF SRB TO FREE*/
         LR    REG2,REG0                                           0147
*GEN(SETLOCK OBTAIN,TYPE=LOCAL,RELATED=(SRB),MODE=UNCOND,REGS=USE) 0148
*REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);                    0148
         SETLOCK OBTAIN,TYPE=LOCAL,RELATED=(SRB),MODE=UNCOND,REGS=USE
*TSBXSRBI=0;                           /* CLEAR ADDRESS OF SRB       */
         L     @14,PSAAOLD                                         0149
         L     @01,ASCBTSB(,@14)                                   0149
         L     @01,TSBEXTNT(,@01)                                  0149
         SLR   @14,@14                                             0149
         ST    @14,TSBXSRBI(,@01)                                  0149
*GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SRB),REGS=USE)            0150
*REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);                    0150
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SRB),REGS=USE
*/********************************************************************/
*/* FREE SRB                                                         */
*/********************************************************************/
*RFY (REG7,REG8,REG4) RSTD;            /* USED TO HOLD WORK ADDRESS  */
*REG8=REG4;                            /* REG4 DESTROYED BY FREEMAIN */
         LR    REG8,REG4                                           0152
*REG7=SRBLEN;                          /* LENGTH OF SRB              */
         L     REG7,SRBLEN                                         0153
*GEN REFS(REG2,REG7,CVTSPSA,CVTMAP,WSAGGMFM,WSAG,CVTGLMN,PSALITA,FLC)
*    SETS(REG0,REG1,REG14,REG15,REG4,REG3);                        0154
A1 SETLOCK  OBTAIN,TYPE=SALLOC,MODE=UNCOND,RELATED=(A2),REGS=USE
   FREEMAIN RC,LV=(REG7),A=(REG2),SP=239,BRANCH=(YES,GLOBAL)
A2 SETLOCK  RELEASE,TYPE=SALLOC,RELATED=(A1),REGS=USE
*REG4=REG8;                            /* RESTORE WORK AREA ADDR     */
         LR    REG4,REG8                                           0155
*STM(REG0,REG15,SAVEREGS);             /* SAVE REGS FOR RESTART      */
         L     @14,TIMGETMN                                        0156
         STM   REG0,REG15,SAVEREGS(@14)                            0156
*RFY (REG0,REG1,REG2,REG4,REG8,REG7) UNRSTD;/* RELEASE REGISTERS     */
*RESTARTA:                             /* CALL RTN TO SET UP RPL     */
*GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RPL),REGS=USE) 0158
*    REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);        /*@ZM20546*/
RESTARTA SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RPL),REGS=USE
*CALL PRIMPROC;                        /* CALL RTN TO SET UP RPL     */
         BAL   @14,PRIMPROC                                        0159
*GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RPL),REGS=USE)            0160
*    REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);        /*@ZM20546*/
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RPL),REGS=USE
         EJECT
*/********************************************************************/
*/* THE FOLLOWING CODE CONTAINS THE INFINITE LOOP THAT PERFORMS      */
*/* THE BASIC FUNCTION OF IKTIMIDS. THAT FUNCTION IS TO ISSUE        */
*/* THE VTAM RECEIVE MACRO, DETERMINE WHAT THE INPUT IS WHEN         */
*/* THE RECEIVE IS SATISFIED, CALL THE APPROPRIATE ROUTINE           */
*/* TO PROCESS THE INPUT AND LOOP BACK TO REISSUE THE RECEIVE.       */
*/* EACH TIME VTAM GAINS CONTROL THROUGH THE RECEIVE MACRO           */
*/* THE FRR THAT IKTTIMRT SETS UP IS DELETED AND MUST BE RESET.      */
*/********************************************************************/
*REC: RFY (REG0,REG1,REG2,REG3,REG7) RSTD;      /*RESTRICT REGISTERS */
REC      DS    0H                                                  0162
*                                               /*           @ZM20858*/
* DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/        0162
*CKMC0003:                                                         0163
*   STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */    0163
CKMC0003 L     @14,TVWAPTR                                         0163
         L     @14,TVWAFRWI(,@14)                                  0163
         STM   REG0,REG15,FRWACKRG(@14)                            0163
*   FRWACKPT(2)=ADDR(CKMC0003); /* SAVE ADDRESS    @ZM20856 */     0164
         LA    @12,CKMC0003                                        0164
         ST    @12,FRWACKPT+4(,@14)                                0164
* END;                  /* CHECK POINT        @ZM20856*/           0165
*RPLRRESP='1'B;                        /*                    @ZA24528*/
*RPLDFASY='1'B;                        /*                    @ZA24528*/
*RPLNFSYN='0'B;                        /*                    @ZA24528*/
         L     @14,TIMGETMN                                        0168
         LA    @12,2348                                            0168
         LR    @06,@14                                             0168
         ALR   @06,@12                                             0168
         OI    RPLRRESP+16(@06),B'00001010'                        0168
         NI    RPLNFSYN+16(@06),B'11111011'                        0168
*REG2=ADDR(IFGRPL);                    /* ADDR OF RPL FOR RECEIVE    */
         ALR   @14,@12                                             0169
         LR    REG2,@14                                            0169
*REG7=ADDR(IKTIOFRR);                  /* PTR TO FRR ENTRY   @G58AK3B*/
         L     @14,CVTPTR                                          0170
         L     @14,CVTTCASP(,@14)                                  0170
         L     REG7,TCASFRR(,@14)                                  0170
*GEN REFS(REG1,REG2,REG3,REG7,FRRSCURR,FRRS,FRRSLAST,PSALSFCC,PSA, 0171
*         FRRSELEN,PSACSTK,FRRSFRRA,FRRSENTR,FRRSPARM,PSALITA,FLC) 0171
*    SETS(REG1,REG2,REG3,REG14,REG15,REG0); /*RTN CDS IN 15,0 ZM20858*/
  RECEIVE  RPL=(REG2)
  SETFRR A,FRRAD=(REG7),WRKREGS=(REG1,REG2),PARMAD=(REG3)
*PARMPTR = REG3;                       /* POINTER TO FRR PARAMETERS  */
         ST    REG3,PARMPTR                                        0172
*RFY (REG1,REG2,REG3,REG7) UNRSTD;     /* FREE REGISTERS     @ZM20856*/
*CALL INITFRR;                         /* CALL FRR INITIALIZE      0174
*                                         ROUTINE            @ZM20856*/
         BAL   @14,INITFRR                                         0174
*/********************************************************************/
*/* EXAMINE INPUT FROM RECEIVE                                       */
*/********************************************************************/
*GEN(STM REG14,REG1,12(REG13))                                     0175
*    REFS(REG13);                      /* SAVE RECEIVE RETURN CODE 0175
*                                         REGISTERS 15,0     @ZM20858*/
         STM REG14,REG1,12(REG13)
*GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RECV),REGS=USE)
*    REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);        /*@ZM20546*/
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RECV),REGS=USE
*GEN(LM REG14,REG1,12(REG13))                                      0177
*    REFS(REG13);                      /* RESTORE RETURN CODE REGS 0177
*                                         15,0               @ZM20858*/
         LM REG14,REG1,12(REG13)
*IF TVWATOD='1'B THEN                  /* IS IKTOMIDS DEAD?  @ZM20856*/
         L     @14,TVWAPTR                                         0178
         TM    TVWATOD(@14),B'10000000'                            0178
         BNO   @RF00178                                            0178
*  DO;                                 /*                    @ZM20856*/
*      RFY REG1 RSTD;                  /* RESTRICT REG 1     @ZM20856*/
*      REG1 = ADDR(XITPARMS);          /* PTR TO PARAMETERS  @ZM20856*/
         L     @14,TIMGETMN                                        0181
         LA    REG1,XITPARMS(,@14)                                 0181
*      WRKAREAL = TIMWAL;              /* WORK AREA LENGTH   @ZM20856*/
         MVC   WRKAREAL(4,@14),@CF02479                            0182
*      LIOWA = TOTALL;                 /* I-O WORK LENGTH    @ZM20856*/
         MVC   LIOWA(4,@14),@CF02481                               0183
*      CALL IKTEXIT;                   /* EXIT PERMANENTLY   @ZM20856*/
         L     @12,CVTPTR                                          0184
         L     @12,CVTTCASP(,@12)                                  0184
         L     @15,TCASEXIT(,@12)                                  0184
         BALR  @14,@15                                             0184
*      RFY REG1 UNRSTD;                /* FREE REGISTER 1    @G58AK3B*/
*  END;                                /*                    @ZM20856*/
*ELSE                                  /* IKTOMIDS IS ALIVE  @ZM20856*/
*IF REG15=0 THEN                       /* CHECK FOR DATA RECEIVED    */
         B     @RC00178                                            0187
@RF00178 LTR   REG15,REG15                                         0187
         BNZ   @RF00187                                            0187
*  IF RPLDATA='1'B THEN                /* CHECK CONTROL IS DATA      */
         LA    @14,2348                                            0188
         AL    @14,TIMGETMN                                        0188
         TM    RPLDATA+16(@14),B'10000000'                         0188
         BNO   @RF00188                                            0188
*     CALL DATARTN;                    /* YES, CALL DATA ROUTINE     */
         BAL   @14,DATARTN                                         0189
*  ELSE                                /* CONTROL IS NOT DATA        */
*      IF RECERCNT >=3 THEN            /* CHECK NUMBER OF TIMES      */
         B     @RC00188                                            0190
@RF00188 L     @14,TIMGETMN                                        0190
         L     @14,RECERCNT(,@14)                                  0190
         C     @14,@CF00073                                        0190
         BL    @RF00190                                            0190
*        CALL EXITFRR;                 /* CALL ABEND ROUTINE @ZM20856*/
         BAL   @14,EXITFRR                                         0191
*      ELSE                            /* LESS THAN 3, UPDATE COUNT  */
*        RECERCNT=RECERCNT+1;          /* BY ONE                     */
         B     @RC00190                                            0192
@RF00190 L     @14,TIMGETMN                                        0192
         LA    @12,1                                               0192
         AL    @12,RECERCNT(,@14)                                  0192
         ST    @12,RECERCNT(,@14)                                  0192
*ELSE                                  /* ERROR                      */
*  DO;                                 /* CHECK ERROR CODE           */
         B     @RC00187                                            0193
@RF00187 DS    0H                                                  0194
*    CALL VTAMERR;                     /* CHECK VTAM ERROR CODE      */
         BAL   @14,VTAMERR                                         0194
*    IF REGRTN='1'B THEN               /* REFRESH RPL NEEDED?        */
         L     @14,TIMGETMN                                        0195
         TM    REGRTN(@14),B'00001000'                             0195
         BNO   @RF00195                                            0195
*       DO;                            /* YES, GO TO REFRESH RTN     */
*         LM(REG0,REG15,SAVEREGS);     /* RESTORE REGISTERS          */
         L     @14,TIMGETMN                                        0197
         LM    REG0,REG15,SAVEREGS(@14)                            0197
*         CALL PRIMPROC;               /* CALL INITIALIZATION ROUTINE*/
         BAL   @14,PRIMPROC                                        0198
*         REGRTN='0'B;                 /* RESET REFRESH INDICATOR    */
         L     @14,TIMGETMN                                        0199
         NI    REGRTN(@14),B'11110111'                             0199
*       END;                           /* RPL REFRESHED              */
*    ELSE;                             /* RPL NEED NOT BE REFRESHED  */
@RF00195 DS    0H                                                  0202
*  END;                                /* END OF ERROR CHECK         */
*GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RECV),REGS=USE)           0203
*    REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);        /*@ZM20546*/
@RC00187 DS    0H                                                  0203
@RC00178 DS    0H                                                  0203
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RECV),REGS=USE
*RFY REG0 UNRSTD;                      /* FREE UP REG 0      @ZM20858*/
*GO TO REC;                            /* LOOP BACK TO ISSUE RECEIVE */
         B     REC                                                 0205
         EJECT
*/********************************************************************/
*/*     THE FOLLOWING CODE BUILDS AND INITIALIZES THE RPL FOR THE    */
*/*     VTAM RECEIVE MACRO AND WILL BE THE PRIMARY RESTARTING POINT  */
*/*     FOR IKTIMIDS IN CASE OF FAILURE DUE TO AN ERROR CONDITON.    */
*/********************************************************************/
*PRIMPROC:                                                         0206
*  PROCEDURE OPTIONS(SAVE(14));                                    0206
PRIMPROC ST    @14,@SA00002                                        0206
*  DCL DUMMYRPL CHAR(LENGTH(IFGRPL)) STATIC LOCAL GENERATED;       0207
*  GEN REFS(TIMBUFF) DATA DEFS(DUMMYRPL);                  /*@ZA13627*/
*  RFY ISTNIB BASED(TVWANIB);          /* BASE THE NIB DSECT         */
*  IFGRPL=DUMMYRPL;                    /* INITIALIZE RPL             */
         L     @14,TIMGETMN                                        0210
         LA    @12,2348                                            0210
         LR    @04,@14                                             0210
         ALR   @04,@12                                             0210
         MVC   IFGRPL(112,@04),DUMMYRPL                            0210
*  RPLDACB=TVWAACB;                    /* PLACE ACB ADDR IN RPL      */
         LR    @04,@14                                             0211
         ALR   @04,@12                                             0211
         L     @11,TVWAPTR                                         0211
         L     @10,TVWAACB(,@11)                                   0211
         ST    @10,RPLDACB(,@04)                                   0211
*  RPLARG=NIBCID;                      /* PLACE CID IN RPL           */
         LR    @04,@14                                             0212
         ALR   @04,@12                                             0212
         L     @01,TVWANIB(,@11)                                   0212
         L     @10,NIBCID(,@01)                                    0212
         ST    @10,RPLARG(,@04)                                    0212
*  RPLAREA=ADDR(TIMBUFF);              /* INITIALIZE ADDRESS OF INPUT*/
         ALR   @12,@14                                             0213
         LA    @04,TIMBUFF(,@14)                                   0213
         ST    @04,RPLAREA(,@12)                                   0213
*  IPTRMTYP=TSBXTMTP;                  /* INITIALIZE TERMINAL TYPE   */
         L     @12,PSAAOLD                                         0214
         L     @12,ASCBTSB(,@12)                                   0214
         L     @12,TSBEXTNT(,@12)                                  0214
         IC    @04,TSBXTMTP(,@12)                                  0214
         STC   @04,IPTRMTYP(,@14)                                  0214
*  TVWATOPQ=ADDR(FLASHBK1);            /* INITIALIZE ADDR OF FLASHBK */
         LA    @04,FLASHBK1(,@14)                                  0215
         ST    @04,TVWATOPQ(,@11)                                  0215
*  IF TSBXTMBF=1920 THEN                                           0216
         CLC   TSBXTMBF(2,@12),@CH02539                            0216
         BNE   @RF00216                                            0216
*    PHYSLNNO = 24;                    /* NUMBER OF LINES    @ZA13627*/
         MVC   PHYSLNNO(4,@14),@CF00090                            0217
*  ELSE                                                            0218
*    PHYSLNNO = 12;                    /* NUMBER OF LINES    @ZA13627*/
         B     @RC00216                                            0218
@RF00216 L     @14,TIMGETMN                                        0218
         MVC   PHYSLNNO(4,@14),@CF02046                            0218
*RETURN;                                                           0219
@EL00002 DS    0H                                                  0219
@EF00002 DS    0H                                                  0219
@ER00002 L     @14,@SA00002                                        0219
         BR    @14                                                 0219
*END PRIMPROC;                         /* END OF PRIMPROC            */
         B     @EL00002                                            0220
         EJECT
*/********************************************************************/
*/*    THIS ROUTINE WILL CHECK TO SEE IF IKTOMIDS IS STILL           */
*/*    AVAILABLE TO BE SCHEDULED.  THE TVWATIS FLAG IS 1 WHEN        */
*/*    IKTOMIDS IS EITHER RUNNING OR SCHEDULED.  TVWATAS MEANS       */
*/*    THAT ONLY IKTIMIDS CAN SCHEDULE IKTOMIDS.                     */
*/********************************************************************/
*SCHTOM: PROCEDURE OPTIONS(SAVE(14));                              0221
*                                                                  0221
SCHTOM   ST    @14,@SA00003                                        0221
*  TVWATAS = '0'B;                     /* TOM AVAILABLE      @ZM20546*/
         L     @14,TVWAPTR                                         0222
         NI    TVWATAS(@14),B'11011111'                            0222
*  IF TVWATIS='0'B THEN                /* IKTOMIDS NOT SCHEDULED     */
         TM    TVWATIS(@14),B'01000000'                            0223
         BNZ   @RF00223                                            0223
*    DO;                               /* SCHEDULE IKTOMIDS          */
*      RFY REG2 RSTD;                  /* RESTRICT REGISTER 2        */
*      REG2=TSBXSRB;                   /* ADDRESS OF SRB TO FREE     */
         L     @14,PSAAOLD                                         0226
         L     @14,ASCBTSB(,@14)                                   0226
         L     @14,TSBEXTNT(,@14)                                  0226
         L     REG2,TSBXSRB(,@14)                                  0226
*      GEN(SCHEDULE SRB=(REG2),SCOPE=LOCAL)                        0227
*              REFS(CVTPTR,CVTLSMQ,CVTMAP,SRBFLNK,SRBSECT);        0227
         SCHEDULE SRB=(REG2),SCOPE=LOCAL
*      TVWATIS='1'B;                   /* SET TOM SCHEDULED ON       */
         L     @14,TVWAPTR                                         0228
         OI    TVWATIS(@14),B'01000000'                            0228
*      RFY REG2 UNRSTD;                /* FREE REGISTER 2            */
*    END;                              /* IKTOMIDS IS SCHEDULED      */
*  ELSE;                               /* IKTOMIDS HAS BEEN SCHEDULED*/
@RF00223 DS    0H                                                  0232
*  TIMSCHTM = '0'B;                    /* RESET SCHEDULE-TOM FLAG    */
         L     @14,TIMGETMN                                        0232
         NI    TIMSCHTM(@14),B'10111111'                           0232
*  RETURN;                             /* RETURN TO CALLER           */
@EL00003 DS    0H                                                  0233
@EF00003 DS    0H                                                  0233
@ER00003 L     @14,@SA00003                                        0233
         BR    @14                                                 0233
*  END SCHTOM;                         /* END OF SCHTOM              */
         EJECT
*/********************************************************************/
*/*    THIS ROUTINE WILL RECEIVE CONTROL WHEN THE RECEIVE MACRO      */
*/*    IS SATISFIED WITH DATA.                                       */
*/********************************************************************/
*DATARTN:                                                          0235
*  PROCEDURE;                                                      0235
DATARTN  STM   @14,@12,@SA00004                                    0235
*  RECERCNT=0;                         /* INITIALIZE ERROR COUNT     */
         L     @14,TIMGETMN                                        0236
         SLR   @12,@12                                             0236
         ST    @12,RECERCNT(,@14)                                  0236
*  /******************************************************************/
*  /* THE FIRST CHECK MADE IS TO SEE IF THE RECEIVE DONE WAS THE     */
*  /* SECOND RECEIVE NEEDED TO COMPLETE THE TRANSFER OF ALL THE      */
*  /* DATA FROM THE VTAM BUFFER TO IKTIMIDS. IF REC2FLG IS ON, AND   */
*  /* THERE WAS NO PRIOR ERROR, DATA PROCESSING CONTINUES.           */
*  /******************************************************************/
*  IF REC2FLG='1'B THEN                /* CHECK SECOND RECEIVE FLAG  */
         TM    REC2FLG(@14),B'00000010'                            0237
         BNO   @RF00237                                            0237
*    IF TIMADDER='0'B THEN             /* IF ON, CHECK FOR ERROR     */
         TM    TIMADDER(@14),B'00010000'                           0238
         BNZ   @RF00238                                            0238
*      CALL IKT3270I;                  /* NO ERROR, CALL IKT3270I    */
         L     @12,CVTPTR                                          0239
         L     @12,CVTTCASP(,@12)                                  0239
         L     @15,TCASDATI(,@12)                                  0239
         BALR  @14,@15                                             0239
*    ELSE                              /* OTHERWISE DATA IS DISCARDED*/
*      IF RPLRLEN<=LENGTH(TIMBUFF) THEN /* RU FITS IN BUFFER?@ZM20856*/
         B     @RC00238                                            0240
@RF00238 L     @14,TIMGETMN                                        0240
         LA    @12,2348                                            0240
         ALR   @12,@14                                             0240
         CLC   RPLRLEN(4,@12),@CF02417                             0240
         BH    @RF00240                                            0240
*        DO;                                                       0241
*          TIMADDER='0'B;              /* RESET ERROR INDICATOR      */
*          REC2FLG='0'B;               /* RESET SECOND RECEIVE FLAG  */
         NI    TIMADDER(@14),B'11101101'                           0243
*        END;                                                      0244
*      ELSE;                           /* DO ANOTHER RECEIVE @ZM20856*/
@RF00240 DS    0H                                                  0246
*   ELSE                                                           0246
*    DO;                                                           0246
         B     @RC00237                                            0246
@RF00237 DS    0H                                                  0247
*     TIMADDER='0'B;                   /* INITIALIZE ERROR FLAG      */
         L     @14,TIMGETMN                                        0247
         NI    TIMADDER(@14),B'11101111'                           0247
*     /***************************************************************/
*     /*  CHECK TO SEE IF THE PAGING FLAG IS ON. IF IT IS, NO INPUT  */
*     /*  IS PROCESSED AND ONLY AN ATTENTION AID IS RECOGNIZED.      */
*     /*  LINE NUMBERING IS RESET TO 1, THE PAGING FLAG IS TURNED OFF*/
*     /*  THE FORMAT SCREEN FLAG IS TURNED ON, AND AN INDICATOR IS   */
*     /*  SET FOR TIM TO SCHEDULE TOM.                               */
*     /***************************************************************/
*     IF TVWAPGN='1'B THEN             /* CHECK THE PAGING SWITCH    */
         L     @14,TVWAPTR                                         0248
         TM    TVWAPGN(@14),B'01000000'                            0248
         BNO   @RF00248                                            0248
*      DO;                                                         0249
*      /**************************************************************/
*      /*  THE FOLLOWING CODE HAS BEEN ADDED TO SUPPORT     @YM03254 */
*      /*  FULL SCREEN MODE                                 @YM03254 */
*      /**************************************************************/
*      IF TVWAFSW='1'B THEN           /* IF FULL SCREEN MODE @YM03254*/
