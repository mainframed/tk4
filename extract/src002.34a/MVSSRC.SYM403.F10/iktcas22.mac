IKTCAS22 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKTCAS22  77.272'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
*      R8=WEVRPL ;                     /* R8 PTR TO RPL              */
         L     R8,WEVRPL(,R10)                                     0028
*      RFY  WORKSPAC BASED(EVAL(R8+LENGTH(IFGRPL))) ;              0029
*      R9=R8-LENGTH(WESTD) ;           /* GET LOGON W.E.             */
         LR    R9,R8                                               0030
         SL    R9,@CF00502                                         0030
*      R9=R9->WELBUF ;                 /* R9 PTR TO LOGON BUF WHERE  */
         L     R9,WELBUF(,R9)                                      0031
*                                      /* APPL ID WAS SAVED          */
*         R9=R9+4 ;                    /* SKIP LENGTH FIELD          */
         AL    R9,@CF00036                                         0032
*      IF WECODE1F=FCVITT              /* IS THIS TERM TRANSFER REQ  */
*         THEN DO ;                    /* YSE ISSUE  CLSDST PASS     */
         CLI   WECODE1F(R10),X'21'                                 0033
         BNE   @RF00033                                            0033
*         WSCLSPAS='1'B ;              /* SET PASS INDICATOR         */
*         WSCLSREL='0'B ;              /* SET RELEASE BIT OFF        */
         LA    @06,112                                             0036
         ALR   @06,R8                                              0036
         OI    WSCLSPAS(@06),B'00010000'                           0036
         NI    WSCLSREL(@06),B'11011111'                           0036
*         GEN ;                                                    0037
         L     R7,IKTCASCX            EXIT ROUTINE ADDR
         CLSDST RPL=(8),AAREA=(9),EXIT=(7),OPTCD=(ASY,PASS)
*         END ;                                                    0038
*        ELSE DO ;                     /* NOT TERM PASS THEN RELEASE */
         B     @RC00033                                            0039
@RF00033 DS    0H                                                  0040
*         WSCLSREL='1'B ;              /* SET RELEASE BIT ON         */
*         WSCLSPAS='0'B ;              /* SET PASS BIT OFF           */
         LA    @06,112                                             0041
         ALR   @06,R8                                              0041
         OI    WSCLSREL(@06),B'00100000'                           0041
         NI    WSCLSPAS(@06),B'11101111'                           0041
*         GEN ;                                                    0042
         L     R7,IKTCASCX            EXIT ROUTINE ADDR
         CLSDST RPL=(8),EXIT=(7),OPTCD=(ASY,RELEASE)
*          END ;                                                   0043
*      IF R5=0 THEN                    /* IS REG 5 0? IF SO, @ZA19531*/
@RC00033 LTR   R5,R5                                               0044
         BNZ   @RF00044                                            0044
*        DO;                           /* FREE THE LOGON W.E.@ZA19531*/
*          R9=WELEN ;                  /* GET W.E. LENGTH    @ZA19879*/
         LH    R9,WELEN(,R10)                                      0046
*          WA1=R10 ;               /* STORE ADDR FOR FREEMAIN@ZA19879*/
         ST    R10,WA1                                             0047
*          GEN REFS(WA1) ;                                         0048
         FREEMAIN E,LV=(R9),A=WA1,SP=231  FREEMAIN FOR LOGON W.E.
*        END;                          /*                    @ZA19531*/
*      ELSE;                           /* DO NOT FREE W.E.   @ZA19531*/
@RF00044 DS    0H                                                  0051
*      GEN DATA ;                                                  0051
*      RFY R5 UNRSTD;                  /* FREE REG 5         @ZA19531*/
*      END IKTCAS22                                                0053
