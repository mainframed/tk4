*   RFY                                                         01S0610
*    (REG2,                                                     01S0610
*     REG15,                                                    01S0610
*     REG0) RSTD;                   /* RESTRICT REGISTERS    @ZM20858*/
*/********************************************************************/
*/* CHECK IMMEDIATE VTAM RETURN CODE                         @ZM20858*/
*/********************************************************************/
*                                                               01S0611
*   IF REG15=32×                    /* IF RPL IS INVALID OR  @ZM20858*/
*       (REG15=4&REG0=24) THEN      /* VTAM NOT ACTIVE       @ZM20858
*                                      RETRY 3 TIMES         @ZM20858*/
         C     REG15,@CF00036                                   01S0611
         BE    @RT00611                                         01S0611
         C     REG15,@CF00123                                   01S0611
         BNE   @RF00611                                         01S0611
         C     REG0,@CF00284                                    01S0611
         BNE   @RF00611                                         01S0611
@RT00611 DS    0H                                               01S0612
*     IF VRTRYCNT>=3×TOMERCNT>=3 THEN/* RETRY LIMIT EXCEEDED @ZM20858*/
         LA    @14,3                                            01S0612
         C     @14,VRTRYCNT                                     01S0612
         BNH   @RT00612                                         01S0612
         C     @14,TOMERCNT(,TOMWAPTR)                          01S0612
         BH    @RF00612                                         01S0612
@RT00612 DS    0H                                               01S0613
*       CALL EXITFRR;               /* CALL ABEND ROUTINE    @ZM20856*/
         BAL   @14,EXITFRR                                      01S0613
*     ELSE                                                      01S0614
*       CALL TOMINIT;               /* REFRESH RPL AND RETRY@ZM20858 */
         B     @RC00612                                         01S0614
@RF00612 BAL   @14,TOMINIT                                      01S0614
*   ELSE                            /* IMMEDIATE VTAM RC OK  @ZM20858*/
*     DO;                           /* ***** CONTINUE NORMAL    01S0615
*                                      PROCEEDURE**          @ZM20858*/
         B     @RC00611                                         01S0615
@RF00611 DS    0H                                               01S0616
*/*  *****************************************************************/
*/*   CHECK LOSTERM SITUATIONS                                       */
*/*  *****************************************************************/
*                                                               01S0616
*       IF(RPLRTNCD=12&(RPLFDB2=2×  /* REQUEST RESET         @ZM20858*/
*           RPLFDB2=7×              /* RECOVERY IN PROGRESS  @ZM20858*/
*           RPLFDB2=8×              /* RECORD TERM RESTARTED AFTER
*                                      FAILURE               @ZM20858*/
*           RPLFDB2=9×              /* CLSDST CANCELED OPNDST@ZM20858*/
*           RPLFDB2=11))×           /* CLSDST OCCURED        @ZM20858*/
*           (RPLRTNCD=20&(RPLFDB2=18×RPLFDB2=65))×/* NO SDT ISSUED?  */
*           (RPLRTNCD=16&(RPLFDB2=3×/* HALT QUICK ISSUED?            */
*           RPLFDB2=5×              /* CHANNEL OR LINK FAIL?@ZM20858 */
*           RPLFDB2=6×              /* NCP SHUTDOWN?         @ZM20858*/
*           RPLFDB2=7×              /* V NET INACT ISSUED?           */
*           RPLFDB2=9×              /* TERMINATE SELF?               */
*           RPLFDB2=15))×           /* BUFFERS FILLED?               */
*           TVWALTE='1'B            /* LOSTERM ENTERED       @ZM20858*/
*         THEN                      /* LOSTERM SITUATION             */
         CLI   RPLRTNCD(RPLPTR),12                              01S0616
         BNE   @GL00026                                         01S0616
         CLI   RPLFDB2(RPLPTR),2                                01S0616
         BE    @RT00616                                         01S0616
         CLI   RPLFDB2(RPLPTR),7                                01S0616
         BE    @RT00616                                         01S0616
         CLI   RPLFDB2(RPLPTR),8                                01S0616
         BE    @RT00616                                         01S0616
         CLI   RPLFDB2(RPLPTR),9                                01S0616
         BE    @RT00616                                         01S0616
         CLI   RPLFDB2(RPLPTR),11                               01S0616
         BE    @RT00616                                         01S0616
@GL00026 CLI   RPLRTNCD(RPLPTR),20                              01S0616
         BNE   @GL00025                                         01S0616
         CLI   RPLFDB2(RPLPTR),18                               01S0616
         BE    @RT00616                                         01S0616
         CLI   RPLFDB2(RPLPTR),65                               01S0616
         BE    @RT00616                                         01S0616
@GL00025 CLI   RPLRTNCD(RPLPTR),16                              01S0616
         BNE   @GL00024                                         01S0616
         CLI   RPLFDB2(RPLPTR),3                                01S0616
         BE    @RT00616                                         01S0616
         CLI   RPLFDB2(RPLPTR),5                                01S0616
         BE    @RT00616                                         01S0616
         CLI   RPLFDB2(RPLPTR),6                                01S0616
         BE    @RT00616                                         01S0616
         CLI   RPLFDB2(RPLPTR),7                                01S0616
         BE    @RT00616                                         01S0616
         CLI   RPLFDB2(RPLPTR),9                                01S0616
         BE    @RT00616                                         01S0616
         CLI   RPLFDB2(RPLPTR),15                               01S0616
         BE    @RT00616                                         01S0616
@GL00024 TM    TVWALTE(TVWAPTR),B'00000010'                     01S0616
         BNO   @RF00616                                         01S0616
@RT00616 DS    0H                                               01S0617
*         IF TVWATID='1'B THEN      /* IS TIM DEAD?                  */
         TM    TVWATID(TVWAPTR),B'00010000'                     01S0617
         BNO   @RF00617                                         01S0617
*           CALL ERREXIT;           /* YES, EXIT PERMANENTLY         */
         BAL   @14,ERREXIT                                      01S0618
*         ELSE                      /* NO, EXIT NORMALLY             */
*           DO;                     /* EXIT NORMALLY                 */
         B     @RC00617                                         01S0619
@RF00617 DS    0H                                               01S0620
*             TVWATAS='1'B;         /* TOM UNAVAILABLE       @ZM20661*/
         OI    TVWATAS(TVWAPTR),B'00100000'                     01S0620
*             TOMESEND='1'B;        /* EXIT SEND LOOP                */
*             TOMEMAIN='1'B;        /* EXIT MAINLINE LOOP            */
         OI    TOMESEND(TOMWAPTR),B'00110000'                   01S0622
