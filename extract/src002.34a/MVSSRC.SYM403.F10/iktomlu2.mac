         TITLE '*** IKTOMLU2, OUTPUT MANAGER FOR NDS/DSE TERMINALS *** *
                        '
IKTOMLU2 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IKTOMLU2  78.016'                              01S0001
         DROP  @15
@PROLOG  BALR  @05,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@05                                      01S0001
*   TOMWAPTR=REG1;                  /* ADDRESS OF COMPILER STORAGE   */
         LR    TOMWAPTR,REG1                                    01S0140
*   GEN(USING @DATD,TOMWAPTR)                                   01S0141
*   REFS(TOMWAPTR);                 /* ADDRESSABILITY FOR COMPILER   */
         USING @DATD,TOMWAPTR
*   RETADR00=REG14;                 /* SAVE RETURN ADDR TO CALLER    */
         ST    REG14,RETADR00                                   01S0142
*   RFY                                                         01S0143
*    (REG14) UNRSTD;                /* UNRESTRICT REGISTER 14        */
*   RPLPTR=TOMWAPTR+LEN@DATA+LENGTH(TOMWA);/* SET ADDRESS OF WORKING
*                                      RPL                           */
         LA    RPLPTR,744                                       01S0144
         ALR   RPLPTR,TOMWAPTR                                  01S0144
*   RFY                                                         01S0145
*     IFGRPL BASED(RPLPTR);                                     01S0145
*   TVWAPTR=TSBXTVWA;               /* TVWA POINTER                  */
         L     @12,PSAAOLD                                      01S0146
         L     @12,ASCBTSB(,@12)                                01S0146
         L     @12,TSBEXTNT(,@12)                               01S0146
         L     TVWAPTR,TSBXTVWA(,@12)                           01S0146
*   TOMGETMN=RPLPTR+LENGTH(IFGRPL); /* SET ADDRESS OF OUTPUT RU BUF  */
         LA    @12,112                                          01S0147
         ALR   @12,RPLPTR                                       01S0147
         ST    @12,TOMGETMN(,TOMWAPTR)                          01S0147
*   DO;                             /* CHECKPOINT REGISTERS AND THE
*                                      ADDRESS               @ZM20856*/
*CKMC0002:                                                      01S0149
*     STM(REG0,REG15,FRWACKRG);     /* SAVE ALL REGISTERS    @ZM20856*/
CKMC0002 L     @12,TVWAFRWO(,TVWAPTR)                           01S0149
         STM   REG0,REG15,FRWACKRG(@12)                         01S0149
*     FRWACKPT(2)=ADDR(CKMC0002);   /* SAVE ADDRESS          @ZM20856*/
         LA    @04,CKMC0002                                     01S0150
         ST    @04,FRWACKPT+4(,@12)                             01S0150
*   END;                            /* CHECK POINT           @ZM20856*/
*   STM(REG0,REG15,RTRYREGS);       /* SAVE REGS FOR ERROR RECOVERY  */
         STM   REG0,REG15,RTRYREGS                              01S0152
*   RFY                                                         01S0153
*    (REG1,                                                     01S0153
*     REG14) UNRSTD;                /* FREE UP REGS 1,14     @ZM20856*/
*   CALL OFRRLOCK(ADDR(RESTART1));  /* ADD FRR AND OBTAIN LOCAL LOCK
*                                                            @ZM20856*/
         LA    @12,RESTART1                                     01S0154
         ST    @12,@AFTEMPS+12                                  01S0154
         LA    @12,@AFTEMPS+12                                  01S0154
         ST    @12,@AL00001                                     01S0154
         LA    @01,@AL00001                                     01S0154
         BAL   @14,OFRRLOCK                                     01S0154
*RESTART1:                                                      01S0155
*   TOMERCNT=0;                     /* RESET ERROR COUNT             */
RESTART1 SLR   @12,@12                                          01S0155
         ST    @12,TOMERCNT(,TOMWAPTR)                          01S0155
*   IF TOMINITS='0'B THEN           /* IS THIS FIRST PASS THROUGH?   */
         TM    TOMINITS(TOMWAPTR),B'10000000'                   01S0156
         BNZ   @RF00156                                         01S0156
*/*  *****************************************************************/
*/*                                                                  */
*/*         INITIALIZE TOM'S WORKING STORAGE ON FIRST ENTRY.         */
*/*                                                                  */
*/*  *****************************************************************/
*                                                               01S0157
*     CALL TOMINIT;                 /* INITIALIZE RPL & INDICATORS   */
         BAL   @14,TOMINIT                                      01S0157
*   ELSE                                                        01S0158
*     ;                             /* NOT FIRST PASS                */
@RF00156 DS    0H                                               01S0159
*   TVWATIR='1'B;                   /* SET TOM IS RUNNING FLAG       */
         OI    TVWATIR(TVWAPTR),B'00001000'                     01S0159
*   IF TVWASDSG='1'B THEN           /* SEND SIGNAL?          @ZM20268*/
         TM    TVWASDSG(TVWAPTR),B'01000000'                    01S0160
         BNO   @RF00160                                         01S0160
*     DO;                           /* SEND SIGNAL           @ZM20268*/
*       CALL SENDSIG(ADDR(RESTART1));/* SEND SIGNAL          @ZM20856*/
         LA    @12,RESTART1                                     01S0162
         ST    @12,@AFTEMPS+12                                  01S0162
         LA    @12,@AFTEMPS+12                                  01S0162
         ST    @12,@AL00001                                     01S0162
         LA    @01,@AL00001                                     01S0162
         BAL   @14,SENDSIG                                      01S0162
*       IF TVWASDSG='0'B THEN       /* CMD RECEIVED BY TIM?  @ZM20268*/
         TM    TVWASDSG(TVWAPTR),B'01000000'                    01S0163
         BNZ   @RF00163                                         01S0163
*         TOMEMAIN='0'B;            /* CONTINUE MAINLINE     @ZM20268*/
         NI    TOMEMAIN(TOMWAPTR),B'11011111'                   01S0164
*       ELSE                        /* CMD NOT RECEIVED      @ZM20268*/
*         DO;                       /* PREPARE FOR EXIT      @ZM20268*/
         B     @RC00163                                         01S0165
@RF00163 DS    0H                                               01S0166
*           TOMEMAIN='1'B;          /* EXIT MAIN LOOP        @ZM20268*/
         OI    TOMEMAIN(TOMWAPTR),B'00100000'                   01S0166
*           TVWATAS='1'B;           /* TOM UNAVAILABLE       @ZM20268*/
         OI    TVWATAS(TVWAPTR),B'00100000'                     01S0167
*         END;                      /* PREPARED FOR EXIT     @ZM20268*/
*     END;                          /* SIGNAL SENT           @ZM20268*/
*   ELSE                            /* DO NOT SEND SIGNAL    @ZM20268*/
*     TOMEMAIN='0'B;                /* CONTINUE MAINLINE PROCESSING  */
         B     @RC00160                                         01S0170
@RF00160 NI    TOMEMAIN(TOMWAPTR),B'11011111'                   01S0170
*/********************************************************************/
*/*                                                                  */
*/* THIS DO GROUP ENCOMPASSES THE MAINLINE PROCESSING FOR THE 3270   */
*/* TERMINAL OUTPUT MANAGER (TOM)                                    */
*/*                                                                  */
*/********************************************************************/
*                                                               01S0171
*   DO WHILE(TOMEMAIN='0'B);        /*                       @ZM20268*/
@RC00160 B     @DE00171                                         01S0171
@DL00171 DS    0H                                               01S0172
*     DO;                           /* CHECKPOINT REGISTERS AND THE
*                                      ADDRESS               @ZM20856*/
*CKMC0003:                                                      01S0173
*       STM(REG0,REG15,FRWACKRG);   /* SAVE ALL REGISTERS    @ZM20856*/
CKMC0003 L     @12,TVWAFRWO(,TVWAPTR)                           01S0173
         STM   REG0,REG15,FRWACKRG(@12)                         01S0173
*       FRWACKPT(2)=ADDR(CKMC0003); /* SAVE ADDRESS          @ZM20856*/
         LA    @04,CKMC0003                                     01S0174
         ST    @04,FRWACKPT+4(,@12)                             01S0174
*     END;                          /* CHECK POINT           @ZM20856*/
*     IF(TVWATID='0'B)&             /* HAS TIM EXITED PERMANENTLY?   */
*         (TVWALTE='0'B) THEN       /* HAS LOSTERM BEEN ENTERED?     */
*                                                               01S0176
         TM    TVWATID(TVWAPTR),B'00010010'                     01S0176
         BNZ   @RF00176                                         01S0176
*       /*************************************************************/
*       /*                                                           */
*       /* TIM IS NOT DEAD AND LOSTERM HAS NOT BEEN ENTERED SO CON-  */
*       /* TINUE PROCESSING.                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0177
*       DO;                                                     01S0177
*         IF TOMERMG9='1'B THEN                                 01S0178
*                                                               01S0178
         TM    TOMERMG9(TOMWAPTR),B'00100000'                   01S0178
         BNO   @RF00178                                         01S0178
*           /*********************************************************/
*           /*                                                       */
*           /* IF THIS IS THE 1ST ENTRY AFTER A PAGING RESPONSE FROM */
*           /* A SCRN ERASURE AS AN ERP, THEN RESET LINE-CNT AND RE- */
*           /* SEND RU (ALREADY BUILT).                              */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0179
*           DO;                                                 01S0179
*             TVWALNCT=TOMLNCNT;    /* USE LINE-COUNT BEFORE ERROR   */
         L     @12,TOMLNCNT(,TOMWAPTR)                          01S0180
         STC   @12,TVWALNCT(,TVWAPTR)                           01S0180
*             TOMERMG9='0'B;        /* OFF, REQ FOR LINE-CNT RESET   */
         NI    TOMERMG9(TOMWAPTR),B'11011111'                   01S0181
*           END;                                                01S0182
*         ELSE                                                  01S0183
*           DO;                                                 01S0183
         B     @RC00178                                         01S0183
@RF00178 DS    0H                                               01S0184
*             TOMLNCNT=TVWALNCT;    /* USE LINE-COUNT AS LOCAL       */
         SLR   @12,@12                                          01S0184
         IC    @12,TVWALNCT(,TVWAPTR)                           01S0184
         ST    @12,TOMLNCNT(,TOMWAPTR)                          01S0184
*             IF VRTRYCNT^=0 THEN                               01S0185
*                                                               01S0185
         SLR   @12,@12                                          01S0185
         C     @12,VRTRYCNT                                     01S0185
         BE    @RF00185                                         01S0185
*               /*****************************************************/
*               /*                                                   */
*               /* IF THIS ENTRY IS FOR ERROR RETRY OUT OF TOM'S FRR,*/
*               /* THEN RESET OUTPUT QUEUE POINTER TO TOP OF QUEUE.  */
*               /*                                                   */
*               /*****************************************************/
*                                                               01S0186
*               DO;                 /* BEGIN RESET                   */
*                 TOMCOLMN=0;       /* RESET COLUMN COUNT            */
         ST    @12,TOMCOLMN(,TOMWAPTR)                          01S0187
*                 OPREQ=RESET;      /* QUE MANAGER'S CODE FOR RESET  */
         MVI   OPREQ(TOMWAPTR),X'07'                            01S0188
*                 CALL IKTQMOUT(OPARMS);/* CALL OUTPUT QUEUE MANAGER */
         LA    @12,OPARMS(,TOMWAPTR)                            01S0189
         ST    @12,@AL00001                                     01S0189
         L     @12,CVTPTR                                       01S0189
         L     @12,CVTTCASP(,@12)                               01S0189
         L     @15,TCASOQM(,@12)                                01S0189
         LA    @01,@AL00001                                     01S0189
         BALR  @14,@15                                          01S0189
*                 TOMBRKMG='0'B;    /* RESET BREAK-MESSAGE SWITCH    */
         NI    TOMBRKMG(TOMWAPTR),B'01111111'                   01S0190
*               END;                /* END OF RESET                  */
*             ELSE                                              01S0192
*               ;                   /* NOT A RETRY SITUATION         */
*                                                               01S0192
@RF00185 DS    0H                                               01S0193
*             /*******************************************************/
*             /*                                                     */
*             /* BUILD OUTPUT RU IF OUTPUT DATA & CONTROLS ARE       */
*             /* PENDING                                             */
*             /*                                                     */
*             /*******************************************************/
*                                                               01S0193
*             TOMESEND='0'B;        /* INIT LOOP CONTROL FOR SEND    */
         NI    TOMESEND(TOMWAPTR),B'11101111'                   01S0193
*             IF TVWAFMEW='1'B THEN /* IS THIS ENTRY FOR TIM'S ERASE */
         TM    TVWAFMEW(TVWAPTR),B'00001000'                    01S0194
         BNO   @RF00194                                         01S0194
*               DO;                                             01S0195
*                 TVWAFMEW='0'B;    /* RESET TVWAFMEW                */
         NI    TVWAFMEW(TVWAPTR),B'11110111'                    01S0196
*                 TOMREQCD=TOMFMWRT;/* SET REQ CODE FOR FORMAT WRT   */
         MVI   TOMREQCD(TOMWAPTR),X'01'                         01S0197
*                 CALL IKT3270O;    /* CALL SCRN MANAGER TO BLD RU   */
         L     @12,CVTPTR                                       01S0198
         L     @12,CVTTCASP(,@12)                               01S0198
         L     @15,TCASDATO(,@12)                               01S0198
         BALR  @14,@15                                          01S0198
*               END;                                            01S0199
*             ELSE                  /* ENTRY IS NOT FOR TIM'S ERASE  */
*               IF TVWABRIN='1'B THEN/* BREAK-IN PROCESS IN PROGRESS?*/
         B     @RC00194                                         01S0200
@RF00194 TM    TVWABRIN(TVWAPTR),B'10000000'                    01S0200
         BNO   @RF00200                                         01S0200
*                 IF TVWASCD='1'B THEN/* YES, HAS TIM RECEIVED CD?   */
         TM    TVWASCD(TVWAPTR),B'00100000'                     01S0201
         BNO   @RF00201                                         01S0201
*                   DO;                                         01S0202
*                     TVWARDBF='1'B;/* TURN ON READ BUFFER INDICATOR */
         OI    TVWARDBF(TVWAPTR),B'00100000'                    01S0203
*                     TVWASCD='0'B; /* OFF, REQ TO SEND CD           */
         NI    TVWASCD(TVWAPTR),B'11011111'                     01S0204
*                     TOMREQCD=TOMRDBF;/* REQ CODE FOR READ BUFFER
*                                      CMD                           */
         MVI   TOMREQCD(TOMWAPTR),X'02'                         01S0205
*                     CALL IKT3270O;/* CALL SCRN MANAGER TO BLD RU   */
         L     @12,CVTPTR                                       01S0206
         L     @12,CVTTCASP(,@12)                               01S0206
         L     @15,TCASDATO(,@12)                               01S0206
         BALR  @14,@15                                          01S0206
*                   END;                                        01S0207
*                 ELSE                                          01S0208
*                   IF TVWARCDT='1'B THEN/* HAS TIM RECEIVED RD 01S0208
*                                      BUFFER?                       */
         B     @RC00201                                         01S0208
@RF00201 TM    TVWARCDT(TVWAPTR),B'00010000'                    01S0208
         BNO   @RF00208                                         01S0208
*                     IF(TOMBPREQ='1'B)&/* HAS BREAK-IN PAGE STARTED?*/
*                         (TOMBPRSP='1'B) THEN/* HAS BRK-IN PAGE
*                                      FINISH?                       */
         TM    TOMBPREQ(TOMWAPTR),B'00001100'                   01S0209
         BNO   @RF00209                                         01S0209
*                       DO;                                     01S0210
*                         TVWARCDT='0'B;/* TURN OFF READ BUFF   01S0211
*                                      RECEIVED                      */
         NI    TVWARCDT(TVWAPTR),B'11101111'                    01S0211
*                         TOMBPREQ='0'B;/* TURN OFF BRK-IN PAGE REQ  */
*                         TOMBPRSP='0'B;/* TURN OFF BRK-IN PAGE RESP */
         NI    TOMBPREQ(TOMWAPTR),B'11110011'                   01S0213
*                         TOMREQCD=TOMSCRES;/* REQ CODE FOR SCRN
*                                      RESTORE                       */
         MVI   TOMREQCD(TOMWAPTR),X'04'                         01S0214
*                         CALL IKT3270O;/* CALL SCRN MANAGER TO BLD
*                                      RU                            */
         L     @12,CVTPTR                                       01S0215
         L     @12,CVTTCASP(,@12)                               01S0215
         L     @15,TCASDATO(,@12)                               01S0215
         BALR  @14,@15                                          01S0215
*                       END;                                    01S0216
*                     ELSE                                      01S0217
*                       DO;                                     01S0217
         B     @RC00209                                         01S0217
@RF00209 DS    0H                                               01S0218
*                         TOMBPREQ='1'B;/* REQUEST PAGING AFTER 01S0218
*                                      BRK-IN                        */
         OI    TOMBPREQ(TOMWAPTR),B'00001000'                   01S0218
*                         TOMREQCD=TOMOPRU;/* SET REQ CODE FOR RU BLD*/
         MVI   TOMREQCD(TOMWAPTR),X'00'                         01S0219
*                         CALL IKT3270O;/* CALL SCRN MANAGER TO BLD
*                                      RU                            */
         L     @12,CVTPTR                                       01S0220
         L     @12,CVTTCASP(,@12)                               01S0220
         L     @15,TCASDATO(,@12)                               01S0220
         BALR  @14,@15                                          01S0220
*                       END;                                    01S0221
*                   ELSE                                        01S0222
*                     DO;                                       01S0222
         B     @RC00208                                         01S0222
@RF00208 DS    0H                                               01S0223
*                       TOMESEND='1'B;/* SET EXIT FROM SEND LOOP     */
*                       TOMEMAIN='1'B;/* SET EXIT FROM MAIN LOOP     */
         OI    TOMESEND(TOMWAPTR),B'00110000'                   01S0224
*                       TVWATAS='1'B;/* TOM NOT AVAILABLE            */
         OI    TVWATAS(TVWAPTR),B'00100000'                     01S0225
*                     END;                                      01S0226
*               ELSE                                            01S0227
*                 DO;               /*                       @ZM20753*/
         B     @RC00200                                         01S0227
@RF00200 DS    0H                                               01S0228
*                   IF TVWAGMPT^=0 THEN/*                    @ZM20753*/
         L     @12,TVWAGMPT(,TVWAPTR)                           01S0228
         LTR   @12,@12                                          01S0228
         BZ    @RF00228                                         01S0228
*                     CALL FREERDBF;/*                       @ZM20753*/
         BAL   @14,FREERDBF                                     01S0229
*                   ELSE                                        01S0230
*                     ;             /*                       @ZM20753*/
@RF00228 DS    0H                                               01S0231
*                   TOMBPREQ='0'B;  /*                       @ZM20753*/
*                   TOMBPRSP='0'B;  /*                       @ZM20753*/
         NI    TOMBPREQ(TOMWAPTR),B'11110011'                   01S0232
*                   IF TVWAKBDL='0'B&/* KEYBOARD UNLOCKED?           */
*                       TVWASCD='0'B&/* NO CHANGE DIRECTION?         */
*                       (TVWAATTN^=0×TVWAAIGN='1'B)&/* ATTENTION?    */
*                       TOMEBSV='0'B THEN/* NO END BRACKET SENT?
*                                                            @ZM20919*/
         TM    TVWAKBDL(TVWAPTR),B'00100000'                    01S0233
         BNZ   @RF00233                                         01S0233
         TM    TVWASCD(TVWAPTR),B'00100000'                     01S0233
         BNZ   @RF00233                                         01S0233
         CLI   TVWAATTN(TVWAPTR),0                              01S0233
         BNE   @GL00001                                         01S0233
         TM    TVWAAIGN(TVWAPTR),B'00100000'                    01S0233
         BNO   @RF00233                                         01S0233
@GL00001 TM    TOMEBSV(TOMWAPTR),B'00001000'                    01S0233
         BNZ   @RF00233                                         01S0233
*                     DO;                                       01S0234
*                       IF RPLFIRST='1'B×/* BEGINNING OF CHAIN?      */
*                           RPLMIDLE='1'B THEN/* MIDDLE OF CHAIN?    */
         LA    @12,16                                           01S0235
         ALR   @12,RPLPTR                                       01S0235
         TM    RPLFIRST(@12),B'11000000'                        01S0235
         BZ    @RF00235                                         01S0235
*                         DO;       /* SET UP FOR END OF CHAIN       */
*                           RPLFIRST='0'B;/* ^ BEGINNING OF CHAIN    */
*                           RPLMIDLE='0'B;/* ^ MIDDLE OF CHAIN       */
*                           RPLLAST='1'B;/* END OF CHAIN             */
         OI    RPLLAST(@12),B'00100000'                         01S0239
         NI    RPLFIRST(@12),B'00111111'                        01S0239
*                         END;                                  01S0240
*                       ELSE        /* END OF OR ONLY CHAIN          */
*                         DO;                                   01S0241
         B     @RC00235                                         01S0241
@RF00235 DS    0H                                               01S0242
*                           RPLONLY='1'B;/* ONLY CHAIN               */
*                           RPLLAST='0'B;/* RESET END OF CHAIN       */
         OI    RPLONLY+16(RPLPTR),B'00010000'                   01S0243
         NI    RPLLAST+16(RPLPTR),B'11011111'                   01S0243
*                         END;                                  01S0244
*                       CALL SENDSIG(ADDR(RESTART1));/* SEND SIGNAL  */
@RC00235 LA    @12,RESTART1                                     01S0245
         ST    @12,@AFTEMPS+12                                  01S0245
         LA    @12,@AFTEMPS+12                                  01S0245
         ST    @12,@AL00001                                     01S0245
         LA    @01,@AL00001                                     01S0245
         BAL   @14,SENDSIG                                      01S0245
*                       IF TVWASCD='1'B×/* CHANGE DIRECTION RECVD?   */
*                           TVWAKBDL='1'B THEN/* KEYBOARD LOCKED?    */
         TM    TVWASCD(TVWAPTR),B'00100000'                     01S0246
         BO    @RT00246                                         01S0246
         TM    TVWAKBDL(TVWAPTR),B'00100000'                    01S0246
         BNO   @RF00246                                         01S0246
@RT00246 DS    0H                                               01S0247
*                         DO;                                   01S0247
*                           TVWASCD='0'B;/* RESET CHANGE DIRECTION   */
         NI    TVWASCD(TVWAPTR),B'11011111'                     01S0248
*                           TOMREQCD=TOMOPRU;/* REQUEST TO BUILD RU  */
         MVI   TOMREQCD(TOMWAPTR),X'00'                         01S0249
*                           CALL IKT3270O;/* DATA HANDLING ROUTINE   */
         L     @12,CVTPTR                                       01S0250
         L     @12,CVTTCASP(,@12)                               01S0250
         L     @15,TCASDATO(,@12)                               01S0250
         BALR  @14,@15                                          01S0250
*                         END;                                  01S0251
*                       ELSE                                    01S0252
*                         DO;                                   01S0252
         B     @RC00246                                         01S0252
@RF00246 DS    0H                                               01S0253
*                           TOMESEND='1'B;/* EXIT SEND LOOP          */
*                           TOMEMAIN='1'B;/* EXIT MAINLINE LOOP      */
         OI    TOMESEND(TOMWAPTR),B'00110000'                   01S0254
*                           TVWATAS='1'B;/* TOM UNAVAILABLE          */
         OI    TVWATAS(TVWAPTR),B'00100000'                     01S0255
*                         END;                                  01S0256
*                     END;                                      01S0257
*                   ELSE                                        01S0258
*                     DO;                                       01S0258
         B     @RC00233                                         01S0258
@RF00233 DS    0H                                               01S0259
*                       TVWASCD='0'B;/* RESET CHANGE-DIRECTION BIT   */
         NI    TVWASCD(TVWAPTR),B'11011111'                     01S0259
*                       TOMREQCD=TOMOPRU;/* SET REQ CODE FOR NORMAL
*                                      RU                            */
         MVI   TOMREQCD(TOMWAPTR),X'00'                         01S0260
*                       CALL IKT3270O;/* CALL SCRN MANAGER TO BLD RU */
         L     @12,CVTPTR                                       01S0261
         L     @12,CVTTCASP(,@12)                               01S0261
         L     @15,TCASDATO(,@12)                               01S0261
         BALR  @14,@15                                          01S0261
*                     END;                                      01S0262
*                 END;              /*                       @ZM20753*/
@RC00233 DS    0H                                               01S0264
*             STM(REG0,REG15,RTRYREGS);/* SAVE REGISTERS FOR RECOVERY*/
@RC00200 DS    0H                                               01S0264
@RC00194 STM   REG0,REG15,RTRYREGS                              01S0264
*             RFY                                               01S0265
*               INFODSCT BASED(ADDR(FRWARTRY));/* POINT TO   @ZM20856
*                                      RETRY INFO AREA       @ZM20856*/
*             INFOADDR=ADDR(RESTART2);/* CHANGE RETRY ADDRESS   01S0266
*                                                            @ZM20856*/
         L     @12,TVWAFRWO(,TVWAPTR)                           01S0266
         LA    @04,RESTART2                                     01S0266
         ST    @04,INFOADDR+8(,@12)                             01S0266
*           END;                    /*                               */
*RESTART2:                                                      01S0268
*         IF TOMBRKMG='1'B&         /* BREAK MESSAGE?        @ZM20919*/
*             TVWABRIN='0'B&        /* ^BREAKIN PROCESSING?  @ZM20919*/
*             TVWAKBDL='0'B&        /* KEYBOARD UNLOCK?      @ZM20919*/
*             TVWASCD='0'B&         /* NO CHANGE DIRECTION   @ZM20919*/
*             TOMEBSV='0'B THEN     /* END BRACKET ^ SENT?   @ZM20919*/
