         ST    @12,RPLRLEN(,RPLPTR)                             02S0293
*                       RPLAREA=DATAADDR;/* SET ADDRESS OF OUTPUT
*                                      BUFFER                        */
         L     @04,DATAADDR                                     02S0294
         ST    @04,RPLAREA(,RPLPTR)                             02S0294
*                       DATACNT=DATACNT-MAXRUSZO;/* UPDATE CNT OF
*                                      DATA REMAINING                */
         SLR   @14,@12                                          02S0295
         ST    @14,DATACNT                                      02S0295
*                       DATAADDR=DATAADDR+MAXRUSZO;/* UPDATE ADDR OF
*                                      REMAINING DATA                */
         ALR   @04,@12                                          02S0296
         ST    @04,DATAADDR                                     02S0296
*                       IF TOMWRKVI=0 THEN/* IS THIS THE FIRST PASS? */
         L     @14,TOMWRKVI(,TOMWAPTR)                          02S0297
