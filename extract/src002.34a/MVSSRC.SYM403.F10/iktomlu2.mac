         L     @14,PSAAOLD                                      02S0277
         L     @01,ASCBTSB(,@14)                                02S0277
         L     @01,TSBEXTNT(,@01)                               02S0277
         TM    TSBXASCI(@01),B'10000000'                        02S0277
         BNO   @RF00277                                         02S0277
