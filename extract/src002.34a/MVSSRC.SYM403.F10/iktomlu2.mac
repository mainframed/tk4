         L     @14,DATACNT                                      02S0291
         L     @12,MAXRUSZO                                     02S0291
         CR    @14,@12                                          02S0291
         BNH   @RF00291                                         02S0291
*                     DO;                                       02S0292
*                       RPLRLEN=MAXRUSZO;/* SET LENGTH OF OUTPUT
