         TM    TOMEXBRU(TOMWAPTR),B'00000010'                   03S0321
         BNZ   @RF00321                                         03S0321
         TM    TVWABRIN(TVWAPTR),B'10000000'                    03S0321
         BNZ   @RF00321                                         03S0321
*                             DO;                               03S0322
*                               RPLEB='1'B;/* END BRACKET            */
         OI    RPLEB+16(RPLPTR),B'01000000'                     03S0323
*                               TOMEB='1'B;/* END BRACKET            */
*                               TOMEBSV='1'B;/* SAVE--END BRACKET
*                                                            @ZM20919*/
         OI    TOMEB(TOMWAPTR),B'01001000'                      03S0325
*                               TVWAINB='0'B;/* NO LONGER IN BRACKETS*/
         NI    TVWAINB(TVWAPTR),B'11111011'                     03S0326
*                             END;                              03S0327
*                           ELSE                                03S0328
*                             DO;                               03S0328
         B     @RC00321                                         03S0328
@RF00321 DS    0H                                               03S0329
*                               RPLEB='0'B;/* NO END BRACKET         */
         NI    RPLEB+16(RPLPTR),B'10111111'                     03S0329
*                               TOMEB='0'B;/* RESET END-BRACKET FLAG */
*                               TOMEBSV='0'B;/* SAVE--NO END BRACKET
*                                                            @ZM20919*/
         NI    TOMEB(TOMWAPTR),B'10110111'                      03S0331
*                             END;                              03S0332
*                         END BGNCHAIN;/* END OF BGNCHAIN            */
*                       ELSE        /* NO, NOT FIRST PASS            */
*                         DO;       /* MIDDLE-OF-CHAIN PROCEDSSING   */
         B     @RC00297                                         02S0334
@RF00297 DS    0H                                               02S0335
*                           TOMFMLO='0100'B;/* NO, SEND RU AS MIDDLE
*                                      CHAIN                         */
         LA    @14,16                                           02S0335
         ALR   @14,RPLPTR                                       02S0335
         NI    TOMFMLO(@14),B'01001111'                         02S0335
         OI    TOMFMLO(@14),B'01000000'                         02S0335
*                           RPLBB='0'B;/* SET RPL FOR CONTINUE  02S0336
