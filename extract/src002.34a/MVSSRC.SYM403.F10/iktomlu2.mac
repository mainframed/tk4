*                                                               01S0581
*BREAKIN:                                                       01S0581
*   PROCEDURE OPTIONS(SAVE(14));    /*                       @ZM20919*/
BREAKIN  ST    @14,@SA00008                                     01S0581
*   TOMESEND='1'B;                  /* EXIT FROM SEND LOOP           */
         OI    TOMESEND(TOMWAPTR),B'00010000'                   01S0582
*   IF TOMBRKMG='1'B&               /* TPUT BREAK MESSAGE?           */
*       TSBNOBRK='0'B THEN          /* BREAK TERMINAL?               */
         TM    TOMBRKMG(TOMWAPTR),B'10000000'                   01S0583
         BNO   @RF00583                                         01S0583
         L     @12,PSAAOLD                                      01S0583
         L     @12,ASCBTSB(,@12)                                01S0583
         TM    TSBNOBRK(@12),B'00000010'                        01S0583
         BNZ   @RF00583                                         01S0583
*     DO;                           /* DO BREAK-IN PROCESSING        */
*       TVWABRIN='1'B;              /* START BREAK-IN PROCESSING     */
         OI    TVWABRIN(TVWAPTR),B'10000000'                    01S0585
*       TVWALNCT=1;                 /* RESET LINE COUNT      @ZM20919*/
         MVI   TVWALNCT(TVWAPTR),X'01'                          01S0586
*       TVWAPGN='0'B;               /* RESET PAGING BIT      @ZM20919*/
         NI    TVWAPGN(TVWAPTR),B'10111111'                     01S0587
*       IF RPLFIRST='1'B×           /* BEGINNING OF CHAIN?           */
*           RPLMIDLE='1'B THEN      /* MIDDLE OF CHAIN?              */
         LA    @12,16                                           01S0588
         ALR   @12,RPLPTR                                       01S0588
         TM    RPLFIRST(@12),B'11000000'                        01S0588
         BZ    @RF00588                                         01S0588
*         DO;                       /* SET CHAIN ELEMENT TO EOC      */
*           RPLFIRST='0'B;          /* NOT BEGINNING OF CHAIN        */
*           RPLMIDLE='0'B;          /* NOT MIDDLE OF CHAIN           */
*           RPLLAST='1'B;           /* LAST OF CHAIN                 */
         OI    RPLLAST(@12),B'00100000'                         01S0592
         NI    RPLFIRST(@12),B'00111111'                        01S0592
*         END;                      /* CHAIN-ELEMENT SET TO EOC      */
*       ELSE                        /* END OF CHAIN OR ONLY CHAIN    */
*         DO;                       /* SET CHAIN ELEMENT TO OC       */
         B     @RC00588                                         01S0594
@RF00588 DS    0H                                               01S0595
*           RPLONLY='1'B;           /* ONLY CHAIN                    */
*           RPLLAST='0'B;           /* NOT END OF CHAIN              */
         OI    RPLONLY+16(RPLPTR),B'00010000'                   01S0596
         NI    RPLLAST+16(RPLPTR),B'11011111'                   01S0596
*         END;                      /* CHAIN ELEMENT SET TO OC       */
*       CALL SENDSIG(ADDR(RESTART2));/* SEND SIGNAL          @ZM20856*/
@RC00588 LA    @12,RESTART2                                     01S0598
         ST    @12,@AFTEMPS+4                                   01S0598
         LA    @12,@AFTEMPS+4                                   01S0598
         ST    @12,@AL00001                                     01S0598
         LA    @01,@AL00001                                     01S0598
         BAL   @14,SENDSIG                                      01S0598
*     END;                          /* END OF BREAK-IN PROCESSING    */
*   ELSE                            /* DO NOT BREAK IN               */
*     DO;                                                       01S0600
         B     @RC00583                                         01S0600
@RF00583 DS    0H                                               01S0601
*       TVWABFC='1'B;               /* BUFFER CONTENTION             */
         OI    TVWABFC(TVWAPTR),B'10000000'                     01S0601
*       TOMEMAIN='1'B;              /* EXIT MAINLINE LOOP            */
         OI    TOMEMAIN(TOMWAPTR),B'00100000'                   01S0602
*     END;                                                      01S0603
*   TOMBRKMG='0'B;                  /* RESET TOMBRKMG        @ZM20919*/
@RC00583 NI    TOMBRKMG(TOMWAPTR),B'01111111'                   01S0604
*   TOMCOLMN=0;                     /* RESET COLUMN COUNT    @ZM20919*/
         SLR   @12,@12                                          01S0605
         ST    @12,TOMCOLMN(,TOMWAPTR)                          01S0605
*   OPREQ=RESET;                    /* RESET MESSAGES        @ZM20919*/
         MVI   OPREQ(TOMWAPTR),X'07'                            01S0606
*   CALL IKTQMOUT(OPARMS);          /* CALL QUEUE MANAGER    @ZM20919*/
         LA    @12,OPARMS(,TOMWAPTR)                            01S0607
         ST    @12,@AL00001                                     01S0607
         L     @12,CVTPTR                                       01S0607
         L     @12,CVTTCASP(,@12)                               01S0607
         L     @15,TCASOQM(,@12)                                01S0607
         LA    @01,@AL00001                                     01S0607
         BALR  @14,@15                                          01S0607
*   END BREAKIN;                    /* END OF BREAKIN        @ZM20919*/
@EL00008 DS    0H                                               01S0608
