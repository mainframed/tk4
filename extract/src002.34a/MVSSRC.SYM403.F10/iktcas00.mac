         ST    TWAPTR,TCASWA(,@07)                                 0203
*      TCASXECB=ADDR(TWAVECB) ;        /* TERMINAL REQ ECB ADDR      */
         LA    @05,TWAVECB(,TWAPTR)                                0204
         ST    @05,TCASXECB(,@07)                                  0204
*    /*                                                              */
*    /*      PARAMETER  PROCESSING                                   */
*    /*                                                              */
*    /*        PARAMETER PROCESSING ROUTINE IS CALLED TO PROCESS     */
*    /*        TCAS PARAMETERS. THE PARAMETERS FROM CIB IF ANY IS    */
*    /*        PASSED TO P.P. ROUTINE, PARAMETERS FROM PARMLIB ARE   */
*    /*        READ, AND TCAS TABLE IS UPDATED ACCORDING TO THE      */
