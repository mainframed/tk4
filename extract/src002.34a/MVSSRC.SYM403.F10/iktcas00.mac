MLWAIT   DS    0H                                                  0096
*      GEN REFS(MLECBSA) ;                                         0096
         WAIT  ECBLIST=MLECBSA        WAIT ON ECB LIST
*      RFY (R14,R15,R0,R1) UNRSTD /*UNRESTRI PARA R */  ;          0097
*/*                                                                  */
*/*        THE FOLLOWING CODE CHECKS IF ANY SUBTASK ABENDED AND      */
*/*        ESTAE EXIT ROUTINE DID NOT COMPLETE THE WORK. IN THAT CASE*/
*/*        MAIN TASK IS TO BE ABENDED BY CALLING MAINLINE EOT        */
*/*        ROUTINE .                                                 */
*/*                                                                  */
*         R10=0 ;                      /* INIT R10 FOR CALLING EOT   */
         SLR   R10,R10                                             0098
*         R8=FCMLAT ;                  /* SET CODE TO ABEND          */
         LA    R8,146                                              0099
*      RFY ECB BASED(ADDR(TWAVCOMP)) ;                             0100
*      IF ECBPOSTB='1'B                /* IS VTAM INT SUBTASK ABEND  */
*         THEN DO ;                    /* YES                        */
         LA    @15,68                                              0101
         ALR   @15,TWAPTR                                          0101
         TM    ECBPOSTB(@15),B'01000000'                           0101
         BNO   @RF00101                                            0101
*            ECBPOSTB='0'B ;           /* ZERO ECB                   */
         NI    ECBPOSTB(@15),B'10111111'                           0103
*            R1=ADDR(TWAVTCB) ;        /* VTAM INT TASK TCB ADDR     */
         LA    R1,TWAVTCB(,TWAPTR)                                 0104
*            GEN ;                                                 0105
         DETACH (R1)      DETACH VTAM INT SUBTASK
*            TWAVFL1='0'B ;            /* RESET ATTACH BET           */
*            TWAVFL2='1'B ;            /* SET ABEND BIT ON           */
         OI    TWAVFL2(TWAPTR),B'01000000'                         0107
         NI    TWAVFL1(TWAPTR),B'01111111'                         0107
*            IF  TWAVFL4='0'B          /* DID ESTAE NOT COMPLETE     */
*               THEN DO ;              /* NO                         */
         TM    TWAVFL4(TWAPTR),B'00010000'                         0108
         BNZ   @RF00108                                            0108
*            R9=RCVABE ;               /* STORE ABEND REASON CODE    */
         LA    R9,48                                               0110
*            CALL MLEOT ;              /* CALL END OF TASK ROUTINE   */
         BAL   @14,MLEOT                                           0111
*            END ;                                                 0112
*            ELSE  TWAVFL4='0'B ;      /* RESET ESTAE COMP BIT       */
         B     @RC00108                                            0113
@RF00108 NI    TWAVFL4(TWAPTR),B'11101111'                         0113
*         END ;                                                    0114
@RC00108 DS    0H                                                  0115
*      RFY ECB BASED(ADDR(TWAUCOMP)) ;                             0115
@RF00101 DS    0H                                                  0116
*      IF ECBPOSTB='1'B                /* IS USER INT SUBTASK ABEND  */
*         THEN DO ;                    /* YES                        */
         LA    @15,72                                              0116
         ALR   @15,TWAPTR                                          0116
         TM    ECBPOSTB(@15),B'01000000'                           0116
         BNO   @RF00116                                            0116
*            ECBPOSTB='0'B ;           /* ZERO ECB                   */
         NI    ECBPOSTB(@15),B'10111111'                           0118
*            R1=ADDR(TWAUTCB) ;        /* USER IN SUBTASK TCB ADDR   */
         LA    R1,TWAUTCB(,TWAPTR)                                 0119
*            GEN ;                                                 0120
         DETACH (R1)      DETACH USER INT SUBTASK
*            TWAUFL1='0'B ;            /* RESET ATTACH BIT           */
*            TWAUFL2='1'B ;            /* SET ABEND BIT ON           */
         OI    TWAUFL2(TWAPTR),B'01000000'                         0122
         NI    TWAUFL1(TWAPTR),B'01111111'                         0122
*            IF  TWAUFL4='0'B          /* DID ESTAE NOT COMPLETE     */
*               THEN DO ;              /* NO                         */
         TM    TWAUFL4(TWAPTR),B'00010000'                         0123
         BNZ   @RF00123                                            0123
*                R9=RCUABE ;           /* SET ABEND REASON CODE      */
         LA    R9,52                                               0125
*                CALL  MLEOT ;         /* CALL EOT ROUTINE           */
         BAL   @14,MLEOT                                           0126
*                END ;                                             0127
*                ELSE TWAUFL4='0'B ;   /* RESET COMP BIT             */
         B     @RC00123                                            0128
@RF00123 NI    TWAUFL4(TWAPTR),B'11101111'                         0128
*         END ;                                                    0129
@RC00123 DS    0H                                                  0130
*      RFY ECB BASED(ADDR(TWACCOMP)) ;                             0130
@RF00116 DS    0H                                                  0131
*      IF ECBPOSTB='1'B                /* IS CON COMM SUBTASK ABEND  */
*         THEN DO ;                    /* YES                        */
         LA    @15,76                                              0131
         ALR   @15,TWAPTR                                          0131
         TM    ECBPOSTB(@15),B'01000000'                           0131
         BNO   @RF00131                                            0131
*            ECBPOSTB='0'B ;           /* ZERO ECB                   */
         NI    ECBPOSTB(@15),B'10111111'                           0133
*            R1=ADDR(TWACTCB) ;        /* CON COMM SUBTASK TCB ADDR  */
         LA    R1,TWACTCB(,TWAPTR)                                 0134
*            GEN ;                                                 0135
         DETACH (R1)        DETACH CON COMM SUBTASK
*            TWACFL1='0'B ;            /* RESET ATTACH BIT           */
*            TWACFL2='1'B ;            /* SET ABEND BIT ON           */
         OI    TWACFL2(TWAPTR),B'01000000'                         0137
         NI    TWACFL1(TWAPTR),B'01111111'                         0137
*            IF  TWACFL4='0'B          /* DID ESTAE NOT COMPLETE     */
*               THEN DO ;              /* NO                         */
         TM    TWACFL4(TWAPTR),B'00100000'                         0138
         BNZ   @RF00138                                            0138
*                R9=RCCABE ;           /* SET ABEND REASON CODE      */
         LA    R9,56                                               0140
*                CALL MLEOT ;          /* CALL EOT ROUTINE           */
         BAL   @14,MLEOT                                           0141
*                END ;                                             0142
*                ELSE TWACFL4='0'B ;   /* RESET ESTAE COMP BIT       */
         B     @RC00138                                            0143
@RF00138 NI    TWACFL4(TWAPTR),B'11011111'                         0143
*         END ;                                                    0144
@RC00138 DS    0H                                                  0145
*      TWAMECB='00'X  ;                     /* ZERO ECB              */
@RF00131 XC    TWAMECB+1(3,TWAPTR),TWAMECB+1(TWAPTR)               0145
         MVI   TWAMECB(TWAPTR),X'00'                               0145
*      GOTO  MLSLT    ;                     /* GOTO SELECT WE ROUTINE*/
         B     MLSLT                                               0146
*    /*                                                              */
*    /*      START COMMAND PROCESSING   - - -   FRONT  END           */
