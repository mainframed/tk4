*    /*                                                              */
*    /*      WAIT       ROUTINE                                      */
*    /*                                                              */
*MLSLT:                                     /* BEGIN SELECT WE ROUTIN*/
*      DO WHILE(TWASYNQH ^=0);              /* IS THERE WE           */
MLSLT    B     @DE00053                                            0053
@DL00053 DS    0H                                                  0054
*      RFY   WESTD BASED (TWASYNQH) ;       /* POINTS TO WE          */
*         R10=TWASYNQH ;               /* R10 PTR TO FIRST W.E.      */
         L     @15,TWASYNQH(,TWAPTR)                               0055
         LR    R10,@15                                             0055
*      IF WECODE1T=TCML                     /* IS WE FOR MAINLINE    */
*         THEN DO;                          /* YES                   */
         CLI   WECODE1T(@15),X'01'                                 0056
         BNE   @RF00056                                            0056
*         DO I=1 TO 8   BY 1                /* TRANSLATE FUNC CODE   */
*            WHILE(WECODE1F ^= FUNCODE(I)); /* TO ROUTINE NUMBER     */
         LA    I,1                                                 0058
@DL00058 L     @03,TWASYNQH(,TWAPTR)                               0058
         LA    @01,FUNCODE-1(I)                                    0058
         CLC   WECODE1F(1,@03),0(@01)                              0058
         BE    @DC00058                                            0058
*         END ;                                                    0059
         AL    I,@CF00062                                          0059
         C     I,@CF00060                                          0059
         BNH   @DL00058                                            0059
@DC00058 DS    0H                                                  0060
*         IF I > 0 & I< 8                   /* CHECK VALID ROUTINE   */
*            THEN DO;                       /* YES                   */
         LTR   I,I                                                 0060
         BNP   @RF00060                                            0060
         C     I,@CF00060                                          0060
         BNL   @RF00060                                            0060
*            MLWA1 =WECODE1 ;               /* RETAIN WE CODE        */
         L     @03,TWASYNQH(,TWAPTR)                               0062
         MVC   MLWA1(2),WECODE1(@03)                               0062
*            CALL  FUNCTION ;               /* CALL THE FUNCTION     */
         LR    @03,I                                               0063
         SLA   @03,2                                               0063
         L     @15,FUNADD-4(@03)                                   0063
         BALR  @14,@15                                             0063
*            IF  MLWA1 ^= WECODE1           /* IS SAME WE            */
*               THEN GOTO MLSLTEND;         /* NO PROCESS AGAIN      */
         L     @01,TWASYNQH(,TWAPTR)                               0064
         CLC   MLWA1(2),WECODE1(@01)                               0064
         BNE   @RT00064                                            0064
*            END ;                                                 0066
*         END ;                                                    0067
*         ELSE                              /* NOT FOR MAINLINE      */
*         IF WECODE1T=TCVI                  /* IS IT FOR VTAM INT    */
*            THEN DO;                       /* YES                   */
         B     @RC00056                                            0068
@RF00056 L     @01,TWASYNQH(,TWAPTR)                               0068
         CLI   WECODE1T(@01),X'02'                                 0068
         BNE   @RF00068                                            0068
*            R8=ADDR(TWAVECB);              /* GET ECB ADDR          */
         LA    R8,TWAVECB(,TWAPTR)                                 0070
*            RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/;     0071
*            GEN;                                                  0072
         POST (8)                      ISSUE POST
*            RFY (R14,R15,R0,R1) UNRSTD /*UNRESTRI PARA R */ ;     0073
*               GOTO MLWAIT ;               /* GOTO WAIT             */
         B     MLWAIT                                              0074
*            END;                                                  0075
*            ELSE                           /* NOT VTAM              */
*            IF WECODE1T=TCUI               /* IS IT FOR USER INT    */
*               THEN DO;                    /* YES                   */
@RF00068 L     @01,TWASYNQH(,TWAPTR)                               0076
         CLI   WECODE1T(@01),X'03'                                 0076
         BNE   @RF00076                                            0076
*               R8=ADDR(TWAUECB);           /* GET ECB ADDR          */
         LA    R8,TWAUECB(,TWAPTR)                                 0078
*               RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/ ; 0079
*               GEN ;                                              0080
         POST (8)                      ISSUE POST
*      RFY (R14,R15,R0,R1) UNRSTD /*UNRESTRI PARA R */ ;           0081
*               GOTO MLWAIT ;               /* GOTO WAIT             */
         B     MLWAIT                                              0082
*               END;                                               0083
*               ELSE                        /* NOT USER              */
*               IF WECODE1T=TCCI            /* IS IT FOR CON COMM    */
*                  THEN DO;                 /* YES                   */
@RF00076 L     @01,TWASYNQH(,TWAPTR)                               0084
         CLI   WECODE1T(@01),X'04'                                 0084
         BNE   @RF00084                                            0084
*                  R8=ADDR(TWACECB);        /* GET ECB ADDR          */
         LA    R8,TWACECB(,TWAPTR)                                 0086
*                  RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/ ;
*                  GEN ;                                           0088
         POST (8)                      ISSUE POST
*      RFY (R14,R15,R0,R1) UNRSTD /*UNRESTRI PARA R */ ;           0089
