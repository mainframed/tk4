*MLSCPF:     PROC OPTIONS(NOSAVE,NOSAVEAREA);                      0147
MLSCPF   DS    0H                                                  0148
*      MLSAVE1=R14;                         /* SAVE RETURN ADDR      */
         LR    MLSAVE1,R14                                         0148
*/*                                                                  */
*/*          VALIDITY  CHECK  OF START COMMAND                       */
*/*                                                                  */
*      RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/  ;         0149
*      GEN CODE REFS(PSALITA) ;                                    0150
         MODESET KEY=ZERO,MODE=SUP  GET INTO SUPV STATE KEY ZERO
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,              CC
               RELATED=('CHECK TCAST')
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,                CC
               RELATED=('CHECK TCAST')
*      RFY (R14,R15,R0,R1) UNRSTD /*UNRESTRI PARA R */  ;          0151
*      ASCBPTR=PSAAOLD;                     /* ESTABLISH ASCB ADDR   */
         L     ASCBPTR,PSAAOLD                                     0152
*      CSCBPTR=ASCBCSCB;                    /* GET CSCB ADDR         */
         L     @15,ASCBCSCB(,ASCBPTR)                              0153
         ST    @15,CSCBPTR                                         0153
*      IF CVTTCASP=0                        /* IS TCAST POINTER ZERO */
*         THEN MLNOTB='1'B;                 /* INDICATE NO TCAS TABLE*/
         L     @01,CVTPTR                                          0154
         L     @15,CVTTCASP(,@01)                                  0154
         LTR   @15,@15                                             0154
         BNZ   @RF00154                                            0154
         OI    MLNOTB,B'10000000'                                  0155
*         ELSE DO;                          /* THERE IS TABLE        */
         B     @RC00154                                            0156
@RF00154 DS    0H                                                  0157
*         TCASTPTR=CVTTCASP;                /* TCAST POINTER         */
         L     @01,CVTPTR                                          0157
         L     @15,CVTTCASP(,@01)                                  0157
         ST    @15,TCASTPTR                                        0157
*         IF TCASNACT='1'B                  /* IS TCAS ACTIVE        */
*            THEN DO;                       /* NO                    */
         TM    TCASNACT(@15),B'00001000'                           0158
         BNO   @RF00158                                            0158
*            TCASNACT='0'B;                 /* INDICATE ACTIVE       */
*            TCASABND='0'B ;                /* CLEAR ABEND   @YM03216*/
         NI    TCASNACT(@15),B'11010111'                           0161
*            TCASASCB=ASCBPTR;              /* STORE ASCB ADDRESS    */
         ST    ASCBPTR,TCASASCB(,@15)                              0162
*            END;                                                  0163
*            ELSE DO;                       /* YES TCAS ACTIVE       */
         B     @RC00158                                            0164
@RF00158 DS    0H                                                  0165
*      RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/  ;         0165
*      GEN CODE REFS(PSALITA) ;                                    0166
         SETLOCK RELEASE,TYPE=CMS,REGS=USE,                           CC
               RELATED=('CHECK TCAST')
         SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,                         CC
               RELATED=('CHECK TCAST')
         MODESET KEY=NZERO,MODE=PROB  BACK TO ORIGINAL
*      RFY (R14,R15,R0,R1) UNRSTD /*UNRESTRI PARA R */  ;          0167
*            R10=0 ;                        /* NO WE                 */
         SLR   R10,R10                                             0168
*            TWAPTR=0 ;                /* INDICATE NO TWA            */
         SLR   TWAPTR,TWAPTR                                       0169
*            R9=RCINVS  ;                   /* INVALID START COMM    */
         LA    R9,4                                                0170
*            R8=FCMLET ;                    /* NORMAL END            */
         LA    R8,145                                              0171
*            CALL MLEOT;                    /* CALL END OF TASK      */
         BAL   @14,MLEOT                                           0172
*            END;                                                  0173
*         END;                                                     0174
@RC00158 DS    0H                                                  0175
*      RFY (R14,R15,R0,R1) RSTD  /* RESTRICT PARA REG*/  ;         0175
@RC00154 DS    0H                                                  0176
*      GEN CODE REFS(PSALITA) ;                                    0176
         SETLOCK RELEASE,TYPE=CMS,REGS=USE,                           CC
               RELATED=('CHECK TCAST')
         SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,                         CC
               RELATED=('CHECK TCAST')
         MODESET KEY=NZERO,MODE=PROB  BACK TO ORIGINAL
*********************************************************************
