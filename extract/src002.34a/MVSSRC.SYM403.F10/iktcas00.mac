*    /*                                                              */
*    /*                                                              */
*      MLPMPML=MLPMPML && MLPMPML;          /* ZERO PARAM AREA       */
         XC    MLPMPML(8),MLPMPML                                  0205
*      IF CHCIBP ^=0                        /* IS THERE CIB          */
*         THEN DO;                          /* YES                   */
         L     @07,CSCBPTR                                         0206
         L     @07,CHCIBP(,@07)                                    0206
         LTR   @07,@07                                             0206
         BZ    @RF00206                                            0206
