*         LOAD TCAS INITIALIZATION ROUTINE AND BRANCH TO IT         *
*         TO INITIALIZE TWA. ON RETURN R1 POINTS TO TWA             *
*                                                                   *
*********************************************************************
      LOAD  EP=IKTCAS51                    /* LOAD TCAS INIT ROUTINE*/
         LR    15,0                    BRANCH TO TCAS INIT ROUTINE
         BALR  14,15
*         TWAPTR=R1 ;                  /* STORE TWA ADDRESS          */
         LR    TWAPTR,R1                                           0177
