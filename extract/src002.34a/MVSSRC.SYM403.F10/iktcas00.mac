         LR    R9,R15                                              0191
*         IF R9 ^=0                         /* CHECK RETURN CODE     */
*            THEN DO;                       /* NOT ZERO              */
         LTR   R9,R9                                               0192
         BZ    @RF00192                                            0192
*            IF R9=RCINVS                   /* IS INVALID START COMM */
*               THEN R8=FCMLET ;            /* YES GOTO END OF TASK  */
         C     R9,@CF00168                                         0194
         BNE   @RF00194                                            0194
         LA    R8,145                                              0195
*               ELSE R8=FCMLAT ;            /* NO  ABEND             */
         B     @RC00194                                            0196
@RF00194 LA    R8,146                                              0196
*            R10=0 ;                        /* NO WE                 */
@RC00194 SLR   R10,R10                                             0197
*            CALL MLEOT ;                   /* CALL END OF TASK      */
         BAL   @14,MLEOT                                           0198
*            END ;                                                 0199
