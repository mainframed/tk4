         ST    ASCBPTR,TWAASCB(,TWAPTR)                            0179
*      TWACSCB=CSCBPTR ;                    /* STORE CSCB PTR IN TWA */
         L     @07,CSCBPTR                                         0180
         ST    @07,TWACSCB(,TWAPTR)                                0180
*      MLQECBA=ADDR(TWAMECB) ;         /* MAIN TASK ECB ADDR         */
         LA    @07,TWAMECB(,TWAPTR)                                0181
         ST    @07,MLQECBA                                         0181
*      MLVECBA=ADDR(TWAVCOMP) ;        /* VTAM INT SUBTASK COMP ECB  */
         LA    @07,TWAVCOMP(,TWAPTR)                               0182
         ST    @07,MLVECBA                                         0182
*      MLUECBA=ADDR(TWAUCOMP) ;        /* USER INT SUBTASK COMP ECB  */
         LA    @07,TWAUCOMP(,TWAPTR)                               0183
         ST    @07,MLUECBA                                         0183
*      MLCECBA=ADDR(TWACCOMP) ;        /* CON COOM SUBTASK COMP ECB  */
         LA    @07,TWACCOMP(,TWAPTR)                               0184
         STCM  @07,7,MLCECBA                                       0184
*    /*                                                              */
