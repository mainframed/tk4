*      MLPMRLIB='1'B ;                      /* INDICATE READ LIB     */
@RF00206 DS    0H                                                  0213
*      MLPMUPTB='1'B ;                      /* UPDATE TCAST          */
*      MLPMWPM ='1'B ;                      /* WRITE OUTPUT          */
         OI    MLPMRLIB,B'00011100'                                0214
*      R9=ADDR(MLPMPML);                    /* PARAMETER REG         */
         LA    R9,MLPMPML                                          0215
*      CALL  IKTCAS54  ;                    /* CALL PARM PROC ROUTINE*/
         L     @15,TWAPPSR(,TWAPTR)                                0216
         BALR  @14,@15                                             0216
*      DO  WHILE(MLPMINV='1'B);             /* INVALID PARM PASSED   */
         B     @DE00217                                            0217
@DL00217 DS    0H                                                  0218
*         RFY  R1  RSTD ;                                          0218
*         R1=4 ;                            /* MSG  4                */
         LA    R1,4                                                0219
*         CALL MSGBLK ;                     /* CALL MSG BLOCK GET M  */
         L     @15,TWAMSG(,TWAPTR)                                 0220
         BALR  @14,@15                                             0220
