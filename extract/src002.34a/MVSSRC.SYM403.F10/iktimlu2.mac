*LUSTATR:  PROCEDURE OPTIONS(SAVE(RTNREG));                        0313
*                                                                  0313
LUSTATR  ST    @14,@SA00005                                        0313
*  /******************************************************************/
*  /*                                                                */
*  /* (A) IF COMPONENT AVAILABLE, SCHEDULE TOM AND SEND RESPONSE.    */
*  /* (B) IF COMPONENT FAILURE, SEND RESPONSE AND CALL IKTEXIT.      */
*  /* (C) IF INTEGRITY LOST, TURN ON INPUT-ERROR MESSAGE AND         */
*  /*     FORMATTING-ERASE-WRITE FLAGS, SCHEDULE TOM, AND SEND       */
*  /*     RESPONSE.                                                  */
*  /* (D) IF NONE OF THE ABOVE, INCREMENT ERROR-RECEIVE COUNT BY 1   */
*  /*     AND SEND RESPONSE.                                         */
*  /*                                                                */
*  /******************************************************************/
*                                                                  0314
*  IF RPLSSNSI = '0001'X THEN          /* COMPONENT AVAILABLE?       */
         L     @12,RPLPTR                                          0314
         CLC   RPLSSNSI(2,@12),@CB02740                            0314
         BNE   @RF00314                                            0314
*    TIMSCHTM='1'B;                    /* SCHEDULE TOM       @ZM20816*/
         L     @12,TIMGETMN                                        0315
         OI    TIMSCHTM(@12),B'01000000'                           0315
*  ELSE                                                            0316
*    IF RPLSSNSI = '081C'X THEN        /* COMPONENT FAILURE?         */
         B     @RC00314                                            0316
@RF00314 L     @12,RPLPTR                                          0316
         CLC   RPLSSNSI(2,@12),@CB02741                            0316
         BNE   @RF00316                                            0316
*      DO;                             /* PREPARE FOR PERMANENT EXIT */
*        RPLEX='0'B;                   /* RESET RPLEX        @ZM20759*/
         NI    RPLEX+16(@12),B'11111011'                           0318
*        CALL SENDRESP;                /* SEND RESPONSE              */
         BAL   @14,SENDRESP                                        0319
*        CALL EXITFRR;                 /* CALL ABEND ROUTINE         */
         BAL   @14,EXITFRR                                         0320
*      END;                            /* END OF CLEAN-UP.           */
*    ELSE                                                          0322
*      IF RPLSSNSI ^= '082B'X THEN     /* NOT INTEGRITY LOST?@ZM20274*/
         B     @RC00316                                            0322
@RF00316 L     @12,RPLPTR                                          0322
         CLC   RPLSSNSI(2,@12),@CB02742                            0322
         BE    @RF00322                                            0322
*        RECERCNT = RECERCNT + 1;      /* UP ERROR-RECEIVE COUNT     */
         L     @12,TIMGETMN                                        0323
         LA    @07,1                                               0323
         AL    @07,RECERCNT(,@12)                                  0323
         ST    @07,RECERCNT(,@12)                                  0323
*      ELSE                            /* INTEGRITY LOST     @ZM20816*/
*        TIMSCHTM='1'B;                /* SCHEDULE IKTOMLU2  @ZM20816*/
*                                                                  0324
         B     @RC00322                                            0324
@RF00322 L     @12,TIMGETMN                                        0324
         OI    TIMSCHTM(@12),B'01000000'                           0324
*  RPLEX='0'B;                         /* RESET RPLEX        @ZM20759*/
@RC00322 DS    0H                                                  0325
@RC00316 DS    0H                                                  0325
@RC00314 L     @12,RPLPTR                                          0325
         NI    RPLEX+16(@12),B'11111011'                           0325
*  CALL SENDRESP;                      /* SEND RESPONSE              */
         BAL   @14,SENDRESP                                        0326
*  IF RECERCNT>=3 THEN                 /* CHECK RECEIVE COUNT@ZM21205*/
         L     @12,TIMGETMN                                        0327
         L     @12,RECERCNT(,@12)                                  0327
         C     @12,@CF00290                                        0327
         BL    @RF00327                                            0327
*    CALL EXITFRR;                     /*CALL ABEND ROUTINE  @ZM21205*/
         BAL   @14,EXITFRR                                         0328
*  ELSE;                               /* NOT ABEND SITUATION@ZM21205*/
@RF00327 DS    0H                                                  0330
*  IF TIMSCHTM='1'B THEN               /* SCHEDULE IKTOMLU2? @ZM20816*/
         L     @12,TIMGETMN                                        0330
         TM    TIMSCHTM(@12),B'01000000'                           0330
         BNO   @RF00330                                            0330
*    CALL SCHTOM;                      /* SCHEDULE IKTOMLU2  @ZM20816*/
         BAL   @14,SCHTOM                                          0331
*  ELSE;                               /* DO NOT SCHEDULE TOM@ZM20816*/
@RF00330 DS    0H                                                  0333
*END LUSTATR;                          /* END OF LUSTATR             */
@EL00005 DS    0H                                                  0333
@EF00005 DS    0H                                                  0333
@ER00005 L     @14,@SA00005                                        0333
         BR    @14                                                 0333
         EJECT
*SENDRESP:  PROCEDURE OPTIONS(SAVE(RTNREG));                       0334
*                                                                  0334
SENDRESP ST    @14,@SA00006                                        0334
*  /******************************************************************/
*  /*  THIS ROUTINE WILL RECEIVE CONTROL WHEN A RESPONSE IS REQUESTED*/
*  /*  FROM THE TERMINAL. IT WILL BE CALLED WHEN AN EXCEPTION MESSAGE*/
*  /*  HAS BEEN RECEIVED, AND WHEN DFC CANCEL OR LUSTAT IS RECEIVED  */
*  /******************************************************************/
*                                                                  0335
*  MPSTSOIR='0'B;                      /* RESET IWAIT SYSEVENT REQUEST
*                                                            @ZM21354*/
*  MPSTSOIW='0'B;                      /* RESET IWAIT SYSEVENT     0336
*                                                            @ZM21354*/
         L     @12,PSAAOLD                                         0336
         L     @12,ASCBASXB(,@12)                                  0336
         L     @12,ASXBMPST(,@12)                                  0336
         NI    MPSTSOIR(@12),B'00111111'                           0336
*  RPLSRESP='1'B;                      /* SEND ERROR RESPONSE        */
         L     @12,RPLPTR                                          0337
         LA    @07,16                                              0337
         ALR   @07,@12                                             0337
         OI    RPLSRESP(@07),B'10000000'                           0337
*  RPLNFME = '0'B;                     /* RESET POSITIVE-FME FLAG    */
*  RPLRRN='0'B;                        /* RESET RPLRRN       @ZM20759*/
         NI    RPLNFME(@07),B'11111100'                            0339
*  RFY (REG1,REG2,REG3,REG7) RSTD;     /* RESTRICT WORK REGISTERS    */
*  REG7=ADDR(IKTIOFRR);                /* ADDRESS OF FRR             */
         L     @15,CVTPTR                                          0341
         L     @15,CVTTCASP(,@15)                                  0341
         L     REG7,TCASFRR(,@15)                                  0341
*  REG2=ADDR(IFGRPL);                  /* ADDRESS OF RPL FOR SEND    */
         LR    REG2,@12                                            0342
*  GEN REFS(REG1,REG2,REG3,REG7,FRRSCURR,FRRS,FRRSLAST,PSALITA,FLC,
*           PSALSFCC,PSA,FRRSELEN,PSACSTK,FRRSFRRA,                0343
*           FRRSENTR,FRRSPARM) SETS(REG0,REG1,REG2,REG14,REG15);   0343
    SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SDRP),REGS=USE
    SEND RPL=(REG2)
    SETFRR A,FRRAD=(REG7),WRKREGS=(REG1,REG2),PARMAD=(REG3)
    SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(SDRP),REGS=USE
*  /******************************************************************/
*  /* INITIALIZE FRR PARAMETER WORK AREA                             */
*  /******************************************************************/
*                                                                  0344
*  PARMPTR=(REG3);                     /* ADDRESS OF PARM AREA       */
         ST    REG3,PARMPTR                                        0344
*  RFY (REG1,REG2,REG3,REG7) UNRSTD;   /* FREE WORK REGISTERS        */
*  CALL INITFRR;                       /* CALL FRR INITIALIZE      0346
*                                         ROUTINE            @ZM20856*/
         BAL   @14,INITFRR                                         0346
*  RPLEX='0'B;                         /* RESET FIELDS IN RPL        */
         LA    @12,16                                              0347
         AL    @12,RPLPTR                                          0347
         NI    RPLEX(@12),B'11111011'                              0347
*  RPLSRESP='0'B;                      /* RESET SEND-RESPONSE FLAG   */
*                                                                  0348
         NI    RPLSRESP(@12),B'01111111'                           0348
*END SENDRESP;                         /* END OF SENDRESP            */
@EL00006 DS    0H                                                  0349
@EF00006 DS    0H                                                  0349
@ER00006 L     @14,@SA00006                                        0349
         BR    @14                                                 0349
         EJECT
*/********************************************************************/
*/*     THE FOLLOWING CODE BUILDS AND INITIALIZES THE RPL FOR THE    */
*/*     VTAM RECEIVE MACRO AND WILL BE THE PRIMARY RESTARTING POINT  */
*/*     FOR IKTIMLU2 IN CASE OF FAILURE DUE TO AN ERROR CONDITON.    */
*/********************************************************************/
*PRIMPROC:                                                         0350
*  PROCEDURE OPTIONS(SAVE(RTNREG));                                0350
PRIMPROC ST    @14,@SA00007                                        0350
*  DCL DUMMYRPL CHAR(LENGTH(IFGRPL)) STATIC LOCAL GENERATED;       0351
