         EJECT
* ASCITRAN: PROCEDURE OPTIONS(SAVE(RTNREG));                       0550
*                                                                  0550
         B     @PB00011                                            0550
ASCITRAN ST    @14,12(,@13)                                        0550
*   /*****************************************************************/
*   /* PERFORM ASCII TRANSLATION.                                    */
*   /*****************************************************************/
*                                                                  0551
*   SCANPTR = TVWAGMPT + 10;                /* SET SCAN AREA POINTER */
         L     @12,TVWAPTR                                         0551
         L     @12,TVWAGMPT(,@12)                                  0551
         LA    @07,10                                              0551
         ALR   @07,@12                                             0551
         ST    @07,SCANPTR                                         0551
*                                           /* 10 =  4 GTMNLEN       */
*                                           /*      +4 DATALEN       */
*                                           /*      +2 RSV-TOM       */
*   COUNT = DATALEN;                        /* SET TRANSLATION COUNT */
         L     @07,TIMGETMN                                        0552
         L     @12,DATALEN+4(,@12)                                 0552
         ST    @12,COUNT(,@07)                                     0552
*   DONE = '0'B;                            /* RESET DO-WHILE FLAG   */
         NI    DONE(@07),B'11111011'                               0553
*   DO WHILE (DONE = '0'B);                                        0554
         B     @DE00554                                            0554
@DL00554 DS    0H                                                  0555
*     IF COUNT > 256 THEN                   /* COUNT > 256?          */
         LA    @12,256                                             0555
         L     @07,TIMGETMN                                        0555
         L     @15,COUNT(,@07)                                     0555
         CR    @15,@12                                             0555
         BNH   @RF00555                                            0555
*       DO;                                                        0556
*         VALUE = 256;                      /* AMOUNT TO TRANSLATE   */
         ST    @12,VALUE(,@07)                                     0557
*         COUNT = COUNT - 256;              /* NO. LEFT TO TRANSLATE */
         SLR   @15,@12                                             0558
         ST    @15,COUNT(,@07)                                     0558
*       END;                                                       0559
*     ELSE                                  /* COUNT <= 256          */
*       DO;                                                        0560
         B     @RC00555                                            0560
@RF00555 DS    0H                                                  0561
*         VALUE = COUNT;                    /* AMOUNT TO TRANSLATE   */
         L     @12,TIMGETMN                                        0561
         L     @07,COUNT(,@12)                                     0561
         ST    @07,VALUE(,@12)                                     0561
*         DONE = '1'B;                      /* NO DATA LEFT          */
         OI    DONE(@12),B'00000100'                               0562
*       END;                                                       0563
*     TR(SCANAREA(1:VALUE),ASCIITAB);       /* TRANSLATE DATA        */
@RC00555 L     @12,SCANPTR                                         0564
         L     @07,TIMGETMN                                        0564
         L     @07,VALUE(,@07)                                     0564
         BCTR  @07,0                                               0564
         L     @01,TVWAPTR                                         0564
         L     @01,TVWAATBI(,@01)                                  0564
         EX    @07,@SB02829                                        0564
*     SCANPTR = SCANPTR + VALUE;            /* UPDATE SCAN-AREA PTR  */
         L     @07,TIMGETMN                                        0565
         AL    @12,VALUE(,@07)                                     0565
         ST    @12,SCANPTR                                         0565
*   END;                                                           0566
*                                                                  0566
@DE00554 L     @12,TIMGETMN                                        0566
         TM    DONE(@12),B'00000100'                               0566
         BZ    @DL00554                                            0566
* END ASCITRAN;                             /* END OF ASCITRAN       */
@EL00011 DS    0H                                                  0567
@EF00011 DS    0H                                                  0567
@ER00011 L     @14,12(,@13)                                        0567
         BR    @14                                                 0567
         EJECT
* GETAREAR:  PROCEDURE OPTIONS(SAVE(RTNREG));                      0568
*                                                                  0568
GETAREAR ST    @14,@SA00012                                        0568
*   /*****************************************************************/
*   /* GETMAIN AREA FOR CONTENTS OF TERMINAL BUFFER.                 */
*   /*****************************************************************/
*                                                                  0569
*   RFY (REG1,REG7,REG9,REG10) RSTD;                               0569
*   IF RPLONLY = '1'B THEN            /* ONLY CHAIN?                 */
         L     @12,RPLPTR                                          0570
         TM    RPLONLY+16(@12),B'00010000'                         0570
         BNO   @RF00570                                            0570
*     REG10 = RPLRLEN + 14;           /* GETMAIN LENGTH              */
         LA    REG10,14                                            0571
         AL    REG10,RPLRLEN(,@12)                                 0571
*                                     /* 14 =  4 GTMNLEN             */
*                                     /*      +4 DATALEN             */
*                                     /*      +6 RSV FOR IKTOMLU2    */
*   ELSE                              /* MULTIPLE-RU CHAIN           */
*                                                                  0572
*     /***************************************************************/
*     /* FOR EACH ATTRIBUTE IN THE BUFFER, 2 BYTES (SF + ATTRIBUTE)  */
*     /* WILL BE PASSED IN THE RECEIVE.                              */
*     /* A POSSIBLE READ HEADING WILL REQUIRE 3 BYTES.               */
*     /* GTMNLEN REQUIRES 4 BYTES.                                   */
*     /* DATALEN REQUIRES 4 BYTES.                                   */
*     /* 6 BYTES WILL BE RESERVED FOR IKTOMLU2.                      */
*     /* THEREFORE, THE MAXIMUM NUMBER OF BYTES REQUIRED WILL BE     */
*     /* 2*TSBXTMBF + 17.                                            */
*     /***************************************************************/
*                                                                  0572
*     REG10 = 2*TSBXTMBF + 17;        /* GETMAIN LENGTH              */
*                                                                  0572
         B     @RC00570                                            0572
@RF00570 L     @12,PSAAOLD                                         0572
         L     @12,ASCBTSB(,@12)                                   0572
         L     @12,TSBEXTNT(,@12)                                  0572
         LH    REG10,TSBXTMBF(,@12)                                0572
         ALR   REG10,REG10                                         0572
         AL    REG10,@CF02778                                      0572
*   REG9 = WRKSPC;                    /* SAVE COMPILER WORK ADDRESS  */
@RC00570 LR    REG9,WRKSPC                                         0573
*   REG4 = TVWATCB;                   /* ADDRESS OF TCB              */
         L     @12,TVWAPTR                                         0574
         L     REG4,TVWATCB(,@12)                                  0574
*   REG7 = PSAAOLD;                   /* ADDRESS OF ASCB             */
*                                                                  0575
         L     REG7,PSAAOLD                                        0575
*   GEN REFS(REG4,REG7,REG10,CVTMAP,CVTCRMN,CVTPTR)                0576
*       SETS(REG0,REG1,REG3,REG14,REG15,REG13);                    0576
*                                                                  0576
    GETMAIN RC,LV=(REG10),SP=229,BRANCH=YES
*   WRKSPC = REG9;                    /* RESTORE COMPILER WORK ADDR  */
         LR    WRKSPC,REG9                                         0577
*   RFY (REG7,REG9) UNRSTD;           /* FREE WORK REGISTERS         */
*   RFY REG15 RSTD;                   /* RESTRICT CODE REGISTER      */
*   IF REG15 = 0 THEN                 /* GETMAIN SUCCESSFUL?         */
         LTR   REG15,REG15                                         0580
         BNZ   @RF00580                                            0580
*     DO;                             /* PROCESS GETMAINED AREA      */
*       TVWAGMPT = REG1;              /* ADDRESS OF GETMAINED AREA   */
         L     @14,TVWAPTR                                         0582
         ST    REG1,TVWAGMPT(,@14)                                 0582
*       TEMPBFPT = REG1 + 10;         /* POINTER TO DATA AREA        */
         LA    @14,10                                              0583
         ALR   @14,REG1                                            0583
         L     @12,TIMGETMN                                        0583
         ST    @14,TEMPBFPT(,@12)                                  0583
*       GTMNLEN = REG10;              /* GETMAINED-AREA LENGTH       */
         L     @14,TVWAPTR                                         0584
         L     @14,TVWAGMPT(,@14)                                  0584
         ST    REG10,GTMNLEN(,@14)                                 0584
*     END;                            /* END OF AREA PROCESSING      */
*   ELSE                              /* GETMAIN NOT SUCCESSFUL      */
*     TVWAERMG = '1'B;                /* INPUT-ERROR MESSAGE         */
         B     @RC00580                                            0586
@RF00580 L     @14,TVWAPTR                                         0586
         OI    TVWAERMG(@14),B'00001000'                           0586
*   RFY (REG10,REG15) UNRSTD;         /* FREE REGISTERS 10 AND 15    */
*                                                                  0587
*END GETAREAR;                        /* END OF GETAREAR             */
@EL00012 DS    0H                                                  0588
@EF00012 DS    0H                                                  0588
@ER00012 L     @14,@SA00012                                        0588
         BR    @14                                                 0588
         EJECT
*END DATARDBF;                        /* END OF DATARDBF             */
@EL00010 DS    0H                                                  0589
@EF00010 DS    0H                                                  0589
@ER00010 L     @14,@SA00010                                        0589
         BR    @14                                                 0589
