VTAMERR  ST    @14,@SA00013                                        0590
*  /******************************************************************/
*  /* CHECK IMMEDIATE RETURN CODES                           @ZM20858*/
*  /******************************************************************/
*  RFY (REG15,REG0) RSTD;         /* RESTRICT R15,R0         @ZM20858*/
*  IF REG15=32 ×                  /* RPL INVALID OR          @ZM20858*/
*    (REG15=4 & REG0=24) THEN     /* VTAM NOT ACTIVE         @ZM20858*/
         C     REG15,@CF00774                                      0592
         BE    @RT00592                                            0592
         C     REG15,@CF00055                                      0592
         BNE   @RF00592                                            0592
         C     REG0,@CF00072                                       0592
         BNE   @RF00592                                            0592
@RT00592 DS    0H                                                  0593
*    IF RECERCNT>=3 THEN          /* RETRY COUNT EXCEEDED    @ZM20858*/
         L     @14,TIMGETMN                                        0593
         L     @14,RECERCNT(,@14)                                  0593
         C     @14,@CF00290                                        0593
         BL    @RF00593                                            0593
*      DO;                        /* PREPARE FOR ABEND       @ZM20858*/
*        RFY (REG0,REG1,REG15) UNRSTD; /*FREE REGS 0,1,15    @ZM20856*/
