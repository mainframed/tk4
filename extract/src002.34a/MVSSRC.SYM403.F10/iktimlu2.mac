*/********************************************************************/
*/*    THIS ROUTINE WILL CHECK TO SEE IF IKTOMLU2 IS STILL           */
*/*    AVAILABLE TO BE SHCEDULED.  THE TVWATIS FLAG IS 1 WHEN        */
*/*    IKTOMLU2 IS EITHER RUNNING OR SCHEDULED.  TVWATAS MEANS       */
*/*    THAT ONLY IKTIMLU2 CAN SCHEDULE IKTOMLU2.                     */
*/********************************************************************/
*                                                                  0364
*SCHTOM: PROCEDURE OPTIONS(SAVE(RTNREG));                          0364
SCHTOM   ST    @14,@SA00008                                        0364
*  TVWATAS = '0'B;                     /* IKTOMLU2 AVAILABLE @ZM20546*/
         L     @12,TVWAPTR                                         0365
         NI    TVWATAS(@12),B'11011111'                            0365
*  IF TVWATIS='0'B THEN                /* IKTOMLU2 NOT SCHEDULED     */
         TM    TVWATIS(@12),B'01000000'                            0366
         BNZ   @RF00366                                            0366
*    DO;                               /* SCHEDULE IKTOMLU2          */
*      RFY REG2 RSTD;                  /* RESTRICT REGISTER 2        */
*      REG2=TSBXSRB;                   /* ADDRESS OF SRB TO FREE     */
         L     @12,PSAAOLD                                         0369
         L     @12,ASCBTSB(,@12)                                   0369
         L     @12,TSBEXTNT(,@12)                                  0369
         L     REG2,TSBXSRB(,@12)                                  0369
*      GEN(SCHEDULE SRB=(REG2),SCOPE=LOCAL)                        0370
*              REFS(CVTPTR,CVTLSMQ,CVTMAP,SRBFLNK,SRBSECT);        0370
         SCHEDULE SRB=(REG2),SCOPE=LOCAL
*      TVWATIS='1'B;                   /* SET TOM SCHEDULED ON       */
         L     @12,TVWAPTR                                         0371
         OI    TVWATIS(@12),B'01000000'                            0371
*      RFY REG2 UNRSTD;                /* FREE REGISTER 2            */
*    END;                              /* IKTOMLU2 IS SCHEDULED      */
*  ELSE;                               /* IKTOMLU2 HAS BEEN SCHEDULED*/
@RF00366 DS    0H                                                  0375
*  TIMSCHTM = '0'B;                    /* IKTOMLU2 SCHEDULED         */
         L     @12,TIMGETMN                                        0375
         NI    TIMSCHTM(@12),B'10111111'                           0375
*  END SCHTOM;                         /* END OF SCHTOM              */
@EL00008 DS    0H                                                  0376
@EF00008 DS    0H                                                  0376
@ER00008 L     @14,@SA00008                                        0376
         BR    @14                                                 0376
         EJECT
*/********************************************************************/
*/*    THIS ROUTINE WILL RECEIVE CONTROL WHEN THE RECEIVE MACRO      */
*/*    IS SATISFIED WITH DATA.                                       */
*/********************************************************************/
*                                                                  0377
*DATARTN:                                                          0377
*  PROCEDURE;                                                      0377
DATARTN  STM   @14,@12,@SA00009                                    0377
*   DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/      0378
*CKMC0004:                                                         0379
*     STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */  0379
CKMC0004 L     @12,TVWAPTR                                         0379
         L     @12,TVWAFRWI(,@12)                                  0379
         STM   REG0,REG15,FRWACKRG(@12)                            0379
*     FRWACKPT(2)=ADDR(CKMC0004); /* SAVE ADDRESS    @ZM20856 */   0380
         LA    @07,CKMC0004                                        0380
         ST    @07,FRWACKPT+4(,@12)                                0380
*   END;                /* CHECK POINT        @ZM20856*/           0381
*  RECERCNT=0;                         /* INITIALIZE ERROR COUNT     */
         L     @12,TIMGETMN                                        0382
         SLR   @07,@07                                             0382
         ST    @07,RECERCNT(,@12)                                  0382
*  IF TSBNOBRK = '0'B THEN             /* BREAK MODE?                */
         L     @07,PSAAOLD                                         0383
         L     @07,ASCBTSB(,@07)                                   0383
         TM    TSBNOBRK(@07),B'00000010'                           0383
         BNZ   @RF00383                                            0383
*    TIMSCHTM = '1'B;                  /* SCHEDULE IKTOMLU2          */
         OI    TIMSCHTM(@12),B'01000000'                           0384
*  ELSE;                               /* NOT BREAK MODE             */
*                                                                  0385
@RF00383 DS    0H                                                  0386
*  /******************************************************************/
*  /* THE FIRST CHECK MADE IS TO SEE IF THE RECEIVE DONE WAS THE     */
*  /* SECOND RECEIVE NEEDED TO COMPLETE THE TRANSFER OF ALL THE      */
*  /* DATA FROM THE VTAM BUFFER TO IKTIMLU2. IF REC2FLG IS ON, AND   */
*  /* THERE WAS NO PRIOR ERROR, DATA PROCESSING CONTINUES.           */
*  /******************************************************************/
*                                                                  0386
*  IF REC2FLG='1'B THEN                /* CHECK SECOND RECEIVE FLAG  */
         L     @12,TIMGETMN                                        0386
         TM    REC2FLG(@12),B'00000010'                            0386
         BNO   @RF00386                                            0386
*    IF TIMADDER='0'B THEN             /* IF ON, CHECK FOR ERROR     */
         TM    TIMADDER(@12),B'00010000'                           0387
         BNZ   @RF00387                                            0387
*      IF TVWARDBF = '1'B THEN         /* HAS TOM ISSUED READ-BUFFER?*/
         L     @12,TVWAPTR                                         0388
         TM    TVWARDBF(@12),B'00100000'                           0388
         BNO   @RF00388                                            0388
*        CALL DATARDBF;                /* CALL READ-BUFFER ROUTINE   */
         BAL   @14,DATARDBF                                        0389
*      ELSE                            /* READ-BUFFER NOT ISSUED     */
*        DO;                                                       0390
         B     @RC00388                                            0390
@RF00388 DS    0H                                                  0391
*          TVWABRIN = '0'B;            /* RESET BREAK-IN INDICATOR   */
         L     @12,TVWAPTR                                         0391
         NI    TVWABRIN(@12),B'01111111'                           0391
*          CALL IKT3270I;              /* NO ERROR, CALL IKT3270I    */
         L     @12,CVTPTR                                          0392
         L     @12,CVTTCASP(,@12)                                  0392
         L     @15,TCASDATI(,@12)                                  0392
         BALR  @14,@15                                             0392
