         B     @RC00613                                            0615
@RF00613 L     @12,RPLPTR                                          0615
         CLI   RPLRTNCD(@12),12                                    0615
         BNE   @RF00615                                            0615
         CLI   RPLFDB2(@12),10                                     0615
         BNE   @RF00615                                            0615
*          REGRTN = '1'B;                  /* REFRESH RPL            */
         L     @12,TIMGETMN                                        0616
         OI    REGRTN(@12),B'00001000'                             0616
*        ELSE                              /* NOT RESETSR            */
*                                                                  0617
*          /**********************************************************/
*          /* CHECK FOR EXCEPTION REQUEST OR RESPONSE                */
*          /**********************************************************/
*                                                                  0617
*          IF RPLRTNCD='04'X & (RPLFDB2=USFEXRQ ×                  0617
*                                  RPLFDB2=USFEXRS) THEN           0617
         B     @RC00615                                            0617
@RF00615 L     @12,RPLPTR                                          0617
         CLI   RPLRTNCD(@12),4                                     0617
