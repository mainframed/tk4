         LA    @12,TIMBUFF(,@12)                                   0540
         ST    @12,RPLAREA(,@07)                                   0540
*          TVWARCDT = '1'B;                     /* DATA RECEIVED     */
         L     @12,TVWAPTR                                         0541
         OI    TVWARCDT(@12),B'00010000'                           0541
*          TVWARDBF = '0'B;                     /* RESET RDBF FLAG   */
         NI    TVWARDBF(@12),B'11011111'                           0542
*        END;                                                      0543
*      ELSE                                     /* RU > BUFFER       */
*        DO;                                                       0544
         B     @RC00533                                            0544
@RF00533 DS    0H                                                  0545
*          REC2FLG = '1'B;                      /* DO ANOTHER RECEIVE*/
         L     @12,TIMGETMN                                        0545
         OI    REC2FLG(@12),B'00000010'                            0545
*          RPLAREA = TEMPBFPT;                  /* RESET BUFFER PTR  */
         L     @07,RPLPTR                                          0546
         L     @12,TEMPBFPT(,@12)                                  0546
         ST    @12,RPLAREA(,@07)                                   0546
*          RPLBUFL = GTMNLEN - DATALEN - 10;    /* RESET BUFFER SIZE */
         L     @12,TVWAPTR                                         0547
         L     @12,TVWAGMPT(,@12)                                  0547
         L     @15,GTMNLEN(,@12)                                   0547
         SL    @15,DATALEN+4(,@12)                                 0547
         SL    @15,@CF02237                                        0547
         ST    @15,RPLBUFL(,@07)                                   0547
