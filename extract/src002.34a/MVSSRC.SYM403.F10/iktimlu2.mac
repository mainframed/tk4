         LA    @14,FLASHBK1(,@15)                                  0359
         ST    @14,TVWATOPQ(,@07)                                  0359
*  IF TSBXTMBF=480 THEN                                            0360
         CLC   TSBXTMBF(2,@12),@CH02752                            0360
         BNE   @RF00360                                            0360
*    PHYSLNNO = 12;                    /* NUMBER OF LINES            */
         MVC   PHYSLNNO(4,@15),@CF02241                            0361
