         L     @12,TSBEXTNT(,@12)                                  0535
         TM    TSBXASCI(@12),B'10000000'                           0535
         BNO   @RF00535                                            0535
*            CALL ASCITRAN;                     /* ASCII TRANSLATION */
         BAL   @14,ASCITRAN                                        0536
*          ELSE;                                /* NOT ASCII         */
@RF00535 DS    0H                                                  0538
*          REC2FLG = '0'B;                      /* RESET RECEIVE FLAG*/
         L     @12,TIMGETMN                                        0538
         NI    REC2FLG(@12),B'11111101'                            0538
*          RPLBUFL = LENGTH(TIMBUFF);           /* RPL BUFFER LENGTH */
         L     @07,RPLPTR                                          0539
