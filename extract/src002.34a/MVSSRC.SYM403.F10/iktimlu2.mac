         CLI   RPLFDB2(@12),7                                      0603
         BE    @RT00603                                            0603
         CLI   RPLFDB2(@12),8                                      0603
         BE    @RT00603                                            0603
         CLI   RPLFDB2(@12),9                                      0603
         BE    @RT00603                                            0603
         CLI   RPLFDB2(@12),11                                     0603
         BE    @RT00603                                            0603
@GL00011 L     @12,RPLPTR                                          0603
         CLI   RPLRTNCD(@12),16                                    0603
         BNE   @GL00010                                            0603
         CLI   RPLFDB2(@12),3                                      0603
         BE    @RT00603                                            0603
         CLI   RPLFDB2(@12),5                                      0603
         BE    @RT00603                                            0603
         CLI   RPLFDB2(@12),6                                      0603
         BE    @RT00603                                            0603
         CLI   RPLFDB2(@12),7                                      0603
