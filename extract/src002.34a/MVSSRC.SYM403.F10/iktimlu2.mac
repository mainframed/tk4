         L     @12,CVTPTR                                          0609
         L     @12,CVTTCASP(,@12)                                  0609
         L     @15,TCASEXIT(,@12)                                  0609
         BALR  @14,@15                                             0609
*      RFY REG1 UNRSTD;                    /* FREE REGISTER 1        */
*    END;                                  /* END OF CLEAN-UP        */
*  ELSE                                    /* NOT LOSTERM            */
*    DO;                                                           0612
*                                                                  0612
         B     @RC00603                                            0612
@RF00603 DS    0H                                                  0613
*      /**************************************************************/
*      /* CHECK NUMBER OF ERRORS PREVIOUS TO THIS ONE                */
*      /**************************************************************/
*                                                                  0613
*      IF RECERCNT>=3 THEN                                         0613
         L     @12,TIMGETMN                                        0613
         L     @12,RECERCNT(,@12)                                  0613
         C     @12,@CF00290                                        0613
         BL    @RF00613                                            0613
*        CALL EXITFRR;                     /* CALL ABEND           0614
