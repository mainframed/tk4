*        END;                                                      0532
*      IF RPLRLEN <= RPLBUFL &                  /* RU <= BUFFER AND  */
*        (RPLLAST = '1'B × RPLONLY = '1'B)      /* EOC OR OC?        */
*      THEN                                     /* RU <= BUFFER AND  */
@RC00517 L     @12,RPLPTR                                          0533
         CLC   RPLRLEN(4,@12),RPLBUFL(@12)                         0533
         BH    @RF00533                                            0533
         TM    RPLLAST+16(@12),B'00110000'                         0533
         BZ    @RF00533                                            0533
*        DO;                                    /* EOC OR OC         */
