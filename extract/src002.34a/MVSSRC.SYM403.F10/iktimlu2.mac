         B     @RC00624                                            0626
@RF00624 L     @12,TVWAPTR                                         0626
         NI    TVWAINB(@12),B'11111011'                            0626
*                     ELSE;       /*OTHER EXCEPTION RESPONSE @ZM20858*/
@RF00623 DS    0H                                                  0628
*                     CALL SCHTOM;         /* SCHEDULE TOM   @ZM20858*/
@RC00623 BAL   @14,SCHTOM                                          0628
*                    END;                  /* END BRKT ERROR @ZM20858*/
*                   REGRTN='0'B;           /* DO NOT REFRESH RPL     */
@RT00620 L     @12,TIMGETMN                                        0630
         NI    REGRTN(@12),B'11110111'                             0630
*                   RECERCNT=RECERCNT+1;   /* INCR RETRY     @ZM20858*/
