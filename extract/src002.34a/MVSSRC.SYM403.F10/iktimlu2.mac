         TITLE '*** IKTIMLU2: INPUT MANAGER FOR 3270 DSE LU2 TERMINALS *
                ***     '
IKTIMLU2 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKTIMLU2  78.016'                                 0001
         DROP  @15
@PROLOG  BALR  @05,0                                               0001
@PSTART  LA    @06,4095(,@05)                                      0001
         USING @PSTART,@05                                         0001
         USING @PSTART+4095,@06                                    0001
         LR    WRKSPC,REG1         LOAD ADDR OF GETMAINED AREA
         USING @DATD,WRKSPC        SET UP ADDRESSABILITY
         ST    @13,TIMGETMN        SET UP ADDRESSABILITY
*   RPLPTR=TIMGETMN+LENGTH(TIMWA)+CMPSZ;                           0149
         L     @15,TIMGETMN                                        0149
         LA    @12,2348                                            0149
         ALR   @12,@15                                             0149
         ST    @12,RPLPTR                                          0149
*   RFY IFGRPL BASED(RPLPTR);                                      0150
*   RFY WSAG   BASED(CVTSPSA);         /* USED IN GLOBAL FREEMAIN    */
*   TVWAPTR=TSBXTVWA;                  /* ADDRESSING TO TVWA         */
         L     @12,PSAAOLD                                         0152
         L     @12,ASCBTSB(,@12)                                   0152
         L     @12,TSBEXTNT(,@12)                                  0152
         L     @12,TSBXTVWA(,@12)                                  0152
         ST    @12,TVWAPTR                                         0152
*   SRBRTN=REG14;                      /* SAVE RETURN POINT ADDRESS  */
         ST    REG14,SRBRTN(,@15)                                  0153
*   STM(REG0,REG15,SAVEREGS);          /* FREEMAIN RECOVERY POINT    */
         STM   REG0,REG15,SAVEREGS(@15)                            0154
*   RFY (REG1,REG14) UNRSTD;           /* FREE REGISTER 1 AND 14     */
*/********************************************************************/
*/*     THE FOLLOWING CODE WILL BE EXECUTED UPON INITIAL ENTRY TO    */
*/*     IKTIMLU2.  THE SRB WILL BE FREED AND THE RPL FOR THE VTAM    */
*/*     RECEIVE MACRO WILL BE INITIALIZED.                           */
*/********************************************************************/
* DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/        0156
*CKMC0002:                                                         0157
*   STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */    0157
CKMC0002 L     @12,TVWAPTR                                         0157
         L     @12,TVWAFRWI(,@12)                                  0157
         STM   REG0,REG15,FRWACKRG(@12)                            0157
*   FRWACKPT(2)=ADDR(CKMC0002); /* SAVE ADDRESS    @ZM20856 */     0158
         LA    @01,CKMC0002                                        0158
         ST    @01,FRWACKPT+4(,@12)                                0158
* END;                  /* CHECKPOINT         @ZM20856*/           0159
* REG7=ADDR(IKTIOFRR);                                             0160
         L     @12,CVTPTR                                          0160
         L     @01,CVTTCASP(,@12)                                  0160
         L     REG7,TCASFRR(,@01)                                  0160
* GEN  REFS(FRRSCURR,FRRS,FRRSLAST,PSALSFCC,PSA,FRRSELEN,PSACSTK,  0161
*           FRRSFRRA,FRRSENTR,FRRSPARM,REG7)                       0161
*           SETS(REG1,REG2,REG3,FRRSCURR);                         0161
   SETFRR A,FRRAD=(REG7),WRKREGS=(REG1,REG2),PARMAD=(REG3)
* PARMPTR = REG3;                      /* ADDRESS OF PARM AREA       */
         ST    REG3,PARMPTR                                        0162
* RFY (REG3,REG7) UNRSTD;              /* FREE REGS FOR COMPILER     */
*                                                                  0163
* CALL INITFRR;                        /* CALL FRR INITIALIZE      0164
*                                         ROUTINE            @ZM20856*/
         BAL   @14,INITFRR                                         0164
* /*******************************************************************/
* /*  OBTAIN THE LOCAL LOCK AND FREE THE ADDRESS OF THE TIM SRB      */
* /*  IN THE TSB EXTENSION.                                          */
* /*******************************************************************/
* REG2=REG0;                           /* SAVE ADDRESS OF SRB TO FREE*/
         LR    REG2,REG0                                           0165
* GEN(SETLOCK OBTAIN,TYPE=LOCAL,RELATED=(SRB),MODE=UNCOND,REGS=USE)
* REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);                   0166
         SETLOCK OBTAIN,TYPE=LOCAL,RELATED=(SRB),MODE=UNCOND,REGS=USE
* TSBXSRBI=0;                          /* CLEAR ADDRESS OF SRB       */
         L     @12,PSAAOLD                                         0167
         L     @01,ASCBTSB(,@12)                                   0167
         L     @01,TSBEXTNT(,@01)                                  0167
         SLR   @12,@12                                             0167
         ST    @12,TSBXSRBI(,@01)                                  0167
* GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SRB),REGS=USE)           0168
* REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);                   0168
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SRB),REGS=USE
* /*******************************************************************/
* /* FREE SRB                                                        */
* /*******************************************************************/
* RFY (REG7,REG8) RSTD;                /* USED TO HOLD WORK ADDRESS  */
* REG8=REG4;                           /* REG4 DESTROYED BY FREEMAIN */
         LR    REG8,REG4                                           0170
* REG7=SRBLEN;                         /* SRB LENGTH                 */
         L     REG7,SRBLEN                                         0171
* GEN    REFS(REG2,REG7,CVTSPSA,CVTMAP,WSAGGMFM,WSAG,CVTGLMN,      0172
*           PSALITA,FLC) SETS(REG0,REG1,REG14,REG15,REG4,REG3);    0172
A1 SETLOCK  OBTAIN,TYPE=SALLOC,MODE=UNCOND,RELATED=(A2),REGS=USE
   FREEMAIN RC,LV=(REG7),A=(REG2),SP=239,BRANCH=(YES,GLOBAL)
A2 SETLOCK  RELEASE,TYPE=SALLOC,RELATED=(A1),REGS=USE
* REG4=REG8;                           /* RESTORE WORK AREA ADDR     */
         LR    REG4,REG8                                           0173
* STM(REG0,REG15,SAVEREGS);            /* SAVE REGS FOR RESTART      */
         L     @12,TIMGETMN                                        0174
         STM   REG0,REG15,SAVEREGS(@12)                            0174
* RESTARTA:                                                        0175
* RFY (REG0,REG2,REG8,REG7) UNRSTD;    /* FREE UP REGS       @ZM20858*/
RESTARTA DS    0H                                                  0176
* GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RPL),REGS=USE)
*     REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);               0176
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RPL),REGS=USE
* CALL PRIMPROC;                       /* CALL RTN TO SET UP RPL     */
         BAL   @14,PRIMPROC                                        0177
* GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RPL),REGS=USE)           0178
*     REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);               0178
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RPL),REGS=USE
         EJECT
*/********************************************************************/
*/*     THE FOLLOWING CODE CONTAINS THE INFINITE LOOP THAT PERFORMS  */
*/*     THE BASIC FUNCTION OF IKTIMLU2. THAT FUNCTION IS TO ISSUE    */
*/*     THE VTAM RECEIVE MACRO, DETERMINE WHAT THE INPUT IS WHEN     */
*/*     THE RECEIVE IS SATISFIED, CALL THE APPROPRIATE ROUTINE       */
*/*     TO PROCESS THE INPUT AND LOOP BACK TO REISSUE THE RECEIVE.   */
*/*     EACH TIME VTAM GAINS CONTROL THROUGH THE RECEIVE MACRO       */
*/*     THE FRR THAT IKTIMLU2 SETS UP IS DELETED AND MUST BE RESET.  */
*/********************************************************************/
*DO UNTIL (1 ^= 1);                                                0179
@DL00179 DS    0H                                                  0180
*   DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/      0180
*CKMC0003:                                                         0181
*     STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */  0181
CKMC0003 L     @12,TVWAPTR                                         0181
         L     @07,TVWAFRWI(,@12)                                  0181
         STM   REG0,REG15,FRWACKRG(@07)                            0181
*     FRWACKPT(2)=ADDR(CKMC0003); /* SAVE ADDRESS    @ZM20856 */   0182
         LA    @15,CKMC0003                                        0182
         ST    @15,FRWACKPT+4(,@07)                                0182
*   END;                /* CHECK POINT        @ZM20856*/           0183
*  RPLCNTRL = '000000'X;               /* RESET RPL CNTRL FIELD      */
         LA    @07,16                                              0184
         AL    @07,RPLPTR                                          0184
         MVC   RPLCNTRL(3,@07),@CB02715                            0184
*  IF TVWARCRS = '1'B THEN             /* NEED TIM RECEIVE RESPONSES?*/
         TM    TVWARCRS(@12),B'00000010'                           0185
         BNO   @RF00185                                            0185
*    RPLRRESP = '1'B;                  /* TURN ON RECEIVE-RESPONSE SW*/
         OI    RPLRRESP(@07),B'00001000'                           0186
*  ELSE                                /* DO NOT RECEIVE RESPONSES   */
*    RPLRRESP = '0'B;                                              0187
         B     @RC00185                                            0187
@RF00185 L     @12,RPLPTR                                          0187
         NI    RPLRRESP+16(@12),B'11110111'                        0187
*  RPLDFASY = '1'B;                                                0188
@RC00185 DS    0H                                                  0189
*  RPLNFSYN='0'B;                                                  0189
         L     @12,RPLPTR                                          0189
         OI    RPLDFASY+16(@12),B'00000010'                        0189
         NI    RPLNFSYN+16(@12),B'11111011'                        0189
*  RFY (REG0,REG1,REG2,REG3,REG7,REG15) RSTD; /*             @ZM20858*/
*  REG2=ADDR(IFGRPL);                  /* ADDR OF RPL FOR RECEIVE    */
         LR    REG2,@12                                            0191
*  REG7=ADDR(IKTIOFRR);                /* PTR TO FRR ENTRY           */
         L     @14,CVTPTR                                          0192
         L     @14,CVTTCASP(,@14)                                  0192
         L     REG7,TCASFRR(,@14)                                  0192
*  GEN REFS(REG1,REG2,REG3,REG7,FRRSCURR,FRRS,FRRSLAST,PSALSFCC,PSA,
*           FRRSELEN,PSACSTK,FRRSFRRA,FRRSENTR,FRRSPARM)           0193
*           SETS(REG1,REG2,REG3,REG14,REG15,REG0);                 0193
  RECEIVE  RPL=(REG2)
  SETFRR A,FRRAD=(REG7),WRKREGS=(REG1,REG2),PARMAD=(REG3)
  STM REG14,REG1,12(REG13)   /*SAVE RECEIVE RTN CD REGS 0,15@ZM20858*/
*  PARMPTR = REG3;                     /* POINTER TO FRR PARAMETERS  */
         ST    REG3,PARMPTR                                        0194
*  RFY (REG1,REG2,REG3,REG7) UNRSTD;   /* FREE REGISTERS FOR USE     */
*  CALL INITFRR;                       /* CALL FRR INITIALIZE      0196
*                                         ROUTINE            @ZM20856*/
         BAL   @14,INITFRR                                         0196
*  /******************************************************************/
*  /* EXAMINE INPUT FROM RECEIVE HERE                                */
*  /******************************************************************/
*                                                                  0197
*  GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RECVE),REGS=USE)
*      REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);      /*@ZM20753*/
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(RECVE),REGS=USE
*  GEN(LM REG14,REG1,12(REG13))                                    0198
*     REFS(REG13);                     /* RESTORE RTN CD REGS@ZM20858*/
         LM REG14,REG1,12(REG13)
*  IF TVWATOD='1'B THEN                /* IS IKTOMLU2 DEAD?  @ZM20856*/
         L     @14,TVWAPTR                                         0199
         TM    TVWATOD(@14),B'10000000'                            0199
         BNO   @RF00199                                            0199
*    DO;                               /*                    @ZM20856*/
*        RFY REG1 RSTD;                /* RESTRICT REG 1     @ZM20586*/
*        REG1 = ADDR(XITPARMS);        /* PARAMETER LIST ADDR@ZM20856*/
         L     @14,TIMGETMN                                        0202
         LA    REG1,XITPARMS(,@14)                                 0202
*        WRKAREAL = TIMWAL;            /* TIM WORKAREA LENGTH@ZM20856*/
         MVC   WRKAREAL(4,@14),@CF02670                            0203
*        LIOWA = TOTALL;               /* TIM-TOM WORK AREA L@ZM20856*/
         MVC   LIOWA(4,@14),@CF02672                               0204
*        CALL IKTEXIT;                 /* EXIT PERMANENTLY   @ZM20856*/
         L     @12,CVTPTR                                          0205
         L     @12,CVTTCASP(,@12)                                  0205
         L     @15,TCASEXIT(,@12)                                  0205
         BALR  @14,@15                                             0205
*        RFY REG1 UNRSTD;              /* FREE REGISTER 1    @ZM20856*/
*    END;                              /*                    @ZM20856*/
*  ELSE                                /* IKTOMLU2 IS ALIVE  @ZM20856*/
*  IF REG15=0 THEN                     /* CHECK FOR DATA RECEIVED    */
         B     @RC00199                                            0208
@RF00199 LTR   REG15,REG15                                         0208
         BNZ   @RF00208                                            0208
*    CALL POSRESPR;                    /* CHECK CONTROL              */
         BAL   @14,POSRESPR                                        0209
*  ELSE                                                            0210
*    DO;                                                           0210
         B     @RC00208                                            0210
@RF00208 DS    0H                                                  0211
*      TVWABRIN='0'B;                  /* RESET TVWABRIN     @ZM20753*/
         L     @14,TVWAPTR                                         0211
         NI    TVWABRIN(@14),B'01111111'                           0211
*      TVWABKPG='0'B;                  /* RESET TVWABKPG     @ZM20753*/
         NI    TVWABKPG(@14),B'01111111'                           0212
*      TVWARCDT='0'B;                  /* RESET TVWARCDT     @ZM20753*/
         NI    TVWARCDT(@14),B'11101111'                           0213
*      TVWARDBF='0'B;                  /* RESET TVWARDBF     @ZM20753*/
         NI    TVWARDBF(@14),B'11011111'                           0214
*      CALL VTAMERR;                   /* CHECK VTAM ERROR CODE      */
         BAL   @14,VTAMERR                                         0215
*      IF REGRTN='1'B THEN             /* REFRESH RPL NEEDED?        */
         L     @14,TIMGETMN                                        0216
         TM    REGRTN(@14),B'00001000'                             0216
         BNO   @RF00216                                            0216
*        DO;                           /* YES, GO TO REFRESH RTN     */
*          LM(REG0,REG15,SAVEREGS);    /* RESTORE REGISTERS          */
         L     @14,TIMGETMN                                        0218
         LM    REG0,REG15,SAVEREGS(@14)                            0218
*          CALL PRIMPROC;              /* CALL INITIALIZATION ROUTINE*/
         BAL   @14,PRIMPROC                                        0219
*          REGRTN='0'B;                /* RESET REFRESH INDICATOR    */
         L     @14,TIMGETMN                                        0220
         NI    REGRTN(@14),B'11110111'                             0220
*        END;                                                      0221
*      ELSE;                                                       0222
@RF00216 DS    0H                                                  0223
*    END;                                                          0223
*  RFY (REG0,REG15) UNRSTD;            /* FREE UP REGS 15,0  @ZM20858*/
@RC00208 DS    0H                                                  0224
@RC00199 DS    0H                                                  0225
*  GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RECVE),REGS=USE)        0225
*      REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);      /*@ZM20753*/
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(RECVE),REGS=USE
*END;                                  /* LOOP BACK TO ISSUE RECEIVE */
@DE00179 B     @DL00179                                            0226
         EJECT
*POSRESPR:  PROCEDURE OPTIONS(SAVE(RTNREG));                       0227
*                                                                  0227
POSRESPR ST    @14,@SA00002                                        0227
*  /******************************************************************/
*  /* CHECK CONTROL FIELD OF RPL.                                    */
*  /******************************************************************/
*                                                                  0228
*                                                                  0228
*  IF RPLDATA = '1'B THEN              /* DATA RECEIVED?             */
         L     @12,RPLPTR                                          0228
         TM    RPLDATA+16(@12),B'10000000'                         0228
         BNO   @RF00228                                            0228
*    CALL DATARTN;                     /* CALL DATA ROUTINE          */
         BAL   @14,DATARTN                                         0229
*  ELSE                                /* DATA NOT RECEIVED          */
*    DO;                                                           0230
         B     @RC00228                                            0230
@RF00228 DS    0H                                                  0231
*      TVWABRIN='0'B;                  /* RESET TVWABRIN     @ZM20753*/
         L     @12,TVWAPTR                                         0231
         NI    TVWABRIN(@12),B'01111111'                           0231
*      TVWABKPG='0'B;                  /* RESET TVWABKPG     @ZM20753*/
         NI    TVWABKPG(@12),B'01111111'                           0232
*      TVWARCDT='0'B;                  /* RESET TVWARCDT     @ZM20753*/
         NI    TVWARCDT(@12),B'11101111'                           0233
*      TVWARDBF='0'B;                  /* RESET TVWARDBF     @ZM20753*/
         NI    TVWARDBF(@12),B'11011111'                           0234
*      IF RPLSIGNL = '1'B THEN         /* SIGNAL RECEIVED?           */
         L     @07,RPLPTR                                          0235
         TM    RPLSIGNL+16(@07),B'00010000'                        0235
         BNO   @RF00235                                            0235
*        DO;                           /* BEGIN SIGNAL PROCESSING    */
*          RPLFMHDR='0'B;              /* RESET FM HEADER    @ZM20453*/
         NI    RPLFMHDR(@07),B'11111110'                           0237
*          TVWAIOTR='1'B;              /* INPUT RECEIVED     @ZM20753*/
         OI    TVWAIOTR(@12),B'01000000'                           0238
*          IF TVWAPGN='1'B THEN        /* CHECK THE PAGING SWITCH    */
         TM    TVWAPGN(@12),B'01000000'                            0239
         BNO   @RF00239                                            0239
*            DO;                                                   0240
*              IF TVWAFSW='1'B THEN    /* IF  FULL-SCREEN MODE       */
         TM    TVWAFSW(@12),B'00010000'                            0241
         BNO   @RF00241                                            0241
*                DO;                   /* FORCE A RESHOW             */
*                  CALL IKT3270I;      /* CALL TO INPUT RESHOW       */
         L     @12,CVTPTR                                          0243
         L     @12,CVTTCASP(,@12)                                  0243
         L     @15,TCASDATI(,@12)                                  0243
         BALR  @14,@15                                             0243
*                  TVWAWO='0'B;        /* RESET FS INDICATOR         */
         L     @12,TVWAPTR                                         0244
         NI    TVWAWO(@12),B'11011111'                             0244
*                  TVWAULK='0'B;       /* TGET SATISFIED             */
         NI    TVWAULK(@12),B'11111011'                            0245
*                  TVWAFSW='0'B;       /* RESET WAIT FLAG            */
         NI    TVWAFSW(@12),B'11101111'                            0246
*                END;                  /* END OF RESHOW PROCESSING   */
*              ELSE;                   /* NOT FULL-SCREEN MODE       */
@RF00241 DS    0H                                                  0249
*              TVWAPGN='0'B;           /* TURN OFF PAGING FLAG       */
         L     @12,TVWAPTR                                         0249
         NI    TVWAPGN(@12),B'10111111'                            0249
*              IF ADDR(IKTIDSX5) ^= 0 THEN /* ATTN EXIT ROUTINE    0250
*                                             AVAILABLE?             */
         L     @12,@CV02662                                        0250
         LTR   @12,@12                                             0250
         BZ    @RF00250                                            0250
*                CALL IKTIDSX5;        /* CALL ATTENTION EXIT ROUTINE*/
         L     @15,@CV02662                                        0251
         BALR  @14,@15                                             0251
*              ELSE                    /* NO ATTN EXIT ROUTINE       */
*                CALL IKTATTN;         /* ATTENTION-HANDLING ROUTINE */
         B     @RC00250                                            0252
@RF00250 L     @12,CVTPTR                                          0252
         L     @12,CVTTCASP(,@12)                                  0252
         L     @15,TCASATTN(,@12)                                  0252
         BALR  @14,@15                                             0252
*              TVWALNCT=1;             /* RESET CURRENT LINE NUMB    */
@RC00250 L     @12,TVWAPTR                                         0253
         MVI   TVWALNCT(@12),X'01'                                 0253
*              TVWAFMSC='1'B;          /* SET FORMAT SCREEN SWITCH   */
         OI    TVWAFMSC(@12),B'10000000'                           0254
*            END;                      /* END PAGING FLAG ON PROCESS */
*          ELSE                        /* NOT A PAGING SITUATION     */
*            IF TSBKEYS='1'B THEN      /* KEYS=ALL?          @ZA20268*/
         B     @RC00239                                            0256
@RF00239 L     @12,PSAAOLD                                         0256
         L     @12,ASCBTSB(,@12)                                   0256
         TM    TSBKEYS(@12),B'00000010'                            0256
         BNO   @RF00256                                            0256
*              IF TVWAKBDL='1'B THEN   /* KEYBOARD LOCKED?   @ZA20268*/
         L     @12,TVWAPTR                                         0257
         TM    TVWAKBDL(@12),B'00100000'                           0257
         BNO   @RF00257                                            0257
*                CALL PASSPA1;         /* PASS PA1-AID       @ZA20268*/
         BAL   @14,PASSPA1                                         0258
*              ELSE                    /* KEYBOARD UNLOCKED  @ZA20268*/
*                DO;                   /* HAVE SIGNAL SENT   @ZA20268*/
         B     @RC00257                                            0259
@RF00257 DS    0H                                                  0260
*                  TVWASDSG='1'B;      /* SEND SIGNAL        @ZA20268*/
         L     @12,TVWAPTR                                         0260
         OI    TVWASDSG(@12),B'01000000'                           0260
*                  TIMSCHTM='1'B;      /* SCHEDULE TOM       @ZA20268*/
         L     @12,TIMGETMN                                        0261
         OI    TIMSCHTM(@12),B'01000000'                           0261
*                END;                  /*                    @ZA20268*/
*            ELSE                      /* NOT KEYS=ALL       @ZA20268*/
*              IF ADDR(IKTIDSX5) ^= 0 THEN /* ATTENTION EXIT ROUTINE
*                                             AVAILABLE?             */
         B     @RC00256                                            0263
@RF00256 L     @12,@CV02662                                        0263
         LTR   @12,@12                                             0263
         BZ    @RF00263                                            0263
*                CALL IKTIDSX5;        /* CALL ATTENTION EXIT ROUTINE*/
         L     @15,@CV02662                                        0264
         BALR  @14,@15                                             0264
*              ELSE                    /* NO ATTN EXIT ROUTINE       */
*                CALL IKTATTN;         /* ATTENTION-HANDLING ROUTINE */
         B     @RC00263                                            0265
@RF00263 L     @12,CVTPTR                                          0265
         L     @12,CVTTCASP(,@12)                                  0265
         L     @15,TCASATTN(,@12)                                  0265
         BALR  @14,@15                                             0265
*          IF TIMSCHTM='1'B THEN       /* SCHEDULE IKTOMLU2?         */
@RC00263 DS    0H                                                  0266
@RC00256 DS    0H                                                  0266
@RC00239 L     @12,TIMGETMN                                        0266
         TM    TIMSCHTM(@12),B'01000000'                           0266
         BNO   @RF00266                                            0266
*            CALL SCHTOM;              /* SCHEDULE IKTOMLU2          */
         BAL   @14,SCHTOM                                          0267
*          ELSE;                       /* DO NOT SCHEDULE IKTOMLU2   */
@RF00266 DS    0H                                                  0269
*        END;                          /* END OF SIGNAL PROCESSING   */
*      ELSE                            /* SIGNAL NOT RECEIVED        */
*        IF RPLCNCEL = '1'B THEN       /* CANCEL RECEIVED?           */
         B     @RC00235                                            0270
@RF00235 L     @12,RPLPTR                                          0270
         TM    RPLCNCEL+16(@12),B'01000000'                        0270
         BNO   @RF00270                                            0270
*          CALL CANCELR;               /* CALL CANCEL ROUTINE        */
         BAL   @14,CANCELR                                         0271
*        ELSE                          /* CANCEL NOT RECEIVED        */
*          IF RPLLUS = '1'B THEN       /* LUSTAT RECEIVED?           */
         B     @RC00270                                            0272
@RF00270 L     @12,RPLPTR                                          0272
         TM    RPLLUS+16(@12),B'00100000'                          0272
         BNO   @RF00272                                            0272
*            CALL LUSTATR;             /* CALL LUSTAT ROUTINE        */
         BAL   @14,LUSTATR                                         0273
*          ELSE                        /* LUSTAT NOT RECEIVED        */
*            IF RPLCHASE = '1'B THEN   /* CHASE?                     */
         B     @RC00272                                            0274
@RF00272 LA    @12,16                                              0274
         AL    @12,RPLPTR                                          0274
         TM    RPLCHASE(@12),B'00001000'                           0274
         BNO   @RF00274                                            0274
*              DO;                     /*                    @ZM20759*/
