         BAL   @14,EXITFRR                                         0596
*      END;                       /* END OF ABEND            @ZM20858*/
*    ELSE                         /* RETRY COUNT NOT EXCEEDED@ZM20858*/
*      DO;                        /* REFRESH RPL AND RETRY   @ZM20858*/
         B     @RC00593                                            0598
@RF00593 DS    0H                                                  0599
*        RECERCNT=RECERCNT+1;     /* INCREMENT RETRY COUNT   @ZM20858*/
         L     @12,TIMGETMN                                        0599
         LA    @07,1                                               0599
         AL    @07,RECERCNT(,@12)                                  0599
         ST    @07,RECERCNT(,@12)                                  0599
*        REGRTN='1'B;             /* REFRESH RPL             @ZM20858*/
         OI    REGRTN(@12),B'00001000'                             0600
*      END;                       /*                         @ZM20858*/
*  ELSE                           /* IMMED RTN CODE OK       @ZM20858*/
*    DO;                          /* CONTINUE NORMAL RECOVERY@ZM20858*/
         B     @RC00592                                            0602
@RF00592 DS    0H                                                  0603
*  /******************************************************************/
*  /* CONTINUE WITH NORMAL ERROR RECOVERY PROCEDURES         @ZM20858*/
*  /******************************************************************/
*                                                                  0603
*  /******************************************************************/
*  /* CHECK FIRST FOR LOSTERM BEING ENTERED                          */
*  /******************************************************************/
*                                                                  0603
*  IF (RPLRTNCD='0C'X & (RPLFDB2='02'X ×   /*REQUEST RESET   @ZM20858*/
*                        RPLFDB2='07'X ×   /*RECOVERY IN PROG@ZM20858*/
*                        RPLFDB2='08'X ×   /*TERM RSTD - FAIL@ZM20858*/
*                        RPLFDB2='09'X ×   /*OPNDST CANCELLED@ZM20858*/
*                        RPLFDB2='0B'X)) × /*CLSDST OCCURED  @ZM20858*/
*     (RPLRTNCD='10'X &(RPLFDB2='03'X ×    /* HALT QUICK ISSUED      */
*                       RPLFDB2='05'X ×    /*PERM PATH FAIL  @ZM20858*/
*                       RPLFDB2='06'X ×    /*AUTO NET SHUTDOW@ZM20858*/
*                       RPLFDB2='07'X ×    /* VARY INACT IMMEDIATE   */
*                       RPLFDB2='09'X ×    /* TERMINATE SELF         */
*                       RPLFDB2='0F'X)) ×  /* BUFFERS FILLED @ZM20858*/
*     (RPLRTNCD='14'X & RPLFDB2=USFCLSIP) × TVWALTE='1'B THEN      0603
         L     @12,RPLPTR                                          0603
         CLI   RPLRTNCD(@12),12                                    0603
         BNE   @GL00011                                            0603
         CLI   RPLFDB2(@12),2                                      0603
