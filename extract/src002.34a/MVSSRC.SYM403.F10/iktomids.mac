         TITLE '*** IKTOMIDS: OUTPUT MANAGER FOR 3270 TERMINALS ***    *
                        '
IKTOMIDS CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKTOMIDS  77.343'                                 0001
         DROP  @15
@PROLOG  BALR  @05,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@05                                         0001
*                                                                  0139
*TOMWAPTR=REG1;                      /* ADR COMPILER STORAGE @G58AK2Q*/
         LR    TOMWAPTR,REG1                                       0140
*GEN(USING @DATD,TOMWAPTR)                                         0141
*    REFS(TOMWAPTR);                 /* ADDRESSING-COMPILER  @G58AK2Q*/
         USING @DATD,TOMWAPTR
*RETADR00=REG14;                     /* SAVE RETURN ADDR TO CALLER.  */
         ST    REG14,RETADR00                                      0142
*TVWAPTR=TSBXTVWA;                   /* SET POINTER TO TVWA          */
         L     @15,PSAAOLD                                         0143
         L     @15,ASCBTSB(,@15)                                   0143
         L     @15,TSBEXTNT(,@15)                                  0143
         L     TVWAPTR,TSBXTVWA(,@15)                              0143
*RFY IFGRPL BASED(EVAL(TOMWAPTR+                                   0144
*          +LEN@DATA+LENGTH(TOMWA)));/* SET RPL ADDRESS      @G58AK2Q*/
*TOMGETMN=TOMWAPTR+LEN@DATA+                                       0145
*       LENGTH(TOMWA)+LENGTH(IFGRPL);/* SET OUTPUT RU ADDR   @G58AK2Q*/
         LA    @15,856                                             0145
         ALR   @15,TOMWAPTR                                        0145
         ST    @15,TOMGETMN(,TOMWAPTR)                             0145
*/*   GEN(LA REG13,@SA00001)                                 @ZM20856*/
*/* SETS(REG13)                         SET UP SAVE AREA ADDR@ZM20856*/
* DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/        0146
*CKMC0002:                                                         0147
*   STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */    0147
CKMC0002 L     @15,TVWAFRWO(,TVWAPTR)                              0147
         STM   REG0,REG15,FRWACKRG(@15)                            0147
*   FRWACKPT(2)=ADDR(CKMC0002); /* SAVE ADDRESS    @ZM20856 */     0148
         LA    @12,CKMC0002                                        0148
         ST    @12,FRWACKPT+4(,@15)                                0148
* END;                /* CHECK POINT          @ZM20856*/           0149
*CALL OFRRLOCK;                      /* ADD FRR AND OBTAIN LOCAL LOCK*/
         BAL   @14,OFRRLOCK                                        0150
*STM(REG0,REG15,RTRYREGS);           /* SAVE REGISTERS       @GM03215*/
         STM   REG0,REG15,RTRYREGS                                 0151
*RFY (REG1,REG14) UNRSTD;            /* FREE UP REGS 1,14    @ZM20856*/
*CALL INITFRR;                       /* CALL FRR INITIALIZE        0153
*                                       ROUTINE              @ZM20856*/
         BAL   @14,INITFRR                                         0153
*RESTART1:                           /* FIRST RESTART PLACE  @ZM20856*/
*IF TOMINITS='0'B THEN               /* IS THIS FIRST PASS THROUGH?  */
RESTART1 TM    TOMINITS(TOMWAPTR),B'10000000'                      0154
         BNZ   @RF00154                                            0154
*  /******************************************************************/
*  /*                                                                */
*  /*       INITIALIZE TOM'S WORKING STORAGE ON FIRST ENTRY.         */
*  /*                                                                */
*  /******************************************************************/
*  CALL TOMINIT;                     /* INITIALIZE RPL & INDICATORS  */
         BAL   @14,TOMINIT                                         0155
*ELSE;                               /* NOT THE FIRST ENTRY IN TOM   */
@RF00154 DS    0H                                                  0157
*TVWATIR='1'B;                       /* TOM IS RUNNING       @YM03259*/
         OI    TVWATIR(TVWAPTR),B'00001000'                        0157
*VRTRYCNT=0;                         /* INIT VTAM ERR RETRY COUNTER  */
         SLR   @12,@12                                             0158
         ST    @12,VRTRYCNT                                        0158
*TOMERCNT=0;                         /* RESET ERROR COUNT    @ZA17031*/
         ST    @12,TOMERCNT(,TOMWAPTR)                             0159
*TOMEMAIN='0'B;                      /* CONTINUE MAINLINE    @G58AK2Q*/
         NI    TOMEMAIN(TOMWAPTR),B'11011111'                      0160
*/********************************************************************/
*/*                                                                  */
*/* THIS DO GROUP ENCOMPASSES THE MAINLINE PROCESSING FOR THE 3270   */
*/* TERMINAL OUTPUT MANAGER (TOM)                                    */
*/*                                                                  */
*/********************************************************************/
*DO UNTIL (TOMEMAIN='1'B);           /* BEGIN MAINLINE LOOP  @G58AK2Q*/
@DL00161 DS    0H                                                  0162
*   DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/      0162
*CKMC0003:                                                         0163
*     STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */  0163
CKMC0003 L     @12,TVWAFRWO(,TVWAPTR)                              0163
         STM   REG0,REG15,FRWACKRG(@12)                            0163
*     FRWACKPT(2)=ADDR(CKMC0003); /* SAVE ADDRESS    @ZM20856 */   0164
         LA    @09,CKMC0003                                        0164
         ST    @09,FRWACKPT+4(,@12)                                0164
*   END;              /* CHECK POINT          @ZM20856*/           0165
*  IF TVWATID='1'B THEN              /* HAS TIM EXITED PERMANENTLY?  */
         TM    TVWATID(TVWAPTR),B'00010000'                        0166
         BNO   @RF00166                                            0166
*    CALL ERREXIT;                   /* EXIT PERMANENTLY     @G58AK2Q*/
         BAL   @14,ERREXIT                                         0167
*  ELSE                              /* TIM IS NOT DEAD SO CONTINUE  */
*    IF TVWALTE='1'B THEN            /* HAS LOSTERM BEEN ENTERED?    */
         B     @RC00166                                            0168
@RF00166 TM    TVWALTE(TVWAPTR),B'00000010'                        0168
         BNO   @RF00168                                            0168
*      TOMEMAIN='1'B;                /* YES, EXIT NORMALLY   @G58AK2Q*/
         OI    TOMEMAIN(TOMWAPTR),B'00100000'                      0169
*    ELSE                            /* NO LOSTERM SO CONTINUE       */
*      /**************************************************************/
*      /*                                                            */
*      /* TIM IS NOT DEAD AND LOSTERM HAS NOT BEEN ENTERED SO CON-   */
*      /* TINUE PROCESSING.                                          */
*      /*                                                            */
*      /**************************************************************/
*      DO;                           /* TIM NOT DEAD & NO LOSTERM    */
         B     @RC00168                                            0170
@RF00168 DS    0H                                                  0171
*        IF TOMERLCT='1'B THEN       /* 1ST ENTRY AFTER ERR  @G58AK2Q*/
         TM    TOMERLCT(TOMWAPTR),B'00000010'                      0171
         BNO   @RF00171                                            0171
*          /**********************************************************/
*          /*                                                        */
*          /* IF THIS IS THE 1ST ENTRY AFTER A PAGING RESPONSE FROM  */
*          /* A SCRN ERASURE AS AN ERP, THEN RESET LINE-CNT AND RE-  */
*          /* SEND RU (ALREADY BUILT).                               */
*          /*                                                        */
*          /**********************************************************/
*          DO;                       /* RESET LINE COUNT & ERROR BIT */
*            TVWALNCT=TOMLNCNT;      /* USE LINE-COUNT BEFORE ERROR  */
         L     @12,TOMLNCNT(,TOMWAPTR)                             0173
         STC   @12,TVWALNCT(,TVWAPTR)                              0173
*            TOMERLCT='0'B;          /* OFF, LINE-CNT RESET  @G58AK2Q*/
         NI    TOMERLCT(TOMWAPTR),B'11111101'                      0174
*          END;                      /* END ERROR RESET              */
*        ELSE                        /* NOT FIRST ENTRY AFTER ERR MSG*/
*          DO;                       /* NORMAL ENTRY                 */
         B     @RC00171                                            0176
@RF00171 DS    0H                                                  0177
*            TOMLNCNT=TVWALNCT;      /* USE LINE-CNT AS LOCAL        */
         SLR   @12,@12                                             0177
         IC    @12,TVWALNCT(,TVWAPTR)                              0177
         ST    @12,TOMLNCNT(,TOMWAPTR)                             0177
*            IF VRTRYCNT^=0 THEN     /* IS THIS ENTRY RETRY FROM FRR?*/
         SLR   @12,@12                                             0178
         C     @12,VRTRYCNT                                        0178
         BE    @RF00178                                            0178
*              /******************************************************/
*              /*                                                    */
*              /* IF THIS ENTRY IS FOR ERROR RETRY OUT OF TOM'S FRR, */
*              /* THEN RESET OUTPUT QUEUE POINTER TO TOP OF QUEUE.   */
*              /*                                                    */
*              /******************************************************/
*              DO;                   /* RESET OUTPUT QUEUE POINTER   */
*                TOMCOLMN=0;         /* RESET COLUMN COUNT   @ZA17886*/
         ST    @12,TOMCOLMN(,TOMWAPTR)                             0180
*                OPREQ=RESET;        /* QUE MANAGER'S CODE FOR RESET */
         MVI   OPREQ(TOMWAPTR),X'07'                               0181
*                CALL IKTQMOUT(OPARMS);/* CALL OUTPUT QUEUE MANAGER  */
         LA    @12,OPARMS(,TOMWAPTR)                               0182
         ST    @12,@AL00001                                        0182
         L     @12,CVTPTR                                          0182
         L     @12,CVTTCASP(,@12)                                  0182
         L     @15,TCASOQM(,@12)                                   0182
         LA    @01,@AL00001                                        0182
         BALR  @14,@15                                             0182
*              END;                  /* END RESET OF OUTPUT QUEUE    */
*            ELSE;                   /* NOT A RETRY FROM FRR         */
@RF00178 DS    0H                                                  0185
*            TOMREQCD=TOMOPRU;       /* SET REQ CODE FOR RU  @G58AK2Q*/
         MVI   TOMREQCD(TOMWAPTR),X'00'                            0185
*            /********************************************************/
*            /*                                                      */
*            /* BUILD OUTPUT RU IF OUTPUT DATA & CONTROLS ARE PENDING*/
*            /*                                                      */
*            /********************************************************/
*            CALL IKT3270O;          /* BLD & EDIT OUTPUT RU @G58AK2Q*/
         L     @12,CVTPTR                                          0186
         L     @12,CVTTCASP(,@12)                                  0186
         L     @15,TCASDATO(,@12)                                  0186
         BALR  @14,@15                                             0186
*            STM(REG0,REG15,RTRYREGS);/* SAVE REGISTERS      @YM03215*/
         STM   REG0,REG15,RTRYREGS                                 0187
*            RFY INFODSCT BASED(ADDR(FRWARTRY)); /* POINT TO @ZM20856*/
*                                    /* RETRY INFORMATION    @ZM20856*/
*            INFOADDR=ADDR(RESTART2);/* SET RETRY ADDRESS TO @ZM20856*/
         L     @12,TVWAFRWO(,TVWAPTR)                              0189
         LA    @09,RESTART2                                        0189
         ST    @09,INFOADDR+8(,@12)                                0189
*          END;                      /* NORMAL PATH                  */
*RESTART2:                                                         0191
*        TOMESEND='0'B;              /* INIT LOOP CONTROL    @G58AK2Q*/
@RC00171 DS    0H                                                  0191
RESTART2 NI    TOMESEND(TOMWAPTR),B'11101111'                      0191
*        DO WHILE TOMESEND='0'B;     /* BEGIN SEND LOOP      @G58AK2Q*/
         B     @DE00192                                            0192
@DL00192 DS    0H                                                  0193
*           DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/
*CKMC0004:    STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856
*/            FRWACKPT(2)=ADDR(CKMC0004); /* SAVE ADDRESS    @ZM20856 *
CKMC0004 L     @12,TVWAFRWO(,TVWAPTR)                              0194
         STM   REG0,REG15,FRWACKRG(@12)                            0194
         LA    @09,CKMC0004                                        0195
         ST    @09,FRWACKPT+4(,@12)                                0195
*           END;      /* CHECK POINT          @ZM20856*/           0196
*          IF TOMBFCNT^=0 THEN       /* ANY DATA IN RU?      @G58AK2Q*/
         L     @12,TOMBFCNT(,TOMWAPTR)                             0197
         LTR   @12,@12                                             0197
         BZ    @RF00197                                            0197
*            /********************************************************/
*            /*                                                      */
*            /* THIS DO GROUP WILL SEND RU TO THE TERMINAL, INVOKE   */
*            /* ERROR RECOVERY IF NECESSARY, DELETE MESSAGES FROM    */
*            /* OUTPUT QUEUE, AND STATUS START TASTS WHEN NECESSARY. */
*            /*                                                      */
*            /********************************************************/
*            DO;                     /* YES, CONTINUE PROCESSING     */
*              RFY (REG0,REG15) RSTD;/* RESTRICT REGS 15,0   @ZM20858*/
*              /******************************************************/
*              /*                                                    */
*              /* MAKE NECESSARY CHANGES TO RPL & SEND RU TO TERM    */
*              /*                                                    */
*              /******************************************************/
*              CALL SENDRU;          /* SEND OUTPUT TO 3270 TERMINAL */
         BAL   @14,SENDRU                                          0200
*                                    /* DELETE CWT REFERENCE @ZM20856*/
*              TVWAISYS='0'B;        /* NO IWAIT SYSEVENT     OZ12001*/
*              TVWAIOP='0'B;         /* I-O NOT PENDING      @ZA24418*/
         NI    TVWAISYS(TVWAPTR),B'01011111'                       0202
*              IF REG15^=0 THEN      /* EXCEPTION CONDITION ON SEND? */
         LTR   REG15,REG15                                         0203
         BZ    @RF00203                                            0203
*                /****************************************************/
*                /*                                                  */
*                /* IF EXCEPTION ON SEND THEN ATTEMPT ERROR RECOVERY */
*                /*                                                  */
*                /****************************************************/
*                CALL CKVTAMRC;      /* YES, PROCESS RETURN CODES    */
         BAL   @14,CKVTAMRC                                        0204
*              ELSE                  /* NO EXCEPTION ON SEND         */
*                DO;                 /* BEGIN CODE FOR NO EXCEPTION  */
         B     @RC00203                                            0205
@RF00203 DS    0H                                                  0206
*                  STM(REG0,REG15,RTRYREGS); /* SAVE REGS    @YM03215*/
         STM   REG0,REG15,RTRYREGS                                 0206
*                  RFY INFODSCT BASED(ADDR(FRWARTRY));/*POINT@ZM20856*/
*                                    /* TO RETRY INFORMATION @ZM20856*/
*                  INFOADDR=ADDR(RESTART3); /* SET ERROR     @ZM20856*/
         L     @01,TVWAFRWO(,TVWAPTR)                              0208
         LA    @14,RESTART3                                        0208
         ST    @14,INFOADDR+8(,@01)                                0208
*                                    /* RETRY ADDRESS        @ZM20856*/
*RESTART3:         VRTRYCNT=0;       /* SET VTAM ERROR RETRY CNT = 0 */
RESTART3 SLR   @14,@14                                             0209
         ST    @14,VRTRYCNT                                        0209
*                  TOMERCNT=0;       /* RESET ERROR COUNT    @ZA17031*/
         ST    @14,TOMERCNT(,TOMWAPTR)                             0210
*                  TOMBRKSW='0'B;    /* SET BRACKET-NBB/NEB  @G58AK2Q*/
         NI    TOMBRKSW(TOMWAPTR),B'10111111'                      0211
*                  IF TOMERMG9='1'B THEN /* LAST SEND FOR ERR ERASE? */
         TM    TOMERMG9(TOMWAPTR),B'00100000'                      0212
         BNO   @RF00212                                            0212
*                    /************************************************/
*                    /*                                              */
*                    /* IF LAST SEND ERASED SCREEN AS AN ERP, RESEND */
*                    /* RU (ALREADY BUILT) UPON WHICH ERR OCCURRED   */
*                    /* IF PAGING RESPONSE HAS ALREADY BEEN RECEIVED.*/
*                    /*                                              */
*                    /************************************************/
*                    DO;                    /* RESEND FROM OUTPUT RU */
*                      TOMERMG9='0'B;       /* RESET REQ FOR ERR MSG */
         NI    TOMERMG9(TOMWAPTR),B'11011111'                      0214
*                      IF TVWAPGN='0'B THEN /* RESP TO ERR PAGE IN?  */
         TM    TVWAPGN(TVWAPTR),B'01000000'                        0215
         BNZ   @RF00215                                            0215
*                        DO;                /* YES, RESET LINE-CNT   */
*                                           /* DELETE RESTORE@ZM20856*/
*                                           /* OF RESTART2   @ZM20856*/
*                          TVWALNCT=TOMLNCNT;/* LINE-CNT BEFORE ERROR*/
         L     @14,TOMLNCNT(,TOMWAPTR)                             0217
         STC   @14,TVWALNCT(,TVWAPTR)                              0217
*                          TOMERLCT='0'B;   /* OFF, LINE-CNT @G58AK2Q*/
         NI    TOMERLCT(TOMWAPTR),B'11111101'                      0218
*                        END;               /* END LINE-CNT RESET    */
*                      ELSE                 /* PAGE RESPONSE NOT IN  */
*                        DO;                /* SET FLAGS FOR EXIT    */
         B     @RC00215                                            0220
@RF00215 DS    0H                                                  0221
*                          TOMESEND='1'B;   /* EXIT SEND LOOP@G58AK2Q*/
*                          TOMEMAIN='1'B;   /* EXIT MAIN LOOP@G58AK2Q*/
         OI    TOMESEND(TOMWAPTR),B'00110000'                      0222
*                        END;               /* END SETTING EXIT FLAGS*/
*                    END;                   /* END RESEND FROM OUT RU*/
*                  ELSE                     /* LAST SEND NO ERASE MSG*/
*                    DO;                    /*                       */
         B     @RC00212                                            0225
@RF00212 DS    0H                                                  0226
*                      RFY REG15 UNRSTD;    /* FREE REGISTER 15      */
*                      OPREQ=DELETO;        /* Q MNGR REQ TO DELETE  */
         MVI   OPREQ(TOMWAPTR),X'03'                               0227
*                      /**********************************************/
*                      /*                                            */
*                      /* DELETE SENT MESSAGES FROM OUTPUT QUEUE     */
*                      /*                                            */
*                      /**********************************************/
*                      CALL IKTQMOUT(OPARMS);/* DELETE MESSAGE +FME  */
         LA    @12,OPARMS(,TOMWAPTR)                               0228
         ST    @12,@AL00001                                        0228
         L     @12,CVTPTR                                          0228
         L     @01,CVTTCASP(,@12)                                  0228
         L     @15,TCASOQM(,@01)                                   0228
         LA    @01,@AL00001                                        0228
         BALR  @14,@15                                             0228
*                      IF TOMOWAIT='1'B THEN/* OWAIT ENDED?  @G58AK2Q*/
         TM    TOMOWAIT(TOMWAPTR),B'00000001'                      0229
         BNO   @RF00229                                            0229
*                        DO;                /* REMOVE TASK FROM OWAIT*/
*                          RFY (REG0,REG1,REG4) RSTD;              0231
*                          REG0=8;          /* RESET TASK    @YM03241*/
         LA    REG0,8                                              0232
*                          REG1='80000000'X;/* CALL IS FOR RESET     */
         L     REG1,@CF01537                                       0233
*                          REG4=REG13;      /* SAVE REGISTER 13    0234
*                                                            @ZM20856*/
         LR    REG4,REG13                                          0234
*                          REG13='0400'X;   /* RESET NON-DISPATCH'   */
         LA    REG13,1024                                          0235
*                                           /* BIT TO RESET IS OWAIT */
*                          /******************************************/
*                          /*                                        */
*                          /* REMOVE TASK FROM OUTPUT-WAIT STATUS    */
*                          /*                                        */
*                          /******************************************/
*                          CALL STATUS;     /* STATUS START TASK     */
         L     @12,CVTPTR                                          0236
         L     @12,CVTABEND(,@12)                                  0236
         L     @15,SCVTSTAT(,@12)                                  0236
         BALR  @14,@15                                             0236
*                          TSBOWAIT='0'B;   /* TURN OFF OWAIT BIT    */
         L     @12,PSAAOLD                                         0237
         L     @12,ASCBTSB(,@12)                                   0237
         NI    TSBOWAIT(@12),B'11011111'                           0237
*                          GEN(LR REG13,REG4)                      0238
*                              SETS(REG13); /* RESTORE SAVEAREA ADDR
*                                                            @ZM20856*/
         LR REG13,REG4
*                          RFY (REG0,REG1,REG4) UNRSTD;            0239
*                        END;               /* END OWAIT MSG COMPLETE*/
*                      ELSE;                /* END OWAIT MSG COMPLETE*/
@RF00229 DS    0H                                                  0242
*                      IF (TVWAPGN='1'B)×(TVWAKBDL='0'B) THEN      0242
         TM    TVWAPGN(TVWAPTR),B'01000000'                        0242
         BO    @RT00242                                            0242
         TM    TVWAKBDL(TVWAPTR),B'00100000'                       0242
         BNZ   @RF00242                                            0242
@RT00242 DS    0H                                                  0243
*                        /********************************************/
*                        /*                                          */
*                        /* PAGING RESPONSE IS OUTSTANDING OR KEYBD  */
*                        /* IS UNLOCK - NO WORK CAN BE DONE SO EXIT  */
*                        /*                                          */
*                        /********************************************/
*                        TOMEMAIN='1'B;   /* EXIT MAINLINE   @G58AK2Q*/
         OI    TOMEMAIN(TOMWAPTR),B'00100000'                      0243
*                      ELSE;              /* CONTINUE MAINLINE LOOP  */
@RF00242 DS    0H                                                  0245
*                      TOMESEND='1'B;     /* EXIT SEND LOOP  @G58AK2Q*/
         OI    TOMESEND(TOMWAPTR),B'00010000'                      0245
*                    END;                 /*                         */
*                END;                     /*                         */
*            END;                         /*                         */
*          ELSE                           /* NO DATA IS IN OUTPUT RU */
*            DO;                          /* BEGIN SETTING EXIT FLAGS*/
         B     @RC00197                                            0249
@RF00197 DS    0H                                                  0250
*              TOMESEND='1'B;             /* EXIT SEND LOOP  @G58AK2Q*/
*              TOMEMAIN='1'B;             /* EXIT MAINLINE   @G58AK2Q*/
         OI    TOMESEND(TOMWAPTR),B'00110000'                      0251
*            END;                         /* END SETTING EXIT FLAGS  */
*        END;                             /* END OF SEND LOOP        */
@RC00197 DS    0H                                                  0253
@DE00192 TM    TOMESEND(TOMWAPTR),B'00010000'                      0253
         BZ    @DL00192                                            0253
*      END;                                                        0254
*END;                                /***** END OF MAINLINE LOOP *****/
@RC00168 DS    0H                                                  0255
@RC00166 DS    0H                                                  0255
@DE00161 TM    TOMEMAIN(TOMWAPTR),B'00100000'                      0255
         BNO   @DL00161                                            0255
*/********************************************************************/
*/*                                                                  */
*/* THIS MARKS THE START OF EXIT CODE FOR THE NORMAL EXIT FROM TOM   */
*/*                                                                  */
*/********************************************************************/
*TVWATIS='0'B;                       /* INDICATE TOM'S NOT SCHEDULED */
         NI    TVWATIS(TVWAPTR),B'10111111'                        0256
*IF TVWAPGN='1'B×TVWAKBDL='0'B THEN  /* PAGING OR UNLOCKED KEYBOARD? */
         TM    TVWAPGN(TVWAPTR),B'01000000'                        0257
         BO    @RT00257                                            0257
         TM    TVWAKBDL(TVWAPTR),B'00100000'                       0257
         BNZ   @RF00257                                            0257
@RT00257 DS    0H                                                  0258
*  TVWATAS='1'B;                     /* YES, ONLY TIM CAN SCHED TOM  */
         OI    TVWATAS(TVWAPTR),B'00100000'                        0258
*ELSE                                /* NO, ALL CAN SCHEDULE TOM     */
*  TVWATAS='0'B;                     /* NO, ALL CAN SCHEDULE TOM     */
         B     @RC00257                                            0259
@RF00257 NI    TVWATAS(TVWAPTR),B'11011111'                        0259
*TVWATIR='0'B;                       /* TOM IS NOT RUNNING   @YM03259*/
@RC00257 NI    TVWATIR(TVWAPTR),B'11110111'                        0260
*/********************************************************************/
*/*                                                                  */
*/*        RELEASE LOCAL LOCK AND DELETE FRR BEFORE EXITING          */
*/*                                                                  */
*/********************************************************************/
*CALL RELLOCK;                       /* RELEASE LOCAL LOCK   @G58AK2Q*/
         BAL   @14,RELLOCK                                         0261
*GEN REFS(PSACSTK,FRRSCURR,FRRSEMP,FRRSELEN,PSA,FRRS)              0262
*    SETS(REG2,REG3);                /* VARIABLES REFERENCED AND SET */
        SETFRR D,WRKREGS=(REG2,REG3),RELATED=(ADDFRR)
*RFY (REG14) RSTD;                   /* RESTRICT REGISTER 14         */
*REG14=RETADR00;                     /* RESTORE RETURN ADR TO SYSTEM */
         L     REG14,RETADR00                                      0264
*RFY (REG14) UNRSTD;                 /* UNRESTRICT REGISTER 14       */
*GEN(BR REG14)                       /* RETURN CONTROL TO DISPATCHER */
*   REFS(REG14);                     /*                              */
         BR REG14
*/***************** END OF MAINLINE PROCESSING ***********************/
         EJECT
*/********************************************************************/
*/*                                                                  */
*/* THIS SEGMENT OF CODE BUILDS THE DUMMY RPL FROM WHICH TOM'S WORK- */
*/* ING RPL IS INITIALIZED AND REBUILT (DURING ERROR RECOVERY). THIS */
*/* CODE IS NEVER EXECUTED                                           */
*/********************************************************************/
*GEN;                                                              0267
WRKRPL RPL AM=VTAM,BRANCH=YES,POST=RESP,RESPOND=(NEX,                 CC
               FME,NRRN),CONTROL=DATA,CHAIN=ONLY,BRACKET=(NBB,NEB),   CC
               STYPE=REQ,OPTCD=(SYN,CS),CHNGDIR=(NCMD,NREQ)
         EJECT
*/********************************************************************/
*/*                                                                  */
*/* THIS PROCEDURE, TOMINIT, WILL BE CALLED TO DO ONE-TIME INITIALI- */
*/* ZATION FOR TOM AND TO REBUILD THE RPL AS NECESSARY.              */
*/*                                                                  */
