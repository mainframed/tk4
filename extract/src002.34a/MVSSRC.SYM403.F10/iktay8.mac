*                                                               01S0506
*   /*****************************************************************/
*   /*                                                               */
*   /* SRCHTCAM SEARCH TCAM TSB CHAIN FOR ASID TPUT FROM TERMINATING */
*   /* ADDRESS SPACE. WHEN A TSB IS FOUND WHICH INDICATES AN ASID    */
*   /* TPUT FROM THE TERMINATING ADDRESS SPACE, THE ASID TPUT BITS IN*/
*   /* THE TSB ARE CLEARED. IEDAYTPQ IS INVOKED TO INFORM TCAM NOT TO*/
*   /* EXPECT MORE OUTPUT. IN ADDITION THE STATUS ROUTINE IS INVOKED */
*   /* TO TO MAKE ALL TASKS ASSOCIATED WITH THE TSB ACTIVE.          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0506
*SRCHTCAM:                                                      01S0506
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                       01S0506
SRCHTCAM DS    0H                                               01S0507
*   RFY                                                         01S0507
*     RLINK RSTD;                                               01S0507
*   SAVE14A=RLINK;                  /* SAVE RETURN ADDRESS           */
         ST    RLINK,SAVE14A                                    01S0508
*   RFY                                                         01S0509
*     RLINK UNRSTD;                                             01S0509
*                                                               01S0509
*   /*****************************************************************/
*   /*                                                               */
*   /* SEE IF TSO/TCAM IS UP                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0510
*   IF CVTAQAVT^=0&                 /* SEE IF TCAM IS UP             */
*       TCXRPT^=0 THEN              /* AND TSO IS UP                 */
*                                                               01S0510
         SLR   @07,@07                                          01S0510
         L     @04,CVTPTR                                       01S0510
         L     @04,CVTAQAVT(,@04)                               01S0510
         CR    @04,@07                                          01S0510
         BE    @RF00510                                         01S0510
         L     @04,TCXRPT(,@04)                                 01S0510
         CR    @04,@07                                          01S0510
         BE    @RF00510                                         01S0510
*     /***************************************************************/
*     /*                                                             */
*     /* IF TSO/TCAM IS UP SET UP FOR SEARCH OF TCAM TSB CHAIN       */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0511
*     DO;                           /* START **TSO UP**              */
*       RFY                                                     01S0512
*        (RWORKY,                                               01S0512
*         RWORKW,                                               01S0512
*         RWORKX) RSTD;                                         01S0512
*       RWORKY=TCXRPT;              /* GET ADDR OF TIOCRPT           */
         LR    RWORKY,@04                                       01S0513
*       GEN(USING TIOCRPT,RWORKY)                               01S0514
*       REFS(RWORKY);                                           01S0514
         USING TIOCRPT,RWORKY
*       RWORKW=TIOCTSB;             /* GET ADDR OF FIRST TCAM TSB    */
         L     RWORKW,TIOCTSB-1                                 01S0515
         LA    RWORKW,0(,RWORKW)                                01S0515
*       RWORKX=TIOCTSBS;            /* GET SIZE OF TSB               */
         SLR   RWORKX,RWORKX                                    01S0516
         IC    RWORKX,TIOCTSBS                                  01S0516
*       RFY                                                     01S0517
*         TSB BASED(RWORKW);                                    01S0517
*                                                               01S0517
*       /*************************************************************/
*       /*                                                           */
*       /* FOR EACH TSB ON THE CHAIN, SEE IF THE TSB IS IN USE AND   */
*       /* THE TSBWTJID IS THE ASID OF THE TERMINATING ADDRESS SPACE */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0518
*       DO I=1 TO TIOCNTSB;         /* START **CHECK TSB/ASID** 01S0518
*                                      TIOCNTSB IS NUMBER OF TSBS    */
         LA    I,1                                              01S0518
         B     @DE00518                                         01S0518
@DL00518 DS    0H                                               01S0519
*         IF TSBINUSE='1'B&         /* IF TSB IN USE                 */
*             TSBWTJID=RMPLASID THEN/* AND ASIDS MATCH FOR XMTPUT    */
*                                                               01S0519
         TM    TSBINUSE(RWORKW),B'10000000'                     01S0519
         BNO   @RF00519                                         01S0519
         LH    @04,RMPLASID(,RRMPL)                             01S0519
         N     @04,@CF01708                                     01S0519
         CH    @04,TSBWTJID(,RWORKW)                            01S0519
         BNE   @RF00519                                         01S0519
*           /*********************************************************/
*           /*                                                       */
*           /* IF ASID MATCH IS FOUND, ZERO THE HIGH ORDER BYTE OF   */
*           /* TSBECB. IF THE TPUT IS IN PROGRESS, LOOK AT THE NEXT  */
*           /* TSB                                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0520
*           DO;                     /* START **TSB MATCH FOUND**     */
*             WKTSECB1=0;           /* TURN OFF POST AND WAIT BITS   */
         MVI   WKTSECB1(RWORKW),X'00'                           01S0521
*             IF TSBTJIP='0'B THEN  /* IF TPUT IN PROGRESS (TJIP=1)
*                                      LOOK AT NEXT TSB ELSE CONTIN  */
*                                                               01S0522
         TM    TSBTJIP(RWORKW),B'00000010'                      01S0522
         BNZ   @RF00522                                         01S0522
*               /*****************************************************/
*               /*                                                   */
*               /* CLEAR X-MEMORY TPUT INDICATORS IN THE TSB         */
*               /*                                                   */
*               /*****************************************************/
*                                                               01S0523
*               DO;                 /* START **USE THIS TSB**        */
*                 TSBOWIP='0'B;     /* CLEAR CROSS MEMORY TPUT       */
*                 TSBTJIP='0'B;     /* RELATED TSB INDICATORS        */
*                 TSBTJBF='0'B;                                 01S0526
*                 TSBTJOW='0'B;                                 01S0527
         NI    TSBOWIP(RWORKW),B'11011000'                      01S0527
*                 TSBNOBUF='0'B;                                01S0528
         NI    TSBNOBUF(RWORKW),B'11101111'                     01S0528
*                 IF CVTTCRDY='1'B& /* IF TCAM STILL UP              */
*                     TSBCANC='0'B THEN/* AND USER STILL AROUND      */
*                                                               01S0529
         L     @04,CVTPTR                                       01S0529
         TM    CVTTCRDY(@04),B'10000000'                        01S0529
         BNO   @RF00529                                         01S0529
         TM    TSBCANC(RWORKW),B'00000100'                      01S0529
         BNZ   @RF00529                                         01S0529
*                   /*************************************************/
*                   /*                                               */
*                   /* SET TSBF1M AND TSBF1V USING COMPARE AND SWAP  */
*                   /* ON THE WORD CONTAINING THEM                   */
*                   /*                                               */
*                   /*************************************************/
*                                                               01S0530
*                   DO;             /* START **TPOST**               */
*                     RFY                                       01S0531
*                      (REG0,                                   01S0531
*                       REG15) RSTD;                            01S0531
*                     REG0=WKTSBF2M;/* GET WORD CONTAINING MASK AND
*                                      VALUE FOR QCBTSOF1            */
         L     REG0,WKTSBF2M(,RWORKW)                           01S0532
*CSLOOP4:                           /* SET UP CS TO TURN OFF QCBPARTO
*                                      SO TCAM WONT EXPECT MORE 01S0533
*                                      OUTPUT                        */
*                     REG15=REG0;   /* SAVE IN WORK REG              */
CSLOOP4  LR    REG15,REG0                                       01S0533
*                     GEN(O REG15,TPSTMSK1)/* SET MASK               */
*                     REFS(TPSTMSK1) SETS(REG15);               01S0534
         O REG15,TPSTMSK1
*                     GEN(N REG15,TPSTMSK2)/* SET VALUE              */
*                     REFS(TPSTMSK2) SETS(REG15);               01S0535
         N REG15,TPSTMSK2
*                     CS(REG0,REG15,WKTSBF2M);                  01S0536
         CS    REG0,@15,WKTSBF2M(RWORKW)                        01S0536
*                     BC(NE,CSLOOP4);/* LOOP TIL UPDATE DONE         */
*                                                               01S0537
         BC    7,CSLOOP4                                        01S0537
*                     /***********************************************/
*                     /*                                             */
*                     /* INVOKE IEDAYTPQ TO TPOST THE TSB TO IEDAYP  */
*                     /* SO THAT TCAM WILL NOT EXPECT MORE OUTPUT.   */
*                     /* THE REQUIRED INTERFACE IS: REG0 = RETURN    */
*                     /* ADDRESS REG1 = 0 TO TPOST TSB TO IEDAYP REG2*/
*                     /* = TSB ADDRESS REG13 = ADDRESS OF 6-WORD SAVE*/
*                     /* AREA IN TIOCRPT REG15 = EPA OF IEDAYTPQ     */
*                     /*                                             */
*                     /***********************************************/
*                                                               01S0538
*                     RFY                                       01S0538
*                      (REG1,                                   01S0538
*                       REG2,                                   01S0538
*                       REG3) RSTD;                             01S0538
*                                                               01S0538
*                     /***********************************************/
*                     /*                                             */
*                     /* THE ADDRESS OF IEDAYTPQ (TPOST) IS OBTAINED */
*                     /* BY ISSUING THE LOAD MACRO. LOAD WILL RETURN */
*                     /* THE ADDRESS OF THE ROUTINE IN REG 0. THE CMS*/
*                     /* AND LOCAL LOCKS MUST BE RELEASED SINCE THERE*/
*                     /* IS NOT A BRANCH ENTRY TO LOAD. THE LOCKS ARE*/
*                     /* RE-OBTAINED AS SOON AS THE LOAD IS DONE.    */
*                     /* IEDAYTPQ IS IN LPALIB                       */
*                     /*                                             */
*                     /***********************************************/
*                                                               01S0539
*                     GEN CODE REFS(CVTPTR,CVTSPOST,CVTMAP,PSALITA,FLC,
*                         RSAVE,TPQBASE)/*                   @ZM21005*/
*                         SETS(REG0,REG1,REG14,REG15,TPQBASE);  01S0539
         SETLOCK RELEASE,TYPE=CMS,REGS=USE,RELATED=(LOAD) /* @ZM21005*/
         SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,RELATED=(LOAD)  /*
*                                                            @ZM21005*/
         LOAD  EP=IEDAYTPQ          /* GET ADDRESS OF TPOST          */
         ST    REG0,TPQBASE         /* SAVE BASE OF TPOST    @ZM21005*/
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,               *
               RELATED=(LOAD)       /*                       @ZM21005*/
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,RELATED=(LOAD) /*
*                                                            @ZM21005*/
*                     REG15=ADDR(IEDAYTPQ);/* GET ADDRESS OF ROUTINE
*                                      FOR TPOST             @ZM21005*/
         L     REG15,TPQBASE                                    01S0540
*                     REG1=0;       /* SET REG1 TO ZERO              */
         SLR   REG1,REG1                                        01S0541
*                     REG2=RWORKW;  /* ADDR TSB IN RWORKW            */
         LR    REG2,RWORKW                                      01S0542
*                     REG3=RSAVE;   /* SAVE ADDR OF SAVEAREA         */
         LR    REG3,RSAVE                                       01S0543
*                     GEN(LA RSAVE,TIOCSAVE+44)                 01S0544
*                     SETS(RSAVE) REFS(TIOCSAVE);/* ADDR OF LAST 6
*                                      WORDS IN TIOCSAVE             */
         LA RSAVE,TIOCSAVE+44
*                     GEN(BALR REG0,REG15)/* INVOKE IEDAYTPQ         */
*                     REFS(REG15) SETS(REG0);                   01S0545
         BALR REG0,REG15
*                     GEN(MODESET EXTKEY=SUPR);/* CONTROL RETURNS IN
*                                      KEY 6-GET SUPR AGAIN  @YM03226*/
         MODESET EXTKEY=SUPR
*                     RSAVE=REG3;   /* RESTORE SAVE AREA ADD         */
         LR    RSAVE,REG3                                       01S0547
*                     RFY                                       01S0548
*                      (REG0,                                   01S0548
*                       REG1,                                   01S0548
*                       REG2,                                   01S0548
*                       REG3,                                   01S0548
*                       REG15) UNRSTD;                          01S0548
*                   END;            /* END OF **TPOST**              */
*                 ELSE                                          01S0550
*                   ;               /* TPOST NOT NEEDED              */
*                                                               01S0550
@RF00529 DS    0H                                               01S0551
*                 /***************************************************/
*                 /*                                                 */
*                 /* CALL STATUS IF AN OWAIT IS IN PROGRESS AND TSO  */
*                 /* HAS NOT OUT OF CORE ABENDED                     */
*                 /*                                                 */
*                 /***************************************************/
*                                                               01S0551
*                 IF TSBWOWIP='1'B& /* IF OWAIT IN PROGRESS          */
*                     TSBOCAB='0'B THEN/* AND NOT OUT-OF-CORE ABEND  */
         TM    TSBWOWIP(RWORKW),B'00010000'                     01S0551
         BNO   @RF00551                                         01S0551
         TM    TSBOCAB(RWORKW),B'10000000'                      01S0551
         BNZ   @RF00551                                         01S0551
*                   DO;             /* START **STATUS**              */
*                     TSBOWAIT='0'B;/* TURN OFF OWAIT BIT - THIS
*                                      INSTR IN TCAM IS NI      01S0553
*                                      TSBFLG4,TSBOWAIT - APPARENTLY
*                                      AN ERROR THAT FOR SOME REASON
*                                      DOES NO HARM. REPORTED TO SJ
*                                      FE 9-15-75                    */
*                                                               01S0553
         NI    TSBOWAIT(RWORKW),B'11011111'                     01S0553
*                     /***********************************************/
*                     /*                                             */
*                     /* A NORMAL TPUT IS WAITING FOR THIS TSB MAKE  */
*                     /* ALL TASKS ASSOCIATED WITH THIS TSB ACTIVE.  */
*                     /* INTERFACE TO STATUS: REG0 = ASID IN BYTES   */
*                     /* 0-1 STATUS ENTRY CODE(8) IN BYTES 2-3 REG1 =*/
*                     /* '80000000'X RESET REG13 = TCB NON-DISPATCH  */
*                     /* BIT MASK '0400'X REG14 = RETURN ADDRESS     */
*                     /* REG15 = EPA TO STATUS                       */
*                     /*                                             */
*                     /***********************************************/
*                                                               01S0554
*                     RFY                                       01S0554
*                      (REG0,                                   01S0554
*                       REG1,                                   01S0554
*                       REG3,                                   01S0554
*                       REG14,                                  01S0554
*                       REG15) RSTD;                            01S0554
*                     RFY                                       01S0555
*                       ASCB BASED(TSBASCBA);/*              @YM03242
*                                      GET TARGET ASCB               */
*                     REG0=(ASCBASID*65536)×'0008'X;/* PUT ASID IN
*                                      BYTES 0-1 AND ENTRY CODE 01S0556
*                                      '0008'X IN BYTES 2-3.         */
         L     @04,TSBASCBA(,RWORKW)                            01S0556
         LH    REG0,ASCBASID(,@04)                              01S0556
         N     REG0,@CF01708                                    01S0556
         SLA   REG0,16                                          01S0556
         O     REG0,@CF01681                                    01S0556
*                     RFY                                       01S0557
*                       ASCB BASED(PSAAOLD);/*               @YM03242*/
*                     REG1='80000000'X;/* SHOW RESET REQUEST         */
         L     REG1,@CF01623                                    01S0558
*                     REG3=RSAVE;   /* SAVE SAVEAREA ADDR            */
         LR    REG3,RSAVE                                       01S0559
*                     RSAVE='0400'X;/* NON-DISPATCH MASK             */
         LA    RSAVE,1024                                       01S0560
*                     CALL STATUS;  /* DISPATCH WAITING TASKS        */
         L     @04,CVTPTR                                       01S0561
         L     @04,CVTABEND(,@04)                               01S0561
         L     @15,SCVTSTAT(,@04)                               01S0561
         BALR  @14,@15                                          01S0561
*                     RSAVE=REG3;   /* RESTORE SAVEAREA ADD          */
         LR    RSAVE,REG3                                       01S0562
*                     RFY                                       01S0563
*                      (REG0,                                   01S0563
*                       REG1,                                   01S0563
*                       REG3,                                   01S0563
*                       REG14,                                  01S0563
*                       REG15) UNRSTD;                          01S0563
*                   END;            /* END OF **STATUS**             */
*                 ELSE                                          01S0565
*                   ;               /* NO NEED FOR STATUS            */
@RF00551 DS    0H                                               01S0566
*               END;                /* END OF **USE THIS TSB**       */
*             ELSE                                              01S0567
*               ;                   /* ASID TPUT IN PROGRESS         */
@RF00522 DS    0H                                               01S0568
*           END;                    /* END OF **TSB MATCH FOUND**    */
*         ELSE                                                  01S0569
*           ;                       /* TSB NOT IN USE OR NO MATCH    */
*                                                               01S0569
@RF00519 DS    0H                                               01S0570
*         /***********************************************************/
*         /*                                                         */
*         /* INCREMENT POINTER BY SIZE OF TSB                        */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0570
*         RWORKW=RWORKW+RWORKX;     /* INCR TO NEXT TSB              */
         ALR   RWORKW,RWORKX                                    01S0570
*       END;                        /* END OF **CHECK TSB/ASID **
*                                      LOOP TO SEARCH TSB CHAIN      */
         AL    I,@CF00101                                       01S0571
@DE00518 CH    I,TIOCNTSB                                       01S0571
         BNH   @DL00518                                         01S0571
*       RFY                                                     01S0572
*        (RWORKY,                                               01S0572
*         RWORKW,                                               01S0572
*         RWORKX) UNRSTD;                                       01S0572
*       RFY                                                     01S0573
*         TSB BASED(TSBPTR);        /* RESET TSB BASE POINTER        */
*     END;                          /* END OF **TSO UP**             */
*   ELSE                                                        01S0575
*     ;                             /* TCAM/TSO NOT UP               */
*                                                               01S0575
@RF00510 DS    0H                                               01S0576
*   /*****************************************************************/
*   /*                                                               */
*   /* RESTORE RETURN ADDRESS AND RETURN TO CALLER                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0576
*   RFY                                                         01S0576
*     RLINK RSTD;                                               01S0576
*   RLINK=SAVE14A;                  /* RESTORE RETURN ADDRESS        */
         L     RLINK,SAVE14A                                    01S0577
*   RFY                                                         01S0578
*     RLINK UNRSTD;                                             01S0578
*   RETURN;                         /* RETURN TO CALLER              */
@EL00011 DS    0H                                               01S0579
@EF00011 DS    0H                                               01S0579
@ER00011 BR    @14                                              01S0579
*   END SRCHTCAM;                                               01S0580
         EJECT
*                                                               01S0581
*   /*****************************************************************/
*   /*                                                               */
*   /* SRCHVTAM SEARCH VTAM TSB CHAIN FOR ASID TPUT FROM THE         */
*   /* TERMINATING ADDRESS SPACE THEN A VTAM TSB IS FOUND WHICH      */
*   /* INDICATES AN ASID TPUT FROM THE TERMINATING ADRESS SPACE, THE */
*   /* CSA AREA (TVCS) ASSOCIATED WITH THE TPUT IS EXAMINED. IF THE  */
*   /* TARGET HAS RELINQUISED CONTROL, AN SPOST IS ISSUED TO LET     */
*   /* OUTSTANDING POSTS COMPLETE, AND THE SRB AND TVCS AREAS ARE    */
*   /* FREED IF THE TARGET HAS NOT RELINQUISHED CONTROL, THE TVCS BIT*/
*   /* IS SET WHICH INDICATES THE SOURCE RELINQUISHED CONTROL        */
*   /* (TVCSRCRC) AND THE TARGET ADDRESS SPACE IS LEFT WITH THE      */
*   /* RESPONSIBILITY OF FREEING THE TVCS AND RELATED SRB.           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0581
*SRCHVTAM:                                                      01S0581
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                       01S0581
SRCHVTAM DS    0H                                               01S0582
*   RFY                                                         01S0582
*     RLINK RSTD;                                               01S0582
*   SAVE14A=RLINK;                  /* SAVE RETURN ADDRESS           */
         ST    RLINK,SAVE14A                                    01S0583
*   RFY                                                         01S0584
*     RLINK UNRSTD;                                             01S0584
*   RFY                                                         01S0585
*     RWORKV RSTD;                                              01S0585
*   RWORKV=TCASTSB;                 /* POINT TO FIRST VTAM TSB       */
         L     @07,TCASTPTR                                     01S0586
         L     RWORKV,TCASTSB(,@07)                             01S0586
*   RFY                                                         01S0587
*     TSB BASED(RWORKV);                                        01S0587
*                                                               01S0587
*   /*****************************************************************/
*   /*                                                               */
*   /* EACH TSB ON THE TSO/VTAM TSB CHAIN IS EXAMINED FOR ASID TPUT  */
*   /* FROM THE TERMINATING ADDRESS SPACE. WHEN THE ADDRESS OF THE   */
*   /* NEXT (OR FIRST) TSB IS 0, THE SEARCH IS ENDED.                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0588
*   DO WHILE(RWORKV^=0);            /* START **LOOP THRU TSBS**      */
         B     @DE00588                                         01S0588
@DL00588 DS    0H                                               01S0589
*     IF TSBXCSAP^=0&               /* IS THERE A CROSS-MEM TPUT     */
*         TSBWTJID=RMPLASID THEN    /* FROM TERMINATING A.S.         */
*                                                               01S0589
         L     @07,TSBEXTNT(,RWORKV)                            01S0589
         L     @07,TSBXCSAP(,@07)                               01S0589
         LTR   @07,@07                                          01S0589
         BZ    @RF00589                                         01S0589
         LH    @15,RMPLASID(,RRMPL)                             01S0589
         N     @15,@CF01708                                     01S0589
         CH    @15,TSBWTJID(,RWORKV)                            01S0589
         BNE   @RF00589                                         01S0589
*       /*************************************************************/
*       /*                                                           */
*       /* WHEN A TSB IS FOUND WHICH INDICATES AN ASID TPUT FROM THE */
*       /* TERMINATING ADDRESS SPACE, THE TVCS INDICATORS ARE PICKED */
*       /* UP FOR EXAMINATION. A COMPARE AND SWAP LOOP IS SET UP TO  */
*       /* DETERMINE IF THE TARGET HAS RELINQUISHED CONTROL, AND IF  */
*       /* NOT, TO SET THE SOURCE AS RELINQUISHED CONTROL.           */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0590
*       DO;                         /* START **ASID TPUT FOUND**     */
*         RFY                                                   01S0591
*          (RWORKW,                                             01S0591
*           RWORKX) RSTD;                                       01S0591
*         RWORKW=TVCSINDS;          /* GET TVCS INDICATORS           */
         L     RWORKW,TVCSINDS(,@07)                            01S0592
*CSLOOP5:                                                       01S0593
*         WRKINDS=RWORKW;           /* RWORKW IS SET WHEN CS FAILS   */
CSLOOP5  ST    RWORKW,WRKINDS                                   01S0593
*         IF WRKTGRC='1'B THEN      /* IF TARGET GONE - FREE SRB
*                                      AND TVCS                      */
*                                                               01S0594
         TM    WRKTGRC,B'01000000'                              01S0594
         BNO   @RF00594                                         01S0594
*           /*********************************************************/
*           /*                                                       */
*           /* WHEN THE TARGET HAS RELINQUISHED CONTROL, SPOST IS    */
*           /* ISSUED TO ALLOW OUTSTANDING POSTS TO COMPLETE. THE    */
*           /* LOCKS HAVE TO BE RELEASED SINCE THERE IS NOT A BRANCH */
*           /* ENTRY TO SPOST. FOLLOWING THE SPOST, THE TVCS AND SRB */
*           /* ARE FREED.                                            */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0595
*           DO;                                                 01S0595
*             RFY                                               01S0596
*               REG1 RSTD;                                      01S0596
*             GEN CODE REFS(CVTPTR,CVTSPOST,CVTMAP,PSALITA,FLC,RSAVE)
*                 SETS(REG0,REG1,REG14,REG15);                  01S0597
*                                   /*                       @ZM21005*/
         SETLOCK RELEASE,TYPE=CMS,REGS=USE,RELATED=(SPOST)   /*
*                                                            @ZM21005*/
         SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,RELATED=(SPOST) /*
*                                                            @ZM21005*/
         SPOST
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,               *
               RELATED=(SPOST)      /*                       @ZM21005*/
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,RELATED=(SPOST)
*                                   /*                       @ZM21005*/
*             TSBWTJID=0;           /* CLEAR TSBWTJID                */
         SLR   @07,@07                                          01S0598
         STH   @07,TSBWTJID(,RWORKV)                            01S0598
*             REG1=TVCSSRBA;        /* GET SRB ADDRESS               */
         L     @07,TSBEXTNT(,RWORKV)                            01S0599
         L     @07,TSBXCSAP(,@07)                               01S0599
         L     REG1,TVCSSRBA-1(,@07)                            01S0599
         LA    REG1,0(,REG1)                                    01S0599
*             CALL FREESRB;         /* FREE SRB                      */
         BAL   @14,FREESRB                                      01S0600
*             REG1=RWORKV;          /* GET ADDRESS OF THIS TSB       */
         LR    REG1,RWORKV                                      01S0601
*             CALL FREETVCS;        /* FREE TVCS                     */
         BAL   @14,FREETVCS                                     01S0602
*             RFY                                               01S0603
*               REG1 UNRSTD;                                    01S0603
*           END;                                                01S0604
*         ELSE                      /* TARGET STILL THERE            */
*                                                               01S0605
*           /*********************************************************/
*           /*                                                       */
*           /* SET THE SOURCE RELINQUISHES CONTROL BIT IN THE TVCS   */
*           /* INDICATORS AND PERFORM THE COMPARE AND SWAP TO STORE  */
*           /* THE INDICATORS BACK IN THE TVCS.                      */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0605
*           DO;                                                 01S0605
         B     @RC00594                                         01S0605
@RF00594 DS    0H                                               01S0606
*             WRKSRCRC='1'B;        /* SET SOURCE RELINQUISHES CNTL  */
         OI    WRKSRCRC,B'10000000'                             01S0606
*             RWORKX=WRKINDS;       /* SET UPDATE REGISTER           */
         L     RWORKX,WRKINDS                                   01S0607
*             CS(RWORKW,RWORKX,TVCSINDS);                       01S0608
         L     @07,TSBEXTNT(,RWORKV)                            01S0608
         L     @07,TSBXCSAP(,@07)                               01S0608
         CS    RWORKW,@06,TVCSINDS(@07)                         01S0608
*             BC(NE,CSLOOP5);       /* REPEAT IF CS FAILED           */
         BC    7,CSLOOP5                                        01S0609
*           END;                                                01S0610
*         RFY                                                   01S0611
*          (RWORKW,                                             01S0611
*           RWORKX) UNRSTD;                                     01S0611
@RC00594 DS    0H                                               01S0612
*       END;                        /* END OF **ASID TPUT FOUND**    */
*     ELSE                                                      01S0613
*       ;                           /* TPUT NOT FOUND IN THIS TSB    */
@RF00589 DS    0H                                               01S0614
*     RWORKV=TSBXFWD;               /* POINT TO NEXT TSB             */
         L     @07,TSBEXTNT(,RWORKV)                            01S0614
         L     @07,TSBXFWD(,@07)                                01S0614
         LR    RWORKV,@07                                       01S0614
*   END;                            /* END OF ** LOOP THRU TSBS**    */
@DE00588 LTR   RWORKV,RWORKV                                    01S0615
         BNZ   @DL00588                                         01S0615
*   RFY                                                         01S0616
*     TSB BASED(TSBPTR);                                        01S0616
*   RFY                                                         01S0617
*     RWORKV UNRSTD;                                            01S0617
*                                                               01S0617
*   /*****************************************************************/
*   /*                                                               */
*   /* RESTORE RETURN ADDRESS AND RETURN TO CALLER                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0618
*   RFY                                                         01S0618
*     RLINK RSTD;                                               01S0618
*   RLINK=SAVE14A;                  /* RESTORE RETURN ADDRESS        */
         L     RLINK,SAVE14A                                    01S0619
*   RFY                                                         01S0620
*     RLINK UNRSTD;                                             01S0620
*   RETURN;                                                     01S0621
@EL00012 DS    0H                                               01S0621
@EF00012 DS    0H                                               01S0621
@ER00012 BR    @14                                              01S0621
*   END SRCHVTAM;                                               01S0622
*                                                               01S0622
@PB00012 DS    0H                                               01S0622
*   /*****************************************************************/
*   /*                                                               */
*   /* THE TIOCRPT DOES NOT HAVE A PLS VERSION. THE MACRO IS INVOKED */
*   /* WITH A GEN STATEMENT.                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0623
*   GEN(IKJTIOCP LIST=YES)                                      01S0623
*   DEFS(TIOCNTSB,TIOCSAVE,TIOCTSB,TIOCTSBS,TIOCRPT);           01S0623
         IKJTIOCP LIST=YES
*   END IKTAY8                                                  01S0624
