FREETVCS DS    0H                                               01S0397
*   RFY                                                         01S0397
*    (RLINK,                                                    01S0397
*     REG4) RSTD;                                               01S0397
*   SAVE14B=RLINK;                  /* SAVE RETURN ADDRESS           */
         ST    RLINK,SAVE14B                                    01S0398
*   SAVE04A=REG4;                   /* SAVE REG4 DESTROYED BY BRANCH
*                                      ENTERED FREEMAIN-NEEDED BY
*                                      SRCHVTAM                      */
         ST    REG4,SAVE04A                                     01S0399
*   RFY                                                         01S0400
*    (RWORKX,                                                   01S0400
*     RWORKW,                                                   01S0400
*     REG0,                                                     01S0400
*     REG1,                                                     01S0400
*     REG3,                                                     01S0400
*     REG15,                                                    01S0400
*     REG7) RSTD;                   /*                       @YM03242*/
*   RFY                                                         01S0401
*     RWORKY RSTD;                                              01S0401
