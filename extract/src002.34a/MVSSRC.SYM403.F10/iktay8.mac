*                                                               01S0344
*   /*****************************************************************/
*   /*                                                               */
*   /* REQREL: REQUEST TCAS TO RELEASE TERMINAL NOT YET OWNED BY THE */
*   /* TERMINATING ADDRESS SPACE. THE LOGON BUFFER IS USED TO BUILD  */
*   /* THE WORK ELEMENT WHICH IS INITIALIZED TO REQUEST RELEASE OF   */
*   /* THE TERMINAL. COMPARE AND SWAP IS USED TO HANG THE WORK       */
*   /* ELEMENT LIFO IN THE QUEUE, POINTED TO BY TCASTTQH.            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0344
*REQREL:                                                        01S0344
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                       01S0344
*                                                               01S0344
REQREL   DS    0H                                               01S0345
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE WORK ELEMENT TO REQUEST RELEASE                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0345
*   RFY                                                         01S0345
*     RLINK RSTD;                   /*                       @YM03223*/
*   SAVE14A=RLINK;                  /*                       @YM03223*/
         ST    RLINK,SAVE14A                                    01S0346
*   RFY                                                         01S0347
*     RWORKX RSTD;                                              01S0347
*   RWORKX=LBUFLEN;                 /* GET LENGTH OF LOGON BUFFER    */
         L     @15,TSBEXTNT(,TSBPTR)                            01S0348
         L     @04,TSBXLBUF(,@15)                               01S0348
         LH    RWORKX,LBUFLEN(,@04)                             01S0348
*   RFY                                                         01S0349
*     WESTD BASED(TSBXLBUF);        /* USE LOGON BUFFER FOR WORK EL  */
*   WECODEIT=TASKCD;                /* INITIALIZE WORK ELEM  @YM03223*/
         MVI   WECODEIT(@04),X'02'                              01S0350
*   WECODEIF=RELCODE;               /* TO REQ RELEASE OF TERMINAL    */
         MVI   WECODEIF(@04),X'22'                              01S0351
*   WECODE2=0;                                                  01S0352
         SLR   @07,@07                                          01S0352
         STH   @07,WECODE2(,@04)                                01S0352
*   WELEN=RWORKX;                   /* GET LENGTH OF WORK ELEMENT    */
         STH   RWORKX,WELEN(,@04)                               01S0353
*   WEASCB=0;                                                   01S0354
         ST    @07,WEASCB(,@04)                                 01S0354
*   WEECB=0;                                                    01S0355
         ST    @07,WEECB(,@04)                                  01S0355
*   WERPL=TSBXRPL;                  /* GET ADDR OF RPL IN TCAS       */
         L     @15,TSBXRPL(,@15)                                01S0356
         ST    @15,WERPL(,@04)                                  01S0356
*   RFY                                                         01S0357
*     RWORKX UNRSTD;                                            01S0357
*                                                               01S0357
*   /*****************************************************************/
*   /*                                                               */
*   /* COMPARE AND SWAP TO HANG THE WORK ELEMENT ON THE TCAS WORK    */
*   /* ELEMENT QUEUE                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0358
*   RFY                                                         01S0358
*    (REG4,                                                     01S0358
*     RWORKW) RSTD;                                             01S0358
*   REG4=TCASTTQH;                  /* GET ADDR OF FIRST Q-ELEMENT   */
         ST    @04,@TF00001                                     01S0359
         L     @15,TCASTPTR                                     01S0359
         L     REG4,TCASTTQH(,@15)                              01S0359
*   RWORKW=ADDR(WESTD);             /* GET ADDR OF NEW Q-ELEMENT     */
         L     RWORKW,@TF00001                                  01S0360
*CSLOOP3:                                                       01S0361
*   IF REG4>=0 THEN                                             01S0361
CSLOOP3  LTR   REG4,REG4                                        01S0361
         BM    @RF00361                                         01S0361
*     DO;                                                       01S0362
*       WENEXT=REG4;                /* POINT NEW ELEM TO NEXT (OR TO
*                                      TCASTTQH WHEN CS FAILED       */
         L     @15,TSBEXTNT(,TSBPTR)                            01S0363
         L     @15,TSBXLBUF(,@15)                               01S0363
         ST    REG4,WENEXT(,@15)                                01S0363
*       CS(REG4,RWORKW,TCASTTQH);   /* COMPARE AND SWAP TO PUT NEW
*                                      ELEM ON QUEUE                 */
         L     @15,TCASTPTR                                     01S0364
         CS    REG4,@05,TCASTTQH(@15)                           01S0364
*       BC(NE,CSLOOP3);             /* LOOP UNTIL DONE               */
         BC    7,CSLOOP3                                        01S0365
*       RFY                                                     01S0366
*         WESTD BASED;              /* DROP WESTD ADDRESSABILITY     */
*       TSBXLBUF=0;                 /* CLEAR BUFFER POINTER          */
*                                                               01S0367
         L     @15,TSBEXTNT(,TSBPTR)                            01S0367
         SLR   @07,@07                                          01S0367
         ST    @07,TSBXLBUF(,@15)                               01S0367
*       /*************************************************************/
*       /*                                                           */
*       /* POST TCAS THAT A WORK ELEMENT HAS BEEN PLACED ON THE QUEUE*/
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0368
*       RFY                                                     01S0368
*        (RWORKX,                                               01S0368
*         RWORKY) RSTD;                                         01S0368
*       REG4=REG10;                 /* SAVE REGS 10-13 IN WORK REGS  */
         LR    REG4,REG10                                       01S0369
*       RWORKW=REG11;               /* SINCE THEY ARE DESTROYED BY   */
         LR    RWORKW,REG11                                     01S0370
*       RWORKX=REG12;               /* POST                          */
         LR    RWORKX,REG12                                     01S0371
*       RWORKY=REG13;                                           01S0372
*                                                               01S0372
         LR    RWORKY,REG13                                     01S0372
*       /*************************************************************/
*       /*                                                           */
*       /* POST IS INVOKED VIA A BRANCH ENTRY WITH THE FOLLOWING     */
*       /* INTERFACE REG10 = COMPLETION CODE REG11 = ECB             */
*       /* ADDRESS-'80'X IN BYTE 0 FOR CROSS MEMORY POST REG12 =     */
*       /* ERRET ADDRESS REG13 = ASCB ADDRESS REG14 = RETURN ADDRESS */
*       /* REG15 = POST EPA (CVT0PT01) UPON RETURN REGS 0-9,14 ARE   */
*       /* UNCHANGED THE LOCAL LOCK ONLY IS HELD FOR THE BRANCH TO   */
*       /* CONFORM WITH DOCUMENTATION. THEREFORE THE CMS LOCK IS     */
*       /* RELEASED BEFORE AND OBTAINED AFTER THE POST               */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0373
*       REG11=TCASXECB;             /* ADDR OF ECB ----- NOTE THAT
*                                      SINCE R10 IS BASE FOR @DATD
*                                      (TCASPTR IS THERE) R11 IS SET
*                                      BEFORE R10            @YM03223*/
         L     @15,TCASTPTR                                     01S0373
         L     REG11,TCASXECB(,@15)                             01S0373
*       REG10=0;                    /* SET CONDITION CODE    @YM03223*/
         SLR   REG10,REG10                                      01S0374
*       REG11=REG11×'80000000'X;    /* SET XMPOST            @YM03223*/
         O     REG11,@CF01623                                   01S0375
*       REG12=ADDR(CVTBRET);        /* ADDR OF ERRET                 */
         L     REG12,CVTPTR                                     01S0376
         LA    REG12,CVTBRET(,REG12)                            01S0376
*       GEN CODE REFS(REG13,PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);
*                                   /*                       @ZM21005*/
         SETLOCK RELEASE,TYPE=CMS,REGS=USE,RELATED=(POST) /* @ZM21005*/
         DROP  RDATA                /*                       @ZA12000*/
         USING @DATD,REG4           /* USE REG4,R10 IS GONE  @YM03232*/
*       RFY                                                     01S0378
*         REG15 RSTD;               /*                       @YM03223*/
*       REG13=TCASASCB;             /* ADDR OF ASCB          @ZA12000*/
         L     @07,TCASTPTR                                     01S0379
         L     REG13,TCASASCB(,@07)                             01S0379
*       REG15=CVT0PT01;             /* ADDR OF ENTRY                 */
         L     @07,CVTPTR                                       01S0380
         L     REG15,CVT0PT01(,@07)                             01S0380
*       GEN(BALR REG14,REG15)                                   01S0381
*       REFS(REG15) SETS(REG14);    /* CALL POST-TERMINAL RELEASE    */
         BALR REG14,REG15
*       REG13=RWORKY;               /* RESTORE SAVE AREA ADDR FOR
*                                      SETLOCK                       */
         LR    REG13,RWORKY                                     01S0382
*       GEN CODE REFS(REG13,PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);
*                                   /*                       @ZM21005*/
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,                 *
               RELATED=(POST)       /*                       @ZM21005*/
*       REG10=REG4;                 /* RESTORE REGS 10-12 FROM       */
         LR    REG10,REG4                                       01S0384
*       REG11=RWORKW;               /* SAVE REGS                     */
         LR    REG11,RWORKW                                     01S0385
*       REG12=RWORKX;                                           01S0386
         LR    REG12,RWORKX                                     01S0386
*       GEN(DROP REG4);             /* DROP TEMP ADDRESSING  @ZA12000*/
         DROP REG4
*       GEN(USING @DATD,RDATA)                                  01S0388
*       REFS(RDATA);                /* RESTORE AUTOAREA         01S0388
*                                      ADDRESSABILITY        @YM03232*/
         USING @DATD,RDATA
*     END;                                                      01S0389
*   ELSE                                                        01S0390
*     ;                             /* TCAS NOT ACTIVE               */
@RF00361 DS    0H                                               01S0391
*   RFY                                                         01S0391
*    (REG4,                                                     01S0391
*     RWORKW,                                                   01S0391
*     RWORKX,                                                   01S0391
*     RWORKY,                                                   01S0391
*     REG15) UNRSTD;                /*                       @YM03223*/
*   RLINK=SAVE14A;                  /* RESTORE RETURN REG    @YM03223*/
         L     RLINK,SAVE14A                                    01S0392
*   RFY                                                         01S0393
*     RLINK UNRSTD;                 /*                       @YM03223*/
*   RETURN;                         /* RETURN TO CALLER              */
@EL00004 DS    0H                                               01S0394
@EF00004 DS    0H                                               01S0394
@ER00004 BR    @14                                              01S0394
*   END REQREL;                                                 01S0395
