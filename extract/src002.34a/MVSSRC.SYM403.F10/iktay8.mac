*     RWORKV) RSTD;                                             01S0293
*   DO UNTIL(RWORKU=0);                                         01S0294
@DL00294 DS    0H                                               01S0295
*     IF WEECB^=0 THEN              /* SEE IF ECB AVAILABLE          */
         L     @02,WEECB(,RWORKU)                               01S0295
         LTR   @02,@02                                          01S0295
         BZ    @RF00295                                         01S0295
*       DO;                         /* POST THE ECB                  */
*         RWORKV=REG10;             /* SAVE REGS 10-13 IN WORK REGS  */
         LR    RWORKV,REG10                                     01S0297
*         RWORKW=REG11;             /* SINCE THEY ARE DESTROYED BY   */
         LR    RWORKW,REG11                                     01S0298
*         RWORKX=REG12;             /* POST                          */
         LR    RWORKX,REG12                                     01S0299
*         RWORKY=REG13;                                         01S0300
*                                                               01S0300
         LR    RWORKY,REG13                                     01S0300
*         /***********************************************************/
*         /*                                                         */
*         /* POST IS INVOKED VIA A BRANCH ENTRY WITH THE FOLLOWING   */
*         /* INTERFACE REG10 = COMPLETION CODE REG11 = ECB           */
*         /* ADDRESS-'80'X IN BYTE 0 FOR CROSS MEMORY POST REG12 =   */
*         /* ERRET ADDRESS REG13 = ASCB ADDRESS REG14 = RETURN       */
*         /* ADDRESS REG15 = POST EPA (CVT0PT01) UPON RETURN REGS    */
*         /* 0-9,14 ARE UNCHANGED THE LOCAL LOCK ONLY IS HELD FOR THE*/
*         /* BRANCH TO CONFORM WITH DOCUMENTATION. THEREFORE THE CMS */
*         /* LOCK IS RELEASED BEFORE AND OBTAINED AFTER THE POST     */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0301
*         REG10=0;                  /* SET CONDITION CODE            */
         SLR   REG10,REG10                                      01S0301
*         REG11=WEECB;              /* ADDR OF ECB                   */
         LR    REG11,@02                                        01S0302
*         REG11=REG11×'80000000'X;  /* SET XMPOST            @YM03223*/
         O     REG11,@CF01623                                   01S0303
*         REG12=ADDR(CVTBRET);      /* ADDR OF ERRET                 */
         L     REG12,CVTPTR                                     01S0304
         LA    REG12,CVTBRET(,REG12)                            01S0304
*         GEN CODE REFS(REG13,PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);
*                                   /*                       @ZM21005*/
         SETLOCK RELEASE,TYPE=CMS,REGS=USE,RELATED=(POST)
*         RFY                                                   01S0306
*          (REG14,                                              01S0306
*           REG15) RSTD;                                        01S0306
*         REG13=WEASCB;             /* ADDR OF ASCB                  */
         L     REG13,WEASCB(,RWORKU)                            01S0307
*         REG15=CVT0PT01;           /* ADDR OF ENTRY                 */
         L     @02,CVTPTR                                       01S0308
         L     REG15,CVT0PT01(,@02)                             01S0308
*         GEN(BALR REG14,REG15)                                 01S0309
*         REFS(REG15) SETS(REG14);  /* CALL POST-WESTD               */
         BALR REG14,REG15
*         REG13=RWORKY;             /* RESTORE SAVE AREA ADDR FOR
*                                      SETLOCK                       */
         LR    REG13,RWORKY                                     01S0310
*         GEN CODE REFS(REG13,PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);
*                                   /*                       @ZM21005*/
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,                 *
               RELATED=(POST)       /*                       @ZM21005*/
*         REG10=RWORKV;             /* RESTORE REGS 10-12 FROM       */
         LR    REG10,RWORKV                                     01S0312
*         REG11=RWORKW;             /* SAVE REGS                     */
         LR    REG11,RWORKW                                     01S0313
*         REG12=RWORKX;                                         01S0314
         LR    REG12,RWORKX                                     01S0314
*       END;                                                    01S0315
*     ELSE                                                      01S0316
*       ;                                                       01S0316
@RF00295 DS    0H                                               01S0317
*     RWORKU=WENEXT;                /* POINT TO NEXT ELEMENT         */
         L     RWORKU,WENEXT(,RWORKU)                           01S0317
*   END;                                                        01S0318
@DE00294 LTR   RWORKU,RWORKU                                    01S0318
         BNZ   @DL00294                                         01S0318
*   RFY                                                         01S0319
*    (RWORKW,                                                   01S0319
*     RWORKX,                                                   01S0319
*     RWORKY,                                                   01S0319
*     RWORKV,                                                   01S0319
*     REG14,                                                    01S0319
*     REG15) UNRSTD;                                            01S0319
*   RLINK=SAVE14A;                  /* RESTORE RETURN REG    @YM03223*/
         L     RLINK,SAVE14A                                    01S0320
*   RFY                                                         01S0321
*    (RWORKU,                                                   01S0321
*     RLINK) UNRSTD;                /* RELEASE RWORKU,RLINK  @YM03223*/
*   RETURN;                         /* RETURN TO CALLER              */
@EL00002 DS    0H                                               01S0322
@EF00002 DS    0H                                               01S0322
@ER00002 BR    @14                                              01S0322
*   END POSTWECB;                                               01S0323
         EJECT
*                                                               01S0324
*   /*****************************************************************/
*   /*                                                               */
*   /* FRWESTD: FREE THE WORK ELEMENT CHAIN THE ADDRESS OF THE WORK  */
*   /* ELEMENT CHAIN IS EXPECTED IN COMPLIMENT FORM IN TCASTTQH. THE */
*   /* ACTUAL ADDRESS IS COMPUTED, AND EACH ELEMENT ON THE CHAIN IS  */
*   /* FREED                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0324
*FRWESTD:                                                       01S0324
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                       01S0324
FRWESTD  DS    0H                                               01S0325
*   RFY                                                         01S0325
*     RLINK RSTD;                                               01S0325
*   SAVE14A=RLINK;                  /* SAVE RETURN ADDRESS           */
         ST    RLINK,SAVE14A                                    01S0326
*   RFY                                                         01S0327
*    (REG4,                                                     01S0327
*     RWORKW,                                                   01S0327
*     RWORKX,                                                   01S0327
*     RWORKY,                                                   01S0327
*     REG7,                                                     01S0327
*     REG0,                                                     01S0327
*     REG1,                                                     01S0327
*     REG3,                                                     01S0327
*     REG15) RSTD;                  /*                       @YM03242*/
*   RWORKW=TCASTTQH;                /* GET COMP OF QUEUE ADDRESS     */
         L     @02,TCASTPTR                                     01S0328
         L     RWORKW,TCASTTQH(,@02)                            01S0328
*   GEN(LCR RWORKW,RWORKW)                                      01S0329
*   REFS(RWORKW);                   /* GET ACTUAL ADDRESS            */
         LCR RWORKW,RWORKW
*   RFY                                                         01S0330
*     WESTD BASED(RWORKW);          /* ADDRESS THE Q-ELEMENT         */
*                                                               01S0330
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE EACH WORK ELEMENT IN THE CHAIN.                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0331
*   DO UNTIL(RWORKW=0);             /* STEP THROUGH CHAIN FREEING
*                                      EACH ELEMENT                  */
@DL00331 DS    0H                                               01S0332
*     RWORKX=WENEXT;                /* SAVE ADDR OF NEXT ELEMENT     */
         L     RWORKX,WENEXT(,RWORKW)                           01S0332
*     RWORKY=WELEN;                 /* GET LENGTH OF ELEMENT         */
         LH    RWORKY,WELEN(,RWORKW)                            01S0333
*     REG4=PSATOLD;                 /* ADDR OF CURRENT TCB   @YM03242*/
         L     REG4,PSATOLD                                     01S0334
*     REG7=PSAAOLD;                 /* ADDR OF CURRENT ASCB  @YM03242*/
         L     REG7,PSAAOLD                                     01S0335
*     GEN CODE REFS(RWORKW,RWORKY,CVTPTR,CVTCRMN,CVTMAP) SETS(REG15,
*         REG0,REG1,RLINK,REG3);                                01S0336
*                                   /*                       @YM03242*/
         FREEMAIN RC,LV=(RWORKY),A=(RWORKW),SP=SPTCAST,KEY=KEYTCAS,    *
               BRANCH=YES,RELATED=(WESTD)  /*                @YM03232*/
*     RWORKW=RWORKX;                /* POINT TO NEXT WORK ELEMENT    */
         LR    RWORKW,RWORKX                                    01S0337
*   END;                                                        01S0338
@DE00331 LTR   RWORKW,RWORKW                                    01S0338
         BNZ   @DL00331                                         01S0338
*   RFY                                                         01S0339
*    (REG4,                                                     01S0339
*     RWORKW,                                                   01S0339
*     RWORKX,                                                   01S0339
*     RWORKY,                                                   01S0339
*     REG7,                                                     01S0339
*     REG0,                                                     01S0339
*     REG1,                                                     01S0339
*     REG3,                                                     01S0339
*     REG15) UNRSTD;                /*                       @YM03242*/
*   RLINK=SAVE14A;                  /* RESTORE RETURN REGISTER       */
         L     RLINK,SAVE14A                                    01S0340
*   RFY                                                         01S0341
*     RLINK UNRSTD;                                             01S0341
*   RETURN;                         /* RETURN TO CALLER              */
@EL00003 DS    0H                                               01S0342
@EF00003 DS    0H                                               01S0342
@ER00003 BR    @14                                              01S0342
*   END FRWESTD;                                                01S0343
