         TITLE '****  IKTAY8: VTIOC LOGOFF/TERMINATION RESOURCE MANAGEM*
               MENT  ***'
IKTAY8   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                          01S0001
         DC    C'IKTAY8    78.013'                              01S0001
IKTAY81  DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00083-IKTAY81)
         ENTRY IKTAY81
IKTAY82  DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00138-IKTAY82)
         ENTRY IKTAY82
IKTAY83  DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00263-IKTAY83)
         ENTRY IKTAY83
         DROP  @15
@PROLOG  BALR  @06,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@06                                      01S0001
         LR    @03,@15                                          01S0001
         L     @00,@SIZDATD                                     01S0001
         GETMAIN  R,LV=(0)
         LR    @05,@01                                          01S0001
         USING @DATD,@05                                        01S0001
         LR    @15,@03                                          01S0001
         AH    @15,4(,@15)                                      01S0001
         BR    @15                                              01S0001
@EP00001 DS    0H                                               01S0002
SPSRB    EQU   239                  /* SUB POOL FOR TSO/VTAM SRBS    */
SPTCAST  EQU   231                  /* SUB POOL FOR TCAST TABLE      */
SPTSB    EQU   231                  /* SUB POOL FOR TSB, TSBX        */
SPLBUF   EQU   231                  /* SUB POOL FOR LOGON BUFFER     */
SPTVCS   EQU   231                  /* SUB POOL FOR TVCS             */
KEYTCAS  EQU   6                    /* TSO/VTAM STORAGE KEY          */
OFF12    EQU   12                   /* OFFSET IN SAVE AREA           */
         EJECT
*                                                               01S0083
*   /*****************************************************************/
*   /*                                                               */
*   /* IKTAY81: TCAS ADDRESS SPACE IS TERMINATING IF TSO/VTAM USERS  */
*   /* ARE STILL ACTIVE, THE REQUESTORS FOR ELEMENTS ON THE CROSS    */
*   /* MEMORY WORK ELEMENT CHAIN ARE POSTED AND THE CHAIN IS FREED.  */
*   /* IF NO TSO/VTAM USERS ARE ACTIVE, THE WORK ELEMENT CHAIN, THE  */
*   /* TIM/TOM LIST, AND THE TCAST TABLE ARE FREED                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0083
*IKTAY81:                                                       01S0083
*   ENTRY;                                                      01S0083
@EP00083 DS    0H                                               01S0084
*                                                               02S0084
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT-                                                         */
*/*      EPTLINK                                                     */
*/*                                                                  */
*/* FUNCTION-                                                        */
*/*      ENTRY POINT LINKAGE                                         */
*/*                                                          @ZM21130*/
*/********************************************************************/
*                                                               02S0084
*   DO;                             /* ENTRY LINKAGE         @ZM31130*/
*     RFY                                                       02S0085
*      (REG0,                                                   02S0085
*       REG15) RSTD;                /* FOR STM               @ZM31130*/
*     GEN(USING @PSTART,@06)        /* FOR STM               @ZM31130*/
*     (USING @DATD,@05)             /* FOR STM               @ZM31130*/
*     (L     @00,@SIZDATD)          /* FOR EXIT LINKAGE      @ZM31130*/
*     (LR    @01,@05);              /* FOR EXIT LINKAGE      @ZM31130*/
         USING @PSTART,@06
         USING @DATD,@05
         L     @00,@SIZDATD
         LR    @01,@05
*     STM(REG0,REG15,SAVEREGS);     /* SAVE ENTRY REGISTERS  @ZM21130*/
         STM   REG0,REG15,SAVEREGS                              02S0087
*     RFY                                                       02S0088
*      (REG0,                                                   02S0088
*       REG15) UNRSTD;              /* FOR STM               @ZM31130*/
*     GEN SETS(RBASE,RDATA,RSAVE);                              02S0089
*                                   /* COMMON BASE REGISTERS @ZM31130*/
         LR    RBASE,@06            /* ESTABLISH BASE        @ZM31130*/
         LR    RDATA,@05            /* ADDRESS AUTODATA AREA @ZM31130*/
         LR    RSAVE,@05            /* ADDRESS AUTODATA AREA @ZM31130*/
         DROP  @05,@06              /* ENTRY CODE BASE       @ZM31130*/
         USING @PSTART,RBASE        /* BASE ADDRESSABILITY   @ZM31130*/
         USING @DATD,RDATA          /* SAVE AREA ADDRESSABILITY
*                                                            @ZM31130*/
*   END;                            /* END OF SEGMENT        @ZM31130*/
*   TCASTPTR=REG4;                  /* GET ADDRESS OF TCAS POINTER
*                                                            @ZM21005*/
         ST    REG4,TCASTPTR                                    01S0091
*   RFY                                                         01S0092
*     REG4 UNRSTD;                  /*                       @ZM21005*/
*   RFY                                                         01S0093
*     REG15 RSTD;                                               01S0093
*   GEN CODE REFS(PSALITA,FLC,RSAVE) SETS(REG0,REG1,REG14,REG15);
*                                   /*                       @ZM21005*/
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,               *
               RELATED=(IKTAY81)    /*                       @ZM21005*/
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,                 *
               RELATED=(IKTAY81)    /*                       @ZM21005*/
*   RFY                                                         01S0095
*     REG15 UNRSTD;                                             01S0095
*   TCASNACT='1'B;                  /* SET TCAS INACTIVE             */
*   TCASABND='1'B;                  /* SET TCAS ABENDED              */
*                                                               01S0097
         L     @15,TCASTPTR                                     01S0097
         OI    TCASNACT(@15),B'00101000'                        01S0097
*   /*****************************************************************/
*   /*                                                               */
*   /* IF NO MORE USERS ARE AROUND, THE TCAS WORK ELEMENT QUEUE IS   */
*   /* FREED, THE TCAS TABLE IS FREED AND THE TCAS POINTER CVTTCASP  */
*   /* IS ZEROED, AND THE TIM/TOM LIST IS FREED.                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0098
*   IF TCASUSEC=0 THEN                                          01S0098
         SLR   @04,@04                                          01S0098
         CH    @04,TCASUSEC(,@15)                               01S0098
         BNE   @RF00098                                         01S0098
*     DO;                           /* USER COUNT IS ZERO            */
*       IF TCASTTQH>0 THEN          /* SEE IF WRK-ELEM QUEUE EXISTS  */
         L     @15,TCASTTQH(,@15)                               01S0100
         CR    @15,@04                                          01S0100
         BNH   @RF00100                                         01S0100
*         DO;                       /* SET COMPLIMENT OF TCASTTQH TO
*                                      PREVENT ENQUEUE OF OTHER WORK
*                                      ELEMENTS AND FREE QUEUE       */
*           RFY                                                 01S0102
*            (RWORKV,                                           01S0102
*             RWORKW) RSTD;                                     01S0102
*           RWORKV=TCASTTQH;        /* GET ADDR OF TCASTTQH          */
         LR    RWORKV,@15                                       01S0103
*CSLOOP6:                                                       01S0104
*           RWORKW=RWORKV;          /* GET CURRENT QUEUE ADDRESS     */
CSLOOP6  LR    RWORKW,RWORKV                                    01S0104
*           GEN(LCR RWORKW,RWORKW)                              01S0105
*           REFS(RWORKW);           /* GET COMPLIMENT OF ADDRESS     */
         LCR RWORKW,RWORKW
*           CS(RWORKV,RWORKW,TCASTTQH);/* STORE IN TCASTTQH          */
         L     @15,TCASTPTR                                     01S0106
         CS    RWORKV,@05,TCASTTQH(@15)                         01S0106
*           BC(NE,CSLOOP6);         /* LOOP TIL DONE                 */
         BC    7,CSLOOP6                                        01S0107
*           RFY                                                 01S0108
*            (RWORKV,                                           01S0108
*             RWORKW) UNRSTD;                                   01S0108
*           CALL FRWESTD;           /* FREE WORK ELEMENT QUEUE       */
         BAL   @14,FRWESTD                                      01S0109
*         END;                                                  01S0110
*       ELSE                                                    01S0111
*         ;                                                     01S0111
@RF00100 DS    0H                                               01S0112
*       IF TCASTTL^=0 THEN          /* SEE IF TIM/TOM LIST EXISTS    */
         L     @15,TCASTPTR                                     01S0112
         L     @15,TCASTTL(,@15)                                01S0112
         LTR   @15,@15                                          01S0112
         BZ    @RF00112                                         01S0112
*         CALL FRTTLIST;            /* FREE THE TIM/TOM LIST         */
         BAL   @14,FRTTLIST                                     01S0113
*       ELSE                                                    01S0114
*         ;                                                     01S0114
@RF00112 DS    0H                                               01S0115
*       CALL FRTCAST;               /* FREE TCAS TABLE AND ZERO 01S0115
*                                      CVTTCASP                      */
         BAL   @14,FRTCAST                                      01S0115
*     END;                                                      01S0116
*   ELSE                                                        01S0117
*                                                               01S0117
*     /***************************************************************/
*     /*                                                             */
*     /* IF USERS ARE STILL UP, THEY ARE ALLOWED TO CONTINUE. THE    */
*     /* ECBS FOR WORK ELEMENTS ON THE QUEUE ARE POSTED AND THE      */
*     /* ELEMENTS ARE FREED.                                         */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0117
*     IF TCASTTQH>0 THEN            /* SEE IF WRK-ELEM QUEUE EXISTS  */
         B     @RC00098                                         01S0117
@RF00098 L     @15,TCASTPTR                                     01S0117
         L     @15,TCASTTQH(,@15)                               01S0117
         LTR   @15,@15                                          01S0117
         BNP   @RF00117                                         01S0117
*       DO;                         /* SET COMPLIMENT OF TCASTTQH TO
*                                      PREVENT ENQUEUE OF OTHER WORK
*                                      ELEMENTS, POST THE REQUESTORS,
*                                      FREE QUEUE                    */
*         RFY                                                   01S0119
*          (RWORKV,                                             01S0119
*           RWORKW) RSTD;                                       01S0119
*         RWORKV=TCASTTQH;          /* GET ADDR OF TCASTTQH          */
         LR    RWORKV,@15                                       01S0120
*CSLOOP7:                                                       01S0121
*         RWORKW=RWORKV;            /* GET CURRENT QUEUE ADDRESS     */
CSLOOP7  LR    RWORKW,RWORKV                                    01S0121
*         GEN(LCR RWORKW,RWORKW)                                01S0122
*         REFS(RWORKW);             /* GET COMPLIMENT OF ADDRESS     */
         LCR RWORKW,RWORKW
*         CS(RWORKV,RWORKW,TCASTTQH);/* STORE IN TCASTTQH            */
         L     @15,TCASTPTR                                     01S0123
         CS    RWORKV,@05,TCASTTQH(@15)                         01S0123
*         BC(NE,CSLOOP7);           /* LOOP TIL DONE                 */
         BC    7,CSLOOP7                                        01S0124
*         RFY                                                   01S0125
*          (RWORKV,                                             01S0125
*           RWORKW) UNRSTD;                                     01S0125
*         CALL POSTWECB;            /* POST REQUESTORS' ECBS         */
         BAL   @14,POSTWECB                                     01S0126
*         CALL FRWESTD;             /* FREE WORK ELEMENT QUEUE       */
         BAL   @14,FRWESTD                                      01S0127
*       END;                                                    01S0128
*     ELSE                                                      01S0129
*       ;                                                       01S0129
*                                                               01S0129
@RF00117 DS    0H                                               01S0130
*   /*****************************************************************/
*   /*                                                               */
*   /* RELEASE LOCKS, AND RETURN TO RTM2                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0130
*   RFY                                                         01S0130
*    (REG0,                                                     01S0130
*     REG1) RSTD;                   /*                       @ZM31130*/
@RC00098 DS    0H                                               01S0131
*   GEN CODE REFS(RSAVE,PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);
*                                   /*                       @ZM21005*/
         SETLOCK RELEASE,TYPE=CMS,REGS=USE,RELATED=(IKTAY81)
         SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,RELATED=(IKTAY81)
*   RFY                                                         01S0132
*    (REG0,                                                     01S0132
*     REG1) UNRSTD;                 /*                       @ZM31130*/
*                                                               02S0133
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT-                                                         */
*/*      XITLINK                                                     */
*/*                                                                  */
*/* FUNCTION-                                                        */
*/*      RETURN LINKAGE                                              */
*/*                                                          @ZM21130*/
*/********************************************************************/
*                                                               02S0133
*   DO;                             /* RETURN LINKAGE        @ZM31130*/
*     RFY                                                       02S0134
*      (REG0,                                                   02S0134
*       REG15) RSTD;                /* FOR LM                @ZM31130*/
*     LM(REG0,REG15,SAVEREGS);      /* RESTORE REGISTERS     @ZM31130*/
         LM    REG0,REG15,SAVEREGS                              02S0135
*     GEN EXIT;                                                 02S0136
*                                   /* RETURN LINKAGE        @ZM31130*/
         FREEMAIN R,LV=(0),A=(1)    /* RELEASE AUTODATA AREA @ZM31130*/
         LM    @14,@12,12(13)       /* RESTORE REGISTERS     @ZM21274*/
         SR    @15,@15              /* SET RETURN CODE 0     @ZM31130*/
         BR    @14                  /* AND RETURN            @ZM31130*/
         DROP  RBASE,RDATA          /* COMMON BASE REGISTERS @ZM31130*/
*   END;                            /* END OF SEGMENT        @ZM31130*/
         EJECT
*                                                               01S0138
*   /*****************************************************************/
*   /*                                                               */
*   /* IKTAY82: TSO/VTAM USER IS TERMINATING THE TSB IS REMOVED FROM */
*   /* THE TSB CHAIN. THE TERMINAL IS RELEASED IF IT IS NOT YET OWNED*/
*   /* BY THE TERMINATING SPACE CROSS MEMORY TPUTS TO AND FROM THE   */
*   /* TERMINATING ADDRESS SPACE ARE IDENTIFIED AND RESOLVED. TIM AND*/
*   /* TOM SRBS, TSB AND TSBX, AND THE LOGON BUFFER (IF IT IS STILL  */
*   /* AROUND) ARE FREED. IF THE LAST USER IS GOING DOWN AND TCAS HAS*/
*   /* ABENDED, THE WORK ELEMENT CHAIN, THE TIM/TOM LIST AND THE     */
*   /* TCAST TABLE ARE FREED. IF THE LAST USER IS GOING DOWN AND TCAS*/
*   /* HAS BEEN STOPPED, TCAS IS POSTED TO CONTINUE ITS TERMINATION. */
*   /* IF THE LAST USER IS GOING DOWN AND TCAS IS STILL ACTIVE, TCAS */
*   /* CONTROL BLOCKS ARE LEFT IN TACT TO HANDLE FUTURE USERS.       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0138
*IKTAY82:                                                       01S0138
*   ENTRY;                                                      01S0138
@EP00138 DS    0H                                               01S0139
*                                                               02S0139
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT-                                                         */
*/*      EPTLINK                                                     */
*/*                                                                  */
*/* FUNCTION-                                                        */
*/*      ENTRY POINT LINKAGE                                         */
*/*                                                          @ZM21130*/
*/********************************************************************/
*                                                               02S0139
*   DO;                             /* ENTRY LINKAGE         @ZM31130*/
*     RFY                                                       02S0140
*      (REG0,                                                   02S0140
*       REG15) RSTD;                /* FOR STM               @ZM31130*/
*     GEN(USING @PSTART,@06)        /* FOR STM               @ZM31130*/
*     (USING @DATD,@05)             /* FOR STM               @ZM31130*/
*     (L     @00,@SIZDATD)          /* FOR EXIT LINKAGE      @ZM31130*/
*     (LR    @01,@05);              /* FOR EXIT LINKAGE      @ZM31130*/
         USING @PSTART,@06
         USING @DATD,@05
         L     @00,@SIZDATD
         LR    @01,@05
*     STM(REG0,REG15,SAVEREGS);     /* SAVE ENTRY REGISTERS  @ZM21130*/
         STM   REG0,REG15,SAVEREGS                              02S0142
*     RFY                                                       02S0143
*      (REG0,                                                   02S0143
*       REG15) UNRSTD;              /* FOR STM               @ZM31130*/
*     GEN SETS(RBASE,RDATA,RSAVE);                              02S0144
*                                   /* COMMON BASE REGISTERS @ZM31130*/
         LR    RBASE,@06            /* ESTABLISH BASE        @ZM31130*/
         LR    RDATA,@05            /* ADDRESS AUTODATA AREA @ZM31130*/
         LR    RSAVE,@05            /* ADDRESS AUTODATA AREA @ZM31130*/
         DROP  @05,@06              /* ENTRY CODE BASE       @ZM31130*/
         USING @PSTART,RBASE        /* BASE ADDRESSABILITY   @ZM31130*/
         USING @DATD,RDATA          /* SAVE AREA ADDRESSABILITY
*                                                            @ZM31130*/
*   END;                            /* END OF SEGMENT        @ZM31130*/
*   RFY                                                         01S0146
*     REG4 RSTD;                    /*                       @ZM21005*/
*   TCASTPTR=REG4;                  /* GET ADDRESS OF TCAS POINTER
*                                                            @ZM21005*/
         ST    REG4,TCASTPTR                                    01S0147
*   RFY                                                         01S0148
*     REG4 UNRSTD;                  /*                       @ZM21005*/
*   RFY                                                         01S0149
*     REG15 RSTD;                                               01S0149
*   GEN CODE REFS(PSALITA,FLC,RSAVE) SETS(REG0,REG1,REG14,REG15);
*                                   /*                       @ZM21005*/
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,               *
               RELATED=(IKTAY82)    /*                       @ZM21005*/
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,                 *
               RELATED=(IKTAY82)    /*                       @ZM21005*/
*   RFY                                                         01S0151
*     REG15 UNRSTD;                                             01S0151
*                                                               01S0151
*   /*****************************************************************/
*   /*                                                               */
*   /* UNCHAIN THE TSB. THE TSO/VTAM TSBS ARE CHAINED OFF OF THE     */
*   /* TCAST TABLE WITH THE ADDRESS OF THE FIRST TSB IN TCASTSB.     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0152
*   TSBDISC='1'B;                   /* MARK USER AS DISCONNECTED     */
         OI    TSBDISC(TSBPTR),B'00000100'                      01S0152
*   TSBHUNG='1'B;                                               01S0153
*   TSBCANC='1'B;                                               01S0154
         OI    TSBHUNG(TSBPTR),B'00010100'                      01S0154
*   IF TSBXFWD=0 THEN                                           01S0155
         SLR   @15,@15                                          01S0155
         L     @04,TSBEXTNT(,TSBPTR)                            01S0155
         C     @15,TSBXFWD(,@04)                                01S0155
         BNE   @RF00155                                         01S0155
*     IF TSBXBCK=0 THEN             /* THIS ONLY TSB ON CHAIN        */
         C     @15,TSBXBCK(,@04)                                01S0156
         BNE   @RF00156                                         01S0156
*       TCASTSB=0;                                              01S0157
         L     @04,TCASTPTR                                     01S0157
         ST    @15,TCASTSB(,@04)                                01S0157
*     ELSE                          /* THIS IS LAST TSB ON CHAIN     */
*       TSBXBCK->TSBEXTNT->TSBXFWD=0;                           01S0158
         B     @RC00156                                         01S0158
@RF00156 L     @15,TSBEXTNT(,TSBPTR)                            01S0158
         L     @15,TSBXBCK(,@15)                                01S0158
         L     @15,TSBEXTNT(,@15)                               01S0158
         SLR   @04,@04                                          01S0158
         ST    @04,TSBXFWD(,@15)                                01S0158
*   ELSE                                                        01S0159
*     IF TSBXBCK^=0 THEN            /* THIS IS A MIDDLE TSB          */
         B     @RC00155                                         01S0159
@RF00155 L     @15,TSBEXTNT(,TSBPTR)                            01S0159
         L     @04,TSBXBCK(,@15)                                01S0159
         LTR   @04,@04                                          01S0159
         BZ    @RF00159                                         01S0159
*       DO;                                                     01S0160
*         TSBXBCK->TSBEXTNT->TSBXFWD=TSBXFWD;                   01S0161
         L     @01,TSBEXTNT(,@04)                               01S0161
         L     @15,TSBXFWD(,@15)                                01S0161
         ST    @15,TSBXFWD(,@01)                                01S0161
*         TSBXFWD->TSBEXTNT->TSBXBCK=TSBXBCK;                   01S0162
         L     @15,TSBEXTNT(,@15)                               01S0162
         ST    @04,TSBXBCK(,@15)                                01S0162
*       END;                                                    01S0163
*     ELSE                          /* THIS IS FIRST TSB ON CHAIN    */
*       DO;                                                     01S0164
         B     @RC00159                                         01S0164
@RF00159 DS    0H                                               01S0165
*         TSBXFWD->TSBEXTNT->TSBXBCK=0;                         01S0165
         L     @15,TSBEXTNT(,TSBPTR)                            01S0165
         L     @15,TSBXFWD(,@15)                                01S0165
         L     @04,TSBEXTNT(,@15)                               01S0165
         SLR   @03,@03                                          01S0165
         ST    @03,TSBXBCK(,@04)                                01S0165
*         TCASTSB=TSBXFWD;                                      01S0166
         L     @04,TCASTPTR                                     01S0166
         ST    @15,TCASTSB(,@04)                                01S0166
*       END;                                                    01S0167
*                                                               01S0167
*   /*****************************************************************/
*   /*                                                               */
*   /* REQUEST TCAS TO RELEASE TERMINALS NOT YET OWNED BY THE        */
*   /* TERMINATING ADDRESS SPACE.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0168
*   IF(TSBXACTV='0'B)&              /* IF TERMINAL IS NOT OWNED BY   */
*       (TCASNACT='0'B)&            /* ADDRESS SPACE AND TCAS IS     */
*       (TCASABND='0'B)&            /* ACTIVE AND RELEASE HAS NOT    */
*       (TSBXLBUF^=0) THEN          /* BEEN REQUESTED (LBUF ^= 0)    */
@RC00159 DS    0H                                               01S0168
@RC00155 L     @15,TSBEXTNT(,TSBPTR)                            01S0168
         TM    TSBXACTV(@15),B'01000000'                        01S0168
         BNZ   @RF00168                                         01S0168
         L     @04,TCASTPTR                                     01S0168
         TM    TCASNACT(@04),B'00101000'                        01S0168
         BNZ   @RF00168                                         01S0168
         L     @15,TSBXLBUF(,@15)                               01S0168
         LTR   @15,@15                                          01S0168
         BZ    @RF00168                                         01S0168
*     CALL REQREL;                  /* THEN- REQUEST RELEASE OF 01S0169
*                                      TERMINAL BY TCAS              */
         BAL   @14,REQREL                                       01S0169
*   ELSE                            /* TERMINAL IS OWNED BY ADDRESS
*                                      SPACE                         */
*                                                               01S0170
*     /***************************************************************/
*     /*                                                             */
*     /* LOOK FOR AN ASID TPUT TO TERMINATING ADDRESS SPACE          */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0170
*     DO;                           /* TVCS PROCESSING               */
         B     @RC00168                                         01S0170
@RF00168 DS    0H                                               01S0171
*       IF TSBXCSAP^=0 THEN         /* SEE IF THERE IS AN ASID TPUT
*                                      TO THE TERMINATING ADDRESS
*                                      SPACE                         */
         L     @15,TSBEXTNT(,TSBPTR)                            01S0171
         L     @15,TSBXCSAP(,@15)                               01S0171
         LTR   @15,@15                                          01S0171
         BZ    @RF00171                                         01S0171
*         DO;                                                   01S0172
*           RFY                                                 01S0173
*            (RWORKV,                                           01S0173
*             RWORKW,                                           01S0173
*             RWORKX) RSTD;                                     01S0173
*           RWORKV=TVCSINDS;        /* GET TVCS INDICATORS           */
         L     RWORKV,TVCSINDS(,@15)                            01S0174
*CSLOOP1:                                                       01S0175
*           WRKINDS=RWORKV;         /* RWORKV IS SET WHEN CS FAILS   */
CSLOOP1  ST    RWORKV,WRKINDS                                   01S0175
*           IF WRKSRCRC='1'B THEN   /* SOURCE HAS RELENQUISHED CNTL  */
*                                                               01S0176
         TM    WRKSRCRC,B'10000000'                             01S0176
         BNO   @RF00176                                         01S0176
*             /*******************************************************/
*             /*                                                     */
*             /* IF THE SOURCE ADDR SPACE HAS RELINQUISHED CNTL OF   */
*             /* CSA AREA, FREE THE SRB AND TVCS                     */
*             /*                                                     */
*             /*******************************************************/
*                                                               01S0177
*             DO;                                               01S0177
