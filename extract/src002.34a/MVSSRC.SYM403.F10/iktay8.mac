*   RWORKW=TSBXCSAP;                /* GET ADDR OF TVCS              */
         L     @02,TSBEXTNT(,RWORKY)                            01S0404
         L     RWORKW,TSBXCSAP(,@02)                            01S0404
*   RFY                                                         01S0405
*     TVCS BASED(RWORKW);                                       01S0405
*   RWORKX=TVCSGMSZ;                /* GET LENGTH OF TVCS            */
         LH    RWORKX,TVCSGMSZ(,RWORKW)                         01S0406
*   REG4=PSATOLD;                   /* ADDR OF CURRENT TCB   @YM03242*/
         L     REG4,PSATOLD                                     01S0407
*   REG7=PSAAOLD;                   /* ADDR OF CURRENT ASCB  @YM03242*/
