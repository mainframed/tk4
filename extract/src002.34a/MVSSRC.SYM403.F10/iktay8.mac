*                                                               01S0481
*   /*****************************************************************/
*   /*                                                               */
*   /* FREESRB: FREE SRB AT ADDRESS PASSED IN REGISTER 1 A PURGEDQ   */
*   /* (WHICH REQUIRES THE RELEASE OF THE LOCAL AND CMS LOCKS) IS    */
*   /* ISSUED TO MAKE SURE THAT THE DISPATCHER WILL NOT TRY TO       */
*   /* ADDRESS AN SRB WHICH HAS BEEN FREED. THE SRB WHICH IS POINTED */
*   /* TO BY REG 1 ON ENTRY TO THIS PROCEDURE IS THEN FREED.         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0481
*FREESRB:                                                       01S0481
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                       01S0481
FREESRB  DS    0H                                               01S0482
*   RFY                                                         01S0482
*    (RLINK,                                                    01S0482
*     REG4) RSTD;                                               01S0482
*   SAVE14B=RLINK;                  /* SAVE RETURN ADDRESS           */
         ST    RLINK,SAVE14B                                    01S0483
*   SAVE04A=REG4;                   /* SAVE REG4 DESTROYED BY BRANCH
*                                      ENTERED FREEMAIN - NEEDED BY
*                                      SRCHVTAM                      */
         ST    REG4,SAVE04A                                     01S0484
*   RFY                                                         01S0485
*     RLINK UNRSTD;                                             01S0485
*                                                               01S0485
*   /*****************************************************************/
*   /*                                                               */
*   /* UPON INPUT FREESRB REQUIRES THE ADDRESS OF THE SRB IN REG1    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0486
*   RFY                                                         01S0486
*    (REG1,                                                     01S0486
*     RWORKY) RSTD;                                             01S0486
*   RWORKY=REG1;                    /* SAVE ADDRESS OF SRB           */
*                                                               01S0487
         LR    RWORKY,REG1                                      01S0487
*   /*****************************************************************/
*   /*                                                               */
*   /* PERFORM A PURGEQ ON THE SRB TO ASSURE THAT THE SRB IS         */
*   /* DEQUEUED. THE SRB IS ALLOWED TO RUN IF IT HAS BEEN DISPATCHED.*/
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0488
*   RFY                                                         01S0488
*     SRBSECT BASED(RWORKY);                                    01S0488
*   PURGASID=SRBPASID;                                          01S0489
         MVC   PURGASID(2),SRBPASID(RWORKY)                     01S0489
*   PURGTCB=SRBPTCB;                                            01S0490
         L     @07,SRBPTCB(,RWORKY)                             01S0490
         ST    @07,PURGTCB                                      01S0490
*   RFY                                                         01S0491
*    (RWORKW,                                                   01S0491
*     RWORKX,                                                   01S0491
*     REG0,                                                     01S0491
*     REG3,                                                     01S0491
*     RLINK,                                                    01S0491
*     REG15) RSTD;                                              01S0491
*   RWORKW=SRBRMTR;                                             01S0492
         L     RWORKW,SRBRMTR(,RWORKY)                          01S0492
*   RWORKX=ADDR(PURGPARM);                                      01S0493
         LA    RWORKX,PURGPARM                                  01S0493
*   GEN CODE REFS(RSAVE,PSALITA,FLC,RWORKX,RWORKW) SETS(REG0,REG1,REG14
*       ,REG15);                                                01S0494
*                                                               01S0494
*                                   /*                       @ZM21005*/
         SETLOCK RELEASE,TYPE=CMS,REGS=USE,RELATED=(PURGEDQ) /*
*                                                            @ZM21005*/
         SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,RELATED=(PURGEDQ)  /*
*                                                            @ZM21005*/
         PURGEDQ RMTR=(RWORKW),ASIDTCB=(RWORKX),MF=(E,PURGEDQL)
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,               *
               RELATED=(PURGEDQ)    /*                       @ZM21005*/
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,                 *
               RELATED=(PURGEDQ)    /*                       @ZM21005*/
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE THE SRB USING FREEMAIN                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0495
*   RWORKW=LENGTH(SRB);             /* GET LENGTH OF SRB - ADDRESS OF
*                                      SRB IS IN REG1                */
         LA    RWORKW,44                                        01S0495
*   RFY                                                         01S0496
*     REG7 RSTD;                    /*                       @YM03242*/
*   REG4=PSATOLD;                   /* ADDR OF CURRENT TCB   @YM03242*/
         L     REG4,PSATOLD                                     01S0497
*   REG7=PSAAOLD;                   /* ADDR OF CURRENT ASCB  @YM03242*/
         L     REG7,PSAAOLD                                     01S0498
*   GEN CODE SETS(REG3,REG0,REG15,RLINK) REFS(RWORKW,CVTPTR,CVTMAP,
*       CVTCRMN);                                               01S0499
*                                   /*                       @YM03242*/
         FREEMAIN RC,LV=(RWORKW),A=(RWORKY),SP=SPSRB,                  *
               BRANCH=YES,RELATED=(SRB)  /*                  @YM03232*/
*   RFY                                                         01S0500
*    (REG7,                                                     01S0500
*     REG1,                                                     01S0500
*     RWORKW,                                                   01S0500
*     RWORKX,                                                   01S0500
*     REG0,                                                     01S0500
*     REG3,                                                     01S0500
*     REG15,                                                    01S0500
*     RWORKY) UNRSTD;               /*                       @YM03242*/
*   REG4=SAVE04A;                   /* RESTORE REG4                  */
         L     REG4,SAVE04A                                     01S0501
*   RLINK=SAVE14B;                  /* RESTORE RETURN ADDRESS        */
         L     RLINK,SAVE14B                                    01S0502
*   RFY                                                         01S0503
*    (RLINK,                                                    01S0503
*     REG4) UNRSTD;                                             01S0503
*   RETURN;                                                     01S0504
@EL00010 DS    0H                                               01S0504
@EF00010 DS    0H                                               01S0504
@ER00010 BR    @14                                              01S0504
*   END FREESRB;                                                01S0505
