*/* FUNCTION-                                                        */
*/*      ENTRY POINT LINKAGE                                         */
*/*                                                          @ZM21019*/
*/********************************************************************/
*                                                                  0770
*   DO;                             /* ENTRY POINT LINKAGE   @ZM21019*/
*     RFY                                                          0771
*      (REG1,                                                      0771
*       REG14) RSTD;                                               0771
*     GEN SETS(REG11,REG13);                                       0772
         LR    @04,@01             /* SAVE REGISTER 01      @ZM21019*/
         LR    @05,@14             /* SAVE REGISTER 14      @ZM21019*/
         L     @03,@SIZDATD
         GETMAIN R,LV=(3),SP=229   /* GET AUTOAREA          @ZM21019*/
         LR    REG11,@01           /* SET UP DATAREG        @ZM21019*/
         USING @DATD,REG11         /* ADDRESS AUTOAREA      @ZM21019*/
         LR    REG13,REG11         /* SET UP SAVEAREA ADDR  @ZM21019*/
         MODESET EXTKEY=ZERO       /* GET KEY 0             @ZM21019*/
         L     @10,PSATOLD         /* LOAD BASE ADDR OF TCB @ZM21019*/
         IC    @02,TCBPKF-TCB(,@10) /* SAVE OLD PROTECT KEY @ZM21019*/
         MVI   TCBPKF-TCB(@10),0   /* INSERT KEY ZERO       @ZM21019*/
         STC   @02,OLDKEY          /* SAVE OLD PROTECT KEY  @ZM21019*/
         LR    @01,@04             /* RESTORE REGISTER 01   @ZM21019*/
         LR    @14,@05             /* RESTORE REGISTER 14   @ZM21019*/
*     TSBPTR=ASCBTSB;               /* ADDRESS THE TSB       @ZM21019*/
         L     @06,ASCBPTR                                         0773
         L     @06,ASCBTSB(,@06)                                   0773
         ST    @06,TSBPTR                                          0773
*     PARAMREG=REG1;                /* ADDRESS PARAMETER LIST      0774
*                                                            @ZM21019*/
         LR    PARAMREG,REG1                                       0774
*     LSAVAREA=LSAVAREA&&LSAVAREA;  /* ZERO SAVE AREA        @ZM21019*/
         XC    LSAVAREA(156),LSAVAREA                              0775
*     MAINEXIT=REG14;               /* SAVE RETURN ADDRESS   @ZM21019*/
         ST    REG14,MAINEXIT                                      0776
*     RFY                                                          0777
*      (REG1,                                                      0777
*       REG14) UNRSTD;                                             0777
*   END;                            /* END OF SEGMENT        @ZM21019*/
*   IF VTAMRSN=0 THEN               /* IF NORMAL HALT        @ZM21019*/
         L     @14,VTAMRSN(,PARAMREG)                              0779
         LTR   @14,@14                                             0779
         BZ    @RT00779                                            0779
*     ;                             /* RETURN TO VTAM        @ZM21019*/
*   ELSE                            /* IF NOT NORMAL HALT    @ZM21019*/
*     DO;                           /* TERMINATE THE ADDRESS SPACE 0781
*                                      WITH 622 CODE         @ZM21019*/
*       GEN REFS(PSALITA,FLC);                                     0782
*                                  /* TERMAS NEEDS LOCK     @ZM21019*/
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=SAVE,             **
               RELATED=('TPEND ENTERED')  /* GET THE LOCAL LOCK
*                                                           @ZM21019*/
*       CALL TERMAS;                /* TERMINATE THE ADDRESS SPACE 0783
*                                      WITH 622 CODE         @ZM21019*/
         BAL   @14,TERMAS                                          0783
*     END;                          /* TERMINATE THE ADDRESS SPACE 0784
*                                      WITH 622 CODE         @ZM21019*/
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT-                                                         */
*/*      XITLINK                                                     */
*/* FUNCTION-                                                        */
*/*      RETURN LINKAGE                                              */
*/*                                                          @ZM21019*/
*/********************************************************************/
*                                                                  0785
*   DO;                             /* RESET KEY AND FREE AUTODATA 0785
*                                      AREA                  @ZM21019*/
@RT00779 DS    0H                                                  0786
*     GENERATE REFS(PSALITA,FLC,OLDKEY,PSATOLD,TCBPKF)             0786
*         (SETLOCK RELEASE,TYPE=LOCAL,REGS=SAVE,RELATED=*);/* FREE 0786
*                                      THE LOCAL MEMORY LOCK @ZM21019*/
         SETLOCK RELEASE,TYPE=LOCAL,REGS=SAVE,RELATED=*
*     RFY                                                          0787
*       REG14 RSTD;                 /* RETURN REGISTER       @ZM21019*/
*     REG14=MAINEXIT;               /* RESTORE RETURN REGISTER     0788
*                                                            @ZM21019*/
         L     REG14,MAINEXIT                                      0788
*     GEN EXIT REFS(OLDKEY,PSATOLD,TCBPKF,REG11,REG14);            0789
*                                              /*           @ZM21019*/
         LR    @10,REG14           /* SAVE RETURN ADDRESS   @ZM21019*/
         IC    @02,OLDKEY          /* LOAD OLD TCB KEY      @ZM21019*/
         L     @15,PSATOLD         /* POINT TO THE TCB      @ZM21019*/
         STC   @02,TCBPKF-TCB(,@15) /* SET OLD PROTECT KEY  @ZM21019*/
         MODESET EXTKEY=VTAM       /* SET KEY 6             @ZM21019*/
         L     @03,@SIZDATD        /* GET SIZE OF AUTOAREA  @ZM21019*/
         LR    @01,REG11           /* GET ADDR OF AUTOAREA  @ZM21019*/
         FREEMAIN R,LV=(3),A=(1),SP=229 /* FREE AUTOAREA    @ZM21019*/
         LR    REG14,@10           /* SET RETURN ADDRESS    @ZM21019*/
         BR    REG14               /* AND RETURN            @ZM21019*/
*     RFY                                                          0790
*       REG14 UNRSTD;               /* RETURN REGISTER       @ZM21019*/
*   END;                            /* END OF SEGMENT        @ZM21019*/
