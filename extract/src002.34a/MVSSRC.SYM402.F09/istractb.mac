         TITLE 'ISTRACTB---BUFFER TRACE                                *
                        '
ISTRACTB CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(8)                                              0001
         DC    C'ISTRACTB'                                         0001
         DROP  @15
@PROLOG  BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : TBMAINLN - ISTRACTB MAIN LINE PROCEDURE          */
*/*                                                                  */
*/*  FUNCTION : TO PROVIDE MAIN LINE CONTROL FOR BUFFER TRACE. CALLS */
*/*             A ROUTINE TO BUILD A TRACE RECORD (ISTTRAC) AND SETUP*/
*/*             THE TRAC'S HEADER. IF THE DATA IS CONFIDENTIAL THE   */
*/*             RECORD IS WRITTEN WITHOUT FILLING IN THE BUFFER.     */
*/*             OTHERWISE, ROUTINES ARE INVOKED TO FILL THE TRAC     */
*/*             BUFFER WITH CONTROL LAYER OR TPIOS DATA (TBSRPL OR   */
*/*             TBSFDB RESPECTIVELY). AFTER THE BUFFER IS FILLED THE */
*/*             HEADER INFORMATION IS COMPLETED AND THE TRACE RECORD */
*/*             IS WRITTEN. THE WRITTING OF THE RECORD DEPENDS ON THE*/
*/*             SYSTEM. IN DOS, RECORDING IS ACCOMPLISHED BY TPQUEING*/
*/*             THE TRACE RECORD TO THE TRACE WRITER PAB. IN VS1 AND */
*/*             VS2 THE GTRACE MACRO IS ISSUED, GTF TRACE COPIES THE */
*/*             TRACE RECORD AND RETURNS CONTROL.                    */
*/*                                                                  */
*/*  INPUT : R6-BYTE 0 = X'00' - RPL OUTBOUND                        */
*/*                      X'04' - RPL INBOUND                         */
*/*                      X'20' - FDB INPUT                           */
*/*             BYTES1-3 = ADDRESS OF RPL OR FDB DEPENDING ON BYTE 0 */
*/*                                                                  */
*/*  OUTPUT : NONE                                                   */
*/*                                                                  */
*/*  ROUTINES : TBTRAC - GETS CORE FOR TRACE RECORD. SETS UP HEADER. */
*/*                      IF CONFIDENTIAL DATA, COMPLETES HEADER SO   */
*/*                      THE RECORD IS READY TO WRITE.               */
*/*             TBSFDB - CALLED TO HANDLE TPIOS INBOUND NON-         */
*/*                      CONFIDENTIAL DATA. FILLS THE TRAC BUFFER    */
*/*                      WITH FDB DATA                               */
*/*             TBSRPL - CALLED TO HANDLE CONTROL/LAYER NON-         */
*/*                      CONFIDENTIAL DATA. CALLS ROUTINES THAT FILL */
*/*                      THE TRAC BUFFER WITH RPL DATA.              */
*/*                                                                  */
*/*  MACROS : VERSION - TO PROVIDE DIFFERENT PROCESSING DEPENDING ON */
*/*                     THE SYSTEM.                                  */
*/*           CIDCTL (FIND) - TO OBTAIN THE DAFLOCK AND NCB ADDRESS. */
*/*           CIDCTL (FINISH) - TO RELEASE THE DAFLOCK.              */
*/*           RELSTORE - TO RELEASE THE TRAC WHEN AN ERROR OCCURS. IN*/
*/*                     VS1 AND VS2 TO RELEASE THE TRAC AFTER GTRACE */
*/*                     HAS COPIED IT.                               */
*/*           TPQUE - USED IN DOS TO QUEUE THE TRACE RECORD TO THE   */
*/*                   TRACE WRITER (ISTRAETW) PAB FOR RECORDING.     */
*/*           GTRACE - USED IN VS1 AND VS2 TO RECORD TRACE RECORDS.  */
*/*                                                                  */
*/********************************************************************/
*TBMAINLN:                                                         0456
* R14SAVE=R14;                                    /*SAVE REG14       */
TBMAINLN LR    R14SAVE,R14                                         0456
*  IF (SYSCVTPT->CVTGTFAV) = '0'B     /* IS GTRACE ACTIVE    @VS41640*/
*    THEN;                                        /*  NO-EXIT        */
         L     @11,SYSCVTPT                                        0457
         TM    CVTGTFAV(@11),B'10000000'                           0457
         BZ    @RT00457                                            0457
*    ELSE DO;                                     /*  YES-PROCEED    */
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : TBTRAC - GET TRAC ROUTINE                        */
*/*                                                                  */
*/*  FUNCTION : TO OBTAIN CORE FOR THE TRACE RECORD. INITIALIZE THE  */
*/*             HEADER. IF CONFIDENTIAL DATA IS INDICATED IN THE FMCB*/
*/*             THE ENTIRE HEADER IS FILLED IN. IN THE CASE OF NORMAL*/
*/*             DATA,THE LOST RECORD CNT AND THE DESTINATION NODE    */
*/*             NAME AND THE APPLICATION NAME ARE NOT SET, RATHER    */
*/*             THE CID IS STORED IN THE HEADER FOR LATER USE BY     */
*/*             TBMAINLN.                                            */
*/*                                                                  */
*/*  INPUT : R6 - INPUT PARAMETERS AS DEFINED IN PROLOGUE.           */
*/*                                                                  */
*/*  OUTPUT : RTNCODE = 0 - TRACE RECORD BUILT SUCCESSFULLY.         */
*/*                  ^= 0 - TRACE RECORD NOT BUILT BECAUSE THE CID   */
*/*                         WAS INVALID OR BECAUSE NO STORAGE WAS    */
*/*                         AVAILABLE.                               */
*/*                                                                  */
*/*  ROUTINES : NONE                                                 */
*/*                                                                  */
*/*  MACROS : CIDCTL - (FIND & FINISH) TO OBTAIN THE FMCB & DNCB     */
*/*                    ADDRESSES,AND TO LOCK AND UNLOCK THE          */
*/*                    DAFLOCK.                                      */
*/*           REQSTORE - CALLED TO OBTAIN STORAGE FOR THE TRACE      */
*/*                      RECORD                                      */
*/*                                                                  */
*/********************************************************************/
*  RFY (R2,FMCBR2,R14) RSTD;                                       0460
*  RTNCODE = 0;                             /* INIT RETRN CODE */  0461
         SLR   RTNCODE,RTNCODE                                     0461
*  IF (R6INPUT&INPTMSK)=FDBINPUT            /*FDB INPUT?             */
*    THEN DO;                               /* YES-OBTAIN CID FROM   */
         LR    @11,R6INPUT                                         0462
         N     @11,INPTMSK                                         0462
         CL    @11,FDBINPUT                                        0462
         BNE   @RF00462                                            0462
*      RFY ISTFDB BASED(R6INPUT);           /*  FSB(R6-> TO 1ST FDB) */
*      R14=R6INPUT                          /*    (ISTFSB PRECEDES   */
*          -LENGTH(ISTFSB)                  /*    1ST FDB,FSB LENGTH */
*          +LENGTH(ISTFDB);                 /*    INCLUDES 1ST FDB)  */
         LR    R14,R6INPUT                                         0465
         AL    R14,@CF04445                                        0465
*      R2=FSBCID;                                                  0466
         L     R2,FSBCID(,R14)                                     0466
*  RFY CIDSAVR5 RSTD;                       /*SAVE CID TO DO CID     */
*  CIDSAVR5=R2;                             /*FINISH.                */
*                                                                  0468
         LR    CIDSAVR5,R2                                         0468
*   DO; /* CIDCTL(FIND) LOCK(XCLUSIVE) */                          0469
*                                                                  0469
*   RFY (EMCR2,EMCR3) RSTD;                                        0470
*     GEN SETS(RPHWORK)                                            0471
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      0471
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH
*     EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                          0472
         SLR   EMCR3,EMCR3                                         0472
*     EMCR3 = '02'X; /*INDICATE EXCLUSIVE LOCK*/                   0473
         LA    EMCR3,2                                             0473
*     CALL ATCLCPTR->ATCCDFND->ISTSDCCF; /*PROCESS REQUEST*/       0474
*                                                                  0474
         L     @11,ATCLCPTR                                        0474
         L     @15,ATCCDFND(,@11)                                  0474
         BALR  @14,@15                                             0474
*   RFY (EMCR2,EMCR3) UNRSTD;                                      0475
*   END; /*CIDCTL*/           /*GET FMCB&NCB ADDRS &   */          0476
*     END;                                                         0477
*    ELSE DO;                            /*  NO,LOAD DNCB @ FROM FMCB *
         B     @RC00462                                            0478
@RF00462 DS    0H                                                  0479
*      RFY (R0,FMCBSVR5) RSTD;                                     0479
*      R0 = FMCDNCBA;                    /* GET DNCB PTR */        0480
         L     R0,FMCDNCBA(,FMCBR2)                                0480
*      FMCBSVR5 = FMCBR2;                /* SAVE FMCB PTR */       0481
         LR    FMCBSVR5,FMCBR2                                     0481
*     END;                                                         0482
*  RFY NCBPTR RSTD;                                                0483
@RC00462 DS    0H                                                  0484
*  IF RTNCODE=ZERO                          /* DAF LOCK OBTAINED */
*    THEN DO;                               /* YES-GET CORE FOR TRAC */
         LTR   RTNCODE,RTNCODE                                     0484
         BNZ   @RF00484                                            0484
*      NCBPTR=R0;                                                  0486
         LR    NCBPTR,R0                                           0486
*      RFY R0 UNRSTD;                                              0487
*       /*REQSTORE CBID(FMCB)CLEAR(YES)SAVE(YES)*/                 0488
*        DO;                                                       0488
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)  */                         0489
*/* END OF ATBUILD */                                              0489
*                                                                  0489
*       RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;                    0489
*                                                                  0489
*       RFY (GPR01P,GPR13P) RSTD;                                  0490
*          GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);      0491
*                                                                  0491
           STM GPR02P,GPR13P,RPHWORK(GPR01P)
*       RFY (GPR01P,GPR13P) UNRSTD;                                0492
*                                                                  0492
*       DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */              0493
*         GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/              0494
         L     GPR15P,ATCLCPTR                                     0494
*       END; /*LISATCVT*/                                          0495
*          GPR02P=GPR15P->ATCBPDA × '80000000'X;                   0496
         L     GPR02P,ATCBPDA(,GPR15P)                             0496
         O     GPR02P,@CF01359                                     0496
*          DCL 1 FMZA0010 FIXED(31) STATIC LOCAL,                  0497
*              2 *        FIXED(8) INIT(152),                      0497
*              2 *        PTR(24) INIT(0);                         0497
*          GPR03F=FMZA0010;                                        0498
         L     GPR03F,FMZA0010                                     0498
*          GPR04F=25;                                              0499
         LA    GPR04F,25                                           0499
*          GPR15P=GPR15P->ATCSMRQ;                                 0500
         L     GPR15P,ATCSMRQ(,GPR15P)                             0500
*          DCL FMZL0010 BASED(GPR15P) LABEL VALUERANGE(*);         0501
*          CALL FMZL0010;                                          0502
*                                                                  0502
         BALR  @14,@15                                             0502
*       RFY (GPR13P) RSTD;                                         0503
*          GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);      0504
*                                                                  0504
           LM GPR02P,GPR13P,RPHWORK(GPR01P)
*       RFY (GPR13P) UNRSTD;                                       0505
*                                                                  0505
*       RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;                  0506
*       END ;                                                      0507
*          /********* END OF REQSTORE EXPANSION **********/        0508
*      IF RTNCODE<EIGHT                     /*  CORE AVAILABLE?      */
*        THEN DO;                           /*   YES-SETUP HEADER    */
         LA    @04,8                                               0508
         CR    RTNCODE,@04                                         0508
         BNL   @RF00508                                            0508
*          RTNCODE=ZERO;                    /*    SET GOOD RTN CODE  */
         SLR   @03,@03                                             0510
         LR    RTNCODE,@03                                         0510
*          TRACPTR=RPHSRPRM;                /*    SETUP TRAC BASE    */
         L     TRACPTR,RPHSRPRM(,RPHPTR)                           0511
*          IF PROCFTX='1'B                  /*    CONFIDENTIAL DATA? */
*            THEN DO;                       /*     YES-SETUP ENTIRE  */
         TM    PROCFTX+48(FMCBR2),B'00000001'                      0512
         BNO   @RF00512                                            0512
*              RFY ISTRPRE BASED(NCBRDTE);  /*      TRAC HEADER.     */
*              TRALRC=NCBLTRC;              /*      LOST RECORD CNT  */
         LH    @00,NCBLTRC(,NCBPTR)                                0515
         STH   @00,TRALRC(,TRACPTR)                                0515
*              NCBLTRC=ZERO;                /*      REINITIALIZE     */
         STH   @03,NCBLTRC(,NCBPTR)                                0516
*              TRADNODE=RPRNAME;            /*      DEST. NODE NAME  */
         L     @03,NCBRDTE(,NCBPTR)                                0517
         MVC   TRADNODE(8,TRACPTR),RPRNAME(@03)                    0517
*              TRADNODF='1'B;               /*      DEST NODENAME FLG*/
         OI    TRADNODF(TRACPTR),B'00000100'                       0518
*              RFY ISTRPRE BASED(ACDRDTE);  /*      FMCB->ACD->RDTE  */
*              TRAANODE=RPRNAME;            /*      APPLICATION NAME */
         L     @03,FMCDEBA(,FMCBR2)                                0520
         L     @03,ACDRDTE(,@03)                                   0520
         MVC   TRAANODE(8,TRACPTR),RPRNAME(@03)                    0520
*              TRAANODF='1'B;               /*      APPL NAME FLAG   */
         OI    TRAANODF(TRACPTR),B'00000010'                       0521
*              TRADATCT=LENGTH(TRAANODE);   /*      BUFFER LENGTH    */
         STH   @04,TRADATCT(,TRACPTR)                              0522
*              TRACXTF='1'B;                /*      CONFIDENTIAL DATA*/
         OI    TRACXTF(TRACPTR),B'00100000'                        0523
*             END;                          /* NO-SAVE CID/FMCB IN */
*            ELSE                           /*      TRAC FOR LATER   */
*              TRACID=R5;                   /*      TO FIND DNCB    */
         B     @RC00512                                            0525
@RF00512 ST    R5,TRACID(,TRACPTR)                                 0525
*          TRABF='1'B;                      /*    BUFFER TRACE       */
@RC00512 DS    0H                                                  0527
*          TRAFRST='1'B;                    /*    FIRST BUFFER       */
         OI    TRABF(TRACPTR),B'01001000'                          0527
*          TRATYPE=TRTYPE;                  /*    C/B TYPE = TRAC    */
         MVI   TRATYPE(TRACPTR),X'13'                              0528
*          TRALNG=LENGTH(ISTTRAC);          /*    LENGTH             */
         MVI   TRALNG(TRACPTR),X'20'                               0529
*                                           /*    SET I/O DIRECTION  */
*          IF (R6INPUT&INPTMSK)=FDBINPUT    /*    BASED ON R6 INPUT: */
*            THEN                           /*     TPIOS FDB CALL    */
         LR    @04,R6INPUT                                         0530
         N     @04,INPTMSK                                         0530
         CL    @04,FDBINPUT                                        0530
         BNE   @RF00530                                            0530
*              TRADIO='1'B;                 /*     IS ALWAYS INBOUND */
         OI    TRADIO(TRACPTR),B'00001000'                         0531
*            ELSE DO;                       /*    CONTROL LAYER INPUT*/
         B     @RC00530                                            0532
@RF00530 DS    0H                                                  0533
*              IF (R6INPUT&INPTMSK)=ZERO    /*     OUTBOUND?         */
*                THEN;                      /*      YES-FLG=0(OUTBND)*/
         LR    @04,R6INPUT                                         0533
         N     @04,INPTMSK                                         0533
         LTR   @04,@04                                             0533
         BZ    @RT00533                                            0533
*                ELSE                       /*      NO-FLG=1(INBOUND)*/
*                  TRADIO='1'B;                                    0535
         OI    TRADIO(TRACPTR),B'00001000'                         0535
*             END;                                                 0536
*         END;                                                     0537
*        ELSE                               /*   REQSTORE FAILURE-   */
*          NCBLTRC=NCBLTRC+1;               /*   INCR. LOST RCD CNT  */
         B     @RC00508                                            0538
@RF00508 LA    @04,1                                               0538
         AH    @04,NCBLTRC(,NCBPTR)                                0538
         STH   @04,NCBLTRC(,NCBPTR)                                0538
*      IF (R6INPUT&INPTMSK) = FDBINPUT                             0539
*        THEN DO;                           /* INPUT IS A FDB */   0539
@RC00508 LR    @04,R6INPUT                                         0539
         N     @04,INPTMSK                                         0539
         CL    @04,FDBINPUT                                        0539
         BNE   @RF00539                                            0539
*      R2=CIDSAVR5;                         /*  GET CID              */
         LR    R2,CIDSAVR5                                         0541
*      RFY SAVRCR5 RSTD;                    /*  SAVE REQSTORE RTNCODE*/
*      SAVRCR5=RTNCODE;                     /*  ACROSS CIDCTL.       */
*                                                                  0543
         LR    SAVRCR5,RTNCODE                                     0543
*       DO; /* CIDCTL(FINISH)  */                                  0544
*                                                                  0544
*       RFY (EMCR2,EMCR3) RSTD;                                    0545
*         GEN SETS(RPHWORK)                                        0546
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      0546
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH
*         EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                      0547
         SLR   EMCR3,EMCR3                                         0547
*         CALL ATCLCPTR->ATCCDFIN->ISTSDCCR; /*PROCESS REQUEST*/   0548
*                                                                  0548
         L     @04,ATCLCPTR                                        0548
         L     @15,ATCCDFIN(,@04)                                  0548
         BALR  @14,@15                                             0548
*       RFY (EMCR2,EMCR3) UNRSTD;                                  0549
*       END; /*CIDCTL*/                    /*  UNLOCK THE DAFLOCK.  */
*      RTNCODE=SAVRCR5;                     /*  RESTORE RETURN CODE  */
         LR    RTNCODE,SAVRCR5                                     0551
*             END;                                                 0552
*        ELSE;                            /* DAF LOCK WAS NOT HELD */
@RF00539 DS    0H                                                  0554
*     END;                                                         0554
*    ELSE;                                  /* ERROR ON CID FIND     */
@RF00484 DS    0H                                                  0556
*  RFY (FMCBR2,R2,NCBPTR,CIDSAVR5,SAVRCR5,FMCBSVR5) UNRSTD;        0556
*                                                                  0556
*/*******************************************************************/
*      IF RTNCODE=ZERO                            /*SUCCESSFUL?      */
*        THEN DO;                                 /* YES-PROCEED     */
         LTR   RTNCODE,RTNCODE                                     0557
         BNZ   @RF00557                                            0557
*          IF TRACXTF='0'B                        /*CONFIDENTIAL DATA*/
*            THEN DO;                             /* NO-PROCEED      */
         TM    TRACXTF(TRACPTR),B'00100000'                        0559
         BNZ   @RF00559                                            0559
*              IF (R6INPUT&INPTMSK)=FDBINPUT      /*IS CALLER TPIOS? */
*                THEN DO;                         /* YES-FDB INPUT   */
         LR    @11,R6INPUT                                         0561
         N     @11,INPTMSK                                         0561
         CL    @11,FDBINPUT                                        0561
         BNE   @RF00561                                            0561
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : TBSFDB - TPIOS FDB INPUT PROCESSOR (DOS ONLY)    */
*/*                                                                  */
*/*  FUNCTION : TO PROCESS TPIOS INBOUND INPUT. MOVES DATA FROM      */
*/*             FDBS UNTIL THE TRAC BUFFER IS FILLED OR UNTIL ALL    */
*/*             FDB DATA IS MOVED, WHICHEVER OCCURS FIRST.           */
*/*                                                                  */
*/*  INPUT : R6INPUT-ADDRESS OF FIRST FDB. FDBS ARE CHAINED, AND THE */
*/*                  END OF THE CHAIN IS ZERO.                       */
*/*                                                                  */
*/*  OUTPUT : A COMPLETED TRACE BUFFER.                              */
*/*           TRADATCT=LENGTH OF DATA IN THE BUFFER.                 */
*/*                                                                  */
*/*  ROUTINES : NONE                                                 */
*/*                                                                  */
*/*  MACROS : NONE                                                   */
*/*                                                                  */
*/********************************************************************/
*  RFY (BUFNX,LENR4) RSTD;                                         0563
*  RFY FDBR2 RSTD;                      /*SETUP FDB BASE-FIRST FDB   */
*  RFY ISTFDB BASED(FDBR2);             /*ADDRESS IS AN INPUT        */
*  FDBR2=R6INPUT;                       /*PARAMETER.                 */
         LR    FDBR2,R6INPUT                                       0566
*  DO WHILE (TRADATCT<BUFCT) & (FDBR2^=ZERO); /* FILL BUFFER UNTIL   */
         B     @DE00567                                            0567
@DL00567 DS    0H                                                  0568
*                                       /*FULL OR NO MORE INPUT      */
*    IF FDBDATCT>(BUFCT-TRADATCT)       /*DATA TO MOVE>BUFFER LEFT?  */
*      THEN                             /* YES-SET LENGTH TO MOVE =  */
         L     @11,BUFCT                                           0568
         SH    @11,TRADATCT(,TRACPTR)                              0568
         CH    @11,FDBDATCT(,FDBR2)                                0568
         BNL   @RF00568                                            0568
*        LENR4=(BUFCT-TRADATCT);        /*  SIZE OF REMAINING BUFFER */
         LR    LENR4,@11                                           0569
*      ELSE                             /* NO-SET LENGTH TO MOVE = TO*/
*        LENR4=FDBDATCT;                /*  DATA SIZE IN FDB         */
         B     @RC00568                                            0570
@RF00568 LH    LENR4,FDBDATCT(,FDBR2)                              0570
*    BUFNX=ADDR(TRABFFER)+TRADATCT;     /*ADDRESS TO MOVE TO IN TRAC */
@RC00568 LA    BUFNX,TRABFFER(,TRACPTR)                            0571
         LH    @11,TRADATCT(,TRACPTR)                              0571
         ALR   BUFNX,@11                                           0571
*    BUFNX->BUFDATA(1:LENR4)=FDBDATAA->BUFDATA(1:LENR4);  /*MOVE DATA*/
         LR    @03,LENR4                                           0572
         BCTR  @03,0                                               0572
         L     @09,FDBDATAA(,FDBR2)                                0572
         EX    @03,@SM04453                                        0572
*    FDBR2=FDBFDBA;                     /*GET ADDR OF NEXT FDB       */
         L     FDBR2,FDBFDBA(,FDBR2)                               0573
*    TRADATCT=LENR4+TRADATCT;           /*INCR.DATA COUNT BY AMT MOVD*/
         ALR   @11,LENR4                                           0574
         STH   @11,TRADATCT(,TRACPTR)                              0574
*  END;                                 /*  E N D  L O O P           */
@DE00567 LH    @11,TRADATCT(,TRACPTR)                              0575
         C     @11,BUFCT                                           0575
         BNL   @DC00567                                            0575
         LTR   FDBR2,FDBR2                                         0575
         BNZ   @DL00567                                            0575
@DC00567 DS    0H                                                  0576
*  RFY (LENR4,FDBR2,BUFNX) UNRSTD;                                 0576
*                                                                  0576
*/********************************************************************/
*                 END;                                             0577
*                ELSE DO;                         /* NO-RPL INPUT    */
         B     @RC00561                                            0578
@RF00561 DS    0H                                                  0579
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : TBSRPL - CONTROL LAYER RPL INPUT PROCESSOR       */
*/*                                                                  */
*/*  FUNCTION : TO CHANGE THE PROTECTION KEY AND TO OBTAIN THE LOCAL */
*/*             MEMORY LOCK NECESSARY TO PROCESS USER DATA. TO       */
*/*             INVOKE THE PROPER ROUTINE DEPENDING ON THE RPL       */
*/*             COMMAND CODE.                                        */
*/*                                                                  */
*/*  INPUT : R6INPUT - ADDRESS OF RPL                                */
*/*                                                                  */
*/*  OUTPUT : RTNCODE = 0 NO ERRORS                                  */
*/*                  ^= 0 WHEN AN ERROR OCCURS IN A VALCHECK ISSUED  */
*/*                       BY TBSRWSR OR TBSLDO. OR TBSLDO FOUND AN   */
*/*                       ERROR IN LDO JUMP COMMANDS.                */
*/*  ROUTINES : TBSRWSR-CALLED TO HANDLE READ/WRITE/SEND/RECEIVE     */
*/*                     INPUT.                                       */
*/*             TBSLDO-CALLED TO HANDLE LDO INPUT.                   */
*/*  MACROS : VMDESET-TO SET AND RESET THE PROTECTION KEY FROM VTAM  */
*/*                   PROTECTION KEY TO SUPERVISOR. NO-OP IN DOS.    */
*/*           VSETLCK-TO SET AND RESET THE LOCAL MEMORY LOCK IN VS2. */
*/*                   NO-OP IN DOS AND VS1.                          */
*/*                                                                  */
*/********************************************************************/
*  RFY IFGRPL BASED(R6INPUT);           /*RPL ADDRESS IS INPUT PARM. */
*  RFY R13 RSTD;                        /*SAVE PROTECT KEY IN REG2 . */
*/*VMDESET  ((RTNCODE),ZERO,,)*/                                   0581
*   DO;                                  /* INVOKE MODESET MACRO    */
*     GEN;                                                         0582
         MODESET EXTKEY=ZERO,WORKREG=2
*     RTNCODE=0;                         /*PUT RETURN CODE IN RCREG*/
         SLR   RTNCODE,RTNCODE                                     0583
*   END;  /* VMDESET */         /*SET PROTECT KEY TO 0 (VS2).*/    0584
*  R13=ADDR(RPHSAVE1);                  /*5 WORD SAVEAREA FOR VSETLCK*/
         LA    R13,RPHSAVE1(,RPHPTR)                               0585
*  TBLOCK:                              /*SET LOCAL MEMORY LOCK (VS2)*/
*                                                                  0586
*/*VSETLCK  (RTNCODE,OBTAIN,LOCAL,(LOCAL,ISTRACTB(TBUNLOCK)),)*/   0586
*    DO;                                                           0586
TBLOCK   DS    0H                                                  0587
*                                        /*DECLARE AND RESTRICT REGS */
*                                        /*   2,10,11,12,13,14, AND 15
**/                                                                0587
*    RFY (GPR02P,GPR10P,GPR11P,GPR12P,GPR13P,GPR14P,GPR15P) RSTD;  0587
*                                        /*SAVE REGS 2 AND 10 THRU 14*/
