         TITLE 'ISTORFSR - VS/2 GETSTOR/FREESTOR SERVICE ROUTINE       *
                        '
ISTORFSR CSECT ,                                                   0001
ISTORCSR DS    0H                                                  0001
         ENTRY ISTORCSR
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'ISTORFSR  77.266'                                 0001
         DROP  @15
@PROLOG  BALR  @05,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@05                                         0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*   IF(GPR03F&OPMASK)^=0 THEN       /* GET OR FREE REQUEST           */
         LR    @15,GPR03F                                          0104
         N     @15,OPMASK                                          0104
         LTR   @15,@15                                             0104
         BZ    @RF00104                                            0104
*FREE:                                                             0105
*     DO;                           /* IT IS A FREESTOR              */
FREE     DS    0H                                                  0106
*FREESTOR:                                                         0106
*       DO;                         /* PROCESS A FREESTOR REQUEST    */
FREESTOR DS    0H                                                  0107
*         RFY                                                      0107
*          (GPR10F,                                                0107
*           GPR09F) RSTD;                                          0107
*         GPR04F=ADDRMSK&GPR04F;    /* CLEAR HI BYTE         @ZM31427*/
         N     GPR04F,ADDRMSK                                      0108
*         GPR10P=GPR14P;            /* SAVE RETURN ADDRESS           */
         LR    GPR10P,GPR14P                                       0109
*         RFY                                                      0110
*           GPR14F UNRSTD;                                         0110
*         RFY                                                      0111
*           GPR06F RSTD;                                           0111
*         SPPARM=GPR03F&SPMASK;     /* ISOLATE SUBPOOL PASSED      0112
*                                                            @G35RKD9*/
         LR    SPPARM,GPR03F                                       0112
         N     SPPARM,@CF02016                                     0112
*         IF(SPPARM=SP227)×         /* CSA SUBPOOL ?         @G35RKD9*/
*             (SPPARM=SP228)×       /* CSA SUBPOOL ?         @G35RKD9*/
*             (SPPARM=SP231)×       /* CSA SUBPOOL ?         @G35RKD9*/
*             (SPPARM=SP241)×       /* CSA SUBPOOL ?         @G35RKD9*/
*             (SPPARM=SP239)×       /* CSA SUBPOOL ?         @G35RKD9*/
*             (SPPARM=SP245) THEN   /* FREE STORAGE THROUGH ORMMG  0113
*                                                            @G35RKD9*/
         C     SPPARM,@CF02040                                     0113
         BE    @RT00113                                            0113
         C     SPPARM,@CF02042                                     0113
         BE    @RT00113                                            0113
         C     SPPARM,@CF02044                                     0113
         BE    @RT00113                                            0113
         C     SPPARM,@CF02046                                     0113
         BE    @RT00113                                            0113
         C     SPPARM,@CF02048                                     0113
         BE    @RT00113                                            0113
         C     SPPARM,@CF02050                                     0113
         BNE   @RF00113                                            0113
@RT00113 DS    0H                                                  0114
*           DO;                     /*                       @G35RKD9*/
*             RFY                                                  0115
*               ISTSMHDR BASED(FREEPTR);/* BASE HEADER       @G35RKD9*/
*             RFY                                                  0116
*               ISTATCVT BASED(ATCVTPTR);/* BASE ATCVT       @G35RKD9*/
*             GPR09F=GPR01P;        /* SAVE RPH POINTER      @G35RKD9*/
         LR    GPR09F,GPR01P                                       0117
*             FREEPTR=FREEPTR-LENGTH(ISTSMHDR);/* ADJUST POINTER   0118
*                                                            @G35RKD9*/
         LA    @08,16                                              0118
         SLR   FREEPTR,@08                                         0118
*             GPR00F=GPR00F+LENGTH(ISTSMHDR);/* ADJUST LENGTH      0119
*                                                            @G35RKD9*/
         ALR   GPR00F,@08                                          0119
*             SRL(SPPARM,8);        /* GET IN LOW ORDER BYTE @G35RKD9*/
         SRL   SPPARM,8                                            0120
*             IF(SPPARM=SMHSBPL)&   /* SUBPOOL MATCHES       @G35RKD9*/
*                 (GPR00F=SMHLONG) THEN/* LENGTH MATCHES     @G35RKD9*/
         SLR   @08,@08                                             0121
         IC    @08,SMHSBPL(,FREEPTR)                               0121
         CR    SPPARM,@08                                          0121
         BNE   @RF00121                                            0121
         C     GPR00F,SMHLONG(,FREEPTR)                            0121
         BNE   @RF00121                                            0121
*               DO;                 /* VALID CALL- CHARACTERISTICS 0122
*                                      MATCH                 @G35RKD9*/
*                 IF(GPR03P&CLEARMSK)^=0 THEN/* CLEAR REQUESTED    0123
*                                                            @G35RKD9*/
         SLR   @08,@08                                             0123
         LR    @07,GPR03P                                          0123
         N     @07,CLEARMSK                                        0123
         CLR   @07,@08                                             0123
         BE    @RF00123                                            0123
*                   DO;             /*                       @G35RKD9*/
*                     RFY                                          0125
*                      (GPR14P,                                    0125
*                       GPR15P,                                    0125
*                       GPR07F,                                    0125
*                       GPR06F) RSTD;/*                      @G35RKD9*/
*                     GPR06F=GPR04F+LENGTH(ISTSMHDR);/* ADDRESS TO 0126
*                                      CLEAR                 @G35RKD9*/
         LA    @08,16                                              0126
         LR    GPR06F,GPR04F                                       0126
         ALR   GPR06F,@08                                          0126
*                     GPR07F=GPR00F-LENGTH(ISTSMHDR);/* GET LENGTH 0127
*                                                            @G35RKD9*/
         LCR   @08,@08                                             0127
         ALR   @08,GPR00F                                          0127
         LR    GPR07F,@08                                          0127
*                     GPR14P=GPR06F;/* FROM ADDRESS          @G35RKD9*/
         LR    GPR14P,GPR06F                                       0128
*                     GPR15P=0;     /* FROM LENGTH           @G35RKD9*/
         SLR   @08,@08                                             0129
         LR    GPR15P,@08                                          0129
*                     GEN CODE REFS(GPR06F,GPR07F,GPR14P,GPR15P)/* 0130
*                                                            @G35RKD9*/
*                         SETS(GPR06F,GPR07F,GPR14P)/* CLEAR AREA  0130
*                                                            @G35RKD9*/
*                         (MVCL GPR06F,GPR14P);/*            @G35RKD9*/
         MVCL GPR06F,GPR14P
*                     RFY                                          0131
*                      (GPR14P,                                    0131
*                       GPR15P) UNRSTD;/*                    @G35RKD9*/
*                   END;            /*                       @G35RKD9*/
*                 RFY                                              0133
*                  (OLDREG,                                        0133
*                   NEWREG,                                        0133
*                   ATCVTPTR) RSTD;                                0133
@RF00123 DS    0H                                                  0134
*                 ATCVTPTR=ATCLCPTR;/* GET ATCVT POINTER     @G35RKD9*/
         L     ATCVTPTR,ATCLCPTR                                   0134
*                 OLDREG=ATCORTBF;  /* GET TO-BE-FREE QUEUE HEADER 0135
*                                                            @G35RKD9*/
         L     OLDREG,ATCORTBF(,ATCVTPTR)                          0135
*CSLOOP:                                                           0136
*                 ;                 /*                       @G35RKD9*/
CSLOOP   DS    0H                                                  0137
*                 SMHCHTBF=OLDREG;  /* CHAIN QUEUE TO NEW ELEMENT  0137
*                                                            @G35RKD9*/
         ST    OLDREG,SMHCHTBF(,FREEPTR)                           0137
*                 CS(OLDREG,FREEPTR,ATCORTBF);/* ADD NEW ELEMENT TO
*                                      QUEUE                 @G35RKD9*/
         CS    OLDREG,@04,ATCORTBF(ATCVTPTR)                       0138
*                 BC(FAIL,CSLOOP);  /* CONTINUE UNTIL SUCCESSFUL   0139
*                                                            @G35RKD9*/
         BC    7,CSLOOP                                            0139
*                 OLDREG=ATCORSRB;  /* GET TO-BE-FREE QUEUE GATE   0140
*                                                            @G35RKD9*/
         L     OLDREG,ATCORSRB(,ATCVTPTR)                          0140
*NOSCHE:                                                           0141
*                 ;                 /*                       @G35RKD9*/
NOSCHE   DS    0H                                                  0142
*                 IF OLDREG<=0      /* TO-BE-FREE QUEUE GATED OR SRB
*                                      NOT INITIALIZED YET   @G35RKD9*/
*                   THEN                                           0142
         LTR   OLDREG,OLDREG                                       0142
         BNP   @RT00142                                            0142
*                   ;               /* DONT SCHEDULE ORMMG TO RUN-ITS
*                                      ALREADY RUNNING OR SRB HAS NOT
*                                      BE INITIALIZED YET    @G35RKD9*/
*                 ELSE              /*                       @G35RKD9*/
*                   DO;             /* ATTEMPT TO GATE QUEUE AND   0144
*                                      SCHEDULE ORMMG        @G35RKD9*/
*                     NEWREG=OLDREG×GATEMASK;/* SET UP GATE  @G35RKD9*/
         LR    NEWREG,OLDREG                                       0145
         O     NEWREG,@CF01548                                     0145
*                     CS(OLDREG,NEWREG,ATCORSRB);/* ATTEMPT TO GATE
*                                      QUEUE                 @G35RKD9*/
         CS    OLDREG,@08,ATCORSRB(ATCVTPTR)                       0146
*                     BC(FAIL,NOSCHE);/* CHECK TO SEE IF QUEUE     0147
*                                      ALREADY GATED@G35RKD9         */
         BC    7,NOSCHE                                            0147
*                     GPR04P=ATCORSRB;/* GET SRB ADDRESS     @G35RKD9*/
         L     GPR04P,ATCORSRB(,ATCVTPTR)                          0148
*                     RFY                                          0149
*                      (OLDREG,                                    0149
*                       NEWREG,                                    0149
*                       ATCVTPTR) UNRSTD;                          0149
*                     GEN CODE SETS(GPR02F,GPR06F) REFS(ZERO);     0150
*                                                   /*       @G35RKD9*/
         MODESET KEYADDR=ZERO,SAVEKEY=(2),WORKREG=6
*                     GEN REFS(CVTLSMQ,CVTMAP,SRBFLNK);            0151
*                                        /* SCHEDULE SRB     @G35RKD9*/
           SCHEDULE SRB=(GPR04P),SCOPE=LOCAL
*                     GEN CODE SETS(GPR02F,GPR06F) REFS(ZERO);     0152
*                                                   /*       @G35RKD9*/
         MODESET KEYADDR=(2),WORKREG=6
*                   END;            /* END SCHEDULING SRB    @G35RKD9*/
*                 RFY                                              0154
*                  (GPR14P,                                        0154
*                   GPR15F) RSTD;   /*                       @G35RKD9*/
@RT00142 DS    0H                                                  0155
*                 GPR04F=GPR03F;    /* GET INPUT PARM IN R4 TO RETURN
*                                                            @G35RKD9*/
         LR    GPR04F,GPR03F                                       0155
*                 GPR14P=GPR10P;    /* RESTORE RETURN ADDRESS      0156
*                                                            @G35RKD9*/
         LR    GPR14P,GPR10P                                       0156
*                 GPR01P=GPR09F;    /* RESTORE RPH POINTER   @G35RKD9*/
         LR    GPR01P,GPR09F                                       0157
*                 GPR15F=0;         /* SET RETURN CODE       @G35RKD9*/
         SLR   GPR15F,GPR15F                                       0158
*               END;                /* END VALID FREESTOR          0159
*                                      REQUEST-CHARACTERISTICS MATCH
*                                                            @G35RKD9*/
*             ELSE                                                 0160
*               DO;                 /* CHARACTERISTICS DONT MATCH  0160
*                                                            @G35RKD9*/
         B     @RC00121                                            0160
@RF00121 DS    0H                                                  0161
*                 GPR04F=GPR03F;    /* GET INPUT PARM IN R4 FOR    0161
*                                      RETURN                @G35RKD9*/
         LR    GPR04F,GPR03F                                       0161
*                 GPR14P=GPR10P;    /* RETSTORE RETURN ADDRESS     0162
*                                                            @G35RKD9*/
         LR    GPR14P,GPR10P                                       0162
*                 GPR15F=4;         /* SET RETURN CODE       @G35RKD9*/
         LA    GPR15F,4                                            0163
*               END;                /*                       @G35RKD9*/
*           END;                    /* END FREESTOR FOR CSA SUBPOOLS
*                                                            @G35RKD9*/
*         ELSE                                                     0166
*           DO;                     /* FREESTOR F0R NON-CSA SUBPOOLS
*                                                            @G35RKD9*/
         B     @RC00113                                            0166
@RF00113 DS    0H                                                  0167
*             RFY                                                  0167
*              (GPR07F,                                            0167
*               GPR08F) RSTD;                                      0167
*             GPR07F=GPR11F;        /* SAVE REGS 11 - 13             */
         LR    GPR07F,GPR11F                                       0168
*             GPR08F=GPR12F;                                       0169
         LR    GPR08F,GPR12F                                       0169
*             GPR09F=GPR13F;                                       0170
*                                                                  0170
         LR    GPR09F,GPR13F                                       0170
*/*                   SET STORAGE PROTECT KEY TO 0                   */
*                                                                  0171
*             GEN CODE SETS(GPR02F,GPR06F) REFS(ZERO);             0171
         MODESET KEYADDR=ZERO,SAVEKEY=(2),WORKREG=6
*             GPR06F=GPR02F;        /* SAVE PRIOR PROTECT KEY        */
         LR    GPR06F,GPR02F                                       0172
*             RFY                                                  0173
*               GPR02F UNRSTD;                                     0173
*             RFY                                                  0174
*               GPR14F RSTD;                                       0174
*SETL1:                                                            0175
*             GEN CODE SETS(GPR11F,GPR12F,GPR13F,GPR14F) REFS(PSALITA,
*                 FLC);                                            0175
SETL1    DS    0H                                                  0175
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        X
               RELATED=(IEAVGM00,ISTORFSR(RELL1))
*             RFY                                                  0176
*              (GPR02F,                                            0176
*               GPR14F,                                            0176
*               GPR15F) UNRSTD;                                    0176
*             RFY                                                  0177
*               GPR10F RSTD;                                       0177
*             IF GPR13F=4 THEN      /* DID THIS TASK ALREADY HAVE THE
*                                      LOCAL MEMORY LOCK             */
         C     GPR13F,@CF00048                                     0178
         BNE   @RF00178                                            0178
*HELD:                                                             0179
*               GPR03F=GPR03F×LOCKMASK;/* YES - THEREFORE WE MUST NOT
*                                      RELEASE IT SO SET IND         */
HELD     O     GPR03F,LOCKMASK                                     0179
*             ELSE                                                 0180
*               ;                   /* CALLER DID NOT HAVE LOCK      */
@RF00178 DS    0H                                                  0181
*FSTOR1:                                                           0181
*             DO;                   /* CONTINUE FREESTOR PROCESS     */
FSTOR1   DS    0H                                                  0182
*               GPR11F=GPR07F;      /* RESTORE REGS AFTER SETLOCK    */
         LR    GPR11F,GPR07F                                       0182
*               GPR12F=GPR08F;                                     0183
         LR    GPR12F,GPR08F                                       0183
*               GPR13F=GPR09F;                                     0184
         LR    GPR13F,GPR09F                                       0184
*               GPR09P=GPR01P;      /* PRESERVE RPH POINTER          */
         LR    GPR09P,GPR01P                                       0185
*               IF(GPR03F&FIXMASK)=0 THEN/* IS STORAGE PAGE- FIXED   */
         LR    @15,GPR03F                                          0186
         N     @15,FIXMASK                                         0186
         LTR   @15,@15                                             0186
         BNZ   @RF00186                                            0186
*FST1:                                                             0187
*                 DO;               /* NO - ADJUST REGS FOR CLEAR AND
*                                      FREEING STORAGE               */
FST1     DS    0H                                                  0188
*                   GPR01P=GPR04P;  /* SET UP START ADDRESS OF AREA
*                                      TO FREE                       */
         LR    GPR01P,GPR04P                                       0188
*                   GPR08F=GPR03F;  /* SAVE INPUT PARM REG AROUND  0189
*                                      CLEAR AND FREEMAIN            */
         LR    GPR08F,GPR03F                                       0189
*                 END FST1;         /* END PAGEABLE STOR PROC        */
*               ELSE                /* STORAGE IS FIXED              */
*PGFX:                                                             0191
*                 DO;               /* STORAGE MUST BE PGFREED       */
         B     @RC00186                                            0191
@RF00186 DS    0H                                                  0191
PGFX     DS    0H                                                  0192
*PFREE:                                                            0192
*                   GPR07P=GPR04P;  /* SAVE STORAGE ADDRESS          */
PFREE    LR    GPR07P,GPR04P                                       0192
*                   GPR08F=GPR00F;  /* SAVE LENGTH TO FREE           */
         LR    GPR08F,GPR00F                                       0193
*                   GPR01P=GPR04P;  /* START ADDR TO PGREEE          */
         LR    GPR01P,GPR04P                                       0194
*                   RFY                                            0195
*                    (GPR02P,                                      0195
*                     GPR00P) RSTD; /*                       @Z30APGE*/
*                   GPR00P=GPR01P;  /* GET ADDR OF ECB FOR PGFREE  0196
*                                                            @Z30APGE*/
         LR    GPR00P,GPR01P                                       0196
*                   GPR02P=GPR01P+GPR08F;/* END ADDR TO PGFREE     0197
*                                                            @Z30APGE*/
         LR    GPR02P,GPR01P                                       0197
         ALR   GPR02P,GPR08F                                       0197
*                   GEN CODE SETS(GPR01P) REFS(GPR01P,PAGEFREE)    0198
*                       (ICM GPR01P,8,PAGEFREE);                   0198
         ICM GPR01P,8,PAGEFREE
*                   RFY                                            0199
*                     GPR15F RSTD;                                 0199
*                   GPR15P=FLCCVT;                                 0200
         L     GPR15P,FLCCVT                                       0200
*                   GEN CODE REFS(GPR04P,GPR14P,GPR15P,CVTVPSIB) SETS(
*                       GPR04P,GPR14P,GPR15P);                     0201
         L     GPR15P,CVTVPSIB(GPR15P)   GET ADDR OF PGFIX/PGFREE
         SR    GPR04P,GPR04P             CLEAR TCB POINTER
         BALR  GPR14P,GPR15P             CALL PGFREE
*                   RFY                                            0202
*                    (GPR00P,                                      0202
*                     GPR02P) UNRSTD;/*                      @Z30APGE*/
*                   RFY                                            0203
*                     GPR15F UNRSTD;                               0203
*                   GPR00F=GPR08F;  /* RESTORE LENGTH TO FREE        */
         LR    GPR00F,GPR08F                                       0204
*                   GPR01P=GPR07P;  /* RESTORE ADDR TO FREE          */
         LR    GPR01P,GPR07P                                       0205
*                   GPR08F=GPR03F;  /* PRESERVE FLAGS ACROSS FREEMAIN*/
         LR    GPR08F,GPR03F                                       0206
*                 END PGFX;         /* STORAGE HAS BEEN PGFREED      */
*/********************************************************************/
*/*                                                                  */
*/* REGISTER STATUS REPORT                                           */
*/*                                                                  */
*/*                0 = LENGTH TO BE FREED                            */
*/*                1 = ADDRESS TO FREE                               */
*/*                2 = WORKREG                                       */
*/*                3 = INPUT PARAMETERS (FLAGS KEY SUBPOOL OP)       */
*/*                4 = WORKREG AVAIL                                 */
*/*                5 = BASE REG                                      */
*/*                6 = ORIGINAL PROTECT KEY                          */
*/*                7 = WORKREG AVAILABLE                             */
*/*                8 = COPY OF INPUT PARAMETERS                      */
*/*                9 = ADDRESS OF RPH                                */
*/*               10 = RETURN ADDRESS                                */
*/*               11 = CALLER REG                                    */
*/*               12 = CALLER REG                                    */
*/*               13 = CALLER REG                                    */
*/*               14 = WORKREG (UNRSTD)                              */
*/*               15 = WORKREG (UNRSTD)                              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0208
*CL:                                                               0208
*               IF(GPR03F&CLEARMSK)^=0 THEN                        0208
@RC00186 DS    0H                                                  0208
CL       LR    @15,GPR03F                                          0208
         N     @15,CLEARMSK                                        0208
         LTR   @15,@15                                             0208
         BZ    @RF00208                                            0208
*CLR:                                                              0209
*                 DO;               /* CLEAR TO 0 BEFORE FREEING     */
CLR      DS    0H                                                  0210
*                   RFY                                            0210
*                     GPR04F UNRSTD;                               0210
*                   RFY                                            0211
*                    (GPR14F,                                      0211
*                     GPR15F,                                      0211
*                     GPR02P) RSTD; /*                       @G35RKD9*/
*                   GPR02P=GPR01P;  /* START ADDRESS                 */
         LR    GPR02P,GPR01P                                       0212
*                   GPR03F=GPR00F;  /* LENGTH TO CLEAR               */
         LR    GPR03F,GPR00F                                       0213
*                   GPR14P=GPR02P;  /* FROM ADDRESS                  */
         LR    GPR14P,GPR02P                                       0214
*                   GPR15F=0;       /* FROM LENGTH                   */
         SLR   GPR15F,GPR15F                                       0215
*                   GEN CODE REFS(GPR02P,GPR03F,GPR14P) SETS(GPR02P,
*                       GPR03F,GPR14P)(MVCL GPR02P,GPR14P);        0216
         MVCL GPR02P,GPR14P
*                   RFY                                            0217
*                    (GPR02F,                                      0217
*                     GPR14F,                                      0217
*                     GPR15F,                                      0217
*                     GPR02P) UNRSTD;/*                      @G35RKD9*/
*                   RFY                                            0218
*                     GPR04F RSTD;                                 0218
*                   GPR03F=GPR08F;  /* RESTORE INPUT PARMS           */
         LR    GPR03F,GPR08F                                       0219
*                 END CLR;                                         0220
*               ELSE                                               0221
*                 ;                 /* STORAGE NOT TO BE CLEARED     */
@RF00208 DS    0H                                                  0222
*FSET:                                                             0222
*               GPR07P=PSAAOLD;     /* GET ASCB ADDRESS FOR FREEMAIN
*                                      BRANCH ENTRY                  */
FSET     L     GPR07P,PSAAOLD                                      0222
*               GPR04P=PSATOLD;     /* GET TCBPTR FOR FREEMAIN       */
         L     GPR04P,PSATOLD                                      0223
*               GEN CODE REFS(GPR03F,USERKEY)(CLM GPR03F,4,USERKEY);
         CLM GPR03F,4,USERKEY
*               BC(7,BFM);          /* DID CALLER SAY KEY(USER)      */
         BC    7,BFM                                               0225
*FKEYUSER:                                                         0226
*               DO;                 /* YES - USE KEY OF CURR TCB     */
FKEYUSER DS    0H                                                  0227
*                 GEN CODE REFS(TCBPKF,GPR04P,GPR03F) SETS(GPR03F) 0227
*                     (ICM GPR03F,4,TCBPKF(GPR04P));               0227
         ICM GPR03F,4,TCBPKF(GPR04P)
*               END FKEYUSER;       /* REG 3 NOW HAS KEY OF CALLER 0228
*                                      TASK (USER KEY)               */
*               RFY                                                0229
*                (GPR14F,                                          0229
*                 GPR15F) RSTD;                                    0229
*BFM:                                                              0230
*               GEN CODE REFS(GPR00F,GPR01P,GPR03F,GPR04P,GPR07P,GPR14P
*                   ,GPR15P,ZROBYTE,CVTPTR,CVTCRMN,CVTMAP);        0230
BFM      DS    0H                                                  0230
       ICM   GPR03F,8,ZROBYTE        CLEAR BYTE 0 REG 3
         FREEMAIN RC,LV=(0),A=(1),KEY=(3),SP=(3),BRANCH=YES
*               RFY                                                0231
*                 GPR15F UNRSTD;                                   0231
*               GPR01P=GPR09P;      /* RESTORE RPH POINTER           */
         LR    GPR01P,GPR09P                                       0232
*               GPR04F=GPR08F;      /* PRESERVE INPUT PARM FLAGS     */
         LR    GPR04F,GPR08F                                       0233
*               IF(GPR04F&LOCKMASK)=0 THEN/* DID CALLER ALREADY OWN
*                                      LOCK                          */
         LR    @15,GPR04F                                          0234
         N     @15,LOCKMASK                                        0234
         LTR   @15,@15                                             0234
         BNZ   @RF00234                                            0234
*FST2:                                                             0235
*                 DO;               /* NO, WE GOT IT SO WE FREE IT   */
FST2     DS    0H                                                  0236
*                   GPR07F=GPR11F;  /* PRESERVE REGS AROUND          */
         LR    GPR07F,GPR11F                                       0236
*                   GPR08F=GPR12F;  /* SETLOCK FUNCTION              */
         LR    GPR08F,GPR12F                                       0237
*                   GPR09F=GPR13F;                                 0238
         LR    GPR09F,GPR13F                                       0238
*RELL1:                                                            0239
*                   GEN CODE SETS(GPR11F,GPR12F,GPR13F,GPR14F) REFS(
*                       PSALITA,FLC);                              0239
RELL1    DS    0H                                                  0239
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(IEAVGM00,ISTORFSR(SETL1))
*                   GPR11F=GPR07F;  /* RESTORE REGS                  */
         LR    GPR11F,GPR07F                                       0240
*                   GPR12F=GPR08F;                                 0241
         LR    GPR12F,GPR08F                                       0241
