*   ELSE DO;                          /* THIS RECORD IS LOST NO 01S0818
*                                       DYNAMIC AREA CORE AVAILABLE */
         B     @RC00630                                         01S0818
@RF00630 DS    0H                                               01S0819
*         R4 = RPHSAVE5;    /* RESTORE NCB PTR FOR NODE BEING TRACED */
         L     R4,RPHSAVE5(,RPHPTR)                             01S0819
*         R4->NCBLTRC = R4->NCBLTRC+1;  /* INCR LOST RECORD COUNT */
         LA    @10,1                                            01S0820
         AH    @10,NCBLTRC(,R4)                                 01S0820
         STH   @10,NCBLTRC(,R4)                                 01S0820
*        END;                                                   01S0821
*GEN SETS(R0,R2,R15) REFS(RPHSAVE1,RPHSAVE3,R1);                01S0822
@RC00630 DS    0H                                               01S0822
         L      R0,RPHSAVE1(R1)         RESTORE REG 0
         LM     R2,R15,RPHSAVE3(R1)     RESTORE REGS 2 - 15
*RETURN;                                                        01S0823
@EL00001 DS    0H                                               01S0823
@EF00001 DS    0H                                               01S0823
@ER00001 BR    @14                                              01S0823
*DATAXTNT: PROC OPTIONS(NOSAVE,NOSAVEAREA);                     01S0824
DATAXTNT DS    0H                                               01S0825
*/*********************************************************************
*/* 1. CALCULATE LOGICAL LENGTH OF 1ST BUFFER,I.E. AMNT OF DATA 01S0825
*/* IN THE BUFFER */                                            01S0825
*/* 2. IF LOGICAL LENGTH >= DATA LIMIT IMPOSED BY GTF OR        01S0825
*/* THE PHYSICAL BUFFER SIZE */                                 01S0825
*/*      THEN SET GTF DATA LENGTH = DATA LIMIT & STORE IT       01S0825
*                                  IN THE GTF PARM LIST */      01S0825
*/*      ELSE SET GTF DATA LENGTH = LOGICAL LENGTH & STORE IT         *
*/* 3. CALCULATE ADDR OF LAST BYTE OF GTF DATA                        *
*/*********************************************************************
*                                                               01S0825
*RFY R14 RSTD;                         /* RSTRCT REG.14 FOR RTRN @ */
*LBUFLEN = FDBDATCT + FDBDATAA - GTFDATAA;   /* LOGICAL BUFFER LENGTH *
         L     @15,GTFDATAA                                     01S0826
         LH    @10,FDBDATCT(,@15)                               01S0826
         AL    @10,FDBDATAA(,@15)                               01S0826
         SLR   @10,@15                                          01S0826
         STH   @10,LBUFLEN                                      01S0826
*LBUFENDA = LBUFLEN + GTFDATAA - 1;    /* @ LAST BYTE OF LOGCL BUFR */
         ALR   @15,@10                                          01S0827
         BCTR  @15,0                                            01S0827
         ST    @15,LBUFENDA                                     01S0827
*IF LBUFLEN >= GTFLIM    /* IF LOGCL LEN >= DATA LIMIT       @XA08633*/
*  THEN GTFLEN = GTFLIM;  /* SET GTF LEN = DATA LIMIT        @XA08633*/
         LH    @15,GTFLIM                                       01S0828
         CR    @10,@15                                          01S0828
         BL    @RF00828                                         01S0828
         STH   @15,GTFLEN                                       01S0829
*  ELSE GTFLEN = LBUFLEN;              /* SET GTF LEN = LOGCL LEN     *
         B     @RC00828                                         01S0830
@RF00828 LH    @15,LBUFLEN                                      01S0830
         STH   @15,GTFLEN                                       01S0830
*GTFDATND = GTFDATAA + GTFLEN - 1;     /* ADDR LAST BYTE GTF DATA     *
@RC00828 LH    @15,GTFLEN                                       01S0831
         AL    @15,GTFDATAA                                     01S0831
         BCTR  @15,0                                            01S0831
         ST    @15,GTFDATND                                     01S0831
*RFY R14 UNRSTD;                       /* RELEASE REG.14 */     01S0832
*END DATAXTNT;                                                  01S0833
@EL00002 DS    0H                                               01S0833
@EF00002 DS    0H                                               01S0833
@ER00002 BR    @14                                              01S0833
*ADDRTRF:PROC OPTIONS(NOSAVE,NOSAVEAREA);                       01S0834
ADDRTRF  DS    0H                                               01S0835
*/*********************************************************************
*/* 1. DETERMINE HOW MANY BYTES THE TRAILER ISTTRF WILL HAVE TO BE    *
*/* ADJUSTED SO THAT IT WILL FIT WITHIN THE GTF DATA RECORD AS THE    *
*/* LAST LENGTH(ISTTRF) BYTES OF THE RECORD. THIS ADJUSTMENT,TRFDELTA,*
*/* IS ALSO THE # OF BYTES THAT WILL HAVE TO BE SAVED BEFORE PLACING  *
*/* THE TRAILER INTO THE GTF RECORD & RESTORED AFTER THE GTF HOOK HAS *
*/* BEEN ENGAGED                                                      *
*/*                                                                   *
*/* 2. RECALCULATE THE LENGTH OF THE GTF DATA BLOCK                   *
*/* 3. CALCULATE THE ADDR OF THE TRAILER,ISTTRF                       *
*/*********************************************************************
*                                                               01S0835
*RFY R14 RSTD;                         /* RSTRCT REG.14 FOR RTRN @ */
*RFY ISTPROCD BASED(ADDR(FMCPROCD));   /* BASE FMCB PROC OPTIONS      *
*IF PROCFTX = ON                       /* IF TEXT IS CONFIDENTIAL     *
*  THEN DO;                            /* PREVENT RECORDING IT */
         TM    PROCFTX+48(R5),B'00000001'                       01S0837
         BNO   @RF00837                                         01S0837
*        TRFPTR = TEXTPTR;             /* PUT TRLR AT START OF TEXT */
         L     @15,TEXTPTR                                      01S0839
         LR    TRFPTR,@15                                       01S0839
*        TRFDELTA = LENGTH(ISTTRF);    /* AMNT TXT TO BE OVERLAYED */
         LA    @10,17                                           01S0840
         STH   @10,TRFDELTA                                     01S0840
*        GTFLEN = TEXTPTR - GTFDATAA + LENGTH(ISTTRF);          01S0841
         SL    @15,GTFDATAA                                     01S0841
         ALR   @15,@10                                          01S0841
         STH   @15,GTFLEN                                       01S0841
*       END;                                                    01S0842
*  ELSE DO;                                                     01S0843
         B     @RC00837                                         01S0843
@RF00837 DS    0H                                               01S0844
*   IF GTFLEN = GTFLIM  /* IF GTF DATA LENGTH = MAX AMNT     @XA08633*/
*     THEN TRFDELTA = LENGTH(ISTTRF); /* THE BUFFER IS FULL AND THE
*                                       AMNT OF DATA TO BE OVERLAYED
*                                       = LEN OF THE TRAILER */ 01S0844
         CLC   GTFLEN(2),GTFLIM                                 01S0844
         BNE   @RF00844                                         01S0844
         MVC   TRFDELTA(2),@CH02426                             01S0845
*     ELSE                                                      01S0846
*      IF (((ATCHBSIZ+7)×7)&&7) = LBUFLEN /* IF PHYS SIZE    @XA08633
*                                    = LOGICAL, OVERLAY LAST @XA08633
*                                    BYTES                   @XA08633*/
*        THEN TRFDELTA = LENGTH(ISTTRF);                   /*@XA08633*/
         B     @RC00844                                         01S0846
@RF00844 LA    @15,7                                            01S0846
         L     @10,VTAMCVTA                                     01S0846
         LH    @10,ATCHBSIZ(,@10)                               01S0846
         ALR   @10,@15                                          01S0846
         OR    @10,@15                                          01S0846
         XR    @10,@15                                          01S0846
         CH    @10,LBUFLEN                                      01S0846
         BNE   @RF00846                                         01S0846
         MVC   TRFDELTA(2),@CH02426                             01S0847
*        ELSE                                              /*@XA08633*/
*         IF (GTFLIM -GTFLEN) >= LENGTH(ISTTRF)            /*@XA08633*/
*           THEN DO;              /* TRAILER WILL FIT IN BUFFER */
         B     @RC00846                                         01S0848
@RF00846 LA    @15,17                                           01S0848
         LH    @10,GTFLEN                                       01S0848
         LH    @02,GTFLIM                                       01S0848
         SLR   @02,@10                                          01S0848
         CR    @02,@15                                          01S0848
         BL    @RF00848                                         01S0848
*                 TRFDELTA = 0;   /* NO DATA TO BE OVERLAYED */ 01S0850
         SLR   @02,@02                                          01S0850
         STH   @02,TRFDELTA                                     01S0850
*                 GTFLEN = GTFLEN+LENGTH(ISTTRF); /* ADD LEN OF TRAILER
*                                                 TO LEN OF GTF DATA */
         ALR   @10,@15                                          01S0851
         STH   @10,GTFLEN                                       01S0851
*                END;                                           01S0852
*           ELSE DO;              /* TRAILER WON'T FIT IN BUFFER */
         B     @RC00848                                         01S0853
@RF00848 DS    0H                                               01S0854
*                 TRFDELTA = GTFLEN+LENGTH(ISTTRF)-GTFLIM; /*@XA08633*/
         LH    @15,GTFLIM                                       01S0854
         LA    @10,17                                           01S0854
         AH    @10,GTFLEN                                       01S0854
         SLR   @10,@15                                          01S0854
         STH   @10,TRFDELTA                                     01S0854
*                                      /* AMNT OF DATA TO BE OVERLAYED*
*                 GTFLEN = GTFLIM;   /* GTF DATA LEN = MAX ALLOWED */
         STH   @15,GTFLEN                                       01S0855
*                END;                                           01S0856
*  TRFPTR = GTFDATND - TRFDELTA +1;    /* ADDR OF TRLR ISTTRF */
@RC00848 DS    0H                                               01S0857
@RC00846 DS    0H                                               01S0857
@RC00844 L     TRFPTR,GTFDATND                                  01S0857
         SH    TRFPTR,TRFDELTA                                  01S0857
         AL    TRFPTR,@CF00060                                  01S0857
*       END;                                                    01S0858
*                                                               01S0858
*/*********************************************************************
*/*  IF TRACE IS FOR LINE BUFFERS                                     *
*/*    THEN 1. LOCATE THE TRAILER ON THE NEXT LOWER WORD BDY          *
*/*            RELEATIVE TO THE START OF THE BUFFER TEXT              *
*/*         2. RECALCULATE THE AMNT OF TEXT TO BE OVERLAYED BY        *
*/*            THE TRAILER AND THE AMNT OF GTF DATA                   *
*/*********************************************************************
*                                                               01S0859
*IF LINETRSW = ON                      /* IF TRACE IS FOR LINE BUFFER *
*  THEN DO;                                                     01S0859
@RC00837 TM    LINETRSW,B'00100000'                             01S0859
         BNO   @RF00859                                         01S0859
*GTFTXTCT = TRFPTR - TEXTPTR;          /* GTF TEXT COUNT */     01S0861
         LR    @15,TRFPTR                                       01S0861
         SL    @15,TEXTPTR                                      01S0861
         STH   @15,GTFTXTCT                                     01S0861
*BDYDIFF = GTFTXTCT//4;                /* DIFF BETWEEEN TRAILER @ AND
*                                         RELEATIVE WORD BDY */ 01S0862
         LR    @08,@15                                          01S0862
         SRDA  @08,32                                           01S0862
         D     @08,@CF00052                                     01S0862
         STH   @08,BDYDIFF                                      01S0862
*TRFPTR = TRFPTR - BDYDIFF;             /* ADJ @TRF TO LOWER WRD BDY */
         SLR   TRFPTR,@08                                       01S0863
*TRFDELTA = TRFDELTA + BDYDIFF;         /* RECALC AMNT TO BE SAVED */
         LH    @10,TRFDELTA                                     01S0864
         ALR   @10,@08                                          01S0864
         STH   @10,TRFDELTA                                     01S0864
*GTFLEN = GTFLEN - BDYDIFF;             /* RECALC AMNT GTF DATA */
         LH    @10,GTFLEN                                       01S0865
         SLR   @10,@08                                          01S0865
         STH   @10,GTFLEN                                       01S0865
*GTFTXTCT = GTFTXTCT - BDYDIFF;    /* RECALC AMNT TEXT IN GTF DATA */
         SLR   @15,@08                                          01S0866
         STH   @15,GTFTXTCT                                     01S0866
*       END;                                                    01S0867
*  ELSE;                                                        01S0868
@RF00859 DS    0H                                               01S0869
*RFY R14 UNRSTD;                        /* RELEASE REG.14 */    01S0869
*END ADDRTRF;                                                   01S0870
@EL00003 DS    0H                                               01S0870
@EF00003 DS    0H                                               01S0870
@ER00003 BR    @14                                              01S0870
*MOVEDATA:PROC OPTIONS(NOSAVE,NOSAVEAREA);                      01S0871
MOVEDATA DS    0H                                               01S0872
*/*********************************************************************
*/* MOVE N BYTES OF DATA FROM THE FIELD LOCATED BY Q2                 *
*/* TO THE FIELD LOCATED BY Q1                                        *
*/*********************************************************************
*                                                               01S0872
*RFY R14 RSTD;                         /* RSTRCT REG.14 FOR RTRN @ */
*DO WHILE N > 256;                     /* MOVE BLOCKS OF 256 BYTES    *
         B     @DE00873                                         01S0873
@DL00873 DS    0H                                               01S0874
*Q1->BUFDATA(1:256) = Q2->BUFDATA(1:256);                       01S0874
         L     @15,Q1                                           01S0874
         L     @10,Q2                                           01S0874
         MVC   BUFDATA(256,@15),BUFDATA(@10)                    01S0874
*Q1 = Q1+256;                                                   01S0875
         LA    @06,256                                          01S0875
         ALR   @15,@06                                          01S0875
         ST    @15,Q1                                           01S0875
*Q2 = Q2+256;                                                   01S0876
         ALR   @10,@06                                          01S0876
         ST    @10,Q2                                           01S0876
*N = N-256;                                                     01S0877
         LCR   @06,@06                                          01S0877
         AH    @06,N                                            01S0877
         STH   @06,N                                            01S0877
*END;                                                           01S0878
@DE00873 LH    @15,N                                            01S0878
         C     @15,@CF00680                                     01S0878
         BH    @DL00873                                         01S0878
*Q1->BUFDATA(1:N) = Q2->BUFDATA(1:N); /* MOVE WHATEVER IS LEFT        *
         L     @10,Q1                                           01S0879
         BCTR  @15,0                                            01S0879
         L     @06,Q2                                           01S0879
         EX    @15,@SM04695                                     01S0879
*RFY R14 UNRSTD;                       /* RELEASE REG.14 */     01S0880
*END MOVEDATA;                                                  01S0881
*                                                               01S0881
*                                                               01S0881
@EL00004 DS    0H                                               01S0881
@EF00004 DS    0H                                               01S0881
@ER00004 BR    @14                                              01S0881
