*/*     BY THE TRAILER                                                *
*/*********************************************************************
*                                                               02S0760
*CALL DATAXTNT;                        /* CALC LGCL BUF & GTF DATA LEN*
*                                                               02S0760
         BAL   @14,DATAXTNT                                     02S0760
*RFY ISTZOBUF BASED(GTFDATAA);         /* OUTBOUND BUFFER BASE */
*RFY ISTBTU BASED(ADDR(ZOBBTU));       /* BTU BASE */           02S0762
*IF RPHCRR->CRACRR->ZCROB = ON         /* TEST FOR OUTBOUND I/O */
*  THEN OUTBNDSW = ON;                 /* SET OUTBOUND FLAG */  02S0763
         L     @10,RPHCRR(,RPHPTR)                              02S0763
         L     @10,CRACRR(,@10)                                 02S0763
         TM    ZCROB(@10),B'00010000'                           02S0763
         BNO   @RF00763                                         02S0763
         OI    OUTBNDSW,B'00010000'                             02S0764
*  ELSE OUTBNDSW = OFF;                /* RESET OUTBOUND FLAG */
         B     @RC00763                                         02S0765
@RF00763 NI    OUTBNDSW,B'11101111'                             02S0765
*IF R3->NCBTYPE = LDTYPE               /* IS DEST LOCAL 3270 */ 02S0766
*  THEN TEXTPTR = FDBDATAA;            /* FDB POINTS TO STRT OF TEXT */
@RC00763 CLI   NCBTYPE(R3),X'07'                                02S0766
         BNE   @RF00766                                         02S0766
         L     @10,GTFDATAA                                     02S0767
         L     @10,FDBDATAA(,@10)                               02S0767
         ST    @10,TEXTPTR                                      02S0767
*  ELSE IF OUTBNDSW = ON                                        02S0768
*         THEN IF BTUFIDNO = '0001'B   /* I/O IS OUTBOUND */    02S0768
         B     @RC00766                                         02S0768
@RF00766 TM    OUTBNDSW,B'00010000'                             02S0768
         BNO   @RF00768                                         02S0768
*                THEN TEXTPTR = ADDR(BTUPAD);  /* FID1 FORMAT */
         LA    @10,28                                           02S0769
         AL    @10,GTFDATAA                                     02S0769
         TM    BTUFIDNO(@10),B'00010000'                        02S0769
         BNO   @RF00769                                         02S0769
         TM    BTUFIDNO(@10),B'11100000'                        02S0769
         BNZ   @RF00769                                         02S0769
         LA    @10,BTUPAD(,@10)                                 02S0770
         ST    @10,TEXTPTR                                      02S0770
*                ELSE TEXTPTR = ADDR(BTUTEXT);  /* FID0 FORMAT */
         B     @RC00769                                         02S0771
@RF00769 L     @10,GTFDATAA                                     02S0771
         LA    @10,BTUTEXT+28(,@10)                             02S0771
         ST    @10,TEXTPTR                                      02S0771
*         ELSE TEXTPTR = FDBDATAA;     /* I/O IS INBOUND */     02S0772
         B     @RC00768                                         02S0772
@RF00768 L     @10,GTFDATAA                                     02S0772
         L     @10,FDBDATAA(,@10)                               02S0772
         ST    @10,TEXTPTR                                      02S0772
*CALL ADDRTRF;                         /* CALC TRLR ADDR & AMNT TEXT
*                                         TO BE OVERLAYED             *
@RC00768 DS    0H                                               02S0773
@RC00766 BAL   @14,ADDRTRF                                      02S0773
*RFY BUFDATA BASED(TRFPTR);                                     02S0774
*IF TRFDELTA > 0                        /* WILL TEXT BE OVERLAYED */
*  THEN DATASAVE = BUFDATA(1:TRFDELTA); /* SAVE TEXT THAT WILL BE
*                                         OVERLAYED */          02S0775
         LH    @10,TRFDELTA                                     02S0775
         LTR   @10,@10                                          02S0775
         BNP   @RF00775                                         02S0775
         MVI   DATASAVE+1,C' '                                  02S0776
         MVC   DATASAVE+2(18),DATASAVE+1                        02S0776
         BCTR  @10,0                                            02S0776
         EX    @10,@SM04685                                     02S0776
*  ELSE;                                                        02S0777
@RF00775 DS    0H                                               02S0778
*ISTTRF = '0'B;                        /* INIT TRAILER = ZERO         *
         XC    ISTTRF+1(16,TRFPTR),ISTTRF+1(TRFPTR)             02S0778
         MVI   ISTTRF(TRFPTR),B'00000000'                       02S0778
*TRFDNODE = R4->NCBRDTE->RPRNAME;      /* PUT DEST NODE NAME IN TRLR  *
         L     @10,NCBRDTE(,R4)                                 02S0779
         MVC   TRFDNODE(8,TRFPTR),RPRNAME(@10)                  02S0779
*TRFANODE = FMCDEBA->ACDRDTE->RPRNAME; /* PUT APPL NODE NAME IN TRLR  *
         L     @10,FMCDEBA(,R5)                                 02S0780
         L     @10,ACDRDTE(,@10)                                02S0780
         MVC   TRFANODE(8,TRFPTR),RPRNAME(@10)                  02S0780
*IF OUTBNDSW = OFF                                              02S0781
*  THEN TRFINB = ON;                   /* SET INBOUND DATA FLAG IN TRF*
         TM    OUTBNDSW,B'00010000'                             02S0781
         BNZ   @RF00781                                         02S0781
         OI    TRFINB(TRFPTR),B'10000000'                       02S0782
*  ELSE;                                                        02S0783
@RF00781 DS    0H                                               02S0784
*IF R3->NCBTYPE = LDTYPE                                        02S0784
*  THEN TRFLOCAL = ON;                 /* SET LOCAL 3270 FLAG IN TRF  *
         CLI   NCBTYPE(R3),X'07'                                02S0784
         BNE   @RF00784                                         02S0784
         OI    TRFLOCAL(TRFPTR),B'01000000'                     02S0785
*  ELSE;                                                        02S0786
@RF00784 DS    0H                                               02S0787
*IF R3->NCBTYPE = ICTYPE &  /* SET LOCAL SNA FLAG            @XM08361*/
*   R3->ICNLCLUS = ON                                      /*@XM08361*/
*    THEN                                                  /*@XM08361*/
         CLI   NCBTYPE(R3),X'06'                                02S0787
         BNE   @RF00787                                         02S0787
         TM    ICNLCLUS(R3),B'00100000'                         02S0787
         BNO   @RF00787                                         02S0787
*      TRFLSNA = ON;                                       /*@XM08361*/
         OI    TRFLSNA(TRFPTR),B'00010000'                      02S0788
*    ELSE;                                                 /*@XM08361*/
@RF00787 DS    0H                                               02S0790
*RFY ISTPROCD BASED(ADDR(FMCPROCD));                            02S0790
*IF PROCFTX = ON                                                02S0791
*  THEN TRFCXTF = ON;                  /* SET CONF TEXT FLAG IN TRF   *
         TM    PROCFTX+48(R5),B'00000001'                       02S0791
         BNO   @RF00791                                         02S0791
         OI    TRFCXTF(TRFPTR),B'00100000'                      02S0792
*  ELSE;                                                        02S0793
@RF00791 DS    0H                                               02S0794
*R1 = ADDR(GTFPLIST);             /* POINT TO GTF PARM LIST */  02S0794
*                                                               02S0794
         LA    R1,GTFPLIST                                      02S0794
*GEN;                                                           02S0795
*                                                               02S0795
         GTRACE ID=ISTTPEID,MF=(E,(1))    ENGAGE GTF HOOK
*IF TRFDELTA > 0                        /* IF TEXT WAS SAVED */ 02S0796
*  THEN BUFDATA(1:TRFDELTA) = DATASAVE(1:TRFDELTA); /* RESTORE IT */
         LH    @10,TRFDELTA                                     02S0796
         LTR   @10,@10                                          02S0796
         BNP   @RF00796                                         02S0796
         BCTR  @10,0                                            02S0797
         EX    @10,@SM04688                                     02S0797
*  ELSE;                                                        02S0798
*                                                               02S0798
@RF00796 DS    0H                                               02S0799
*/*********************************************************************
*              END;                                             01S0799
*        GEN SETS(R2) REFS(R12);                                01S0800
@RC00651 DS    0H                                               01S0800
         SR    R2,R2
         ICM R2,7,@SIZDATD+1    GET SIZE DYNAMIC AREA
*        R1 = ADDRRPH;                     /* RESTORE RPH PTR */
         L     R1,ADDRRPH                                       01S0801
*        RFY R2 RSTD;                                           01S0802
*                                                               01S0802
*   /*FREESTOR SIZE(R2)AREA(R11)SAVE(NO)TYPE(FIXED)*/           01S0803
*    DO;                                                        01S0803
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */               01S0804
*/* END OF ATBUILD */                                           01S0804
*   RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;                     01S0804
*   GPR00F = R2;                                       /*SIZE IN REG0*/
         LR    GPR00F,R2                                        01S0805
*   DCL 1 CMZA0056 FIXED(31) STATIC LOCAL,                      01S0806
*         2 * FIXED(8) INIT(0),                      /* OPTION FLAGS */
*         2 * FIXED(8) INIT(96),                      /* STORAGE KEY */
*         2 * FIXED(8) INIT(227),                  /* SUBPOOL NUMBER */
*         2 * FIXED(8) INIT('01'X);                  /* REQUEST TYPE */
*   GPR03F = CMZA0056;                                          01S0807
         L     GPR03F,CMZA0056                                  01S0807
*   RFY (GPR04P) RSTD;                                          01S0808
*   GPR04P = R11;                         /*PUT FREEMAIN ADDR IN REG4*/
*                                                               01S0809
         LR    GPR04P,R11                                       01S0809
*   DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR02P)) */               01S0810
