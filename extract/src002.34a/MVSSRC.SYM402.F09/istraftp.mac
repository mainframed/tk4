         TITLE 'ISTRAFTP - OS/VS TPIOS BUFFER TRACE                    *
                        '
ISTRAFTP CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'ISTRAFTP  76.205'                              01S0001
         DROP  @15
@PROLOG  DS    0H                                               01S0002
         STM R0,R15,RPHWORK(R1)   SAVE REGS 0 - 15
         BALR  R12,0          SET UP ADDRESSABILITY
         USING *,R12
         SR     R2,R2
         ICM R2,7,@SIZDATD+1      GET DYNAMIC AREA SIZE
*                                                               01S0616
* /*GETSTOR SIZE(R2)SAVE(NO)TYPE(FIXED)*/                       01S0617
*  DO;                                                          01S0617
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */               01S0618
*/* END OF ATBUILD */                                           01S0618
* RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;                       01S0618
* GPR00F = R2;                                         /*SIZE IN REG0*/
         LR    GPR00F,R2                                        01S0619
* DCL 1 CMZA0017 FIXED(31) STATIC LOCAL,                        01S0620
*       2 * FIXED(8) INIT(0),                        /* OPTION FLAGS */
*       2 * FIXED(8) INIT(96),                        /* STORAGE KEY */
*       2 * FIXED(8) INIT(227),                    /* SUBPOOL NUMBER */
*       2 * FIXED(8) INIT('00'X);                    /* REQUEST TYPE */
* GPR03F = CMZA0017;                                            01S0621
*                                                               01S0621
         L     GPR03F,CMZA0017                                  01S0621
* DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR02P)) */                 01S0622
*   GPR02P = ATCLCPTR; /*SET UP ATCVT POINTER*/                 01S0623
         L     GPR02P,ATCLCPTR                                  01S0623
* END; /*LISATCVT*/                                             01S0624
* CALL GPR02P -> ATCGTSTR;                            /*CALL ISTORFSR*/
         L     @15,ATCGTSTR(,GPR02P)                            01S0625
         BALR  @14,@15                                          01S0625
* RFY (GPR00P,GPR01P,GPR02P,GPR03P) UNRSTD;                     01S0626
* END ;                                              /*END OF GETSTOR*/
**GET DATA AREA      @Z40GKZA*/                                 01S0627
*RFY R2 UNRSTD;                                                 01S0628
*RFY R15 RSTD;                                                  01S0629
*IF R15 = 0                            /* IF CORE OBTAINED */   01S0630
*  THEN DO;                                                     01S0630
         LTR   R15,R15                                          01S0630
         BNZ   @RF00630                                         01S0630
*        RFY R15 UNRSTD;                                        01S0632
*        GEN SETS(R3,R6,R11) REFS(R1,RPHSAVE4);                 01S0633
         LR    R11,R3   LOAD DYNAMIC AREA BASE REG
         USING @DATD,R11   ESTABL ADDRESSABILITY
         LM    R3,R6,RPHSAVE4(R1)  RELOAD INTERFACE REGS
*        RFY R6 RSTD;                  /* RSTRCT REG.6 FOR BUFR PTR */
*        RFY R11 RSTD;                 /* RSTRCT REG.11  */     01S0635
*        RFY (R2,R3,R4,R5) RSTD;       /* RSTRCT NCB,FMCB & CVT BASES *
*                                                               01S0636
*                                                               01S0636
*         DO; /* LISATCVT CVTBASE(PAGEABLE,(R2)) */             01S0637
*           R2 = ATCLCPTR; /*SET UP ATCVT POINTER*/             01S0638
         L     R2,ATCLCPTR                                      01S0638
*         END; /*LISATCVT*/                                     01S0639
*                                                               01S0639
*        VTAMCVTA = R2;                /* SAVE ADDR VTAM CVT */ 01S0640
         ST    R2,VTAMCVTA                                      01S0640
*        RFY R2 UNRSTD;                                         01S0641
*        ADDRRPH = R1;                  /* SAVE RPH PTR */      01S0642
         ST    R1,ADDRRPH                                       01S0642
*        GTFPLIST = GTFPLIST&&GTFPLIST; /* INIT GTF PARM LIST */
         XC    GTFPLIST(8),GTFPLIST                             01S0643
*        SWITCHES = SWITCHES&&SWITCHES; /* RESET ALL SWITCHES */
         XC    SWITCHES(1),SWITCHES                             01S0644
*        GTFDATAA = R6;               /* SET UP  GTF DATA ADDR       */
         ST    R6,GTFDATAA                                      01S0645
*        GTFFMTID = 'FD'X;            /* FORMAT RTNE ID */      01S0646
         MVI   GTFFMTID,X'FD'                                   01S0646
*        PHYSSIZE = ATCHBSIZ - LENGTH(SRBSECT);/*SUBTR SRB   @XM42102*/
         L     @10,VTAMCVTA                                     01S0647
         LH    PHYSSIZE,ATCHBSIZ(,@10)                          01S0647
         SL    PHYSSIZE,@CF04681                                01S0647
*        IF PHYSSIZE >= BUF256       /* DETERMINE BUF SIZE   @XM42102*/
*          THEN GTFLIM = BUF256;     /* GTFLIMIT=256         @XA08633*/
         LA    @10,256                                          01S0648
         CR    PHYSSIZE,@10                                     01S0648
         BL    @RF00648                                         01S0648
         STH   @10,GTFLIM                                       01S0649
*          ELSE GTFLIM = PHYSSIZE;      /* GTFLIMIT=BUF SIZE @XM42102*/
*                                                               01S0650
         B     @RC00648                                         01S0650
@RF00648 STH   PHYSSIZE,GTFLIM                                  01S0650
*        IF (R3->NCBTYPE = ICTYPE)     /* IF NCB IS AN ICNCB          *
*         & (R5 = R3->NCBFMCBA)        /*  & FMCB BELONGS TO VARY     *
*         & (RPHCRR->CRACRR->ZCROB = OFF)      /*  & DATA IS INBOUND */
*         & NSRUNETS = SRURLNTR        /* & RU IS FOR FUNCT MGMT */
*         THEN DO;                     /* RECORD NCP LINE BUFFERS     *
@RC00648 CLI   NCBTYPE(R3),X'06'                                01S0651
         BNE   @RF00651                                         01S0651
         C     R5,NCBFMCBA(,R3)                                 01S0651
         BNE   @RF00651                                         01S0651
         L     @10,RPHCRR(,RPHPTR)                              01S0651
         L     @10,CRACRR(,@10)                                 01S0651
         TM    ZCROB(@10),B'00010000'                           01S0651
         BNZ   @RF00651                                         01S0651
         L     @10,GTFDATAA                                     01S0651
         L     @10,FDBDATAA(,@10)                               01S0651
         CLC   NSRUNETS(3,@10),@CB03669                         01S0651
         BNE   @RF00651                                         01S0651
*/***********************************************************/  01S0653
*/*    1) TEST IF SLOWDOWN IS IN PROGRESS                   */  01S0653
*/*    2) IF SLOWDOWN IN PROGRESS RESET RN RDTE             */  01S0653
*/*       RPRLTASN TO OFF AND RESET LINE RDTE               */  01S0653
*/*       RPRLTASN TO OFF. THIS WILL ENABLE                 */  01S0653
*/*       RESTARTING LINE TRACE AFTER SLOWDOWN              */  01S0653
*/*       IS ENDED.                                         */  01S0653
*/***********************************************************/  01S0653
*IF TRRSTAT(7:8) = '11'B                 /*TEST FOR SLOWDOWN @XM10332*/
*  THEN DO;                              /*SLOWDOWN ON       @XM10332*/
         TM    TRRSTAT(@10),B'00000011'                         01S0653
         BNO   @RF00653                                         01S0653
*       RDTPTR = NCBRDTE;                /*RN RDTE           @XM10332*/
         L     RDTPTR,NCBRDTE(,R3)                              01S0655
*       RPRLTASN = OFF;                  /*TURN OFF LINE TRC @XM10332*/
         NI    RPRLTASN(RDTPTR),B'01111111'                     01S0656
*       DO WHILE RPRENTED ^=ON;          /*LOOP THRU RDTES   @XM10332*/
         B     @DE00657                                         01S0657
@DL00657 DS    0H                                               01S0658
*         IF RPRENTRY = RPRENTLN         /*LINE RDTE         @XM10332*/
*           THEN RPRLTASN = OFF;         /*TURN OFF LINE TRC @XM10332*/
         CLI   RPRENTRY(RDTPTR),X'50'                           01S0658
         BNE   @RF00658                                         01S0658
         NI    RPRLTASN(RDTPTR),B'01111111'                     01S0659
*           ELSE;                        /*DUMMY ELSE        @XM10332*/
@RF00658 DS    0H                                               01S0661
*         RDTPTR = RDTPTR + RPRELEN;     /*NEXT RDTE         @XM10332*/
         AH    RDTPTR,RPRELEN(,RDTPTR)                          01S0661
*       END;                             /*END DOWHILE       @XM10332*/
@DE00657 TM    RPRENTED(RDTPTR),B'00000100'                     01S0662
         BNO   @DL00657                                         01S0662
*      END;                              /*END SLOWDOWN      @XM10332*/
*  ELSE;                                 /*DUMMY ELSE        @XM10332*/
@RF00653 DS    0H                                               01S0665
*  IF TRRFMT2 = ON       /* IS THIS A FORMAT 2 LINE TRACE    @Z40GKZA*/
*    THEN DO;            /* YES                              @Z40GKZA*/
         L     @10,GTFDATAA                                     01S0665
         L     @10,FDBDATAA(,@10)                               01S0665
         TM    TRRFMT2(@10),B'00000010'                         01S0665
         BNO   @RF00665                                         01S0665
*       RFY (R10,R13) RSTD;                                /*@Z40GKZA*/
*       R10 = ADDR(TLWRK);  /* PASS WORK AREA ADDRESS        @Z40GKZA*/
         LA    R10,TLWRK                                        01S0668
*       R13 = ADDR(TLSAVE); /* PASS SAVE AREA ADDRESS        @Z40GKZA*/
         LA    R13,TLSAVE                                       01S0669
*       CALL ISTRACTL;      /* PROCESS THE BUFFERS           @Z40GKZA*/
         L     @15,@CV04649                                     01S0670
         BALR  @14,@15                                          01S0670
*       RFY (R10,R13) UNRSTD;                              /*@Z40GKZA*/
*     END;                                                 /*@Z40GKZA*/
*    ELSE DO;            /* NO                               @Z40GKZA*/
         B     @RC00665                                         01S0673
@RF00665 DS    0H                                               01S0674
*                                                               02S0674
*/*********************************************************************
*/* RECORD DATA FROM NCP LINE BUFFERS                                 *
*/*  1. SETUP PTR TO NEXT LINE BUFFER IN BUFFER CHAIN                 *
*/*     INIT LOOP SW TO EXECUTE RECORDING LOOP                        *
*/*  2. EXECUTE RECORDING LOOP UNTIL ALL TEXT FROM ALL BUFFERS        *
*/*    HAS BEEN RECODED                                               *
*/*      A. CALC LENGTH OF LOGICAL BUFFER & GTF DATA BLOCK            *
*/*      B. CALC ADDR OF TRAILER TO BE PLACED AT END OF GTF DATA      *
*/*      C. SAVE ANY DATA THAT WILL BE OVERLAYED BY THE TRAILER       *
*/*      D. INIT TRAILER ISTTRF = 0                                   *
*/*      E. PUT DEST NODE NAME INTO TRAILER                           *
*/*      F. ENGAGE GTF HOOK VIA GTRACE                                *
*/*      G. RESTORE ANY DATA,THAT WAS PREVIOUSLY SAVED,INTO           *
*/*        AREA OCCUPIED BY THE TRAILER                               *
*/*      H. CALC TEXT REMAINING IN LOGICAL BUFFER                     *
*/*      J. IF NO TEXT REMAINS TO BE RECORDED                         *
*/*           THEN RESET LOOP SW TO TERMINATE RECORDING               *
*/*           ELSE MOVE AS MUCH OF THE REMAINDER AS POSSIBLE          *
*/*                INTO THE 1ST BUFFER & CONTINUE RECORDING           *
*/*********************************************************************
*                                                               02S0674
*LINETRSW = ON;                        /* SET LINE TRACE SW */  02S0674
*RECORDSW = ON;                        /* INIT RECORDING LOOP SW */
         OI    LINETRSW,B'10100000'                             02S0675
*TEXTPTR = ADDR(TRRDATA);             /* STRT @ TEXT IN 1ST BUFFER */
         L     @10,GTFDATAA                                     02S0676
         L     @07,FDBDATAA(,@10)                               02S0676
         LA    @07,TRRDATA(,@07)                                02S0676
         ST    @07,TEXTPTR                                      02S0676
*P = FDBFDBA;                          /* PTR TO NEXT BUFFER IN CHAIN *
         L     @10,FDBFDBA(,@10)                                02S0677
         ST    @10,P                                            02S0677
*DO WHILE RECORDSW=ON;                                          02S0678
         B     @DE00678                                         02S0678
@DL00678 DS    0H                                               02S0679
*CALL DATAXTNT;                        /* CALC LGCL BUF & GTF DATA LEN*
         BAL   @14,DATAXTNT                                     02S0679
*CALL ADDRTRF;                         /* CALC ADDR OF TRLR ISTTRF    *
         BAL   @14,ADDRTRF                                      02S0680
*RFY BUFDATA BASED(TRFPTR);                                     02S0681
*IF TRFDELTA > 0                                                02S0682
*  THEN DATASAVE = BUFDATA(1:TRFDELTA);  /* SAVE TEXT THAT      02S0682
*                                          WILL BE OVERLAYED */ 02S0682
         LH    @10,TRFDELTA                                     02S0682
         LTR   @10,@10                                          02S0682
         BNP   @RF00682                                         02S0682
         MVI   DATASAVE+1,C' '                                  02S0683
         MVC   DATASAVE+2(18),DATASAVE+1                        02S0683
         BCTR  @10,0                                            02S0683
         EX    @10,@SM04685                                     02S0683
*  ELSE;                                                        02S0684
@RF00682 DS    0H                                               02S0685
*ISTTRF = '0'B;                        /* INIT TRAILER = 0            *
         XC    ISTTRF+1(16,TRFPTR),ISTTRF+1(TRFPTR)             02S0685
         MVI   ISTTRF(TRFPTR),B'00000000'                       02S0685
*TRFDNODE = R4->NCBRDTE->RPRNAME;      /* DEST NODE NAME INTO TRLR    *
         L     @10,NCBRDTE(,R4)                                 02S0686
         MVC   TRFDNODE(8,TRFPTR),RPRNAME(@10)                  02S0686
*TRFINB = ON;                           /* SET INBOUND FLAG IN TRLR */
         OI    TRFINB(TRFPTR),B'10000000'                       02S0687
*R1 = ADDR(GTFPLIST);                   /* POINT TO GTF PARM LIST */
*                                                               02S0688
         LA    R1,GTFPLIST                                      02S0688
*GEN;                                                           02S0689
*                                                               02S0689
         GTRACE ID=ISTLNEID,MF=(E,(1))
*IF TRFDELTA > 0                                                02S0690
*  THEN BUFDATA(1:TRFDELTA) = DATASAVE(1:TRFDELTA); /* RESTORE TEXT */
         LH    @10,TRFDELTA                                     02S0690
         LTR   @10,@10                                          02S0690
         BNP   @RF00690                                         02S0690
         BCTR  @10,0                                            02S0691
         EX    @10,@SM04688                                     02S0691
*  ELSE;                                                        02S0692
@RF00690 DS    0H                                               02S0693
*RTEXTCT = LBUFENDA - TRFPTR + 1;       /* TEXT LEFT IN 1ST BUFFER */
         L     @10,LBUFENDA                                     02S0693
         SLR   @10,TRFPTR                                       02S0693
         AL    @10,@CF00060                                     02S0693
         STH   @10,RTEXTCT                                      02S0693
*IF RTEXTCT > 3                                                 02S0694
*  THEN DO;                             /* MOVE REMAINDER TO START OF
*                                         1ST BUFR TXT AREA */  02S0694
         C     @10,@CF00287                                     02S0694
         BNH   @RF00694                                         02S0694
*        RTEXTA = TRFPTR;               /* ADDR REMAINING TEXT        *
         ST    TRFPTR,RTEXTA                                    02S0696
*                                                               03S0697
*/*********************************************************************
*/* 1. MOVE REMAINING TEXT TO START OF TEXT AREA IN FIRST LINE BUFFER *
*/* 2. CALC # BYTES REQUIRED TO COMPLETE A GTF DATA BLOCK             *
*/* 3. UPDATE THE DATA COUNT IN THE FIRST BUFFER.                     *
*/*   SET IT = TO # BYTES REMAINING TO BE RECORDED & SAVE THIS VALUE  *
*/* 4.INIT SW TO EXECUTE FILL 1ST BUFFER LOOP                         *
*/* 5.EXECUTE LOOP UNTIL 1ST BUFFER TEXT AREA HAS BEEB FILLED         *
*/*     A. IF THERE ARE NO MORE BUFFERS IN BUFFER CHAIN               *
*/*           OR THE NEXT BUFFER HAS NO DATA                          *
*/*          THEN RESET  LOOP SW TO STOP TEXT MOVEMENT       */ 03S0697
*/*          ELSE 1. CALC START ADDR OF ADDITIONAL TEXT FOR 1ST BUFFER*
*/*               2. IF DATA COUNT FOR NXT BUFR > # BYTES RQRD        *
*/*                     TO FILL 1ST BUFFER                            *
*/*                    THEN 1.MOVE JUST ENOUGH TEXT TO FILL 1ST BUFFR *
*/*                         2.INCR   DATA COUNT OF FIRST BUFR BY      *
*/*                           AMNT OF TEXT JUST MOVED                 *
*/*                         3.DECR DATA COUNT OF BUFR FROM WHICH TEXT *
*/*                           WAS MOVED BY THE AMNT MOVED             *
*/*                         4.INCR DATA PTR IN THIS BUFFER BY AMNT    *
*/*                          MOVED SO IT POINTS TO NEW TEXT           *
*/*                         5.RESET THE  LOOP SW TO STOP TXT MOVEMENT *
*/*                    ELSE 1.MOVE ALL TEXT FROM NXT BUFR INTO 1ST BUF*
*/*                         2.INCR DATA COUNT OF FIRST BUFR BY        *
*/*                           AMNT OF TEXT MOVED                      *
*/*                         3.DECR AMNT OF TEXT RQRD TO FILL 1ST BUFFR*
*/*                           BY AMNT OF TEXT MOVED                   *
*/*                         4.IF AMNT OF TEXT RQRD FOR 1ST BUFFER     *
*/*                              IS = ZERO                            *
*/*                             THEN RESET  LOOP SW                   *
*/*                                  TO STOP TEXT MOVEMENT            *
*/*                             ELSE 1.INCR OFFSET TO BE USED IN CALC *
*/*                                    OF START ADDR OF ADDITIONAL TXT*
*/*                                    IN 1ST BUFFER                  *
*/*                                  2.UPDATE PTR TO NEXT BUFFER      *
*/*                                    IN CHAIN                       *
*/*                                  3. SET TEXT-MOVE LOOP SW ON SO   *
*/*                                    TEXT WILL CONTINUE TO BE MOVED *
*/*                                    INTO THE 1ST BUFFER            *
*/*********************************************************************
*                                                               03S0697
*N = RTEXTCT;                          /* INIT AMNT TEXT TO BE MOVED
*                                        WITH # BYTES TEXT REMAING IN
*                                        FIRST BUFFER                 *
         STH   @10,N                                            03S0697
*Q1 = FDBDATAA + RULENGTH;             /* INIT RECEIVING FIELD PTR
*                                        WITH ADDR OF TEXT AREA IN
*                                        FIRST BUFFER                 *
         LH    @10,RULENGTH                                     03S0698
         L     @07,GTFDATAA                                     03S0698
         AL    @10,FDBDATAA(,@07)                               03S0698
         ST    @10,Q1                                           03S0698
*Q2 = RTEXTA;                          /* INIT SENDING FIELD PTR WITH
*                                        ADDR OF TEXT LEFT IN 1ST BUFR*
         L     @10,RTEXTA                                       03S0699
         ST    @10,Q2                                           03S0699
*CALL MOVEDATA;                        /* MOVE TEXT TO START FILLING
*                                        1ST BUFFER           */
         BAL   @14,MOVEDATA                                     03S0700
*FILLCT = GTFTXTCT;                    /* AMNT TEXT RQRD TO FILL
*                                          1ST BUFFER            */
         LH    FILLCT,GTFTXTCT                                  03S0701
*FDBDATCT = RTEXTCT + RULENGTH;        /* UPDATE DATA COUNT IN 1ST BUF*
         LH    @10,RTEXTCT                                      03S0702
         LH    @02,RULENGTH                                     03S0702
         ALR   @02,@10                                          03S0702
         L     @15,GTFDATAA                                     03S0702
         STH   @02,FDBDATCT(,@15)                               03S0702
*NEWDELTA = RTEXTCT;                   /* INIT OFFSET FOR START ADDR
*                                        OF ADDIT'L TEXT FOR 1ST BUF */
         STH   @10,NEWDELTA                                     03S0703
*FILLSW = ON;                          /* INIT SW TO EXECUTE LOOP TO
*                                        CONTINUE FILLING 1ST BUFFER  *
         OI    FILLSW,B'01000000'                               03S0704
*DO WHILE FILLSW=ON;                   /* FILL 1ST BUFFER TXT AREA  */
         B     @DE00705                                         03S0705
@DL00705 DS    0H                                               03S0706
* IF P = 0 × P->FDBDATCT = 0           /* IF NO MORE TEXT IN BUF CHAIN*
*   THEN FILLSW = OFF;                 /* RESET FILL 1ST BUFR SW */
         L     @10,P                                            03S0706
         SLR   @02,@02                                          03S0706
         CR    @10,@02                                          03S0706
         BE    @RT00706                                         03S0706
         CH    @02,FDBDATCT(,@10)                               03S0706
         BNE   @RF00706                                         03S0706
@RT00706 DS    0H                                               03S0707
         NI    FILLSW,B'10111111'                               03S0707
*   ELSE DO;                                                    03S0708
         B     @RC00706                                         03S0708
@RF00706 DS    0H                                               03S0709
*         NEWDATAA = TEXTPTR + NEWDELTA;        /* START ADDR FOR
*                                                     ADDIT'L TEXT   */
         LH    @10,NEWDELTA                                     03S0709
         AL    @10,TEXTPTR                                      03S0709
         ST    @10,NEWDATAA                                     03S0709
*         IF P->FDBDATCT > FILLCT                               03S0710
*           THEN DO;                                            03S0710
         L     @02,P                                            03S0710
         CH    FILLCT,FDBDATCT(,@02)                            03S0710
         BNL   @RF00710                                         03S0710
*                 N = FILLCT;          /* INIT AMNT TEXT TO BE MOVED  *
         STH   FILLCT,N                                         03S0712
*                 Q1 = NEWDATAA;       /* INIT RECEIVING FIELD PTR    *
         ST    @10,Q1                                           03S0713
*                 Q2 = P->FDBDATAA;    /* INIT SENDING FIELD PTR     */
         L     @10,FDBDATAA(,@02)                               03S0714
         ST    @10,Q2                                           03S0714
*                 CALL MOVEDATA;       /* MOVE TEXT INTO 1ST BUFFER   *
         BAL   @14,MOVEDATA                                     03S0715
*                                      /* INCR 1ST BUFR DATA COUNT    *
*                 FDBDATCT = FDBDATCT + FILLCT;                 03S0716
         L     @10,GTFDATAA                                     03S0716
         LR    @02,FILLCT                                       03S0716
         AH    @02,FDBDATCT(,@10)                               03S0716
         STH   @02,FDBDATCT(,@10)                               03S0716
*                 P->FDBDATCT = P->FDBDATCT - FILLCT;           03S0717
         L     @10,P                                            03S0717
         LH    @02,FDBDATCT(,@10)                               03S0717
         SLR   @02,FILLCT                                       03S0717
         STH   @02,FDBDATCT(,@10)                               03S0717
*                                      /* DECR NEXT BUFFER DATA COUNT *
*                 P->FDBDATAA = P->FDBDATAA + FILLCT; /* INCR PTR TO
*                                                       NEW TEXT      *
         LR    @02,FILLCT                                       03S0718
         AL    @02,FDBDATAA(,@10)                               03S0718
         ST    @02,FDBDATAA(,@10)                               03S0718
*                 FILLSW = OFF;        /* RESET FILL 1ST BUFFER SW    *
         NI    FILLSW,B'10111111'                               03S0719
*                END;                                           03S0720
*           ELSE DO;                                            03S0721
         B     @RC00710                                         03S0721
@RF00710 DS    0H                                               03S0722
*                 N = P->FDBDATCT;     /* INIT AMNT TEXT TO BE MOVED  *
         L     @10,P                                            03S0722
         LH    @02,FDBDATCT(,@10)                               03S0722
         STH   @02,N                                            03S0722
*                 Q1 = NEWDATAA;       /* INIT RECEIVING FIELD PTR   */
         L     @02,NEWDATAA                                     03S0723
         ST    @02,Q1                                           03S0723
*                 Q2 = P->FDBDATAA;    /* INIT SENDING FIELD PTR    */
         L     @10,FDBDATAA(,@10)                               03S0724
         ST    @10,Q2                                           03S0724
*                 CALL MOVEDATA;       /* MOVE TEXT                   *
         BAL   @14,MOVEDATA                                     03S0725
*                                      /* INCR 1ST BUF DATA COUNT     *
*                 FDBDATCT = FDBDATCT + P->FDBDATCT;            03S0726
         L     @10,GTFDATAA                                     03S0726
         L     @02,P                                            03S0726
         LH    @02,FDBDATCT(,@02)                               03S0726
         LH    @00,FDBDATCT(,@10)                               03S0726
         ALR   @00,@02                                          03S0726
         STH   @00,FDBDATCT(,@10)                               03S0726
*                 FILLCT = FILLCT - P->FDBDATCT; /* DECR AMNT TEXT RQRD
*                                                  TO FILL 1ST BUFFER *
         SLR   FILLCT,@02                                       03S0727
*                 IF FILLCT = 0                                 03S0728
*                   THEN FILLSW = OFF;  /* RESET FILL 1ST BUFFER SW   *
         LTR   FILLCT,FILLCT                                    03S0728
         BNZ   @RF00728                                         03S0728
         NI    FILLSW,B'10111111'                               03S0729
*                   ELSE DO;                                    03S0730
         B     @RC00728                                         03S0730
@RF00728 DS    0H                                               03S0731
*                                     /* INCR OFFSET FOR ADDIT'L TEXT *
*                         NEWDELTA = NEWDELTA + P->FDBDATCT;    03S0731
         LH    @10,NEWDELTA                                     03S0731
         L     @02,P                                            03S0731
         AH    @10,FDBDATCT(,@02)                               03S0731
         STH   @10,NEWDELTA                                     03S0731
