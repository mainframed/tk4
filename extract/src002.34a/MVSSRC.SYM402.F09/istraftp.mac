         L     GPR02P,ATCLCPTR                                  01S0811
*   END; /*LISATCVT*/                                           01S0812
*   CALL GPR02P -> ATCGTSTR;                          /*CALL ISTORFSR*/
         L     @15,ATCGTSTR(,GPR02P)                            01S0813
         BALR  @14,@15                                          01S0813
*   RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P) UNRSTD;            01S0814
