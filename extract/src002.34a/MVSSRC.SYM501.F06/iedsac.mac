         B     @RC00772                                         03S0774
@RF00772 L     @10,IEDQSPTR                                     03S0774
         OI    SCBCTLUN(@10),B'00000100'                        03S0774
*                 CALL BFRBDPST;    /* POST BUFFER TO IEDQBD         */
@RC00772 BAL   @14,BFRBDPST                                     03S0775
*               END;                                            03S0776
*             ELSE                                              03S0777
*               DO;                                             03S0777
         B     @RC00770                                         03S0777
@RF00770 DS    0H                                               03S0778
*                 IF TRMNLOUT=ON THEN/* IF NULL WRITE IS OUTSTANDING */
         TM    TRMNLOUT(RTRM),B'00000100'                       03S0778
         BNO   @RF00778                                         03S0778
*                   IF LCBRESP=ON THEN/* AND RESPONSE OWED IS ON     */
         TM    LCBRESP(RLCB),B'00000001'                        03S0779
         BNO   @RF00779                                         03S0779
*                     DO;           /* (IE., NULL WR EOT HAD ERROR)  */
*                       LCBRESP=OFF;/* THEN TURN OFF RESP OWED       */
         NI    LCBRESP(RLCB),B'11111110'                        03S0781
*                       TRMNLOUT=OFF;/* TURN OFF NULL OUT            */
         NI    TRMNLOUT(RTRM),B'11111011'                       03S0782
*                       CALL FREEBFR;/* FREE PIU BFR         @YM07714*/
         BAL   @14,FREEBFR                                      03S0783
