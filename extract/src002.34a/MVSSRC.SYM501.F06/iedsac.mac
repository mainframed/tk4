         TITLE 'IEDSAC: WRITE, CONTACT, DISCONNECT AND RESET COMMAND PR*
               ROCESSOR '
IEDSAC   CSECT ,                                                01S0001
@PROLOG  DS    0H                                               01S0002
         DC    A(0)        QCB
         DC    A(0)
         DC    X'06'       STCB
         DC    AL3(*-1)
         USING *,RBASE
         LR    RBASE,RLINK
IEDSAC IEDHJN START,HJN
*   RPRF=RPARM;                     /* ESTABLISH PRF ADDRESSABILITY  */
         LR    RPRF,RPARM                                       02S0517
*   RPARM=TTHOAF;                   /* CALL TNT CODE TO              */
         LA    @10,PRFPIUO(,RPRF)                               02S0518
         SLR   @07,@07                                          02S0518
         IC    @07,PRFPIUO(,RPRF)                               02S0518
         ALR   @10,@07                                          02S0518
         SLR   RPARM,RPARM                                      02S0518
         ICM   RPARM,3,TTHOAF(@10)                              02S0518
*   CALL IEDQTNT;                   /* DEVELOPE THE                  */
         L     @15,AVTRNMPT(,IEDQAPTR)                          02S0519
         BALR  @14,@15                                          02S0519
*   RTRM=RPARM-LENGTH(IEDNTRM);     /* TRM ADDRESS           @YM04622*/
         LR    RTRM,RPARM                                       02S0520
         SL    RTRM,@CF01995                                    02S0520
*   IEDQQPTR=TRMDESTQ;              /* ESTABLISH QCB ADDRESSABILITY  */
         SLR   IEDQQPTR,IEDQQPTR                                02S0521
         ICM   IEDQQPTR,7,TRMDESTQ(RTRM)                        02S0521
*   IF QCBPLCBN=ON THEN             /* THERE WILL BE A PLCB FOR ALL
*                                      RESPONSES EXCEPT TSO     02S0522
*                                      UNSOLICITED INPUT             */
         LR    @10,IEDQQPTR                                     02S0522
         SL    @10,@CF00044                                     02S0522
         TM    QCBPLCBN(@10),B'10000000'                        02S0522
         BNO   @RF00522                                         02S0522
*     DO;                                                       02S0523
*       RLCB=QCBPLCBA;              /* ESTABLISH PLCB ADDRESSABILITY */
         L     RLCB,QCBPLCBA-1(,@10)                            02S0524
         LA    RLCB,0(,RLCB)                                    02S0524
*       IEDQSPTR=LCBSCBA;           /* ESTABLISH SCB ADDRESSABILITY  */
         L     @10,LCBSCBA-1(,RLCB)                             02S0525
         LA    @10,0(,@10)                                      02S0525
         ST    @10,IEDQSPTR                                     02S0525
*     END;                                                      02S0526
*   BDUPTR=ADDR(PIU0RU);            /* ESTABLISH BDU ADDRESSABILITY  */
@RF00522 LA    BDUPTR,PRFPIUO(,RPRF)                            02S0527
         SLR   @10,@10                                          02S0527
         IC    @10,PRFPIUO(,RPRF)                               02S0527
         ALR   BDUPTR,@10                                       02S0527
         LA    BDUPTR,PIU0RU(,BDUPTR)                           02S0527
*                                                               02S0528
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS: THIS SEGMENT CALLS SUBROUTINES WHICH PERFORM THE     */
*   /* FUNCTION OF THE MODULE.                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0528
*   IF BDUCMND=CD0WRT THEN          /* CALL WRITE PROCESSOR IF THIS
*                                      IS A WRITE                    */
         CLI   BDUCMND(BDUPTR),X'02'                            02S0528
         BNE   @RF00528                                         02S0528
*                                                               03S0529
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS.WRITE: THIS SEGMENT DOES THE WRITE COMMAND          */
*     /* PROCESSING. INTERNAL PROCEDURES ARE CALLED TO PROCESS THESE */
*     /* CONDITIONS: (1)WRITE TERMINATION. (2)SESSION INITIATION AND */
*     /* TERMINATION, (3)RESET IMMEDIATE, (4)MSGGEN, (5)NULL WRITE,  */
*     /* (6)ERROR WRITE, (7)NONERROR WRITE.                          */
*     /*                                                             */
*     /***************************************************************/
*                                                               03S0529
*     DO;                                                       03S0529
*       LCBRESP=OFF;                /* ANY WRITE SATISFIES RESPONSE
*                                      OWED REQUIREMENT              */
         NI    LCBRESP(RLCB),B'11111110'                        03S0530
*       RFY                                                     03S0531
*         RZERO RSTD;               /*                       @YM05678*/
*       TRMPREP=OFF;                /* INDICATE THAT MONITOR IS NO
*                                      LONGER ACTIVE IN THE NCP.
*                                                            @YM05638*/
*       TRMWRBRK=OFF;               /* INDICATE THAT A WRITE BREAK IS
*                                      NOT IN PROGRESS       @YM08156*/
         LA    @15,TRMWRBRK(,RTRM)                              03S0533
         LR    @14,@15                                          03S0533
         LA    @10,3                                            03S0533
         NR    @14,@10                                          03S0533
         LCR   @14,@14                                          03S0533
         ALR   @15,@14                                          03S0533
         L     @10,0(,@15)                                      03S0533
@GL00001 LR    @05,@10                                          03S0533
         N     @05,@CB02345+3(@14)                              03S0533
         CS    @10,@05,0(@15)                                   03S0533
         BNE   @GL00001                                         03S0533
*       RFY                                                     03S0534
*         RZERO UNRSTD;             /*                       @YM05678*/
*       IF TTHTAG2=TTHABORT&        /* IF NULL WRITE TO ABORT   03S0535
*                                                            @YM08030*/
*           TTHTAG1=0 THEN          /* INPUT OPERATION THEN  @YM08038*/
         LA    @10,PRFPIUO(,RPRF)                               03S0535
         SLR   @05,@05                                          03S0535
         IC    @05,PRFPIUO(,RPRF)                               03S0535
         ALR   @10,@05                                          03S0535
         CLI   TTHTAG2(@10),X'04'                               03S0535
         BNE   @RF00535                                         03S0535
         CLI   TTHTAG1(@10),0                                   03S0535
         BNE   @RF00535                                         03S0535
*         DO;                       /*                       @YM08038*/
*           LCBUSUE=ON;             /* INDICATE END OF RCV OPERATION
*                                                            @YM08038*/
         OI    LCBUSUE(RLCB),B'00000001'                        03S0537
*           CALL OUTDHPST;          /* RETURN RESPONSE TO IEDNKA
*                                                            @YM08038*/
         BAL   @14,OUTDHPST                                     03S0538
*         END;                      /*                       @YM08038*/
*       ELSE                        /*                       @YM08038*/
*         DO;                       /*                       @YM08038*/
         B     @RC00535                                         03S0540
@RF00535 DS    0H                                               03S0541
*           IF LCBABRTN=ON THEN     /* IF WRITE IS TO BE TERMINTAED  */
         TM    LCBABRTN(RLCB),B'00000001'                       03S0541
         BNO   @RF00541                                         03S0541
*                                                               04S0542
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS.WRITE.ABORT: WHEN AN ERROR WRITE RESPONSES  */
*             /* IS RECOGNIZED, LCBABRTN IS TURNED ON. A RESET       */
*             /* IMMEDIATE IS BUILT IN THE FIRST ERROR RESPONSE      */
*             /* BUFFER AND POSTED TO THE NCP (IEDPCOUT). THIS RESET */
*             /* IMMEDIATE TURNS OFF THE NCP'S ERROR LOCK AND ON     */
*             /* RETURN IS POSTED TO THE MH AS AN ERROR BUFFER. TO   */
*             /* PREVENT A DOUBLE POST TO MH, THIS SUBROUTINE FREES  */
*             /* ALL WRITE RESPONSE BUFFERS. ABORT PROCESSING IS NOT */
*             /* DONE FOR MSGGEN BUFFERS. LCBABRTN IS ALSO TESTED IN */
*             /* DEBLOCK OUTBOUND (IEDSAO) WRTIE PROCESSING. IF FOUND*/
*             /* ON, WRITE BUFFERS ARE FREED                         */
*             /*                                                     */
*             /*******************************************************/
*                                                               04S0542
*             DO;                                               04S0542
*               IF LCBMSGNN=ON THEN /* THIS IS A MSGGEN RESPONSE     */
         TM    LCBMSGNN(RLCB),B'01000000'                       04S0543
         BNO   @RF00543                                         04S0543
*                 DO;                                           04S0544
*                   LCBMSGNN=OFF;   /* RESET MSGGEN FLAG             */
         NI    LCBMSGNN(RLCB),B'10111111'                       04S0545
*                   CALL ERBBD;     /* POST ERB TO DRIVE IN/OUT MSG
*                                      PROCESSING                    */
         BAL   @14,ERBBD                                        04S0546
*                 END;                                          04S0547
*               CALL FREEBFR;       /* FREE BUFFER                   */
@RF00543 BAL   @14,FREEBFR                                      04S0548
*             END;                                              04S0549
*           ELSE                    /* FOR ALL ELSE DO               */
*             DO;                                               03S0550
         B     @RC00541                                         03S0550
@RF00541 DS    0H                                               03S0551
*               IF BDUMODIF=>CD0WD THEN/* IF COMMAND CAN MODIFY SESSN*/
         CLI   BDUMODIF(BDUPTR),X'03'                           03S0551
         BL    @RF00551                                         03S0551
*                                                               04S0552
*                 /***************************************************/
*                 /*                                                 */
*                 /* PROCESS.WRITE.SESSION: IF A WRITE COMMAND IS    */
*                 /* CAPABLE OF MODIFING SESSION, THE RESPONSE IS    */
*                 /* EXAMINED FOR NORMAL OR ERROR AND PHASE. THE     */
*                 /* SESSION FLAG IS THEN SET AS REQUIRED.           */
*                 /*                                                 */
*                 /***************************************************/
*                                                               04S0552
*                 DO;                                           04S0552
*                   IF BDUPHASE=RSP60 THEN/* IF PHASE IS 3           */
         TM    BDUPHASE(BDUPTR),B'01100000'                     04S0553
         BNO   @RF00553                                         04S0553
*                     DO;                                       04S0554
*                       IF(BDUMODIF=CD0WD×/* IF MODIFIER IS .........*/
*                           BDUMODIF=CD0WCD) THEN/* .....A DISCONNECT*/
         CLI   BDUMODIF(BDUPTR),X'03'                           04S0555
         BE    @RT00555                                         04S0555
         CLI   BDUMODIF(BDUPTR),X'0B'                           04S0555
         BNE   @RF00555                                         04S0555
@RT00555 DS    0H                                               04S0556
*                         TRMSESSN=OFF;/* THEN END SESSION           */
         NI    TRMSESSN(RTRM),B'10111111'                       04S0556
*                       ELSE                                    04S0557
*                         IF BDUMODIF=>CD0WC THEN/* IF          04S0557
*                                      MODIFIER=CONTACT              */
         B     @RC00555                                         04S0557
@RF00555 CLI   BDUMODIF(BDUPTR),X'08'                           04S0557
         BL    @RF00557                                         04S0557
*                           TRMSESSN=ON;/* INITIATE SESSION          */
         OI    TRMSESSN(RTRM),B'01000000'                       04S0558
*                     END;                                      04S0559
*                   ELSE            /* IT IS NOT PHASE 3             */
*                     DO;           /* IT IS PHASE 1 OR 2    @YM08112*/
         B     @RC00553                                         04S0560
@RF00553 DS    0H                                               04S0561
*                       IF(BDUMODIF=>CD0WC&BDUMODIF=<CD0WCD) THEN/*
*                                      IF CONTACT                    */
         CLI   BDUMODIF(BDUPTR),X'08'                           04S0561
         BL    @RF00561                                         04S0561
         CLI   BDUMODIF(BDUPTR),X'0B'                           04S0561
         BH    @RF00561                                         04S0561
*                         IF(BDURESP^=RSPE09&/* AND NOT SESSN^STARTED*/
*                             BDURESP^=RSPE1E) THEN/* AND ^CMD  04S0562
*                                      REJ/RESET                     */
         TM    BDURESP(BDUPTR),B'00001001'                      04S0562
         BNO   @GL00005                                         04S0562
         TM    BDURESP(BDUPTR),B'00010110'                      04S0562
         BZ    @RF00562                                         04S0562
@GL00005 TM    BDURESP(BDUPTR),B'00011110'                      04S0562
         BNO   @RT00562                                         04S0562
         TM    BDURESP(BDUPTR),B'00000001'                      04S0562
         BZ    @RF00562                                         04S0562
@RT00562 DS    0H                                               04S0563
*                           TRMSESSN=ON;/* INDICATE SESSION          */
         OI    TRMSESSN(RTRM),B'01000000'                       04S0563
*                     END;                                      04S0564
@RF00562 DS    0H                                               04S0564
@RF00561 DS    0H                                               04S0565
*                 END;                                          04S0565
@RC00553 DS    0H                                               04S0566
*               IF TRMASNXI=ON THEN /* IF RESET IMMEDIATE ISSUED     */
@RF00551 TM    TRMASNXI(RTRM),B'00010000'                       03S0566
         BNO   @RF00566                                         03S0566
*                                                               04S0567
*                 /***************************************************/
*                 /*                                                 */
*                 /* PROCESS.WRITE.RESETIMM: PROCESS STOPLINE,I      */
*                 /* DURING WRITE COMMAND. IF THIS IS MSGGEN, TURN   */
*                 /* OFF MSGGEN INDICATOR, FREE BUFFER AND POST ERB  */
*                 /* TO BD. IF THIS IS A NULL WRITE TX, TURN OFF NULL*/
*                 /* WRITE INDICATOR, FREE BUFFER AND POST PLCB TO   */
*                 /* BD. IF THIS IS A NULL WRITE BLOCK, POST BUFFER  */
*                 /* TO OUTDH. (OUTDH WILL TEST FOR STOP LINE IN     */
*                 /* PROGRESS BEFORE ATTEMPTING TO CONTINUE THE      */
*                 /* SEND). IF THIS IS WRITE DATA, INDICATE WRITE    */
*                 /* ABORT, DO WRITE STATUS ANALYSIS, TRANSFORM THIS */
*                 /* RESPONSE TO A TAGGED RESET IMMEDIATE AND POST TO*/
*                 /* PATH CONTROL OUTBOUND. THE PURPOSE OF THIS      */
*                 /* TAGGED RESET IS TO RETURN ANY OUTSTANDING       */
*                 /* COMMANDS. THE TAGGED RESET RESPONSE WILL BE     */
*                 /* ERROR POSTED TO THE MH.                         */
*                 /*                                                 */
*                 /***************************************************/
*                                                               04S0567
*                 DO;                                           04S0567
*                   IF LCBMSGNN=ON THEN/* IF THIS IS A MSGGEN        */
         TM    LCBMSGNN(RLCB),B'01000000'                       04S0568
         BNO   @RF00568                                         04S0568
*                     DO;                                       04S0569
*                       LCBMSGNN=OFF;                           04S0570
         NI    LCBMSGNN(RLCB),B'10111111'                       04S0570
*                       CALL FREEBFR;                           04S0571
         BAL   @14,FREEBFR                                      04S0571
*                       CALL ERBBD; /* POST ERB TO BD                */
         BAL   @14,ERBBD                                        04S0572
*                     END;                                      04S0573
*                   ELSE                                        04S0574
*                     IF TRMNLOUT=OFF THEN/* IF IT IS A DATA WRITE   */
         B     @RC00568                                         04S0574
@RF00568 TM    TRMNLOUT(RTRM),B'00000100'                       04S0574
         BNZ   @RF00574                                         04S0574
*                       CALL ERRWRITE;/* THIS SUBROUTINE WILL ABORT
*                                      THE WRITE, SET SCB AND LCB
*                                      ERROR BITS AND PRESERVE STATUS
*                                      OF THE ERROR FOR THE TCAM
*                                      CONSOLE ERROR MESSAGE         */
         BAL   @14,ERRWRITE                                     04S0575
*                     ELSE                                      04S0576
*                       IF BDUMODIF=CD0WT THEN/* IF THIS IS A NULL WR
*                                      EOT                           */
         B     @RC00574                                         04S0576
@RF00574 CLI   BDUMODIF(BDUPTR),X'02'                           04S0576
         BNE   @RF00576                                         04S0576
*                         DO;                                   04S0577
*                           TRMNLOUT=OFF;/* TURN OFF NULL OUT FLAG   */
         NI    TRMNLOUT(RTRM),B'11111011'                       04S0578
*                           CALL FREEBFR;/* FREE PIU BFR     @YM07714*/
         BAL   @14,FREEBFR                                      04S0579
*                           CALL PLCBPOST;/* POST PLCB TO BD         */
         BAL   @14,PLCBPOST                                     04S0580
*                         END;                                  04S0581
*                       ELSE        /* ELSE, IT IS A NULL WR BLK     */
*                         CALL OUTDHPST;/* WHICH SHOULD BE RETURNED
*                                      TO OUTDH WHERE THIS WRITE WILL
*                                      BE TERMINATED                 */
         B     @RC00576                                         04S0582
@RF00576 BAL   @14,OUTDHPST                                     04S0582
*                 END;                                          04S0583
*               ELSE                                            03S0584
*                 IF LCBMSGNN=ON THEN/* IF MSGGEN IN PROGRESS        */
         B     @RC00566                                         03S0584
@RF00566 TM    LCBMSGNN(RLCB),B'01000000'                       03S0584
         BNO   @RF00584                                         03S0584
*                                                               04S0585
*                   /*************************************************/
*                   /*                                               */
*                   /* PROCESS.WRITE.MSGGEN: IF INPUT IS MARKED BSC  */
*                   /* STATUS PENDING, THEN SET STATUS PENDING FLAG, */
*                   /* FREE THE BUFFER AND EXIT TO DISPATCHER. ON THE*/
*                   /* NEXT INPUT FROM SAME SOURCE, IEDSAI WILL POST */
*                   /* ERB TO BD TO DRIVE OUTMSG. THIS PROCESS       */
*                   /* PREVENTS A RESET IMMEDIATE FROM RESETTING THE */
*                   /* PENDING STATUS. IN NORMAL AND ERROR CASES     */
*                   /* OTHER THAN BSC STATUS PENDING AND TSO SIM ATTN*/
*                   /* RD, THE ERB IS POSTED TO IEDQBD. IF INPUT IS  */
*                   /* TSO SIM ATTN RD, THE BUFFER IS FREED AND THE  */
*                   /* THE ERB IS POSTED TO IEDQBD                   */
*                   /*                                               */
*                   /*************************************************/
*                                                               04S0585
*                   DO;                                         04S0585
*                     IF BDUSYRSP=RSPSTPND THEN/* IF BSC STATUS 04S0586
*                                      PENDING                       */
         CLI   BDUSYRSP(BDUPTR),X'A5'                           04S0586
         BNE   @RF00586                                         04S0586
*                       DO;                                     04S0587
*                         TRMSTPND=ON;/* INDICATE STAT PENDING       */
         OI    TRMSTPND(RTRM),B'10000000'                       04S0588
*                         CALL FREEBFR;                         04S0589
         BAL   @14,FREEBFR                                      04S0589
*                       END;                                    04S0590
*                     ELSE                                      04S0591
*                       DO;         /*                       @OY20320*/
         B     @RC00586                                         04S0591
@RF00586 DS    0H                                               04S0592
*                         LCBMSGNN=OFF;/* MSGGEN NOT OUT     @OZ27777*/
         NI    LCBMSGNN(RLCB),B'10111111'                       04S0592
*                         IF QCBTSSES=ON THEN/* FOR TSO SESSIONS
*                                                            @YM08420*/
         TM    QCBTSSES(IEDQQPTR),B'10000000'                   04S0593
         BNO   @RF00593                                         04S0593
*                           IF^(QCBSIMRD=ON&/* IF NOT TSO SIM ATTN RD
*                                      RESP IN ERR                   */
*                               BDUERROR=ON) THEN               04S0594
         TM    QCBSIMRD(IEDQQPTR),B'00001000'                   04S0594
         BNO   @RT00594                                         04S0594
         TM    BDUERROR(BDUPTR),B'10000000'                     04S0594
         BO    @RF00594                                         04S0594
@RT00594 DS    0H                                               04S0595
*                             DO;                               04S0595
*                               IF LCBSATRD=ON THEN/* IS THIS A SIM
*                                      ATTN READ REQ?                */
         TM    LCBSATRD(RLCB),B'00100000'                       04S0596
         BNO   @RF00596                                         04S0596
*                                                               05S0597
*                                 /***********************************/
*                                 /*                                 */
*                                 /* PROCESS.WRITE.MSGGEN.ERBAYM:    */
*                                 /* POST ERB TO AYM                 */
*                                 /*                                 */
*                                 /***********************************/
*                                                               05S0597
*                                 DO;                           05S0597
*                                   LCBERBPY=PRILNEND;/* PRIORITY
*                                      'E4'                          */
         MVI   LCBERBPY(RLCB),X'E4'                             05S0598
*                                   LCBERBQB=AVTTSOPT->TSIMSGEN;/*
*                                      AYM'S QCB ADDRESS             */
         L     @10,AVTTSOPT(,IEDQAPTR)                          05S0599
         MVC   LCBERBQB(3,RLCB),TSIMSGEN+1(@10)                 05S0599
*                                   RPARM=ADDR(LCBERB);/* POSTED
*                                      ELEMENT IS ERB                */
         LA    RPARM,LCBERB(,RLCB)                              05S0600
*                                   CALL DSPPOSTR;              05S0601
         LA    @15,DSPPOSTR(,RDISP)                             05S0601
         BALR  @14,@15                                          05S0601
*                                 END;                          05S0602
*                               ELSE                            04S0603
*                                 CALL ERBBD;                   04S0603
         B     @RC00596                                         04S0603
@RF00596 BAL   @14,ERBBD                                        04S0603
*                               CALL FREEBFR;                   04S0604
@RC00596 BAL   @14,FREEBFR                                      04S0604
*                             END;                              04S0605
*                           ELSE                                04S0606
*                             CALL NCPERP;/* DO TSO SECONDARY ERROR
*                                      RECOVERY PROCESSING           */
         B     @RC00594                                         04S0606
@RF00594 BAL   @14,NCPERP                                       04S0606
*                         ELSE      /*                       @YM08470*/
*                           DO;     /*                       @YM08420*/
         B     @RC00593                                         04S0607
@RF00593 DS    0H                                               04S0608
*                             CALL ERBBD;/*                  @YM08420*/
         BAL   @14,ERBBD                                        04S0608
*                             CALL FREEBFR;/*                @YM08420*/
         BAL   @14,FREEBFR                                      04S0609
*                           END;    /*                       @YM08420*/
*                       END;                                    04S0611
*                   END;            /*                       @OY20320*/
*                 ELSE                                          03S0613
*                   IF TRMNLOUT=ON THEN/* IF NULL WRITE OUT          */
         B     @RC00584                                         03S0613
@RF00584 TM    TRMNLOUT(RTRM),B'00000100'                       03S0613
         BNO   @RF00613                                         03S0613
*                                                               04S0614
*                     /***********************************************/
*                     /*                                             */
*                     /* PROCESS.WRITE.NULLWRIT: IF BSC STATUS IS    */
*                     /* PENDING, BUILD A RESET IMMEDIATE IN THIS    */
*                     /* BUFFER AND POST IT TO NCP WHEN THE STATUS   */
*                     /* COMES IN. IF INPUT IS A WRITE TX AND ITS    */
*                     /* TAGGED, KILL THE SEND AND POST TO OUTDH. IF */
*                     /* IT IS A WRITE TX AND ITS NOT TAGGED, FREE   */
*                     /* BUFFER AND POST PLCB TO IEDQBD. IF INPUT IS */
*                     /* A NULL WRITE BLOCK WITHOUT ERROR, POST TO   */
*                     /* OUTDH. IF INPUT IS A NULL WRITE BLOCK       */
*                     /* WITHOUT ERROR BUT SOMETHING IS ON WAIT      */
*                     /* CHAIN, BUILD A TAGGED NULL WR EOT IN THIS   */
*                     /* BUFFER AND POST TO PCOUT. IF INPUT IS A NULL*/
*                     /* WR BLK IN ERROR, KILL THE SEND AND POST     */
*                     /* BUFFER TO OUTDH.                            */
*                     /*                                             */
*                     /***********************************************/
*                                                               04S0614
*                     DO;                                       04S0614
*                       IF BDUSYRSP=RSPSTPND THEN/* IF THIS IS BSC
*                                      STATUS PENDING                */
         CLI   BDUSYRSP(BDUPTR),X'A5'                           04S0615
         BNE   @RF00615                                         04S0615
*                         DO;                                   04S0616
*                           LCBABRTN=ON;/* TERMINATE THE WRITE       */
         OI    LCBABRTN(RLCB),B'00000001'                       04S0617
*                           IF BDUMODIF=CD0WT THEN              04S0618
         CLI   BDUMODIF(BDUPTR),X'02'                           04S0618
         BNE   @RF00618                                         04S0618
*                             LCBRESP=ON;/* IF NULL WR TX, SET  04S0619
*                                                            @YM08064
*                                      FLAG FOR REDRIVE      @YM08064
*                                      WHEN RESET RCVD       @YM08064*/
         OI    LCBRESP(RLCB),B'00000001'                        04S0619
*                           CALL ERRORBIT;/* SET SCB/LCB ERROR BITS  */
@RF00618 BAL   @14,ERRORBIT                                     04S0620
*                           CALL BLDRESET;/* BUILD A TAGGED RESET IN
*                                      THIS BUFFER                   */
         BAL   @14,BLDRESET                                     04S0621
*                           LCBBFRW=RPRF;/* HOLD THIS BUFFER UNTIL
*                                      THE BSC STATUS COMES IN AND
*                                      THEN USE IT TO RESET NCP ERR
*                                      LOCK. IEDSAI WILL POST THIS
*                                      BUFFER TO PATH CNTRL OUT WHEN
*                                      THE STATUS COMES IN           */
         STCM  RPRF,7,LCBBFRW(RLCB)                             04S0622
*                         END;                                  04S0623
*                       ELSE                                    04S0624
*                         IF BDUMODIF=CD0WT THEN/* IF THIS IS NULL WR
*                                      TX                            */
         B     @RC00615                                         04S0624
@RF00615 CLI   BDUMODIF(BDUPTR),X'02'                           04S0624
         BNE   @RF00624                                         04S0624
*                           DO;                                 04S0625
*                             IF TTHTAG2=TTHNULWR&/* AND IT IS TAGGED*/
*                                 TTHTAG1=0 THEN                04S0626
         LA    @10,PRFPIUO(,RPRF)                               04S0626
         SLR   @05,@05                                          04S0626
         IC    @05,PRFPIUO(,RPRF)                               04S0626
         ALR   @10,@05                                          04S0626
         CLI   TTHTAG2(@10),X'01'                               04S0626
