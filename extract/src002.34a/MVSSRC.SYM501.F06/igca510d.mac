         TITLE 'IGCA510D, ACTIVATE PHYSICAL UNIT (NON-NCP)             *
                        '
IGCA510D CSECT ,                                                   0001
@PROLOG  DS    0H                                                  0002
*                                                                  0474
         USING  *,RBASE
         LR     RBASE,RF                 /* SET UP ADDRESSIBILITY    */
IGCA510D IEDHJN START,HJN
*/*                                                                  */
*                                                                  0475
*                                   /* INITIALIZATION                */
*   ROPCAVT=RPARM;                  /* OP CONTROL AVT ADDRESS        */
         LR    ROPCAVT,RPARM                                       0475
*   RELEMENT=OPCCOPCE;              /* CURRENT ELEMENT ADDRESS       */
         L     RELEMENT,OPCCOPCE(,ROPCAVT)                         0476
*   RAVT=OPCAVTPT;                  /* TCAM AVT ADDRESS              */
         L     RAVT,OPCAVTPT(,ROPCAVT)                             0477
*   RWORK=OPCWORK;                  /* OP CONTROL WORK AREA          */
         L     RWORK,OPCWORK(,ROPCAVT)                             0478
*   RSAVE=ADDR(OPCSAVE);            /* OP CONTROL SAVE AREA          */
         LA    RSAVE,OPCSAVE(,ROPCAVT)                             0479
*/*                                                                  */
*                                                                  0480
*   IF RELEMENT->PRFPRI=PRIHIFLG THEN/* ELEMENT AN OPCE              */
         CLI   PRFPRI(RELEMENT),X'E9'                              0480
         BNE   @RF00480                                            0480
*     DO;                           /* YES... PROCESS                */
*       RPARM=OCTRMTBL;             /* TNT FOR RESOURCE              */
         LH    RPARM,OCTRMTBL(,ROPCE)                              0482
         N     RPARM,@CF01760                                      0482
*       CALL IEDQTNT;               /* CONVERT TNT TO TTE            */
         L     @15,AVTRNMPT(,RAVT)                                 0483
         BALR  @14,@15                                             0483
*       RTRM=RPARM;                 /* SAVE TTE ADDRESS              */
         LR    RTRM,RPARM                                          0484
*       IF OCRESP=OFF THEN          /* REQUEST FOR ACTPU             */
         TM    OCRESP(ROPCE),B'00000001'                           0485
         BNZ   @RF00485                                            0485
*         CALL ACTPURQ;             /* START PROCESS NECESSARY FOR 0486
*                                      ACTIVATE PU                   */
         BAL   @14,ACTPURQ                                         0486
*       ELSE                                                       0487
*         DO;                                                      0487
         B     @RC00485                                            0487
@RF00485 DS    0H                                                  0488
*           WKCMDADR=ADDR(PRUDATA); /* SET UP PTR TO COMMAND         */
         L     @09,OCUNIT(,ROPCE)                                  0488
         LA    @11,PRUDATA(,@09)                                   0488
         ST    @11,WKCMDADR(,RWORK)                                0488
*           IF PRURHSDI=ON THEN     /* SENSE INCLUDED?               */
         AL    @09,@CF01761                                        0489
         TM    PRURHSDI(@09),B'00000100'                           0489
         BNO   @RF00489                                            0489
*             WKCMDADR=WKCMDADR+4;  /* BUMP COMMAND POINTER          */
         AL    @11,@CF00327                                        0490
         ST    @11,WKCMDADR(,RWORK)                                0490
*           IF COMMAND=CD1XCON THEN /* RESPONSE TO CONTACT           */
@RF00489 L     @09,WKCMDADR(,RWORK)                                0491
         CLC   COMMAND(3,@09),@CB00720                             0491
         BNE   @RF00491                                            0491
*             CALL CONTCTRP;        /* PROCESS CONTACT RESPONSE      */
         BAL   @14,CONTCTRP                                        0492
*           ELSE                                                   0493
*             IF COMMAND(1:1)=CD1ACTPU THEN/* RESPONSE TO ACTPU      */
         B     @RC00491                                            0493
@RF00491 L     @09,WKCMDADR(,RWORK)                                0493
         CLI   COMMAND(@09),X'11'                                  0493
         BNE   @RF00493                                            0493
*               CALL ACTPURP;       /* PROCESS ACTPU RESPONSE        */
         BAL   @14,ACTPURP                                         0494
*             ELSE                                                 0495
*               CALL DISCONRP;      /* PROCESS DISCONTACT RESP       */
         B     @RC00493                                            0495
@RF00493 BAL   @14,DISCONRP                                        0495
*         END;                                                     0496
*     END;                                                         0497
*   ELSE                            /* CONTACTED REQUEST             */
*     CALL CONTCTRQ;                /* PROCESS CONTACTED             */
         B     @RC00480                                            0498
@RF00480 BAL   @14,CONTCTRQ                                        0498
*/*                                                                  */
*                                                                  0499
*   RPARM=ROPCAVT;                  /* RPARM HAS OP CNTRL AVT        */
@RC00480 LR    RPARM,ROPCAVT                                       0499
*   GEN REFS(ROPCAVT,OPCXCTL) EXIT;                                0500
         XCTL  SF=(E,OPCXCTL(ROPCAVT))
*ACTPURP:                                                          0501
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0501
*                                                                  0501
@EL00001 DS    0H                                                  0501
@EF00001 DS    0H                                                  0501
@ER00001 BR    @14                                                 0501
ACTPURP  DS    0H                                                  0502
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE PROCESSES RESPONSES TO ACTPU. FOR A GOOD RESPONSE*/
*   /* IT SETS A GOOD RETURN CODE IN OCPARM AND RETURNS THE RESPONSE */
*   /* TO THE ORIGINAL REQUESTOR (OP SERVICES). FOR A BAD RESPONSE IT*/
*   /* FREES THE SIB AND SENDS A DISCONTACT.                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0502
*   SAVE1=RE;                       /* SAVE REG 14                   */
         ST    RE,SAVE1(,RWORK)                                    0502
*/*                                                                  */
*                                                                  0503
*   IF PRURHSDI=OFF×SNSMAJ=SNSRQREJ&SNSMIN=SNSACTV THEN/* GOOD RESP? */
         L     @09,OCUNIT(,ROPCE)                                  0503
         LR    @11,@09                                             0503
         AL    @11,@CF01761                                        0503
         TM    PRURHSDI(@11),B'00000100'                           0503
         BZ    @RT00503                                            0503
         AL    @09,@CF00235                                        0503
         CLI   SNSMAJ(@09),X'08'                                   0503
         BNE   @RF00503                                            0503
         CLI   SNSMIN(@09),X'15'                                   0503
         BNE   @RF00503                                            0503
@RT00503 DS    0H                                                  0504
*     DO;                                                          0504
*       IF PRURHSDI=ON THEN         /* SENSE INCLUDED ?              */
         L     @09,OCUNIT(,ROPCE)                                  0505
         AL    @09,@CF01761                                        0505
         TM    PRURHSDI(@09),B'00000100'                           0505
         BNO   @RF00505                                            0505
*         OCPARM=OCRCGNEG;          /* SET GOOD RETURN CODE          */
         MVC   OCPARM(2,ROPCE),@CH00127                            0506
*       ELSE                                                       0507
*         OCPARM=OCRCGPOS;          /* SET GOOD RETURN CODE          */
         B     @RC00505                                            0507
@RF00505 MVC   OCPARM(2,ROPCE),@CH00060                            0507
*       OPCMODID=OCWTGB;            /* RETURN TO REQUESTOR           */
@RC00505 MVC   OPCMODID(2,ROPCAVT),OCWTGB(ROPCE)                   0508
*       OCWTG=0;                    /* CLEAR 'WHERE TO GO'           */
         SLR   @09,@09                                             0509
         STH   @09,OCWTG(,ROPCE)                                   0509
*     END;                                                         0510
*                                                                  0510
*/*                                                                  */
*                                                                  0511
*   ELSE                                                           0511
*     DO;                           /* BAD RESPONSE TO ACTPU         */
         B     @RC00503                                            0511
@RF00503 DS    0H                                                  0512
*       REQTYPE=REQSFREE;           /* PREPARE TO FREE SIB           */
         MVI   REQTYPE+12(RWORK),X'04'                             0512
*       CALL SIBRTN;                /* FREE THE SIB                  */
         BAL   @14,SIBRTN                                          0513
*       OCPARM=OCRCBPOS;            /* SET BAD RETURN CODE           */
         MVC   OCPARM(2,ROPCE),@CH01130                            0514
*       OCRESP=OFF;                 /* TURN OFF OCRESP               */
         NI    OCRESP(ROPCE),B'11111110'                           0515
*       CALL DISCONRQ;              /* SEND DISCONTACT               */
         BAL   @14,DISCONRQ                                        0516
*       OPCMODID=MODID02;           /* XCTL TO IGG0210D              */
         MVC   OPCMODID(2,ROPCAVT),@CC01716                        0517
*       OCWTG=MODIDA5;              /* RETURN TO THIS MODULE         */
         MVC   OCWTG(2,ROPCE),@CC01714                             0518
*     END;                                                         0519
*                                                                  0519
*/*                                                                  */
*                                                                  0520
*   RE=SAVE1;                       /* RESTORE REG 14                */
@RC00503 L     RE,SAVE1(,RWORK)                                    0520
*   END ACTPURP;                    /* RETURN                        */
@EL00002 DS    0H                                                  0521
@EF00002 DS    0H                                                  0521
@ER00002 BR    @14                                                 0521
*ACTPURQ:                                                          0522
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0522
*                                                                  0522
ACTPURQ  DS    0H                                                  0523
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE HANDLES A REQUEST FROM OP SERVICES TO ACTIVATE A */
*   /* PHYSICAL UNIT. THIS ROUTINE BUILDS A CONTACT COMMAND AND      */
*   /* PREPARES TO SENT OUT THE COMMAND VIA IGC0210D.                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0523
*   SAVE1=RE;                       /* SAVE REG 14                   */
         ST    RE,SAVE1(,RWORK)                                    0523
*   RPARM=1;                        /* SET BUFFER UNIT TO 1          */
         LA    RPARM,1                                             0524
*   CALL ALLOCBUF;                  /* GET A BUFFER                  */
         L     @15,OPCGETBF(,ROPCAVT)                              0525
         BALR  @14,@15                                             0525
*   OCUNIT=RPARM;                   /* SAVE UNIT ADDRESS             */
         ST    RPARM,OCUNIT(,ROPCE)                                0526
*   WKCMDADR=ADDR(PRUDATA);         /* POINTER TO DATA AREA          */
         L     @09,OCUNIT(,ROPCE)                                  0527
         LA    @11,PRUDATA(,@09)                                   0527
         ST    @11,WKCMDADR(,RWORK)                                0527
*   OCWTGB=OCWTG;                   /* SAVE "WHERE TO GO"            */
         MVC   OCWTGB(2,ROPCE),OCWTG(ROPCE)                        0528
*/*                                                                  */
*                                                                  0529
*   PRURHTH=RHTHFMD;                /* INITIALIZE RH AND TH          */
         LR    @10,@09                                             0529
         AL    @10,@CF01761                                        0529
         MVC   PRURHTH(4,@10),@CB01709                             0529
*   PRUDATCT=LENGTH(IEDXCOND);      /* SET UP DATA COUNT FIELD       */
         MVC   PRUDATCT(2,@09),@CH00153                            0530
*   XCONCODE=CD1XCON;               /* CONTACT COMMAND               */
         MVC   XCONCODE(3,@11),@CB00720                            0531
*   RPARM=OCTRMTBL;                 /* RESOURCE TNT                  */
         LH    RPARM,OCTRMTBL(,ROPCE)                              0532
         N     RPARM,@CF01760                                      0532
*   CALL IEDIAP04;                  /* CONVERT TNT TO NET ADDR       */
         L     @09,AVTSAVTP(,RAVT)                                 0533
         L     @15,SAVTTNTX(,@09)                                  0533
         BALR  @14,@15                                             0533
*   XCONADDR=RF;                    /* PUT NET ADDR IN RU            */
         L     @09,WKCMDADR(,RWORK)                                0534
         STCM  RF,3,XCONADDR(@09)                                  0534
*   OCPUADR=RF;                     /* SAVE NETWORK ADDRESS          */
         STH   RF,OCPUADR(,ROPCE)                                  0535
*/*                                                                  */
*                                                                  0536
*   TRMPCONT=TRMCCSNT;              /* SET CONTACT SENT STATE        */
         LR    @09,RTRM                                            0536
         AL    @09,@CF01763                                        0536
         NI    TRMPCONT-1(@09),B'11111001'                         0536
         OI    TRMPCONT-1(@09),B'00000001'                         0536
*   WKTRMPTR=RTRM;                  /* SAVE PU'S TTE ADDRESS         */
         ST    RTRM,WKTRMPTR(,RWORK)                               0537
*   RPARM=RTRM;                     /* TERM TABLE ADDR OF PU         */
         LR    RPARM,RTRM                                          0538
*   DO UNTIL TRMTYPE=TRMLNCP;       /* LOCAL NCP?... NO              */
@DL00539 DS    0H                                                  0540
*     CALL COHORTLK;                /* GET SUPERIOR RESOURCE         */
         L     @15,OPCHORT(,ROPCAVT)                               0540
         BALR  @14,@15                                             0540
*     RTRM=RPARM;                   /* NEW TERM TABLE ADDRESS        */
         LR    RTRM,RPARM                                          0541
*   END;                                                           0542
@DE00539 LR    @09,RTRM                                            0542
         AL    @09,@CF01130                                        0542
         CLI   TRMTYPE-11(@09),X'0F'                               0542
         BNE   @DL00539                                            0542
*   RTRM=WKTRMPTR;                  /* RESTORE PU'S TTE ADDR         */
         L     RTRM,WKTRMPTR(,RWORK)                               0543
*   PRUTTCIN=RZERO;                 /* SET UP DESTINATION IN RU      */
         L     @09,OCUNIT(,ROPCE)                                  0544
         STH   RZERO,PRUTTCIN(,@09)                                0544
*   OCNCP=RZERO;                    /* SAVE LOCAL NCP TNT            */
         STH   RZERO,OCNCP(,ROPCE)                                 0545
*/*                                                                  */
*                                                                  0546
*   OPCMODID=MODID02;               /* XCTL TO IGC0210D              */
         MVC   OPCMODID(2,ROPCAVT),@CC01716                        0546
*   OCWTG=MODIDA5;                  /* SET UP RETURN TO 'A5'         */
         MVC   OCWTG(2,ROPCE),@CC01714                             0547
*/*                                                                  */
*                                                                  0548
*   RE=SAVE1;                       /* RESTORE REG 14                */
         L     RE,SAVE1(,RWORK)                                    0548
*   END ACTPURQ;                    /* RETURN                        */
@EL00003 DS    0H                                                  0549
@EF00003 DS    0H                                                  0549
@ER00003 BR    @14                                                 0549
*CNTDPROC:                                                         0550
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0550
*                                                                  0550
CNTDPROC DS    0H                                                  0551
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE CONTINUES PROCESSING FOR A CONTACTED REQUEST. THE*/
*   /* STATUS OF THE CONTACTED IS CHECKED. IF GOOD AN ACTIVATE       */
*   /* PHYSICAL UNIT REQUEST IS BUILT . IF BAD THE CONTACT STATE IS  */
*   /* SET TO RESET. A DISCONTACT COMMAND IS NOT NEEDED BECAUSE THE  */
*   /* NCP HAS ALREADY CLEANED UP THE PHYSICAL UNITS CONTACT STATES. */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0551
*   SAVE2=RE;                       /* SAVE REG 14                   */
         ST    RE,SAVE2(,RWORK)                                    0551
*   RELEMENT=WKCURRNT;              /* SAVE OPCE ADDRESS             */
         L     RELEMENT,WKCURRNT(,RWORK)                           0552
*   OPCCOPCE=RELEMENT;              /* SET UP CURRENT ELEMENT      0553
*                                                            @YM03743*/
         ST    RELEMENT,OPCCOPCE(,ROPCAVT)                         0553
*   OCUNIT=WKBUFADR;                /* SAVE BUFFER ADDRESS           */
         L     @09,WKBUFADR(,RWORK)                                0554
         ST    @09,OCUNIT(,ROPCE)                                  0554
*   RPARM=OCTRMTBL;                 /* TNT OF RESOURCE               */
         LH    RPARM,OCTRMTBL(,ROPCE)                              0555
         N     RPARM,@CF01760                                      0555
*   CALL IEDQTNT;                   /* CONVERT TNT TO TTE ADDR       */
         L     @15,AVTRNMPT(,RAVT)                                 0556
         BALR  @14,@15                                             0556
*   RTRM=RPARM;                     /* SAVE TTE ADDRESS              */
         LR    RTRM,RPARM                                          0557
*/*                                                                  */
*                                                                  0558
*   IF XCTDSTAT=STATGD THEN         /* GOOD STATUS IN CONTACTED      */
         L     @09,WKCMDADR(,RWORK)                                0558
         CLI   XCTDSTAT(@09),1                                     0558
         BNE   @RF00558                                            0558
*     DO;                                                          0559
*       REQTYPE=REQSGET;            /* SET UP TO GET SIB             */
         MVI   REQTYPE+12(RWORK),X'03'                             0560
*       CALL SIBRTN;                /* TRY TO GET A SIB              */
         BAL   @14,SIBRTN                                          0561
*/*                                                                  */
*                                                                  0562
*       IF REQRCD1=0 THEN           /* SUCCESSFUL? ... YES           */
         LA    @09,12                                              0562
         LR    @11,RWORK                                           0562
         ALR   @11,@09                                             0562
         CLI   REQRCD1(@11),0                                      0562
         BNE   @RF00562                                            0562
*         DO;                                                      0563
*           TRMPCONT=TRMCACTV;      /* SET CONTACT STATE ACTIVE      */
         LR    @11,RTRM                                            0564
         AL    @11,@CF01763                                        0564
         NI    TRMPCONT-1(@11),B'11111100'                         0564
         OI    TRMPCONT-1(@11),B'00000100'                         0564
*           PRURHTH=RHTHSC;         /* INITIALIZE RH AND TH          */
         L     @11,OCUNIT(,ROPCE)                                  0565
         LR    @10,@11                                             0565
         AL    @10,@CF01761                                        0565
         MVC   PRURHTH(4,@10),@CB01711                             0565
*           PRUTTCIN=OCTRMTBL;      /* SET UP DESTINATION            */
         LH    @10,OCTRMTBL(,ROPCE)                                0566
         N     @10,@CF01760                                        0566
         STH   @10,PRUTTCIN(,@11)                                  0566
*           PRUDATCT=LENGTH(IEDXAPUD);/* SET UP DATA COUNT FIELD     */
         MVC   PRUDATCT(2,@11),@CH01628                            0567
*           XAPUCODE=CD1ACTPU;      /* SET COMMAND CODE              */
         ALR   @11,@09                                             0568
         MVI   XAPUCODE(@11),X'11'                                 0568
*           XAPUTYPE=TYPECOLD;      /* SET TYPE ACTIVATE             */
         MVI   XAPUTYPE(@11),X'01'                                 0569
*           XAPUPROF=PUPROF;        /* SET FM AND TS PROFILES        */
         MVI   XAPUPROF(@11),X'01'                                 0570
*           XAPUSSCP=SSCPID;        /* SET SSCP IDENTIFICATION       */
         MVC   XAPUSSCP(6,@11),@CB01723                            0571
*         END;                                                     0572
*                                                                  0572
*/*                                                                  */
*                                                                  0573
*       ELSE                        /* CAN'T GET A SIB               */
*         DO;                                                      0573
         B     @RC00562                                            0573
@RF00562 DS    0H                                                  0574
*           CALL DISCONRQ;          /* BUILD DISCONTACT              */
         BAL   @14,DISCONRQ                                        0574
*           OCPARM=OCRCBSIB;        /* SET BAD RETURN CODE           */
         MVC   OCPARM(2,ROPCE),@CH01128                            0575
*         END;                                                     0576
*                                                                  0576
*/*                                                                  */
*                                                                  0577
*                                   /* PREPARE TO EXIT               */
*       OPCMODID=MODID02;           /* SET UP TO XCTL TO "02"        */
@RC00562 MVC   OPCMODID(2,ROPCAVT),@CC01716                        0577
*       OCWTG=MODIDA5;              /* SET UP RETURN TO "A5"         */
         MVC   OCWTG(2,ROPCE),@CC01714                             0578
*     END;                                                         0579
*                                                                  0579
*/*                                                                  */
*                                                                  0580
*   ELSE                            /* BAD STATUS IN CONTACTED       */
*     DO;                                                          0580
         B     @RC00558                                            0580
@RF00558 DS    0H                                                  0581
*       CALL DISCONRP;              /* HANDLE AS IF A DISCONTACT   0581
*                                      RESPONSE WAS RECEIVED         */
         BAL   @14,DISCONRP                                        0581
*       OCPARM=OCRCBNEG;            /* SET BAD RETURN CODE           */
         MVC   OCPARM(2,ROPCE),@CH01126                            0582
*     END;                                                         0583
*                                                                  0583
*/*                                                                  */
*                                                                  0584
*   RE=SAVE2;                       /* RESTORE REG 14                */
@RC00558 L     RE,SAVE2(,RWORK)                                    0584
*   END CNTDPROC;                                                  0585
@EL00004 DS    0H                                                  0585
@EF00004 DS    0H                                                  0585
@ER00004 BR    @14                                                 0585
*CONTCTRP:                                                         0586
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0586
*                                                                  0586
CONTCTRP DS    0H                                                  0587
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE PROCESSES A RESPONSE TO CONTACT. WHEN A GOOD     */
*   /* RESPONSE IS RECEIVED THIS ROUTINE FREES THE RESPONSE BUFFER , */
*   /* SETS APPROPRIATE CONTACT STATE AND ENQUEUES THE OPCE. WHEN A  */
*   /* BAD RESPONSE IS RECEIVED THIS ROUTINE SETS A BAD RETURN CODE  */
*   /* IN OCPARM, SETS THE APPROPRIATE CONTACT STATE AND RETURNS TO  */
*   /* THE ORIGINAL REQUESTOR(OP SERVICES).                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0587
*   SAVE1=RE;                       /* SAVE REG 14                   */
         ST    RE,SAVE1(,RWORK)                                    0587
*/*                                                                  */
*                                                                  0588
*   IF PRURHSDI=OFF THEN            /* GOOD RESPONSE                 */
         L     @09,OCUNIT(,ROPCE)                                  0588
         LR    @11,@09                                             0588
         AL    @11,@CF01761                                        0588
         TM    PRURHSDI(@11),B'00000100'                           0588
         BNZ   @RF00588                                            0588
*     DO;                                                          0589
*       TRMPCONT=TRMCAPND;          /* SET CONTACT ACT PEND          */
         LR    @11,RTRM                                            0590
         AL    @11,@CF01763                                        0590
         NI    TRMPCONT-1(@11),B'11111010'                         0590
         OI    TRMPCONT-1(@11),B'00000010'                         0590
*       OCRESP=OFF;                 /* TURN OFF OCRESP               */
         NI    OCRESP(ROPCE),B'11111110'                           0591
*       RPARM=OCUNIT;               /* ADDR OF UNIT TO BE FREED      */
         LR    RPARM,@09                                           0592
*       CALL FREEBUF;               /* FREE THE BUFFER               */
         L     @15,OPCFREBF(,ROPCAVT)                              0593
         BALR  @14,@15                                             0593
*/*                                                                  */
*                                                                  0594
*       OCLINK=OPCWAITP;            /* ADD OPCE TO TOP OF CHAIN      */
         L     @09,OPCWAITP(,ROPCAVT)                              0594
         ST    @09,OCLINK(,ROPCE)                                  0594
*       OPCWAITP=RELEMENT;          /* ASSIGN CHAIN POINTER TO NEW 0595
*                                      OPCE                          */
         ST    RELEMENT,OPCWAITP(,ROPCAVT)                         0595
*       OPCMODID=MODID00;           /* SET UP TO XCTL TO '00'        */
         MVC   OPCMODID(2,ROPCAVT),@CC01718                        0596
*       RZERO=0;                    /* CLEAR REG 0           @YM03724*/
         SLR   RZERO,RZERO                                         0597
*/*                                                                  */
*                                                                  0598
*     END;                                                         0598
*                                                                  0598
*/*                                                                  */
*                                                                  0599
*   ELSE                            /* BAD RESPONSE                  */
*     DO;                                                          0599
         B     @RC00588                                            0599
@RF00588 DS    0H                                                  0600
*       TRMPCONT=TRMCRSET;          /* CONTACT RESET STATE           */
         LR    @09,RTRM                                            0600
         AL    @09,@CF01763                                        0600
         NI    TRMPCONT-1(@09),B'11111000'                         0600
*       OCPARM=OCRCBNEG;            /* SET BAD RETURN CODE           */
         MVC   OCPARM(2,ROPCE),@CH01126                            0601
*       OPCMODID=OCWTGB;            /* SET UP RETURN TO OP SERV      */
         MVC   OPCMODID(2,ROPCAVT),OCWTGB(ROPCE)                   0602
*       OCWTG=0;                    /* CLEAR 'WHERE TO GO'           */
         SLR   @09,@09                                             0603
         STH   @09,OCWTG(,ROPCE)                                   0603
*     END;                                                         0604
*                                                                  0604
*/*                                                                  */
*                                                                  0605
*   RE=SAVE1;                       /* RESTORE REG 14                */
@RC00588 L     RE,SAVE1(,RWORK)                                    0605
*   END CONTCTRP;                   /* RETURN                        */
@EL00005 DS    0H                                                  0606
@EF00005 DS    0H                                                  0606
@ER00005 BR    @14                                                 0606
*CONTCTRQ:                                                         0607
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0607
*                                                                  0607
CONTCTRQ DS    0H                                                  0608
*/* ******************************************************************/
*/*                                                                  */
*/*    THIS ROUTINE PROCESSES A CONTACTED REQUEST. THERE ARE FOUR    */
*/*    POSSIBILITIES TO CONSIDER WHEN THE CONTACTED REQUEST ARRIVES. */
*/*                         ...                                      */
*/*    1. NO OPCE ON THE OPCWAITP CHAIN FOR THIS PU.                 */
*/*       FOR THIS CASE DO THE FOLLOWING:                            */
*/*       A. FREE THE CONTACTED BUFFER                               */
*/*       B. PASS CONTROL TO IGC0010D                                */
*/*                         ...                                      */
*/*    2. CONTACTED REQUEST CONTAINS BAD STATUS.                     */
*/*       FOR THIS CASE                                              */
*/*       A. DELINK THE OPCE                                         */
*/*       B. SET BAD RETURN CODE IN OCPARM                           */
*/*       C. RETURN TO ORIGINAL REQUESTOR                            */
*/*                         ...                                      */
*/*    3. SIB CAN NOT BE OBTAINED.                                   */
*/*       FOR THIS CASE                                              */
*/*       A. DELINK THE OPCE                                         */
*/*       B. BUILD DISCONTACT REQUEST                                */
*/*       C. SET BAD RETURN CODE (INDICATING THAT SIB CAN NOT BE     */
*/*          OBTAINED) IN OCPARM                                     */
*/*       D. PASS CONTROL TO IGC0210D                                */
*/*                         ...                                      */
*/*    4. NORMAL NON-ERROR CASE                                      */
*/*       FOR THIS CASE                                              */
*/*       A. DELINK OPCE                                             */
*/*       B. GET A SIB                                               */
*/*       C. BUILD ACTPU REQUEST                                     */
*/*       D. PASS CONTROL TO IGC0210D                                */
*/*                                                                  */
*/* ******************************************************************/
*                                                                  0608
*   SAVE1=RE;                       /* SAVE REG 14                   */
         ST    RE,SAVE1(,RWORK)                                    0608
*   WKBUFADR=RELEMENT;              /* SAVE BUFFER ADDRESS           */
         ST    RELEMENT,WKBUFADR(,RWORK)                           0609
*   WKCMDADR=AVTHDRSZ+ADDR(PRFSUNIT)+PRFISIZE;/* CALCULATE CMD ADDR  */
         L     @09,WKBUFADR(,RWORK)                                0610
         LA    @11,PRFSUNIT(,@09)                                  0610
         AL    @11,@CF00439                                        0610
         SLR   @10,@10                                             0610
         IC    @10,PRFISIZE(,@09)                                  0610
         ALR   @11,@10                                             0610
         ST    @11,WKCMDADR(,RWORK)                                0610
*/*                                                                  */
*                                                                  0611
*   WKFNDSW=OFF;                    /* INITIALIZE SWITCH OFF         */
         NI    WKFNDSW(RWORK),B'01111111'                          0611
*   WKCURRNT=OPCWAITP;              /* FIRST OPCE ON CHAIN           */
         L     @09,OPCWAITP(,ROPCAVT)                              0612
         ST    @09,WKCURRNT(,RWORK)                                0612
*   WKPREV=ADDR(OPCWAITP);          /* SET UP CHAIN ANCHOR           */
         LA    @09,OPCWAITP(,ROPCAVT)                              0613
         ST    @09,WKPREV(,RWORK)                                  0613
*/*                                                                  */
*                                                                  0614
*   DO WHILE WKFNDSW=OFF&WKCURRNT>0;/* NOT END OF CHAIN AND MATCH NOT
*                                      FOUND                         */
         B     @DE00614                                            0614
@DL00614 DS    0H                                                  0615
*     IF XCTDADDR=WKCURRNT->OCPUADR THEN/* OPCE FOR THIS RESOURCE    */
         L     @09,WKCMDADR(,RWORK)                                0615
         L     @11,WKCURRNT(,RWORK)                                0615
         CLC   XCTDADDR(2,@09),OCPUADR(@11)                        0615
         BNE   @RF00615                                            0615
*       DO;                                                        0616
*         WKPREV->OCLINK=WKCURRNT->OCLINK;/* DELINK OPCE             */
         L     @09,WKPREV(,RWORK)                                  0617
         L     @11,OCLINK(,@11)                                    0617
         ST    @11,OCLINK(,@09)                                    0617
*         WKFNDSW=ON;               /* SET FOUND SWITCH              */
         OI    WKFNDSW(RWORK),B'10000000'                          0618
*       END;                                                       0619
*     ELSE                                                         0620
*       DO;                                                        0620
         B     @RC00615                                            0620
@RF00615 DS    0H                                                  0621
*         WKPREV=WKCURRNT;          /* SAVE ADDR FOE DELINKING       */
         L     @09,WKCURRNT(,RWORK)                                0621
         ST    @09,WKPREV(,RWORK)                                  0621
*         WKCURRNT=WKCURRNT->OCLINK;/* GET NEXT OPCE ON CHAIN        */
         L     @09,OCLINK(,@09)                                    0622
         ST    @09,WKCURRNT(,RWORK)                                0622
*       END;                                                       0623
*   END;                                                           0624
*                                                                  0624
@RC00615 DS    0H                                                  0624
@DE00614 TM    WKFNDSW(RWORK),B'10000000'                          0624
         BNZ   @DC00614                                            0624
         L     @09,WKCURRNT(,RWORK)                                0624
         LTR   @09,@09                                             0624
         BP    @DL00614                                            0624
@DC00614 DS    0H                                                  0625
*/*                                                                  */
*                                                                  0625
*   IF WKFNDSW=ON THEN              /* OPCE ON THE CHAIN             */
         TM    WKFNDSW(RWORK),B'10000000'                          0625
         BNO   @RF00625                                            0625
*     CALL CNTDPROC;                /* MORE PROCESSING FOR CONTACTED
*                                      REQUEST                       */
         BAL   @14,CNTDPROC                                        0626
*/*                                                                  */
*                                                                  0627
*   ELSE                            /* NOT WAITING ON CONTACTED      */
*     DO;                           /* SET UP TO FREE BUFFER         */
         B     @RC00625                                            0627
@RF00625 DS    0H                                                  0628
*       RPARM=WKBUFADR;             /* BUFFER ADDRESS                */
         L     RPARM,WKBUFADR(,RWORK)                              0628
*       CALL FREEBUF;               /* FREE BUFFER                   */
         L     @15,OPCFREBF(,ROPCAVT)                              0629
         BALR  @14,@15                                             0629
*       OPCMODID=MODID00;           /* SET UP TO XCTL TO "00"        */
         MVC   OPCMODID(2,ROPCAVT),@CC01718                        0630
*       RZERO=0;                    /* CLEAR REG 0           @YM03724*/
         SLR   RZERO,RZERO                                         0631
*     END;                                                         0632
*                                                                  0632
*/*                                                                  */
*                                                                  0633
*   RE=SAVE1;                       /* RESTORE REG 14                */
@RC00625 L     RE,SAVE1(,RWORK)                                    0633
*   END CONTCTRQ;                   /* RETURN                        */
@EL00006 DS    0H                                                  0634
@EF00006 DS    0H                                                  0634
@ER00006 BR    @14                                                 0634
*DISCONRP:                                                         0635
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0635
*                                                                  0635
DISCONRP DS    0H                                                  0636
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE PROCESSES A DISCONTACT RESPONSE. THE CONTACT     */
*   /* STATE IS SET AND IT SETS UP FIELDS NECESSARY TO RETURN TO THE */
*   /* ORIGINAL REQUESTOR (OPERATOR SERVICES).                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0636
*   TRMPCONT=TRMCRSET;              /* SET CONTACT RESET STATE       */
         LR    @09,RTRM                                            0636
         AL    @09,@CF01763                                        0636
         NI    TRMPCONT-1(@09),B'11111000'                         0636
*   OPCMODID=OCWTGB;                /* SET UP RETURN TO OP SERV      */
         MVC   OPCMODID(2,ROPCAVT),OCWTGB(ROPCE)                   0637
*   OCWTG=0;                        /* CLEAR "WHERE TO GO"           */
         SLR   @09,@09                                             0638
         STH   @09,OCWTG(,ROPCE)                                   0638
*   END DISCONRP;                   /* RETURN                        */
@EL00007 DS    0H                                                  0639
@EF00007 DS    0H                                                  0639
@ER00007 BR    @14                                                 0639
*DISCONRQ:                                                         0640
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0640
*                                                                  0640
DISCONRQ DS    0H                                                  0641
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE BUILDS A DISCONTACT REQUEST EITHER IN THE BUFFER */
*   /* THAT HAS THE CONTACTED REQUEST OR IN THE ACTPU RESPONSE       */
*   /* BUFFER.                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0641
*/*                                                                  */
*                                                                  0641
*   WKCMDADR=ADDR(PRUDATA);         /* SET UP CMD PTR        @YM03746*/
         L     @09,OCUNIT(,ROPCE)                                  0641
         LA    @11,PRUDATA(,@09)                                   0641
         ST    @11,WKCMDADR(,RWORK)                                0641
*   PRURHTH=RHTHFMD;                /* INITIALIZE RH AND TH          */
         LR    @10,@09                                             0642
         AL    @10,@CF01761                                        0642
         MVC   PRURHTH(4,@10),@CB01709                             0642
*   PRUDATCT=LENGTH(IEDXDSCD);      /* SET UP DATA COUNT FIELD       */
         MVC   PRUDATCT(2,@09),@CH00153                            0643
*   PRUTTCIN=OCNCP;                 /* SET UP DESTINATION            */
         LH    @10,OCNCP(,ROPCE)                                   0644
         N     @10,@CF01760                                        0644
         STH   @10,PRUTTCIN(,@09)                                  0644
*   XDSCCODE=CD1XDSC;               /* DISCONTACT COMMAND            */
         MVC   XDSCCODE(3,@11),@CB00724                            0645
*   XDSCADDR=OCPUADR;               /* SET UP NETWORK ADDRESS        */
         MVC   XDSCADDR(2,@11),OCPUADR(ROPCE)                      0646
*   TRMPCONT=TRMCRPND;              /* SET DISCONTACT SENT           */
         LR    @09,RTRM                                            0647
         AL    @09,@CF01763                                        0647
         NI    TRMPCONT-1(@09),B'11111011'                         0647
         OI    TRMPCONT-1(@09),B'00000011'                         0647
*/*                                                                  */
*                                                                  0648
*   END DISCONRQ;                   /* RETURN                        */
@EL00008 DS    0H                                                  0648
@EF00008 DS    0H                                                  0648
@ER00008 BR    @14                                                 0648
*SIBRTN:                                                           0649
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0649
*                                                                  0649
SIBRTN   DS    0H                                                  0650
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE IS CALLED EITHER TO GET A SIB OR FREE A SIB. IN  */
*   /* ORDER TO ACCOMPLISH THIS TASK A REQUEST ELEMENT MUST BE BUILT */
*   /* AND PASSED TO THE POSTWAIT SUBROUTINE IN IEDQCA. NOTE: REQTYPE*/
*   /* MUST BE SET UP BEFORE CALLING THIS ROUTINE.                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0650
*   SAVE3=RE;                       /* SAVE REG 14                   */
         ST    RE,SAVE3(,RWORK)                                    0650
*/*                                                                  */
*                                                                  0651
*   REQQCBA=SAVTXTRQ;               /* QCB ADDRESS                   */
         LA    @09,12                                              0651
         ALR   @09,RWORK                                           0651
         L     @11,AVTSAVTP(,RAVT)                                 0651
         MVC   REQQCBA(3,@09),SAVTXTRQ+1(@11)                      0651
*   REQPRI=PRISIBRQ;                /* PRIORITY                      */
         MVI   REQPRI(@09),X'E5'                                   0652
*   REQRPRI=PRISIBRQ;               /* RETURN PRIORITY               */
         MVI   REQRPRI(@09),X'E5'                                  0653
*   REQRQCBA=ADDR(OPCPWQCB);        /* RETURN QCB ADDRESS    @YM08403*/
         LA    @10,OPCPWQCB(,ROPCAVT)                              0654
         STCM  @10,7,REQRQCBA(@09)                                 0654
*   REQSTTE=RTRM;                   /* TTE OF RESOURCE               */
         ST    RTRM,REQSTTE(,@09)                                  0655
*   REQSTNT=SAVTSCPT;               /* TNT INDEX OF SSCP             */
         LH    @11,SAVTSCPT(,@11)                                  0656
         N     @11,@CF01760                                        0656
         STH   @11,REQSTNT(,@09)                                   0656
*/*                                                                  */
*                                                                  0657
*   RPARM=ADDR(WKREQELE);           /* REQUEST ELEMENT ADDR          */
         LA    RPARM,WKREQELE(,RWORK)                              0657
*   CALL POSTWAIT;                  /* GET OR FREE A SIB             */
         L     @15,OPCPSTWT(,ROPCAVT)                              0658
         BALR  @14,@15                                             0658
*/*                                                                  */
*                                                                  0659
*   RE=SAVE3;                       /* RESTORE REG 14                */
         L     RE,SAVE3(,RWORK)                                    0659
*   END SIBRTN;                     /* RETURN TO CALLER              */
@EL00009 DS    0H                                                  0660
@EF00009 DS    0H                                                  0660
@ER00009 BR    @14                                                 0660
*   END IGCA510D                                                   0661
