         LA    R11,1                                               0448
*               END;                /*                       @Y30DSMD*/
*             CALL SETLOCK;         /* SET THE LOCAL LOCK    @Y021130*/
@RF00445 BAL   @14,SETLOCK                                         0450
*             IF UCBVOLI^='000000000000'X THEN/* IF UCB USED         */
         CLC   UCBVOLI(6,RUCB),@CB04476                            0451
         BE    @RF00451                                            0451
*               IF UCBRESV=OFF&UCBPRES=OFF&UCBSYSR=OFF&UCBBSVL=ON&/*
*                                      IF VOLUME DISMOUNTABLE        */
*                   (EOVDM=ON(VVOLLEOV=OFF&UCBDMCT=0)) THEN/* IS  0452
*                                      VOLUME UNUSED         @Z30SSJH*/
         TM    UCBRESV(RUCB),B'00100110'                           0452
         BNZ   @RF00452                                            0452
         TM    UCBBSVL(RUCB),B'10000000'                           0452
         BNO   @RF00452                                            0452
         TM    EOVDM,B'00100000'                                   0452
         BO    @RT00452                                            0452
         L     @05,VOLLPTR                                         0452
         TM    VVOLLEOV(@05),B'00001000'                           0452
         BNZ   @RF00452                                            0452
         CLI   UCBDMCT(RUCB),0                                     0452
         BNE   @RF00452                                            0452
@RT00452 DS    0H                                                  0453
*                 DO;               /* YES                           */
*                   DMSG=ON;        /* IND NEED FOR DISMOUNT MESSAGE */
         OI    DMSG,B'00000001'                                    0454
*                   CALL ALOCUNIT;  /* CALL FOR MOUNT OF VOLUME      */
         BAL   @14,ALOCUNIT                                        0455
*                 END;                                             0456
*               ELSE                /* VOLUME NOT DISMOUNTABLE     0457
*                                                            @Z30SSRL*/
*                 VUCBLBAD(C2)=ON;  /* UNIT IS UNUSABLE              */
         B     @RC00452                                            0457
@RF00452 L     @05,C2                                              0457
         SLA   @05,2                                               0457
         L     @01,UCBLPTR                                         0457
         ALR   @01,@05                                             0457
         OI    VUCBLBAD-4(@01),B'01000000'                         0457
*             ELSE                  /* UCB IS UNUSED                 */
