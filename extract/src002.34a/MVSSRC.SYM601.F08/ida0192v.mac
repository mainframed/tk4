*   DEBDCBB=ADDR(IHADCB);           /* DEB POINTS TO DCB             */
         STCM  DCBPTR,7,DEBDCBB(@09)                               0481
*   DEBDEBID=IDXDEB;                /* SET DEB ID                    */
         MVI   DEBDEBID(@09),B'00001111'                           0482
*   DEBNMEXT=1;                     /* SET EXTENT NUMBER             */
         MVI   DEBNMEXT(@09),X'01'                                 0483
*   IOBUNREL=ON;                    /* SET NON-SEQUENTIAL BIT        */
         OI    IOBUNREL(@11),B'00000010'                           0484
*   IOBDCBPT=ADDR(IHADCB);          /* IOB POINTS TO DCB             */
         ST    DCBPTR,IOBDCBPT(,@11)                               0485
*   IOBECBPT=ADDR(DXECB);           /* IOB POINTS TO ECB             */
         LA    @05,DXECB                                           0486
