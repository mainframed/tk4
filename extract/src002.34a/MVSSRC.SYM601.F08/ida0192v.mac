*                     R1=R0;        /* SET UP FOR CS         @Y021130*/
         LR    R1,R0                                               0411
*                     R1=R1+1;      /* BUMP OPEN COUNT       @Y021130*/
         AL    R1,@CF00062                                         0412
*                     GEN(CS   R0,R1,0(R14));/* COMPARE AND SWAP   0413
*                                                            @Y021130*/
         CS   R0,R1,0(R14)
*                     GEN(BNE  CSLOOP);/* BR NO COMPARE      @Y021130*/
         BNE  CSLOOP
*                     RFY                                          0415
*                      (R0,                                        0415
*                       R1,                                        0415
*                       R14) UNRSTD;/*                       @Y021130*/
*                     VVOLLUCB(C1)=RUCB;/* COPY POINTER TO UCB       */
         L     @11,C1                                              0416
         MH    @11,@CH00033                                        0416
         L     @05,VOLLPTR                                         0416
         ST    RUCB,VVOLLUCB-12(@11,@05)                           0416
*                     VUCBLALC(C2)=ON;/* NOTE UCB ALLOCATED          */
         L     @11,C2                                              0417
         SLA   @11,2                                               0417
         L     @05,UCBLPTR                                         0417
         ALR   @05,@11                                             0417
         OI    VUCBLALC-4(@05),B'10000000'                         0417
*                   END;                                           0418
*                 CALL FREELOCK;    /* FREE THE LOCAL LOCK           */
@RF00402 BAL   @14,FREELOCK                                        0419
*               END;                /* END OF VOLSER MATCH LOGIC.  0420
*                                                            @Y30DSMD*/
*           END;                    /* END OF UCB NOT ALLOCATED    0421
*                                      LOGIC.                @Y30DSMD*/
@RF00399 DS    0H                                                  0422
*       END;                        /* END OF UCB LIST LOOP. @Y30DSMD*/
@RF00396 LA    @11,1                                               0422
         AL    @11,C2                                              0422
@DE00395 ST    @11,C2                                              0422
         L     @05,UCBLPTR                                         0422
         CH    @11,VUCBLNBR(,@05)                                  0422
         BNH   @DL00395                                            0422
@DC00395 DS    0H                                                  0423
*       IF MWASWTCH=OFF THEN        /* UCB NOT FOUND         @ZA33246*/
         TM    MWASWTCH,B'10000000'                                0423
         BNZ   @RF00423                                            0423
*         DO;                       /*                       @ZA33246*/
*           MWAMTCNT=MWAMTCNT+1;    /* INCREMENT THE COUNT OF VOLUMES
