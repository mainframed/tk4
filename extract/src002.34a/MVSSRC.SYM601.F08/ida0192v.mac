         BNO   @RF00390                                            0390
*     CALL VMTSCAN;                 /* YES, SCAN THE VMT'S   @Z30SSJH*/
         BAL   @14,VMTSCAN                                         0391
*   ELSE                            /* NOT AN EOV REQUEST    @Z30SSJH*/
*     VMTHIT=OFF;                   /* IND VOLUME NOT IN VMT @Z30SSJH*/
         B     @RC00390                                            0392
@RF00390 NI    VMTHIT,B'10111111'                                  0392
*   IF VMTHIT=OFF THEN              /* IF VOLUME NOT IN VMT  @Z30SSJH*/
@RC00390 TM    VMTHIT,B'01000000'                                  0393
         BNZ   @RF00393                                            0393
*     DO C1=1 TO VVOLLNBR;          /* LOOP THRU VOLUME LIST         */
         LA    @11,1                                               0394
         B     @DE00394                                            0394
@DL00394 DS    0H                                                  0395
*       DO C2=1 TO VUCBLNBR WHILE MWASWTCH=OFF;/* LOOP THRU UCB LIST
*                                      LOOKING FOR THE VOLSER MOUNTED
*                                      ON A UCB              @ZA33246*/
         LA    @11,1                                               0395
         B     @DE00395                                            0395
@DL00395 TM    MWASWTCH,B'10000000'                                0395
         BNZ   @DC00395                                            0395
*         IF VUCBLALC(C2)=OFF THEN  /* IF UCB IS NOT ALLOCATED       */
         L     @05,UCBLPTR                                         0396
         SLA   @11,2                                               0396
         ST    @11,@TF00001                                        0396
         ALR   @11,@05                                             0396
         TM    VUCBLALC-4(@11),B'10000000'                         0396
         BNZ   @RF00396                                            0396
*           DO;                                                    0397
*             RUCB=VUCBLADR(C2);    /* LOAD POINTER TO UCB           */
         L     @01,@TF00001                                        0398
         L     RUCB,VUCBLADR-5(@01,@05)                            0398
         LA    RUCB,0(,RUCB)                                       0398
*             IF UCBVOLI=VVOLLVSR(C1) THEN/* IF VOLSER MATCH         */
         L     @11,C1                                              0399
         MH    @11,@CH00033                                        0399
