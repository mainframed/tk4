GOMOUNT  DS    0H                                                  0436
*   RESPECIFY                                                      0436
*    (R11,                                                         0436
*     RET) RESTRICTED;              /*                       @ZA33246*/
*   RSAVLA=RET;                     /* SAVE RETURN ADDRESS           */
         ST    RET,RSAVLA                                          0437
*   RESPECIFY                                                      0438
*     RET UNRESTRICTED;             /*                       @ZA33246*/
*   DO C1=1 TO VVOLLNBR;            /* LOOP THRU VOLUME LIST         */
         LA    @05,1                                               0439
         B     @DE00439                                            0439
@DL00439 DS    0H                                                  0440
*     IF VVOLLMNT(C1)=OFF&VVOLLVFY(C1)=OFF THEN/* IF MOUNT MSG NEED  */
         MH    @05,@CH00033                                        0440
         L     @01,VOLLPTR                                         0440
         ALR   @01,@05                                             0440
         TM    VVOLLMNT-12(@01),B'11000000'                        0440
         BNZ   @RF00440                                            0440
*       DO C2=1 TO VUCBLNBR WHILE(VVOLLVFY(C1)=OFF);/* LOOP THRU UCBL*/
         LA    @05,1                                               0441
         B     @DE00441                                            0441
@DL00441 L     @14,VOLLPTR                                         0441
         L     @09,C1                                              0441
         MH    @09,@CH00033                                        0441
         ALR   @09,@14                                             0441
         TM    VVOLLVFY-12(@09),B'01000000'                        0441
         BNZ   @DC00441                                            0441
*         IF VUCBLALC(C2)=OFF&      /* SKIP IF UCB ALREADY USED OR   */
*             VUCBLBAD(C2)=OFF THEN /* IF UCB UNUSABLE               */
         L     @09,UCBLPTR                                         0442
         SLA   @05,2                                               0442
         ST    @05,@TF00001                                        0442
         ALR   @05,@09                                             0442
         TM    VUCBLALC-4(@05),B'11000000'                         0442
         BNZ   @RF00442                                            0442
*           DO;                                                    0443
*             RUCB=VUCBLADR(C2);    /* LOAD UCB ADDRESS FR UCB LIST  */
         L     @05,@TF00001                                        0444
         L     RUCB,VUCBLADR-5(@05,@09)                            0444
         LA    RUCB,0(,RUCB)                                       0444
*             IF VVOLLEOV=ON THEN   /* IF CALLER IS EOV      @Y30DSMD*/
         TM    VVOLLEOV(@14),B'00001000'                           0445
         BNO   @RF00445                                            0445
*               DO;                 /* CONTINUE              @Y30DSMD*/
*                 CALL EOVDMT;      /* EOV DISMOUNT CHK      @Y30DSMD*/
