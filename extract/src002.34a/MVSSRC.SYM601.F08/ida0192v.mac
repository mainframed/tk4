         B     @RC00404                                            0406
@RF00404 L     @11,C1                                              0406
         MH    @11,@CH00033                                        0406
         L     @05,VOLLPTR                                         0406
         ALR   @05,@11                                             0406
         OI    VVOLLVFY-12(@05),B'01000000'                        0406
*                     MWASWTCH=ON;  /* IND UCB FOUND         @ZA33246*/
*                                                                  0407
@RC00404 OI    MWASWTCH,B'10000000'                                0407
*                     /***********************************************/
*                     /*                                             */
*                     /* BUMP OPEN COUNT USING CS INST FOR INSURANCE */
