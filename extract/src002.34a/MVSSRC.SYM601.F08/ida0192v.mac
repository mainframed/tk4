         TM    UCBJES3(RUCB),B'11000000'                           0426
         BNO   @RF00426                                            0426
*             MWAJS3CT=MWAJS3CT+1;  /* INCREMENT THE COUNT OF JES3 0427
*                                      CONTROLLER MOUNT      @ZA33246*/
         AL    @11,MWAJS3CT                                        0427
         ST    @11,MWAJS3CT                                        0427
*         END;                      /*                       @ZA33246*/
*       ELSE                        /* UCB FOUND             @ZA33246*/
*         MWASWTCH=OFF;             /* RESET WORK SWITCH     @ZA33246*/
         B     @RC00423                                            0429
@RF00423 NI    MWASWTCH,B'01111111'                                0429
*     END;                          /* END OF VOL LIST LOOP. @Y30DSMD*/
@RC00423 LA    @11,1                                               0430
         AL    @11,C1                                              0430
@DE00394 ST    @11,C1                                              0430
         L     @05,VOLLPTR                                         0430
         CH    @11,VVOLLNBR(,@05)                                  0430
         BNH   @DL00394                                            0430
