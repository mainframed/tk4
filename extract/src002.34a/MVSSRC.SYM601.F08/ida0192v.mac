         L     R1,TSPT                                             0315
*           IF ACVTDBUG=OFF THEN    /* IF OK TO FREEMAIN             */
         L     @11,CVTPTR                                          0316
         TM    ACVTDBUG(@11),B'00000010'                           0316
         BNZ   @RF00316                                            0316
*             GEN(FREEMAIN R,LV=(0),A=(1));/* FREE THE WORK AREA     */
         FREEMAIN R,LV=(0),A=(1)
*         END;                                                     0318
@RF00316 DS    0H                                                  0319
*       IF FAEVUCBL=ON THEN         /* DOES UCB LIST NEED FREEMAIN   */
@RF00312 TM    FAEVUCBL,B'00000010'                                0319
         BNO   @RF00319                                            0319
