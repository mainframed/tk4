*/*    FUNCTION - DETERMINE FOR AN EOV REQUEST IF THE VOLUME SERIAL  */
*/*               NUMBER IS ALREADY IN THE VMT'S FOR THE SPHERE.     */
*/*                                                                  */
*/*    INPUT    - PTR TO BIB IN IDAVVOLL.                            */
*/*                                                                  */
*/*    OUTPUT   - IF VOLUME IS ALREADY MOUNTED, THEN UPDATE VMT      */
*/*               USE COUNT.                                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0663
*   PROC OPTIONS(NOSAVE);           /*                       @Z30SSJH*/
VMTSCAN  DS    0H                                                  0664
*   RFY                                                            0664
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/
*   RSAVLB=RET;                     /* SAVE RETURN ADDRESS   @Z30SSJH*/
         ST    RET,RSAVLB                                          0665
*   RFY                                                            0666
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/
*   VMTHIT=OFF;                     /* RESET ENTRY FOUND INDR@Z30SSJH*/
         NI    VMTHIT,B'10111111'                                  0667
*   VMTPT=BIBVMT;                   /* PT TO FIRST VMT       @Z30SSJH*/
         L     @11,VOLLPTR                                         0668
         L     @11,VVOLLBIB(,@11)                                  0668
         L     @11,BIBVMT(,@11)                                    0668
         ST    @11,VMTPT                                           0668
*   RUCB=VUCBLADR(1);               /* PT TO FIRST UCB       @Z30SSJH*/
         L     @05,UCBLPTR                                         0669
         L     RUCB,VUCBLADR-1(,@05)                               0669
         LA    RUCB,0(,RUCB)                                       0669
*   DO WHILE(VMTPT^=0&VMTHIT=OFF);  /* SCAN VMT'S            @Z30SSJH*/
         B     @DE00670                                            0670
@DL00670 DS    0H                                                  0671
*     IF VMTDVTYP=UCBTYP(3:4) THEN  /* CORRECT DEVICE TYPE ? @Z30SSJH*/
         L     @11,VMTPT                                           0671
         CLC   VMTDVTYP(2,@11),UCBTYP+2(RUCB)                      0671
         BNE   @RF00671                                            0671
*       DO;                         /* YES, SCAN THE VMT     @Z30SSJH*/
*         DO CTR=1 TO VMTNOVOL WHILE(VMTVLSER(CTR)^=VVOLLVSR(1));/*
*                                      SCAN THE VMT          @Z30SSJH*/
         LA    @11,1                                               0673
         B     @DE00673                                            0673
@DL00673 LR    @05,@11                                             0673
         SLA   @05,4                                               0673
         L     @01,VMTPT                                           0673
         ALR   @01,@05                                             0673
         L     @05,VOLLPTR                                         0673
         CLC   VMTVLSER-16(6,@01),VVOLLVSR(@05)                    0673
         BE    @DC00673                                            0673
*         END;                                                     0674
         AL    @11,@CF00062                                        0674
@DE00673 STC   @11,CTR                                             0674
         L     @05,VMTPT                                           0674
         CH    @11,VMTNOVOL(,@05)                                  0674
         BNH   @DL00673                                            0674
@DC00673 DS    0H                                                  0675
*         IF CTR<=VMTNOVOL THEN     /* WAS ENTRY FOUND       @Z30SSJH*/
         SLR   @11,@11                                             0675
         IC    @11,CTR                                             0675
         L     @05,VMTPT                                           0675
         CH    @11,VMTNOVOL(,@05)                                  0675
         BH    @RF00675                                            0675
*           DO;                     /* YES, IND VOL ALREADY MOUNTED
*                                                            @Z30SSJH*/
*             VMTHIT=ON;            /* IND VOL FOUND         @Z30SSJH*/
         OI    VMTHIT,B'01000000'                                  0677
*             VVOLLMNT(1)=ON;       /* IND VOL MOUNTED       @Z30SSJH*/
         L     @15,VOLLPTR                                         0678
         OI    VVOLLMNT(@15),B'10000000'                           0678
*             VVOLLUCB(1)=VMTUCB(CTR);/* INIT PTR TO UCB     @Z30SSJH*/
         SLA   @11,4                                               0679
         L     @14,VMTUCB-16(@11,@05)                              0679
         ST    @14,VVOLLUCB(,@15)                                  0679
*             IF VVOLLSWD=ON THEN   /* IS THIS SS/DATA       @Z30SSJH*/
         TM    VVOLLSWD(@15),B'00000010'                           0680
         BNO   @RF00680                                            0680
*               VMTUSECT(CTR)=VMTUSECT(CTR)+2;/* YES, INCR USE COUNT
*                                      BY 2                  @Z30SSJH*/
         LA    @15,2                                               0681
         AL    @15,VMTUSECT-16(@11,@05)                            0681
         ST    @15,VMTUSECT-16(@11,@05)                            0681
*             ELSE                  /* NOT SS/DATA           @Z30SSJH*/
*               VMTUSECT(CTR)=VMTUSECT(CTR)+1;/* INCR VMT USE COUNT
*                                      BY 1                  @Z30SSJH*/
         B     @RC00680                                            0682
@RF00680 L     @11,VMTPT                                           0682
         SLR   @05,@05                                             0682
         IC    @05,CTR                                             0682
         SLA   @05,4                                               0682
         LA    @15,1                                               0682
         AL    @15,VMTUSECT-16(@05,@11)                            0682
         ST    @15,VMTUSECT-16(@05,@11)                            0682
*           END;                    /*                       @Z30SSJH*/
@RC00680 DS    0H                                                  0684
*       END;                        /*                       @Z30SSJH*/
@RF00675 DS    0H                                                  0685
*     IF VMTHIT=OFF THEN            /* IF ENTRY NOT FOUND    @Z30SSJH*/
@RF00671 TM    VMTHIT,B'01000000'                                  0685
         BNZ   @RF00685                                            0685
*       VMTPT=VMTNXT;               /* THEN PT TO NEXT VMT   @Z30SSJH*/
         L     @11,VMTPT                                           0686
         L     @11,VMTNXT(,@11)                                    0686
         ST    @11,VMTPT                                           0686
*   END;                            /*                       @Z30SSJH*/
@RF00685 DS    0H                                                  0687
@DE00670 L     @11,VMTPT                                           0687
         LTR   @11,@11                                             0687
         BZ    @DC00670                                            0687
         TM    VMTHIT,B'01000000'                                  0687
         BZ    @DL00670                                            0687
@DC00670 DS    0H                                                  0688
*   RFY                                                            0688
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/
*   RET=RSAVLB;                     /* RESTORE RETURN ADDR   @Z30SSJH*/
         L     RET,RSAVLB                                          0689
*   RFY                                                            0690
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/
*   END VMTSCAN;                    /* RETRUN TO THE CALLER  @Z30SSJH*/
@EL00006 DS    0H                                                  0691
@EF00006 DS    0H                                                  0691
@ER00006 BR    @14                                                 0691
*EOVDMT:                                                           0692
*                                                                  0692
*/********************************************************************/
*/*  FUNCTION - DETERMINE FOR AN EOV REQUEST IF THE SELECTED @Y30DSMD*/
*/*             UCB IS ELIGIBLE FOR DISMOUNT                 @Y30DSMD*/
*/*                                                          @Y30DSMD*/
*/*  INPUT    - RUCB CONTAINS SELECTED UCB PTR.  VVOLL PARM  @Y30DSMD*/
*/*             LIST CONTAINS PTR TO DEB AND BIB.            @Y30DSMD*/
*/*                                                          @Y30DSMD*/
*/*  OUTPUT   - IF OK TO DISMOUNT THEN EOVDM IS TURNED ON    @Y30DSMD*/
*/*             AND VMTPT INDEXED BY CTR REFLECTS VMT ENTRY  @Y30DSMD*/
*/*             ASSOCIATED WITH RUCB                         @Y30DSMD*/
*/********************************************************************/
*                                                                  0692
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @Y30DSMD*/
EOVDMT   DS    0H                                                  0693
*   RESPECIFY                                                      0693
*     RET RESTRICTED;               /*                       @Y30DSMD*/
*   RSAVLB=RET;                     /* SAVE RETURN ADDRESS   @Y30DSMD*/
         ST    RET,RSAVLB                                          0694
*   RESPECIFY                                                      0695
*     RET UNRESTRICTED;             /*                       @Y30DSMD*/
*   DEBPTR=VVOLLDEB;                /* INIT BASE PTR FOR DEB @Y30DSMD*/
         L     @11,VOLLPTR                                         0696
         L     @05,VVOLLDEB(,@11)                                  0696
         ST    @05,DEBPTR                                          0696
*   VMTPT=BIBVMT;                   /* INIT BASE PTR FOR VMT @Y30DSMD*/
         L     @11,VVOLLBIB(,@11)                                  0697
         L     @11,BIBVMT(,@11)                                    0697
         ST    @11,VMTPT                                           0697
*   VMTHIT=OFF;                     /* CLEAR SWITCH          @Y30DSMD*/
*   EOVDM=OFF;                      /* CLEAR DISMOUNT OK BIT @Y30DSMD*/
         NI    VMTHIT,B'10011111'                                  0699
*   CTR=0;                          /* CLEAR VMT INDEX       @Y30DSMD*/
         MVI   CTR,X'00'                                           0700
*   DO WHILE VMTPT^=0&VMTHIT=OFF;   /* PROCESS VMT'S         @Y30DSMD*/
         B     @DE00701                                            0701
@DL00701 DS    0H                                                  0702
*     IF VMTDVTYP=UCBTYP(3:4) THEN  /* DEVICE TYPE MATCH     @Y30DSMD*/
         L     @11,VMTPT                                           0702
         CLC   VMTDVTYP(2,@11),UCBTYP+2(RUCB)                      0702
         BNE   @RF00702                                            0702
*       DO UNTIL(VMTHIT=ONCTR=VMTNOVOL);/* YES, SCAN VMT    @Y30DSMD*/
@DL00703 DS    0H                                                  0704
*         CTR=CTR+1;                /* NXT OR FIRST VMT ENTRY      0704
*                                                            @Y30DSMD*/
         LA    @11,1                                               0704
         SLR   @05,@05                                             0704
         IC    @05,CTR                                             0704
         ALR   @05,@11                                             0704
         STC   @05,CTR                                             0704
*         IF RUCB=VMTUCB(CTR) THEN  /* UCB MATCH             @Y30DSMD*/
         SLA   @05,4                                               0705
         L     @01,VMTPT                                           0705
         C     RUCB,VMTUCB-16(@05,@01)                             0705
         BNE   @RF00705                                            0705
*           DO;                     /* YES, CONTINUE         @Y30DSMD*/
*             VMTHIT=ON;            /* BREAK LOOPS           @Y30DSMD*/
         OI    VMTHIT,B'01000000'                                  0707
*             DEBDAPTR=ADDR(DEBBASND);/* PT 1ST DEB EXTENT   @Y30DSMD*/
         L     @05,DEBPTR                                          0708
         LA    @05,DEBBASND(,@05)                                  0708
         ST    @05,DEBDAPTR                                        0708
*             DO CTR1=1 TO DEBNMEXT WHILE VMTUCB(CTR)^=DEBUCBA;/*  0709
*                                      SEARCH FOR UCB MATCH  @Y30DSMD*/
         LR    CTR1,@11                                            0709
         B     @DE00709                                            0709
@DL00709 SLR   @11,@11                                             0709
         IC    @11,CTR                                             0709
         SLA   @11,4                                               0709
         L     @15,DEBDAPTR                                        0709
         L     @01,VMTPT                                           0709
         L     @11,VMTUCB-16(@11,@01)                              0709
         L     @14,DEBUCBA-1(,@15)                                 0709
         LA    @14,0(,@14)                                         0709
         CR    @11,@14                                             0709
         BE    @DC00709                                            0709
*               DEBDAPTR=DEBDAPTR+LENGTH(DEBDASD);/*         @Y30DSMD
*                                      PT NEXT EXTENT        @Y30DSMD*/
         AL    @15,@CF00078                                        0710
         ST    @15,DEBDAPTR                                        0710
*             END;                  /*                       @Y30DSMD*/
         AL    CTR1,@CF00062                                       0711
@DE00709 L     @11,DEBPTR                                          0711
         SLR   @15,@15                                             0711
         IC    @15,DEBNMEXT(,@11)                                  0711
         CR    CTR1,@15                                            0711
         BNH   @DL00709                                            0711
@DC00709 DS    0H                                                  0712
*             IF VMTUCB(CTR)=DEBUCBA THEN/* DEB USING UCB    @Y30DSMD*/
         SLR   @11,@11                                             0712
         IC    @11,CTR                                             0712
         SLA   @11,4                                               0712
         L     @05,VMTPT                                           0712
         L     @11,VMTUCB-16(@11,@05)                              0712
         L     @05,DEBDAPTR                                        0712
         L     @05,DEBUCBA-1(,@05)                                 0712
         LA    @05,0(,@05)                                         0712
         CR    @11,@05                                             0712
         BNE   @RF00712                                            0712
*               IF VVOLLSWD=ON THEN /* YES, SEQ SET WITH DATA        */
         L     @11,VOLLPTR                                         0713
         TM    VVOLLSWD(@11),B'00000010'                           0713
         BNO   @RF00713                                            0713
*                 STCNT=2;          /* YES, USE COUNT IS 2   @Y30DSMD*/
         MVI   STCNT,X'02'                                         0714
*               ELSE                /* NOT SEQ SET WITH DATA @Y30DSMD*/
*                 STCNT=1;          /* USE COUNT IS 1        @Y30DSMD*/
         B     @RC00713                                            0715
@RF00713 MVI   STCNT,X'01'                                         0715
*             ELSE                  /* DEB NOT USING UCB     @Y30DSMD*/
*               STCNT=0;            /* USE COUNT IS 0        @Y30DSMD*/
         B     @RC00712                                            0716
@RF00712 MVI   STCNT,X'00'                                         0716
*             IF STCNT>=VMTUSECT(CTR) THEN/* OTHER STRUCTURES USING
*                                      UCB                           */
@RC00712 SLR   @11,@11                                             0717
         IC    @11,CTR                                             0717
         SLA   @11,4                                               0717
         SLR   @05,@05                                             0717
         IC    @05,STCNT                                           0717
         L     @01,VMTPT                                           0717
         C     @05,VMTUSECT-16(@11,@01)                            0717
         BL    @RF00717                                            0717
*               EOVDM=ON;           /* NO, OK TO DISMOUNT    @Y30DSMD*/
         OI    EOVDM,B'00100000'                                   0718
*           END;                    /*                       @Y30DSMD*/
@RF00717 DS    0H                                                  0720
*       END;                        /*                       @Y30DSMD*/
@RF00705 DS    0H                                                  0720
@DE00703 TM    VMTHIT,B'01000000'                                  0720
         BO    @DC00703                                            0720
         SLR   @11,@11                                             0720
         IC    @11,CTR                                             0720
         L     @05,VMTPT                                           0720
         CH    @11,VMTNOVOL(,@05)                                  0720
         BNE   @DL00703                                            0720
@DC00703 DS    0H                                                  0721
*     IF VMTHIT=OFF THEN            /* IF NOT FOUND IN THIS VMT THEN,
*                                                            @Z30SSMD*/
@RF00702 TM    VMTHIT,B'01000000'                                  0721
         BNZ   @RF00721                                            0721
*       VMTPT=VMTNXT;               /* ADDR NEXT VMT         @Y30DSMD*/
         L     @11,VMTPT                                           0722
         L     @11,VMTNXT(,@11)                                    0722
         ST    @11,VMTPT                                           0722
*   END;                            /*                       @Y30DSMD*/
@RF00721 DS    0H                                                  0723
@DE00701 L     @11,VMTPT                                           0723
         LTR   @11,@11                                             0723
         BZ    @DC00701                                            0723
         TM    VMTHIT,B'01000000'                                  0723
         BZ    @DL00701                                            0723
@DC00701 DS    0H                                                  0724
*   RESPECIFY                                                      0724
*     RET RESTRICTED;               /*                       @Y30DSMD*/
*   RET=RSAVLB;                     /* RESTORE RETURN REG    @Y30DSMD*/
         L     RET,RSAVLB                                          0725
*   RESPECIFY                                                      0726
*     RET UNRESTRICTED;             /*                       @Y30DSMD*/
*   END EOVDMT;                     /*                       @Y30DSMD*/
@EL00007 DS    0H                                                  0727
@EF00007 DS    0H                                                  0727
@ER00007 BR    @14                                                 0727
*MOUNT:                                                            0728
*                                                                  0728
*/********************************************************************/
*/*                                                                  */
*/*   FUNCTION  - SETS POINTER TO MOUNT MESSAGE AND CALLS SEND       */
*/*               TO ISSUE MESSAGE.                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0728
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0728
MOUNT    DS    0H                                                  0729
*   RESPECIFY                                                      0729
*     RET RESTRICTED;               /*                       @ZA33246*/
*   RSAVLC=RET;                     /* SAVE RETURN ADDRESS           */
         ST    RET,RSAVLC                                          0730
*   RESPECIFY                                                      0731
*     RET UNRESTRICTED;             /*                       @ZA33246*/
*   IF EOVDM=ON THEN                /* ENTRY FOUND           @Y30DSMD*/
         TM    EOVDM,B'00100000'                                   0732
         BNO   @RF00732                                            0732
*     DO;                           /* YES,                  @Y30DSMD*/
*       VMTVOL(CTR)=VMTVOL(CTR)&&VMTVOL(CTR);/* CLEAR VMT ENTRY      */
         L     @11,VMTPT                                           0734
         SLR   @05,@05                                             0734
         IC    @05,CTR                                             0734
         SLA   @05,4                                               0734
         ST    @05,@TF00001                                        0734
         ALR   @05,@11                                             0734
         XC    VMTVOL-16(16,@05),VMTVOL-16(@05)                    0734
*       VMTUCB(CTR)=RUCB;           /* INIT UCB PTR          @Y30DSMD*/
         L     @05,@TF00001                                        0735
         ST    RUCB,VMTUCB-16(@05,@11)                             0735
*       VMTVLSER(CTR)=UCBVOLI;      /* INIT VOL SER          @Y30DSMD*/
         LR    @05,@11                                             0736
         AL    @05,@TF00001                                        0736
         MVC   VMTVLSER-16(6,@05),UCBVOLI(RUCB)                    0736
*       VMTUSECT(CTR)=STCNT;        /* INIT USE COUNT        @Y30DSMD*/
         SLR   @05,@05                                             0737
         IC    @05,STCNT                                           0737
         L     @01,@TF00001                                        0737
         ST    @05,VMTUSECT-16(@01,@11)                            0737
*     END;                          /*                       @Y30DSMD*/
*   IF UCB2OPT4=ON THEN             /* CHECK IF VIRTUAL DEV  @Y30LSPS*/
@RF00732 TM    UCB2OPT4(RUCB),B'00001000'                          0739
         BNO   @RF00739                                            0739
*     DO;                           /* SPECIAL MOUNT IF YES  @Y30LSPS*/
*       MAINLEN=LENGTH(VMNTLST);    /* LENGTH OF PARM LIST   @Y30LSPS*/
         MVC   MAINLEN(3),@CF00084+1                               0741
*       CALL GETMAIN;               /* GET CORE FOR LIST     @Y30LSPS*/
         BAL   @14,GETMAIN                                         0742
*       RESPECIFY                                                  0743
*        (R1,                                                      0743
*         R15,                                                     0743
*         R11) RESTRICTED;          /*                       @Y30LSPS*/
*       R1=MAINADDR;                /* POINT TO GOTTEN CORE  @Y30LSPS*/
         L     R1,MAINADDR                                         0744
*       R15=ADDR(UCBVOLI);          /* POINT TO VOLSER OF DE @Y30LSPS*/
         LA    R15,UCBVOLI(,RUCB)                                  0745
*       R11=ADDR(UCBCHA);           /* POINT TO UNIT ADDR    @Y30LSPS*/
         LA    R11,UCBCHA(,RUCB)                                   0746
*       GEN(ICBMNTDE MF=(E,(R1)),TYPE=MNT,VOLID=(R15),VUA=(R11));  0747
         ICBMNTDE MF=(E,(R1)),TYPE=MNT,VOLID=(R15),VUA=(R11)
*       IF RETCODE^=0 THEN          /* CHECK IF SUCCESSFUL   @Y30LSPS*/
         LTR   RETCODE,RETCODE                                     0748
         BZ    @RF00748                                            0748
*         DO;                       /* ERROR MSG IN NO MOUNT @Y30LSPS*/
*           VVOLLCNT=C1;            /* IND VOL NBR IN ERR    @Y30LSPS*/
         L     @14,C1                                              0750
         L     @05,VOLLPTR                                         0750
         STC   @14,VVOLLCNT(,@05)                                  0750
*           VVOLLSFC=RETCODE;       /* SAVE VIRTUAL DEV ERR  @Y30LSPS*/
         STC   RETCODE,VVOLLSFC(,@05)                              0751
*           VVOLLPDC=PP90;          /* ERROR ON SS/1 MNT     @Y30LSPS*/
         MVI   VVOLLPDC(@05),X'5A'                                 0752
*           VVOLLVFY(C1)=OFF;       /* DO NOT VERIFY VOLUME  @Y30LSPS*/
*           VVOLLMNT(C1)=OFF;       /* IND VOLUME NOT MNTED  @Y30LSPS*/
         MH    @14,@CH00033                                        0754
         ALR   @05,@14                                             0754
         NI    VVOLLVFY-12(@05),B'00111111'                        0754
*           UCBDMCT=0;              /* NO VOLUME NO COUNT    @Y30LSPS*/
         MVI   UCBDMCT(RUCB),X'00'                                 0755
*           UCBVOLI=UCBVOLI&&UCBVOLI;/* CLEAR VOLSER IN UCB  @Y30LSPS*/
         XC    UCBVOLI(6,RUCB),UCBVOLI(RUCB)                       0756
*           UCBMOUNT=OFF;           /* CLEAR VOL BEING MNTED @Y30LSPS*/
         NI    UCBMOUNT(RUCB),B'01111111'                          0757
*           IF EOVDM=ON THEN        /* VMT ENTRY FOUND       @Y30DSMD*/
         TM    EOVDM,B'00100000'                                   0758
         BNO   @RF00758                                            0758
*             DO;                   /*                       @Y30DSMD*/
*               VMTVOL(CTR)=VMTVOL(CTR)&&VMTVOL(CTR);/* YES, CLEAR 0760
*                                                            @Y30DSMD*/
         L     @14,VMTPT                                           0760
         SLR   @05,@05                                             0760
         IC    @05,CTR                                             0760
         SLA   @05,4                                               0760
         ST    @05,@TF00001                                        0760
         ALR   @05,@14                                             0760
         XC    VMTVOL-16(16,@05),VMTVOL-16(@05)                    0760
*               VMTUCB(CTR)=RUCB;   /* INIT UCB PTR          @Y30DSMD*/
         L     @05,@TF00001                                        0761
         ST    RUCB,VMTUCB-16(@05,@14)                             0761
*             END;                  /*                       @Y30DSMD*/
*         END;                      /* END IF VIRTUAL MNT ER #Y30LSPS*/
*       ELSE                        /* RELAX NO ERR NOTHING  @Y30LSPS*/
*         ;                                                        0764
@RF00748 DS    0H                                                  0765
*       RESPECIFY                                                  0765
*         R0 RESTRICTED;            /*                       @ZA33246*/
@RC00748 DS    0H                                                  0766
*       R0=LENGTH(VMNTLST);         /* GET LENGTH OF LIST    @Y30LSPS*/
         LA    R0,28                                               0766
*       R1=MAINADDR;                /* POINT TO LIST AGAIN   @Y30LSPS*/
         L     R1,MAINADDR                                         0767
*       GEN(FREEMAIN R,LV=(0),A=(1));/* FREE MNT LIST        @Y30LSPS*/
         FREEMAIN R,LV=(0),A=(1)
*       IF VVOLLPDC^=0 THEN         /* CK IF MOUNT OK        @Y30LSPS*/
         L     @14,VOLLPTR                                         0769
         CLI   VVOLLPDC(@14),0                                     0769
         BNE   @RT00769                                            0769
*         GOTO EXIT;                /* EXIT IF ERROR ON MNT  @Y30LSPS*/
*       RESPECIFY                                                  0771
*        (R0,                                                      0771
*         R1,                                                      0771
*         R15,                                                     0771
*         R11) UNRESTRICTED;        /*                       @ZA33246*/
*     END;                          /* END IF VIRTUAL MOUNT  @Y30LSPS*/
*   ELSE                            /* NORMAL DEVICE MOUNT   @Y30LSPS*/
*     DO;                           /* NORMAL MOUNT MSG      @Y30LSPS*/
         B     @RC00739                                            0773
@RF00739 DS    0H                                                  0774
*       MSGDPTR=ADDR(M01A);         /* PT TO MOUNT MSG DESCRIPTION   */
         LA    @11,M01A                                            0774
         ST    @11,MSGDPTR                                         0774
*       CALL SEND;                  /* WRITE IT TO OPERATOR  @Y30LSPS*/
         BAL   @14,SEND                                            0775
*     END;                          /* END IF NORMAL MOUNT   @Y30LSPS*/
*   RESPECIFY                                                      0777
*     RET RESTRICTED;               /*                       @ZA33246*/
@RC00739 DS    0H                                                  0778
*   RET=RSAVLC;                     /* RESTORE RETURN ADDRESS        */
         L     RET,RSAVLC                                          0778
*   RESPECIFY                                                      0779
*     RET UNRESTRICTED;             /*                       @ZA33246*/
*   END MOUNT;                                                     0780
@EL00008 DS    0H                                                  0780
@EF00008 DS    0H                                                  0780
@ER00008 BR    @14                                                 0780
*DISMOUNT:                                                         0781
*                                                                  0781
*/********************************************************************/
*/*                                                                  */
*/*   FUNCTION  - SETS POINTER TO DEMOUNT OR RETAIN MESSAGE          */
*/*               AND CALLS SEND TO ISSUE MESSAGE.                   */
*/*                                                                  */
*/********************************************************************/
