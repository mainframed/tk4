         L     @05,VOLLPTR                                         0342
         LH    R0,VVOLLNBR(,@05)                                   0342
         SLA   R0,2                                                0342
*           R1=DOMLISTP;            /* POINT TO BLOCK                */
         LR    R1,@11                                              0343
