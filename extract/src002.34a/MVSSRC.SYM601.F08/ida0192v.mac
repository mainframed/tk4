@RF00344 DS    0H                                                  0347
*       NOMOUNT=OFF;                /* SET ERROR SWITCH OFF          */
@RF00340 NI    NOMOUNT,B'11101111'                                 0347
*       IF VVOLLPDC=0 THEN          /* IF NO PREVIOUS ERROR  @Y30LSPS*/
         L     @11,VOLLPTR                                         0348
         CLI   VVOLLPDC(@11),0                                     0348
         BNE   @RF00348                                            0348
*         DO C1=1 TO VVOLLNBR;      /* LOOP THRU VOLUME LIST         */
         LA    @11,1                                               0349
         B     @DE00349                                            0349
@DL00349 DS    0H                                                  0350
*           IF VVOLLMNT(C1)=OFF THEN/* IF VOLUME NOT MOUNTED,        */
         MH    @11,@CH00033                                        0350
         L     @05,VOLLPTR                                         0350
         ALR   @05,@11                                             0350
         TM    VVOLLMNT-12(@05),B'10000000'                        0350
         BNZ   @RF00350                                            0350
*             NOMOUNT=ON;           /* THEN SET SWITCH               */
         OI    NOMOUNT,B'00010000'                                 0351
*         END;                                                     0352
@RF00350 LA    @11,1                                               0352
         AL    @11,C1                                              0352
@DE00349 ST    @11,C1                                              0352
         L     @05,VOLLPTR                                         0352
         CH    @11,VVOLLNBR(,@05)                                  0352
         BNH   @DL00349                                            0352
*       IF MWAJS3ID=ON THEN         /* IF JES3 SUPPORT CORE GOTTEN 0353
