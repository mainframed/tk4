*           R0=LENGTH(IDAVUCBL)+    /* FIND CORE LENGTH              */
*               VUCBLNBR*LENGTH(VUCBLENT);                         0321
         L     @11,UCBLPTR                                         0321
         LH    R0,VUCBLNBR(,@11)                                   0321
         SLA   R0,2                                                0321
         AL    R0,@CF00064                                         0321
*           R1=ADDR(IDAVUCBL);      /* POINT TO CORE                 */
         LR    R1,@11                                              0322
*           IF ACVTDBUG=OFF THEN    /* IF OK TO FREEMAIN             */
         L     @11,CVTPTR                                          0323
         TM    ACVTDBUG(@11),B'00000010'                           0323
         BNZ   @RF00323                                            0323
*             GEN(FREEMAIN R,LV=(0),A=(1));/* FREE THE WORK AREA     */
         FREEMAIN R,LV=(0),A=(1)
