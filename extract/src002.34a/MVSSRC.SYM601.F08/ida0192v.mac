         L     R1,CCHHRPTR                                         0336
*           IF ACVTDBUG=OFF THEN    /* IF OK TO FREEMAIN             */
         L     @11,CVTPTR                                          0337
         TM    ACVTDBUG(@11),B'00000010'                           0337
         BNZ   @RF00337                                            0337
*             GEN(FREEMAIN R,LV=(0),A=(1));/* FREE THE WORK AREA     */
         FREEMAIN R,LV=(0),A=(1)
*         END;                                                     0339
@RF00337 DS    0H                                                  0340
*       IF DOMLISTP^=0 THEN         /* IF DOMLIST BUILT              */
@RF00333 L     @11,DOMLISTP                                        0340
         LTR   @11,@11                                             0340
         BZ    @RF00340                                            0340
