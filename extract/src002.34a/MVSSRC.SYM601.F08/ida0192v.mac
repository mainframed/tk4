*   DXCCW1(1)='31'X;                /* SET SEARCH ID COMMAND CODE    */
         MVI   DXCCW1,X'31'                                        0493
*   DXCCW1(5)='40'X;                /* SET COMMAND CHAIN BIT         */
         MVI   DXCCW1+4,X'40'                                      0494
*   DXCCW1(8)='05'X;                /* SET LENGTH                    */
         MVI   DXCCW1+7,X'05'                                      0495
*   DXCCW2(1:4)=ADDR(DXCCW1);                                      0496
         LA    @05,DXCCW1                                          0496
         STCM  @05,15,DXCCW2                                       0496
*   DXCCW2(1)='08'X;                /* SET TIC COMMAND CODE          */
         MVI   DXCCW2,X'08'                                        0497
*   DXCCW2(5)='40'X;                /* SET COMMAND CHAIN BIT         */
         MVI   DXCCW2+4,X'40'                                      0498
*   DXCCW3(1:4)=ADDR(DXLBL);        /* SET ADDRESS OF READ DATA      */
         LA    @05,DXLBL                                           0499
         STCM  @05,15,DXCCW3                                       0499
*   DXCCW3(1)='06'X;                /* SET READ COMMAND CODE         */
         MVI   DXCCW3,X'06'                                        0500
*   DXCCW3(8)='50'X;                /* SET LENGTH OF DATA            */
         MVI   DXCCW3+7,X'50'                                      0501
*   DEBSTRCC(1:2)='0000'X;          /* ZERO STARTING CYLINDER        */
         MVC   DEBSTRCC(2,@11),@CB04489                            0502
*   DEBSTRHH(1:2)='0000'X;          /* ZERO STARTING HEAD            */
         MVC   DEBSTRHH(2,@11),@CB04489                            0503
*   DEBENDHH(1:2)='0000'X;          /* ZERO ENDING HEAD              */
         MVC   DEBENDHH(2,@11),@CB04489                            0504
*   DEBENDCC(1:2)='7FFF'X;          /* SET END CYLINDER NUMBER       */
         MVC   DEBENDCC(2,@11),@CB04490                            0505
*   DEBNMTRK(1:2)='7FFF'X;          /* SET NUMBER OF TRACKS          */
         MVC   DEBNMTRK(2,@11),@CB04490                            0506
*                                                                  0507
*   /*****************************************************************/
*   /*                                                               */
*   /* VERIFY THE VOLUMES THAT HAVE BEEN MOUNTED                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0507
*   DO C1=1 TO VVOLLNBR;            /* LOOP THRU VOLUME LIST         */
         LA    @11,1                                               0507
         B     @DE00507                                            0507
@DL00507 DS    0H                                                  0508
*     IF VVOLLVFY(C1)=ON THEN       /* IF VOL NEEDS VERIFICATION     */
         L     @05,VOLLPTR                                         0508
