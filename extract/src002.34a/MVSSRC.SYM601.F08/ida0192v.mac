*   RESPECIFY                                                      0471
*     RET RESTRICTED;               /*                       @ZA33246*/
*   RSAVLA=RET;                     /* SAVE RETURN ADDRESS           */
         ST    RET,RSAVLA                                          0472
*   RESPECIFY                                                      0473
*     RET UNRESTRICTED;             /*                       @ZA33246*/
*   DXCB=DXCB&&DXCB;                /* ZERO CONTROL BLOCK AREA       */
*                                                                  0474
         XC    DXCB(200),DXCB                                      0474
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP CONTROL BLOCK BASES AND BUILD CHANNEL PROGRAMS         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0475
*   IOBSTPTR=ADDR(DXCB);            /* IOB IS FIRST IN CB            */
         LA    @11,DXCB                                            0475
         ST    @11,IOBSTPTR                                        0475
*   DEBPTR=IOBSTPTR+LENGTH(IOBSTDRD);/* DEB IS SECOND                */
         LA    @14,32                                              0476
         LR    @09,@11                                             0476
         ALR   @09,@14                                             0476
         ST    @09,DEBPTR                                          0476
*   DCBPTR=DEBPTR+LENGTH(DEBBASIC); /* DCB IS THIRD                  */
         ALR   @14,@09                                             0477
         LR    DCBPTR,@14                                          0477
*   DCBDEBAD=ADDR(DEBBASIC);        /* DCB POINTS TO DEB             */
         STCM  @09,15,DCBDEBAD(DCBPTR)                             0478
