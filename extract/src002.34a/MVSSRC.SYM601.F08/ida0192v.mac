@RC00451 BAL   @14,FREELOCK                                        0462
*           END;                                                   0463
*       END;                                                       0464
@RF00442 LA    @05,1                                               0464
         AL    @05,C2                                              0464
@DE00441 ST    @05,C2                                              0464
         L     @01,UCBLPTR                                         0464
         CH    @05,VUCBLNBR(,@01)                                  0464
         BNH   @DL00441                                            0464
@DC00441 DS    0H                                                  0465
