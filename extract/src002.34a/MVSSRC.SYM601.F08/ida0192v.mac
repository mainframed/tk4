JSCBAOS1 EQU  72
JSCBAOS2 EQU 192
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL 0
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED
*                                       IF NO ERROR WAS ENCOUNTERED
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA
         MVCL  @00,@14                  0 THE GOTTEN AREA, R15=0
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA
         ST    @01,88(,@15)             STORE LENGTH OF AREA
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE
         B     GMN00200                 BYPASS SETTING ERROR
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR 0
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR
*        0 IF THE GETMAIN WAS UNSUCCESSFUL.
*   IF RETCODE=0 THEN               /* IF GETMAIN UNSUCCESSFUL       */
         LTR   RETCODE,RETCODE                                     0238
         BNZ   @RF00238                                            0238
*     DO;                                                          0239
*       VVOLLERR=GMFAIL;            /* INIT PROB DET INFO    @Y30DSMD*/
         MVC   VVOLLERR(4,R1),GMFAIL                               0240
*       RETCODE=8;                  /* LOAD RETURN CODE OF 8 @Y30DSMD*/
         LA    RETCODE,8                                           0241
*     END;                                                         0242
*   ELSE                            /* IF GETMAIN SUCCESSFUL         */
*     DO;                                                          0243
         B     @RC00238                                            0243
@RF00238 DS    0H                                                  0244
*       GEN(ST R13,4(R15));         /* SAVE SAVE AREA ADDRESS        */
         ST R13,4(R15)
*       GEN( L R0,20(,R13));        /* IDAGMAIN USES R0, RESTORE IT.
*                                                            @Z30SSMD*/
          L R0,20(,R13)
*       GEN( L R1,24(,R13));        /* IDAGMAIN USES R1, RESTORE IT.
*                                                            @Z30SSMD*/
          L R1,24(,R13)
*       R13=R15;                    /* LOAD PTR TO AUTOMATIC CORE    */
         LR    R13,R15                                             0247
*       GEN(USING @DATD,R13);       /* GET ADDRESSING                */
         USING @DATD,R13
*       RESPECIFY                                                  0249
*         R6 RESTRICTED;                                           0249
*       R6=R1;                      /* COPY PARM PTR                 */
         LR    R6,R1                                               0250
*       RESPECIFY                                                  0251
*         IDAVVOLL BASED(R6);       /*                       @Y30LSPS*/
*       VVOLLFNC=VOLINIT;           /* SET FUNCTION CODE     @Y30LSPS*/
         MVI   VVOLLFNC(R6),X'14'                                  0252
*       RESPECIFY                                                  0253
*         IDAVUCBL BASED(R6);                                      0253
*       IF VUCBLID=ON THEN          /* IF USER PASSED UCB LIST       */
         TM    VUCBLID(R6),B'10000000'                             0254
         BNO   @RF00254                                            0254
*         DO;                       /* INIT LIST PTRS                */
*           UCBLPTR=R6;             /* INIT UCB LIST PTR             */
         ST    R6,UCBLPTR                                          0256
*           VOLLPTR=VUCBLVLA;       /* INIT VOLUME LIST PTR          */
         L     @14,VUCBLVLA(,R6)                                   0257
         ST    @14,VOLLPTR                                         0257
*         END;                                                     0258
*       ELSE                        /* IF USER PASSED VOLUME LIST    */
*         DO;                                                      0259
         B     @RC00254                                            0259
@RF00254 DS    0H                                                  0260
*           RESPECIFY                                              0260
*             IDAVVOLL BASED(R6);                                  0260
*           VOLLPTR=R6;             /* INIT VOLUME LIST PTR          */
         ST    R6,VOLLPTR                                          0261
*           CALL BLDUCBL;           /* BUILD UCB LIST                */
         BAL   @14,BLDUCBL                                         0262
*         END;                                                     0263
*       RESPECIFY                                                  0264
*        (R1,                                                      0264
*         R6,                                                      0264
*         RETCODE) UNRESTRICTED;    /*                       @ZA33246*/
@RC00254 DS    0H                                                  0265
*       RESPECIFY                                                  0265
*         IDAVUCBL BASED(UCBLPTR);                                 0265
*       RESPECIFY                                                  0266
*         IDAVVOLL BASED(VOLLPTR);                                 0266
*       DO C1=1 TO VVOLLNBR;        /* LOOP THRU VOLUME LIST         */
         LA    @11,1                                               0267
         B     @DE00267                                            0267
@DL00267 DS    0H                                                  0268
*         VVOLLMNT(C1)=OFF;         /* RESET MOUNTED BIT             */
*         VVOLLVFY(C1)=OFF;         /* RESET VERIFIED BIT            */
         LR    @05,@11                                             0269
         MH    @05,@CH00033                                        0269
         L     @01,VOLLPTR                                         0269
         ALR   @01,@05                                             0269
         NI    VVOLLMNT-12(@01),B'00111111'                        0269
*       END;                                                       0270
         AL    @11,@CF00062                                        0270
@DE00267 ST    @11,C1                                              0270
         L     @05,VOLLPTR                                         0270
         CH    @11,VVOLLNBR(,@05)                                  0270
         BNH   @DL00267                                            0270
*       MAINLEN=LENGTH(CCHHRF4)*VVOLLNBR;/* SET LENGTH OF CCHHR LIST */
         L     @11,VOLLPTR                                         0271
         LH    @11,VVOLLNBR(,@11)                                  0271
         MH    @11,@CH00882                                        0271
         STCM  @11,7,MAINLEN                                       0271
*       CALL GETMAIN;               /* GET CORE FOR CCHHR LIST       */
         BAL   @14,GETMAIN                                         0272
*       CCHHRCRE=ON;                /* INDICATE GETMAIN OK           */
         OI    CCHHRCRE,B'00100000'                                0273
*       CCHHRPTR=MAINADDR;          /* SET UP BASE FOR CCHHR LIST    */
         L     @11,MAINADDR                                        0274
         ST    @11,CCHHRPTR                                        0274
*       VVOLLFNC=PDF23;             /* SET FUNCTION CODE     @Y30DSMD*/
         L     @11,VOLLPTR                                         0275
         MVI   VVOLLFNC(@11),X'17'                                 0275
*       CALL DTMOUNT;               /* DETERMINE WHICH VOLUMES     0276
*                                      ALREADY MOUNTED               */
         BAL   @14,DTMOUNT                                         0276
*       IF MWAMTCNT^=0 THEN         /* CHECK IF ANY VOLUMES NEEDED 0277
*                                      MOUNTING              @ZA33246*/
         SLR   @11,@11                                             0277
         C     @11,MWAMTCNT                                        0277
         BE    @RF00277                                            0277
*         DO;                       /*                       @ZA33246*/
*           IF MWAJS3CT^=0 THEN     /* CHECK IF ANY OF THE UCB'S TO
*                                      BE MOUNTED ARE MANAGED BY JES3
*                                                            @ZA33246*/
         C     @11,MWAJS3CT                                        0279
         BE    @RF00279                                            0279
*             DO;                   /*                       @ZA33246*/
*               MAINLEN=LENGTH(SSOB)+LENGTH(SSMO);/* SSOB & SSOB EXT
*                                      LENGTH                @ZA33246*/
         MVC   MAINLEN(3),@CF00094+1                               0281
*               CALL GETMAIN;       /* ISSUE GETMAIN         @ZA33246*/
         BAL   @14,GETMAIN                                         0282
*               MWAJS3ID=ON;        /* INDICATE CORE GOTTEN FOR JES3
*                                      PROCESSING            @ZA33246*/
         OI    MWAJS3ID,B'10000000'                                0283
*               MWASSOB=MAINADDR;   /* POINT TO GOTTEN CORE  @ZA33246*/
         L     @11,MAINADDR                                        0284
         STCM  @11,7,MWASSOB                                       0284
*               SSOBLEN=LENGTH(SSOB);/* SET SSOB LENGTH      @ZA33246*/
         LA    @05,20                                              0285
         STH   @05,SSOBLEN(,@11)                                   0285
*               MWASSMO=MWASSOB+SSOBLEN;/* PT TO SSOB EXTENSION    0286
*                                                            @ZA33246*/
         ALR   @05,@11                                             0286
         ST    @05,MWASSMO                                         0286
*               SSOBID='SSOB';      /* SET SSOB ID           @ZA33246*/
         MVC   SSOBID(4,@11),@CC04458                              0287
*               SSOBFUNC=SSOBOEOV;  /* INDICATE MOUNT DISMOUNT     0288
*                                      FUNCTION              @ZA33246*/
         MVC   SSOBFUNC(2,@11),@CH01746                            0288
*               SSOBINDV=MWASSMO;   /* POINT TO THE MOUNT/DISMOUNT 0289
*                                      FUNCTION EXTENSION    @ZA33246*/
         ST    @05,SSOBINDV(,@11)                                  0289
*               SSMOPSTN=ADDR(SCTSNUMB);/* PTR TO STEP NUMBER      0290
*                                                            @ZA33246*/
         LA    @11,16                                              0290
         L     @01,CVTPTR                                          0290
         L     @01,CVTTCBP(,@01)                                   0290
         L     @01,TCBPTR(,@01)                                    0290
         L     @15,TCBJSCB(,@01)                                   0290
         L     @14,JSCSCT(,@15)                                    0290
         ALR   @14,@11                                             0290
         LA    @14,SCTSNUMB(,@14)                                  0290
         ST    @14,SSMOPSTN(,@05)                                  0290
*               SSMOLEN=LENGTH(SSMO);/* SIZE OF EXTENSION    @ZA33246*/
         MVC   SSMOLEN(2,@05),@CH00084                             0291
*               MWAFGFIN=ON;        /* INIT FINAL CALL BIT   @ZA33246*/
         OI    MWAFGFIN,B'01000000'                                0292
*               SSMOFLG1=MWAFLAG;   /* SET FLAG BYTE         @ZA33246*/
         MVC   SSMOFLG1(1,@05),MWAFLAG                             0293
*               SSMOPNAM=ADDR(JCTJNAME);/* PTR TO JOB NAME   @ZA33246*/
         AL    @11,JSCBJCT(,@15)                                   0294
         LA    @11,JCTJNAME(,@11)                                  0294
         ST    @11,SSMOPNAM(,@05)                                  0294
*             END;                  /*                       @ZA33246*/
*           CALL GOMOUNT;           /* MOUNT VOLUMES         @ZA33246*/
@RF00279 BAL   @14,GOMOUNT                                         0296
*         END;                      /*                       @ZA33246*/
*       CALL GOVERIFY;              /* VERIFY VOLUMES                */
@RF00277 BAL   @14,GOVERIFY                                        0298
*       IF VVOLLACB^=0 THEN         /* IF PTR TO CATALOG ACB EXISTS  */
         L     @11,VOLLPTR                                         0299
         L     @05,VVOLLACB(,@11)                                  0299
         LTR   @05,@05                                             0299
         BZ    @RF00299                                            0299
*         DO;                                                      0300
*           VVOLLFNC=PDF21;         /* SET FUNCTION CODE     @Y30DSMD*/
         MVI   VVOLLFNC(@11),X'15'                                 0301
*           CALL TSCHK;             /* DO TIMESTAMP CHECK            */
         BAL   @14,TSCHK                                           0302
*         END;                                                     0303
