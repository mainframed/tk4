@RF00348 TM    MWAJS3ID,B'10000000'                                0353
         BNO   @RF00353                                            0353
*         DO;                       /*                       @ZA33246*/
*           R0=SSOBLEN+SSMOLEN;     /* CORE LENGTH           @ZA33246*/
         L     @11,MWASSOB-1                                       0355
         LA    @11,0(,@11)                                         0355
         LH    R0,SSOBLEN(,@11)                                    0355
         L     @05,MWASSMO                                         0355
         AH    R0,SSMOLEN(,@05)                                    0355
*           R1=MWASSOB;             /* PT TO ADDRESS OF CORE @ZA33246*/
         LR    R1,@11                                              0356
*           IF ACVTDBUG=OFF THEN    /* IF OKAY TO FREEMAIN   @ZA33246*/
         L     @11,CVTPTR                                          0357
         TM    ACVTDBUG(@11),B'00000010'                           0357
         BNZ   @RF00357                                            0357
*             GEN(FREEMAIN R,LV=(0),A=(1));/* FREE CORE      @ZA33246*/
         FREEMAIN R,LV=(0),A=(1)
*         END;                      /*                       @ZA33246*/
@RF00357 DS    0H                                                  0360
*       RESPECIFY                                                  0360
*        (R6,                                                      0360
*         R7) RESTRICTED;           /*                       @Y30LSPS*/
@RF00353 DS    0H                                                  0361
*       IF NOMOUNT=ON THEN          /* VOLUMES NOT MOUNTED   @Z30SSRL*/
         TM    NOMOUNT,B'00010000'                                 0361
         BNO   @RF00361                                            0361
