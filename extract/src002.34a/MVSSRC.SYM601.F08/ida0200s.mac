*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**       FUNCTION = RESET DCB                                         *
**       THE DCB IS RESTORED TO ITS PRE-OPEN STATUS.  THIS INVOLVES   *
**       CLEARING THE FIELDS MERGED BY ISAM INTERFACE OPEN AND        *
**       MOVING THE DCB FOUNDATION SEGMENT FIELDS TO THE PRE-OPEN     *
**       FORMAT.                                                      *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         DCBBFA                                                     *
**         DCBBLKSI                                                   *
**         DCBBUFL                                                    *
**         DCBBUFNO                                                   *
**         DCBDEVT                                                    *
**         DCBDDNAM                                                   *
**         DCBIFLG                                                    *
**         DCBKEYLE                                                   *
**         DCBLRECL                                                   *
**         DCBMACR                                                    *
**         DCBNCRHI                                                   *
**         DCBOFLGS                                                   *
**         DCBOPTCD                                                   *
**         DCBRECFM                                                   *
**         DCBRKP                                                     *
**         DCBRORG2                                                   *
**         DCBSVCXA                                                   *
**         DCBWKPT1                                                   *
**         DCBWKPT5                                                   *
**         DCBWKPT6                                                   *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0425
*RESETDCB:                                                         0425
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0425
RESETDCB DS    0H                                                  0426
*   IF MRECFM=ON THEN               /* IF RECFM SET BY OPEN THEN     */
         TM    MRECFM(RIICB),B'00001000'                           0426
         BNO   @RF00426                                            0426
*     DCBRECFM=ZERO;                /* SET DCBRECFM TO ZERO          */
         MVI   DCBRECFM+28(RDCB),X'00'                             0427
*   IF MOPTCD=ON THEN               /* IF OPTCD SET BY OPEN THEN     */
@RF00426 TM    MOPTCD(RIICB),B'00010000'                           0428
         BNO   @RF00428                                            0428
*     DCBOPTCD=ZERO;                /* SET DCBOPTCD TO ZERO          */
         MVI   DCBOPTCD+52(RDCB),X'00'                             0429
*   IF MKEYLE=ON THEN               /* IF KEYLE SET BY OPEN THEN     */
@RF00428 TM    MKEYLE(RIICB),B'00000001'                           0430
         BNO   @RF00430                                            0430
*     DCBKEYLE=ZERO;                /* SET DCBKEYLE TO ZERO          */
         MVI   DCBKEYLE+16(RDCB),X'00'                             0431
*   IF MRKP=ON THEN                 /* IF RKP SET BY OPEN THEN       */
@RF00430 TM    MRKP(RIICB),B'10000000'                             0432
         BNO   @RF00432                                            0432
*     DCBRKP=ZERO;                  /* SET DCBRKP TO ZERO            */
         SLR   @15,@15                                             0433
         STH   @15,DCBRKP+52(,RDCB)                                0433
*   IF MLRECL=ON THEN               /* IF LRECL SET BY OPEN THEN     */
@RF00432 TM    MLRECL(RIICB),B'01000000'                           0434
         BNO   @RF00434                                            0434
*     DCBLRECL=ZERO;                /* SET DCBLRECL TO ZERO          */
         SLR   @15,@15                                             0435
         STH   @15,DCBLRECL+52(,RDCB)                              0435
*   IF MBLKSI=ON THEN               /* IF BLKSI SET BY OPEN THEN     */
@RF00434 TM    MBLKSI(RIICB),B'00100000'                           0436
         BNO   @RF00436                                            0436
*     DCBBLKSI=ZERO;                /* SET DCBBLKSI TO ZERO          */
         SLR   @15,@15                                             0437
         STH   @15,DCBBLKSI+52(,RDCB)                              0437
*   IF MBFALN=ON THEN               /* IF BFALN SET BY OPEN THEN     */
@RF00436 TM    MBFALN(RIICB),B'10000000'                           0438
         BNO   @RF00438                                            0438
*     DCBBFA=BZERO;                 /* SET DCBBFA TO ZERO            */
         NI    DCBBFA+28(RDCB),B'11111100'                         0439
*   IF MBUFNO=ON THEN               /* IF BUFNO SET BY OPEN THEN     */
@RF00438 TM    MBUFNO(RIICB),B'00000010'                           0440
         BNO   @RF00440                                            0440
*     DCBBUFNO=ZERO;                /* SET DCBBUFNO TO ZERO          */
         MVI   DCBBUFNO+16(RDCB),X'00'                             0441
*   IF MBUFL=ON THEN                /* IF BUFL SET BY OPEN THEN      */
@RF00440 TM    MBUFL(RIICB),B'00000100'                            0442
         BNO   @RF00442                                            0442
*     DCBBUFL=ZERO;                 /* SET DCBBUFL TO ZERO           */
*                                                                  0443
         SLR   @15,@15                                             0443
         STH   @15,DCBBUFL+16(,RDCB)                               0443
*   /*****************************************************************/
*   /*                                                               */
*   /* RESTORE THE DCB FOUNDATION SEGMENT TO PRE-OPEN STATUS         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0444
*   DCBMACR=DCBMACRF;               /* MOVE THE MACRO FORMAT         */
@RF00442 LA    @15,40                                              0444
         ALR   @15,RDCB                                            0444
         MVC   DCBMACR(2,@15),DCBMACRF(@15)                        0444
*   DCBIFLG=DCBIFLGS;               /* MOVE THE DCB I FLAGS          */
         MVC   DCBIFLG(1,@15),DCBIFLGS(@15)                        0445
*   RESPECIFY                                                      0446
*     RTIOT RESTRICTED;                                            0446
*   RTIOT=TIOTPTR+DCBTIOT;          /* LOCATE TIOT DD ENTRY          */
         L     @10,CVTPTR                                          0447
         L     @10,CVTTCBDW(,@10)                                  0447
         L     @10,TCBPTR(,@10)                                    0447
         LH    RTIOT,DCBTIOT(,@15)                                 0447
         AL    RTIOT,TIOTPTR(,@10)                                 0447
*   DCBDDNAM=TIOEDDNM;              /* RESTORE DDNAME IN DCB         */
         MVC   DCBDDNAM(8,@15),TIOEDDNM(RTIOT)                     0448
*   RESPECIFY                                                      0449
*     RTIOT UNRESTRICTED;                                          0449
*   DCBDEVT=ZERO;                   /* CLEAR DEVICE TYPE             */
         MVI   DCBDEVT+16(RDCB),X'00'                              0450
*   DCBNCRHI=ZERO;                  /* CLEAR DCBNCRHI                */
         SLR   @10,@10                                             0451
         LA    @07,52                                              0451
         ALR   @07,RDCB                                            0451
         STH   @10,DCBNCRHI(,@07)                                  0451
*   DCBRORG2=ZERO;                  /* CLEAR DCB REORGANIZATION      */
         STH   @10,DCBRORG2(,@07)                                  0452
*   DCBSVCXA=ONE;                   /* CLEAR SVC EXIT LIST PTR       */
         LA    @10,1                                               0453
         STCM  @10,7,DCBSVCXA+28(RDCB)                             0453
*   DCBWKPT1=ONE;                   /* CLEAR DCB WORK AREA PTR       */
         ST    @10,DCBWKPT1(,@07)                                  0454
*   DCBWKPT5=ONE;                   /* CLEAR DCB WORK AREA PTR       */
         ST    @10,DCBWKPT5(,@07)                                  0455
*   DCBWKPT6=ONE;                   /* CLEAR DCB WORK AREA PTR SET 0456
*                                      DCB OPEN FLAGS                */
         ST    @10,DCBWKPT6(,@07)                                  0456
*   DCBOFLGS=ZERO;                  /* CLEAR OFLGS           @Y021130*/
         MVI   DCBOFLGS(@15),X'00'                                 0457
*   DCBOFOPN=OFF;                   /* OPEN BIT OFF - 0              */
*   DCBOFIOF=OFF;                   /* BUSY BIT OFF - 0              */
*   DCBOFUEX=ON;                    /* LOCK BIT OFF - 1              */
         OI    DCBOFUEX(@15),B'00000010'                           0460
         NI    DCBOFOPN(@15),B'11101110'                           0460
*   END RESETDCB;                                                  0461
@EL00009 DS    0H                                                  0461
@EF00009 DS    0H                                                  0461
@ER00009 BR    @14                                                 0461
*                                                                  0462
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = FREE WORK AREAS                                       *
**       THIS ROUTINE ISSUES A CALL TO THE FREEMAIN ROUTINE TO FREE   *
**       THE CORE OBTAINED BY THE ISAM INTERFACE OPEN MODULE FOR      *
**       THE IICB, ACB AND EXLST                                      *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       REGISTERS - PASSED TO FREEMAIN SUBROUTINE                    *
**         RFMADDR - R7 - ADDRESS OF CORE TO BE FREED                 *
**         RGMSP - R8 - SUBPOOL NUMBER FOR FREEMAIN                   *
**         RGMLEN - R9 - LENGTH OF CORE FOR FREEMAIN                  *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0462
*FREEWA:                                                           0462
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0462
FREEWA   DS    0H                                                  0463
*   RESPECIFY                                                      0463
*    (RLKRETSV,                                                    0463
*     RGMSP,                                                       0463
*     RGMLEN,                                                      0463
*     RFMADDR) RESTRICTED;          /*                       @Y021130*/
*   RLKRETSV=R14;                   /* SAVE RETURN ADDR      @Y021130*/
         LR    RLKRETSV,R14                                        0464
*   RGMLEN=LENGTH(IDAIICB)+         /* INIT LENGTH OF IICB PLUS      */
*       LENGTH(IFGACB)+             /* ACB PLUS                      */
*       20;                         /* EXIT LIST                     */
         LA    RGMLEN,264                                          0465
*   RGMSP=USERPOOL;                 /* INIT SUBPOOL          @Y021130*/
         LA    RGMSP,250                                           0466
*   RFMADDR=RIICB;                  /* INIT ADDRESS OF IICB,ACB    0467
*                                                            @Y021130*/
         LR    RFMADDR,RIICB                                       0467
*   DO;                             /* MODESET EXTKEY(DATAMGT)       */
*     SPKA(80);                     /* SET PSW KEY                   */
         LA    @15,80                                              0469
         SPKA  0(@15)                                              0469
*   END;                            /* MODESET EXTKEY(DATAMGT)     0470
*                                                            @Y021130*/
*                                                                  0470
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT - INDICATE THAT AUDIT NO LONGER ACTIVE          @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0471
*   IIAUDPTR=AUDEND;                /* MARK WITH X'FFFFFFFF' @Y021130*/
         MVC   IIAUDPTR(4,RCORE),@CF02234                          0471
*   CALL FREEMAIN;                  /* CALL FREEMAIN                 */
         BAL   @14,FREEMAIN                                        0472
*   R14=RLKRETSV;                   /* RESTORE RET ADDR      @Y021130*/
         LR    R14,RLKRETSV                                        0473
*   RESPECIFY                                                      0474
*    (RLKRETSV,                                                    0474
*     RGMSP,                                                       0474
*     RGMLEN,                                                      0474
*     RFMADDR) UNRESTRICTED;        /*                       @Y021130*/
*   END FREEWA;                                                    0475
@EL00010 DS    0H                                                  0475
@EF00010 DS    0H                                                  0475
@ER00010 BR    @14                                                 0475
