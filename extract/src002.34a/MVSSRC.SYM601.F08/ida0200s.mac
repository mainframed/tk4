         TITLE 'IDA0200S - MVM ISAM INTERFACE CLOSE                    *
                        '
IDA0200S CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IDA0200S  78.038'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*                                                                  0153
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT - INDICATE AUDIT NOT STARTED                    @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0154
*   IIAUDPTR=ZERO;                  /* ZERO IICB PTR         @Y021130*/
         SLR   @15,@15                                             0154
         ST    @15,IIAUDPTR(,RCORE)                                0154
*   IIRSAVE=R13;                    /* PTR TO SAVE AREA              */
         ST    R13,IIRSAVE(,RCORE)                                 0155
*   RESPECIFY                                                      0156
*     R13 UNRESTRICTED;             /*                       @Y021130*/
*   RDCB=DXUDCBAD;                  /* INIT DCB POINTER TO DCB     0157
*                                                            @Y021130
*                                      COMMON CLOSE ISSUES A DEBCHK
*                                      VERIFY AND STORES THE DEB PTR
*                                      IN DCBDEBA                    */
         L     RDCB,DXUDCBAD(,RCORE)                               0157
*   RESPECIFY                                                      0158
*     IHADCB BASED(DXPDCBAD);       /* USE COPY DCB          @Y021130*/
*   RDEB=DCBDEBA;                   /* INIT DEB POINTER      @Y021130*/
         L     @15,DXPDCBAD(,RCORE)                                0159
         L     RDEB,DCBDEBA+39(,@15)                               0159
         LA    RDEB,0(,RDEB)                                       0159
*   RESPECIFY                                                      0160
*     IHADCB BASED(RDCB);           /* USER DCB              @Y021130*/
*   RIICB=DEBWKPT5;                 /* INIT IICBPOINTER              */
*                                                                  0161
         L     @15,DEBEXPT(,RDEB)                                  0161
         L     RIICB,DEBWKPT5(,@15)                                0161
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT - INITIALIZE POINTER TO IICB VIA A POINTER      @Y021130*/
*   /* LOCATED IN THE OPEN CLOSE WORK AREA                   @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0162
*   IIAUDPTR=RIICB;                 /* BASE FOR AUDIT        @Y021130*/
         ST    RIICB,IIAUDPTR(,RCORE)                              0162
*   DO;                             /* MODESET KEYADDR(DXUKEY)       */
*     SPKA(DXUKEY);                 /* SET PSW KEY                   */
         SLR   @15,@15                                             0164
         IC    @15,DXUKEY(,RCORE)                                  0164
         SPKA  0(@15)                                              0164
*   END;                            /* MODESET KEYADDR(DXUKEY) ASSUME
*                                      USER KEY              @Y021130*/
*   RACB=IIACBPTR;                  /* INIT ACB POINTER              */
         L     RACB,IIACBPTR(,RIICB)                               0166
*   CALL FLUSHBFR;                  /* FLUSH BUFFERS                 */
         BAL   @14,FLUSHBFR                                        0167
*   CALL CLOSEACB;                  /* CLOSE ACB             @YM04657*/
         BAL   @14,CLOSEACB                                        0168
*   CALL DELETRTN;                  /* DELETE II PROCESSING RTNS   0169
*                                                            @YM04657*/
         BAL   @14,DELETRTN                                        0169
*   CALL FREEBFRS;                  /* FREE II RPL'S AND BUFFERS     */
         BAL   @14,FREEBFRS                                        0170
*   CALL FREEDEB;                   /* FREE THE II DEB               */
         BAL   @14,FREEDEB                                         0171
*   CALL RESETDCB;                  /* RESET DCB TO PRE-OPEN COND    */
         BAL   @14,RESETDCB                                        0172
*   CALL FREEWA;                    /* FREE II WORK AREAS            */
         BAL   @14,FREEWA                                          0173
*   DO;                             /* MODESET EXTKEY(ZERO)          */
*     SPKA(0);                      /* SET PSW KEY                   */
         SLR   @15,@15                                             0175
         SPKA  0(@15)                                              0175
*   END;                            /* MODESET EXTKEY(ZERO) RESUME 0176
*                                      SYSTEM KEY FOR RETURN @Y021130*/
*   PCOPYDCB=ISAMDCB;               /* REFRESH THE COPY WITH USER DCB
*                                      AS COMMON CLOSE REFRESHES THE
*                                      USER DCB FROM THE COPY      0177
*                                                            @Y021130*/
         L     @15,DXPDCBAD(,RCORE)                                0177
         MVC   PCOPYDCB(236,@15),ISAMDCB+16(RDCB)                  0177
*   RESPECIFY                                                      0178
*     R13 RESTRICTED;                                              0178
*   R13=IIRSAVE;                    /* RESTORE SAVE AREA PTR         */
         L     R13,IIRSAVE(,RCORE)                                 0179
*   RETURN;                                                        0180
@EL00001 DS    0H                                                  0180
@EF00001 DS    0H                                                  0180
@ER00001 LM    @14,@12,12(@13)                                     0180
         BR    @14                                                 0180
*                                                                  0181
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = FLUSH BUFFER                                          *
**       WHEN LOCATE MODE IS BEING USED BY THE ISAM PROBLEM PROGRAM   *
**       THIS ROUTINE WILL PASS CONTROL TO THE ISAM INTERFACE         *
**       LOAD ROUTINE TO ISSUE THE LAST PUT MACRO TO VSAM.            *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**       REGISTER 6 - RACB - ACB POINTER                              *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         DCBST                                                      *
**         IIFCLOSE                                                   *
**         XRBSAV                                                     *
**         DCBDEBAD (COPY DCB - FORMERLY DXPDEBAD)                    *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = DATA MANAGEMENT KEY 5                 *
**                              SYSTEM KEY ZERO                       *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0181
*FLUSHBFR:                                                         0181
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0181
FLUSHBFR DS    0H                                                  0182
*   RESPECIFY                                                      0182
*     R15 RESTRICTED;                                              0182
*   RESPECIFY                                                      0183
*    (RLKRETSV) RESTRICTED;         /*                       @Y021130*/
*   RLKRETSV=R14;                   /* SAVE RETURN ADDR      @Y021130*/
         LR    RLKRETSV,R14                                        0184
*   IF ACBOPEN=ON&                  /* IF ACB STILL OPEN COULD BE  0185
*                                      PREVIOUSLY CLOSED BY ABEND OR
*                                      TASK TERM                     */
*       DCBMFLCP=ON&                /* AND PUT LOCATE                */
*       DCBSTKSQ=ON&                /* NOT THE FIRST PUT ISSUED      */
*       DCBEXCD1=ZERO&              /* AND NO ERRORS INDICATED       */
*       DCBEXCD2=ZERO THEN          /* IN THE DCB THEN               */
         TM    ACBOPEN(RACB),B'00010000'                           0185
         BNO   @RF00185                                            0185
         TM    DCBMFLCP+40(RDCB),B'00001000'                       0185
         BNO   @RF00185                                            0185
         LA    @07,52                                              0185
         ALR   @07,RDCB                                            0185
         TM    DCBSTKSQ(@07),B'01000000'                           0185
         BNO   @RF00185                                            0185
         CLI   DCBEXCD1(@07),0                                     0185
         BNE   @RF00185                                            0185
         CLI   DCBEXCD2(@07),0                                     0185
         BNE   @RF00185                                            0185
*     DO;                                                          0186
*       IIFCLOSE=ON;                /* TURN ON CLOSE BIT             */
         OI    IIFCLOSE(RIICB),B'00010000'                         0187
*       R14=ADDR(SVRB);             /* GET ADDR OF SVRB              */
         L     @07,CVTPTR                                          0188
         L     @07,CVTTCBDW(,@07)                                  0188
         L     @07,TCBPTR(,@07)                                    0188
         L     R14,SVRBPTR(,@07)                                   0188
*       R15=DCBIIPUT+26;            /* ADDR OF II PUT RTN PLUS     0189
*                                      DISPLACEMENT TO BRANCH IN PM1
*                                      AFTER STM INST AT BALR        */
         LA    R15,26                                              0189
         L     @07,DCBIIPUT+47(,RDCB)                              0189
         LA    @07,0(,@07)                                         0189
         ALR   R15,@07                                             0189
*       DO;                         /* MODESET EXTKEY(ZERO)          */
*         SPKA(0);                  /* SET PSW KEY                   */
         SLR   @07,@07                                             0191
         SPKA  0(@07)                                              0191
*       END;                        /* MODESET EXTKEY(ZERO) SET KEY
*                                      ZERO FOR REG SAVE IN SVRB   0192
*                                                            @Y021130*/
*       GEN SETS(XRBXSAV);                                         0193
           STM R2,R13,XRBXSAV(R14)      /* SAVE REGS IN EXTEND SAVE A*/
*       DO;                         /* MODESET KEYADDR(DXUKEY)       */
*         SPKA(DXUKEY);             /* SET PSW KEY                   */
         SLR   @07,@07                                             0195
         IC    @07,DXUKEY(,RCORE)                                  0195
         SPKA  0(@07)                                              0195
*       END;                        /* MODESET KEYADDR(DXUKEY) GET 0196
*                                      INTO USER KEY         @Y021130*/
*                                                                  0196
*       /*************************************************************/
*       /*                                                           */
*       /* AUDIT - INDICATE PROCESSING MOD IDAIIPM1 HAS BEEN ENTERED */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0197
*       AUDBFREX=ON;                /* FLAG IN IICB          @Y021130*/
         OI    AUDBFREX(RIICB),B'00010000'                         0197
*       GEN;                                                       0198
*                                       /*                   @Y021130*/
           SYNCH (15)                    /* GO TO II PUT             */
*       R14=ADDR(SVRB);             /* GET ADDR OF SVRB              */
         L     @07,CVTPTR                                          0199
         L     @07,CVTTCBDW(,@07)                                  0199
         L     @07,TCBPTR(,@07)                                    0199
         L     R14,SVRBPTR(,@07)                                   0199
*       GEN REFS(XRBXSAV);                                         0200
*                                                                  0200
*                                       /*                   @Y021130*/
           LM  R2,R13,XRBXSAV(R14)      /* RELOAD REGISTERS          */
*       /*************************************************************/
*       /*                                                           */
*       /* AUDIT - CONTROL RETURNED FROM PROCESSING MODULE IDAIIPM1  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0201
*       AUDBFRRT=ON;                /* FLAG IN IICB          @Y021130*/
         OI    AUDBFRRT(RIICB),B'00001000'                         0201
*       RDCB=DXUDCBAD;              /* RESTORE DCB PTR       @Y021130*/
         L     RDCB,DXUDCBAD(,RCORE)                               0202
*       DO;                         /* MODESET EXTKEY(DATAMGT)       */
*         SPKA(80);                 /* SET PSW KEY                   */
         LA    @07,80                                              0204
         SPKA  0(@07)                                              0204
*       END;                        /* MODESET EXTKEY(DATAMGT) DATA
*                                      MANAGEMENT KEY FOR STORING  0205
*                                      INTO COPY DCB         @Y021130*/
*       GEN;                                                       0206
           DEBCHK (1),TYPE=VERIFY,AM=ISAM /* VERIFY THE DEB POINTER  */
*       RESPECIFY                                                  0207
*         IHADCB BASED(DXPDCBAD);   /* COPY DCB              @Y021130*/
*       RDEB=R1;                    /* INIT DEB POINTER      @Y021130*/
         LR    RDEB,R1                                             0208
*       DCBDEBA=RDEB;               /* STORE VALID DEB PTR IN WA   0209
*                                      PROTECTED DCB         @Y021130*/
         L     @07,DXPDCBAD(,RCORE)                                0209
         STCM  RDEB,7,DCBDEBA+40(@07)                              0209
*       RESPECIFY                                                  0210
*         IHADCB BASED(RDCB);       /* USER DCB              @Y021130*/
*       DO;                         /* MODESET KEYADDR(DXUKEY)       */
*         SPKA(DXUKEY);             /* SET PSW KEY                   */
         SLR   @07,@07                                             0212
         IC    @07,DXUKEY(,RCORE)                                  0212
         SPKA  0(@07)                                              0212
*       END;                        /* MODESET KEYADDR(DXUKEY) RESUME
*                                      USER KEY              @Y021130*/
*       RDCB=DXUDCBAD;              /* RESTORE DCB PTR       @Y021130*/
         L     RDCB,DXUDCBAD(,RCORE)                               0214
*     END;                                                         0215
*   IF AMDNLR>ZERO THEN             /* IF DATA SET HAS RECORDS       */
@RF00185 L     @07,ACBAMBL(,RACB)                                  0216
         L     @07,AMBLDTA(,@07)                                   0216
         L     @07,AMBDSB(,@07)                                    0216
         L     @07,AMDNLR(,@07)                                    0216
         LTR   @07,@07                                             0216
         BNP   @RF00216                                            0216
*     DCBSTLOD=ON;                  /* TURN ON RESUME LOAD BIT       */
         OI    DCBSTLOD+52(RDCB),B'00100000'                       0217
*   R14=RLKRETSV;                   /* RESTORE RET ADDR      @Y021130*/
@RF00216 LR    R14,RLKRETSV                                        0218
*   RESPECIFY                                                      0219
*     R15 UNRESTRICTED;                                            0219
*   RESPECIFY                                                      0220
*    (RLKRETSV) UNRESTRICTED;       /*                       @Y021130*/
*   END FLUSHBFR;                                                  0221
@EL00002 DS    0H                                                  0221
@EF00002 DS    0H                                                  0221
@ER00002 BR    @14                                                 0221
*                                                                  0222
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = DELETE ROUTINE                                        *
**       THIS ROUTINE DELETES THE ISAM INTERFACE PROCESSING AND       *
**       SYNAD MODULES AND ZEROS THE DCB VECTORS THAT POINT           *
**       TO THE ROUTINES.  THE ISAM SYNAD ROUTINE SPECIFIED IN THE    *
**       AMP PARAMETER IS ALSO DELETED.                               *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         DCBESETL                                                   *
**         DCBFREED                                                   *
**         DCBIIGET                                                   *
**         DCBIIPUT                                                   *
**         DCBLRAN                                                    *
**         DCBLWKN                                                    *
**         DCBSETL                                                    *
**         DCBSYNAD                                                   *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0222
*DELETRTN:                                                         0222
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0222
DELETRTN DS    0H                                                  0223
*   RESPECIFY                                                      0223
*     RWORK2 RESTRICTED;                                           0223
*   RESPECIFY                                                      0224
*     IHADCB BASED(RWORK2);                                        0224
*   RESPECIFY                                                      0225
*    (RLKRETSV) RESTRICTED;         /*                       @Y021130*/
*   RLKRETSV=R14;                   /* SAVE RET ADDRESS      @Y021130*/
         LR    RLKRETSV,R14                                        0226
*   RWORK2=RDCB;                    /* SET DCB BASE                  */
*                                                                  0227
         LR    RWORK2,RDCB                                         0227
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT - INDICATE THAT DELETE OF IDAIISM1 WAS ATTEMPTED        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0228
*   AUDIISYN=OFF;                   /* INDICATOR OFF         @Y021130*/
         NI    AUDIISYN(RIICB),B'11111101'                         0228
*   GEN(DELETE EP=IDAIISM1);        /* DELETE II SYNAD               */
         DELETE EP=IDAIISM1
*   IF DCBMFGET=ON THEN             /* IF QISAM SCAN MODE THEN       */
         TM    DCBMFGET+40(RWORK2),B'01000000'                     0230
         BNO   @RF00230                                            0230
*     DO;                                                          0231
*                                                                  0231
*       /*************************************************************/
*       /*                                                           */
*       /* AUDIT - INDICATE THAT DELETE OF IDAIIPM2 WAS ATTEMPTED    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0232
*       AUDPRMOD=MODPRDEL;          /* INDICATOR OFF         @Y021130*/
         NI    AUDPRMOD(RIICB),B'11110011'                         0232
*       GEN(DELETE EP=IDAIIPM2);    /* DELETE II QISAM SCAN RTN      */
         DELETE EP=IDAIIPM2
*       DCBIIGET=ZERO;              /* CLEAR GET ADDR IN DCB         */
         SLR   @15,@15                                             0234
         STCM  @15,7,DCBIIGET+48(RWORK2)                           0234
*       DCBSETL=ONE;                /* CLEAR SETL ADDR IN DCB        */
         LA    @15,1                                               0235
         LA    @05,52                                              0235
         ALR   @05,RWORK2                                          0235
         ST    @15,DCBSETL(,@05)                                   0235
*       DCBESETL=ONE;               /* CLEAR ESETL ADDR IN DCB       */
         ST    @15,DCBESETL(,@05)                                  0236
*     END;                                                         0237
*   ELSE                                                           0238
*     IF DCBMFPUT=ON THEN           /* IF QISAM PUT                  */
         B     @RC00230                                            0238
@RF00230 TM    DCBMFPUT+40(RWORK2),B'01000000'                     0238
         BNO   @RF00238                                            0238
*       DO;                                                        0239
*                                                                  0239
*         /***********************************************************/
*         /*                                                         */
*         /* AUDIT - INDICATE THAT DELETE OF IDAIIPM1 WAS ATTEMPTED  */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0240
*         AUDPRMOD=MODPRDEL;        /* INDICATOR OFF         @Y021130*/
         NI    AUDPRMOD(RIICB),B'11110011'                         0240
*         GEN(DELETE EP=IDAIIPM1);  /* DELETE II PUT ROUTINE         */
         DELETE EP=IDAIIPM1
*         DCBIIPUT=ZERO;            /* CLEAR PUT ADDR IN DCB         */
         SLR   @15,@15                                             0242
         STCM  @15,7,DCBIIPUT+48(RWORK2)                           0242
*       END;                        /* IF BISAM THEN                 */
*   IF DCBMFRD=ON×DCBMFWRT=ON THEN                                 0244
@RF00238 DS    0H                                                  0244
@RC00230 LA    @15,40                                              0244
         ALR   @15,RWORK2                                          0244
         TM    DCBMFRD(@15),B'00100000'                            0244
         BO    @RT00244                                            0244
         TM    DCBMFWRT(@15),B'00100000'                           0244
         BNO   @RF00244                                            0244
@RT00244 DS    0H                                                  0245
*     DO;                                                          0245
*                                                                  0245
*       /*************************************************************/
*       /*                                                           */
*       /* AUDIT - INDICATE THAT DELETE OF IDAIIPM3 WAS ATTEMPTED    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0246
*       AUDPRMOD=MODPRDEL;          /* INDICATOR OFF         @Y021130*/
         NI    AUDPRMOD(RIICB),B'11110011'                         0246
*       GEN(DELETE EP=IDAIIPM3);    /* DELETE II BISAM ROUTINE       */
         DELETE EP=IDAIIPM3
*       DCBSETL=ONE;                /* CLEAR CHECK RTN ADDR - DCB    */
         LA    @15,1                                               0248
         LA    @05,52                                              0248
         ALR   @05,RWORK2                                          0248
         ST    @15,DCBSETL(,@05)                                   0248
*       DCBLRAN=ONE;                /* CLEAR READ/WRITE RNT ADDR     */
         ST    @15,DCBLRAN(,@05)                                   0249
*       DCBLWKN=ONE;                /* CLEAR WRITE KN RTN ADDR       */
         ST    @15,DCBLWKN(,@05)                                   0250
*       IF DCBMFDBF=ON THEN         /* IF DYNAMIC BUFFERING          */
         TM    DCBMFDBF+40(RWORK2),B'00000100'                     0251
         BNO   @RF00251                                            0251
*         DO;                                                      0252
*                                                                  0252
*           /*********************************************************/
*           /*                                                       */
*           /* AUDIT - INDICATE THAT DELETE OF IDAIIFBF WAS ATTEMPTED*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0253
*           AUDIIFBF=OFF;           /* INDICATOR OFF         @Y021130*/
         NI    AUDIIFBF(RIICB),B'01111111'                         0253
*           GEN(DELETE EP=IDAIIFBF);/* DELETE II FREEDBUF RTN        */
         DELETE EP=IDAIIFBF
*           DCBFREED=ONE;           /* CLEAR FREEDBUF ADDR IN DCB    */
         MVC   DCBFREED+52(4,RWORK2),@CF00087                      0255
*         END;                                                     0256
*     END;                                                         0257
@RF00251 DS    0H                                                  0258
*   IF IISAMSYN(1:4)^=ZERO THEN     /* IF AN ISAM SYNAD RTN          */
@RF00244 ICM   @15,15,IISAMSYN(RIICB)                              0258
         BZ    @RF00258                                            0258
*     DO;                           /* WAS SPECIFIED IN THE AMP      */
*       RESPECIFY                                                  0260
*         R8 RESTRICTED;                                           0260
*       R8=ADDR(IISAMSYN);          /* PARAMETER, THEN DELETE        */
*                                                                  0261
         LA    R8,IISAMSYN(,RIICB)                                 0261
*       /*************************************************************/
*       /*                                                           */
*       /* AUDIT - INDICATE THAT DELETE OF USER SYNAD WAS ATTEMPTED  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0262
*       AUDURSYN=OFF;               /* INDICATOR OFF         @Y021130*/
         NI    AUDURSYN(RIICB),B'11111110'                         0262
*       GEN(DELETE EPLOC=(R8));     /* IT FROM THE USER'S REGION     */
         DELETE EPLOC=(R8)
*       DCBSYNAD=ONE;               /* CLEAR SYNAD ADDR              */
         MVC   DCBSYNAD+52(4,RWORK2),@CF00087                      0264
*       RESPECIFY                                                  0265
*         R8 UNRESTRICTED;                                         0265
*     END;                                                         0266
*   RDCB=RWORK2;                    /* RESTORE DCB POINTER           */
@RF00258 LR    RDCB,RWORK2                                         0267
*   R14=RLKRETSV;                   /* RESTORE RET ADDR      @Y021130*/
         LR    R14,RLKRETSV                                        0268
*   RESPECIFY                                                      0269
*     IHADCB BASED(RDCB);                                          0269
*   RESPECIFY                                                      0270
*     RWORK2 UNRESTRICTED;                                         0270
*   RESPECIFY                                                      0271
*    (RLKRETSV) UNRESTRICTED;       /*                       @Y021130*/
*   END DELETRTN;                                                  0272
@EL00003 DS    0H                                                  0272
@EF00003 DS    0H                                                  0272
@ER00003 BR    @14                                                 0272
*                                                                  0273
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = CLOSE ACB                                             *
**       THIS ROUTINE CLOSES THE ISAM INTERFACE ACB THAT WAS          *
**       USED FOR THIS DCB.                                           *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         DXPDEBAD                                                   *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = DATA MANAGEMENT KEY 5                 *
**                              SYSTEM KEY ZERO                       *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0273
*CLOSEACB:                                                         0273
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0273
CLOSEACB DS    0H                                                  0274
*   RESPECIFY                                                      0274
*    (RWORK2) RESTRICTED;                                          0274
*   RESPECIFY                                                      0275
*    (RLKRETSV,                                                    0275
*     RWORK3) RESTRICTED;           /*                       @YM04657*/
*   RESPECIFY                                                      0276
*    (R0,                                                          0276
*     R15) RESTRICTED;              /*                       @ZA29184*/
*   RLKRETSV=R14;                   /* SAVE RET ADDRESS      @Y021130*/
         LR    RLKRETSV,R14                                        0277
*   RWORK2=ADDR(IIACBPTR);          /* CLOSE PARM LIST POINTER       */
         LA    RWORK2,IIACBPTR(,RIICB)                             0278
*   PARCOPT(1)=ON;                  /* LAST ACB IN LIST              */
         OI    PARCOPT(RPARC),B'10000000'                          0279
*   RWORK3=EXLSYNP;                 /* SAVE ADDR OF MODULE IDAIISM1
*                                                            @YM04657*/
         L     @05,ACBEXLST(,RACB)                                 0280
         ICM   RWORK3,15,EXLSYNP(@05)                              0280
*   EXLSYNP=ADDR(CLOSESYN);         /* COME BACK TO THIS MODULE IF 0281
*                                      ERROR DURING CLOSE    @YM04657*/
         LA    @09,CLOSESYN                                        0281
         STCM  @09,15,EXLSYNP(@05)                                 0281
*   EXLLERP=ADDR(CLOSESYN);         /* COME BACK TO THIS MODULE IF 0282
*                                      ERROR DURING CLOSE    @YM04657*/
         LA    @09,CLOSESYN                                        0282
         STCM  @09,15,EXLLERP(@05)                                 0282
*   IIFCLOSE=ON;                    /* INDICATE CLOSE IN CONTROL   0283
*                                      CONTROL               @YM04657*/
*                                                                  0283
         OI    IIFCLOSE(RIICB),B'00010000'                         0283
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT - INDICATE THAT A CLOSE WAS ISSUED ON ACB       @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0284
*   AUDACBCL=ON;                    /* FLAG IN IICB          @Y021130*/
         OI    AUDACBCL(RIICB),B'01000000'                         0284
*   R15=ADDR(SYNCHACB);             /* ADDR OF CLOSE RTN     @ZA29184*/
         LA    R15,SYNCHACB                                        0285
*   R14=ADDR(SVRB);                 /* GET SVRB ADDR         @ZA29184*/
         L     @09,CVTPTR                                          0286
         L     @09,CVTTCBDW(,@09)                                  0286
         L     @09,TCBPTR(,@09)                                    0286
         L     R14,SVRBPTR(,@09)                                   0286
*   SPKA(0);                        /* SET PSW KEY           @ZA29184*/
         SLR   @09,@09                                             0287
         SPKA  0(@09)                                              0287
*   GEN SETS(XRBXSAV);                                             0288
*                                                         /* @ZA29184*/
         STM R2,R13,XRBXSAV(R14)    /* SAVE REGS             @ZA29184*/
*   SPKA(DXUKEY);                   /* GET TO USER KEY       @ZA29184*/
         SLR   @09,@09                                             0289
         IC    @09,DXUKEY(,RCORE)                                  0289
         SPKA  0(@09)                                              0289
*   R0=RWORK2;                      /* PASS ACB ADDR         @ZA29184*/
         LR    R0,RWORK2                                           0290
*   GEN(SYNCH (15));                /* SYNCH TO CLOSE VSAM ACB IN PP
*                                      STATE                 @ZA29184*/
         SYNCH (15)
*   R14=ADDR(SVRB);                 /* GET SVRB ADDR         @ZA29184*/
         L     @09,CVTPTR                                          0292
         L     @09,CVTTCBDW(,@09)                                  0292
         L     @09,TCBPTR(,@09)                                    0292
         L     R14,SVRBPTR(,@09)                                   0292
*   GEN SETS(XRBXSAV);                                             0293
*                                                                  0293
*                                                         /* @ZA29184*/
         LM  R2,R13,XRBXSAV(R14)    /* RESTORE REGS          @ZA29184*/
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT - INDICATE THAT CONTROL HAS RETURNED FROM ACB CLOSE     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0294
*   AUDACBRC=ON;                    /* FLAG IN IICB          @Y021130*/
         OI    AUDACBRC(RIICB),B'00100000'                         0294
*   PARCOPT(1)=OFF;                 /* RESET THE CLOSE FLAGS         */
         NI    PARCOPT(RPARC),B'01111111'                          0295
*   RDCB=DXUDCBAD;                  /* RESTORE DCB PTR       @Y021130
*                                      VERIFY THE DEB POINTER        */
         L     RDCB,DXUDCBAD(,RCORE)                               0296
*   DO;                             /* MODESET EXTKEY(DATAMGT)       */
*     SPKA(80);                     /* SET PSW KEY                   */
         LA    @09,80                                              0298
         SPKA  0(@09)                                              0298
*   END;                            /* MODESET EXTKEY(DATAMGT) DATA
*                                      MANAGEMENT KEY FOR STORING  0299
*                                      INTO COPY DCB         @Y021130*/
*   GEN(DEBCHK (1),TYPE=VERIFY,AM=ISAM);                           0300
         DEBCHK (1),TYPE=VERIFY,AM=ISAM
*   RESPECIFY                                                      0301
*     IHADCB BASED(DXPDCBAD);       /* COPY DCB              @Y021130*/
*   RDEB=R1;                        /* INIT DEB POINTER      @Y021130*/
         LR    RDEB,R1                                             0302
*   DCBDEBA=RDEB;                   /* STORE VALID DEB PTR IN WA   0303
*                                      PROTECTED DCB         @Y021130*/
         L     @09,DXPDCBAD(,RCORE)                                0303
         STCM  RDEB,7,DCBDEBA+40(@09)                              0303
*   RESPECIFY                                                      0304
*     IHADCB BASED(RDCB);           /* USER DCB              @Y021130*/
*   DO;                             /* MODESET KEYADDR(DXUKEY)       */
*     SPKA(DXUKEY);                 /* SET PSW KEY                   */
         SLR   @09,@09                                             0306
         IC    @09,DXUKEY(,RCORE)                                  0306
         SPKA  0(@09)                                              0306
*   END;                            /* MODESET KEYADDR(DXUKEY) RESUME
*                                      USER KEY              @Y021130*/
*   RDCB=DXUDCBAD;                  /* RESTORE DCB PTR       @Y021130*/
         L     RDCB,DXUDCBAD(,RCORE)                               0308
*   R14=RLKRETSV;                   /* RESTORE RET ADDR      @Y021130*/
         LR    R14,RLKRETSV                                        0309
*   RESPECIFY                                                      0310
*    (RPARC) UNRESTRICTED;                                         0310
*   RESPECIFY                                                      0311
*    (RLKRETSV,                                                    0311
*     RWORK3) UNRESTRICTED;         /*                       @YM04657*/
*   RESPECIFY                                                      0312
*    (R0,                                                          0312
*     R15) UNRESTRICTED;            /*                       @ZA29184*/
*   END CLOSEACB;                                                  0313
@EL00004 DS    0H                                                  0313
@EF00004 DS    0H                                                  0313
@ER00004 BR    @14                                                 0313
*                                                                  0314
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = ISSUE THE CLOSE FOR THE ACB IN USER KEY, PROBLEM      *
**              PROGRAM STATE                                         *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 0 - ACB ADDRESS                                     *
**       REGISTER 14 - RETURN ADDRESS                                 *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTER 14 - RETURN ADDRESS                                 *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0314
*SYNCHACB:                          /*                       @ZA29184*/
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @ZA29184*/
SYNCHACB DS    0H                                                  0315
*   RESPECIFY                                                      0315
*    (R0,                                                          0315
*     R5,                                                          0315
*     RWORK2,                                                      0315
*     R15) RESTRICTED;              /*                       @ZA29184*/
*   R5=R14;                         /* SAVE RETURN ADDR      @ZA29184*/
         LR    R5,R14                                              0316
*   RWORK2=R0;                      /* GET ACB ADDR          @ZA29184*/
         LR    RWORK2,R0                                           0317
*   GEN(CLOSE MF=(E,(RWORK2)));     /* CLOSE ACB             @ZA29184*/
         CLOSE MF=(E,(RWORK2))
*   R14=R5;                         /* GET RETURN ADDR       @ZA29184*/
         LR    R14,R5                                              0319
*   RESPECIFY                                                      0320
*    (R0,                                                          0320
*     R5,                                                          0320
*     RWORK2,                                                      0320
*     R15) UNRESTRICTED;            /*                       @ZA29184*/
*   END SYNCHACB;                   /*                       @ZA29184*/
@EL00005 DS    0H                                                  0321
@EF00005 DS    0H                                                  0321
@ER00005 BR    @14                                                 0321
*                                                                  0322
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = CLOSE SYNAD                                           *
**       THIS ROUTINE IS USED AS A SYNAD EXIT FOR ERRORS OCCURRING    *
**       DURING CLOSE ON THE ACB.                                     *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RPL POINTER (THE RPL THAT VSAM CLOSE BUILDS)    *
**       REGISTER 2-12 - CONTENTS OF REGISTERS AS THEY EXISTED WHEN   *
**                       THE CLOSE WAS ISSUED (SEE CLOSEACB ROUTINE). *
**       REGISTER 13 - SAVE AREA ADDRESS (SAVE AREA PROVIDED BY       *
**                     VSAM CLOSE FOR RECORD MANAGEMENT).             *
**       REGISTER 14 - RETURN ADDRESS                                 *
**       REGISTER 15 - ADDR OF CLOSESYN ROUTINE. NOTE THAT REG 12     *
**                     CONTAINS THE BASE ADDRESS.                     *
**                                                                    *
**   OUTPUT = SEE BELOW                                               *
**       OUTPUT TO MODULE IDAIISM1 =                                  *
**            REGISTER 1 - RPL POINTER (ISAM INTERFACE RPL)           *
**            REGISTER 8 - RWORK3 - RETURN ADDR TO THIS MODULE        *
**            REGISTER 15 - ADDRESS OF MODULE IDAIISM1                *
**       OUTPUT ON RETURN TO VSAM CLOSE =                             *
**            REGISTER 1 - RPL POINTER (RPL THAT CLOSE BUILT)         *
**            REGISTER 13 - SAVE AREA POINTER THAT VSAM CLOSE PASSED  *
**            REGISTER 14 - RETURN ADDRESS                            *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0322
*CLOSESYN:                                                         0322
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @YM04657*/
CLOSESYN DS    0H                                                  0323
*   RESPECIFY                                                      0323
*    (RWORK1,                                                      0323
*     RWORK2,                                                      0323
*     RWORK3,                                                      0323
*     R13,                                                         0323
*     R15) RESTRICTED;                                             0323
*   RRPL=R1;                        /* SAVE RPL ADDR PASSED  @YM04657*/
         LR    RRPL,R1                                             0324
*   RWORK1=R14;                     /* SAVE RETURN ADDR      @YM04657*/
         LR    RWORK1,R14                                          0325
*   RWORK2=DXUDCBAD;                /* GET DCB ADDR FROM WORK AREA 0326
*                                                            @YM04657*/
         L     RWORK2,DXUDCBAD(,RCORE)                             0326
*   RESPECIFY                                                      0327
*     IHADCB BASED(RWORK2);         /*                       @YM04657*/
*                                                                  0327
*   /*****************************************************************/
*   /*                                                               */
*   /* THE USER SYNAD EXIT WILL ONLY BE TAKEN FOR QISAM, AN I/O ERROR*/
*   /* WAS DETECTED, AND A PREVIOUS EXIT WAS NOT TAKEN DURING CLOSE. */
*   /*                                                       @YM04657*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0328
*   IF(DCBMFPUT=ON×DCBMFGET=ON)&RPLERREG='0C'X&DCBEXCLD=OFF THEN   0328
         LA    @10,40                                              0328
         ALR   @10,RWORK2                                          0328
         TM    DCBMFPUT(@10),B'01000000'                           0328
         BO    @GL00007                                            0328
         TM    DCBMFGET(@10),B'01000000'                           0328
         BNO   @RF00328                                            0328
@GL00007 CLI   RPLERREG(RRPL),12                                   0328
         BNE   @RF00328                                            0328
         TM    DCBEXCLD+52(RWORK2),B'00100000'                     0328
         BNZ   @RF00328                                            0328
*     DO;                           /*                       @YM04657*/
*       R15=ADDR(IIREGSAV);         /* SAVE AREA IN IICB     @YM04657*/
         LA    R15,IIREGSAV(,RIICB)                                0330
*       GEN(STM R14,R12,12(R15));   /* SAVE REGS IN IICB     @YM04657*/
         STM R14,R12,12(R15)
*       R13=R15;                    /* USER MUST RETURN REG 13     0332
*                                                            @YM04657*/
         LR    R13,R15                                             0332
*       IIRPLPTR->RPLFDBK=RPLFDBK;  /* COPY RPL PASSED TO OWN      0333
*                                                            @YM04657*/
         L     @10,IIRPLPTR(,RIICB)                                0333
         MVC   RPLFDBK(3,@10),RPLFDBK(RRPL)                        0333
*       IIRPLPTR->RPLERMSA->IDAERMSG=IDAERMSG;/* COPY ERROR MESSAGE
*                                      TO OWN                @YM04657*/
         L     @09,RPLERMSA(,@10)                                  0334
         L     @05,RPLERMSA(,RRPL)                                 0334
         MVC   IDAERMSG(128,@09),IDAERMSG(@05)                     0334
*       R15=RWORK3;                 /* ADDR OF MODULE IDAIISM1     0335
*                                                            @YM04657*/
         LR    R15,RWORK3                                          0335
*       RWORK3=ADDR(SYNADRET);      /* RETURN TO THIS MODULE FROM  0336
*                                      USER EXIT             @YM04657*/
         LA    RWORK3,SYNADRET                                     0336
*       R1=IIRPLPTR;                /* PASS OWN RPL          @YM04657*/
         LR    R1,@10                                              0337
*       GO TO SYNAD;                /* GO ENTER MODULE IDAIISM1    0338
*                                                            @YM04657*/
         BR    R15                                                 0338
*SYNADRET:                                                         0339
*       GEN(LM R14,R12,12(R13));    /* RESTORE OWN REGS      @YM04657*/
SYNADRET LM R14,R12,12(R13)
*       R14=RWORK1;                 /* RETURN ADDR OF VSAM CLOSE   0340
*                                                            @YM04657*/
         LR    R14,RWORK1                                          0340
*       R1=RRPL;                    /* ADDR OF RPL PASSED    @YM04657*/
         LR    R1,RRPL                                             0341
*     END;                          /* END OF SYNAD EXIT     @YM04657*/
*   RESPECIFY                                                      0343
*     IHADCB BASED(RDCB);           /*                       @YM04657*/
*   RESPECIFY                                                      0344
*    (RWORK1,                                                      0344
*     RWORK2,                                                      0344
*     RWORK3,                                                      0344
*     R13,                                                         0344
*     R15) UNRESTRICTED;                                           0344
*   END CLOSESYN;                   /* END CLOSE SYNAD       @YM04657*/
@EL00006 DS    0H                                                  0345
@EF00006 DS    0H                                                  0345
@ER00006 BR    @14                                                 0345
*                                                                  0346
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**   FUNCTION = FREE BUFFERS                                          *
**       FREE THE CORE OBTAINED BY THE ISAM INTERFACE OPEN            *
**       MODULE FOR THE RPL, RPL EXTENSION, ERROR MESSAGE AREAS,      *
**       KEYSAVE AREA AND II BUFFERS.                                 *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       REGISTERS - PASSED TO FREEMAIN SUBROUTINE                    *
**         RFMADDR - R7 - ADDRESS OF CORE TO BE FREED                 *
**         RGMSP - R8 - SUBPOOL NUMBER FOR FREEMAIN                   *
**         RGMLEN - R9 - LENGTH OF CORE FOR FREEMAIN                  *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = NONE                                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0346
*FREEBFRS:                                                         0346
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0346
FREEBFRS DS    0H                                                  0347
*   RESPECIFY                                                      0347
*    (RLKRETSV,                                                    0347
*     RGMSP,                                                       0347
*     RGMLEN,                                                      0347
*     RFMADDR) RESTRICTED;          /*                       @Y021130*/
*   RLKRETSV=R14;                   /* SAVE RET ADDRESS      @Y021130*/
         LR    RLKRETSV,R14                                        0348
*   RGMLEN=IIRPLL+                  /* FREEMAIN LENGTH - RPL +       */
*       IIKEYSL;                    /* KEYSAVE               @Y021130*/
         L     RGMLEN,IIRPLL-1(,RIICB)                             0349
         LA    RGMLEN,0(,RGMLEN)                                   0349
         AH    RGMLEN,IIKEYSL(,RIICB)                              0349
*   IF IIQBFRS=OFF THEN             /* IF QISAM WITH BUFFERS         */
         TM    IIQBFRS(RIICB),B'00000001'                          0350
         BNZ   @RF00350                                            0350
*     RGMLEN=RGMLEN+IITBUFL;        /* ADD BUFFER LENGTH     @Y021130*/
         L     @15,IITBUFL-1(,RIICB)                               0351
         LA    @15,0(,@15)                                         0351
         ALR   RGMLEN,@15                                          0351
*   RGMSP=USERPOOL;                 /* INIT SUBPOOL OF RPL   @Y021130*/
@RF00350 LA    RGMSP,250                                           0352
*   RFMADDR=IIRPLPTR;               /* INIT ADDR OF CORE TO FREE   0353
*                                                            @Y021130*/
*                                                                  0353
         L     RFMADDR,IIRPLPTR(,RIICB)                            0353
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT - INDICATE AN ATTEMPT HAS BEEN MADE TO FREE BUFFERS     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0354
*   AUDCBFRS=ZERO;                  /* ZERO ADDR IN IICB     @Y021130*/
         SLR   @15,@15                                             0354
         ST    @15,AUDCBFRS(,RIICB)                                0354
*   CALL FREEMAIN;                  /* FREE ERROR MESSAGE AREAS      */
         BAL   @14,FREEMAIN                                        0355
*   RGMLEN=IIMSGL;                  /* INIT MSG AREA LENGTH  @Y021130*/
         L     RGMLEN,IIMSGL-1(,RIICB)                             0356
         LA    RGMLEN,0(,RGMLEN)                                   0356
*   RGMSP=USERPOOL;                 /* INIT SUBPOOL OF USER  @Y021130*/
         LA    RGMSP,250                                           0357
*   RFMADDR=IIMSGPTR;               /* INIT MSG AREA POINTER @Y021130*/
*                                                                  0358
         L     RFMADDR,IIMSGPTR(,RIICB)                            0358
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT - INDICATE AN ATTEMPT HAS BEEN MADE TO FREE THE MESSAGE */
*   /* AREA                                                  @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0359
*   AUDCMSGA=ZERO;                  /* ZERO ADDR IN IICB     @Y021130*/
         SLR   @15,@15                                             0359
         ST    @15,AUDCMSGA(,RIICB)                                0359
*   CALL FREEMAIN;                  /* CALL FREEMAIN ROUTINE         */
         BAL   @14,FREEMAIN                                        0360
*   R14=RLKRETSV;                   /* RESTORE RET ADDR      @Y021130*/
         LR    R14,RLKRETSV                                        0361
*   RESPECIFY                                                      0362
*    (RLKRETSV,                                                    0362
*     RGMSP,                                                       0362
*     RGMLEN,                                                      0362
*     RFMADDR) UNRESTRICTED;        /*                       @Y021130*/
*   END FREEBFRS;                                                  0363
@EL00007 DS    0H                                                  0363
@EF00007 DS    0H                                                  0363
@ER00007 BR    @14                                                 0363
*                                                                  0364
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = FREE DEB                                              *
**       THIS ROUTINE CALLS THE FREEMAIN ROUTINE TO FREE THE CORE     *
**       OBTAINED BY THE ISAM INTERFACE OPEN MODULE FOR THE           *
**       DUMMY DEB.                                                   *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 2 - RIICB - IICB POINTER                            *
**       REGISTER 11 - RDEB - DEB POINTER                             *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTERS - SAME AS ABOVE                                    *
**                                                                    *
**       REGISTERS - PASSED TO FREEMAIN SUBROUTINE                    *
**         RFMADDR - R7 - ADDRESS OF CORE TO BE FREED                 *
**         RGMSP - R8 - SUBPOOL NUMBER FOR FREEMAIN                   *
**         RGMLEN - R9 - LENGTH OF CORE FOR FREEMAIN                  *
**                                                                    *
**       CONTROL BLOCK FIELDS INITIALIZED                             *
**         TCBDEB                                                     *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = DATA MANAGMENT KEY 5                  *
**                              SYSTEM KEY 0                          *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0364
*FREEDEB:                                                          0364
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0364
FREEDEB  DS    0H                                                  0365
*   RESPECIFY                                                      0365
*    (RLKRETSV,                                                    0365
*     R12) RESTRICTED;              /*                       @Y021130*/
*   RLKRETSV=R14;                   /* SAVE RET ADDRESS      @Y021130*/
*                                                                  0366
         LR    RLKRETSV,R14                                        0366
*   /*****************************************************************/
*   /*                                                               */
*   /* UNCHAIN THE DEB                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0367
*   DO;                             /* MODESET EXTKEY(ZERO)          */
*     SPKA(0);                      /* SET PSW KEY                   */
         SLR   @15,@15                                             0368
         SPKA  0(@15)                                              0368
*   END;                            /* MODESET EXTKEY(ZERO) USE    0369
*                                      SYSTEM KEY FOR DEB UNCHAINING
*                                                            @Y021130*/
*   RESPECIFY                                                      0370
*     RWORK2 RESTRICTED;                                           0370
*   RESPECIFY                                                      0371
*    (R7,                                                          0371
*     R8) RESTRICTED;               /*                       @Y021130*/
*   R7=RDEB;                        /* SAVE CONTENTS         @Y021130*/
         LR    R7,RDEB                                             0372
*   R8=R12;                         /* SAVE CONTENTS         @Y021130*/
*                                                                  0373
         LR    R8,R12                                              0373
*   /*****************************************************************/
*   /*                                                               */
*   /* OBTAIN THE LOCAL MEMORY LOCK                          @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0374
*LOCK1:                             /* LOCAL LOCK FREED * + 3      0374
*                                                            @Y021130*/
*   GEN SETS(R11,R12,R13) REFS(PSALITA);                           0374
LOCK1    DS    0H                                                  0374
*                                                         /* @Y021130*/
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        *
               RELATED=(TCBDEBCH,(IDA0200S(UNLOCK1)))
*   R12=R8;                         /* RESTORE BASE          @Y021130*/
         LR    R12,R8                                              0375
*   RDEB=R7;                        /* RESTORE DEB PTR       @Y021130*/
*                                                                  0376
         LR    RDEB,R7                                             0376
*   /*****************************************************************/
*   /*                                                               */
*   /* UNCHAIN THE DUMMY DEB                                 @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0377
*   IF TCBDEB=RDEB THEN             /* UNCHAIN THE II DEB FROM       */
         L     @15,CVTPTR                                          0377
         L     @15,CVTTCBDW(,@15)                                  0377
         L     @15,TCBPTR(,@15)                                    0377
         C     RDEB,TCBDEB(,@15)                                   0377
         BNE   @RF00377                                            0377
*     DO;                           /* THE TCB DEB CHAIN     @Y021130*/
*       R8=RDEB;                    /* FOR COMPARE AND SWAP  @Y021130*/
         LR    R8,RDEB                                             0379
*       RWORK2=DEBDEBB;             /* FOR COMPARE AND SWAP  @Y021130*/
         L     RWORK2,DEBDEBB-1(,RDEB)                             0380
         LA    RWORK2,0(,RWORK2)                                   0380
*       CS(R8,RWORK2,TCBDEB);       /* REMOVE IF DEB IS FIRST ON   0381
*                                      CHAIN                 @Y021130*/
         CS    R8,@07,TCBDEB(@15)                                  0381
*       BC(8,UNLOCK1);              /* FINISHED              @Y021130*/
         BC    8,UNLOCK1                                           0382
*     END;                          /*                       @Y021130*/
*   RWORK2=TCBDEB;                  /* GET PTR TO FIRST ON CHAIN   0384
*                                                            @Y021130
*                                      LOOP SEARCHING FOR THE DEB  0384
*                                      THAT POINTS TO THE II DEB   0384
*                                                            @Y021130*/
@RF00377 L     @15,CVTPTR                                          0384
         L     @15,CVTTCBDW(,@15)                                  0384
         L     @15,TCBPTR(,@15)                                    0384
         L     RWORK2,TCBDEB(,@15)                                 0384
*   DO;                             /* MODESET EXTKEY(DATAMGT)       */
*     SPKA(80);                     /* SET PSW KEY                   */
         LA    @15,80                                              0386
         SPKA  0(@15)                                              0386
*   END;                            /* MODESET EXTKEY(DATAMGT) ASSUME
*                                      DATA MANAGEMENT KEY TO UPDATE
*                                      DEB                   @Y021130*/
*   DO WHILE RWORK2->DEBDEBB^=RDEB; /*                       @Y021130*/
         B     @DE00388                                            0388
@DL00388 DS    0H                                                  0389
*     IF RWORK2->DEBDEBB^=ZERO THEN /* POINT TO NEXT DEB IN CHN      */
         L     @15,DEBDEBB-1(,RWORK2)                              0389
         LA    @15,0(,@15)                                         0389
         LTR   @15,@15                                             0389
         BZ    @RF00389                                            0389
*       RWORK2=RWORK2->DEBDEBB;     /*                       @Y021130*/
         LR    RWORK2,@15                                          0390
*     ELSE                                                         0391
*       GO TO UNLOCK1;              /* IF DEB NOT FOUND RELEASE LOCAL
*                                      LOCK                  @Y021130*/
*   END;                            /*                       @Y021130
*                                      UNCHAIN II DEB                */
@DE00388 L     @15,DEBDEBB-1(,RWORK2)                              0392
         LA    @15,0(,@15)                                         0392
         CR    RDEB,@15                                            0392
         BNE   @DL00388                                            0392
*   RWORK2->DEBDEBB=DEBDEBB;        /*                       @Y021130*/
*                                                                  0393
         MVC   DEBDEBB(3,RWORK2),DEBDEBB(RDEB)                     0393
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE LOCAL MEMORY LOCK                                @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0394
*UNLOCK1:                           /* LOCAL LOCK ACQUIRED * - 3   0394
*                                                            @Y021130*/
*   DO;                             /* MODESET EXTKEY(ZERO)          */
UNLOCK1  DS    0H                                                  0395
*     SPKA(0);                      /* SET PSW KEY                   */
         SLR   @15,@15                                             0395
         SPKA  0(@15)                                              0395
*   END;                            /* MODESET EXTKEY(ZERO) ASSUME 0396
*                                      SYSTEM KEY FOR RELEASING LOCK
*                                                            @Y021130*/
*   R8=R12;                         /* SAVE CONTENTS         @Y021130*/
         LR    R8,R12                                              0397
*   R7=RDEB;                        /* SAVE CONTENTS         @Y021130*/
         LR    R7,RDEB                                             0398
*   GEN SETS(R11,R12,R13) REFS(PSALITA);                           0399
*                                                         /* @Y021130*/
        SETLOCK RELEASE,TYPE=LOCAL,RELATED=(TCBDEBCH,(IDA0200S(LOCK1)))
*   R12=R8;                         /* RESET BASE            @Y021130*/
         LR    R12,R8                                              0400
*   RDEB=R7;                        /* RESET DEB ADDR        @Y021130*/
         LR    RDEB,R7                                             0401
*   DO;                             /* MODESET KEYADDR(DXUKEY)       */
*     SPKA(DXUKEY);                 /* SET PSW KEY                   */
         SLR   @15,@15                                             0403
         IC    @15,DXUKEY(,RCORE)                                  0403
         SPKA  0(@15)                                              0403
*   END;                            /* MODESET KEYADDR(DXUKEY) RESUME
*                                      USER KEY              @Y021130*/
*   RDCB=DXUDCBAD;                  /* INIT DCB PTR FOR DEBCHK     0405
*                                                            @Y021130
*                                      ISSUE DEBCHK DELETE           */
         L     RDCB,DXUDCBAD(,RCORE)                               0405
*   GEN(DEBCHK (1),TYPE=DELETE,AM=ISAM);                           0406
         DEBCHK (1),TYPE=DELETE,AM=ISAM
*   RDCB=DXUDCBAD;                  /* RESTORE DCB PTR       @Y021130*/
         L     RDCB,DXUDCBAD(,RCORE)                               0407
*   RESPECIFY                                                      0408
*     RWORK2 UNRESTRICTED;                                         0408
*   RESPECIFY                                                      0409
*    (R7,                                                          0409
*     R8) UNRESTRICTED;             /*                       @Y021130*/
*                                                                  0409
*   /*****************************************************************/
*   /*                                                               */
*   /*                                                       @Y021130*/
*   /* FREE THE DEB EXTENSION                                @Y021130*/
*   /*                                                       @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0410
*   RESPECIFY                                                      0410
*    (RGMLEN,                                                      0410
*     RGMSP,                                                       0410
*     RFMADDR) RESTRICTED;          /*                       @Y021130*/
*   RFMADDR=DEBXTNP;                /* ADDR TO FREE          @Y021130*/
         LR    @15,RDEB                                            0411
         AL    @15,@CF02344                                        0411
         L     @15,DEBXTNP-28(,@15)                                0411
         LR    RFMADDR,@15                                         0411
*   RGMLEN=DEBXLNGH;                /* LENGTH EXTENSION      @Y021130*/
         LH    RGMLEN,DEBXLNGH(,@15)                               0412
*   RGMSP=DEBPOOL;                  /* SUBPOOL               @Y021130*/
*                                                                  0413
         LA    RGMSP,230                                           0413
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT - INDICATE AN ATTEMPT HAS BEEN MADE TO FREE THE DEB     */
*   /* EXTENSION                                             @Y021130*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0414
*   AUDDEBXF=ON;                    /* FLAG IN IICB          @Y021130*/
         OI    AUDDEBXF(RIICB),B'00000100'                         0414
*   CALL FREEMAIN;                  /* CALL FREEMAIN         @Y021130*/
*                                                                  0415
         BAL   @14,FREEMAIN                                        0415
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE THE CORE OBTAINED BY II OPEN FOR THE DEB                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0416
*   RGMLEN=DEBLNGTH*EIGHT;          /* INIT LENGTH OF DEB    @Y021130*/
         LR    @15,RDEB                                            0416
         SL    @15,@CF02327                                        0416
         SLR   RGMLEN,RGMLEN                                       0416
         IC    RGMLEN,DEBLNGTH(,@15)                               0416
         SLA   RGMLEN,3                                            0416
*   RGMSP=DEBPOOL;                  /* INIT SUBPOOL OF DEB   @Y021130*/
         LA    RGMSP,230                                           0417
*   RFMADDR=RDEB-LENGTH(DEB);       /* INIT ADDR OF DEB      @OZ02677*/
*                                                                  0418
         LR    RFMADDR,@15                                         0418
*   /*****************************************************************/
*   /*                                                               */
*   /* AUDIT - INDICATE AN ATTEMPT HAS BEEN MADE TO FREE DEB         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0419
*   AUDCDEB=ZERO;                   /* ZERO ADDR IN IICB     @Y021130*/
         SLR   @15,@15                                             0419
         ST    @15,AUDCDEB(,RIICB)                                 0419
*   CALL FREEMAIN;                  /* CALL FREEMAIN                 */
         BAL   @14,FREEMAIN                                        0420
*   R14=RLKRETSV;                   /* RESTORE RET ADDR      @Y021130*/
         LR    R14,RLKRETSV                                        0421
*   RESPECIFY                                                      0422
*    (RLKRETSV,                                                    0422
*     RGMLEN,                                                      0422
*     RGMSP,                                                       0422
*     RFMADDR) UNRESTRICTED;        /*                       @Y021130*/
*   RESPECIFY                                                      0423
*    (R12) UNRESTRICTED;            /*                       @Y021130*/
*   END FREEDEB;                                                   0424
@EL00008 DS    0H                                                  0424
@EF00008 DS    0H                                                  0424
@ER00008 BR    @14                                                 0424
