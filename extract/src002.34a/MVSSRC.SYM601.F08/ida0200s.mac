*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
**                                                                    *
**   FUNCTION = FREEMAIN                                              *
**       THIS ROUTINE IS CALLED BY THE FREEBFRS, FREEDEB AND FREEWA   *
**       ROUTINES IN THIS MODULE.  ALL ISAM INTERFACE CORE IS FREED   *
**       BY THIS ROUTINE.                                             *
**                                                                    *
**   INPUT =                                                          *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**       REGISTER 7 - RFMADDR = ADDRESS OF CORE TO BE FREED           *
**       REGISTER 8 - RGMSP = SUBPOOL NUMBER FOR FREEMAIN             *
**       REGISTER 9 - RGMLEN = LENGTH OF CORE FOR FREEMAIN            *
**                                                                    *
**   OUTPUT =                                                         *
**       REGISTER 1 - RDCB - DCB POINTER                              *
**                                                                    *
**   PROTECTION KEY = SEE BELOW                                       *
**       KEY ON ENTRY - USER KEY                                      *
**       KEY ON EXIT - USER KEY                                       *
**       INTERIM KEYS ASSUMED = DATA MANAGMENT KEY 5                  *
**                                                                    *
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
*                                                                  0476
*FREEMAIN:                                                         0476
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0476
FREEMAIN DS    0H                                                  0477
*   RESPECIFY                                                      0477
*    (R0) RESTRICTED;                                              0477
*   RESPECIFY                                                      0478
*    (RLKRETSV,                                                    0478
*     RGMSP,                                                       0478
*     RGMLEN,                                                      0478
*     RFMADDR) RESTRICTED;          /*                       @Y021130*/
*   SPKA(80);                       /*                       @Y021130*/
         LA    @15,80                                              0479
         SPKA  0(@15)                                              0479
*   MAINLEN=RGMLEN;                 /* SAVE LENGTH           @Y021130*/
         STCM  RGMLEN,7,MAINLEN(RCORE)                             0480
*   MAINPOOL=RGMSP;                 /* SAVE SUBPOOL          @Y021130*/
         STC   RGMSP,MAINPOOL(,RCORE)                              0481
*   MAINADDR=RFMADDR;               /* SAVE ADDR             @Y021130*/
         ST    RFMADDR,MAINADDR(,RCORE)                            0482
*   IIRSAVE2=R14;                   /* SAVE RETURN ADDRESS           */
         ST    R14,IIRSAVE2(,RCORE)                                0483
*   IIRSAVE3=RDCB;                                                 0484
         ST    RDCB,IIRSAVE3(,RCORE)                               0484
*   R0=MAINR0;                      /* INIT R0 - LENGTH/SUBPOOL      */
         L     R0,MAINR0(,RCORE)                                   0485
*   R1=MAINADDR;                    /* INIT R1 - ADDR OF CORE        */
         L     @15,MAINADDR(,RCORE)                                0486
         LR    R1,@15                                              0486
