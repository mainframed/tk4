         TITLE 'IDA0192W - VS2 VSAM CHANNEL PROGRAM AREA BUILD         *
                        '
IDA0192W CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IDA0192W  75.057'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @03,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@03                                         0001
RBPRFXLN EQU 64
RBPRFLNA EQU 32
PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 200
*                                                                  0246
*   /*****************************************************************/
*   /*                                                               */
*   /* IDA0192W MODULE INITIALIZATION                        @Z30SSDT*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0246
*   RSTATRTN=IDAR192W;              /* NOTE 192W IN CONTROL  @Z30SSDT*/
         L     @05,OPWCOMWA(,RCORE)                                0246
         MVC   RSTATRTN(4,@05),@CC03972                            0246
*/********************************************************************/
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/
*/********************************************************************/
*                                                                  0247
*   GEN;                                                           0247
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED
*                                       IF NO ERROR WAS ENCOUNTERED
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA
         ST    @01,88(,@15)             STORE LENGTH OF AREA
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE
         B     GMN00200                 BYPASS SETTING ERROR
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.
*   RESPECIFY                                                      0248
*    (R15,                                                         0248
*     R1,                                                          0248
*     R0,                                                          0248
*     R7) RSTD;                     /*                       @Z30SSDT*/
*   IF R15^=K0 THEN                 /* IF WORKING STORAGE OBTAINED   */
         LTR   R15,R15                                             0249
         BZ    @RF00249                                            0249
*     DO;                                                          0250
*       R15->SABWRD=R13;            /* SAVE SAVEREG          @Z30SSDT*/
         ST    R13,SABWRD(,R15)                                    0251
*       R7=ADDR(SACODE);            /* SAVE POINTER TO CALLERS R15   */
         LA    R7,SACODE(,R13)                                     0252
*       R13=R15;                    /* LOAD CORE PTR                 */
         LR    R13,R15                                             0253
*       GEN(USING @DATD,R13);                                      0254
         USING @DATD,R13
*       PDPTR=R7;                   /* LOAD POINTER TO R15   @X04SVCJ*/
         ST    R7,PDPTR                                            0255
*       PD=K0;                      /* SET RETURN TO ZERO    @Z30SSRL*/
         L     @14,PDPTR                                           0256
         SLR   @06,@06                                             0256
         ST    @06,PD(,@14)                                        0256
*       PPFUNC=PDF71;               /* FUNCTION ID           @X04SVCJ*/
         MVI   PPFUNC(@14),X'47'                                   0257
*       CALL BLDCPA;                /* BUILD CHANNEL PROGRAM AREA    */
         BAL   @14,BLDCPA                                          0258
*     END;                                                         0259
*   ELSE                            /* NO CORE AVAILABLE             */
*     DO;                                                          0260
         B     @RC00249                                            0260
@RF00249 DS    0H                                                  0261
*       RESPECIFY                                                  0261
*         PD BASED(ADDR(SACODE));   /* CALLERS REG15         @Z30SSDT*/
*       PD=K0;                      /* ZERO PD               @Z30SSRL*/
         LA    @14,16                                              0262
         ALR   @14,R13                                             0262
         SLR   @06,@06                                             0262
         ST    @06,PD(,@14)                                        0262
*       PPFUNC=PDF70;               /* SET FUNCTION CODE     @X04SVCJ*/
         MVI   PPFUNC(@14),X'46'                                   0263
*       PPCODE=PP20;                /* GETMAIN FAILURE       @X04SVCJ*/
         MVI   PPCODE(@14),X'14'                                   0264
*       LM(RET,R12,SAREGS);         /* LOAD R14-R12          @Z30SSDT*/
         LM    RET,R12,SAREGS(R13)                                 0265
*       GEN(BR RET);                                               0266
         BR RET
*     END;                                                         0267
*   RESPECIFY                                                      0268
*     PD BASED(PDPTR);              /*                       @X04SVCJ*/
@RC00249 DS    0H                                                  0269
*EXIT:                                                             0269
*   IF PPCODE=K0 THEN               /* ERRORS                @Z30SSRL*/
EXIT     L     @14,PDPTR                                           0269
         CLI   PPCODE(@14),0                                       0269
         BNE   @RF00269                                            0269
*     PD=K0;                        /* NO, ZERO RETURN CODE  @Z30SSRL*/
         SLR   @06,@06                                             0270
         ST    @06,PD(,@14)                                        0270
*   GEN(L R0,@SIZDATD);             /* LOAD BLOCK SIZE       @Z30SSDT*/
@RF00269 DS    0H                                                  0271
         L R0,@SIZDATD
*   R1=R13;                         /* SET ADDRESS           @Z30SSDT*/
         LR    R1,R13                                              0272
*   R13=SABWRD;                     /* POINT TO LAST SAVEAREA      0273
*                                                            @Z30SSDT*/
         L     R13,SABWRD(,R13)                                    0273
*   GEN(FREEMAIN R,LV=(0),A=(1));   /* FREE AUTOMATICS               */
         FREEMAIN R,LV=(0),A=(1)
*   LM(RET,R12,SAREGS);             /* RESTORE REGISTERS 14-12     0275
*                                                            @Z30SSDT*/
         LM    RET,R12,SAREGS(R13)                                 0275
*   GEN(BR RET);                    /* RETURN TO CALLER              */
         BR RET
*   RESPECIFY                                                      0277
*    (R15,                                                         0277
*     R1,                                                          0277
*     R0,                                                          0277
*     R7,                                                          0277
*     R12) UNRSTD;                  /*                       @Z30SSDT*/
*BLDCPA:                                                           0278
*                                                                  0278
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD CHANNEL PROGRAM AREA                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0278
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0278
BLDCPA   DS    0H                                                  0279
*   RESPECIFY                                                      0279
*    (R6,                                                          0279
*     R8,                                                          0279
*     R9,                                                          0279
*     RET) RESTRICTED;              /*                       @X04SVCJ*/
*   RSAVL(K1)=RET;                  /* SAVE RETURN ADDRESS   @X04SVCJ*/
         ST    RET,RSAVL                                           0280
*   RESPECIFY                                                      0281
*     RET UNRSTD;                   /*                       @X04SVCJ*/
*   PARMPTR=SABWRD->SAREG0;         /* LOAD POINTER TO PARMS @Z30SSDT*/
         L     @07,SABWRD(,R13)                                    0282
         L     PARMPTR,SAREG0(,@07)                                0282
*   RESPECIFY                                                      0283
*     IDABUFC BASED(R9);            /*                       @X04SVCJ*/
*   R9=BUFCPTR;                     /* LOAD POINTER TO BUFC  @X04SVCJ*/
         L     R9,BUFCPTR(,PARMPTR)                                0284
*   DO WHILE R9^=0;                 /* LOOP THROUGH BUFCS    @X04SVCJ*/
         B     @DE00285                                            0285
@DL00285 DS    0H                                                  0286
*     IF BUFCFMT=ON&                /* IF PREFORMAT SPEC &           */
*         BUFCPFCP=ON THEN          /* IF PREF COMPLETE              */
         TM    BUFCFMT(R9),B'01000010'                             0286
         BNO   @RF00286                                            0286
*       IF AMBTYPE=AMBINDEX&        /* IF INDEX AMB PASS     @Z30SSDT*/
*           AMDSDT=ON THEN          /* AND SS WITH DATA      @Z30SSDT*/
         L     @07,AMBPTR(,PARMPTR)                                0287
         TM    AMBTYPE(@07),B'01000000'                            0287
         BNO   @RF00287                                            0287
         L     @01,AMBDSB(,@07)                                    0287
         TM    AMDSDT(@01),B'00100000'                             0287
         BNO   @RF00287                                            0287
*         R6=MAX(LPMBLKTR,(ADDR(IDALPMB)+LPMBLEN)->LPMBLKTR);/* SET
*                                      MAX BLOCKS PER TRACK  @Z30SSDT*/
         L     @01,AMBEDB(,@07)                                    0288
         L     @15,EDBLPMBC(,@01)                                  0288
         LH    @14,LPMBLEN(,@15)                                   0288
         ALR   @14,@15                                             0288
         LH    R6,LPMBLKTR(,@15)                                   0288
         LH    @07,LPMBLKTR(,@14)                                  0288
         CR    R6,@07                                              0288
         BNL   *+6
         LR    R6,@07                                              0288
*       ELSE                        /* NOT INDEX & SS W/DATA @Z30SSDT*/
*         R6=LPMBLKTR;              /* FIND BLKS/TRK         @Z30SSDT*/
         B     @RC00287                                            0289
@RF00287 L     @01,AMBPTR(,PARMPTR)                                0289
         L     @01,AMBEDB(,@01)                                    0289
         L     @01,EDBLPMBC(,@01)                                  0289
         LH    R6,LPMBLKTR(,@01)                                   0289
*     ELSE                          /* NOT PREFORMAT & PREF NOT    0290
*                                                            @Z30SSRL*/
*       R6=AMDCINV/LPMBLKSZ;        /* FIND BLKS/CINV        @X04SVCJ*/
         B     @RC00286                                            0290
@RF00286 L     @15,AMBPTR(,PARMPTR)                                0290
         L     @01,AMBDSB(,@15)                                    0290
         L     @15,AMBEDB(,@15)                                    0290
         L     @15,EDBLPMBC(,@15)                                  0290
         L     @00,AMDCINV(,@01)                                   0290
         SRDA  @00,32                                              0290
         D     @00,LPMBLKSZ(,@15)                                  0290
         LR    R6,@01                                              0290
*     IF LPMBLKSZ>IOPAGESZ THEN     /* IF PHYSICAL BLKSIZE>PAGE    0291
*                                                            @Z30SSDT*/
@RC00286 L     @07,AMBPTR(,PARMPTR)                                0291
         L     @07,AMBEDB(,@07)                                    0291
         L     @07,EDBLPMBC(,@07)                                  0291
         L     @07,LPMBLKSZ(,@07)                                  0291
         C     @07,@CF00039                                        0291
         BNH   @RF00291                                            0291
*       R8=K12;                     /* SET IDAL ENTRY SIZE   @Z30SSRL*/
         LA    R8,12                                               0292
*     ELSE                          /* IF PHYSICAL BLKSIZE<=PAGE     */
*       R8=K8;                      /* SET IDAL ENTRY SIZE   @Z30SSRL*/
         B     @RC00291                                            0293
@RF00291 LA    R8,8                                                0293
*     R8=(R8*(R6+K7)&(-K8));        /* GET SIZE OF IDAL AND VPL    0294
*                                                            @X04SVCJ*/
@RC00291 LA    @01,7                                               0294
         ALR   @01,R6                                              0294
         MR    @00,R8                                              0294
         N     @01,@CF04051                                        0294
         LR    R8,@01                                              0294
*     CALL BLDCPAAR;                /* BUILD THE CHANNEL PROGRAM AREA
*                                                            @X04SVCJ*/
         BAL   @14,BLDCPAAR                                        0295
*     CALL INITCPAH;                /* BUILD CPA HEADER      @X04SVCJ*/
         BAL   @14,INITCPAH                                        0296
*     IF BUFCFMT=ON THEN            /* IF PREFORMAT SPEC     @X04SVCJ*/
         TM    BUFCFMT(R9),B'01000000'                             0297
         BNO   @RF00297                                            0297
*       CALL INITFMTW;              /* INITIALIZE FORMAT WRITE     0298
*                                      CHANNEL PROGRAM               */
         BAL   @14,INITFMTW                                        0298
*     ELSE                          /* NOT PREFORMAT         @X04SVCJ*/
*       CALL INITUWCP;              /* INITIALIZE UPDATE CHANNEL   0299
*                                      PROGRAM               @X04SVCJ*/
         B     @RC00297                                            0299
@RF00297 BAL   @14,INITUWCP                                        0299
*     IF BUFCWC=ON THEN             /* IF BUFC SPECIFIED WRITE CHECK
*                                                            @X04SVCJ*/
@RC00297 TM    BUFCWC(R9),B'00001000'                              0300
         BNO   @RF00300                                            0300
*       CALL BLDWCCP;               /* BUILD WRITE CHECK CHANNEL   0301
*                                      PROGRAM               @X04SVCJ*/
         BAL   @14,BLDWCCP                                         0301
*     IF CPAVPLV=ON THEN            /* IS VIRTUAL PAGE LIST REQUIRED
*                                                            @X04SVCJ*/
@RF00300 L     @07,BUFCCPA(,R9)                                    0302
         TM    CPAVPLV(@07),B'10000000'                            0302
         BNO   @RF00302                                            0302
*       CALL BLDVPL;                /* BUILD VIRTUAL PAGE LIST     0303
*                                                            @X04SVCJ*/
         BAL   @14,BLDVPL                                          0303
*     IF RCCWN^=K0 THEN             /* READ CHANNEL PROGRAM REQUIRED
*                                                            @X04SVCJ*/
@RF00302 L     @07,RCCWN                                           0304
         LTR   @07,@07                                             0304
         BZ    @RF00304                                            0304
*       CALL BLDRCP;                /* BUILD READ CHANNEL PROGRAM  0305
*                                                            @X04SVCJ*/
         BAL   @14,BLDRCP                                          0305
*     RCCWN=K0;                     /* CLEAR READ CCW COUNT  @X04SVCJ*/
@RF00304 SLR   @07,@07                                             0306
         ST    @07,RCCWN                                           0306
*     IF PREFRMAT=OFF               /* PREFORMAT PASS YET    @X04SVCJ*/
*         &BUFCHAIN=K0 THEN         /* LAST IN CHAIN         @X04SVCJ*/
         TM    PREFRMAT,B'10000000'                                0307
         BNZ   @RF00307                                            0307
         C     @07,BUFCHAIN(,R9)                                   0307
         BNE   @RF00307                                            0307
*       DO;                         /* LAST NORMAL           @X04SVCJ*/
*         R9=PREFBUFC;              /* LOAD POINTER TO PREFORMAT   0309
*                                                            @X04SVCJ*/
         L     R9,PREFBUFC(,PARMPTR)                               0309
*         PREFRMAT=ON;              /* SET PREFORMAT SWITCH  @X04SVCJ*/
         OI    PREFRMAT,B'10000000'                                0310
*       END;                        /*                       @X04SVCJ*/
*     ELSE                          /* NO STILL NORMAL PASS  @X04SVCJ*/
*       R9=BUFCHAIN;                /* POINT TO NEXT BUFC    @X04SVCJ*/
         B     @RC00307                                            0312
@RF00307 L     R9,BUFCHAIN(,R9)                                    0312
*   END;                            /*                       @X04SVCJ*/
@RC00307 DS    0H                                                  0313
@DE00285 LTR   R9,R9                                               0313
         BNZ   @DL00285                                            0313
*   RESPECIFY                                                      0314
*     RET RSTD;                     /*                       @X04SVCJ*/
*   RET=RSAVL(K1);                  /* LOAD RETURN ADDRESS   @X04SVCJ*/
         L     RET,RSAVL                                           0315
*   RESPECIFY                                                      0316
*    (RET) UNRSTD;                  /*                       @X04SVCJ*/
*   END BLDCPA;                     /*                       @X04SVCJ*/
@EL00002 DS    0H                                                  0317
@EF00002 DS    0H                                                  0317
@ER00002 BR    @14                                                 0317
*BLDCPAAR:                                                         0318
*                                                                  0318
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD THE CHANNEL PROGRAM AREA                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0318
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @X04SVCJ*/
BLDCPAAR DS    0H                                                  0319
*   RESPECIFY                                                      0319
*    (R7,                                                          0319
*     RET) RSTD;                    /*                       @X04SVCJ*/
*   RSAVL(K2)=RET;                  /* SAVE RETURN ADDRESS   @X04SVCJ*/
         ST    RET,RSAVL+4                                         0320
*   RESPECIFY                                                      0321
*     RET UNRSTD;                   /*                       @X04SVCJ*/
*   IF BUFCFMT=ON THEN              /* IF PREFORMAT IS REQD          */
         TM    BUFCFMT(R9),B'01000000'                             0322
         BNO   @RF00322                                            0322
*     DO;                                                          0323
*       IF BUFCPFCP=ON THEN         /* IF PREF COMPLETE              */
         TM    BUFCPFCP(R9),B'00000010'                            0324
         BNO   @RF00324                                            0324
*         R7=CCWSFWC*R6+CCWCFWC;    /* CCWS FOR FORMAT WRITE COMPLETE*/
         LR    R7,R6                                               0325
         ALR   R7,R7                                               0325
         AL    R7,@CF00047                                         0325
*       ELSE                        /* PREF NOT COMPLETE     @Z30SSRL*/
*         DO;                       /*                       @Z30SSDT*/
         B     @RC00324                                            0326
@RF00324 DS    0H                                                  0327
*           IF LPMBTOFL=ON THEN     /* TRACK OVERFLOW        @Z30SSDT*/
         L     @01,AMBPTR(,PARMPTR)                                0327
         L     @01,AMBEDB(,@01)                                    0327
         L     @01,EDBLPMBC(,@01)                                  0327
         TM    LPMBTOFL(@01),B'00010000'                           0327
         BNO   @RF00327                                            0327
*             R6=R6+K1;             /* YES, ADD FOR OVERFLOW @Z30SSDT*/
         AL    R6,@CF00043                                         0328
*           R7=CCWSFW*R6+CCWCFW;    /* NUMBER OF CCWS FOR FORMAT   0329
*                                      WRITE                         */
@RF00327 LR    R7,R6                                               0329
         SLA   R7,2                                                0329
         AL    R7,@CF00032                                         0329
*         END;                      /*                       @Z30SSDT*/
*     END;                                                         0331
*   ELSE                            /* PREFORMAT NOT REQD    @Z30SSRL*/
*     R7=CCWSUW*R6+CCWCUW;          /* CCWS FOR UPDATE WRITE         */
         B     @RC00322                                            0332
@RF00322 LA    @15,3                                               0332
         LR    @01,R6                                              0332
         MR    @00,@15                                             0332
         ALR   @01,@15                                             0332
         LR    R7,@01                                              0332
*   WCCWN=R7;                                                      0333
@RC00322 LR    WCCWN,R7                                            0333
*   IF BUFCWC=ON THEN               /* IF WRITE CHECK SPEC           */
         TM    BUFCWC(R9),B'00001000'                              0334
         BNO   @RF00334                                            0334
*     R7=R7+(CCWSWC*R6+CCWCWC);     /* ADD CCWS FOR WRITE CHECK      */
         LA    @10,5                                               0335
         ALR   @10,R6                                              0335
         ALR   R7,@10                                              0335
*   IF PREFRMAT=OFF THEN            /* DETERMINE IF READ CCWS      0336
*                                                            @X04SVCJ*/
@RF00334 TM    PREFRMAT,B'10000000'                                0336
         BNZ   @RF00336                                            0336
*     RCCWN=CCWSRD*R6+CCWCRD;       /* ADD CCWS FOR READ CCW         */
         LA    @10,5                                               0337
         ALR   @10,R6                                              0337
         ST    @10,RCCWN                                           0337
*   R7=R7+RCCWN;                    /* ADD FOR READ CCWS             */
@RF00336 AL    R7,RCCWN                                            0338
*   OPWVMLNG=((LENGTH(IDACPA)+K7)&(-K8))+/* BASIC SECTION    @X04SVCJ*/
*       R7*DWORDLEN+                /* CCWS SPACE PLUS-              */
*       R8+                         /* REAL PAGE LIST PLUS-          */
*       R8+                         /* VIRTUAL PAGE LIST PLUS-       */
*       (R6*(LENGTH(SEARCHID)+K7&(-K8)));/* SEARCHID AREA    @X04SVCJ*/
         LR    @10,R7                                              0339
         SLA   @10,3                                               0339
         AL    @10,@CF01959                                        0339
         ALR   @10,R8                                              0339
         ALR   @10,R8                                              0339
         LR    @15,R6                                              0339
         SLA   @15,3                                               0339
         ALR   @10,@15                                             0339
         STCM  @10,7,OPWVMLNG(RCORE)                               0339
*   IF BUFCFMT=ON THEN              /* IF PREFORMAT REQD     @Z30SSRL*/
         TM    BUFCFMT(R9),B'01000000'                             0340
         BNO   @RF00340                                            0340
*     OPWVMLNG=OPWVMLNG+R6*DWORDLEN;/* ROOM FOR FWCNT        @X04SVCJ*/
         ALR   @10,@15                                             0341
         STCM  @10,7,OPWVMLNG(RCORE)                               0341
*   OPWVMFLG=K0;                    /* SET FLAGS TO ZERO     @X04SVCJ*/
@RF00340 MVI   OPWVMFLG(RCORE),X'00'                               0342
*   IF OPWUPGR=ON THEN              /* UPGRADE               @X04SVCJ*/
         L     @10,OPWCURPT(,RCORE)                                0343
         TM    OPWUPGR(@10),B'00100000'                            0343
         BNO   @RF00343                                            0343
*     OPWVMTYP=PRUSTRBK;            /* INDICATE PROTECTED    @Z30SSDT
*                                      @ UPGRADE STRING BLOCK      0344
*                                                            @Z30SSDT*/
         MVI   OPWVMTYP(RCORE),X'0C'                               0344
*   ELSE                            /* STRING BLOCK          @X04SVCJ*/
*     OPWVMTYP=PRSTRBLK;            /* PROTECTED STRING BLK  @Z30SSDT*/
         B     @RC00343                                            0345
@RF00343 MVI   OPWVMTYP(RCORE),X'0B'                               0345
*   OPWVMANC=ADDR(IDACMB);          /* LOAD ADDR OF ANCHOR BLOCK   0346
*                                                            @X04SVCJ*/
@RC00343 L     @10,ACBAMBL(,RACB)                                  0346
         L     @10,AMBLCMB(,@10)                                   0346
         ST    @10,OPWVMANC(,RCORE)                                0346
*   CALL IDA0192M;                  /* GETCORE FOR CPA       @X04SVCJ*/
         L     @15,@CV00122                                        0347
         BALR  @14,@15                                             0347
*   IF OPWVMADR=K0 THEN             /* WAS STORAGE OBTAINED  @X04SVCJ*/
         L     @10,OPWVMADR(,RCORE)                                0348
         LTR   @10,@10                                             0348
         BNZ   @RF00348                                            0348
*     DO;                           /*                       @X04SVCJ*/
*       PPCODE=PP20;                /* SET ERROR CODE        @X04SVCJ*/
         L     @05,PDPTR                                           0350
         MVI   PPCODE(@05),X'14'                                   0350
*       RETURN TO EXIT;             /* TERMINATE             @X04SVCJ*/
         LA    @14,EXIT                                            0351
@EL00003 DS    0H                                                  0351
@EF00003 DS    0H                                                  0351
@ER00003 BR    @14                                                 0351
*     END;                          /*                       @X04SVCJ*/
*   BUFCCPA=OPWVMADR;               /* SET CPA ADDR IN BUFC  @X04SVCJ*/
@RF00348 L     @10,OPWVMADR(,RCORE)                                0353
         ST    @10,BUFCCPA(,R9)                                    0353
*   CPANXT1=AMBCPA;                 /* CHAIN THIS CPA        @Z30SSDT*/
         L     @15,AMBPTR(,PARMPTR)                                0354
         L     @14,AMBCPA(,@15)                                    0354
         ST    @14,CPANXT1(,@10)                                   0354
*   AMBCPA=OPWVMADR;                /* SET CPA ADDR IN AMB   @Z30SSDT*/
         ST    @10,AMBCPA(,@15)                                    0355
*   CPALEN=OPWVMLNG;                /* SET LENGTH IN HEADER  @X04SVCJ*/
         L     @15,OPWVMLNG-1(,RCORE)                              0356
