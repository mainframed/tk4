*/* OUTPUT - THIS ROUTINE CAUSES IDA0195A TO REGAIN CONTROL.         */
*/********************************************************************/
*                                                                  0872
*   GEN;                                                           0872
*       /* GENERATE PROLOG. */
         BALR @09,0 ESTABLISH BASE REGISTER.
         DROP @10   RELEASE BASE REGISTER.
         DROP @11   RELEASE OTHER BASE REGISTER.
RSTART2  SR  @15,@15 CLEAR REGISTER OF RESIDUE.
         USING  RSTART2,@09  MODULE BASE IS NEW NINE.
         LA  @15,12(0,0) PLACE CODE FOR TEST IN R15.
         CR  @00,@15 WAS A SDWA PASSED TO THIS ROUTINE.
         BNE RSTART3 IF SDWA PASSED GOTO RSTART3.
* NO SDWA WAS PASSED, RETURN TO RTM WITH PERCOLATION.
         SR  @15,@15 CLEAR RETURN CODE TO ZERO.
         BR  @14  TAKE EXIT TO RTM.
* SDWA WAS PASSED, PROCEED WITH RECOVERY ROUTINE.
RSTART3 STM  @14,@12,12(@13) SAVE REGISTERS PASSED.
         USING  @DATD,@12 RE-ESTABLISH DATAREG AS R12.
         DROP R13                     STOP USING R13 FOR DATARE@ZA11576
*   RFY                                                            0873
*    (R12,                                                         0873
*     R13) RSTD;                    /* MAKE A NOTE FOR THE COMPILER.
*                                                            @ZA11576*/
*   R12=SDWAPARM;                   /* RESTORE DATAREG AS 12.        */
*                                                                  0874
         L     R12,SDWAPARM(,R1)                                   0874
*   /*****************************************************************/
*   /*                                                               */
*   /* TEST FOR ADDITIONAL CONDITIONS WHICH PROHIBIT RETRY.          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0875
*   IF SDWACLUP^=ON&                /* ENTRY NOT FOR CLEANUP?        */
*       SDWANRBE^=ON THEN           /* REALLY THIS RB LEVEL?         */
         TM    SDWACLUP(R1),B'11000000'                            0875
         BNZ   @RF00875                                            0875
*     DO;                           /* RETRY MAY BE POSSIBLE.        */
*       IF SDWAEAS^=ON THEN         /* ERROR RECORD NEEDED?          */
         TM    SDWAEAS(R1),B'00001000'                             0877
         BO    @RF00877                                            0877
*         DO;                       /* MAKE AN ERROR RECORD.         */
*                                                                  0878
*           /*********************************************************/
*           /*                                                       */
*           /* SWITCH TO KEY ZERO.                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0879
*           SDWAMODN='IDA0195A';    /* NOTE MODULE IN ERROR.         */
         MVC   SDWAMODN(8,R1),@CC03310                             0879
*           SDWACSCT='IDA0195A';    /* NOTE CSECT IN CONTROL.        */
         MVC   SDWACSCT(8,R1),@CC03310                             0880
*           SDWAREXN='IDA0195A';    /* NOTE RECOVERY RTNE NAME.      */
*                                                                  0881
         MVC   SDWAREXN(8,R1),@CC03310                             0881
*           /*********************************************************/
*           /*                                                       */
*           /* NOW BACK TO TCB PKF.                                  */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0882
*         END;                      /* END OF ERROR RECORDING.       */
*                                                                  0882
*       /*************************************************************/
*       /*                                                           */
*       /* HAS THIS CODE JUST BEEN ENTERED OR ARE WE LOOPING?        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0883
*       IF RETRY=ON THEN            /* IS RETRY ALREADY IN PROGRESS? */
@RF00877 TM    RETRY,B'10000000'                                   0883
         BNO   @RF00883                                            0883
*         DO;                       /* RECURSION IN PROGRESS.        */
*           GEN;                                                   0885
*         /* RETURN TO RTM WITH PERCOLATION. */
         LM @14,@12,12(@13) RESTORE PASSED REGISTERS.
         SR  @15,@15 CLEAR RETURN CODE.
         BR  @14 EXIT TO RTM.
*         END;                      /* END OF PATH FOR RECURSION.    */
*                                                                  0886
*       /*************************************************************/
*       /*                                                           */
*       /* RETRY MAY BE ATTEMPTED, PROCEED.                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0887
*       ELSE                        /* OTHERWISE RECURSION IS NOT IN
*                                      PROGRESS.                     */
*         DO;                       /* PROCESSING FOR ATTEMPT AT   0887
*                                      RETRY.                        */
         B     @RC00883                                            0887
@RF00883 DS    0H                                                  0888
*           RETRY=ON;               /* NOTE RETRY ATTEMPT.           */
*                                                                  0888
         OI    RETRY,B'10000000'                                   0888
*           /*********************************************************/
*           /*                                                       */
*           /* SWITCH TO KEY ZERO.                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0889
*           SDWARTYA=RETRYPTR;      /* ESTABLISH POINT OF RETRY.     */
         L     @08,RETRYPTR                                        0889
         ST    @08,SDWARTYA(,R1)                                   0889
*           SDWARCDE=RETRYCDE;      /* SET CODE FOR RETRY.           */
         MVI   SDWARCDE(R1),X'04'                                  0890
*           SDWAUPRG=ON;            /* USE REGISTERS FOR RETRY.      */
*           SDWAFREE=ON;            /* FREE MAIN SDWA AT RETRY.      */
         OI    SDWAUPRG(R1),B'00001100'                            0892
*           SDWASR01=RETRYR1;       /* ESTABLISH RETRY REGISTER.     */
         L     @08,RETRYR1                                         0893
         ST    @08,SDWASR01(,R1)                                   0893
*           SDWASR09=RETRYR9;       /* ESTABLISH RETRY REGISTER.     */
         L     @08,RETRYR9                                         0894
         ST    @08,SDWASR09(,R1)                                   0894
*           SDWASR10=RETRYR10;      /* ESTABLISH RETRY REGISTER.     */
         L     @08,RETRYR10                                        0895
         ST    @08,SDWASR10(,R1)                                   0895
*           SDWASR11=RETRYR11;      /* ESTABLISH RETRY REGISTER.     */
         L     @08,RETRYR11                                        0896
         ST    @08,SDWASR11(,R1)                                   0896
*           SDWASR12=RETRYR12;      /* ESTABLISH RETRY REGISTER.     */
         L     @08,RETRYR12                                        0897
         ST    @08,SDWASR12(,R1)                                   0897
*           SDWASR13=RETRYR13;      /* ESTABLISH RETRY REGISTER.     */
         L     @08,RETRYR13                                        0898
         ST    @08,SDWASR13(,R1)                                   0898
*           GEN;                                                   0899
*          /* BUILD EXIT CODE. */
         L  @00,SDWARTYA(,@01)  LOAD RETRY ADDRESS.
         LM  @01,@12,24(@13)  RESTORE ENTRY REGS.
         L  @14,12(,@13) RESTORE RETURN ADDRESS.
         SR  @15,@15 CLEAR REGISTER OF RESIDUE.
         LA  @15,4(0,0) SET RETURN TO RETRY.
         BR  @14  TAKE EXIT TO RTM.
*         END;                      /* END OF PROCESSING FOR RETRY.  */
*     END;                          /* END OF PATH, RETRY POSSIBLE.  */
*                                                                  0901
*   /*****************************************************************/
*   /*                                                               */
*   /* RETRY IS PROHIBITTED BY REASONS FOR ESTAE ENTRY.              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0902
*   ELSE                            /* OTHERWISE REASON FOR ENTRY  0902
*                                      PRECLUDES RETRY.              */
*     DO;                           /* RETRY PROHIBITTED..           */
         B     @RC00875                                            0902
@RF00875 DS    0H                                                  0903
*       GEN;                                                       0903
*         /* RETURN TO RTM WITH PERCOLATION. */
         LM @14,@12,12(@13) RESTORE PASSED REGISTERS.
         SR  @15,@15 CLEAR RETURN CODE.
         BR  @14 EXIT TO RTM.
*     END;                          /* END OF PATH FOR RETRY       0904
*                                      PROHIBITTED..                 */
*   END RCVRRTN;                    /* END OF RCVRRTN PROCUDURE.     */
@EL00013 DS    0H                                                  0905
@EF00013 DS    0H                                                  0905
