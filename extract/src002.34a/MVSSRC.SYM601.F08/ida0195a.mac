*       RWORK3=BIBVMT;              /* OBTAIN FIRST VMT ADDRESS.     */
         L     RWORK3,BIBVMT(,RWORK2)                              0658
*       CBCTR1=ONE;                 /* INITIALIZE LOOP COUNTER.      */
         LA    CBCTR1,1                                            0659
