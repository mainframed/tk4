*DUMPLINE:                                                         0590
*                                                                  0590
*/********************************************************************/
*/* FUNCTION - THIS ROUTINE IS CALLED BY DUMPER TO DISPLAY HEX 20    */
*/*            BYTES OF STORAGE.                                     */
*/* INPUT - PASSED IS DUMPAD.                                        */
*/* OUTPUT - FROM LEFT TO RIGHT OUTPUT IS: 1) DATA ADDRESS, 2) THE   */
*/*          DATA ITSELF GROUPED INTO WORDS AND 3) A TRANSLATION     */
*/*          TO EBCDIC FROM BINARY(HEX).                             */
*/********************************************************************/
*                                                                  0590
*   PROC;                                                          0590
DUMPLINE STM   @14,@12,@SA00007                                    0590
*   LINE=' ';                       /* CLEAR PRINT LINE.             */
         MVI   LINE+1,C' '                                         0591
         MVC   LINE+2(118),LINE+1                                  0591
         MVI   LINE,C' '                                           0591
*   ASTRX1='*';                     /* INITAILZE LEFT *.             */
         MVI   ASTRX1,C'*'                                         0592
*   ASTRX2='*';                     /* INITIALIZE RIGHT *.           */
         MVI   ASTRX2,C'*'                                         0593
*   EBCDIC=CURRLINE;                /* SAVE THE LINE FOR TRANSLATION.*/
         L     @07,CDUMPAD                                         0594
         MVC   EBCDIC(32),CURRLINE(@07)                            0594
*   TR(EBCDIC,TRTAB);               /* TRANSLATE NOW TO EBCDIC FOR 0595
*                                      OUTPUT.                       */
         TR    EBCDIC(32),TRTAB                                    0595
*   CONIN=ADDR(CDUMPAD);            /* CONVERT THE STARTING ADDRESS
*                                      OF THE LINE.                  */
         LA    CONIN,CDUMPAD                                       0596
*   CONOUT=0;                       /* LEAVE THE DATA IN CONWORK.    */
         SLR   CONOUT,CONOUT                                       0597
*   CALL CONVERT;                   /* CHANGE ADDRESS TO PRINT FORM  */
         BAL   @14,CONVERT                                         0598
*   LINE(1:6)=CONWORK(3:8);         /* MOVE DATA TO PRINT LINE.      */
         MVC   LINE(6),CONWORK+2                                   0599
*   I=10;                           /* SET STARTING COLUMN OF DATA.  */
         LA    I,10                                                0600
*   DO J=1 TO 8;                    /* DO 2 GROUPS OF FOUR WORDS   0601
*                                      EACH.                         */
         LA    J,1                                                 0601
@DL00601 DS    0H                                                  0602
*     IF CDUMPAD=DUMPAD THEN        /* ANY STARTING BLANKS NEEDED?   */
         L     @12,DUMPAD                                          0602
         C     @12,CDUMPAD                                         0602
         BNE   @RF00602                                            0602
*       DO;                         /* NO BLANKS NEEDED, CONTINUE.   */
*         CONIN=DUMPAD;             /* PASS DATA.                    */
         L     @07,DUMPAD                                          0604
         LR    CONIN,@07                                           0604
*         CONOUT=ADDR(LINE(I));     /* PLACE PRINT FORM IN PRINT   0605
*                                      LINE.                         */
         LA    CONOUT,LINE-1(I)                                    0605
*         CALL CONVERT;             /* CONVERT DATA TO PRINT FORM.   */
         BAL   @14,CONVERT                                         0606
*         DUMPAD=DUMPAD+4;          /* INCREMENT FOR THE WORD DUMPED.*/
         LA    @12,4                                               0607
         AL    @12,DUMPAD                                          0607
         ST    @12,DUMPAD                                          0607
*       END;                        /* TO HERE IF BLANKS NEEDED.     */
*                                                                  0608
*     /***************************************************************/
*     /*                                                             */
*     /* PROVIDE BLANKS FOR BOTH LEFT AND RIGHT SIDES OF PAGE.       */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0609
*     ELSE                          /* BLANKS ARE NEEDED FOR AT LEAST
*                                      ONE WORD.                     */
*       EBCDIC((((J-1)*4)+1):(((J-1)*4)+4))='    ';/* FOUR BLANKS.   */
         B     @RC00602                                            0609
@RF00602 LR    @12,J                                               0609
         BCTR  @12,0                                               0609
         SLA   @12,2                                               0609
         LA    @15,EBCDIC(@12)                                     0609
         MVC   0(4,@15),@CC03220                                   0609
*     CDUMPAD=CDUMPAD+4;            /* INCREMENT FOR NEXT WORD.      */
@RC00602 LA    @12,4                                               0610
         AL    @12,CDUMPAD                                         0610
         ST    @12,CDUMPAD                                         0610
*     IF DUMPAD>=EDUMPAD THEN       /* LAST BYTE OF DATA DUMPED?     */
         C     EDUMPAD,DUMPAD                                      0611
         BH    @RF00611                                            0611
*       DUMPAD=ZERO;                /* FORCE BLANKING TO END OF LINE.*/
         SLR   @12,@12                                             0612
         ST    @12,DUMPAD                                          0612
*     I=I+9;                        /* A WORD IS EIGHT CHARS PLUS A
*                                      BLANK.                        */
@RF00611 AL    I,@CF00033                                          0613
*     IF J=4 THEN                   /* IF AT THE END OF A GROUP OF 0614
*                                      FOUR,                         */
         C     J,@CF00118                                          0614
         BNE   @RF00614                                            0614
*       I=I+3;                      /* BETWEEN GROUPS OF FOUR GIVE 0615
*                                      3+1 BLANKS.                   */
         AL    I,@CF00088                                          0615
*   END;                            /* END OF PATH FOR A DATA WORD.  */
@RF00614 AL    J,@CF00043                                          0616
         C     J,@CF00064                                          0616
         BNH   @DL00601                                            0616
*   CALL PRINT;                     /* MOVE THE LINE TO DATASET.     */
         BAL   @14,PRINT                                           0617
*   END DUMPLINE;                   /* END OF PROC DUMPLINE.         */
@EL00007 DS    0H                                                  0618
@EF00007 DS    0H                                                  0618
@ER00007 LM    @14,@12,@SA00007                                    0618
         BR    @14                                                 0618
*CONVERT:                                                          0619
*                                                                  0619
*/********************************************************************/
*/* FUNCTION - THIS ROUTINE IS CALLED TO CONVERT DATA FOR PRINTING.  */
*/* INPUT - PASSED ARE CONIN AND CONOUT.                             */
*/* OUTPUT - DATA IN CONIN IS CONVERTED FROM BINARY TO PRINTER       */
*/*          FORMAT HEX. IF CONOUT IS NOT ZERO THE DATA IS PLACED    */
*/*          VIA CONOUT. IF CONOUT IS ZERO THEN THE DATA REMAINS     */
*/*          IN CONWORK.                                             */
*/********************************************************************/
*                                                                  0619
*   PROC;                                                          0619
CONVERT  STM   @14,@12,12(@13)                                     0619
*   CONWORK(1:4)=CONIN->CONMASK(1:4);/* MOVE TO WKAREA       @OZ07692*/
         MVC   CONWORK(4),CONMASK(CONIN)                           0620
*   UNPK(CONWORK,CONWORK(1:5));     /* EXPAND INPUT WORD     @OZ07692*/
         UNPK  CONWORK(9),CONWORK(5)                               0621
*   CONWORK=CONWORK&'0F0F0F0F0F0F0F0F0F'X;/* MASK OFF ZONES          */
         NC    CONWORK(9),@CB03225                                 0622
*   TR(CONWORK,CONTAB);             /* CONVERT TO EBCDIC             */
         TR    CONWORK(9),CONTAB                                   0623
*   IF CONOUT^=0 THEN               /* IF NEED FOR OUTPUT            */
         LTR   CONOUT,CONOUT                                       0624
         BZ    @RF00624                                            0624
*     CONOUT->CONMASK=CONWORK;      /* MOVE OUT OUTPUT               */
         MVC   CONMASK(8,CONOUT),CONWORK                           0625
*   END CONVERT;                                                   0626
@EL00008 DS    0H                                                  0626
@EF00008 DS    0H                                                  0626
@ER00008 LM    @14,@12,12(@13)                                     0626
         BR    @14                                                 0626
