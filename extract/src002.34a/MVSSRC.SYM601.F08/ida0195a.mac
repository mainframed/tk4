         B     @DE00660                                            0660
@DL00660 DS    0H                                                  0661
*         DUMPID=IDVMT;             /* NAME THE DATA.                */
         MVI   DUMPID+3,C' '                                       0661
         MVC   DUMPID+4(27),DUMPID+3                               0661
         MVC   DUMPID(3),IDVMT                                     0661
*         IDSIZE=LENGTH(IDVMT);     /* PASS THE SIZE FO THE DUMP ID. */
         MVI   IDSIZE,X'03'                                        0662
*         DUMPAD=ADDR(IDAVMT);      /* PASS THE VMT ADDRESS.         */
         ST    RWORK3,DUMPAD                                       0663
*         DUMPLEN=VMTLEN;           /* VMT IS VARIABLE PASS THE    0664
*                                      LENGTH.                       */
         LH    @12,VMTLEN(,RWORK3)                                 0664
