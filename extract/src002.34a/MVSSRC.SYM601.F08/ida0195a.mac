         TITLE 'IDA0195A-VSAM SNAP FORMAT ROUTINE.                     *
                        '
IDA0195A CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IDA0195A  78.151'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @09,0                                               0001
@PSTART  LA    @10,4095(,@09)                                      0001
         LA    @11,4095(,@10)                                      0001
         USING @PSTART,@09                                         0001
         USING @PSTART+4095,@10                                    0001
         USING @PSTART+8190,@11                                    0001
JSCBDISP EQU 188
JSCBAOS1 EQU  72
JSCBAOS2 EQU 192
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED
*                                       IF NO ERROR WAS ENCOUNTERED
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA
         ST    @01,88(,@15)             STORE LENGTH OF AREA
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE
         B     GMN00200                 BYPASS SETTING ERROR
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.
*   IF R15=0 THEN                   /* GETMAIN FAILURE       @ZA11576*/
         LTR   R15,R15                                             0208
         BNZ   @RF00208                                            0208
*     DO;                           /* YES                   @ZA11576*/
*       GEN;                                                       0210
*                                    /* START ASSEM CODE     @ZA11576*/
      LA @15,4                       INIT RETURN CODE        @ZA11576
      LM R0,R12,20(R13)              RESTORE REGS            @ZA11576
      L  R14,12(R13)                 FETCH RETURN ADDRESS    @ZA11576
      BR R14                         RETURN TO         @ZA11576
*     END;                          /* END GETMAIN ERROR     @ZA11576*/
*   RESPECIFY                                                      0212
*     R13 RESTRICTED;               /* HOLD SAVE AREA ADDR   @ZA11576*/
@RF00208 DS    0H                                                  0213
*   GEN;                                                           0213
*                                                                  0213
*                                    /* START ASSEM CODE     @ZA11576*/
    ST  R13,4(R15)                   SAVE SAVE REGISTER      @ZA11576
    L   R0,20(R13)         IDAGMAIN USES R0, RESTORE IT      @ZA11576
    L   R1,24(R13)         IDAGMAIN USES R1, RESTORE IT      @ZA11576
    LR  R13,R15                      SET NEW SAVE AREA ADDR  @ZA11576
    USING @DATD,R13                  ADDRESSABILITY TO WORKAR@ZA11576
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS CODE IS NOW RUNNING IN PSW KEY TCBPKF.                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0214
*   TCBAD=ADPLTCB;                  /* MAKE LOCAL TCB PTR.           */
         L     @12,ADPLTCB(,R1)                                    0214
         ST    @12,TCBAD                                           0214
*   RFY                                                            0215
*     TCB BASED(TCBAD);             /* TCB IS NOW LOCATED FROM     0215
*                                      TCBPTR.                       */
*   KEYTCB=TCBPKF;                  /* SAVE USERS KEY.               */
         IC    @08,TCBPKF(,@12)                                    0216
         STC   @08,KEYTCB                                          0216
*   RFY                                                            0217
*     IDAVSRT BASED(CBSVPTR(KEYTCB/16+ONE));/* SUPPORT MULTI-VSRT.   */
*   ASCBPTR=ASVTENTY(ADPLASID);     /* MAKE LOCAL ASCB PTR.          */
         LH    @12,ADPLASID(,R1)                                   0218
         SLA   @12,2                                               0218
         L     @08,CVTPTR                                          0218
         L     @08,CVTASVT(,@08)                                   0218
         L     @12,ASVTENTY-4(@12,@08)                             0218
         ST    @12,ASCBPTR                                         0218
*   TRTAB(1)='.';                   /* SET 1ST BYTE OF TABLE.        */
         MVI   TRTAB,C'.'                                          0219
*   MVC(TRTAB(2:256),TRTAB(1:255)); /* PROPAGATE THE PERIOD.         */
         MVC   TRTAB+1(255),TRTAB                                  0220
*   DO I=1 TO LENGTH(VALTAB);       /* LOOP THROUGH ENTIRE TABLE.    */
         LA    I,1                                                 0221
@DL00221 DS    0H                                                  0222
*     TRTAB(VALTAB(I)+1)=VALTAB(I); /* SET TRANSLATABLE CHARACTERS.  */
         SLR   @12,@12                                             0222
         IC    @12,VALTAB-1(I)                                     0222
         LA    @08,TRTAB(@12)                                      0222
         LA    @12,VALTAB-1(I)                                     0222
         MVC   0(1,@08),0(@12)                                     0222
*   END;                            /* END OF LOOP FOR TRANLATE    0223
*                                      TABLE.                        */
         AL    I,@CF00043                                          0223
         C     I,@CF03067                                          0223
         BNH   @DL00221                                            0223
*   DO I=1 TO DIM(BIBTABL);         /* SCAN ENTIRE TABLE.            */
         LA    I,1                                                 0224
@DL00224 DS    0H                                                  0225
*     BIBTABL(I)=ZERO;              /* CLEAR ANY RESIDUE.            */
         LR    @12,I                                               0225
         SLA   @12,2                                               0225
         SLR   @08,@08                                             0225
         ST    @08,BIBTABL-4(@12)                                  0225
*   END;                            /* END OF TABLE CLEARING.        */
         AL    I,@CF00043                                          0226
         C     I,@CF01219                                          0226
         BNH   @DL00224                                            0226
*   DO I=1 TO DIM(CMBTABL);         /* SCAN ENTIRE TABLE.            */
         LA    I,1                                                 0227
@DL00227 DS    0H                                                  0228
*     CMBTABL(I)=ZERO;              /* CLEAR ANY RESIDUE.            */
         LR    @12,I                                               0228
         SLA   @12,2                                               0228
         SLR   @08,@08                                             0228
         ST    @08,CMBTABL-4(@12)                                  0228
*   END;                            /* END OF TABLE CLEARING.        */
*                                                                  0229
         AL    I,@CF00043                                          0229
         C     I,@CF01219                                          0229
         BNH   @DL00227                                            0229
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE ESTAE PARAMETER LIST PASSED TO SVC ESTAE.      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0230
*   RFY                                                            0230
*    (R9,                                                          0230
*     R10,                                                         0230
*     R11,                                                         0230
*     R12,                                                         0230
*     R13) RSTD;                    /* MAKE NOTE FOR COMPILER.       */
*   RETRYR1=R1;                     /* SAVE REGISTER FOR RETRY.      */
         ST    R1,RETRYR1                                          0231
*   RETRYR9=R9;                     /* SAVE REGISTER FOR RETRY.      */
         ST    R9,RETRYR9                                          0232
*   RETRYR10=R10;                   /* SAVE REGISTER FOR RETRY.      */
         ST    R10,RETRYR10                                        0233
*   RETRYR11=R11;                   /* SAVE REGISTER FOR RETRY.      */
         ST    R11,RETRYR11                                        0234
*   RETRYR12=R12;                   /* SAVE REGISTER FOR RETRY.      */
         ST    R12,RETRYR12                                        0235
*   RETRYR13=R13;                   /* SAVE REGISTER FOR RETRY.      */
         ST    R13,RETRYR13                                        0236
*   RFY                                                            0237
*    (R9,                                                          0237
*     R10,                                                         0237
*     R11,                                                         0237
*     R12,                                                         0237
*     R13) UNRSTD;                  /* MAKE NOTE FOR COMPILER.       */
*   RETRYPTR=ADDR(LEMSG);           /* ESTABLISH RETRY POINT FOR   0238
*                                      ERROR MSG.                    */
         LA    @12,LEMSG                                           0238
         ST    @12,RETRYPTR                                        0238
*   RETRYADR=ADDR(LEOD);            /* ESTABLISH RETRY ADDRESS.      */
         LA    @12,LEOD                                            0239
         ST    @12,RETRYADR                                        0239
*   RETRY=OFF;                      /* NOTE TAHT NO RETRY YET      0240
*                                      PERFORMED.                    */
         NI    RETRY,B'01111111'                                   0240
*   LISTESTA=ESTAELST;              /* MOVE LIST TO RE-ENTRANT AREA. */
         MVC   LISTESTA(16),ESTAELST                               0241
*   RFY                                                            0242
*    (R0,                                                          0242
*     R14,                                                         0242
*     R15) RSTD;                    /* RESERVE THESE REGS FOR      0242
*                                      COMPILER.                     */
*   MWAR0=R0;                       /* SAVE R0 ACROSS SVC CALL.      */
         ST    R0,MWAR0                                            0243
*   MWAR14=R14;                     /* SAVE R14 ACROSS SVC INTERFACE.*/
         ST    R14,MWAR14                                          0244
*   MWAR15=R15;                     /* SAVE THIS REGISTER ACROSS SVC.*/
         ST    R15,MWAR15                                          0245
*   RFY                                                            0246
*    (R0,                                                          0246
*     R14,                                                         0246
*     R15) UNRSTD;                  /* RELEASE THESE REGS TO       0246
*                                      COMPILER.                     */
*   GEN REFS(LISTESTA) SETS(R0,R1,R14,R15);                        0247
*                                          /* ESTABLISH RECOVERY */
      ESTAE RCVRRTN,CT,PARAM=(R13),RECORD=YES,MF=(E,LISTESTA)  @ZA11576
*   RFY                                                            0248
*    (R0,                                                          0248
*     R14,                                                         0248
*     R15) RSTD;                    /* RESERVE THESE REGS FOR      0248
*                                      COMPILER.                     */
*   R0=MWAR0;                       /* SAVE R0 ACROSS SVC CALL.      */
         L     R0,MWAR0                                            0249
*   R1=RETRYR1;                     /* SAVE R1 ACROSS SVC.           */
         L     R1,RETRYR1                                          0250
*   R14=MWAR14;                     /* SAVE R14 ACROSS SVC INTERFACE.*/
         L     R14,MWAR14                                          0251
*   R15=MWAR15;                     /* SAVE THIS REGISTER ACROSS SVC.*/
         L     R15,MWAR15                                          0252
*   RFY                                                            0253
*    (R0,                                                          0253
*     R14,                                                         0253
*     R15) UNRSTD;                  /* RELEASE THESE REGS TO       0253
*                                      COMPILER.                     */
*   CALL SPACE;                     /* START DISPLAY WITH BLANK LINE.*/
*                                                                  0254
         BAL   @14,SPACE                                           0254
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCATE AND DISPLAY THE VSAM FIELD IN THE JSCB (JSCBSHR).      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0255
*LVAT:                              /* LABEL FOR CALL TO DUMPVAT.    */
*                                                                  0255
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FOLLOWING IMBEDDED CHARACTER STRING PROVIDES A POINT AT   */
*   /* WHICH A PROGRAM CHECK MAY BE FORCED TO TEST THE RETRY FACILITY*/
*   /* OF THIS MODULE.                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0255
*   GOTO LVAT1;                     /* BRANCH AROUND THE CHARACTER 0255
*                                      STRING.                       */
         B     LVAT1                                               0255
*   GEN(DC  CL8'RETRY   '       LABEL FOR FORCING ERROR. );/* LABEL  */
         DC  CL8'RETRY   '       LABEL FOR FORCING ERROR.
*LVAT1:                             /* RESUME NORMAL CODE.           */
*   GEN(BCR  0,0);                  /* PROVIDE RECOGNIZABLE OPCODE.  */
LVAT1    BCR  0,0
*   CALL DUMPVAT;                   /* DISPLAY THE VAT LOCATOR FIELD.*/
*                                                                  0258
         BAL   @14,DUMPVAT                                         0258
*   /*****************************************************************/
*   /*                                                               */
*   /* DOES THE TCB HAVE A SYSTEM KEY ('00'X TO '70'X)?      @ZA12980*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0259
*   IF KEYTCB<PPKEYTCB THEN         /* IS THE CALLER A SYSTEM KEY? 0259
*                                                            @ZA12980*/
         CLI   KEYTCB,128                                          0259
         BNL   @RF00259                                            0259
*     DO;                           /* YES, SCAN FOR GSR D/S AND GSR
*                                      POOLS.                @ZA12980*/
*                                                                  0260
*       /*************************************************************/
*       /*                                                           */
*       /* LOCATE AND DISPLAY ANY OPEN VSAM GSR DATA SETS.           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0261
*LOGSR:                             /* LABEL FOR CALL TO DUMP OPEN 0261
*                                      GSR D/S.                      */
*       CALL DUMPOGSR;              /* DUMP OPEN GSR DATA SETS.      */
*                                                                  0261
LOGSR    BAL   @14,DUMPOGSR                                        0261
*       /*************************************************************/
*       /*                                                           */
*       /* LOCATE AND DISPLAY, IF APPLICABLE, THE VSAM GSR POOL.     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0262
*LAGSR:                             /* LABEL FOR CALL TO DUMP ACTIVE
*                                      GSR POOL.                     */
*       CALL DUMPAGSR;              /* DUMP GSR POOL IF PRESENT.     */
LAGSR    BAL   @14,DUMPAGSR                                        0262
*     END;                          /* END, SCAN FOR GSR D/S AND GSR
*                                      POOLS.                @ZA12980*/
*                                                                  0263
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCATE AND DISPLAY THE VGTT CHAIN FOR THE ASCB.               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0264
*LVGTT:                             /* LABEL FOR CALL TO DISPLAY VGTT
*                                      CHAIN.                        */
*   CALL DUMPVGTT;                  /* DUMP ANY VGTTS.               */
@RF00259 DS    0H                                                  0264
LVGTT    BAL   @14,DUMPVGTT                                        0264
*LEOD:                              /* LABEL FOR PROCESS OF END OF 0265
*                                      VSAM DATA.                    */
*   LINE='END OF VSAM DATA';        /* PRINT FINAL MESSAGE.          */
LEOD     MVI   LINE+16,C' '                                        0265
         MVC   LINE+17(103),LINE+16                                0265
         MVC   LINE(16),@CC03123                                   0265
*   CALL PRINT;                     /* DISPLAY THE MESSAGE.          */
         BAL   @14,PRINT                                           0266
*   CALL SPACE;                     /* INSURE A LINE SPACE.          */
         BAL   @14,SPACE                                           0267
*   GEN( ESTAE  0 CANCEL ESTAE. );  /* RELEASE ESTAE EXIT.           */
*                                                                  0268
          ESTAE  0 CANCEL ESTAE.
*   /*****************************************************************/
*   /*                                                               */
*   /* BEFORE RETURNING TO IEAVAD08 COMPILER WILL FORCE OLDKEY(0).   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0269
*   GEN;                                                           0269
*                                                                  0269
*                                     /* START ASSEM CODE    @ZA11576*/
       LR  @01,@13                    FETCH ADDR OF CORE     @ZA11576
       L   @13,4(@13)                 RESET SAVE AREA ADDR   @ZA11576
       L   @00,@SIZDATD               FETCH LENGTH TO FREE   @ZA11576
       FREEMAIN R,LV=(0),A=(1)        FREE WORK AREA         @ZA11576
       LM  @14,@12,12(@13)            LOAD RETURN REGISTERS    @ZA11576
       SLR @15,@15                    SET RETURN CODE TO ZERO  @ZA11576
       BR  @14                        RETURN TO          @ZA11576
*   /*****************************************************************/
*   /*                                                               */
*   /* LEMSG IS THE RETRY POINT TO DISPLAY AN ERROR MSG.             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0270
*LEMSG:                             /* LABEL FOR THE RETRY POINT IS
*                                      LEMSG.                        */
*   LINE='IDA0195A DATA SUPPRESSED DUE TO ERROR';/* ERROR MESSAGE.   */
LEMSG    MVI   LINE+37,C' '                                        0270
         MVC   LINE+38(82),LINE+37                                 0270
         MVC   LINE(37),@CC03127                                   0270
*   CALL PRINT;                     /* DISPLAY THE ERROR MESSAGE.    */
         BAL   @14,PRINT                                           0271
*   CALL SPACE;                     /* FOLLOW THAT WITH A BLANK LINE.*/
         BAL   @14,SPACE                                           0272
*   GOTO RETRYADR;                  /* GOTO RETRY ADDRESS.           */
*                                                                  0273
         L     @12,RETRYADR                                        0273
         BR    @12                                                 0273
*   /*****************************************************************/
*   /*                                                               */
*   /* CONTROL HAS NOW PASSED TO THE ADDRESS IN RETRYADR.            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0274
*DUMPVAT:                                                          0274
*                                                                  0274
*/********************************************************************/
*/*  FUNCTION - DISPLAY THE VSAM FIELD IN THE JSCB(JSCBSHR).         */
*/*  INPUT - TCB IS USED TO FIND THE JSCB FOR THIS TCB.              */
*/*  OUTPUT - THE VALUE CONTAINED IN JSCBSHR IS DISPLAYED.           */
*/********************************************************************/
*                                                                  0274
*   PROC;                                                          0274
DUMPVAT  STM   @14,@12,@SA00002                                    0274
*   DUMPID=IDVAT;                   /* IDENTIFY THE DATA.            */
         MVI   DUMPID+12,C' '                                      0275
         MVC   DUMPID+13(18),DUMPID+12                             0275
         MVC   DUMPID(12),IDVAT                                    0275
*   IDSIZE=LENGTH(IDVAT);           /* INDICATE ID SZIE.             */
         MVI   IDSIZE,X'0C'                                        0276
*   DUMPAD=ADDR(JSCBSHR);           /* DUMP JSCBSHR FIELD.           */
         L     @12,TCBAD                                           0277
         L     @12,TCBJSCB(,@12)                                   0277
         LA    @12,JSCBSHR(,@12)                                   0277
         ST    @12,DUMPAD                                          0277
*   DUMPLEN=4;                      /* PASS DATA SIZE.               */
         MVC   DUMPLEN(4),@CF00118                                 0278
*   CALL DUMPER;                    /* DISPLAY THE VAT LOCATION.     */
         BAL   @14,DUMPER                                          0279
*   END;                            /* END OF PROCEDURE DUMPVAT.     */
@EL00002 DS    0H                                                  0280
@EF00002 DS    0H                                                  0280
@ER00002 LM    @14,@12,@SA00002                                    0280
         BR    @14                                                 0280
*DUMPOGSR:                                                         0281
*                                                                  0281
*/********************************************************************/
*/*  FUNCTION - DUMP CONTROL BLOCKS FOR ANY OPEN VSAM GSR DATA SETS. */
*/*  INPUT - TCB IS USED TO FIND DATA SETS OPEN FOR THIS TCB.        */
*/*  OUTPUT - CONTROL BLOCKS FOR ANY AND ALL GSR DATA SETS.          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0281
*   PROC;                                                          0281
DUMPOGSR STM   @14,@12,@SA00003                                    0281
*   RWORK1=TCBDEB;                  /* LOCATE FIRST DEB OFF TCB.     */
*                                                                  0282
         L     @12,TCBAD                                           0282
         L     RWORK1,TCBDEB(,@12)                                 0282
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE OPERATES IN PSW KEY TCBPKF, IF ANY FETCH         */
*   /* PROTECTED DEB.S ARE ENCOUNTERED A PROGRAM CHECK WILL OCCUR.   */
*   /* DEB.S SHOULD BE IN STORE PROTECTED-ONLY KEY 5 STORAGE         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0283
*   GSRFOUND=OFF;                   /* INITIALIZE THE SWITCH.        */
*                                                                  0283
         NI    GSRFOUND,B'01111111'                                0283
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN THE TCB DEB CHAIN FINDING VSAM GSR DEB.S                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0284
*   DEBCTR=ONE;                     /* INITIALIZE DEB LOOPING      0284
*                                      COUNTER.                      */
         LA    DEBCTR,1                                            0284
*   RFY                                                            0285
*     DEB BASED(ADDR(DEBBASIC)-LENGTH(DEB));/* USE PHYSICAL OFFSET   */
*   DO WHILE RWORK1^=ZERO;          /* LOCATE ALL OPEN DATA SETS FOR
*                                      THIS TCB.                     */
         B     @DE00286                                            0286
@DL00286 DS    0H                                                  0287
*     IF DEBAMTYP=VSAMDEB THEN      /* IS THIS A VSAM DEB ?          */
         LR    @12,RWORK1                                          0287
         SL    @12,@CF02382                                        0287
         CLI   DEBAMTYP(@12),X'01'                                 0287
         BNE   @RF00287                                            0287
*       DO;                         /* PROCESS THE VSAM DEB.         */
*         RFY                                                      0289
*           IDAAMB BASED(DEBDCBAD); /* AMB IS LOCATED FROM THE DEB.  */
*         IF AMBID=VSAMAMB&AMBGSR=ON THEN/* IS THIS A GSR DATA SET?  */
         L     @08,DEBDCBAD(,RWORK1)                               0290
         CLI   AMBID(@08),X'40'                                    0290
         BNE   @RF00290                                            0290
         TM    AMBGSR(@08),B'00100000'                             0290
         BNO   @RF00290                                            0290
*           DO;                     /* PROCESS THE AMB GSR DATA SET. */
*             GSRFOUND=ON;          /* INDICATE GSR DATA SET FOUND.  */
*                                                                  0292
         OI    GSRFOUND,B'10000000'                                0292
*             /*******************************************************/
*             /*                                                     */
*             /* DISPLAY THE DEB ID AND IT.S ADDRESS                 */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0293
*             DUMPID=IDDEB;         /* ID OF DATA IS DEB             */
         MVI   DUMPID+12,C' '                                      0293
         MVC   DUMPID+13(18),DUMPID+12                             0293
         MVC   DUMPID(12),IDDEB                                    0293
*             IDSIZE=LENGTH(IDDEB); /* INDICATE DUMP ID SIZE.        */
         MVI   IDSIZE,X'0C'                                        0294
*             DUMPAD=RWORK1;        /* ADDRESS OF DATA TO DISPLAY    */
         ST    RWORK1,DUMPAD                                       0295
*             DUMPLEN=(DEBLNGTH*DWDSIZE)+DEBXLNGH;/* CALCULATE SIZE. */
         SLR   @08,@08                                             0296
         IC    @08,DEBLNGTH(,@12)                                  0296
         SLA   @08,3                                               0296
         L     @12,DEBXTNP(,@12)                                   0296
         AH    @08,DEBXLNGH(,@12)                                  0296
         ST    @08,DUMPLEN                                         0296
*             DEBCB=ON;             /* INDICATE DEB CONTROL BLOCK FOR
*                                      SUPPRESSION                   */
         OI    DEBCB,B'10000000'                                   0297
*             CALL DUMPER;          /* DUMP THE INDICATED DATA.      */
*                                                                  0298
         BAL   @14,DUMPER                                          0298
*             /*******************************************************/
*             /*                                                     */
*             /* DISPLAY THE AMB ASSOCIATED WITH THE DEB.            */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0299
*             DUMPID=IDAMB;         /* ID OF DATA IS AMB.            */
         MVI   DUMPID+3,C' '                                       0299
         MVC   DUMPID+4(27),DUMPID+3                               0299
         MVC   DUMPID(3),IDAMB                                     0299
*             IDSIZE=LENGTH(IDAMB); /* INDICATE DUMP ID SIZE.        */
         MVI   IDSIZE,X'03'                                        0300
*             DUMPAD=ADDR(IDAAMB);  /* DATA IS THE AMB               */
         L     @12,DEBDCBAD(,RWORK1)                               0301
         ST    @12,DUMPAD                                          0301
*             DUMPLEN=LENGTH(IDAAMB);/* IDICATE THE SIZE             */
         MVC   DUMPLEN(4),@CF02928                                 0302
*             CALL DUMPER;          /* DUMP THE DATA.                */
*                                                                  0303
         BAL   @14,DUMPER                                          0303
*             /*******************************************************/
*             /*                                                     */
*             /* CALL DUMPDEB TO DISPLAY DATA SET CONTROL BLOCKS.    */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0304
*             CALL DUMPDEB;         /* DISPLAY DEB DATA SET.         */
         BAL   @14,DUMPDEB                                         0304
*             RFY                                                  0305
*               IFGACB BASED(DEBDCBAD);/* LOCATE THE ACB FROM THE  0305
*                                      DEB.                          */
*           END;                    /* END OF PATH FOR AMB GSR DATA
*                                      SET.                          */
*         ELSE                      /* IF NOT AN AMB THEN,           */
*           IF ACBID=VSAMACB&ACBGSR=ON THEN/* DOES DEB POINT TO ACB? */
         B     @RC00290                                            0307
