*/********************************************************************/
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/
*/********************************************************************/
*                                                                  0225
*   GEN;                                                           0225
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED
*                                       IF NO ERROR WAS ENCOUNTERED
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA
         ST    @01,88(,@15)             STORE LENGTH OF AREA
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE
         B     GMN00200                 BYPASS SETTING ERROR
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.
*   RESPECIFY                                                      0226
*    (R15,                                                         0226
*     R13) RSTD;                    /*                       @X04SVCJ*/
*   PD=ZERO;                        /* ZERO RETURN CODE      @X04SVCJ*/
         SLR   @14,@14                                             0227
         ST    @14,PD(,R13)                                        0227
*   IF R15=ZERO THEN                /* IF NO CORE AVAILABLE  @X04SVCJ*/
         CR    R15,@14                                             0228
         BNE   @RF00228                                            0228
*     DO;                           /* ERROR                 @X04SVCJ*/
*       OPWMODWK=OFF;               /* NO MODULE WORK AREA   @Z30SSDT*/
         L     @14,OPWCURPT(,RCORE)                                0230
         NI    OPWMODWK(@14),B'10111111'                           0230
*       PPCODE=PP20;                /* SET PROBLEM CODE      @X04SVCJ*/
         MVI   PPCODE(R13),X'14'                                   0231
*       PPFUNC=PDF50;               /* SET FUNCTION CODE     @X04SVCJ*/
         MVI   PPFUNC(R13),X'32'                                   0232
*     END;                          /*                       @X04SVCJ*/
*   ELSE                            /* NO ERROR              @X04SVCJ*/
*     DO;                           /* MAINLINE              @X04SVCJ*/
         B     @RC00228                                            0234
@RF00228 DS    0H                                                  0235
*       PPFUNC=PDF53;               /* SET FUNCTION CODE     @Z30SSDT*/
         MVI   PPFUNC(R13),X'35'                                   0235
*       R15->SABWRD=R13;            /* SAVE SAVE AREA ADDR   @Z30SSDT*/
         ST    R13,SABWRD(,R15)                                    0236
*       R13=R15;                    /* LOAD PTR TO AUTOMATICS      0237
*                                                            @Z30SSDT*/
         LR    R13,R15                                             0237
*       GEN(USING @DATD,R13);       /* GET BASE FOR AUTOMATICS       */
         USING @DATD,R13
*       RETCONT=ZERO;               /* INIT ARRAY CONTROL    @Z30SSDT*/
         SLR   @14,@14                                             0239
         ST    @14,RETCONT                                         0239
*       RESPECIFY                                                  0240
*         R15 UNRESTRICTED;         /*                       @Z30SSDT*/
*       IF CVTSMCA^=ZERO&           /* IF SMCA EXIST AND     @OZ04709*/
*           SMCADSA=ON&             /* DATA SET INFORMATION REQUIRED
*                                      AND                   @OZ04709*/
*           SMCAUSER=ON&SMCAMAN=ON& /* SMF RECORDING AND     @OZ04709*/
*           TCBTCT^=ZERO THEN       /* THE TCT EXIST         @OZ04709*/
         L     @05,FLCCVT                                          0241
         L     @05,CVTSMCA(,@05)                                   0241
         CR    @05,@14                                             0241
         BE    @RF00241                                            0241
         TM    SMCADSA(@05),B'00010000'                            0241
         BNO   @RF00241                                            0241
         TM    SMCAUSER(@05),B'11000000'                           0241
         BNO   @RF00241                                            0241
         L     @05,PSATOLD                                         0241
         C     @14,TCBTCT(,@05)                                    0241
         BE    @RF00241                                            0241
*         SMFACTV=ON;               /* INDICATE SMF ACTIVE   @OZ04709*/
         OI    SMFACTV,B'00001000'                                 0242
*       DO C1=AMBLNUM TO 1 BY-1;    /* LOOP THRU AMB LIST            */
*                                                                  0243
@RF00241 L     @05,ACBAMBL(,RACB)                                  0243
         LH    @05,AMBLNUM(,@05)                                   0243
         ST    @05,C1                                              0243
         B     @DE00243                                            0243
@DL00243 DS    0H                                                  0244
*         /***********************************************************/
*         /*                                                         */
*         /* BUILD BLOCKS PER AMB                                    */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0244
*         IF C1=AMBLNUM THEN        /* IF DATA PASS          @X04SVCJ*/
         L     @01,ACBAMBL(,RACB)                                  0244
         CH    @05,AMBLNUM(,@01)                                   0244
         BNE   @RF00244                                            0244
*           OPWWRKPT=OPWDTWRK;      /* INIT AMB WORK POINTER @X04SVCJ*/
         L     @05,OPWDTWRK(,RCORE)                                0245
         ST    @05,OPWWRKPT(,RCORE)                                0245
*         ELSE                      /* INDEX PASS            @X04SVCJ*/
*           OPWWRKPT=OPWIXWRK;      /* INIT AMB WORK POINTER @X04SVCJ*/
         B     @RC00244                                            0246
@RF00244 L     @05,OPWIXWRK(,RCORE)                                0246
         ST    @05,OPWWRKPT(,RCORE)                                0246
*         IF OPWRSTRT=ON THEN       /* IF A RESTART          @Z30SSDT*/
@RC00244 TM    OPWRSTRT(RCORE),B'00001000'                         0247
         BNO   @RF00247                                            0247
*           CALL RESTINIT;          /* SETUP FOR RESTART     @Z30SSDT*/
         BAL   @14,RESTINIT                                        0248
*         ELSE                      /* IF NOT A RESTART      @Z30SSDT*/
*           DO;                     /* NORMAL OPEN PATH      @Z30SSDT*/
         B     @RC00247                                            0249
@RF00247 DS    0H                                                  0250
*             CALL BLDAMB;          /* BUILD AN AMB          @Z30SSDT*/
         BAL   @14,BLDAMB                                          0250
*             CALL BLDRMEXT;        /* BUILD RECORD MGT EXT  @Z30SSDT*/
         BAL   @14,BLDRMEXT                                        0251
*             CALL BLDAMDSB;        /* BUILD AN AMDSB        @Z30SSDT*/
         BAL   @14,BLDAMDSB                                        0252
*             CALL BLDIWA;          /* BUILD INSERT WORK AREA        */
         BAL   @14,BLDIWA                                          0253
*             CALL BLDARDB;         /* BUILD ARDB            @Z30SSDT*/
         BAL   @14,BLDARDB                                         0254
*             CALL BLDLPMB;         /* BUILD LPMB            @Z30SSDT*/
         BAL   @14,BLDLPMB                                         0255
*           END;                    /*                       @Z30SSDT*/
*         CALL BLDEDB;              /* BUILD EDB             @Z30SSDT*/
@RC00247 BAL   @14,BLDEDB                                          0257
*         CALL BLDRDEB;             /* BUILD THE REAL DEB            */
         BAL   @14,BLDRDEB                                         0258
*         IF OPWLSR=OFF&OPWGSR=OFF THEN/* NOT SHRD RESOURCES @Z30SSDT*/
         TM    OPWLSR(RCORE),B'01100000'                           0259
         BNZ   @RF00259                                            0259
*           CALL BLDSTRBK;          /* BUILD STRING BLOCKS   @X04SVCJ*/
         BAL   @14,BLDSTRBK                                        0260
*         ELSE                      /* SHARED RESOURCES      @Z30SSDT*/
*           IF OPWRSTRT=OFF THEN    /* IF NOT A RESTART      @Z30SSDT*/
         B     @RC00259                                            0261
@RF00259 TM    OPWRSTRT(RCORE),B'00001000'                         0261
         BNZ   @RF00261                                            0261
*             CALL CHNBLKS;         /* CHAIN CONTROL BLOCKS  @Z30SSDT*/
         BAL   @14,CHNBLKS                                         0262
*       END;                                                       0263
*                                                                  0263
@RF00261 DS    0H                                                  0263
@RC00259 SLR   @05,@05                                             0263
         BCTR  @05,0                                               0263
         AL    @05,C1                                              0263
         ST    @05,C1                                              0263
@DE00243 LTR   @05,@05                                             0263
         BP    @DL00243                                            0263
*       /*************************************************************/
*       /*                                                           */
*       /* IDA0192Z - FINAL PROCESSING                               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0264
*       RESPECIFY                                                  0264
*         RDEB RESTRICTED;                                         0264
*       IF AMBLCAT=OFF THEN         /* CAT, SCRA OR VVIC     @Z30SSDT*/
         L     @10,ACBAMBL(,RACB)                                  0265
         TM    AMBLCAT(@10),B'00000010'                            0265
         BNZ   @RF00265                                            0265
*         DO C1=AMBLNUM TO 1 BY-1;  /* LOOP THRU AMB LIST            */
         LH    @10,AMBLNUM(,@10)                                   0266
         ST    @10,C1                                              0266
         B     @DE00266                                            0266
@DL00266 DS    0H                                                  0267
*           AMBP=(ACBAMBL+(C1-K1)*LENGTH(AMBLDTA))->AMBLDTA;/* GET 0267
*                                      AMB                           */
         BCTR  @10,0                                               0267
         SLA   @10,2                                               0267
         AL    @10,ACBAMBL(,RACB)                                  0267
         L     @10,AMBLDTA(,@10)                                   0267
         ST    @10,AMBP                                            0267
*           RDEB=AMBDEBAD;          /* LOAD PTR TO DEB               */
         L     RDEB,AMBDEBAD-1(,@10)                               0268
         LA    RDEB,0(,RDEB)                                       0268
*           CALL HOOKDEB;           /* CHAIN THE DEB                 */
         BAL   @14,HOOKDEB                                         0269
*         END;                                                     0270
         SLR   @10,@10                                             0270
         BCTR  @10,0                                               0270
         AL    @10,C1                                              0270
         ST    @10,C1                                              0270
@DE00266 LTR   @10,@10                                             0270
         BP    @DL00266                                            0270
*       RESPECIFY                                                  0271
*         RDEB UNRESTRICTED;                                       0271
@RF00265 DS    0H                                                  0272
*                                                                  0272
*       /*************************************************************/
*       /*                                                           */
*       /* EXIT FROM IDA0192Z                                        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0272
*EXIT:                                                             0272
*       RESPECIFY                                                  0272
*        (R0,                                                      0272
*         R1) RSTD;                 /*                       @Z30SSDT*/
EXIT     DS    0H                                                  0273
*       GEN(L R0,@SIZDATD);         /* LOAD BLOCK SIZE       @Z30SSDT*/
         L R0,@SIZDATD
*       R1=R13;                     /* SET ADDRESS           @Z30SSDT*/
         LR    R1,R13                                              0274
*       R13=SABWRD;                 /* POINT TO LAST SAVE AREA     0275
*                                                            @Z30SSDT*/
         L     R13,SABWRD(,R13)                                    0275
*       IF PPCODE=K0 THEN           /* ERRORS                @Z30SSRL*/
         CLI   PPCODE(R13),0                                       0276
         BNE   @RF00276                                            0276
*         PPFUNC=K0;                /* NO, ZERO RETURN CODE  @Z30SSRL*/
         MVI   PPFUNC(R13),X'00'                                   0277
*       GEN(FREEMAIN R,LV=(0),A=(1));/*                      @X04SVCJ*/
@RF00276 DS    0H                                                  0278
         FREEMAIN R,LV=(0),A=(1)
*       RESPECIFY                                                  0279
*        (R0,                                                      0279
*         R1) UNRESTRICTED;         /*                       @Z30SSDT*/
*     END;                          /*                       @X04SVCJ*/
*   LM(RET,R12,SAREGS);             /* RESTORE REGISTERS 14-12     0281
*                                                            @Z30SSDT*/
@RC00228 LM    RET,R12,SAREGS(R13)                                 0281
*   GEN(BR RET);                    /* RETURN TO CALLER              */
         BR RET
*   RESPECIFY                                                      0283
*     SAVAREA BASED(R13->SABWRD);   /* ADDRESSIBILITY FOR PD @Z30SSDT*/
*BLDAMB:                                                           0284
*                                                                  0284
*/********************************************************************/
*/*                                                                  */
*/*   FUNCTION -  THIS ROUTINE BUILDS AN AMB                         */
*/*   INPUT -     1. CATALOG WORKAREA FOR COMPONENT DATA SET         */
*/*               2. ACB                                             */
*/*               3. AMBL                                            */
*/*   OUTPUT -    1. AMB IS BUILT IN PROTECT BLOCK           @Z30SSDT*/
*/*               2. FIELDS ARE INITIALIZED TO REFLECT               */
*/*                  CURRENT PROCESSING                              */
*/*               3. AMBP IS LOADED WITH AMB ADDRESS         @X04SVCJ*/
*/*               4. AMBAD IS SET TO AMB ADDRESS IN AUTOMATICS       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0284
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0284
BLDAMB   DS    0H                                                  0285
*   RESPECIFY                                                      0285
*     RET RSTD;                     /*                       @X04SVCJ*/
*   RFY                                                            0286
*     IDAAMDSB BASED(ADDR(CTAMDSB));/* NOT IN AMB YET        @ZA18252*/
*   RETCONT=RETCONT+K1;             /* BUMP CONTROL BY ONE   @Z30SSRL*/
         LA    @15,1                                               0287
         AL    @15,RETCONT                                         0287
         ST    @15,RETCONT                                         0287
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS   @Z30SSDT*/
         SLA   @15,2                                               0288
         ST    RET,RETSAVE-4(@15)                                  0288
*   RESPECIFY                                                      0289
*     RET UNRSTD;                   /*                       @X04SVCJ*/
*   OPWVMLNG=LENGTH(IDAAMB);        /* INIT FOR GET CORE REQUEST   0290
*                                                            @Z30SSDT*/
         MVC   OPWVMLNG(3,RCORE),@CF02849+1                        0290
*   OPWVMTYP=PROTBLK;               /* INDICATE PROTECT BLOCK      0291
*                                                            @Z30SSDT*/
         MVI   OPWVMTYP(RCORE),X'02'                               0291
*   OPWVMANC=ADDR(IDACMB);          /* SET ADDRESS OF ANCHOR BLOCK 0292
*                                                            @X04SVCJ*/
         L     @05,ACBAMBL(,RACB)                                  0292
         L     @05,AMBLCMB(,@05)                                   0292
         ST    @05,OPWVMANC(,RCORE)                                0292
*   OPWVMFLG=ZERO;                  /* RESET FLAG FIELD      @X04SVCJ*/
         MVI   OPWVMFLG(RCORE),X'00'                               0293
*   CALL IDA0192M;                  /* GET CORE FOR AMB      @Z30SSDT*/
         L     @15,@CV05525                                        0294
         BALR  @14,@15                                             0294
*   RSTATRTN=IDAR192Z;              /* NOTE 192Z IN CONTROL  @Z30SSDT*/
         L     @05,OPWCOMWA(,RCORE)                                0295
         MVC   RSTATRTN(4,@05),@CC05338                            0295
*   IF OPWVMADR=ZERO THEN           /* WAS STORAGE OBTAINED  @X04SVCJ*/
         L     @05,OPWVMADR(,RCORE)                                0296
         LTR   @05,@05                                             0296
         BNZ   @RF00296                                            0296
*     DO;                           /* ERROR                 @X04SVCJ*/
*       PPCODE=PP20;                /* SET ERROR CODE        @X04SVCJ*/
         L     @05,SABWRD(,R13)                                    0298
         MVI   PPCODE(@05),X'14'                                   0298
*       RETURN TO EXIT;             /* TERMINATE             @Z30SSDT*/
         LA    @14,EXIT                                            0299
@EL00002 DS    0H                                                  0299
@EF00002 DS    0H                                                  0299
@ER00002 BR    @14                                                 0299
*     END;                          /*                       @X04SVCJ*/
*   AMBAD=OPWVMADR;                 /* SAVE AMB ADDRESS      @X04SVCJ*/
@RF00296 L     @05,OPWVMADR(,RCORE)                                0301
         ST    @05,AMBAD                                           0301
*   AMBP=AMBAD;                     /* LOAD AMB POINTER      @X04SVCJ*/
         ST    @05,AMBP                                            0302
*   AMBDORGA=ON;                    /* DSORG INDICATES AM/0          */
         OI    AMBDORGA(@05),B'00001000'                           0303
*   AMBEXFG=AMBEXOFF;               /* SET LOCK BIT OFF              */
         OI    AMBEXFG(@05),B'00000010'                            0304
*   AMBCDSN=CTHSHCAT;               /* INIT CATALOG NAME, HASHED     */
         L     @15,OPWWRKPT(,RCORE)                                0305
         L     @14,CTHSHCAT(,@15)                                  0305
         STCM  @14,7,AMBCDSN(@05)                                  0305
*   AMBCACB=CTHSHCAT;               /* SAVE CATL ACB PTR             */
         ST    @14,AMBCACB(,@05)                                   0306
*   AMBDDSN=CTHSHDSN;               /* INIT AMD DATA SET NAME        */
         MVC   AMBDDSN(3,@05),CTHSHDSN(@15)                        0307
*   IF CTEXEX^=NOEXEXIT THEN        /* EXCEPTION EXIT EXIST  @Z30SSDT*/
         CLC   CTEXEX(8,@15),NOEXEXIT                              0308
         BE    @RF00308                                            0308
*     AMBEXEX=CTEXEX;               /* INITIALIZE EXIT NAME  @Z30SSDT*/
         MVC   AMBEXEX(8,@05),CTEXEX(@15)                          0309
*   IF AMBLCAT=OFF THEN             /* IF NOT SYSTEM DS      @Z30SSDT*/
@RF00308 L     @05,ACBAMBL(,RACB)                                  0310
         TM    AMBLCAT(@05),B'00000010'                            0310
         BNZ   @RF00310                                            0310
*     CALL TIOTSRCH;                /* THEN DETERMINE OFFSET OF TIOT
*                                      ENTRY                 @X04SVRP*/
         BAL   @14,TIOTSRCH                                        0311
*   ELSE                            /* ELSE SYSTEM DATA SET  @X04SVRP*/
*     AMBTIOT=ACBTIOT;              /* INIT TIOT OFFSET      @X04SVRP*/
         B     @RC00310                                            0312
@RF00310 LH    @05,ACBTIOT(,RACB)                                  0312
         L     @01,AMBP                                            0312
         STH   @05,AMBTIOT(,@01)                                   0312
*   AMBID=IDXAMB;                   /* INIT AMB ID                   */
@RC00310 L     @05,AMBP                                            0313
         MVI   AMBID(@05),B'01000000'                              0313
*   AMBLEN=LENGTH(IDAAMB);          /* INIT AMB LENGTH               */
         MVC   AMBLEN(2,@05),@CH02849                              0314
*   IF ACBCAT=ON THEN               /* IF CATALOG INDICATED  @Z30SSRL*/
         TM    ACBCAT(RACB),B'00010000'                            0315
         BNO   @RF00315                                            0315
*     AMBCAT=ON;                    /* SET CATALOG BIT ON    @Z30SSRL*/
         OI    AMBCAT(@05),B'00010000'                             0316
*   IF ACBSCRA=ON THEN              /* IF SCRA INDICATED     @Z30SSRL*/
@RF00315 TM    ACBSCRA(RACB),B'00001000'                           0317
         BNO   @RF00317                                            0317
*     AMBSCRA=ON;                   /* SET SCRA BIT ON       @Z30SSRL*/
         L     @05,AMBP                                            0318
         OI    AMBSCRA(@05),B'00001000'                            0318
*   IF ACBUCRA=ON THEN              /* IF UCRA INDICATED     @Z30SSRL*/
@RF00317 TM    ACBUCRA(RACB),B'00000100'                           0319
         BNO   @RF00319                                            0319
*     AMBUCRA=ON;                   /* SET UCRA BIT ON       @Z30SSRL*/
         L     @05,AMBP                                            0320
         OI    AMBUCRA(@05),B'00000100'                            0320
*   IF ACBSDS=ON THEN               /* IF SYSTEM DATA SET    @Z40WSRL*/
@RF00319 TM    ACBSDS(RACB),B'00000010'                            0321
         BNO   @RF00321                                            0321
*     AMBSDS=ON;                    /* INDICATE IN AMB       @Z40WSRL*/
         L     @05,AMBP                                            0322
         OI    AMBSDS(@05),B'00000001'                             0322
*   AMBAMETH=ACBAMETH;              /* COPY AMETH FIELD FROM ACB     */
@RF00321 L     @05,AMBP                                            0323
         IC    @15,ACBAMETH(,RACB)                                 0323
         STC   @15,AMBAMETH(,@05)                                  0323
*   AMBBUSY=ON;                     /* SET IOS FLAG ON               */
         OI    AMBBUSY(@05),B'00000001'                            0324
*   AMBWKA=OPWWRKPT;                /* INIT PTR TO OPW       @X04SVRP*/
         L     @15,OPWWRKPT(,RCORE)                                0325
         ST    @15,AMBWKA(,@05)                                    0325
*   IF OPWFSTP=ON THEN              /* IF ICI SPECIFIED      @Z30SSDT*/
         TM    OPWFSTP(RCORE),B'00010000'                          0326
         BNO   @RF00326                                            0326
*     AMBICI=ON;                    /* SET ICI INDICATOR ON  @Z30SSDT*/
         OI    AMBICI(@05),B'00010000'                             0327
*   IF AMBLNUM=C1 THEN              /* IS AMB FOR DATA -             */
@RF00326 L     @05,ACBAMBL(,RACB)                                  0328
         LH    @15,AMBLNUM(,@05)                                   0328
         C     @15,C1                                              0328
         BNE   @RF00328                                            0328
*     DO;                           /*                       @Z30SSDT*/
*       AMBTYPE=AMBDATA;            /* YES, INDICATE SUCH            */
         L     @15,AMBP                                            0330
         NI    AMBTYPE(@15),B'10111111'                            0330
*       IF JFCTRACE=ON&AMBLUPGR=OFF THEN/* IF AMP=TRACE AND NOT    0331
*                                      UPGRADE THEN          @ZA29388*/
         L     @01,OPWCOMWA(,RCORE)                                0331
         TM    JFCTRACE+100(@01),B'00000100'                       0331
         BNO   @RF00331                                            0331
         TM    AMBLUPGR(@05),B'01000000'                           0331
         BNZ   @RF00331                                            0331
*         DO;                       /*                       @ZA29388*/
*           IF AMBLPATH=ON&AMBLXPT^=ZERO THEN/* IF PATH AMBL AND BASE
*                                      OPEN THEN             @ZA29388*/
         TM    AMBLPATH(@05),B'10000000'                           0333
         BNO   @RF00333                                            0333
         L     @05,AMBLXPT(,@05)                                   0333
         LTR   @05,@05                                             0333
         BZ    @RF00333                                            0333
*             AMBTRACE=AMBLXPT->AMBLACB->ACBAMBL->AMBLDTA->AMBTRACE;
*                                                                  0334
         L     @05,AMBLACB(,@05)                                   0334
         L     @05,ACBAMBL(,@05)                                   0334
         L     @05,AMBLDTA(,@05)                                   0334
         L     @05,AMBTRACE(,@05)                                  0334
         ST    @05,AMBTRACE(,@15)                                  0334
*           /*********************************************************/
*           /*                                                       */
*           /* USE BASE AMBTRACE ADDR                        @ZA29388*/
*           /*                                                       */
*           /*********************************************************/
