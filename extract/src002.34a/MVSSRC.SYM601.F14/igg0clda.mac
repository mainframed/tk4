*     CCARAREC=DCRACIPT(DCRARCTR+INCR1);/* POINT TO RECORD TO PUT    */
*                                                                  0479
         LR    @10,DCRARCTR                                        0479
         SLA   @10,2                                               0479
         L     @10,DCRACIPT(@10,DCRAWAPR)                          0479
         ST    @10,CCARAREC(,R11)                                  0479
*     /***************************************************************/
*     /*                                                             */
*     /* CALL TO MODULE IGG0CLBO                                     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0480
