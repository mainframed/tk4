*         ELSE                      /* IGGPXIO ROUTINE UNSUCCESSFUL  */
*           PROCESS=STOP;           /* SET HALT LOOP INDICATOR       */
         B     @RC00511                                            0518
@RF00511 L     @10,CCACMSWA(,R11)                                  0518
         MVC   PROCESS(2,@10),@CC04181                             0518
