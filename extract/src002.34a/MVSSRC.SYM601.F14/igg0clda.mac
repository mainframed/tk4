         TITLE 'IGG0CLDA - CATALOG RECOVERY AREA SERVICES              *
                        '
IGG0CLDA CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGG0CLDA  74.345'                                 0001
         DROP  @15
@PROLOG  DS    0H                                                  0002
         USING @MAINENT,@15
         USING @DATD-(CCATEMPS-IGGCCA),R11
         DS 0F
         DC CL8'IGGPRAPV'
         EJECT
         B     @PB00002                                            0288
IGGPRAPV DS    0H                                                  0289
         ENTRY IGGPRAPV
         DROP @15
Z003     DS    0H                                                  0291
Z003RAPV DS    0H                                                  0292
         STM 12,14,12(13)
         LA 13,12(13)
         BALR 12,0
IGGSEGST DS 0H
*                                                                  0297
         USING *,12
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE : 1. COMPARES THE VOLUME ENTRY TIMESTAMP TO THE  */
*   /* VOLUME"S F4 DSCB TIMESTAMP. 2. STORES A NEW TIMESTAMP INTO    */
*   /* BOTH THE CATALOG VOLUME ENTRY AND THE VOLUMES F4 DSCB.        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0298
*   CCARALSA=CCAURAB;               /* SV INPUT RAB PTR              */
         MVC   CCARALSA(4,R11),CCAURAB(R11)                        0298
*   CCAURAB=CCAR0REC;               /* PT TO F4 RD/WR ROUT WK AREA   */
         L     @10,CCAR0REC(,R11)                                  0299
         ST    @10,CCAURAB(,R11)                                   0299
*   F4WDUCB=CAXUCB;                 /* ST UCB PTR FOR F4 RD/WR RT    */
*                                                                  0300
         L     @05,CCARAACB(,R11)                                  0300
         L     @05,ACBUAPTR(,@05)                                  0300
         MVC   F4WDUCB(3,@10),CAXUCB+1(@05)                        0300
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL TO MODULE IGG0CLBU                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0301
*Z004F4RD:                                                         0301
*   CALL IGGPF4RD;                  /* READ F4 DSCB                  */
Z004F4RD L     @15,@CV04232                                        0301
         BALR  @14,@15                                             0301
*   IF CCACD1=RCS THEN              /* IF SUCCESSFUL                 */
         CLI   CCACD1(R11),0                                       0302
         BNE   @RF00302                                            0302
*     DO;                                                          0303
*       IF F4TS2^=CCAVTS THEN       /* DO TMESTMPS MATCH NO,SET ERROR*/
         L     @10,CCAR0REC(,R11)                                  0304
         CLC   F4TS2+120(8,@10),CCAVTS(R11)                        0304
         BE    @RF00304                                            0304
*         DO;                                                      0305
*           CCAMODID='DA';          /* SET MODULE ID                 */
         MVC   CCAMODID(2,R11),@CC04250                            0306
*           CCAREASN=RSN2;          /* SET REASON CODE               */
         MVI   CCAREASN(R11),X'02'                                 0307
*           CCARETRN=RCNTFMT4;      /* SET RETURN CODE               */
         MVI   CCARETRN(R11),X'6A'                                 0308
*         END;                                                     0309
*       ELSE                        /* TIMESTAMPS MATCH              */
*         DO;                       /* GET NEW TIMESTAMP             */
         B     @RC00304                                            0310
@RF00304 DS    0H                                                  0311
*           R1=ADDR(CCAVTS);        /* PT TO TIMESTAMP AREA          */
         LA    R1,CCAVTS(,R11)                                     0311
*           GEN REFS(R1) FLOWS(CLOCKOK);                           0312
                     STCK 0(R1)         /*GET TOD CLOCK VALUE        */
                     BZ CLOCKOK         /*BRANCH IF CLOCK IS OK      */
*                                   /* CLOCK NOT WORKING             */
*           DO;                                                    0313
*             CCAMODID='DA';        /* SET MODULE ID                 */
         MVC   CCAMODID(2,R11),@CC04250                            0314
*             CCAREASN=RSN0;        /* SET REASON CODE               */
         MVI   CCAREASN(R11),X'00'                                 0315
*             CCARETRN=RCLOCKER;    /* SET RETURN CODE               */
         MVI   CCARETRN(R11),X'E4'                                 0316
*           END;                                                   0317
*CLOCKOK:                                                          0318
*         END;                      /* END GET TIMESTAMP             */
CLOCKOK  DS    0H                                                  0319
*     END;                                                         0319
@RC00304 DS    0H                                                  0320
*   IF CCACD1^=RCS THEN             /* ANY ERROR SO FAR              */
@RF00302 CLI   CCACD1(R11),0                                       0320
         BE    @RF00320                                            0320
*     DO;                           /* YES                           */
*                                                                  0321
*       /*************************************************************/
*       /*                                                           */
*       /* CALL TO MODULE IGG0CLBU                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0322
*Z007F4DQ:                                                         0322
*       CALL IGGPF4DQ;              /* DEQ THE VOLUME                */
Z007F4DQ L     @15,@CV04231                                        0322
         BALR  @14,@15                                             0322
*       CCAURAB=CCARALSA;           /* RESTORE INPUT RCD PTR         */
         MVC   CCAURAB(4,R11),CCARALSA(R11)                        0323
*       CALL EXIT;                  /* GET OUT                       */
         BAL   @14,EXIT                                            0324
*     END;                                                         0325
*   DS4AMTIM=CCAVTS;                /* NEW VALUE TO 1ST F4 TMESTMP   */
@RF00320 LA    @10,120                                             0326
         AL    @10,CCAR0REC(,R11)                                  0326
         MVC   DS4AMTIM(8,@10),CCAVTS(R11)                         0326
*   F4TS2=CCAVTS;                   /* NEW VALUE TO 2ND F4 TMESTMP   */
*                                                                  0327
         MVC   F4TS2(8,@10),CCAVTS(R11)                            0327
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL TO MODULE IGG0CLBU                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0328
*Z008F4WR:                                                         0328
*   CALL IGGPF4WR;                  /* WRITE BACK UPDATED F4 DSCB    */
Z008F4WR L     @15,@CV04233                                        0328
         BALR  @14,@15                                             0328
*   CCAURAB=CCARALSA;               /* RESTORE PTR TO INPUT RCD      */
         MVC   CCAURAB(4,R11),CCARALSA(R11)                        0329
*   IF CCACD1^=RCS THEN             /* IF WRITE UNSUCCESSFUL         */
         CLI   CCACD1(R11),0                                       0330
         BE    @RF00330                                            0330
*     CALL EXIT;                    /* GET OUT                       */
         BAL   @14,EXIT                                            0331
*   IF RCDID=TYPV THEN              /* IS INPUT RCD TYPE "V"         */
@RF00330 L     @10,CCARAREC(,R11)                                  0332
         CLI   RCDID(@10),C'V'                                     0332
         BNE   @RF00332                                            0332
*     CTVOLTIM=CCAVTS;              /* Y,ST NEW TIMESTMP INTO RCD    */
         MVC   CTVOLTIM+48(8,@10),CCAVTS(R11)                      0333
*   ELSE                            /* INPUT RECORD IS TYPE "W"      */
*     DO;                           /* MUST LOCATE BASE VOL RCD      */
         B     @RC00332                                            0334
@RF00332 DS    0H                                                  0335
*       CCADDDD=CCACBASE*CILEN;     /* PREPARE TO READ VOL RCD       */
         SLR   @10,@10                                             0335
         ICM   @10,7,CCACBASE(R11)                                 0335
         SLA   @10,9                                               0335
         ST    @10,CCADDDD(,R11)                                   0335
*       CCAMLRET=R13+TWELVE;        /* ST RETURN ADDR                */
         LA    @10,12                                              0336
         ALR   @10,R13                                             0336
         ST    @10,CCAMLRET(,R11)                                  0336
*       XIOOPT=160;                 /* GET OPTIONS                   */
         MVI   XIOOPT(R11),X'A0'                                   0337
*Z009XIO:                                                          0338
*       CALL IGGPXIO;               /* GET VOL RCD FROM CATLG        */
Z009XIO  L     @15,@CV04236                                        0338
         BALR  @14,@15                                             0338
*       IF CCACD1^=RCS THEN         /* IF UNSUCCESSFUL               */
         CLI   CCACD1(R11),0                                       0339
         BE    @RF00339                                            0339
*         DO;                                                      0340
*           CCASRCIN=RCDCINO;       /* RESTORE INPUT RCD CI #        */
         L     @10,CCARAREC(,R11)                                  0341
         MVC   CCASRCIN(3,R11),RCDCINO(@10)                        0341
*           CALL EXIT;              /* GET OUT                       */
         BAL   @14,EXIT                                            0342
*         END;                                                     0343
*       R1=CCARABB->CCACPE1;        /* PT TO BASE RCD AREA           */
@RF00339 L     @10,CCARABB(,R11)                                   0344
         L     R1,CCACPE1(,@10)                                    0344
*       IF R1->RCDID=TYPV THEN      /* DOES IT CONTAIN VOL RCD       */
         CLI   RCDID(R1),C'V'                                      0345
         BNE   @RF00345                                            0345
*         DO;                       /* YES                           */
*           RFY                                                    0347
*             CATRCD BASED(R1);                                    0347
*           CTVOLTIM=CCAVTS;        /* ST NEW TMESTMP INTO VOL RCD   */
         MVC   CTVOLTIM+48(8,R1),CCAVTS(R11)                       0348
*           RFY                                                    0349
*             CATRCD BASED(CCARAREC);                              0349
*         END;                                                     0350
*       CCARAREC=CCAR0REC;          /* PT TO VOL RCD                 */
@RF00345 L     @10,CCAR0REC(,R11)                                  0351
         ST    @10,CCARAREC(,R11)                                  0351
*       CTVOLTIM=CCAVTS;            /* ST NEW TMESTMP INTO VOL RCD   */
         MVC   CTVOLTIM+48(8,@10),CCAVTS(R11)                      0352
*       CCARARBA=CATVOLCI;          /* ST VOL RCD CRA CI NO          */
         MVC   CCARARBA(4,R11),@CF02656                            0353
*       XIOOPT=0;                   /* PUT OPTIONS                   */
         MVI   XIOOPT(R11),X'00'                                   0354
*Z010XRIO:                                                         0355
*       CALL IGGPXRIO;              /* WRITE VOL RCD INTO CRA        */
Z010XRIO L     @15,@CV04237                                        0355
         BALR  @14,@15                                             0355
*       IF CCACD1=RCS THEN          /* IF SUCCESSFUL                 */
         CLI   CCACD1(R11),0                                       0356
         BNE   @RF00356                                            0356
*         DO;                                                      0357
*           CCADDDD=RCDCINO*CILEN;  /* ST VOL RCD CATLG CI #         */
         L     @10,CCARAREC(,R11)                                  0358
         SLR   @05,@05                                             0358
         ICM   @05,7,RCDCINO(@10)                                  0358
         SLA   @05,9                                               0358
         ST    @05,CCADDDD(,R11)                                   0358
*           CCAMLRET=R13+TWELVE;    /* ST RETURN ADDR                */
         LA    @10,12                                              0359
         ALR   @10,R13                                             0359
         ST    @10,CCAMLRET(,R11)                                  0359
*           XIOOPT=32;              /* PUT OPTIONS                   */
         MVI   XIOOPT(R11),X'20'                                   0360
*Z011XIO:                                                          0361
*           CALL IGGPXIO;           /* WRITE VOL RCD TO CAT          */
Z011XIO  L     @15,@CV04236                                        0361
         BALR  @14,@15                                             0361
*         END;                                                     0362
*       CCARAREC=CCACPE1;           /* RESTORE PTR TO INPUT RCD      */
@RF00356 L     @10,CCAURAB(,R11)                                   0363
         L     @10,CCACPE1(,@10)                                   0363
         ST    @10,CCARAREC(,R11)                                  0363
*       CCASRCIN=RCDCINO;           /* RESTORE INPUT RCD CAT CI NO   */
         MVC   CCASRCIN(3,R11),RCDCINO(@10)                        0364
*       IF CCACD1^=RCS THEN         /* WERE WRITES OK                */
         CLI   CCACD1(R11),0                                       0365
         BE    @RF00365                                            0365
*         CALL EXIT;                /* NO,GET OUT                    */
         BAL   @14,EXIT                                            0366
*     END;                                                         0367
@RF00365 DS    0H                                                  0368
*   CAXF2VTU=ON;                    /* IND THAT TMESTMPS WERE UPD    */
@RC00332 L     @10,CCARAACB(,R11)                                  0368
         L     @10,ACBUAPTR(,@10)                                  0368
         OI    CAXF2VTU(@10),B'00000001'                           0368
*   CALL EXIT;                      /* RETURN TO CALLER              */
         BAL   @14,EXIT                                            0369
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */
         DS 0F
*   GEN(DC CL8'IGGPMODI');          /* NAME OF FOLLOWING PROCEDURE   */
         DC CL8'IGGPMODI'
*   END;                            /* END OF INNER PROCEDURE        */
@EL00002 DS    0H                                                  0372
@EF00002 DS    0H                                                  0372
@ER00002 BR    @14                                                 0372
@PB00002 DS    0H                                                  0373
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */
         EJECT
*                                                                  0374
*/********************************************************************/
*/*                                                                  */
*/*        IGGPMODI  -  PROCEDURE  EXTERNALLY  CALLED  NESTED        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0374
*IGGPMODI:                                                         0374
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */
         B     @PB00003                                            0374
IGGPMODI DS    0H                                                  0375
*   GEN(ENTRY IGGPMODI );           /* IDENTIFY ENTRY-POINT SYMBOL   */
         ENTRY IGGPMODI
*Z012:                              /* LABEL FOR PROCEDURE TRACKING  */
*Z012MODI:                          /* LABEL FOR PROCEDURE TRACKING  */
*   RFY                                                            0376
*    (R5,                                                          0376
*     R6,                                                          0376
*     R7,                                                          0376
*     R8,                                                          0376
*     R9,                                                          0376
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */
Z012     DS    0H                                                  0376
Z012MODI DS    0H                                                  0377
*   RFY                                                            0377
*    (R2,                                                          0377
*     R3,                                                          0377
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */
         STM 12,14,12(13)
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */
         LA 13,12(13)
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */
         BALR 12,0
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */
         LA 15,*-IGGSEGST(0,0)
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */
         SR 12,15
*/*********************************************************************
**                                                                    *
**   PROCEDURE NAME = IGGPMODI                                        *
**                                                                    *
**   PROCEDURE TYPE = EXTERNALLY CALLED, NESTED                       *
**                                                                    *
**   DESCRIPTIVE NAME = INTERFACE TO MODIFY FUNCTION                  *
**                                                                    *
**   FUNCTION =                                                       *
**      THIS PROCEDURE IS CALLED BY THE DEFINE CRA MODULE (IGG0CLB4)  *
**      IN ORDER TO INTERFACE TO THE CATALOG 'MODIFY' ROUTINE         *
**      IGG0CLAV(IGGPMOD) TO COMPLETE THE RECORD-BUILDING PROCESS     *
**      DURING DEFINE CRA PROCESSING:                                 *
**      1.  CONSTRUCT THE 'MODIFY' CPL                                *
**      2.  CONSTRUCT AN FPL AND RELATED AMDSB                        *
**      3.  CONSTRUCT AN FPL AND RELATED ASSOCIATION SET OF FIELDS    *
**      4.  CONSTRUCT AN FPL AND RELATED VOLUME INFORMATION           *
**          SET OF FIELDS                                             *
**      5.  PREPARE THE CCA FOR THE CALL TO 'MODIFY'                  *
**      6.  CALL THE CATALOG 'MODIFY' FUNCTION                        *
**      7.  RESTORE ESSENTIAL VALUES AND RETURN TO CALLER             *
**                                                                    *
**   INPUT =                                                          *
**      ADDRESS OF CCA IN REGISTER 11                                 *
**      ADDRESS OF DEFINE CRA WORKAREA IN DCRAWAPR (REGISTER 3)       *
**                                                                    *
**   OUTPUT =                                                         *
**      IF ERROR ENCOUNTERED, 'CCAPROB' CONTAINS PROBLEM              *
**         DETERMINATION INFORMATION FROM THE 'MODIFY' FUNCTION.      *
**      CRA BASE DATA RECORD AND EXTENSION RECORD BUILD COMPLETED     *
**         IN DEFINE CRA'S WORKAREA.                                  *
**                                                                    *
**********************************************************************/
*/********************************************************************/
*/*                 CONSTRUCT  THE  MODIFY  CPL                      */
*/********************************************************************/
*                                                                  0383
*   RFY                                                            0383
*     CTGPL BASED(CCAMCPL);         /* MAP THE CPL TO BUILD IN CORE  */
*   CCAMCPL=ADDR(DCRAMCPL);         /* SET ADDR OF MODIFY CPL        */
         LA    @10,DCRAMCPL(,DCRAWAPR)                             0384
         ST    @10,CCAMCPL(,R11)                                   0384
*   CTGNOFLD=NUMFPLS;               /* NUMBER OF DATA OCCURRENCES    */
         MVI   CTGNOFLD(@10),X'03'                                 0385
*   CTGFIELD(1)=ADDR(DCRAMFPL);     /* SET ADDR OF THE AMDSB FPL     */
         LA    @05,DCRAMFPL(,DCRAWAPR)                             0386
         ST    @05,CTGFIELD(,@10)                                  0386
*   CTGFIELD(2)=ADDR(DCRASFPL);     /* SET ADDR OF THE ASSOC FPL     */
         LA    @05,DCRASFPL(,DCRAWAPR)                             0387
         ST    @05,CTGFIELD+4(,@10)                                0387
*   CTGFIELD(3)=ADDR(DCRAVFPL);     /* SET ADDR OF THE VGO FPL       */
         LA    @05,DCRAVFPL(,DCRAWAPR)                             0388
         ST    @05,CTGFIELD+8(,@10)                                0388
*/********************************************************************/
*/*      CONSTRUCT AN FPL AND RELATED AMDSB FOR CALL TO MODIFY       */
*/********************************************************************/
*                                                                  0389
*   RFY                                                            0389
*     CTGFL BASED(CTGFIELD(1));     /* MAP THE AMDSB FPL             */
*   CTGFLDNO=NUMLAPRS;              /* NO. OF LENGTH/ADDR PAIRS      */
         L     @10,CTGFIELD(,@10)                                  0390
         MVI   CTGFLDNO(@10),X'01'                                 0390
*   CTGFLDNM=ADDR(AMDSBCAT);        /* SET FIELD NAME ADDRESS        */
         LA    @05,@CC04202                                        0391
         ST    @05,CTGFLDNM(,@10)                                  0391
*   CTGFLNG(1)=LENGTH(IDAAMDSB);    /* LENGTH OF THE DATA (AMDSB)    */
         LA    @05,96                                              0392
         ST    @05,CTGFLNG(,@10)                                   0392
*   CTGFLPT(1)=ADDR(DCRAMDSB);      /* SET ADDRESS OF THE AMDSB      */
         LA    @15,DCRAMDSB(,DCRAWAPR)                             0393
         ST    @15,CTGFLPT(,@10)                                   0393
*   RFY                                                            0394
*     IDAAMDSB BASED(CTGFLPT(1));   /* MAP THE AMDSB                 */
*   AMDSBID=AMDIDVAL;               /* SET AMDSB CONTROL BLOCK ID    */
         MVI   AMDSBID(@15),X'60'                                  0395
*   AMDCIPCA=DCRABTRK               /* TOTAL NO. OF CI'S PER CA      */
*       *DCRATRCY;                                                 0396
         LH    @10,DCRABTRK(,DCRAWAPR)                             0396
         MH    @10,DCRATRCY(,DCRAWAPR)                             0396
         STH   @10,AMDCIPCA(,@15)                                  0396
*   AMDLEN=LENGTH(IDAAMDSB);        /* SET LENGTH OF THE AMDSB       */
         STH   @05,AMDLEN(,@15)                                    0397
*   AMDCINV=DCRACISZ;               /* SET CI SIZE (512 BYTES)       */
         MVC   AMDCINV(4,@15),@CF01041                             0398
*   AMDLRECL=DCRARCSZ;              /* SET MAX RECRD SIZE (505 BYTES)*/
         MVC   AMDLRECL(4,@15),@CF04146                            0399
*   AMDNEXT=CCANDEXT;               /* SET NUMBER OF EXTENTS FOR CRA */
         SLR   @10,@10                                             0400
         IC    @10,CCANDEXT(,R11)                                  0400
         STH   @10,AMDNEXT(,@15)                                   0400
*   IF CCAF2CB=OFF THEN             /* CALLER IS DEFINE SPACE        */
         TM    CCAF2CB(R11),B'00000010'                            0401
         BNZ   @RF00401                                            0401
*     IF(CCAACB->ACBAMBL->AMBLDTA->AMBDSB->AMDWCK=ON) THEN/* CATALOG
*                                      IS WRITE-CHECK                */
         L     @10,CCAACB(,R11)                                    0402
         L     @10,ACBAMBL(,@10)                                   0402
         L     @10,AMBLDTA(,@10)                                   0402
         L     @10,AMBDSB(,@10)                                    0402
         TM    AMDWCK(@10),B'01000000'                             0402
         BNO   @RF00402                                            0402
*       AMDWCK=ON;                  /* SET CRA AS WRITE-CHECK ALSO   */
         OI    AMDWCK(@15),B'01000000'                             0403
*     ELSE                                                         0404
*       ;                           /* CATALOG IS NOT WRITE-CHECK    */
@RF00402 DS    0H                                                  0405
*   ELSE                            /* CALLER IS DEFINE CATALOG      */
*     IF(CCACPL->CTGFVT->CTGFVDCH->CTGFVAMD->CTGFLPT(1)->AMDWCK=ON)
*       THEN                        /* CATALOG IS WRITE-CHECK        */
         B     @RC00401                                            0405
@RF00401 L     @10,CCACPL(,R11)                                    0405
         L     @10,CTGFVT(,@10)                                    0405
         L     @10,CTGFVDCH(,@10)                                  0405
         L     @10,CTGFVAMD(,@10)                                  0405
         L     @10,CTGFLPT(,@10)                                   0405
         TM    AMDWCK(@10),B'01000000'                             0405
         BNO   @RF00405                                            0405
*       AMDWCK=ON;                  /* SET CRA AS WRITE-CHECK ALSO   */
         L     @10,CCAMCPL(,R11)                                   0406
         L     @10,CTGFIELD(,@10)                                  0406
         L     @10,CTGFLPT(,@10)                                   0406
         OI    AMDWCK(@10),B'01000000'                             0406
*     ELSE                                                         0407
*       ;                           /* CATALOG IS NOT WRITE-CHECK    */
@RF00405 DS    0H                                                  0408
*/********************************************************************/
*/*   CONSTRUCT AN FPL AND RELATED ASSOCIATION FOR CALL TO MODIFY    */
*/********************************************************************/
*                                                                  0408
*   RFY                                                            0408
*     CTGFL BASED(CTGFIELD(2));     /* MAP THE ASSOCIATION FPL       */
@RC00401 DS    0H                                                  0409
*   CTGFLDNO=NUMLAPRS;              /* NO. OF LENGTH/ADDRESS PAIRS   */
         L     @10,CCAMCPL(,R11)                                   0409
         L     @05,CTGFIELD+4(,@10)                                0409
         MVI   CTGFLDNO(@05),X'01'                                 0409
*   CTGFLDNM=ADDR(NAMEDS);          /* SET FIELD NAME ADDRESS        */
         LA    @15,@CC04185                                        0410
         ST    @15,CTGFLDNM(,@05)                                  0410
*   CTGFLNG(1)=LENGTH(DCRASSOC);    /* SET DATA LENGTH               */
         MVC   CTGFLNG(4,@05),@CF00058                             0411
*   CTGFLPT(1)=ADDR(DCRASSOC);      /* SET DATA LOCATION             */
         LA    @15,DCRASSOC(,DCRAWAPR)                             0412
         ST    @15,CTGFLPT(,@05)                                   0412
*   DCRATYPE=CATCLSID;              /* SET RECORD AS TYPE 'C'        */
         MVI   DCRATYPE(DCRAWAPR),C'C'                             0413
*   DCRANAME=CLUSTRCI;              /* SET CI NUMBER OF CLUSTER RECD */
         MVC   DCRANAME(3,DCRAWAPR),@CF00055+1                     0414
*/********************************************************************/
*/*         CONSTRUCT AN FPL AND RELATED VOLUME INFORMATION          */
*/*         SET OF FIELDS FOR CALL TO MODIFY ROUTINE                 */
*/********************************************************************/
*                                                                  0415
*   RFY                                                            0415
*     CTGFL BASED(CTGFIELD(3));     /* MAP THE VOL OCCUR FPL         */
*   CTGFLDNO=NUMLAPRS;              /* NO. OF LENGTH/ADDRESS PAIRS   */
         L     @10,CTGFIELD+8(,@10)                                0416
         MVI   CTGFLDNO(@10),X'01'                                 0416
*   CTGFLDGC=CATVOLGC;              /* SET VOLUME OCCUR GROUP CODE   */
         MVI   CTGFLDGC(@10),X'03'                                 0417
*   CTGFLDNM=ADDR(ENTVOL);          /* SET FIELD NAME ADDRESS        */
