         TITLE 'ISTRCC21: CONTROL LAYER RECEIVE OUTBOUND PROCESSOR     *
                        '
ISTRCC21 CSECT ,                                                   0001
@MAINENT BALR  @15,0                                               0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ISTRCC21  77.320'                                 0001
         DROP  @15
@PROLOG  BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*   ;                               /* INITIATE ASSEMBLER COMMENTING */
*                                                                  0420
*/********************************************************************
*                                                                    */
*/*                                                                  */
*/*  CRRINIT - SEGMENT OF ISTRCC21                                   */
*/*                                                                  */
*/*          CRRINIT ESTABLISHES THE ADDRESSABILITY TO               */
*/*          THE COMPONENT RECOVERY RECORD (CRR), THE                */
*/*          REQUEST PARAMETER LIST (RPL), THE ACCESS                */
*/*          METHOD CVT (ATCVT), AND THE MAJOR CONTROL               */
*/*          BLOCK ( EITHER THE ACDEB OR THE FMCB,                   */
*/*          DEPENDING UPON WHETHER THE REQUEST IS ANY               */
*/*          OR SPECIFIC, RESPECTIVELY ) WHOSE TYPE IS               */
*/*          UNKNOWN DURING THE EXECUTION OF CRRINIT.                */
*/*          THE CRR RPL SATISFIED SWITCH, THE THREE RTYPE           */
*/*          SWITCHES (CRRDFSYN, CRRDFASY, CRRRESP   )               */
*/*          ARE CLEARED TO NOT ACTIVE STATUS.                       */
*/*          THE MODULE AUDIT FIELD IS CLEARED AND THEN              */
*/*          THE MODULE AUDIT AND AUDIT TRAIL SWITCHES               */
*/*          ARE SET. THE RPL REQUEST TYPE IS CHECKED .              */
*/*          IF THE REQUEST IS FOR RECORD DATA, THE                  */
*/*          CRR RTYPE BITS ARE SET TO MATCH THE RPL RTYPE           */
*/*          BITS.                                                   */
*/*          IF  NO RPL RTYPES ARE ACTIVE, THEN THE CRR              */
*/*          MAJOR RETURN CODE IS SET TO INDICATE THAT A             */
*/*          LOGICAL ERROR HAS OCCURED WITH THE MINOR                */
*/*          RETURN CODE SET TO INDICATE THAT NO RTYPE               */
*/*          WAS SPECIFIED. NOTE: THE RPL RTYPE BIT FOR              */
*/*          SYNCHRONOUS DATA REQUESTS (RPLNFSYN) IS A               */
*/*          NEGATIVE LOGIC BIT, IE. '0'B INDICATES A                */
*/*          REQUEST AND IS THE DEFAULT.                             */
*/*                                                                  */
*/********************************************************************
*                                                                    */
*                                                                  0420
*   RESPECIFY                                                      0420
*     ISTCRA BASED(RPHCRR);         /* ADDRESS THE CRA               */
*   RESPECIFY                                                      0421
*     ISTDCCRR BASED(CRACRR);       /* ADDRESS THE DCCRR             */
*   CRRPTR=ADDR(CRRSPACE);          /* GET THE CRR POINTER           */
         L     @15,RPHCRR(,RPHPTR)                                 0422
         L     CRRPTR,CRACRR(,@15)                                 0422
         LA    CRRPTR,CRRSPACE(,CRRPTR)                            0422
*   RESPECIFY                                                      0423
*     CRRSAREA BASED(CRRPTR);       /* ADDRESS THE CRR               */
*   RPLPTR=CRRARPL;                 /* GET THE RPL POINTER           */
         L     RPLPTR,CRRARPL(,CRRPTR)                             0424
*   RESPECIFY                                                      0425
*     IFGRPL BASED(RPLPTR);         /* ADDRESS THE RPL               */
*   RPLRLEN=0;                      /* ZERO RECORD LENGTH@XM9777K    */
         SLR   @15,@15                                             0426
         ST    @15,RPLRLEN(,RPLPTR)                                0426
*   RESPECIFY                                                      0427
*     ISTATCVT BASED(CRRATCVT);     /* ADDRESS THE CVT               */
*   FMCBPTR=RPHMAJCB;               /* GET MAJ CB POINTER            */
         L     FMCBPTR,RPHMAJCB-1(,RPHPTR)                         0428
         LA    FMCBPTR,0(,FMCBPTR)                                 0428
*   CRRRPLS=ZERB;                   /* CLEAR RPL SATISFIED           */
*   CRRDFSYN=ZERB;                  /* CLEAR SYN RTYPE               */
*   CRRDFASY=ZERB;                  /* CLEAR ASY RTYP                */
*   CRRRESP=ZERB;                   /* CLEAR RESP RTYPE              */
         NI    CRRRPLS(CRRPTR),B'01110001'                         0432
*   CRRMAS=''B;                     /* CLEAR MODULE AUDIT            */
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0433
*   CRRRC21=ONEB;                   /* SET AUDIT TRAIL               */
         OI    CRRRC21(CRRPTR),B'10000000'                         0434
*   CRRRM21=ONEB;                   /* SET MODULE AUDIT              */
         OI    CRRRM21(CRRPTR),B'10000000'                         0435
*   IF RPLREQ='1D'X                 /* IS REQUEST A READ ?           */
*     THEN                                                         0436
         CLI   RPLREQ(RPLPTR),29                                   0436
         BNE   @RF00436                                            0436
*     CRRDFSYN=ONEB;                /* YES, SET NSNA RTYPE           */
         OI    CRRDFSYN(CRRPTR),B'00000010'                        0437
*   ELSE                                                           0438
*     DO;                           /* NO                            */
         B     @RC00436                                            0438
@RF00436 DS    0H                                                  0439
*       IF RPLRRESP=ONEB×RPLNFSYN=ZERB×RPLDFASY=ONEB THEN          0439
         TM    RPLRRESP+16(RPLPTR),B'00001010'                     0439
         BNZ   @RT00439                                            0439
         TM    RPLNFSYN+16(RPLPTR),B'00000100'                     0439
         BO    @RF00439                                            0439
@RT00439 DS    0H                                                  0440
*         DO;                       /* YES                           */
*           IF RPLRRESP=ONEB        /* RESPONSE ?                    */
*             THEN                                                 0441
         TM    RPLRRESP+16(RPLPTR),B'00001000'                     0441
         BNO   @RF00441                                            0441
*             CRRRESP=ONEB;         /* YES, SET RESP RTYPE           */
         OI    CRRRESP(CRRPTR),B'00000100'                         0442
*           IF RPLNFSYN=ZERB        /* SYNCHRONOUS ?                 */
*             THEN                                                 0443
@RF00441 TM    RPLNFSYN+16(RPLPTR),B'00000100'                     0443
         BNZ   @RF00443                                            0443
*             CRRDFSYN=ONEB;        /* YES, SET SYN RTYPE            */
         OI    CRRDFSYN(CRRPTR),B'00000010'                        0444
*           IF RPLDFASY=ONEB        /* ASYNCHRONOUS ?                */
*             THEN                                                 0445
@RF00443 TM    RPLDFASY+16(RPLPTR),B'00000010'                     0445
         BNO   @RF00445                                            0445
*             CRRDFASY=ONEB;        /* YES, SET ASY RTYPE            */
         OI    CRRDFASY(CRRPTR),B'00001000'                        0446
*         END;                      /* RTYPES SET IN CRR             */
*       ELSE                                                       0448
*         DO;                       /* NO                            */
         B     @RC00439                                            0448
@RF00439 DS    0H                                                  0449
*           CRRRTNCD=CRRLOGIC;      /* SET LOGIC ERROR               */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0449
*           CRRFDB2=CRRRNORT;       /* SET NO RTYPE                  */
         MVI   CRRFDB2(CRRPTR),X'02'                               0450
*         END;                      /* ERROR RETURN IS SET           */
*     END;                          /* CRR IS INITIALIZED            */
@RC00439 DS    0H                                                  0453
*/********************************************************************
*                                                                    */
*/*   END OF SEGMENT CRRINIT                                         */
*/********************************************************************
