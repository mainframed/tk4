*                 CRRPTR) UNRSTD;   /* RELEASE REGS          @D31KKNL*/
*/*** START -TPLOCK (DEBLOCK)TYPE(XCLSIVE)- **                       */
*                                                                  0471
*               DO;                                                0471
*                 DCL                                              0472
*                   GPR05F FIXED(31) REG(05);                      0472
*                 DCL                                              0473
*                   GPR05P PTR(31) REG(05);                        0473
*                 DCL                                              0474
*                   GPR06F FIXED(31) REG(06);                      0474
*                 DCL                                              0475
*                   GPR06P PTR(31) REG(06);                        0475
*                 DCL                                              0476
*                   GPR07F FIXED(31) REG(07);                      0476
*                 DCL                                              0477
*                   GPR07P PTR(31) REG(07);                        0477
*                 DCL                                              0478
*                   GPR08F FIXED(31) REG(08);                      0478
*                 DCL                                              0479
*                   GPR08P PTR(31) REG(08);                        0479
*                 DCL                                              0480
*                   GPR09F FIXED(31) REG(09);                      0480
*                 DCL                                              0481
*                   GPR09P PTR(31) REG(09);                        0481
*                 DCL                                              0482
*                   GPR10F FIXED(31) REG(10);                      0482
*                 DCL                                              0483
*                   GPR10P PTR(31) REG(10);                        0483
*                 DCL                                              0484
*                   GPR11F FIXED(31) REG(11);                      0484
*                 DCL                                              0485
*                   GPR11P PTR(31) REG(11);                        0485
*                 DCL                                              0486
*                   GPR12F FIXED(31) REG(12);                      0486
*                 DCL                                              0487
*                   GPR12P PTR(31) REG(12);                        0487
*                 DCL                                              0488
*                   GPR13F FIXED(31) REG(13);                      0488
*                 DCL                                              0489
*                   GPR13P PTR(31) REG(13);                        0489
*                 DCL                                              0490
*                   GPR14F FIXED(31) REG(14);                      0490
*                 DCL                                              0491
*                   GPR14P PTR(31) REG(14);                        0491
*                 DCL                                              0492
*                   GPR00F FIXED(31) REG(00);                      0492
*                 DCL                                              0493
*                   GPR00P PTR(31) REG(00);                        0493
*                 DCL                                              0494
*                   GPR01F FIXED(31) REG(01);                      0494
*                 DCL                                              0495
*                   GPR01P PTR(31) REG(01);                        0495
*                 DCL                                              0496
*                   GPR02F FIXED(31) REG(02);                      0496
*                 DCL                                              0497
*                   GPR02P PTR(31) REG(02);                        0497
*                 DCL                                              0498
*                   GPR03F FIXED(31) REG(03);                      0498
*                 DCL                                              0499
*                   GPR03P PTR(31) REG(03);                        0499
*                 DCL                                              0500
*                   GPR04F FIXED(31) REG(04);                      0500
*                 DCL                                              0501
*                   GPR04P PTR(31) REG(04);                        0501
*                 RFY                                              0502
*                  (GPR00P,                                        0502
*                   GPR01P,                                        0502
*                   GPR02P,                                        0502
*                   GPR03P,                                        0502
*                   GPR04P,                                        0502
*                   GPR15P) RSTD;                                  0502
*                 GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(
*                     RPHWORK);                                    0503
         STM GPR00P,GPR14P,RPHWORK(GPR01P)
*                 GPR02P=ADDR(ACDLOCK);/* SET PARAM - LOCKWORD PTR   */
         L     GPR02P,RPHMAJCB-1(,RPHPTR)                          0504
         LA    GPR02P,0(,GPR02P)                                   0504
         LA    GPR02P,ACDLOCK(,GPR02P)                             0504
*                 GPR04P=GPR01P->RPHCRR;/* SET PARAM - ISTCRA PTR    */
         L     GPR04P,RPHCRR(,GPR01P)                              0505
*                 GPR00P=CRALMS05;  /* SET PARAM - LOCK LEVEL FLAG   */
         LA    GPR00P,16                                           0506
*                 GPR03P=ADDR(GPR04P->CRAL5PTR);/* SET PARAM - LOCK
*                                      LEVEL PTR                     */
         LA    GPR03P,CRAL5PTR(,GPR04P)                            0507
*                 GPR15P=ATCLCPTR->ATCAP36;/* ADDRESS OF LOCK ROUTINE*/
         L     @10,ATCLCPTR                                        0508
         L     GPR15P,ATCAP36(,@10)                                0508
*                 DCL                                              0509
*                   FMZL0008 BASED(GPR15P) LABEL VALUERANGE(*);    0509
*                 CALL FMZL0008;                                   0510
         BALR  @14,@15                                             0510
*                 GENERATE REFS(GPR00P,GPR01P,GPR14P,RPHWORK) SETS(
*                     GPR00P,GPR01P,GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,
*                     GPR07P,GPR08P,GPR09P,GPR10P,GPR11P,GPR12P,GPR13P,
*                     GPR14P,GPR15P);                              0511
         LM GPR00P,GPR15P,RPHWORK(GPR01P)
*                 RFY                                              0512
*                  (GPR00P,                                        0512
*                   GPR01P,                                        0512
*                   GPR02P,                                        0512
*                   GPR03P,                                        0512
*                   GPR04P,                                        0512
*                   GPR15P) UNRSTD;                                0512
*               END;                                               0513
*                                                                  0513
*/*** END -TPLOCK (DEBLOCK)TYPE(XCLSIVE)- **                         */
*                                                                  0514
*                                   /* LOCK THE DEB                  */
*               RFY                                                0514
*                (RPLPTR,                                          0514
*                 CRRPTR,                                          0514
*                 FSBPTR) RSTD;     /* RESTRICT REGS         @D31KKNL*/
*               CRRPTR=ADDR(CRRSPACE);/* RE-INIT CRR         @D31KKNL*/
         L     @15,RPHCRR(,RPHPTR)                                 0515
         L     CRRPTR,CRACRR(,@15)                                 0515
         LA    CRRPTR,CRRSPACE(,CRRPTR)                            0515
*               RPLPTR=CRRARPL;     /* RE-INIT RPL           @D31KKNL*/
         L     RPLPTR,CRRARPL(,CRRPTR)                             0516
*               CRRDBLCK=ONEB;      /* SET DEB LOCK FLG      @D33SKD3*/
         OI    CRRDBLCK(CRRPTR),B'00010000'                        0517
*/*VMDESET  ((15),VTAM)                                              */
*                                                                  0518
*               DO;                 /* INVOKE MODESET MACRO          */
*                 GEN;                                             0519
         MODESET EXTKEY=VTAM,WORKREG=2
*                 RFY                                              0520
*                  (GPR15P) RSTD;                                  0520
*                 GPR15F=0;                                        0521
         SLR   GPR15F,GPR15F                                       0521
*                 RFY                                              0522
*                  (GPR15P) UNRSTD; /* PUT RETURN CODE IN RCREG      */
*               END;                /* VMDESET GO TO VTAM KEY        */
*             END;                  /*                       @D33SKD3*/
*/********************************************************************
*                                                                    */
*/*                                                                  */
*/*  ISTRCC21 - MAINLINE 3                                           */
*/*                                                                  */
*/*          THIS PORTION OF THE MAINLINE CODE CHECKS THE            */
*/*          MAJOR CONTROL BLOCK TYPE AND IF ACDEB,                  */
*/*          EXECUTES SEGMENT FNDATAA, IF FMCB, EXECUTES             */
*/*          SEGMENT FNDATAS. THESE SEGMENTS SEARCH THE              */
*/*          APPROPRIATE DATA QUEUES FOR DATA WHICH WILL             */
*/*          SATISFY THE RPL UNDER CONSIDERATION. AFTER              */
*/*          ONE OF THE ABOVE SEGMENTS HAS COMPLETED                 */
*/*          EXECUTION, THE CRR RPL SATISFIED SWITCH IS              */
*/*          TESTED TO SEE IF QUALIFYING DATA WAS FOUND.             */
*/*          IF NO DATA WAS FOUND, THE ACDEB LOCK IS RELEASED        */
*/*          IF IT WAS OBTAINED AND A TPESC MACRO IS ISSUED.         */
*/*          IF ASYNCHRONOUS OR RESPONSE DATA WAS FOUND              */
*/*          A TPDVTS MACRO IS ISSUED TO PASS CONTROL                */
*/*          TO THE ASYNCHRONOUS FEEDBACK PROCESSOR. IF              */
*/*          NON-SNA OR SYNCHRONOUS SNA DATA WAS FOUND               */
*/*          A CHECK FOR OUTBOUND PURGE REQUEST (BIT                 */
*/*          FMCOBPRG IN THE FMCB) IS MADE, AND IF SO,               */
*/*          SEGMENT PURGE IS EXECUTED. IF AFTER PURGE               */
*/*          HAS COMPLETED EXECUTION, THE CRR RPL                    */
*/*          SATISFIED SWITCH IS STILL ON, OR IF PURGE               */
*/*          WAS NOT REQUESTED, A TPDVTS MACRO IS                    */
*/*          ISSUED TO PASS CONTROL TO THE SYNCHRONOUS               */
*/*          FEEDBACK PROCESSOR. IF PURGE WAS EXECUTED               */
*/*          AND CRR RPL SATISFIED IS OFF ( NO DATA                  */
*/*          REMAINS ON THE QUEUE ), ISTRCC63 IS CALLED              */
*/*          ( AS AN IN LINE SUBROUTINE ) TO DO COMMON               */
*/*          HOUSEKEEPING FUNCTIONS FOR THE SYNCHRONOUS              */
*/*          DATA QUEUE AND THE ACDEB LOCK IS RELEASED               */
*/*          IF IT WAS OBTAINED AND A TPESC MACRO IS ISSUED          */
*/*          TO PASS CONTROL TO THE NEXT PROCESS.                    */
*/*                                                                  */
*/********************************************************************
*                                                                    */
*                                                                  0525
*                                                                  0525
*/********************************************************************
*                                                                    */
*/*                                                                  */
*/*  FNDATAA - SEGMENT OF ISTRCC21                                   */
*/*                                                                  */
*/*           FNDATAA SEARCHES THE DATA QUEUES CHAINED               */
*/*           FROM THE ACDEB TO FIND AN FMCB WHICH WILL              */
*/*           SATISFY THE RPL UNDER CONSIDERATION ( EITHER           */
*/*           THE RPL WHICH INITIATED THIS PASS THROUGH              */
*/*           THE CONTROL LAYER OR AN RPL SELECTED FROM              */
*/*           THE WAIT QUEUE ). THIS SEARCH IS DONE IN               */
*/*           SUCH A MANNER AS TO GIVE PRIORITY TO DFASY             */
*/*           DATA, THEN RESP DATA, THEN DFSYN DATA                  */
*/*           IF A QUALIFIED FMCB IS FOUND,                          */
*/*           THE FMCB LOCK IS OBTAINED, THE CRR RPL                 */
*/*           SATISFIED SWITCH (CRRRPLS) IS TURNED ON,               */
*/*           THE ADDRESS OF THE FMCB IS PLACED IN THE               */
*/*           CRR, THE OLDEST FSB ON THE CORRESPONDING               */
*/*           FMCB DATA QUEUE IS OBTAINED, AND THE FSB               */
*/*           ADDRESS IS PLACED IN THE CRR. ALL CRR RTYPES           */
*/*           EXCEPT THE ONE SATISFIED ARE TURNED OFF                */
*/*           IN THE CRR. IF NO FMCB IS FOUND, THE CRRRPLS           */
*/*           SWITCH IS LEFT OFF.                                    */
*/*                                                                  */
*/********************************************************************
*                                                                    */
*                                                                  0525
*           FSBPTR=0;               /* CLEAR FSB POINTER             */
@RF00460 SLR   @15,@15                                             0525
         LR    FSBPTR,@15                                          0525
*           FMCBPTR=ACDASFQ&ZAPREG; /* GET ASY DATA QUEUE            */
         L     @05,RPHMAJCB-1(,RPHPTR)                             0526
         LA    @05,0(,@05)                                         0526
         L     FMCBPTR,ACDASFQ(,@05)                               0526
         N     FMCBPTR,@CF01332                                    0526
*           IF CRRDFASY=ONEB&FMCBPTR^=0/* DATA ON QUEUE ?            */
*             THEN                                                 0527
         TM    CRRDFASY(CRRPTR),B'00001000'                        0527
         BNO   @RF00527                                            0527
         CR    FMCBPTR,@15                                         0527
         BE    @RF00527                                            0527
*             DO;                   /* YES                           */
*/* TPFEL (,ACDASFQ) RETPTR(FMCBPTR)CHAIN(FMCFASYA)                  */
*                                                                  0529
*               DO;                 /* BEGIN TPFEL CODE GENERATION   */
*                 DCL                                              0530
*                   FMZCA016 BIT(32) CONSTANT('80000000'X);        0530
*                 DCL                                              0531
*                   FMZCB016 BIT(32) CONSTANT('7FFFFFFF'X);        0531
*                 RFY                                              0532
*                  (GPR00P,                                        0532
*                   GPR01P,                                        0532
*                   GPR14P,                                        0532
*                   GPR15P) RSTD;                                  0532
*                 GPR15P=ADDR(ACDASFQ);/* ADDRESS QUEUE POINTER      */
         LA    GPR15P,ACDASFQ(,@05)                                0533
*                 GPR14P=GPR15P->PABWEQA×FMZCA016;                 0534
         L     @05,@CF01334                                        0534
         L     GPR14P,PABWEQA(,GPR15P)                             0534
         OR    GPR14P,@05                                          0534
*                 IF GPR14P=FMZCA016 THEN                          0535
         CLR   GPR14P,@05                                          0535
         BE    @RT00535                                            0535
*                   ;                                              0536
*                 ELSE                                             0537
*                   DO;                                            0537
*                     GPR00P=FMZCA016;/* INITIALIZE QUIT FLAG        */
         L     GPR00P,@CF01334                                     0538
*                     DO WHILE(GPR00P^=GPR14P->FMCFASYA);          0539
         B     @DE00539                                            0539
@DL00539 DS    0H                                                  0540
*                       GPR14P=GPR14P->FMCFASYA;/* SEARCH FOR END OF
*                                      CHAIN                         */
         L     GPR14P,FMCFASYA(,GPR14P)                            0540
*                     END;                                         0541
@DE00539 C     GPR00P,FMCFASYA(,GPR14P)                            0541
         BNE   @DL00539                                            0541
*                   END;                                           0542
*                 GPR14P=GPR14P&FMZCB016;                          0543
@RT00535 N     GPR14P,@CF01332                                     0543
*                 FMCBPTR=GPR14P;                                  0544
         LR    FMCBPTR,GPR14P                                      0544
*                 RFY                                              0545
*                  (GPR00P,                                        0545
*                   GPR01P,                                        0545
*                   GPR14P,                                        0545
*                   GPR15P) UNRSTD;                                0545
*               END;                /* END TPFEL GENERATION          */
*               CALL LOCKFMCB;      /* GET AND LOCK FMCB             */
         BAL   @14,LOCKFMCB                                        0547
*               CRRDFSYN=ZERB;      /* CLEAR SYN RTYPE               */
*               CRRRESP=ZERB;       /* CLEAR RESP RTYPE              */
         NI    CRRDFSYN(CRRPTR),B'11111001'                        0549
