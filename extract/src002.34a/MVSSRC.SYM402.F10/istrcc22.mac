         TITLE 'ISTRCC22:SEND OUTBOUND MACRO PROCESSOR                 *
                        '
ISTRCC22 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ISTRCC22  78.173'                                 0001
         DROP  @15
@PROLOG  BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*   CRRPTR=ADDR(CRRSPACE);          /* GET ADDR CRR                  */
         L     @11,RPHCRR(,RPHPTR)                                 0373
         L     CRRPTR,CRACRR(,@11)                                 0373
         LA    CRRPTR,CRRSPACE(,CRRPTR)                            0373
*   LCPBPTR=CRRLCPB;                /* GET ADDR DCLCP                */
         L     LCPBPTR,CRRLCPB(,CRRPTR)                            0374
*   RPLPTR=CRRARPL;                 /* GET ADDR RPL                  */
         L     RPLPTR,CRRARPL(,CRRPTR)                             0375
*   FMCBPTR=CRRFMCB;                /* GET ADDR FMCB                 */
         L     FMCBPTR,CRRFMCB(,CRRPTR)                            0376
*   LCCWPTR=LCPNXLCW;               /* GET ADDR 1ST LCCW             */
         L     LCCWPTR,LCPNXLCW(,LCPBPTR)                          0377
*   CRRMAS=''B;                     /* ZERO OUT MODULE AUDIT SWS     */
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0378
*   CRRRM22=ON;                     /* SET MODULE AUDIT FOR RCC22    */
         OI    CRRRM22(CRRPTR),B'01000000'                         0379
*   CRRRC22=ON;                     /* SET AUDIT TRAIL SWITCH        */
         OI    CRRRC22(CRRPTR),B'01000000'                         0380
*   IF RPLDATA=ON THEN                                             0381
         TM    RPLDATA+16(RPLPTR),B'10000000'                      0381
         BNO   @RF00381                                            0381
*     LCPASYN=OFF;                  /* CHECK IF CONTROL = DATA ?     */
         NI    LCPASYN(LCPBPTR),B'10111111'                        0382
*   ELSE                            /* NOT DATA SO TRANSLATE CTL.    */
*/*  *****************************************************************/
*/*   THIS SEGMENT IS USED TO TRANSLATE  THE THREE BYTE CONTROL      */
*/*   PARAMETER FIELD IN THE RPL INTO A ONE BYTE CONTROL 'RU'-REQUEST*/
*/*   UNIT FORM FOR RECORD TRANSMISSIONS.                            */
*/*   IT DOES THIS BY SEARCHING A TABLE WITHIN ISTRCC22 WHICH HAS THE*/
*/*   CONTROL BYTE CODE MATCHED WITH THE RPL BIT SETTING IN THE RES- */
*/*   PECTIVE BYTE IN THE RPL CONTROL FIELD.  THIS IS DONE BY INDEX- */
*/*   THROUGH AN ARRAY WHICH IS COMPOSED OF THE MASK FOR THE BYTE IN */
*/*   THE RPL, THE CONTROL CODE, THE WITH/AGAINST FLOW MASK, AND THE */
*/*   SYNCH/ASYNCH MASK FOR EACH RESPECTIVE CONTROL CODE IN EACH     */
*/*   ELEMENT OF THE ARRAY.                                          */
*/*   THE ARRAY HAS AT PRESENT A DIMENSION OF 10 ELEMENTS AND   8    */
*/*   FIELDS (BYTE).                                                 */
*/*  *****************************************************************/
*                                                                  0383
*     DO;                                                          0383
         B     @RC00381                                            0383
@RF00381 DS    0H                                                  0384
*       CRRSW1=ON;                  /* FLAG FOR LOOP CNTRL           */
         OI    CRRSW1(CRRPTR),B'01000000'                          0384
*       DO INDXREG=ADDR(SRCHTAB) TO(ADDR(SRCHTAB)+(LENGTH(SRCHTAB)*(DIM
*             (SRCHTAB)-1))) BY LENGTH(SRCHTAB) WHILE CRRSW1=ON;   0385
*                                                                  0385
         LA    INDXREG,SRCHTAB                                     0385
         B     @DE00385                                            0385
@DL00385 TM    CRRSW1(CRRPTR),B'01000000'                          0385
         BNO   @DC00385                                            0385
*         /***********************************************************/
*         /*                                                         */
*         /* SEARCH THROUGH THE TABLE MASKING THE RPL CONTROL FIELD  */
*         /* WITH THE TABLE ELEMENT FIELD(RPLMSK) UNTIL THE CORRECT  */
*         /* CONTROL TYPE IS FOUND OR IF NONE IS FOUND-EXIT.         */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0386
*         IF RPLMSK=(RPLPTR+INDXVAL(CBYTE))->LPRCTFLD THEN         0386
         SLR   @11,@11                                             0386
         IC    @11,CBYTE(,INDXREG)                                 0386
         SLA   @11,2                                               0386
         LR    @02,RPLPTR                                          0386
         AL    @02,INDXVAL-4(@11)                                  0386
         CLC   RPLMSK(1,INDXREG),LPRCTFLD(@02)                     0386
         BNE   @RF00386                                            0386
*           DO;                     /* COMPARE RPLFLD WITH TABLE     */
*             IF RPLSRESP=ON THEN   /* IF THIS IS A RESPONSE         */
         TM    RPLSRESP+16(RPLPTR),B'10000000'                     0388
         BNO   @RF00388                                            0388
*               DO;                 /* SET SYN/ASYN FLAG AS SPEC     */
*                 IF SYNCHER1^=0 THEN                              0390
         CLI   SYNCHER1(INDXREG),0                                 0390
         BE    @RF00390                                            0390
*                   DO;             /* IS IN THE SEARCH TABLE        */
*                     CRRSW0=ON;                                   0392
         OI    CRRSW0(CRRPTR),B'10000000'                          0392
*                     LCPASYN=ON;   /* SET LCPB HEADER ASY           */
         OI    LCPASYN(LCPBPTR),B'01000000'                        0393
*                   END;                                           0394
*                 ELSE                                             0395
*                   DO;                                            0395
         B     @RC00390                                            0395
@RF00390 DS    0H                                                  0396
*                     CRRSW0=OFF;                                  0396
         NI    CRRSW0(CRRPTR),B'01111111'                          0396
*                     LCPASYN=OFF;  /* SET LCPB HEADER SYN           */
         NI    LCPASYN(LCPBPTR),B'10111111'                        0397
*                   END;                                           0398
*                 RH1=FLOWMSK1;     /* SET FLOW ACCORDINGLY          */
@RC00390 MVC   RH1+4(1,LCCWPTR),FLOWMSK1(INDXREG)                  0399
*               END;                                               0400
*             ELSE                  /* IF THIS IS A REQUEST          */
*               DO;                                                0401
         B     @RC00388                                            0401
@RF00388 DS    0H                                                  0402
*                 IF SYNCHER^=0 THEN                               0402
         CLI   SYNCHER(INDXREG),0                                  0402
         BE    @RF00402                                            0402
*                   DO;             /* SET SYN/ASYN FLAG IN CRR      */
*                     CRRSW0=ON;    /* ACCORDING TO TABLE            */
         OI    CRRSW0(CRRPTR),B'10000000'                          0404
*                     LCPASYN=ON;                                  0405
         OI    LCPASYN(LCPBPTR),B'01000000'                        0405
*                   END;                                           0406
*                 ELSE                                             0407
*                   DO;                                            0407
         B     @RC00402                                            0407
@RF00402 DS    0H                                                  0408
*                     CRRSW0=OFF;                                  0408
         NI    CRRSW0(CRRPTR),B'01111111'                          0408
*                     LCPASYN=OFF;                                 0409
         NI    LCPASYN(LCPBPTR),B'10111111'                        0409
*                   END;                                           0410
*                 RH1=FLOWMSK;      /* AND SET FLOW IND.             */
@RC00402 MVC   RH1+4(1,LCCWPTR),FLOWMSK(INDXREG)                   0411
*               END;                                               0412
*             RC22RU=CTLCDE;        /* MOVE CONTROL BYTE TO LCCW     */
@RC00388 MVC   RC22RU(1,LCCWPTR),CTLCDE(INDXREG)                   0413
*             CRRSW1=OFF;           /* INDICATE CONTROL FOUND        */
         NI    CRRSW1(CRRPTR),B'10111111'                          0414
*           END;                                                   0415
*       END;                                                       0416
@RF00386 AL    INDXREG,@CF00037                                    0416
@DE00385 LA    @11,SRCHTAB                                         0416
         AL    @11,@CF03624                                        0416
         CR    INDXREG,@11                                         0416
         BNH   @DL00385                                            0416
@DC00385 DS    0H                                                  0417
*       INDXREG=INDXREG-LENGTH(SRCHTAB);/* DECREMENT INDXREG         */
         SL    INDXREG,@CF00037                                    0417
*       IF CRRSW1=ON THEN           /* IF NO CONTROL WAS FOUND       */
         TM    CRRSW1(CRRPTR),B'01000000'                          0418
         BNO   @RF00418                                            0418
*         DO;                       /* SET ERROR CODES IN THE        */
*           CRRRTNCD=CRRLOGIC;      /* MAJOR AND MINOR ERROR         */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0420
*           CRRFDB2=CRRSDNVC;       /* FIELDS IN THE CRR.            */
         MVI   CRRFDB2(CRRPTR),X'38'                               0421
*         END;                                                     0422
*     END;                                                         0423
@RF00418 DS    0H                                                  0424
*   IF CRRRTNCD=CRRNOERR THEN       /* DID XLATECTL XLATE CONTROL?   */
@RC00381 CLI   CRRRTNCD(CRRPTR),X'00'                              0424
         BNE   @RF00424                                            0424
*     DO;                           /* NO ERROR IN XLATE OR DATA     */
*       RFY                                                        0426
*         ISTNCB BASED(FMCDNCBA);   /*                       @D32CKCF*/
*       RFY                                                        0427
*         ISTDEVCH BASED(ADDR(NCBDEVCH));/*                  @D32CKCF*/
*       IF DEVTCODE>=DEV3277&DEVTCODE<=DEV3275 THEN                0428
         LA    @11,36                                              0428
         AL    @11,FMCDNCBA(,FMCBPTR)                              0428
         CLI   DEVTCODE(@11),X'19'                                 0428
         BL    @RF00428                                            0428
         CLI   DEVTCODE(@11),X'1C'                                 0428
         BH    @RF00428                                            0428
*         DO;                                                      0429
*                                                                  0429
*/****************************************************************** */
*/*                         CHK 3270                                 */
*/*    CHECK FOR VALID REQUEST OPTIONS FOR 3270 DEVICE               */
*/*                                                                  */
*/****************************************************************** */
*                                                                  0430
*           IF(RPLONLY=ON)&         /*                       @D33SKXW*/
*               (RPLNFME=OFF×RPLEX=ON)&(RPLDATA=ON)&(RPLSRESP^=ON) THEN
         LA    @11,16                                              0430
         ALR   @11,RPLPTR                                          0430
         TM    RPLONLY(@11),B'00010000'                            0430
         BNO   @RF00430                                            0430
         TM    RPLEX(@11),B'00000100'                              0430
         BNZ   @GL00003                                            0430
         TM    RPLNFME(@11),B'00000010'                            0430
         BO    @RF00430                                            0430
@GL00003 LA    @11,16                                              0430
         ALR   @11,RPLPTR                                          0430
         TM    RPLDATA(@11),B'10000000'                            0430
         BNO   @RF00430                                            0430
         TM    RPLSRESP(@11),B'10000000'                           0430
         BNO   @RT00430                                            0430
*             ;                                                    0431
*           ELSE                                                   0432
*             DO;                                                  0432
@RF00430 DS    0H                                                  0433
*               CRRRTNCD=CRRLOGIC;                                 0433
         MVI   CRRRTNCD(CRRPTR),X'08'                              0433
*               CRRFDB2=CRRSINVR;                                  0434
         MVI   CRRFDB2(CRRPTR),X'3F'                               0434
*             END;                                                 0435
*         END;                                                     0436
*                                                                  0436
*/****************************************************************** */
*/*         END CHK 3270                                             */
*/****************************************************************** */
*                                                                  0437
*       ELSE                                                       0437
*         ;                                                        0437
@RF00428 DS    0H                                                  0438
*       IF CRRRTNCD=CRRNOERR THEN                                  0438
@RC00428 CLI   CRRRTNCD(CRRPTR),X'00'                              0438
         BNE   @RF00438                                            0438
*         DO;                                                      0439
*           IF RPLSRESP=OFF THEN    /* IS THIS A REQUEST× RESPONSE   */
         LA    @11,16                                              0440
         ALR   @11,RPLPTR                                          0440
         TM    RPLSRESP(@11),B'10000000'                           0440
         BNZ   @RF00440                                            0440
*             DO;                   /* STYPE = REQUEST               */
*               IF RPLDATA=ON THEN  /* CHECK IF DATA× CONTROL        */
         TM    RPLDATA(@11),B'10000000'                            0442
         BNO   @RF00442                                            0442
*/*  *****************************************************************/
*/*   THE SENDDATA SEGMENT IS USED FOR PREPARING  DATA REQUESTS FOR  */
*/*   TRANSMISSION.  IT CHECKS THE VALIDITY OF THE REQUESTS, I.E.    */
*/*   IF THE RESPONSE IS VALID OR MATCHES THE OUTSTANDING RESPONSE   */
*/*   IT ALSO SETS THE UNIQUE LCCW/RH AND LCPB/TH FIELDS FOR DATA    */
*/*   TRANSMISSIONS.                                                 */
*/*  *****************************************************************/
*                                                                  0443
*                 DO;                                              0443
*                   IF RPLEX=ON×(RPLNFME=ON&RPLRRN=OFF) THEN       0444
         TM    RPLEX(@11),B'00000100'                              0444
         BO    @RT00444                                            0444
         TM    RPLNFME(@11),B'00000010'                            0444
         BNO   @RF00444                                            0444
         TM    RPLRRN(@11),B'00000001'                             0444
         BZ    @RT00444                                            0444
*                     GO TO PREPOST;/*                       @D31KKNL*/
*                   ELSE                                           0446
*                     ;                                            0446
@RF00444 DS    0H                                                  0447
*                   IF RPLSCHED=ON THEN/* POST = SCHED REQUESTED     */
         TM    RPLSCHED+16(RPLPTR),B'10000000'                     0447
         BNO   @RF00447                                            0447
*PREPOST:                                                          0448
*                     IF FMCFLGB5=ON THEN/* AND PREV SCHED NOT POSTD */
PREPOST  TM    FMCFLGB5(FMCBPTR),B'00000100'                       0448
         BNO   @RF00448                                            0448
*                       DO;         /* SET ERROR IN CRR.             */
*                         CRRRTNCD=CRRLOGIC;/* SET LOGIC ERROR AND   */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0450
*                         CRRFDB2=CRRSDNP;/* PREV SCHED NOT POSTED   */
         MVI   CRRFDB2(CRRPTR),X'34'                               0451
*                       END;                                       0452
*                     ELSE          /* PREV SCHED POSTED             */
*                       GOTO RTNCD;                                0453
*                   ELSE            /* POST = RESP.                  */
*                     IF FMCRSPLM^=0&/* RESP LIMIT=0 =>NOLIMIT       */
*                         FMCPSTRS>=FMCRSPLM THEN                  0454
         B     @RC00447                                            0454
@RF00447 LH    @11,FMCRSPLM(,FMCBPTR)                              0454
         N     @11,@CF03645                                        0454
         LTR   @11,@11                                             0454
         BZ    @RF00454                                            0454
         CLC   FMCPSTRS(2,FMCBPTR),FMCRSPLM(FMCBPTR)               0454
         BL    @RF00454                                            0454
*                       DO;         /* RESP. LIMIT EXCEEDED ?        */
*                         CRRRTNCD=CRRRLEX;/* THEN SET ERROR FOR THIS*/
         MVI   CRRRTNCD(CRRPTR),X'2C'                              0456
*                         CRRFDB2=ZBYTE;/* CONDITION.                */
         MVI   CRRFDB2(CRRPTR),X'00'                               0457
*                       END;                                       0458
*RTNCD:                                                            0459
*                   IF CRRRTNCD=CRRNOERR THEN/* CHECK IF ANY ERRORS  */
@RF00454 DS    0H                                                  0459
@RC00447 DS    0H                                                  0459
RTNCD    CLI   CRRRTNCD(CRRPTR),X'00'                              0459
         BNE   @RF00459                                            0459
*                     DO;           /* COPY CHAIN VALUE FROM RPL     */
*                       CALL LCCWCOMD;/* SET UP LCCWS FOR DATA       */
         BAL   @14,LCCWCOMD                                        0461
*                       LCCWPTR=LCPNXLCW;/* REQUEST OR RESPONSES.    */
         L     LCCWPTR,LCPNXLCW(,LCPBPTR)                          0462
*                       RHQS=OFF;   /* SET RH FOR REQUEST.           */
*                       RHDIRI=OFF; /* SET FLOW TO 'WITH'. PUT DATA
*                                      LENGTH AND                    */
         NI    RHQS+4(LCCWPTR),B'01101111'                         0464
*                       LCCWPTR=ADDR(DCLLCCW2);/* ADDR. IN 2ND LCCW. */
         LA    LCCWPTR,DCLLCCW2(,LCPBPTR)                          0465
*                       LCCWCNT=RPLRLEN;                           0466
         L     @11,RPLRLEN(,RPLPTR)                                0466
         STH   @11,LCCWCNT(,LCCWPTR)                               0466
*                       LCCWDATA=RPLAREA;                          0467
         L     @11,RPLAREA(,RPLPTR)                                0467
         ST    @11,LCCWDATA(,LCCWPTR)                              0467
*                       CRRSW2=OFF;                                0468
         NI    CRRSW2(CRRPTR),B'11011111'                          0468
*                       IF RPLSCHED=ON×RPLEX=ON×(RPLNFME=ON&RPLRRN=OFF)
*                         THEN                                     0469
         LA    @11,16                                              0469
         ALR   @11,RPLPTR                                          0469
         TM    RPLSCHED(@11),B'10000100'                           0469
         BNZ   @RT00469                                            0469
         TM    RPLNFME(@11),B'00000010'                            0469
         BNO   @RF00469                                            0469
         TM    RPLRRN(@11),B'00000001'                             0469
         BNZ   @RF00469                                            0469
@RT00469 DS    0H                                                  0470
*                         DO;                                      0470
*                           LCPDIRPT=ON;/* TURN ON DIRECT POST     0471
*                                                            @D31KKCF*/
         OI    LCPDIRPT(LCPBPTR),B'00100000'                       0471
*                           CRRDIRPT=ON;/* FOR CHECKING AFTER TPQ TO
*                                      TOP/ATOP              @ZA32152*/
         OI    CRRDIRPT(CRRPTR),B'01000000'                        0472
*                           LCCWLSI=ON;/* SET BUF SYNCH IN LCCW    0473
*                                                             @XW0847*/
         OI    LCCWLSI(LCCWPTR),B'00000100'                        0473
*                         END;                                     0474
*                       IF RPLEX=ON×(RPLNFME=ON&RPLRRN=OFF) THEN   0475
@RF00469 LA    @11,16                                              0475
         ALR   @11,RPLPTR                                          0475
         TM    RPLEX(@11),B'00000100'                              0475
         BO    @RT00475                                            0475
         TM    RPLNFME(@11),B'00000010'                            0475
         BNO   @RF00475                                            0475
         TM    RPLRRN(@11),B'00000001'                             0475
         BZ    @RT00475                                            0475
*                         GO TO PREPOST2;/*                  @D31KKNL*/
*                       ELSE                                       0477
*                         ;                                        0477
@RF00475 DS    0H                                                  0478
*                       IF RPLSCHED=ON THEN/* IF POST = SCHED.       */
         TM    RPLSCHED+16(RPLPTR),B'10000000'                     0478
         BNO   @RF00478                                            0478
*                         DO;                                      0479
*PREPOST2:                                                         0480
*                           FMCFLGB5=ON;/* SET PREV. SCHED NOTPOSTD  */
PREPOST2 OI    FMCFLGB5(FMCBPTR),B'00000100'                       0480
*                         END;                                     0481
*                       ELSE        /* POST = RESP.                  */
*                         FMCPSTRS=FMCPSTRS+1;/* INCRMT OUTSTD     0482
*                                      RESPONSE                      */
         B     @RC00478                                            0482
@RF00478 LA    @11,1                                               0482
         LH    @02,FMCPSTRS(,FMCBPTR)                              0482
         N     @02,@CF03645                                        0482
         ALR   @11,@02                                             0482
         STH   @11,FMCPSTRS(,FMCBPTR)                              0482
*                       CALL QTOTOPS;/* QUEUE LCPB TO 'TOP' PAB.     */
@RC00478 BAL   @14,QTOTOPS                                         0483
*                       IF RCDEREG^=NOERROR THEN                   0484
         LTR   RCDEREG,RCDEREG                                     0484
         BZ    @RF00484                                            0484
*                         DO;       /*                       @ZA25545*/
*                           IF RPLEX=ON×(RPLNFME=ON&RPLRRN=OFF) THEN
         LA    @11,16                                              0486
         ALR   @11,RPLPTR                                          0486
         TM    RPLEX(@11),B'00000100'                              0486
         BO    @RT00486                                            0486
         TM    RPLNFME(@11),B'00000010'                            0486
         BNO   @RF00486                                            0486
         TM    RPLRRN(@11),B'00000001'                             0486
         BZ    @RT00486                                            0486
