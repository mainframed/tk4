@RF00486 DS    0H                                                  0489
*                           IF RPLSCHED=ON THEN/* IF POST = SCHED  0489
*                                                            @ZA25545*/
         TM    RPLSCHED+16(RPLPTR),B'10000000'                     0489
         BNO   @RF00489                                            0489
*                             DO;                                  0490
*PREPOST3:                                                         0491
*                               FMCFLGB5=OFF;/* UNDO FLAG    @ZA25545*/
PREPOST3 NI    FMCFLGB5(FMCBPTR),B'11111011'                       0491
*                             END;                                 0492
*                           ELSE    /* POST = RESP.          @ZA25545*/
*                             FMCPSTRS=FMCPSTRS-1;/* DECRMT OUTSTD 0493
*                                      RESPONSE                      */
         B     @RC00489                                            0493
@RF00489 LH    @11,FMCPSTRS(,FMCBPTR)                              0493
         N     @11,@CF03645                                        0493
         BCTR  @11,0                                               0493
         STH   @11,FMCPSTRS(,FMCBPTR)                              0493
*                         END;      /* TPQUE FAILED          @ZA25545*/
*                     END;                                         0495
*                 END;              /*                       @ZA25545*/
*               ELSE                /* CONTROL=> SENCTRL SEG.        */
*/*  *****************************************************************/
*/*   THE SENDCTRL SEGMENT IS USED FOR PREPARING BOTH TYPES OF DATA  */
*/*   FLOW ( DFSYN,DFASY) REQUESTS.  IT CHECKS FOR THE DATA FLOW     */
*/*   TYPE AND VALIDATES THE REQUEST AS IN SENDDATA.                 */
*/*  *****************************************************************/
*                                                                  0497
*                 DO;               /*                       @DM13792*/
         B     @RC00442                                            0497
@RF00442 DS    0H                                                  0498
*/* THIS ROUTINE IS RESTRUCTURED FOR PTM 3792 & DCR 4069             */
*                                                                  0498
*                   IF FMCRSPLM^=0& /* NOT NO RESP LIMIT             */
*                       FMCPSTRS>=FMCRSPLM THEN/* IF OUTSTANDING     */
         LH    @11,FMCRSPLM(,FMCBPTR)                              0498
         N     @11,@CF03645                                        0498
         LTR   @11,@11                                             0498
         BZ    @RF00498                                            0498
         CLC   FMCPSTRS(2,FMCBPTR),FMCRSPLM(FMCBPTR)               0498
         BL    @RF00498                                            0498
*                     DO;           /* >= RESP LIM.                  */
*                       CRRRTNCD=CRRRLEX;/* SET MAJOR RTN CD         */
         MVI   CRRRTNCD(CRRPTR),X'2C'                              0500
*                       CRRFDB2=ZBYTE;                             0501
         MVI   CRRFDB2(CRRPTR),X'00'                               0501
*                     END;                                         0502
*                   ELSE                                           0503
*                     DO;                                          0503
         B     @RC00498                                            0503
@RF00498 DS    0H                                                  0504
