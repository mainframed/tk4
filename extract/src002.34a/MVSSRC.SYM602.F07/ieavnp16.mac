         TITLE 'IEAVNP16--BUILD EXCP APPENDAGE NAME TABLE              *
                        '
IEAVNP16 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVNP16  75.305'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
** WILL ASSEMBLE PROPERLY.
         EJECT
*/* P INITIALIZE NIP SERVICE ROUTINE FOR READING PARMLIB             */
*        NIPPANAM = 'IEAAPP00';        /* PROVIDE NAME OF MEMBER     */
         L     @09,NVTPAREA(,RNVT)                                 0137
         MVC   NIPPANAM(8,@09),@CC01850                            0137
*/* S GETMAIN: GET CORE FOR NAME TABLE                               */
*        WKREG0 = INITLNTH;            /* BEGIN WITH 128 BYTE TABLE  */
         LA    WKREG0,128                                          0138
*        GEN (GETMAIN RU,LV=(0),SP=245); /* EXECUTE GETMAIN MACRO    */
         GETMAIN RU,LV=(0),SP=245
*/* P STORE POINTER TO TABLE IN CVT                                  */
*        AIDPTR = RPARM;               /* SET PTR TO APPENDAGE ID TBL*/
         LR    AIDPTR,RPARM                                        0140
*        CVTAIDVT = AIDPTR;            /* STORE IN CVT               */
         ST    AIDPTR,CVTAIDVT(,RCVT)                              0141
*/* P ZERO TABLE, FILL IN HEADER FIELDS                              */
*        APPNMTBL (1:128) = '00'X;     /* ZERO OUT THE TABLE         */
         XC    APPNMTBL+1(127,AIDPTR),APPNMTBL+1(AIDPTR)           0142
         MVI   APPNMTBL(AIDPTR),X'00'                              0142
*        APPSIZE = INITLNTH;           /* INITIAL TABLE SIZE IS 128  */
         LA    @09,128                                             0143
         STH   @09,APPSIZE(,AIDPTR)                                0143
*        TGTPTR = ADDR(APPNAMID);      /* INITIALIZE TARGET POINTER  */
         LA    TGTPTR,APPNAMID(,AIDPTR)                            0144
*        APPBAL = APPSIZE - (TGTPTR - AIDPTR); /* BAL OF UNUSED CORE */
         LR    @15,TGTPTR                                          0145
         SLR   @15,AIDPTR                                          0145
         SLR   @09,@15                                             0145
         STH   @09,APPBAL(,AIDPTR)                                 0145
*/*GETFIRST: I GET FIRST RECORD FROM IEAAPP00                        */
*GETFIRST: CALL NIPPMMEM;              /* READ PARMLIB               */
GETFIRST BAL   @14,NIPPMMEM                                        0146
*/* D (YES,EXCEPTN:RRT,NO,) EXCEPTION RETURN CODE?                   */
*        IF WKREG1 < 0                 /* TEST FOR AN ERROR RETURN   */
*          THEN                        /* ERROR IF REG 1 IS NEGATIVE */
         LTR   WKREG1,WKREG1                                       0147
         BM    @RT00147                                            0147
*            GO TO EXCEPTN;            /* GO ANALYZE THE EXCEPTION   */
*/* D (NO,GETFIRST,YES,) VALID ENTRY KEYWORD?                        */
*        SRCPTR = RPARM;               /* SET POINTER TO SOURCE TEXT */
         LR    SRCPTR,RPARM                                        0149
*        DO ENTSUB = 1 TO 5;           /* ESTABLISH ENTRY SUBSCRIPT  */
         LA    ENTSUB,1                                            0150
@DL00150 DS    0H                                                  0151
*          IF IPTYPE = APPTYPE(ENTSUB) /* MATCH INPUT TO CONSTANTS   */
*            THEN                      /* CHK IF VALID ENTRY KEYWORD */
         LR    @09,ENTSUB                                          0151
         MH    @09,@CH00080                                        0151
         LA    @01,APPTYPE-7(@09)                                  0151
         CLC   IPTYPE(7,SRCPTR),0(@01)                             0151
         BNE   @RF00151                                            0151
*              DO;                     /* CONTINUE IF VALID          */
*/* P SET SOURCE POINTER TO FIRST INPUT NAME                         */
*SETSRC:         SRCPTR = SRCPTR + LENGTH(APPTYPE); /* STEP PAST     */
SETSRC   AL    SRCPTR,@CF00080                                     0153
*                                      /* THE APPENDAGE TYPE KEYWORD */
*                DO WKREG1 = 1 TO 64;  /* SCAN THE NEXT 64 COLUMNS   */
         LA    WKREG1,1                                            0154
@DL00154 DS    0H                                                  0155
*                  IF IPNAMID (1:1) ^= ' ' /* FIND 1ST NON-BLANK     */
*                    THEN              /* THEN TEST IF COLUMN 73     */
         CLI   IPNAMID(SRCPTR),C' '                                0155
         BE    @RF00155                                            0155
*                      IF IPNAMID (1:1) ^= 'FF'X /* IF NOT COL 73    */
*                        THEN          /* THEN GO INITIALIZE TYPE    */
         CLI   IPNAMID(SRCPTR),X'FF'                               0156
         BNE   @RT00156                                            0156
*                          GO TO INIT; /* GO INITIALIZE THIS TYPE    */
*                      ELSE            /* OTHERWISE CARD IS BLANK    */
*                          GO TO WRAPUP; /* RECORD ALL BLANK - QUIT  */
         B     WRAPUP                                              0158
*                  ELSE                /* OTHERWISE COMPRESS BLANK   */
*                      SRCPTR = SRCPTR + 1; /* COMPRESS THIS BLANK   */
@RF00155 AL    SRCPTR,@CF00043                                     0159
*                END;                                              0160
         AL    WKREG1,@CF00043                                     0160
         C     WKREG1,@CF01857                                     0160
         BNH   @DL00154                                            0160
*              END;                                                0161
*        END;                                                      0162
@RF00151 AL    ENTSUB,@CF00043                                     0162
         C     ENTSUB,@CF00071                                     0162
         BNH   @DL00150                                            0162
*        GO TO GETFIRST;               /* MUST START WITH KEYWORD    */
         B     GETFIRST                                            0163
*/*INIT: P SET OFFSET TO FIRST NAME THIS TYPE                        */
*INIT:   APPOFFST(ENTSUB) = TGTPTR - ADDR(APPNAMID);               0164
INIT     LR    @09,ENTSUB                                          0164
         SLA   @09,2                                               0164
         LA    @15,APPNAMID(,AIDPTR)                               0164
         LCR   @15,@15                                             0164
         ALR   @15,TGTPTR                                          0164
         STH   @15,APPOFFST-4(@09,AIDPTR)                          0164
*/* D (YES,CPYPRIME,NO,) ROOM FOR NAMES FROM PRIME TABLE?            */
*        TBLPTR = APPLIST(ENTSUB);     /* FIND ENTRY IN PRIME TABLE  */
         L     TBLPTR,APPLIST-4(@09)                               0165
*        IF (PRCOUNT * IDLNGTH) <= APPBAL /* ROOM FOR THIS MOVE?     */
*          THEN                        /* TEST IF ROOM FOR MOVE      */
         LH    @09,PRCOUNT(,TBLPTR)                                0166
         ALR   @09,@09                                             0166
         CH    @09,APPBAL(,AIDPTR)                                 0166
         BNH   @RT00166                                            0166
*            GO TO CPYPRIME;           /* YES, COPY PRIME TABLE      */
*/* S EXTEND: EXTEND THE NAME TABLE                                  */
*        CALL EXTEND;                  /* CALL SUBROUTINE TO EXTEND  */
         BAL   @14,EXTEND                                          0168
*/*CPYPRIME: P MOVE COUNT & NAMES FROM PRIME TABLE TO NAME TABLE     */
*CPYPRIME: APPCOUNT(ENTSUB) = PRCOUNT; /* COPY COUNT FIELD           */
CPYPRIME LR    @09,ENTSUB                                          0169
         SLA   @09,2                                               0169
         LH    @15,PRCOUNT(,TBLPTR)                                0169
         STH   @15,APPCOUNT-4(@09,AIDPTR)                          0169
*        IF PRCOUNT = 0                /* ANY NAMES IN PRIME TABLE?  */
*          THEN                        /* TEST IF NAMES IN PRIME TBL */
         LTR   @15,@15                                             0170
         BZ    @RT00170                                            0170
*            GO TO MOVEIDS;            /* NO, GO MOVE FROM PARMLIB   */
*        MOVLEN = PRCOUNT * IDLNGTH;   /* SET LENGTH OF MOVE         */
         ALR   @15,@15                                             0172
         LR    MOVLEN,@15                                          0172
*        OPNAMID (1:MOVLEN) = PRNAMES (1:MOVLEN); /* MOVE NAMES      */
         LR    @09,MOVLEN                                          0173
         BCTR  @09,0                                               0173
         EX    @09,@SM01951                                        0173
*        TGTPTR = TGTPTR + MOVLEN;     /* STEP TARGET POINTER        */
         ALR   TGTPTR,MOVLEN                                       0174
*        APPBAL = APPBAL - MOVLEN;     /* DECREMENT BALANCE OF CORE  */
         LH    @09,APPBAL(,AIDPTR)                                 0175
         SLR   @09,MOVLEN                                          0175
         STH   @09,APPBAL(,AIDPTR)                                 0175
*/*MOVEIDS: P DECREMENT BALANCE OF UNUSED CORE FOR NEXT NAME         */
*MOVEIDS:  APPBAL = APPBAL - IDLNGTH;  /* SUBTRACT SIZE OF 1 NAME    */
MOVEIDS  LH    @09,APPBAL(,AIDPTR)                                 0176
         BCTR  @09,0                                               0176
         BCTR  @09,0                                               0176
         STH   @09,APPBAL(,AIDPTR)                                 0176
*/* D (YES,MOVEID1,NO,) ROOM FOR ONE MORE NAME?                      */
*/* S EXTEND: EXTEND THE NAME TABLE                                  */
*        IF APPBAL < 0                 /* IF END OF TABLE            */
*          THEN                        /* TEST IF AT END OF TABLE    */
         LTR   @09,@09                                             0177
         BNM   @RF00177                                            0177
*            CALL EXTEND;              /* YES, GO TO EXTEND ROUTINE  */
         BAL   @14,EXTEND                                          0178
*/*MOVEID1: P MOVE ONE NAME ID TO TABLE IF VALID                     */
*MOVEID1:                              /* IS NAME ID VALID     YM2893*/
*        IF IPNAMID (1:1) ^= CHARW &   /* IS 1ST CHAR VALID    YM5776*/
*           IPNAMID (1:1) ^= CHARX &   /* IS 1ST CHAR VALID    YM5776*/
*           IPNAMID (1:1) ^= CHARY &   /* IS 1ST CHAR VALID    YM5776*/
*           IPNAMID (1:1) ^= CHARZ     /* IS 1ST CHAR VALID    YM2893*/
*          THEN                        /* IF NOT W, X, Y OR Z  YM2893*/
@RF00177 DS    0H                                                  0179
MOVEID1  CLI   IPNAMID(SRCPTR),C'W'                                0179
         BE    @RF00179                                            0179
         CLI   IPNAMID(SRCPTR),C'X'                                0179
         BE    @RF00179                                            0179
         CLI   IPNAMID(SRCPTR),C'Y'                                0179
         BE    @RF00179                                            0179
         CLI   IPNAMID(SRCPTR),C'Z'                                0179
         BNE   @RT00179                                            0179
*            GO TO WRAPUP;             /* WRAP UP TABLE BUILD  YM2893*/
*        DO  WKREG1 = 36 TO 1 BY -1;   /* IS 2ND CHAR VALID    YM2893*/
@RF00179 LA    WKREG1,36                                           0181
@DL00181 DS    0H                                                  0182
*          IF IPNAMID (2:2) = VALIDTBL(WKREG1) /* IS IT VALID  YM2893*/
*            THEN                      /* VALID IF IN TABLE    YM2893*/
         LA    @09,VALIDTBL-1(WKREG1)                              0182
         CLC   IPNAMID+1(1,SRCPTR),0(@09)                          0182
         BE    @RT00182                                            0182
*              GO TO MOVEID2;          /* IF VALID, GO MOVE ID YM2893*/
*        END;                          /* IF 2ND IS NOT VALID, YM2893*/
         BCTR  WKREG1,0                                            0184
         LTR   WKREG1,WKREG1                                       0184
         BP    @DL00181                                            0184
*        GO TO WRAPUP;                 /* WRAP UP TABLE BUILD  YM2893*/
         B     WRAPUP                                              0185
*MOVEID2:                              /* MOVE ID TO APP TABLE YM2893*/
*        OPNAMID = IPNAMID;            /* MOVE ONE ID TO TABLE YM2893*/
MOVEID2  MVC   OPNAMID(2,TGTPTR),IPNAMID(SRCPTR)                   0186
*/* P INCREMENT COUNT OF NAMES                                       */
*        APPCOUNT(ENTSUB) = APPCOUNT(ENTSUB) + 1; /* BUMP BY ONE     */
         LR    @09,ENTSUB                                          0187
         SLA   @09,2                                               0187
         LA    @15,1                                               0187
         LH    @14,APPCOUNT-4(@09,AIDPTR)                          0187
         ALR   @14,@15                                             0187
         STH   @14,APPCOUNT-4(@09,AIDPTR)                          0187
*/* P STEP SOURCE & TARGET POINTERS                                  */
*        SRCPTR = SRCPTR + LENGTH(IPNAMID); /* STEP PAST ID FIELD    */
         LA    @09,2                                               0188
         ALR   SRCPTR,@09                                          0188
*        TGTPTR = TGTPTR + LENGTH(OPNAMID); /* BUMP NAME TABLE PTR   */
         ALR   TGTPTR,@09                                          0189
*/* D (NO,MOVEIDS,YES,) END OF NAMES THIS CARD?                      */
*/* D (NO,WRAPUP,YES,) CONTINUATION EXPECTED?                        */
*        IF IPNAMID (1:1) ^= ','       /* IF NOT COMMA, EITHER BLANK */
*          THEN                        /*   OR INVALID SYNTAX        */
         CLI   IPNAMID(SRCPTR),C','                                0190
         BNE   @RT00190                                            0190
*            GO TO WRAPUP;             /* GO WRAP UP TABLE BUILD     */
*        IF IPNAMID (2:2) = ' '        /* TEST FOR BLANK AFTER COMMA */
*          THEN                        /* ,BLANK MEANS END OF RECORD */
         CLI   IPNAMID+1(SRCPTR),C' '                              0192
         BE    @RT00192                                            0192
*            GO TO GETNEXT;            /* GO GET NEXT RECORD         */
*        SRCPTR = SRCPTR + 1;          /* STEP PAST COMMA            */
         ALR   SRCPTR,@15                                          0194
*        GO TO MOVEIDS;                /* CONTINUE MOVING NAME IDS   */
         B     MOVEIDS                                             0195
*/*GETNEXT: I GET NEXT RECORD                                        */
*GETNEXT:  CALL NIPPMMEM;              /* READ PARMLIB               */
GETNEXT  BAL   @14,NIPPMMEM                                        0196
*/* D (YES,EXCEPTN:RRT,NO,) EXCEPTION RETURN CODE?                   */
*        IF WKREG1 < 0                 /* TEST FOR AN ERROR RETURN   */
*          THEN                        /* RPARM NEGATIVE = EXCEPTION */
         LTR   WKREG1,WKREG1                                       0197
         BM    @RT00197                                            0197
*            GO TO EXCEPTN;            /* GO PROCESS THE ERROR       */
*        SRCPTR = RPARM;               /* SET POINTER TO SOURCE      */
         LR    SRCPTR,RPARM                                        0199
*/* D (YES,INIT,NO,MOVEIDS) VALID ENTRY KEYWORD?                     */
*        DO WKREG1 = 1 TO 5;           /* PRESERVE ENTSUB IN CASE OF */
         LA    WKREG1,1                                            0200
@DL00200 DS    0H                                                  0201
*          IF IPTYPE = APPTYPE(WKREG1) /*  CONTINUATION OF THIS TYPE.*/
*            THEN                      /* TEST FOR SAME TYPE         */
         LR    @09,WKREG1                                          0201
         MH    @09,@CH00080                                        0201
         LA    @15,APPTYPE-7(@09)                                  0201
         CLC   IPTYPE(7,SRCPTR),0(@15)                             0201
         BNE   @RF00201                                            0201
*              DO;                                                 0202
*                ENTSUB = WKREG1;      /* RESET SUBSCRIPT REGISTER   */
         LR    ENTSUB,WKREG1                                       0203
*                GO TO SETSRC;         /* INITIALIZE FOR NEW APP TYPE*/
         B     SETSRC                                              0204
*              END;                                                0205
*        END;                                                      0206
@RF00201 AL    WKREG1,@CF00043                                     0206
         C     WKREG1,@CF00071                                     0206
         BNH   @DL00200                                            0206
*        GO TO MOVEIDS;                /* CONTINUE SAME APP TYPE     */
         B     MOVEIDS                                             0207
*/*EXCEPTN: D (YES,WRAPUP,NO,) END OF FILE OR NULL FILE?             */
*EXCEPTN:  IF NIPPARC = NOTFOUND × NIPPARC = EOFCODE /* TEST FOR     */
*            THEN                      /* MEMBER MISSING OR EOF      */
EXCEPTN  L     @09,NVTPAREA(,RNVT)                                 0208
         L     @09,NIPPARC(,@09)                                   0208
         C     @09,@CF00039                                        0208
         BE    @RT00208                                            0208
         C     @09,@CF00041                                        0208
         BE    @RT00208                                            0208
*              GO TO WRAPUP;           /* GO WRAP UP TABLE BUILD     */
*/* I WRITE TO OPERATOR - NO TABLE                                   */
*        GEN (IEAPMNIP TYPE=WTO,PARAM=HDR); /* ERROR READING PARMLIB */
         IEAPMNIP TYPE=WTO,PARAM=HDR
*/*WRAPUP: D (YES,EXIT,NO,) PRIME TABLE COPIED?                      */
*/* P COPY PRIME TABLE TO NAME TABLE                                 */
*WRAPUP: DO ENTSUB = 1 TO 5;                                       0211
WRAPUP   LA    ENTSUB,1                                            0211
@DL00211 DS    0H                                                  0212
*          IF APPCOUNT(ENTSUB) = 0     /* TEST IF COPY WAS DONE      */
*            THEN                      /* IF COPY HAS NOT BEEN DONE  */
         SLR   @09,@09                                             0212
         LR    @15,ENTSUB                                          0212
         SLA   @15,2                                               0212
         CH    @09,APPCOUNT-4(@15,AIDPTR)                          0212
         BNE   @RF00212                                            0212
*              DO;                                                 0213
*                TBLPTR = APPLIST(ENTSUB); /* POINT TO EACH LIST     */
         L     TBLPTR,APPLIST-4(@15)                               0214
*                IF PRCOUNT > 0        /* SEE IF ANYTHING TO COPY    */
*                  THEN                /* IF SOMETHING TO COPY       */
         LH    @15,PRCOUNT(,TBLPTR)                                0215
         CR    @15,@09                                             0215
         BNH   @RF00215                                            0215
*                    DO;                                           0216
*                      IF (PRCOUNT * IDLNGTH) > APPBAL /* CHECK IF   */
*                                      /* ENOUGH ROOM FOR THIS MOVE  */
*                        THEN                                      0217
         ALR   @15,@15                                             0217
         CH    @15,APPBAL(,AIDPTR)                                 0217
         BNH   @RF00217                                            0217
*                          CALL EXTEND; /* NO, EXTEND THE NAME TABLE */
         BAL   @14,EXTEND                                          0218
*                      APPCOUNT(ENTSUB) = PRCOUNT; /* COUNT OF NAMES */
@RF00217 LR    @09,ENTSUB                                          0219
         SLA   @09,2                                               0219
         LH    @15,PRCOUNT(,TBLPTR)                                0219
         STH   @15,APPCOUNT-4(@09,AIDPTR)                          0219
*                      APPOFFST(ENTSUB) = TGTPTR - ADDR(APPNAMID); 0220
         LA    @14,APPNAMID(,AIDPTR)                               0220
         LCR   @14,@14                                             0220
         ALR   @14,TGTPTR                                          0220
         STH   @14,APPOFFST-4(@09,AIDPTR)                          0220
*                                      /* SET OFFSET TO NAMES        */
*                      MOVLEN = PRCOUNT * IDLNGTH; /* NEXT CALCULATE */
         ALR   @15,@15                                             0221
         LR    MOVLEN,@15                                          0221
*                                      /* THE MOVE LENGTH            */
*                      OPNAMID (1:MOVLEN) = PRNAMES (1:MOVLEN);    0222
         LR    @09,MOVLEN                                          0222
         BCTR  @09,0                                               0222
         EX    @09,@SM01951                                        0222
*                                      /* MOVE NAMES TO TABLE        */
*                      TGTPTR = TGTPTR + MOVLEN; /* BUMP TARGET PTR  */
         ALR   TGTPTR,MOVLEN                                       0223
*                      APPBAL = APPBAL - MOVLEN; /* DECREMENT BALANCE*/
         LH    @09,APPBAL(,AIDPTR)                                 0224
         SLR   @09,MOVLEN                                          0224
         STH   @09,APPBAL(,AIDPTR)                                 0224
*                    END;                                          0225
*              END;                                                0226
@RF00215 DS    0H                                                  0227
*        END;                                                      0227
@RF00212 AL    ENTSUB,@CF00043                                     0227
         C     ENTSUB,@CF00071                                     0227
         BNH   @DL00211                                            0227
*/*EXIT: R RETURN TO NUCLEUS INITIALIZATION                          */
*        RETURN;                       /* RETURN TO NIP              */
@EL00001 L     @13,4(,@13)                                         0228
@EF00001 DS    0H                                                  0228
@ER00001 LM    @14,@12,12(@13)                                     0228
         BR    @14                                                 0228
*/*EXTEND: E EXTEND NAME TABLE                                       */
*/* P CALCULATE TWICE TABLE SIZE                                     */
*EXTEND: PROC OPTIONS (NOSAVE);                                    0229
EXTEND   DS    0H                                                  0230
*        RESPECIFY SUBRTN RESTRICTED;  /* RESTRICT RETURN REGISTER   */
*        SUBRTN = REXIT;               /* SAVE SUBROUTINE RETURN PTR */
         LR    SUBRTN,REXIT                                        0231
*        WKREG0 = 2 * APPSIZE;         /* DOUBLE THE TABLE SIZE      */
         LH    WKREG0,APPSIZE(,AIDPTR)                             0232
         ALR   WKREG0,WKREG0                                       0232
*/* P GET CORE FOR THIS AMOUNT                                       */
*        GEN (GETMAIN RU,LV=(0),SP=245); /* EXECUTE GETMAIN MACRO    */
         GETMAIN RU,LV=(0),SP=245
*/* P MOVE OLD TABLE TO NEW & ZERO UNUSED BALANCE                    */
*        WKREG0 = AIDPTR;              /* SOURCE PTR FOR MOVE LONG   */
         LR    WKREG0,AIDPTR                                       0234
*        WKREGE = RPARM;     /* TARGET PTR FOR MOVE LONG     @ZA06784*/
         LR    WKREGE,RPARM                                        0235
*        GEN (LH WKREG1,APPSIZE(,AIDPTR)); /* LEN OF SRC FLD @ZA06784*/
         LH WKREG1,APPSIZE(,AIDPTR)
*        AIDPTR = WKREGE;    /* POINTER TO NEW TABLE         @ZA06784*/
         LR    AIDPTR,WKREGE                                       0237
*        WKREGF = WKREG1;    /* LENGTH OF OLD TABLE          @ZA06784*/
         LR    WKREGF,WKREG1                                       0238
*        GEN (AR WKREGF,WKREGF);  /* DOUBLE IT FOR NEW LEN   @ZA06784*/
         AR WKREGF,WKREGF
*        GEN (MVCL  WKREGE,WKREG0);    /* EXECUTE MOVE LONG          */
         MVCL  WKREGE,WKREG0
*/* P UPDATE TABLE HEADER                                            */
*        APPBAL = APPSIZE + APPBAL;    /* NEW BALANCE OF UNUSED CORE */
         LH    @15,APPSIZE(,AIDPTR)                                0241
         LH    @14,APPBAL(,AIDPTR)                                 0241
         ALR   @14,@15                                             0241
         STH   @14,APPBAL(,AIDPTR)                                 0241
*        APPSIZE = 2 * APPSIZE;        /* NEW TOTAL TABLE SIZE       */
         ALR   @15,@15                                             0242
         STH   @15,APPSIZE(,AIDPTR)                                0242
*/* P POINT CVT TO NEW TABLE                                         */
*        RPARM = CVTAIDVT;             /* PICK UP PTR TO OLD TABLE   */
         L     RPARM,CVTAIDVT(,RCVT)                               0243
*        CVTAIDVT = AIDPTR;            /* STORE PTR TO NEW TABLE     */
         ST    AIDPTR,CVTAIDVT(,RCVT)                              0244
*/* P ADJUST TARGET POINTER INTO NEW TABLE                           */
*        TGTPTR = AIDPTR + (TGTPTR - RPARM); /* SAME OFFSET, NEW BASE*/
         LR    @00,TGTPTR                                          0245
         SLR   @00,RPARM                                           0245
         ALR   @00,AIDPTR                                          0245
         LR    TGTPTR,@00                                          0245
*/* P FREE CORE FOR OLD TABLE                                        */
*        WKREG0 = RPARM -> APPSIZE;    /* GET SIZE OF AREA TO FREE   */
         LH    WKREG0,APPSIZE(,RPARM)                              0246
*        GEN (FREEMAIN RU,LV=(0),A=(1),SP=245); /* FREEMAIN  @ZA06784*/
         FREEMAIN RU,LV=(0),A=(1),SP=245
*/* R RETURN TO MAIN LINE                                            */
*        REXIT = SUBRTN;               /* RESTORE RETURN POINT       */
         LR    REXIT,SUBRTN                                        0248
*        RESPECIFY SUBRTN UNRSTD;      /* RELEASE REGISTER FOR PLS   */
*        END EXTEND;                   /* RETURN TO MAIN-LINE        */
@EL00002 DS    0H                                                  0250
@EF00002 DS    0H                                                  0250
@ER00002 BR    @14                                                 0250
*/*IEAVNP16: END                                                     */
*        GEN (EJECT);                                              0251
         EJECT
