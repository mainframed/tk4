         LA    R0,792                                              0155
*   GEN(IECRES GET,PREFIX=FIRST,LV=(0),KEY=(R4),ID=RR0A)           0156
*   REFS(CVT,CVTDMSR);              /* GET CORE FOR TRR PROCESSING   */
         IECRES GET,PREFIX=FIRST,LV=(0),KEY=(R4),ID=RR0A
*   RFY                                                            0157
*     R4 UNRSTD;                    /* MAKE REG UNRESTRICTED         */
*   RD=R1;                          /* TRR W/A EXTENDED PREFIX PTR   */
         LR    RD,R1                                               0158
