NEXTMOD  SLR   @15,@15                                             0226
         ST    @15,RR1RETRY(,RRRF)                                 0226
*   RFY                                                            0227
*     RF RSTD;                      /* RESTRICT REG                  */
*   IF RRFNRTCA=ON×                 /* IF NO RTCA EXISTS OR          */
*       RRFDADSM=ON×                /* A DADSM ENTRY WAS MADE OR     */
*       RRFRJFCB=ON THEN            /* A RDJFCB ENTRY WAS MADE       */
         TM    RRFNRTCA(RRRF),B'01000000'                          0228
         BO    @RT00228                                            0228
         TM    RRFDADSM(RRRF),B'11000000'                          0228
         BZ    @RF00228                                            0228
@RT00228 DS    0H                                                  0229
*     RF=RR0E(9:12);                /* SET UP TO GO TO CLEAN UP      */
         ICM   RF,15,RR0E+8                                        0229
*   ELSE                            /* OTHERWISE                     */
*     RF=RR0B(9:12);                /* SET UP TO GO TO FORCE CLOSE   */
         B     @RC00228                                            0230
@RF00228 ICM   RF,15,RR0B+8                                        0230
*   GEN(BR    RF);                  /* BRANCH TO SELECTED MODULE     */
@RC00228 DS    0H                                                  0231
         BR    RF
