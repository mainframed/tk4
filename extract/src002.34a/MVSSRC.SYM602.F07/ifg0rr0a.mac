*/********************************************************************/
*/*                SET UP DATA FOR SYS1.LOGREC RECORD                */
*/********************************************************************/
*                                                                  0262
*SUBLOG:                                                           0262
*   PROC OPTIONS(NOSAVEAREA);       /* LOGREC SET UP SUBROUTINE      */
         B     @PB00003                                            0262
SUBLOG   STM   @14,@12,12(@13)                                     0262
*   RECERMOD=WTGMODNM;              /* SET UP ERROR MODULE NAME      */
         MVC   RECERMOD(8,RD),WTGMODNM(RWTG)                       0263
*   RECERCST=WTGMODNM;              /* SET UP ERROR CSECT NAME       */
         MVC   RECERCST(8,RD),WTGMODNM(RWTG)                       0264
*   RECRRID=RR0A;                   /* SET UP TRR ID                 */
         MVC   RECRRID(8,RD),RR0A                                  0265
*   GEN(MODESET EXTKEY=SUPR);       /* INSURE KEY ZERO               */
         MODESET EXTKEY=SUPR
*   DO;                             /* SETRP RECPARM(RECPARML)RECORD(
*                                      YES)WKAREA(SDWAPTR)           */
*     RESPECIFY                                                    0268
*      (GPR00P,                                                    0268
