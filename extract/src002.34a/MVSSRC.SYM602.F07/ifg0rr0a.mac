*           IF SDWAPCHK=ON THEN     /* SYS KEY PCK           @ZA01557*/
         TM    SDWAPCHK(SDWAPTR),B'01000000'                       0193
         BNO   @RF00193                                            0193
*             DO;                   /* APAR PROBLEM          @ZA01557*/
*               WADUMP=ON;          /* SDUMP INDICATOR       @ZA01557*/
         OI    WADUMP(RD),B'00100000'                              0195
*               LIMIT=DIM(B);       /* SETUP COPY WA         @ZA01557*/
         MVI   LIMIT(RD),X'04'                                     0196
