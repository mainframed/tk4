         LR    RA,RF                                               0159
*   RFY                                                            0160
*     RF UNRSTD;                    /* MAKE REG UNRESTRICTED         */
*   GEN;                                                           0161
         BALR  RBASE,0                  SET RBASE FOR BASE REG CALC
BASECALC EQU   *                        LABEL TO RECALCULATE BASE
         LA    RBASE,0(RBASE)           INSURE HI-ORDER BYTE IS ZERO
         LA    RF,BASECALC-START0A      SET RF FOR BASE REG CALC
         SR    RBASE,RF                 SET RBASE AS BASE REG
*   RRRF=RA->IECRRPRM;              /* RESTORE RRPLIST PTR           */
         L     RRRF,IECRRPRM(,RA)                                  0162
*   RWTG=RA->IECWTGTB;              /* RESTORE WTG TABLE PTR         */
         L     RWTG,IECWTGTB(,RA)                                  0163
