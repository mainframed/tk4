START0A  DS    0H                                                  0125
*   IF R0=12 THEN                   /* WAS AN RTCA OBTAINED          */
         C     R0,@CF00067                                         0125
         BNE   @RF00125                                            0125
*     DO;                           /* NO                            */
*       RRRF=R2->IECRRPRM;          /* GET RRPLIST PTR               */
         L     RRRF,IECRRPRM(,R2)                                  0127
