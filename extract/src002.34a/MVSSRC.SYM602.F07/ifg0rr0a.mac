         L     @15,RRFWORK(,RRRF)                                  0169
         ST    RD,SACALLED(,@15)                                   0169
*       GEN(MODESET KEYADDR=(2),WORKREG=2);/* SET PROPER KEY         */
         MODESET KEYADDR=(2),WORKREG=2
*       SDWAPTR=RR1WRKAR;           /* RESTORE RTCA PTR              */
         L     SDWAPTR,RR1WRKAR(,RRRF)                             0171
*     END;                                                         0172
