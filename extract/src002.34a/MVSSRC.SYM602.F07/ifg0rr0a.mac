         LA    @15,ERRECALL                                        0188
         ST    @15,SARETRY(,RD)                                    0188
*   IF RRFNRTCA=OFF THEN            /* IF RTCA EXISTS        @ZA01557*/
         TM    RRFNRTCA(RRRF),B'01000000'                          0189
         BNZ   @RF00189                                            0189
*     IF SDWAPERC=OFF THEN          /* IF NO PERCOLATION     @ZA01557*/
         TM    SDWAPERC(SDWAPTR),B'00010000'                       0190
         BNZ   @RF00190                                            0190
*       IF USERKEY=OFF THEN         /* SYS KEY ABEND         @ZA01557*/
         TM    USERKEY(SDWAPTR),B'10000000'                        0191
         BNZ   @RF00191                                            0191
