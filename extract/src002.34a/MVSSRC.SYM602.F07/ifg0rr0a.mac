*/*             ERROR RECORDING SUBROUTINE DETERMINATION             */
*/********************************************************************/
*                                                                  0185
*ERRREC0A:                                                         0185
*   SARETRY=ADDR(NEXTMOD);          /* SETUP RETRY ADDRESS     YM8511*/
ERRREC0A LA    @15,NEXTMOD                                         0185
         ST    @15,SARETRY(,RD)                                    0185
*   LIMIT=DIM(A);                   /* SET DEFAULT RTNS      @ZA01557*/
         MVI   LIMIT(RD),X'02'                                     0186
*   ARYPTR=ADDR(A);                 /* SUB230 & SUBGTF       @ZA01557*/
         LA    @15,A                                               0187
         ST    @15,ARYPTR(,RD)                                     0187
