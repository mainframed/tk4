*/*                      SYSEVENT OKSWAP ROUTINE                     */
*/********************************************************************/
*                                                                  0234
*SYSEVENT:                                                         0234
*   PROC OPTIONS(NOSAVEAREA);       /* SYSEVENT OKSWAP EPA     YM8511*/
         B     @PB00002                                            0234
SYSEVENT STM   @14,@12,12(@13)                                     0234
*   RFY                                                            0235
*     RWTGC RSTD;                                                  0235
*   RFY                                                            0236
*     RCORE RSTD;                                                  0236
*   RFY                                                            0237
*     DSMADTRL BASED(RRRF-24);      /* DADSM AUDIT TRAIL       YM8511*/
*   IF RRFDADSM=ON THEN             /* IF DADSM THEN           YM8511*/
         TM    RRFDADSM(RRRF),B'10000000'                          0238
         BNO   @RF00238                                            0238
*     IF DSMASWAP=ON THEN           /* CHECK FOR SYSEVENT      YM8511*/
         LR    @15,RRRF                                            0239
         AL    @15,@CF02726                                        0239
         TM    DSMASWAP(@15),B'00000001'                           0239
         BNO   @RF00239                                            0239
*       GEN(SYSEVENT OKSWAP);       /* CALL SYSEVENT           YM8511*/
         SYSEVENT OKSWAP
*     ELSE                                                         0241
*       ;                           /* RETURN                  YM8511*/
@RF00239 DS    0H                                                  0242
*   ELSE                            /* NOT DADSM ENTRY         YM8511*/
*     DO RWTGC=RWTG+LENGTH(WTG) BY LENGTH(WTGENTRY) TO IECRRPRM-16;/*
*                                      OCE WORK AREA LOOP      YM8511*/
         B     @RC00238                                            0242
@RF00238 LA    RWTGC,32                                            0242
         ALR   RWTGC,RWTG                                          0242
         B     @DE00242                                            0242
@DL00242 DS    0H                                                  0243
*       RCORE=WTGCORE;              /* INIT WORK AREA PTR      YM8511*/
         L     RCORE,WTGCORE-1(,RWTGC)                             0243
         LA    RCORE,0(,RCORE)                                     0243
*       IF RCORE^=0 THEN            /* IF VALID WORKAREA       YM8511*/
         LTR   RCORE,RCORE                                         0244
         BZ    @RF00244                                            0244
*         IF DXECB(1)=POSTFLAG THEN /* POSTED YET? ZA00115           */
         CLI   DXECB(RCORE),X'80'                                  0245
         BNE   @RF00245                                            0245
*           DO;                     /* ZA00115                       */
*             PURGELST=PURGELST&&PURGELST;/* CLEAR PARMLIST ZA00115  */
         XC    PURGELST(12,RD),PURGELST(RD)                        0247
*             PURGBYT0=PURGEFLG;    /* SET PURGE FLAGS ZA00115       */
         MVI   PURGBYT0(RD),B'11100100'                            0248
*             PURGEDEB=ADDR(DXDEB); /* INIT DEB ADDR ZA00115         */
         LA    @15,DXDEB(,RCORE)                                   0249
         STCM  @15,7,PURGEDEB(RD)                                  0249
*             R1=ADDR(PURGELST);    /* R1 POINTS TO LIST ZA00115     */
         LA    R1,PURGELST(,RD)                                    0250
*             GEN(PURGE (1));       /* PURGE I/O ZA00115             */
         PURGE (1)
*             IF DXECB(1)=POSTFLAG THEN/* POSTED YET? ZA00115        */
         CLI   DXECB(RCORE),X'80'                                  0252
         BNE   @RF00252                                            0252
*               GEN REFS(DXECB)(POST DXECB(0,4));/* POST ECB ZA00115 */
         POST DXECB(0,4)
*           END;                                                   0254
@RF00252 DS    0H                                                  0255
*       IF DXATSWAP=ON THEN         /* CHECK FOR SYSEVENT      YM8511*/
@RF00245 DS    0H                                                  0255
@RF00244 TM    DXATSWAP(RCORE),B'01000000'                         0255
         BNO   @RF00255                                            0255
*         GEN(SYSEVENT OKSWAP);     /* CALL SYSEVENT           YM8511*/
         SYSEVENT OKSWAP
*     END;                          /* LOOP                    YM8511*/
@RF00255 AL    RWTGC,@CF00062                                      0257
@DE00242 L     @15,WTGPREFX(,RWTG)                                 0257
         L     @15,IECRRPRM(,@15)                                  0257
         SL    @15,@CF00197                                        0257
         CR    RWTGC,@15                                           0257
         BNH   @DL00242                                            0257
*   RFY                                                            0258
*     RWTGC UNRSTD;                                                0258
*   RFY                                                            0259
*     RCORE UNRSTD;                                                0259
*   END SYSEVENT;                   /* RETURN TO CALLER        YM8511*/
@EL00002 DS    0H                                                  0260
@EF00002 DS    0H                                                  0260
@ER00002 LM    @14,@12,12(@13)                                     0260
         BR    @14                                                 0260
@PB00002 DS    0H                                                  0261
