@RF00166 LA    @15,288                                             0173
         ALR   @15,RD                                              0173
         ST    @15,RR1WRKAR(,RRRF)                                 0173
*   TRRPFXP=ADDR(TRRPREFX);         /* SET BASE PREFIX PTR     YM4614*/
         LR    @15,RD                                              0174
         SL    @15,@CF00092                                        0174
         ST    @15,TRRPFXP(,RD)                                    0174
*   SARETRY=ADDR(OKSWAP);           /* SETUP RETRY ADDRESS     YM8511*/
         LA    @15,OKSWAP                                          0175
         ST    @15,SARETRY(,RD)                                    0175
*   RR1RETRY=ADDR(RETRY0A);         /* RETRY THIS MODULE       YM8511*/
         LA    @15,RETRY0A                                         0176
         ST    @15,RR1RETRY(,RRRF)                                 0176
*   GEN REFS(IECREGSV,IECEXTPR,TRRWTG,CVT,CVTDMSR);                0177
*        IECRES LOAD,EXTPR=(RD),MODNM=RR0A,BRANCH=NO  OPTIONAL TRACE
         LR    15,RD                    GET PTR TO EXT PREFIX    Y02080
         STM   15,12,IECREGSV-IECEXTPR+16(15) SAVE REGISTERS     Y02080
         LA    1,32                     BACK UP REGISTER 15 TO   Y02080
         SR    15,1                     POINT TO START OF PREFIX Y02080
         MVC   TRRWTG(12,13),RR0A       MOVE MODULE NAME & EPA   Y02080
         LA    6,TRRWTG(,13)            LOAD PTR TO WTG MOD NAME Y02080
         L     5,CVTPTR                 GET ADDRESS OF CVT       Y02080
         L     5,CVTDMSR-CVT(,5)        ADDRESS OF RESIDENT RTN  Y02080
         BAL   14,24(5)                 LOAD AND RETURN          Y02080
         LM    14,12,44(15)             RESTORE REGISTERS        Y02080
*   GEN(EJECT);                                                    0178
         EJECT
