         TITLE ' *** IKTVTPUT: TSO/VTAM TPUT ROUTINE ***               *
                        '
IKTVTPUT CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKTVTPUT  78.152'                                 0001
         DROP  @15
@PROLOG  BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
RBPRFXLN EQU 64
RBPRFLNA EQU 32
PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 224
*/*                                                                 **/
*/********************************************************************/
*/*                                                                  */
*/*      START OF IKTVTPUT...TSO/VTAM TPUT ROUTINE.                  */
*/*                                                                  */
*/********************************************************************/
*TVWAPTR = TSBXTVWA;                 /* SET UP ADDRESSABILITY TO TVWA*/
         L     @15,TSBEXTNT(,TSBPTR)                               0084
         L     TVWAPTR,TSBXTVWA(,@15)                              0084
*RFY SAVEREG RSTD;                                                 0085
*SAVEREG = TVWALLWA;                 /* SET-UP SAVE AREA FOR CALLS 0086
*                                       TO SRM & QUEUE MGR           */
         L     SAVEREG,TVWALLWA(,TVWAPTR)                          0086
*XSATCBA = TCBPTR ;                  /* SAVE TCB ADDRESS             */
         STCM  TCBPTR,7,XSATCBA(RBSECPTR)                          0087
*RFY  TCBPTR  UNRSTD;                /* FREE-UP REGISTER             */
*RFY TCB BASED(XSATCBA);                                           0089
*IF TSBXACTV = '0'B ×                /* TEST IF INITIALIZ. COMPLETE  */
*   XSAPRMSZ = 0 THEN                /* OR IF ZERO BYTE TPUT         */
         TM    TSBXACTV(@15),B'01000000'                           0090
         BZ    @RT00090                                            0090
         LH    @04,XSAPRMSZ(,RBSECPTR)                             0090
         LTR   @04,@04                                             0090
         BNZ   @RF00090                                            0090
@RT00090 DS    0H                                                  0091
*     XSARC = SUCCESS;               /* IF INIT NOT COMPLETE OR ZERO
*                                       BYTE TPUT, NOOP AND RETURN   */
         IC    @04,SUCCESS                                         0091
         STC   @04,XSARC(,RBSECPTR)                                0091
*ELSE                                                              0092
*  DO;                               /* INITIALIZATION COMPLETE DO   */
         B     @RC00090                                            0092
@RF00090 DS    0H                                                  0093
*    CALL WIFOWAIT;                  /* CALL'WAIT IF OWAIT IN PROGRESS'
*                                       WHICH SETS XSARC, AND CAN  0093
*                                       SUSPEND THE CALLING TASK     */
         BAL   @14,WIFOWAIT                                        0093
*    IF XSARC ^= CONTINUE THEN;      /* IF CODE IS NOT SET TO CONTINUE
*                                       (SET TO SESSION CANCELLED OR
*                                        ATTENTION HIT) THEN RETURN
*                                        XSARC TO CALLER             */
         CLC   XSARC(1,RBSECPTR),CONTINUE                          0094
         BNE   @RT00094                                            0094
*    ELSE                                                          0096
*      DO;                                                         0096
*        TSBOWIP = '1'B;             /* INDICATE TSB 'OWNED' BY TASK */
         OI    TSBOWIP(TSBPTR),B'00100000'                         0097
*        TSBCTCB = XSATCBA;          /* SAVE TCB ADDRESS     @ZA32676*/
         L     @04,XSATCBA-1(,RBSECPTR)                            0098
         LA    @04,0(,@04)                                         0098
         ST    @04,TSBCTCB(,TSBPTR)                                0098
*        CALL DATAEDIT;              /* PERFORM DEVICE INDEPENDENT 0099
*                                       OUTPUT EDITING               */
         BAL   @14,DATAEDIT                                        0099
*        IF TSBNOBRK = '1'B THEN     /* TERMINAL OPR SPECIFY NOBREAK?*/
         TM    TSBNOBRK(TSBPTR),B'00000010'                        0100
         BNO   @RF00100                                            0100
*             XSABRK = '0'B;         /* YES, ENSURE 'BREAKIN'      0101
*                                       WON'T OCCUR                  */
         NI    XSABRK(RBSECPTR),B'11111011'                        0101
*        CALL TRYTOQUE;              /* REQUEST THAT DATA BE PLACED
*                                       ON OUTPUT QUEUE. TASK WILL BE
*                                       SUSPENDED IF HIGH BUFFER   0102
*                                       THRESHOLD HAS BEEN REACHED.
*                                       TRYTOQUE SETS XSARC          */
@RF00100 BAL   @14,TRYTOQUE                                        0102
*        IF XSARC ^= CONTINUE THEN;  /* IF CODE IS NOT SET TO CONTINUE
*                                       (SET TO NOWAIT,            0103
*                                                 SESSION CANCELLED,
*                                        OR ATTENTION HIT) THEN RETURN
*                                        XSARC TO CALLER             */
         CLC   XSARC(1,RBSECPTR),CONTINUE                          0103
         BNE   @RT00103                                            0103
*        ELSE                                                      0105
*         DO;                                                      0105
*          CALL SCHEDTOM;            /* GO SCHEDULE THE TERMINAL OUTPUT
*                                       MANAGER, IF NECESSARY        */
         BAL   @14,SCHEDTOM                                        0106
*          /**********************************************************/
*          /*                                                        */
*          /*      TEST IF TASK SPECIFIED TPUT HOLD.                 */
*          /*      IF SO, SUSPEND CALLER UNTIL DATA ARRIVES          */
*          /*      AT TERMINAL. IF NOT,TELL SRM OF SUCCESFUL         */
*          /*      TPUT AND RETURN TO TASK                           */
*          /*                                                        */
*          /**********************************************************/
*          /*                                                        */
*          IF XSAHOLD = '1'B THEN    /* TASK SPECIFY HOLD?           */
         TM    XSAHOLD(RBSECPTR),B'00001000'                       0107
         BNO   @RF00107                                            0107
*            DO;                     /* YES                          */
*              TSBHOLD = '1'B;       /* INDICATE  TPUT HOLD FOOTPRINT*/
         OI    TSBHOLD(TSBPTR),B'00001000'                         0109
*              CALL WAITCHK;         /* CALL'WAIT&CHECK' TO SUSPEND
*                                       CALLER AND THEN CHECK TERMINAL
*                                       STATUS. WAITCHK SETS XSARC.  */
         BAL   @14,WAITCHK                                         0110
*              TSBHOLD = '0'B;       /* CLEAR TPUT HOLD FOOTPRINT    */
         NI    TSBHOLD(TSBPTR),B'11110111'                         0111
*              IF XSARC = SUCCESS THEN /* TEST IF SUCCESSFUL         */
         CLC   XSARC(1,RBSECPTR),SUCCESS                           0112
         BNE   @RF00112                                            0112
*               DO;                                                0113
*                /*                                                  */
*                /****************************************************/
*                /*                                                  */
*                /*     TELL SRM THAT OUTPUT COMPLETED-'TGETTPUT'    */
*                /*                                                  */
*                /****************************************************/
*                /*                                                  */
*                RFY (PRMREG0,PRMREG1) RSTD;                       0114
*                PRMREG0 = ASCBASID;                               0115
         L     @04,ASCBPTR                                         0115
         LH    PRMREG0,ASCBASID(,@04)                              0115
         N     PRMREG0,@CF02477                                    0115
*                PRMREG1 = TPUT;      /* INDICATE TPUT(BIT 0 = 1)    */
         L     PRMREG1,TPUT                                        0116
*                GEN REFS(SAVEREG,ASCBASID,CVTOPTE,CVT,CVTPTR)     0117
*                    SETS(PRMREG0, RTNREG,ENTRYREG);               0117
***
***      INFORM SRM OF TPUT COMPLETION
***
         SYSEVENT TGETTPUT,ASID=(PRMREG0),ENTRY=BRANCH
*                RFY (PRMREG0,PRMREG1) UNRSTD;                     0118
*               END;                 /* END OF 'SUCCESSFUL RTN FROM
*                                       WAITCHK' DO GROUP            */
*            END;                    /* END OF 'TPUT HOLD' DO GROUP  */
*          ELSE                                                    0121
*            DO;                     /* TPUT NOHOLD DO GROUP         */
         B     @RC00107                                            0121
@RF00107 DS    0H                                                  0122
*              /*                                                    */
*              /******************************************************/
*              /*                                                    */
*              /*       TELL SRM THAT OUTPUT COMPLETED- 'TGETTPUT'   */
*              /*                                                    */
*              /******************************************************/
*              /*                                                    */
*              RFY (PRMREG0,PRMREG1) RSTD;                         0122
*              PRMREG0 = ASCBASID;   /* DEFINE ASID TO SRM           */
         L     @04,ASCBPTR                                         0123
         LH    PRMREG0,ASCBASID(,@04)                              0123
         N     PRMREG0,@CF02477                                    0123
*              PRMREG1 = TPUT;       /* INDICATE TPUT(BIT 0 = 1)     */
         L     PRMREG1,TPUT                                        0124
*              GEN REFS(SAVEREG,ASCBASID,CVTOPTE,CVT,CVTPTR)       0125
*                  SETS(RTNREG,ENTRYREG);                          0125
***
***      INFORM SRM OF TPUT COMPLETION
***
         SYSEVENT TGETTPUT,ASID=(PRMREG0),ENTRY=BRANCH
*              RFY (PRMREG0,PRMREG1);                              0126
*            END;                    /* END OF TPUT NOHOLD DO GROUP  */
*              /* XSARC = 0 FROM RETURN FROM 'TRYTOQUE'              */
*         END;                       /* END OF'SUCCESSFUL ADD TO   0128
*                                       QUEUE'DO GROUP               */
@RC00107 DS    0H                                                  0129
*        CALL RESWAITS;              /* RESET WAITING TASKS, IF ANY
*                                       AND  CLEAR TSBOWIP           */
@RT00103 BAL   @14,RESWAITS                                        0129
*      END;                          /* END OF 'SUCCESSFUL WAIT IF 0130
*                                       OWAIT' DO GROUP              */
*    /*                                                              */
*    /****************************************************************/
*    /*                                                              */
*    /*        END OF TPUT PROCESSING. RESET INDICATORS, RELEASE LOCK*/
*    /*        AND RETURN XSARC TO CALLING TASK.                     */
*    /*                                                              */
*    /****************************************************************/
*    /*                                                              */
*    TCBTIOTG = '0'B;                /* CLEAR ATTN INDICATOR IN TCB  */
@RT00094 L     @04,XSATCBA-1(,RBSECPTR)                            0131
         NI    TCBTIOTG(@04),B'11101111'                           0131
*    TSBATTN  = '0'B;                /* CLEAR ATTN INDICATOR IN TSB  */
         NI    TSBATTN(TSBPTR),B'01111111'                         0132
*  END;                               /* END OF 'INITIALIZATION    0133
*                                       COMPLETE' DO GROUP           */
*IF XSATCBFX = '1'B THEN             /* WERE ASYNCHRONOUS EXITS    0134
*                                      ALLOWED ON ENTRY?             */
@RC00090 TM    XSATCBFX(RBSECPTR),B'00100000'                      0134
         BNO   @RF00134                                            0134
*  TCBFX = '0'B;                     /* YES, RESET TCB ASYN EXIT IND.*/
*                                                                  0135
         L     @04,XSATCBA-1(,RBSECPTR)                            0135
         NI    TCBFX(@04),B'11111110'                              0135
*IF TSBCTCB = XSATCBA THEN           /* IF THIS TCB OWNS THE TSB     */
@RF00134 L     @04,TSBCTCB(,TSBPTR)                                0136
         L     @15,XSATCBA-1(,RBSECPTR)                            0136
         LA    @15,0(,@15)                                         0136
         CR    @04,@15                                             0136
         BNE   @RF00136                                            0136
*   TSBCTCB = 0;                     /* THEN CLEAR  IT               */
*                                                                  0137
         SLR   @04,@04                                             0137
         ST    @04,TSBCTCB(,TSBPTR)                                0137
*/* RELEASE LOCAL LOCK */                                          0138
*GEN SETS(RTNREG,ENTRYREG,REG0,REG1) REFS(PSALITA,FLC);            0138
@RF00136 DS    0H                                                  0138
         SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,                         --
               RELATED=(TSB,IKTVTPUT(ATENTRY))
*RFY RTNREG  RSTD;                                                 0139
*RTNREG = XSARETG;                   /* RESTORE RETURN ADDRESS       */
         L     RTNREG,XSARETG(,RBSECPTR)                           0140
*RETURN CODE(XSARC);                 /* RETURN TO ADDRESS THAT WAS IN
*                                       R14 ON ENTRY, WITH TPUT RETURN
*                                       CODE IN R15                  */
         SLR   @15,@15                                             0141
         IC    @15,XSARC(,RBSECPTR)                                0141
@EL00001 DS    0H                                                  0141
@EF00001 DS    0H                                                  0141
@ER00001 BR    @14                                                 0141
*RFY RTNREG UNRSTD;                                                0142
*WAITCHK: PROC OPTIONS(NOSAVEAREA,NOSAVE);                         0143
         B     @EL00001                                            0143
WAITCHK  DS    0H                                                  0144
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*       WAITCHK IS AN INTERNAL PROCEDURE THAT SUSPENDS THE         */
*/*       CALLING TASK, AND THEN CHECKS THE TERMINAL STATUS          */
*/*       UPON BEING RE-DISPATCHED.                                  */
*/*      **** WAITCHK SAVES RETURN ADDRESS INTO THE LEVEL 'B'        */
*/*           RETURN 'SAVE WORD' - XSASAVEB -                        */
*/*                                                                 **/
*/*       WAITCHK RETURNS CODE IN XSARC THAT IS SET BY TERMSTAT      */
*/*           = 0 -NORMAL                                            */
*/*           = 8 -ATTENTION                                         */
*/*           =14 HEX - SESSION CANCELLED                            */
*/********************************************************************/
*/*                                                                  */
*RFY RTNREG RSTD;                                                  0144
*XSASAVEB = RTNREG;                  /* SAVE RETURN ADDRESS          */
         ST    RTNREG,XSASAVEB(,RBSECPTR)                          0145
*RFY RTNREG UNRSTD;                                                0146
*TSBOWAIT = '1'B;                    /* INDICATE OUTPUT WAIT STATE   */
         OI    TSBOWAIT(TSBPTR),B'00100000'                        0147
*RFY (PRMREG0,PRMREG1) RSTD;                                       0148
*PRMREG0 = ASCBASID;                                               0149
         L     @04,ASCBPTR                                         0149
         LH    PRMREG0,ASCBASID(,@04)                              0149
         N     PRMREG0,@CF02477                                    0149
*PRMREG1 = TPUT;                     /* INDICATE TPUT                */
         L     PRMREG1,TPUT                                        0150
*GEN REFS(SAVEREG,ASCBASID,CVTOPTE,CVT,CVTPTR)                     0151
*    SETS(RTNREG,ENTRYREG);                                        0151
***
***      INFORM SYSTEM RESOURCE MANAGER OF WAIT
***
         SYSEVENT TERMWAIT,ASID=(PRMREG0),ENTRY=BRANCH
*RFY (PRMREG0,PRMREG1) UNRSTD;                                     0152
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*       PREPARE TO SUSPEND CALLER VIA BRANCH TO THE STATUS         */
*/*       ROUTINE. LOCAL LOCK HELD ON ENTRY TO STATUS ROUTINE        */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*STM(PRMREG0,ENTRYREG,TCBGRS);       /* SAVE REGISTERS               */
         L     @04,XSATCBA-1(,RBSECPTR)                            0153
         LA    @04,0(,@04)                                         0153
         STM   PRMREG0,ENTRYREG,TCBGRS(@04)                        0153
*RBOPSW2 = ADDR(REDISPCH);           /* SET UP RESUME PSW            */
         LA    @15,REDISPCH                                        0154
         ST    @15,RBOPSW2(,RBSECPTR)                              0154
*RFY (PRMREG0,PRMREG1) RSTD;                                       0155
*SAVEREG = NDISPBIT;                 /* SET NON-DISPATCHABLE FLAG    */
         LA    SAVEREG,1024                                        0156
*PRMREG0 = SDTCB;                    /* SET ENTRY CODE               */
         LA    PRMREG0,15                                          0157
*PRMREG1 = XSATCBA;                  /* SET TCB TO SUSPEND, AND SET
*                                       RESET CODE(BYTE 30 TO SET(=0)*/
         LR    PRMREG1,@04                                         0158
*CALL STATUS;                        /* CALL STATUS TO SUSPEND TASK  */
         L     @04,CVTPTR                                          0159
         L     @04,SCVTSPTR(,@04)                                  0159
         L     @15,SCVTSTAT(,@04)                                  0159
         BALR  @14,@15                                             0159
*RFY (PRMREG0,PRMREG1) UNRSTD;                                     0160
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*       UPON BEING RE-DISPATCHED, LOCAL LOCK MUST                  */
*/*       BE RE-OBTAINED                                             */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*REDISPCH:                                                         0161
*GEN SETS(RTNREG,ENTRYREG,REG0,REG1) REFS(PSALITA,FLC);            0161
REDISPCH DS    0H                                                  0161
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,              --
               RELATED=(TSB,IKTVTPUT(RETURN))
*CALL TERMSTAT;                      /* GO CHECK TERMINAL STATUS.  0162
*                                       TERMSTAT SETS XSARC.         */
         BAL   @14,TERMSTAT                                        0162
*RFY RTNREG RSTD;                                                  0163
*RTNREG = XSASAVEB;                  /* RESTORE RETURN ADDRESS       */
         L     RTNREG,XSASAVEB(,RBSECPTR)                          0164
*RETURN;                                                           0165
@EL00002 DS    0H                                                  0165
@EF00002 DS    0H                                                  0165
@ER00002 BR    @14                                                 0165
*RFY RTNREG UNRSTD;                                                0166
*END WAITCHK;                        /* END OF WAIT & CHECK ROUTINE  */
*TERMSTAT: PROC OPTIONS(NOSAVEAREA,NOSAVE);                        0168
TERMSTAT DS    0H                                                  0169
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*       TERMSTAT IS AN INTERNAL PROCEDURE THAT                     */
*/*       CHECKS THE STATUS OF THE TERMINAL.                         */
*/*       IT SETS XSARC ON RETURN                                    */
*/*         = 0 - NORMAL                                             */
*/*         = 8 - ATTENTION HIT                                      */
*/*         = 14 HEX - SESSION CANCELLED                             */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*RFY RTNREG RSTD;                                                  0169
*IF TSBCANC = '1'B THEN              /* TEST TERMINAL CANCELLED      */
         TM    TSBCANC(TSBPTR),B'00000100'                         0170
         BNO   @RF00170                                            0170
*  XSARC = TERMCNCL;                 /* IF YES, SET RC = CANCELLED   */
         MVI   XSARC(RBSECPTR),X'14'                               0171
*ELSE                                                              0172
*  IF TCBTIOTG = '1'B THEN           /* TEST ATTN HIT                */
         B     @RC00170                                            0172
@RF00170 L     @15,XSATCBA-1(,RBSECPTR)                            0172
         TM    TCBTIOTG(@15),B'00010000'                           0172
         BNO   @RF00172                                            0172
*    XSARC = ATTEN;                  /* IF SO, SET RC = ATTENTION    */
         MVI   XSARC(RBSECPTR),X'08'                               0173
*  ELSE                                                            0174
*    XSARC = SUCCESS;                /* OTHERWISE SET RC SUCCESSFUL  */
         B     @RC00172                                            0174
@RF00172 IC    @15,SUCCESS                                         0174
         STC   @15,XSARC(,RBSECPTR)                                0174
*END TERMSTAT;                                                     0175
@EL00003 DS    0H                                                  0175
@EF00003 DS    0H                                                  0175
@ER00003 BR    @14                                                 0175
*RFY RTNREG UNRSTD;                                                0176
*WIFOWAIT: PROC OPTIONS(NOSAVEAREA,NOSAVE);                        0177
WIFOWAIT DS    0H                                                  0178
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*       WAIT IF OWAIT IS AN INTERNAL PROCEEDURE THAT               */
*/*       CHECKS THE TERMINAL STATUS AND CHECKS IF ANOTHER           */
*/*       TASK 'OWNS'THE TSB(TSBOWIP). THIS MIGHT BE IF THE          */
*/*       OTHER TASK IS SUSPENDED FOR A TPUT HOLD OR BECAUSE         */
*/*       OF THE HIGH BUFFER THRESHOLD BEING REACHED.                */
*/*       IF TERMINAL STATUS IS NORMAL AND ANOTHER TASK 'OWNS'       */
*/*       THE TSB THEN THE CALLING TASK IS SUSPENDED BY CALLING      */
*/*       WAITCHK                                                    */
*/*      ****WIFOWAIT SAVES RETURN ADDRESS INTO THE LEVEL 'A'        */
*/*         RETURN 'SAVE WORD' - XSASAVEA.                           */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*RFY RTNREG RSTD;                                                  0178
*XSASAVEA = RTNREG;                  /* SAVE RETURN ADDRESS          */
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0179
*RFY RTNREG UNRSTD;                                                0180
*CALL TERMSTAT;                      /* CHECK TERMINAL STATUS      0181
*                                       TERMSTAT SETS XSARC          */
         BAL   @14,TERMSTAT                                        0181
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*       LEAVE TASK IN 'DO WHILE' UNTIL THE TERMINAL STATUS         */
*/*       INDICATES SESSION CANCELLED/ATTENTION OR UNTIL NO          */
*/*       ONE 'OWNS' THE TSB                                         */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*DO WHILE XSARC = CONTINUE & TSBOWIP = '1'B ;                      0182
         B     @DE00182                                            0182
@DL00182 DS    0H                                                  0183
*  TSBWOWIP = '1'B;                  /* INDICATE WAITING ON OWAIT  0183
*                                       IN PROGRESS. TSBOWIP IS CLEARED
*                                       WHEN OTHER TASK COMPLETES  0183
*                                       TPUT.                        */
         OI    TSBWOWIP(TSBPTR),B'00010000'                        0183
*  CALL WAITCHK;                     /* GO SUSPEND CALLING TASK.   0184
*                                       WAITCHK CAN CHANGE VALUE   0184
*                                       OF XSARC                     */
         BAL   @14,WAITCHK                                         0184
*END;                                                              0185
@DE00182 CLC   XSARC(1,RBSECPTR),CONTINUE                          0185
         BNE   @DC00182                                            0185
         TM    TSBOWIP(TSBPTR),B'00100000'                         0185
         BO    @DL00182                                            0185
@DC00182 DS    0H                                                  0186
*RFY RTNREG RSTD;                                                  0186
*RTNREG = XSASAVEA;                  /* RESTORE RETURN ADDRESS       */
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0187
*RETURN;                                                           0188
@EL00004 DS    0H                                                  0188
@EF00004 DS    0H                                                  0188
@ER00004 BR    @14                                                 0188
*RFY RTNREG UNRSTD;                                                0189
*END WIFOWAIT;                                                     0190
*TRYTOQUE: PROC OPTIONS(NOSAVEAREA,NOSAVE);                        0191
TRYTOQUE DS    0H                                                  0192
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*       TRY TO QUEUE IS AN INTERNAL PROCEDURE THAT REQUESTS        */
*/*       THAT THE QUEUE MANAGER ADD THE TPUT DATA TO THE OUTPUT     */
*/*       QUEUE.                                                     */
*/*       IF SUCCESSFUL REQUEST,IT RETURNS                           */
*/*       IF HIGH BUFFER THRESHOLD IS REACHED, IT RETURNS            */
*/*       TO THE CALLER IF TPUT NOWAIT.                              */
*/*       IF TPUT WAIT, IT SUSPENDS THE CALLER BY CALLING            */
*/*       WAITCHK.                                                   */
*/*       ON RETURN XSARC WILL BE SET AS FOLLOWS(BY WAITCHK,TRYTOQUE)*/
*/*         0 - ADDED TO QUEUE                                       */
*/*         4 - NO BUFFERS AVAILABLE & TPUT NOWAIT                   */
*/*         8 - ATTENTION HIT                                        */
*/*        14HEX - SESSION CANCELLED                                 */
*/*                                                                  */
*/*      ****TRYTOQUE SAVES RETURN ADDRESS INTO THE LEVEL 'A'        */
*/*          RETURN 'SAVE WORD' - XSASAVEA.                          */
*/*                                                                  */
*/********************************************************************/
*RFY RTNREG RSTD;                                                  0192
*XSASAVEA = RTNREG;                  /* SAVE RETURN ADDRESS          */
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0193
*RFY RTNREG UNRSTD;                                                0194
*TSBCTCB = XSATCBA;                  /* SET CURRENT TCB TO THIS    0195
*                                       TASK'S TCB.                  */
         L     @04,XSATCBA-1(,RBSECPTR)                            0195
         LA    @04,0(,@04)                                         0195
         ST    @04,TSBCTCB(,TSBPTR)                                0195
*XSADOSWT = '0'B;                    /* INITIALIZE 'DO WHILE' CONTROL*/
         NI    XSADOSWT(RBSECPTR),B'11101111'                      0196
*XSARC = SUCCESS;                    /* INITIALIZE RETURN CODE     0197
*                                       TO SUCCESSFUL                */
         IC    @04,SUCCESS                                         0197
         STC   @04,XSARC(,RBSECPTR)                                0197
*OPBFSZ = XSAPRMSZ;                  /* SET TPUT SIZE                */
         LH    @04,XSAPRMSZ(,RBSECPTR)                             0198
         STH   @04,OPBFSZ(,SAVEREG)                                0198
*RFY WORK1REG RSTD;                                                0199
*WORK1REG = XSAOPTNS;                /* MOVE 4 BITS(XSAHOLD,XSABRK,  */
         SLR   WORK1REG,WORK1REG                                   0200
         IC    WORK1REG,XSAOPTNS(,RBSECPTR)                        0200
*SLL(WORK1REG,20);                   /* XSAEDITO INTO PROPER BIT     */
         SLL   WORK1REG,20                                         0201
*OPFLAGS = WORK1REG;                 /* POSITION OF OPARM FIELD      */
         ST    WORK1REG,OPFLAGS(,SAVEREG)                          0202
*RFY WORK1REG UNRSTD;                                              0203
*IF XSABRK = '0'B THEN               /* IS BREAK-IN SPECIFIED ?      */
         TM    XSABRK(RBSECPTR),B'00000100'                        0204
         BNZ   @RF00204                                            0204
*  OPREQ = ADDL;                     /* NO - NORMAL ADD              */
         MVI   OPREQ(SAVEREG),X'01'                                0205
*ELSE                                                              0206
*  OPREQ = ADDTOP;                   /* YES- ADD TO TOP OF QUEUE     */
         B     @RC00204                                            0206
@RF00204 MVI   OPREQ(SAVEREG),X'08'                                0206
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*          CALL OUTPUT QUEUE MANAGER, IKTQMOUT.                    */
*/*          PARAMETER LIST INTERFACE IS BUILT DIRECTLY              */
*/*          SO THAT PLS WILL NOT GENERATE ANY 'TEMPS'               */
*/*          ITSELF. DIRECTLY BUILT PARAMETER LIST IS IN             */
*/*          LOCALLY LOCKED WORK AREA.                               */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*PRMLSTAD = ADDR(OPARMS);                                          0207
@RC00204 LA    @04,OPARMS(,SAVEREG)                                0207
         ST    @04,PRMLSTAD(,SAVEREG)                              0207
*RFY PRMREG1 RSTD;                                                 0208
*PRMREG1 = ADDR(PRMLSTAD);                                         0209
         LA    PRMREG1,PRMLSTAD(,SAVEREG)                          0209
*CALL IKTQMOUT;                      /* CALL OUTPUT QUEUE MANAGER, 0210
*                                       WHICH SETS OPRC - RETURN CODE*/
         L     @04,CVTPTR                                          0210
         L     @04,TCASTPTR(,@04)                                  0210
         L     @15,TCASOQM(,@04)                                   0210
         BALR  @14,@15                                             0210
*RFY PRMREG1 UNRSTD;                                               0211
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*   LOOP WHILE DATA CAN'T BE ADDED TO QUEUE AND TERMINAL STATUS    */
*/*   IS OK, SUSPENDING CALLER.                                      */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*DO WHILE OPRC = BFRSXCED & XSADOSWT = '0'B;                       0212
         B     @DE00212                                            0212
@DL00212 DS    0H                                                  0213
*  IF XSANOWT = '1'B THEN            /* DID CALLER SPECIFY 'NOWAIT'  */
         TM    XSANOWT(RBSECPTR),B'00010000'                       0213
         BNO   @RF00213                                            0213
*    DO;                                                           0214
*      XSADOSWT = '1'B;              /* YES-FALL OUT OF LOOP         */
         OI    XSADOSWT(RBSECPTR),B'00010000'                      0215
*      XSARC = NOWAIT;               /* SET RETURN CODE              */
         MVI   XSARC(RBSECPTR),X'04'                               0216
*    END;                                                          0217
*  ELSE                                                            0218
*    DO;                                                           0218
         B     @RC00213                                            0218
@RF00213 DS    0H                                                  0219
*      TSBNOBUF = '1'B;              /* INDICATE IN WAIT STATE BECAUSE
*                                       BUFFERS EXCEEDED             */
         OI    TSBNOBUF(TSBPTR),B'00010000'                        0219
*      CALL WAITCHK;                 /* SUSPEND TASK, AND CHECK TERM.
*                                       STATUS WHEN RE-DISPATCHED. 0220
*                                       WAITCHK SETS XSARC           */
         BAL   @14,WAITCHK                                         0220
*      IF XSARC = CONTINUE THEN                                    0221
         CLC   XSARC(1,RBSECPTR),CONTINUE                          0221
         BNE   @RF00221                                            0221
*        DO;                                                       0222
*          IF XSABRK = '0'B THEN     /* IS BREAK-IN SPECIFIED ?      */
         TM    XSABRK(RBSECPTR),B'00000100'                        0223
         BNZ   @RF00223                                            0223
*            OPREQ = ADDL;           /* NO - NORMAL ADD              */
         MVI   OPREQ(SAVEREG),X'01'                                0224
*          ELSE                                                    0225
*            OPREQ = ADDTOP;         /* YES - ADD TO TOP OF QUEUE    */
         B     @RC00223                                            0225
@RF00223 MVI   OPREQ(SAVEREG),X'08'                                0225
*          OPBFSZ = XSAPRMSZ;        /* SET TPUT SIZE                */
@RC00223 LH    @04,XSAPRMSZ(,RBSECPTR)                             0226
         STH   @04,OPBFSZ(,SAVEREG)                                0226
*          RFY WORK1REG RSTD;                                      0227
*          WORK1REG = XSAOPTNS;      /* PUT TPUT OPTIONS BYTE IN REG */
         SLR   WORK1REG,WORK1REG                                   0228
         IC    WORK1REG,XSAOPTNS(,RBSECPTR)                        0228
*          SLL(WORK1REG,20);         /* ADJUST TO PROPER POSITION FOR
*                                       INSERTION IN OPARMS FIELD    */
         SLL   WORK1REG,20                                         0229
*          OPFLAGS = WORK1REG;       /* PUT INTO FLAGS FULL WORD     */
         ST    WORK1REG,OPFLAGS(,SAVEREG)                          0230
*          RFY WORK1REG UNRSTD;                                    0231
*          /*                                                        */
*          /**********************************************************/
*          /*                                                        */
*          /*        CALL OUTPUT QUEUE MANAGER, IKTQMOUT             */
*          /*        PARAMETER LIST INTERFACE IS BUILT DIRECTLY      */
*          /*        SO THAT PLS WILL NOT GENERATE ANY 'TEMPS'       */
*          /*        ITSELF. DIRECTLY BUILT PARAMETER LIST IS IN     */
*          /*        LOCALLY LOCKED WORK AREA.                       */
*          /*                                                        */
*          /**********************************************************/
*          /*                                                        */
*          PRMLSTAD = ADDR(OPARMS);  /* PUT ADDR INTO 'TEMP'         */
         LA    @04,OPARMS(,SAVEREG)                                0232
         ST    @04,PRMLSTAD(,SAVEREG)                              0232
*          RFY PRMREG1 RSTD;                                       0233
*          PRMREG1 = ADDR(PRMLSTAD); /* REG 1 POINTS TO PLS PARM LST */
         LA    PRMREG1,PRMLSTAD(,SAVEREG)                          0234
*          CALL IKTQMOUT;            /* CALL OUTPUT QUEUE MANAGER    */
         L     @04,CVTPTR                                          0235
         L     @04,TCASTPTR(,@04)                                  0235
         L     @15,TCASOQM(,@04)                                   0235
         BALR  @14,@15                                             0235
*          RFY PRMREG1 UNRSTD;                                     0236
*        END;                        /* END CONTINUE DO GROUP        */
*      ELSE XSADOSWT = '1'B;         /* FALL OUT OF LOOP..XSARC IS 0238
*                                       SET FOR PROPER RETURN        */
         B     @RC00221                                            0238
@RF00221 OI    XSADOSWT(RBSECPTR),B'00010000'                      0238
*    END;                            /* END TPUT WAIT DO GROUP       */
@RC00221 DS    0H                                                  0240
*END;                                /* END BIG LOOP WAITING FOR   0240
*                                       QUEUE MANAGER ACCEPTANCE OF
*                                       ADD TO QUEUE REQUEST         */
@RC00213 DS    0H                                                  0240
@DE00212 CLI   OPRC(SAVEREG),2                                     0240
         BNE   @DC00212                                            0240
         TM    XSADOSWT(RBSECPTR),B'00010000'                      0240
         BZ    @DL00212                                            0240
@DC00212 DS    0H                                                  0241
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*       TEST IF ADD TO QUEUE REQUEST WAS SUCCESSFUL                */
*/*       IF SO, MOVE DATA INTO QUEUE ELEMENT, AND UPDATE            */
*/*       LINE COUNT IN SMF CONTROL BLOCK.                           */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*IF XSARC = SUCCESS THEN                                           0241
         CLC   XSARC(1,RBSECPTR),SUCCESS                           0241
         BNE   @RF00241                                            0241
*  DO;                               /* VALIDATE DATA IN TASK'S KEY  */
*    CALL VERIFY;                    /* GO VERIFY THAT CALLER'S    0243
*                                       ADDRESS IS VALID. IF NOT A PGM
*                                       CHECK WILL OCCUR AND TASK WILL
*                                       ABEND. (IKT93EST GETS CONTROL*/
         BAL   @14,VERIFY                                          0243
*    /* MOVE DATA WITH MVCL INSTRUCTION SINCE MORE THAN 256 BYTES    */
*    /* ARE POSSIBLE                                                 */
*    RFY (REG0,REG1,REG2,REG3) RSTD;                               0244
*    REG0 = OPBUFADR;                /* TARGET ADR FOR MVCL          */
         L     REG0,OPBUFADR(,SAVEREG)                             0245
*    REG1 = XSAPRMSZ;                /* TARGET SIZE FOR MVCL         */
         LH    REG1,XSAPRMSZ(,RBSECPTR)                            0246
*    REG2 = XSABFRAD;                /* SOURCE ADR FOR MVCL          */
         L     REG2,XSABFRAD-1(,RBSECPTR)                          0247
         LA    REG2,0(,REG2)                                       0247
*    REG3 = REG1;                    /* SOURCE SIZE SAME AS TARGET   */
         LR    REG3,REG1                                           0248
*    MVCL(REG0,REG2);                /* DO THE DATA MOVE FROM TPUT 0249
*                                       AREA TO OUTPUT QUEUE         */
         MVCL  REG0,REG2                                           0249
*    RFY (REG0,REG1,REG2,REG3) UNRSTD;                             0250
*    IF TCBTCT ^= 0 THEN             /* IS SMF RUNNING ?             */
         L     @04,XSATCBA-1(,RBSECPTR)                            0251
         L     @04,TCBTCT(,@04)                                    0251
         LTR   @04,@04                                             0251
         BZ    @RF00251                                            0251
*      TCTLOUT = TCTLOUT + 1;        /* YES, INCREMENT LINE CNT      */
         LA    @15,1                                               0252
         AL    @15,TCTLOUT(,@04)                                   0252
         ST    @15,TCTLOUT(,@04)                                   0252
*    IF XSABRK = '1'B THEN           /* IS THIS A BREAK-IN REQUEST ? */
@RF00251 TM    XSABRK(RBSECPTR),B'00000100'                        0253
         BNO   @RF00253                                            0253
*      TVWABIR = '1'B;               /* YES, SET INDICATOR FOR TOM   */
         OI    TVWABIR(TVWAPTR),B'00000001'                        0254
*  END;                              /* END 'SUCCESS' DO GROUP       */
@RF00253 DS    0H                                                  0256
*RFY RTNREG RSTD;                                                  0256
@RF00241 DS    0H                                                  0257
*RTNREG = XSASAVEA;                  /* RESTORE RETURN ADDRESS       */
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0257
*RETURN;                             /* RETURN TO MAINLINE IKTVTPUT  */
@EL00005 DS    0H                                                  0258
@EF00005 DS    0H                                                  0258
@ER00005 BR    @14                                                 0258
*RFY RTNREG UNRSTD;                                                0259
*END TRYTOQUE;                                                     0260
*SCHEDTOM: PROC OPTIONS(NOSAVEAREA,NOSAVE);                        0261
SCHEDTOM DS    0H                                                  0262
*/********************************************************************/
*/*                                                                  */
*/*       SCHEDTOM IS AN INTERNAL PROCEDURE THAT IS RESPONSIBLE      */
*/*       FOR SCHEDULING THE TERMINAL OUTPUT MANAGER (TOM) WHENEVER  */
*/*       NECESSARY. THIS IS NECESSARY WHEN THE OUTPUT QUEUE HAS     */
*/*       BEEN EMPTIED, & HENCE TOM HAS EXITED, OR WHEN TOM EXITED   */
*/*       BECAUSE OF BUFFER CONTENTION ON THE 3767/3770 AND THIS     */
*/*       TPUT IS A BREAK-IN REQUEST                                 */
*/*      **** SCHEDTOM SAVES RETURN ADDRESS INTO THE LEVEL 'A'       */
*/*          RETURN 'SAVE WORD' - XSASAVEA                           */
*/********************************************************************/
*/*                                                                  */
*RFY RTNREG RSTD;                                                  0262
*XSASAVEA = RTNREG;                  /* SAVE RETURN ADDRESS          */
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0263
*RFY RTNREG UNRSTD;                                                0264
*IF TVWATIS = '1'B THEN;             /* IF TOM SCHEDULED, NO-OP      */
         TM    TVWATIS(TVWAPTR),B'01000000'                        0265
         BO    @RT00265                                            0265
*ELSE                                                              0267
*  /* IF TOM IS AVAILABLE FOR SCHEDULING OR, IF NOT, IF THIS       0267
*     IS A TPUT BREAKIN AND BUFFER CONTENTION SWITCH IS ON,        0267
*     THEN SCHED. TOM                                                */
*  IF TVWATAS = '0'B × (XSABRK = '1'B & TVWABFC = '1'B) THEN       0267
         TM    TVWATAS(TVWAPTR),B'00100000'                        0267
         BZ    @RT00267                                            0267
         TM    XSABRK(RBSECPTR),B'00000100'                        0267
         BNO   @RF00267                                            0267
         TM    TVWABFC(TVWAPTR),B'10000000'                        0267
         BNO   @RF00267                                            0267
@RT00267 DS    0H                                                  0268
*    DO;                                                           0268
*      TVWATIS = '1'B;               /* SET TOM SCHED INDICATOR      */
*      TVWATAS = '0'B;               /* CLEAR INDICATOR              */
         OI    TVWATIS(TVWAPTR),B'01000000'                        0270
         NI    TVWATAS(TVWAPTR),B'11011111'                        0270
*      TVWABFC = '0'B;               /* CLEAR INDICATOR              */
         NI    TVWABFC(TVWAPTR),B'01111111'                        0271
*      RFY WORK1REG RSTD;                                          0272
*      WORK1REG = TSBXSRB;                                         0273
         L     @04,TSBEXTNT(,TSBPTR)                               0273
         L     WORK1REG,TSBXSRB(,@04)                              0273
*      /* SCHEDULE THE TERMINAL OUTPUT MANAGER                       */
*      GEN (SCHEDULE SRB=(WORK1REG),SCOPE=LOCAL)                   0274
*          REFS(TSBXSRB,CVTPTR,CVTLSMQ,CVTMAP,SRBFLNK,SRBSECT);    0274
         SCHEDULE SRB=(WORK1REG),SCOPE=LOCAL
*    END;                            /* END SCHED TOM DO GROUP       */
*RFY RTNREG RSTD;                                                  0276
@RF00267 DS    0H                                                  0276
@RT00265 DS    0H                                                  0277
*RTNREG =  XSASAVEA;                 /* RESTORE RETURN ADDRESS       */
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0277
*RETURN;                                                           0278
@EL00006 DS    0H                                                  0278
@EF00006 DS    0H                                                  0278
@ER00006 BR    @14                                                 0278
*RFY RTNREG UNRSTD;                                                0279
*END SCHEDTOM;                                                     0280
*RESWAITS: PROC OPTIONS(NOSAVEAREA,NOSAVE);                        0281
RESWAITS DS    0H                                                  0282
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*      RESET WAITING TASKS IS AN INTERNAL PROCEDURE THAT           */
*/*      CALLS STATUS TO MARK ALL TASKS AS DISPATCHABLE FOR TCBOWAIT */
*/*      FOR THIS TASK TO COMPLETE                                   */
*/*      IF'TSBWOWIP' INDICATES THAT ANY TASKS WERE SUSPENDED WAITING*/
*/*     ****RESWAITS SAVES RETURN ADDRESS INTO THE LEVEL 'A'         */
*/*         RETURN 'SAVE WORD' - XSASAVEA                            */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*RFY RTNREG RSTD;                                                  0282
*XSASAVEA = RTNREG;                  /* SAVE RETURN ADDRESS          */
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0283
*RFY RTNREG UNRSTD;                                                0284
*TSBOWIP = '0'B;                     /* CLEAR 'OWAIT IN PROGRESS' FLAG.
*                                       (TSBOWAIT HAS BEEN CLEARED WHEN
*                                       TOM/QMGR CALLED STATUS FOR 0285
*                                       RESETTING TCBOWAIT           */
         NI    TSBOWIP(TSBPTR),B'11011111'                         0285
*IF TSBWOWIP = '1'B THEN             /* ANY TASKS WAITING ON THIS  0286
*                                       TASK ?                       */
         TM    TSBWOWIP(TSBPTR),B'00010000'                        0286
         BNO   @RF00286                                            0286
*   DO;                              /* YES, CALL STATUS TO MARK TASKS
*                                       DISPATCHABLE                 */
*     TSBWOWIP = '0'B;               /* CLEAR WAIT ON OWAIT IN PROGR */
         NI    TSBWOWIP(TSBPTR),B'11101111'                        0288
*     RFY (PRMREG0,PRMREG1) RSTD;                                  0289
*     SAVEREG = NDISPBIT;            /* INDICATE THAT NON-DISPATCH-
*                                       ABILITY BIT TO RESET IS OWAIT*/
         LA    SAVEREG,1024                                        0290
*     PRMREG0  = ASCBASID*65536 × ALLTASKS; /*R0 INPUT TO STATUS. ALL
*                                             TASKS WILL BE RESET    */
         L     @04,ASCBPTR                                         0291
         LH    PRMREG0,ASCBASID(,@04)                              0291
         N     PRMREG0,@CF02477                                    0291
         SLA   PRMREG0,16                                          0291
         O     PRMREG0,@CF00041                                    0291
*     PRMREG1  = RESET;              /* INDICATE RESET VS. SET       */
         L     PRMREG1,RESET                                       0292
*     CALL STATUS;                                                 0293
         L     @04,CVTPTR                                          0293
         L     @04,SCVTSPTR(,@04)                                  0293
         L     @15,SCVTSTAT(,@04)                                  0293
         BALR  @14,@15                                             0293
*     RFY (PRMREG0,PRMREG1)   UNRSTD;                              0294
*   END;                             /* END WOWIP=1 DO GROUP         */
*RFY RTNREG RSTD;                                                  0296
@RF00286 DS    0H                                                  0297
*RTNREG = XSASAVEA;                  /* RESTORE RETURN ADDRESS       */
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0297
*RETURN;                                                           0298
@EL00007 DS    0H                                                  0298
@EF00007 DS    0H                                                  0298
@ER00007 BR    @14                                                 0298
*RFY RTNREG UNRSTD;                                                0299
*END RESWAITS;                                                     0300
*DATAEDIT:PROC OPTIONS(NOSAVEAREA,NOSAVE);                         0301
DATAEDIT DS    0H                                                  0302
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*      DATA EDIT IS AN INTERNAL PROCEDURE THAT SCANS THE           */
*/*      CALLER'S DATA AND REMOVES TRAILING BLANKS ADJUSTING         */
*/*      THE BYTE COUNT ACCORDINGLY.                                 */
*/*    **** DATAEDIT SAVES RETURN ADDRESS INTO THE LEVEL 'A'         */
*/*         RETURN 'SAVE WORD' - XSASAVEA.                           */
*/********************************************************************/
*/*                                                                  */
*DCL I REG(1);                                                     0302
*DCL BLANK CHAR(1) CONSTANT(' ');                                  0303
*RFY RTNREG RSTD;                                                  0304
*XSASAVEA = RTNREG;                                                0305
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0305
*RFY RTNREG UNRSTD;                                                0306
*IF XSAEDITO^= '00'B THEN;           /* IF NOT EDIT, NO-OP           */
         TM    XSAEDITO(RBSECPTR),B'00000011'                      0307
         BNZ   @RT00307                                            0307
*ELSE                                                              0309
*  DO;                               /* EDIT OPTION-REMOVE TRAILING
*                                       BLANKS                       */
*    XSADMOVE = '1'B;                /* SET DATA MOVE INDICATOR      */
         OI    XSADMOVE(RBSECPTR),B'01000000'                      0310
*    RFY REG2 RSTD;                                                0311
*    REG2 = RBLINKB -> RBOPSWB2;     /* OBTAIN CALLERS KEY FROM RB   */
         L     @04,RBLINKB-1(,RBSECPTR)                            0312
         SLR   REG2,REG2                                           0312
         IC    REG2,RBOPSWB2(,@04)                                 0312
*    GEN SETS(REG2);                                               0313
        MODESET KEYADDR=(2),WORKREG=2      GET IN TASKS KEY
*    RFY REG2 UNRSTD;                                              0314
*    RFY I RSTD;                                                   0315
*    DO I = XSAPRMSZ TO 2 BY -1      /* SCAN FROM END OF DATA        */
*       WHILE TPUTDATA(I) = BLANK;   /* CONTINUE WHILE BLANK         */
         LH    I,XSAPRMSZ(,RBSECPTR)                               0316
         B     @DE00316                                            0316
@DL00316 L     @04,XSABFRAD-1(,RBSECPTR)                           0316
         ALR   @04,I                                               0316
         BCTR  @04,0                                               0316
         CLI   TPUTDATA(@04),C' '                                  0316
         BNE   @DC00316                                            0316
*    END;                                                          0317
         BCTR  I,0                                                 0317
@DE00316 C     I,@CF00075                                          0317
         BNL   @DL00316                                            0317
@DC00316 DS    0H                                                  0318
*    RFY REG2 RSTD;                                                0318
*    GEN SETS(REG2)                                                0319
*        REFS(RBLINK,RBOPSW,RBBASIC,TCBRBP);                       0319
        MODESET EXTKEY=ZERO GET BACK INTO KEY 0
*    RFY REG2 UNRSTD;                                              0320
*    XSADMOVE = '0'B;                /* CLEAR DATA MOVE INDICATOR    */
         NI    XSADMOVE(RBSECPTR),B'10111111'                      0321
*    XSAPRMSZ = I;                   /* UPDATE TPUT SIZE. MIN VALUE
*                                       IS 1, EVEN IF WHOLE TPUT IS
*                                       BLANK                        */
         STH   I,XSAPRMSZ(,RBSECPTR)                               0322
*    RFY I UNRSTD;                                                 0323
*  END;                              /* END OF EDIT DO GROUP         */
*RFY RTNREG RSTD;                                                  0325
@RT00307 DS    0H                                                  0326
*RTNREG = XSASAVEA;                  /* RESTORE RETURN ADDRESS       */
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0326
*RETURN;                                                           0327
@EL00008 DS    0H                                                  0327
@EF00008 DS    0H                                                  0327
@ER00008 BR    @14                                                 0327
*RFY RTNREG UNRSTD;                                                0328
*END DATAEDIT;                                                     0329
*VERIFY: PROC OPTIONS(NOSAVEAREA,NOSAVE);                          0330
VERIFY   DS    0H                                                  0331
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*      THIS TESTS IF CALLER'S DATA ADDRESS IS IN THE CALLER'S      */
*/*      KEY. IT DOES SO BY REFERENCING THE FIRST BYTE OF EACH       */
*/*      PAGE REQUIRED FOR THE TPUT.                                 */
*/*    ***VERIFY SAVES RETURN ADDRESS INTO THE LEVEL 'B' RETURN      */
*/*       'SAVE WORD' - XSASAVEB.                                    */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*DCL APAGE FIXED(15) CONSTANT(4096); /* PAGE SIZE                    */
*DCL MODULUS FIXED(8) CONSTANT(12);  /* MODULUS OF A VS2 PAGE IS   0332
*                                       2**12=4096                   */
*RFY RTNREG RSTD;                                                  0333
*XSASAVEB = RTNREG;                  /* SAVE RETURN ADDRESS          */
         ST    RTNREG,XSASAVEB(,RBSECPTR)                          0334
*RFY RTNREG UNRSTD;                                                0335
*XSADMOVE = '1'B;                    /* INDICATE 'DATA VERIFICATION
*                                       UNDERWAY'                    */
         OI    XSADMOVE(RBSECPTR),B'01000000'                      0336
*/*    GET INTO CALLER'S KEY TO VALIDATE DATA                        */
*RFY REG2 RSTD;                                                    0337
*REG2 = RBLINKB -> RBOPSWB2;       /* OBTAIN CALLERS KEY FROM RB     */
         L     @04,RBLINKB-1(,RBSECPTR)                            0338
         SLR   REG2,REG2                                           0338
         IC    REG2,RBOPSWB2(,@04)                                 0338
*GEN SETS(REG2);                                                   0339
        MODESET KEYADDR=(2),WORKREG=2      GET IN TASKS KEY
*RFY REG2 UNRSTD;                                                  0340
*RFY WORK1REG RSTD;                                                0341
*WORK1REG = XSABFRAD;                /* GET INPUT BUFFER ADDRESS     */
         L     WORK1REG,XSABFRAD-1(,RBSECPTR)                      0342
         LA    WORK1REG,0(,WORK1REG)                               0342
*SRL(WORK1REG,MODULUS);              /* SHIFT OFF PARTIAL PAGE       */
         SRL   WORK1REG,12                                         0343
*SLL(WORK1REG,MODULUS);              /* SHIFT IN ZEROS               */
         SLL   WORK1REG,12                                         0344
*/*                                                                  */
*/*       LOOP THRU EACH PAGE, MOVING FIRST BYTE TO ITSELF           */
*/*       ABEND WILL CAUSE IKT93EST TO GAIN CONTROL AND CHANGE       */
*/*       CODE TO 15D X.                                             */
*/*                                                                  */
*RFY WORK2REG RSTD;                                                0345
*WORK2REG = XSABFRAD +XSAPRMSZ;      /* SET UP LOOP UPPER LIMIT      */
         L     WORK2REG,XSABFRAD-1(,RBSECPTR)                      0346
         LA    WORK2REG,0(,WORK2REG)                               0346
         AH    WORK2REG,XSAPRMSZ(,RBSECPTR)                        0346
*DO WHILE WORK1REG<WORK2REG;                                       0347
         B     @DE00347                                            0347
@DL00347 DS    0H                                                  0348
*   GEN REFS(WORK1REG)                                             0348
*       (CLC 0(1,WORK1REG),0(WORK1REG)); /* REFERENCE PAGE           */
         CLC 0(1,WORK1REG),0(WORK1REG)
*   WORK1REG = WORK1REG + APAGE;     /* INCREMENT BY PAGE SIZE       */
         AL    WORK1REG,@CF02469                                   0349
*END;                                                              0350
@DE00347 CR    WORK1REG,WORK2REG                                   0350
         BL    @DL00347                                            0350
*RFY (WORK1REG,WORK2REG) UNRSTD;                                   0351
*/*                                                                  */
*/*         GET BACK INTO KEY 0                                      */
*/*                                                                  */
*GEN (MODESET EXTKEY=ZERO);                                        0352
         MODESET EXTKEY=ZERO
*XSADMOVE = '0'B;                    /* CLEAR INDICATOR THAT DATA  0353
*                                       VERIFICATION WAS GOING  ON   */
         NI    XSADMOVE(RBSECPTR),B'10111111'                      0353
*RFY RTNREG RSTD;                                                  0354
*RTNREG = XSASAVEB;                                                0355
         L     RTNREG,XSASAVEB(,RBSECPTR)                          0355
*RETURN;                                                           0356
@EL00009 DS    0H                                                  0356
@EF00009 DS    0H                                                  0356
@ER00009 BR    @14                                                 0356
*RFY RTNREG UNRSTD;                                                0357
*END VERIFY;                                                       0358
