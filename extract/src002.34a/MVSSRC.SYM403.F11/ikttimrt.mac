VTAMERR  STM   @14,@12,@SA00010                                    0560
* /*******************************************************************/
* /* CHECK VTAM IMMEDIATE RETURN CODES                       @ZM20858*/
* /*******************************************************************/
* RFY (REG15,REG0) RSTD;          /* RESTRICT REGS           @ZM20858*/
* IF REG15=32 ×                   /* INVALID RPL OR          @ZM20858*/
*   (REG15=4 & REG0=24) THEN      /* VTAM NOT ACTIVE RETRY   @ZM20858*/
         C     REG15,@CF00037                                      0562
         BE    @RT00562                                            0562
         C     REG15,@CF00124                                      0562
         BNE   @RF00562                                            0562
         C     REG0,@CF00320                                       0562
         BNE   @RF00562                                            0562
@RT00562 DS    0H                                                  0563
*                                 /* THREE TIMES             @ZM20858*/
*   IF RECERCNT>=3 THEN           /* RETRY EXCEEDED          @ZM20858*/
         L     @14,TIMGETMN                                        0563
         L     @14,RECERCNT(,@14)                                  0563
         C     @14,@CF00080                                        0563
         BL    @RF00563                                            0563
*     DO;                         /* SET UP ABEND            @ZM20858*/
*       CALL EXITFRR;             /* CALL ABEND ROUTINE      @ZM20856*/
         BAL   @14,EXITFRR                                         0565
*     END;                        /* END RETRY EXCEEDED      @ZM20858*/
*   ELSE                          /* RETRY NOT EXCEEDED      @ZM20858*/
*     DO;                         /* REFRESH, RETRY          @ZM20858*/
         B     @RC00563                                            0567
@RF00563 DS    0H                                                  0568
*      RECERCNT=RECERCNT+1;       /* INCREMENT RETRY COUNT   @ZM20858*/
         L     @14,TIMGETMN                                        0568
         LA    @11,1                                               0568
         AL    @11,RECERCNT(,@14)                                  0568
         ST    @11,RECERCNT(,@14)                                  0568
*      REGRTN='1'B;               /* REFRESH RPL             @ZM20858*/
         OI    REGRTN(@14),B'00001000'                             0569
*     END;                        /* END RETRY NOT EXCEEDED  @ZM20858*/
* ELSE                            /* IMMEDIATE RETURN CODE OK@ZM20858*/
*     DO;     /****** CONTINUE WITH NORMAL CHECKING ******** @ZM20858*/
         B     @RC00562                                            0571
@RF00562 DS    0H                                                  0572
*  /* CHECK FIRST FOR LOSTERM BEING ENTERED                          */
*  IF (RPLRTNCD='0C'X & (RPLFDB2=2 ×    /*REQUEST RESET      @ZM20858*/
