         CLC   RPLSSNSI(2,RPLREG),@CB02584                         0593
         BE    @RT00593                                            0593
         CLC   RPLSSNSI(2,RPLREG),@CB02585                         0593
         BNE   @RF00593                                            0593
@RT00593 DS    0H                                                  0594
*               IF TVWATOBB='0'B THEN  /* BEG BRACKET ^SENT  @ZM20858*/
         TM    TVWATOBB(TVWAPTR),B'00000100'                       0594
         BNZ   @RF00594                                            0594
