         L     REG11,PSAAOLD                                       0494
         LA    REG11,ASCBECB(,REG11)                               0494
*        CALL POST;                    /* BALR 14,15 TO POST RCT     */
         L     @08,CVTPTR                                          0495
         L     @15,CVT0PT01(,@08)                                  0495
         BALR  @14,@15                                             0495
*        RPLREG=RPLSAVE;           /* RESTORE RPL ADDR TO REG@ZA24528*/
         L     RPLREG,RPLSAVE                                      0496
*        RFY REG11 UNRSTD;             /* FREE REGS          @ZA24528*/
