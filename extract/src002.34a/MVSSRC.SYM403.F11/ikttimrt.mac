*           DO;                        /* RECOVERABLE ERROR COND     */
         B     @RC00585                                            0590
@RF00585 DS    0H                                                  0591
*            IF RPLFDB2=USFEXRS THEN   /* CHECK FOR EXCEPT RESPONSE  */
         CLI   RPLFDB2(RPLREG),4                                   0591
         BNE   @RF00591                                            0591
