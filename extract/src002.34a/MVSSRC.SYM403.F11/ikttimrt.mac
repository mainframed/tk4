*                                       /*                   @ZM20858*/
*                        RPLFDB2=11)) × /*CLSDST OCCURED     @ZM20858*/
*     (RPLRTNCD='10'X & (RPLFDB2=3 ×    /*VTAM HALTING       @ZM20858*/
*                        RPLFDB2=5 ×    /*PERMANENT PATH ERR @ZM20858*/
*                        RPLFDB2=6 ×    /*AUTO NET SHUTDOWN  @ZM20858*/
*                        RPLFDB2=7 ×    /*VARY DEACT IMMED.  @ZM20858*/
*                        RPLFDB2=9 ×    /*TERMINATE SELF     @ZM20858*/
*                        RPLFDB2=15)) × /*VTAM BUFFER OVERFLW@ZM20858*/
*     (RPLRTNCD='14'X & RPLFDB2=USFCLSIP) × TVWALTE='1'B THEN      0572
         CLI   RPLRTNCD(RPLREG),12                                 0572
         BNE   @GL00011                                            0572
         CLI   RPLFDB2(RPLREG),2                                   0572
         BE    @RT00572                                            0572
         CLI   RPLFDB2(RPLREG),7                                   0572
         BE    @RT00572                                            0572
         CLI   RPLFDB2(RPLREG),8                                   0572
         BE    @RT00572                                            0572
         CLI   RPLFDB2(RPLREG),9                                   0572
         BE    @RT00572                                            0572
         CLI   RPLFDB2(RPLREG),11                                  0572
         BE    @RT00572                                            0572
@GL00011 CLI   RPLRTNCD(RPLREG),16                                 0572
         BNE   @GL00010                                            0572
         CLI   RPLFDB2(RPLREG),3                                   0572
         BE    @RT00572                                            0572
         CLI   RPLFDB2(RPLREG),5                                   0572
         BE    @RT00572                                            0572
         CLI   RPLFDB2(RPLREG),6                                   0572
         BE    @RT00572                                            0572
         CLI   RPLFDB2(RPLREG),7                                   0572
         BE    @RT00572                                            0572
         CLI   RPLFDB2(RPLREG),9                                   0572
         BE    @RT00572                                            0572
         CLI   RPLFDB2(RPLREG),15                                  0572
         BE    @RT00572                                            0572
@GL00010 CLI   RPLRTNCD(RPLREG),20                                 0572
         BNE   @GL00009                                            0572
         CLI   RPLFDB2(RPLREG),18                                  0572
         BE    @RT00572                                            0572
@GL00009 TM    TVWALTE(TVWAPTR),B'00000010'                        0572
         BNO   @RF00572                                            0572
@RT00572 DS    0H                                                  0573
*    CALL FINPROC;                                                 0573
         BAL   @14,FINPROC                                         0573
*  ELSE                                                            0574
*    IF TVWAGERR='1'B THEN             /* CHECK FOR GLOBAL ERROR COND*/
         B     @RC00572                                            0574
@RF00572 TM    TVWAGERR(TVWAPTR),B'00010000'                       0574
         BNO   @RF00574                                            0574
*      REGRTN='0'B;                    /* RETURN FOR NORMAL RECEIVE  */
         L     @14,TIMGETMN                                        0575
         NI    REGRTN(@14),B'11110111'                             0575
*    ELSE                                                          0576
