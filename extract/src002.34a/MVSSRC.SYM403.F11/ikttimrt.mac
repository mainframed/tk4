         LR    REG7,REG13                                          0508
*            REG0=8;                           /* ASID      @YM03241 */
         LA    REG0,8                                              0509
*            REG1= '80000000'X;          /* INDICATES RESET          */
         L     REG1,@CF02563                                       0510
*            REG13= '0400'X;             /* TCBOWAIT IS BIT MANIP    */
         LA    REG13,1024                                          0511
*            CALL STATUS;                                          0512
         L     @11,CVTPTR                                          0512
         L     @11,SCVTSPTR(,@11)                                  0512
         L     @15,SCVTSTAT(,@11)                                  0512
         BALR  @14,@15                                             0512
*            REG13=REG7;                /* RESTORE SAVE REGISTER     */
         LR    REG13,REG7                                          0513
*            RFY (REG0,REG1,REG7) UNRSTD; /* FREE  WORK REGS         */
*          END;                                                    0515
*        ELSE;                                                     0516
@RF00503 DS    0H                                                  0517
*        TVWAAIGN='1'B;                /* ATTN IGNORED FLAG FOR TOM  */
         OI    TVWAAIGN(TVWAPTR),B'00100000'                       0517
*      END;                            /* END OF NO-STAX EXIT CODE   */
*    IPREQ=CLEARI;                     /* CLEAR THE INPUT QUEUE      */
@RC00487 L     @11,TIMGETMN                                        0519
         MVI   IPREQ(@11),X'05'                                    0519
*    CALL IKTQMIN(IPARMS);             /* CALL Q MANAGER             */
         LA    @11,IPARMS(,@11)                                    0520
         ST    @11,@AL00001                                        0520
