         NI    TSBIFLSH(TSBREG),B'11110111'                        0486
*    IF TSBSTAX > TSBATTNC THEN        /* CHECK FOR STAX EXIT        */
         CLC   TSBSTAX(1,TSBREG),TSBATTNC(TSBREG)                  0487
         BNH   @RF00487                                            0487
*      DO;                             /* ROUTINE FOR STAX EXIT      */
*                                                                  0488
*        /************************************************************/
*        /*STAX EXIT ROUTINE INCREMENTS TSBATTNC,TURNS OFF TSBIWAIT  */
