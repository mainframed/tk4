         SLR   @08,@08                                             0500
         IC    @08,TVWAATTN(,TVWAPTR)                              0500
         ALR   @11,@08                                             0500
         STC   @11,TVWAATTN(,TVWAPTR)                              0500
*      END;                            /* END STAX EXIT CODE         */
*    ELSE                                                          0502
*      DO;                             /* CODE FOR NO STAX EXIT      */
*                                                                  0502
         B     @RC00487                                            0502
@RF00487 DS    0H                                                  0503
*        /************************************************************/
*        /* CODE WILL BE EXECUTED WHEN NO STAX EXIT IS AVAILABLE     */
*        /* IF TSBOWAIT IS ON, STATUS IS CALLED                      */
*        /************************************************************/
*                                                                  0503
*        IF TSBOWAIT='1'B THEN         /* CHECK TSBOWAIT ON          */
         TM    TSBOWAIT(TSBREG),B'00100000'                        0503
         BNO   @RF00503                                            0503
*          DO;                                                     0504
*            TSBATTN='1'B;             /* ATTNTION RECEIVED          */
         OI    TSBATTN(TSBREG),B'10000000'                         0505
*            TSBOWAIT='0'B;            /* TURN OFF TSBOWAIT          */
         NI    TSBOWAIT(TSBREG),B'11011111'                        0506
*            RFY (REG0,REG1,REG7) RSTD;  /* CT REGS FOR STATUS       */
