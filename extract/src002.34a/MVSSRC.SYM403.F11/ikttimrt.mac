         TITLE '*** IKTTIMRT: INPUT MANAGER FOR THE 3767/3770 TERMINALS*
               S ***    '
IKTTIMRT CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKTTIMRT  77.347'                                 0001
         DROP  @15
@PROLOG  BALR  @05,0                                               0001
@PSTART  LA    @06,4095(,@05)                                      0001
         USING @PSTART,@05                                         0001
         USING @PSTART+4095,@06                                    0001
         LR    WRKSPC,REG1         LOAD ADDR OF GETMAINED AREA
         USING @DATD,WRKSPC        SET UP ADDRESSABILITY
         A     REG1,C1000          BEGINNING PRIVATE WORK AREA
         ST    REG1,TIMGETMN       SET UP ADDRESSABILITY
*   TSBREG=ASCBTSB;                 /* SET UP TSB POINTER REG@ZA24528*/
         L     @15,PSAAOLD                                         0170
         L     TSBREG,ASCBTSB(,@15)                                0170
*   RFY TSB BASED(TSBREG);          /* REBASE TSB ON REGISTER@ZA24528*/
*   TVWAPTR=TSBXTVWA;               /* SET UP TVWA POINTER   @ZA24528*/
         L     @15,TSBEXTNT(,TSBREG)                               0172
         L     TVWAPTR,TSBXTVWA(,@15)                              0172
*   RPLREG=TIMGETMN+LENGTH(TIMWORK); /* SET UP RPL POINTER   @ZA24528*/
         L     @15,TIMGETMN                                        0173
         LA    RPLREG,632                                          0173
         ALR   RPLREG,@15                                          0173
*   RFY IFGRPL BASED(RPLREG);       /* BASE THE RPL ON REG.  @ZA24528*/
*   RPLSAVE=RPLREG;                 /* SAVE THE RPL ADDRESS  @ZA24528*/
         ST    RPLREG,RPLSAVE                                      0175
*   RFY WSAG   BASED(CVTSPSA);      /* USED IN GLOBAL FREEMAIN       */
*   STM(REG0,REG15,SAVEREGS);       /* RECOVERY FOR FREEMAIN         */
         STM   REG0,REG15,SAVEREGS(@15)                            0177
*   SRBRTN=REG14;                   /* SAVE ADDRESS OF RETURN POINT  */
         ST    REG14,SRBRTN(,@15)                                  0178
*   RFY REG14 UNRSTD;               /* FREE REGISTER 14 FOR USE      */
*    DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/     0180
*CKMC0002:                                                         0181
*      STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */ 0181
CKMC0002 L     @11,TVWAFRWI(,TVWAPTR)                              0181
         STM   REG0,REG15,FRWACKRG(@11)                            0181
*      FRWACKPT(2)=ADDR(CKMC0002); /* SAVE ADDRESS    @ZM20856 */  0182
         LA    @08,CKMC0002                                        0182
         ST    @08,FRWACKPT+4(,@11)                                0182
*    END;                                                          0183
*/********************************************************************/
*/*     THE FOLLOWING CODE WILL BE EXECUTED UPON INITIAL ENTRY TO    */
*/*     IKTTIMRT.  THE SRB WILL BE FREED AND THE RPL FOR THE VTAM    */
*/*     RECEIVE MACRO WILL BE INITIALIZED.                           */
*/********************************************************************/
* REG7=ADDR(IKTIOFRR);              /* SET UP FRR FOR RECOVERY       */
         L     @11,CVTPTR                                          0184
         L     @11,CVTTCASP(,@11)                                  0184
         L     REG7,TCASFRR(,@11)                                  0184
* GEN  REFS(FRRSCURR,FRRS,FRRSLAST,PSALSFCC,PSA,FRRSELEN,PSACSTK,  0185
*           FRRSFRRA,FRRSENTR,FRRSPARM,REG7)                       0185
*           SETS(REG1,REG2,REG3,FRRSCURR);                         0185
   SETFRR A,FRRAD=(REG7),WRKREGS=(REG1,REG2),PARMAD=(REG3)
* PARMPTR=(REG3);                      /* ADDRESS OF PARM AREA       */
         ST    REG3,PARMPTR                                        0186
* RFY (REG1,REG3,REG7) UNRSTD;         /* FREE REGS FOR COMPILER     */
* CALL INITFRR;                        /* CALL FRR INIT      @ZM20856*/
         BAL   @14,INITFRR                                         0188
* /*******************************************************************/
* /* FIRST OBTAIN THE LOCAL LOCK TO DELETE THE SRB POINTER FROM THE  */
* /* TSBX. THEN OBTAIN SALLOC LOCK AND DO FREEMAIN.                  */
* /*******************************************************************/
* REG2=REG0;                           /* SAVE SRB ADDRESS TO FREE  */
         LR    REG2,REG0                                           0189
* GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(SRB),REGS=USE)
*     REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);               0190
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(SRB),REGS=USE
*   TSBXSRBI=0;                        /* CLEAR SRB ADDR IN TSBX    */
         L     @01,TSBEXTNT(,TSBREG)                               0191
         SLR   @11,@11                                             0191
         ST    @11,TSBXSRBI(,@01)                                  0191
* /***FREE SRB********************************************************/
* STM(REG0,REG15,SAVEREGS);            /* SAVE REGS FOR RESTART      */
         L     @11,TIMGETMN                                        0192
         STM   REG0,REG15,SAVEREGS(@11)                            0192
* RFY (REG8,REG7) RSTD;                /* REG8 WILL HOLD R13  POINTER*/
* REG8=REG13;                          /* ADDRESS OF TIM WORK AREA   */
         LR    REG8,REG13                                          0194
* REG7=SRBLEN;                         /* LENGTH OF SRB TO FREE      */
         L     REG7,SRBLEN                                         0195
* GEN    REFS(REG2,REG7,CVTSPSA,CVTMAP,WSAGGMFM,WSAG,CVTGLMN,      0196
*           PSALITA,FLC) SETS(REG0,REG1,REG3,REG4,REG14,REG15);    0196
   SETLOCK  OBTAIN,TYPE=SALLOC,MODE=UNCOND,RELATED=(AL),REGS=USE
   FREEMAIN RC,LV=(REG7),A=(REG2),SP=239,BRANCH=(YES,GLOBAL)
   SETLOCK  RELEASE,TYPE=SALLOC,RELATED=(AL),REGS=USE
* REG13=REG8;                          /* RESTORE ADDR OF TIM WORK   */
         LR    REG13,REG8                                          0197
* LM(REG0,REG15,SAVEREGS);             /* RESTORE DESTROYED REGISTERS*/
         L     @11,TIMGETMN                                        0198
         LM    REG0,REG15,SAVEREGS(@11)                            0198
*RESTARTA: CALL PRIMPROC;              /* CALL RTN TO SET UP RPL     */
RESTARTA BAL   @14,PRIMPROC                                        0199
*RFY (REG0,REG1,REG2,REG8,REG7) UNRSTD; /* FREE REGISTERS            */
         EJECT
*/********************************************************************/
*/*     THE FOLLOWING CODE CONTAINS THE INFINITE LOOP THAT PERFORMS  */
*/*     THE BASIC FUNCTION OF IKTTIMRT. THAT FUNCTION IS TO ISSUE    */
*/*     THE VTAM RECEIVE MACRO, DETERMINE WHAT THE INPUT IS WHEN     */
*/*     THE RECEIVE IS SATISFIED, CALL THE APPROPPRIATE ROUTINE      */
*/*     TO PROCESS THE INPUT AND LOOP BACK TO REISSUE THE RECEIVE.   */
*/*     EACH TIME VTAM GAINS CONTROL THROUGH THE RECEIVE MACRO       */
*/*     THE FRR THAT IKTTIMRT SETS UP IS DELETED AND MUST BE RESET.  */
*/********************************************************************/
*REC: RFY (REG0,REG1,REG2,REG3,REG7,REG15) RSTD; /*REGS FOR GEN      */
REC      DS    0H                                                  0202
*                                     /*                     @ZM20858*/
* DO;    /* CHECKPOINT REGISTERS AND THE ADDRESS @ZM20856*/        0202
*CKMC0003:                                                         0203
*   STM(REG0,REG15,FRWACKRG); /* SAVE ALL REGISTERS @ZM20856 */    0203
CKMC0003 L     @14,TVWAFRWI(,TVWAPTR)                              0203
         STM   REG0,REG15,FRWACKRG(@14)                            0203
*   FRWACKPT(2)=ADDR(CKMC0003); /* SAVE ADDRESS    @ZM20856 */     0204
         LA    @11,CKMC0003                                        0204
         ST    @11,FRWACKPT+4(,@14)                                0204
* END;                 /* CHECK POINT         @ZM20856*/           0205
*RPLSRTYP='02'X;                      /*  REC SYN AND ASYCHR DATA    */
         LA    @14,16                                              0206
         ALR   @14,RPLREG                                          0206
         MVI   RPLSRTYP(@14),X'02'                                 0206
*IF TVWARCRS='1'B THEN                /* IF AUTO PROMPT MODE @ZA24528*/
         TM    TVWARCRS(TVWAPTR),B'00000010'                       0207
         BNO   @RF00207                                            0207
* RPLRRESP='1'B;               /* ACCEPT EXCEPTION RESPONSES @ZA24528*/
         OI    RPLRRESP(@14),B'00001000'                           0208
*ELSE;                                                   /*  @ZA24528*/
@RF00207 DS    0H                                                  0210
*RPLCNTRL='000000'X;         /* TURN OFF ALL BITS IN RPLCNTRL@ZA24528*/
         MVC   RPLCNTRL+16(3,RPLREG),@CB02532                      0210
*REG2=RPLREG;             /* ADDR OF RPL FOR RECEIVE FROM REG@ZA24528*/
         LR    REG2,RPLREG                                         0211
*REG7=ADDR(IKTIOFRR);                  /* PTR TO FRR ENTRY           */
         L     @14,CVTPTR                                          0212
         L     @14,CVTTCASP(,@14)                                  0212
         L     REG7,TCASFRR(,@14)                                  0212
*GEN(SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SRB),REGS=USE)    /*@ZA24528*/
*    REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);        /*@ZA24528*/
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=(SRB),REGS=USE
*GEN REFS(REG1,REG2,REG3,REG7,FRRSCURR,FRRS,FRRSLAST,PSALSFCC,PSA, 0214
*         FRRSELEN,PSACSTK,FRRSFRRA,FRRSENTR,FRRSPARM)             0214
*         SETS(REG1,REG2,REG3,REG15,REG0,REG14);                   0214
  RECEIVE  RPL=(REG2)
  SETFRR A,FRRAD=(REG7),WRKREGS=(REG1,REG2),PARMAD=(REG3)
  STM REG14,REG1,12(REG13)  /*SAVE RTN CODES FROM RECEIVE   @ZM20858*/
*GEN(SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(SRB),REGS=USE) 0215
*    REFS(PSALITA,FLC) SETS(REG0,REG1,REG14,REG15);        /*@ZA24528*/
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,RELATED=(SRB),REGS=USE
*GEN(LM REG14,REG1,12(REG13))                                      0216
*   REFS(REG13);                       /* RESTORE RETURN CODE REGS   */
         LM REG14,REG1,12(REG13)
*                                      /* FROM THE RECEIVER  @ZM20858*/
*PARMPTR=(REG3);                                                   0217
         ST    REG3,PARMPTR                                        0217
*RFY (REG1,REG2,REG3,REG7) UNRSTD;     /* FREE REGISTERS FOR USE     */
*CALL INITFRR;                         /* CALL FRR INIT      @ZM20856*/
         BAL   @14,INITFRR                                         0219
*IF TVWATOD='1'B THEN                  /* IS IKTTOMRT DEAD?  @ZM20856*/
         TM    TVWATOD(TVWAPTR),B'10000000'                        0220
         BNO   @RF00220                                            0220
*  CALL FINPROC;                       /* EXIT PERMANENTLY   @ZM20586*/
         BAL   @14,FINPROC                                         0221
*ELSE                                  /* IKTTOMRT IS ALIVE  @ZM20586*/
*IF REG15 > 0 THEN                                                 0222
         B     @RC00220                                            0222
@RF00220 LTR   REG15,REG15                                         0222
         BNP   @RF00222                                            0222
*  DO;                                  /* NOT 0, ERROR PROCESS      */
*    CALL VTAMERR;                      /* CALL ERROR PROCESS RTN    */
         BAL   @14,VTAMERR                                         0224
*    IF REGRTN='1'B THEN                /* REFRESH RPL NEEDED ?      */
         L     @14,TIMGETMN                                        0225
         TM    REGRTN(@14),B'00001000'                             0225
         BNO   @RF00225                                            0225
*      DO;                                                         0226
*        LM(REG0,REG15,SAVEREGS);       /* RESTORE REGISTERS         */
         L     @14,TIMGETMN                                        0227
         LM    REG0,REG15,SAVEREGS(@14)                            0227
*        CALL PRIMPROC;                 /* REFRESH RPL               */
         BAL   @14,PRIMPROC                                        0228
*        REGRTN='0'B;                   /* RESET FLAG                */
         L     @14,TIMGETMN                                        0229
         NI    REGRTN(@14),B'11110111'                             0229
*      END;                             /* END REFRESH RPL DO PROC   */
*    ELSE;                              /* NO REFRESH RPL, NO PROC   */
@RF00225 DS    0H                                                  0232
*  END;                                 /* END REG15 > 0             */
*ELSE                                   /* GOOD RECEIVE, PROCESS INPT*/
*DO;                                                               0233
         B     @RC00222                                            0233
@RF00222 DS    0H                                                  0234
*RFY (REG0,REG15) UNRSTD;              /* FREE RTN CODE REGS @ZM20858*/
*RECERCNT=0;                            /* RESET INPUT ERROR COUNT   */
         L     @11,TIMGETMN                                        0235
         SLR   @08,@08                                             0235
         ST    @08,RECERCNT(,@11)                                  0235
*IF RPLDATA='1'B THEN                  /* CHECK FOR DATA RECEIVED    */
         TM    RPLDATA+16(RPLREG),B'10000000'                      0236
         BNO   @RF00236                                            0236
*  CALL DATARTN;                       /* YES, PROCESS DATA          */
         BAL   @14,DATARTN                                         0237
*ELSE                                                              0238
*  IF RPLCNCEL='1'B THEN               /* CHECK FOR CANCEL RECEIVED  */
         B     @RC00236                                            0238
@RF00236 TM    RPLCNCEL+16(RPLREG),B'01000000'                     0238
         BNO   @RF00238                                            0238
*    CALL CANCELR;                     /* YES, PROCESS CANCEL        */
         BAL   @14,CANCELR                                         0239
*  ELSE                                                            0240
*     IF RPLLUS='1'B THEN              /*  CHECK FOR LUSTAT RECEIVED */
         B     @RC00238                                            0240
@RF00238 TM    RPLLUS+16(RPLREG),B'00100000'                       0240
         BNO   @RF00240                                            0240
*       CALL LUSTAT;                   /* YES, PROCESS LUSTAT        */
         BAL   @14,LUSTAT                                          0241
*     ELSE                                                         0242
*       IF RPLSIGNL='1'B THEN          /* CHECK FOR ATTENTION SIGNAL */
         B     @RC00240                                            0242
@RF00240 TM    RPLSIGNL+16(RPLREG),B'00010000'                     0242
         BNO   @RF00242                                            0242
*         DO;                          /* YES, ATTEN SIGNAL  @ZA26553*/
*           RPLFMHDR='0'B;             /* RESET FORMAT HEADER@ZA26553*/
         NI    RPLFMHDR(RPLREG),B'11111110'                        0244
*           IF TVWAOOPS='1'B THEN                                  0245
         TM    TVWAOOPS(TVWAPTR),B'00000001'                       0245
         BNO   @RF00245                                            0245
*             DO;                                                  0246
*               TVWAOOPS='0'B;         /* SET PAPER OUT SWITCH OFF   */
         NI    TVWAOOPS(TVWAPTR),B'11111110'                       0247
*               CALL SCHTOM;           /* RESCHEDULE OUTPUT MGR      */
         BAL   @14,SCHTOM                                          0248
*             END;                                                 0249
*           ELSE                       /* ATTN WITH NO OUT OF PAPER  */
*             DO;                      /* CHECK FOR USER EXIT        */
         B     @RC00245                                            0250
@RF00245 DS    0H                                                  0251
*               IF ADDR(IKTRTX3)^= 0 THEN                          0251
         L     @11,@CV02377                                        0251
         LTR   @11,@11                                             0251
         BZ    @RF00251                                            0251
*                 CALL IKTRTX3;        /* CALL USER EXIT IF AVAIL    */
         L     @15,@CV02377                                        0252
         BALR  @14,@15                                             0252
*               ELSE                                               0253
*                 CALL ATTN;           /* NO USER, USE TSO ATTN RTN  */
         B     @RC00251                                            0253
@RF00251 BAL   @14,ATTN                                            0253
*             END;                     /* END OF USER EXIT PROCESS   */
*         END;                         /*                    @ZA26553*/
*       ELSE;                          /* END OF POSSIBLE INPUTS     */
@RF00242 DS    0H                                                  0257
* END;                                 /* END OF GOOD RECEIVE PROCESS*/
* GO TO REC;                           /* LOOP BACK TO ISSUE RECEIVE */
         B     REC                                                 0258
         EJECT
*/********************************************************************/
*/*     THE FOLLOWING CODE BUILDS AND INITIALIZES THE RPL FOR THE    */
*/*     VTAM RECEIVE MACRO AND WILL BE THE PRIMARY RESTARTING POINT  */
*/*     FOR IKTTIMRT IN CASE OF FAILURE DUE TO AN ERROR CONDITON.    */
*/********************************************************************/
*PRIMPROC:                                                         0259
*  PROCEDURE;                                                      0259
PRIMPROC STM   @14,@12,12(@13)                                     0259
*  RFY ISTNIB  BASED(TVWANIB);         /* BASE THE NIB DSECT         */
*  IFGRPL=DUMMYRPL;                    /* INITIALIZE RPL             */
         MVC   IFGRPL(112,RPLREG),DUMMYRPL                         0261
*  RPLDACB=TVWAACB;                    /* PLACE ACB ADDR IN RPL      */
         L     @11,TVWAACB(,TVWAPTR)                               0262
         ST    @11,RPLDACB(,RPLREG)                                0262
*  RPLARG=NIBCID;                      /* PLACE CID IN RPL           */
         L     @11,TVWANIB(,TVWAPTR)                               0263
         L     @11,NIBCID(,@11)                                    0263
         ST    @11,RPLARG(,RPLREG)                                 0263
*  RPLAREA=ADDR(TIMBUFF);              /* ADDRESS OF INPUT BUFFER    */
         L     @11,TIMGETMN                                        0264
         LA    @08,TIMBUFF(,@11)                                   0264
         ST    @08,RPLAREA(,RPLREG)                                0264
*  IPTRMTYP=TSBXTMTP;                  /* INIT TERMINAL TYPE         */
         L     @08,TSBEXTNT(,TSBREG)                               0265
         IC    @07,TSBXTMTP(,@08)                                  0265
         STC   @07,IPTRMTYP(,@11)                                  0265
*RETURN;                                                           0266
@EL00002 DS    0H                                                  0266
@EF00002 DS    0H                                                  0266
@ER00002 LM    @14,@12,12(@13)                                     0266
         BR    @14                                                 0266
*END PRIMPROC;                                                     0267
         EJECT
*/********************************************************************/
*/*    THIS ROUTINE WILL CHECK TO SEE IF IKTTOMRT IS AVAILABLE TO    */
*/*    BE SCHEDULED.  THE TVWATIS FLAG IS 1 WHEN IKTTOMRT IS EITHER  */
*/*    RUNNINT OR SCHEDULED.  TVWATAS MEANS THAT ONLY IKTTIMRT CAN   */
*/*    SCHEDULE IKTTOMRT.                                            */
*/********************************************************************/
*                                                                  0268
*SCHTOM: PROCEDURE;                                                0268
SCHTOM   STM   @14,@12,@SA00003                                    0268
*      TVWATAS='0'B;                   /* MARK TOM AVAILABLE @ZA26553*/
         NI    TVWATAS(TVWAPTR),B'11011111'                        0269
*      IF TVWATIS='0'B THEN            /* IKTTOMRT NOT SCHEDULED     */
         TM    TVWATIS(TVWAPTR),B'01000000'                        0270
         BNZ   @RF00270                                            0270
*        DO;                                                       0271
*          RFY REG2 RSTD;              /* FOR USE IN SCHEDULE        */
*          REG2=TSBXSRB;               /* ADDRESS OF TOM SRB         */
         L     @11,TSBEXTNT(,TSBREG)                               0273
         L     REG2,TSBXSRB(,@11)                                  0273
*          GEN(SCHEDULE SRB=(REG2),SCOPE=LOCAL)                    0274
*          REFS(TSBXSRB,CVTPTR,CVTLSMQ,CVTMAP,SRBFLNK,SRBSECT);    0274
         SCHEDULE SRB=(REG2),SCOPE=LOCAL
*          RFY REG2 UNRSTD;             /* FREE REGISTER2            */
*          TVWABFC='0'B;               /* SET BUFFER CONTENTION OFF  */
         NI    TVWABFC(TVWAPTR),B'01111111'                        0276
*          TVWATIS='1'B;               /* SET TOM SCHEDULED ON       */
         OI    TVWATIS(TVWAPTR),B'01000000'                        0277
*        END;                                                      0278
*      ELSE;                                                       0279
@RF00270 DS    0H                                                  0280
*  END SCHTOM;                                                     0280
@EL00003 DS    0H                                                  0280
@EF00003 DS    0H                                                  0280
@ER00003 LM    @14,@12,@SA00003                                    0280
         BR    @14                                                 0280
         EJECT
*/********************************************************************/
*/*    THIS ROUTINE WILL RECEIVE CONTROL WHEN THE RECEIVE MACRO      */
*/*    IS SATISFIED WITH DATA. THE FIRST CHECK MADE IS AGAINST A     */
*/*    GLOBAL ERROR FLAG THAT IS TURNED ON WHEN ERROR RECOVERY IS    */
*/*    IN PROGRESS.                                                  */
*/********************************************************************/
*                                                                  0281
*DATARTN:                                                          0281
*  PROCEDURE;                                                      0281
DATARTN  STM   @14,@12,@SA00004                                    0281
*    IF TVWAGERR='0'B THEN                                         0282
         TM    TVWAGERR(TVWAPTR),B'00010000'                       0282
         BNZ   @RF00282                                            0282
*      DO;                                                         0283
*        IF RPLCMD='1'B THEN           /* CHANGE DIRECTION SPECIFIED */
         TM    RPLCMD+16(RPLREG),B'00100000'                       0284
         BNO   @RF00284                                            0284
*          DO;                                                     0285
*            TIMSCHTM='1'B;            /* IKTTOMRT NEEDS SCHEDULING  */
         L     @11,TIMGETMN                                        0286
         OI    TIMSCHTM(@11),B'01000000'                           0286
*            TVWABRIN='0'B;            /* BREAKIN COMPLETE           */
         NI    TVWABRIN(TVWAPTR),B'01111111'                       0287
*            TVWASCD='1'B;             /* CD RECIEVED, SEND DATA     */
         OI    TVWASCD(TVWAPTR),B'00100000'                        0288
*            IF RPLRLEN>0  &  TSBIFLSH='0'B THEN /* NO ADD NULL RU   */
         L     @11,RPLRLEN(,RPLREG)                                0289
         LTR   @11,@11                                             0289
         BNP   @RF00289                                            0289
         TM    TSBIFLSH(TSBREG),B'00001000'                        0289
         BNZ   @RF00289                                            0289
*               CALL DATA2;            /* CALL PROCESS ROUTINE       */
         BAL   @14,DATA2                                           0290
*            ELSE;                                                 0291
@RF00289 DS    0H                                                  0292
*          END;                                                    0292
*        ELSE                                                      0293
*          DO;                         /* PROCESS RECIEVED DATA      */
         B     @RC00284                                            0293
@RF00284 DS    0H                                                  0294
*            IF TSBIFLSH='0'B THEN     /* CHECK FOR FLUSH INPUT FLG  */
         TM    TSBIFLSH(TSBREG),B'00001000'                        0294
         BNZ   @RF00294                                            0294
*              CALL DATA2;             /* NO, PROCESS INPUT          */
         BAL   @14,DATA2                                           0295
*            ELSE                                                  0296
*              IF RPLLAST='1'B × RPLONLY='1'B THEN                 0296
         B     @RC00294                                            0296
@RF00294 TM    RPLLAST+16(RPLREG),B'00110000'                      0296
         BZ    @RF00296                                            0296
*                 IF TVWATAS='1'B THEN                             0297
         TM    TVWATAS(TVWAPTR),B'00100000'                        0297
         BNO   @RF00297                                            0297
*                    TIMSCHTM='1'B;     /* SCHEDULE TOM FLAG         */
         L     @11,TIMGETMN                                        0298
         OI    TIMSCHTM(@11),B'01000000'                           0298
*                 ELSE;                                            0299
@RF00297 DS    0H                                                  0300
*              ELSE;                                               0300
@RF00296 DS    0H                                                  0301
*          END;                                                    0301
@RC00294 DS    0H                                                  0302
*        IF TIMSCHTM='1'B THEN                                     0302
@RC00284 L     @11,TIMGETMN                                        0302
         TM    TIMSCHTM(@11),B'01000000'                           0302
         BNO   @RF00302                                            0302
*          DO;                                                     0303
*            CALL SCHTOM;              /* SCHEDULE IKTTOMRT          */
         BAL   @14,SCHTOM                                          0304
*            TIMSCHTM='0'B;            /* SET NEED TO SCHED FLG OFF  */
         L     @11,TIMGETMN                                        0305
         NI    TIMSCHTM(@11),B'10111111'                           0305
*          END;                                                    0306
*        ELSE;                                                     0307
@RF00302 DS    0H                                                  0308
*      END;                                                        0308
*    ELSE;                                                         0309
@RF00282 DS    0H                                                  0310
*    END DATARTN;                                                  0310
@EL00004 DS    0H                                                  0310
@EF00004 DS    0H                                                  0310
@ER00004 LM    @14,@12,@SA00004                                    0310
         BR    @14                                                 0310
         EJECT
*/********************************************************************/
*/*    DATA2 PROCEDURE WILL CONVERT THE INPUT DATA TO EBCDIC IF      */
*/*    CONVERSION IS NECESSARY, GIVE CONTROL TO A USER TRANSLATE     */
*/*    ROUTINE IF AVAILABLE, AND CALL ROUTINES TO COMPUTE LINE LENGTH*/
*/*    AND ADD THE DATA TO THE INPUT QUEUE.                          */
*/********************************************************************/
*                                                                  0311
*DATA2:                                                            0311
*  PROCEDURE;                                                      0311
DATA2    STM   @14,@12,@SA00005                                    0311
*    IF RPLRLEN=0 THEN                 /* CHECK FOR NULL RU          */
