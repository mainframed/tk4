         L     @14,TIMGETMN                                        0577
         L     @14,RECERCNT(,@14)                                  0577
         C     @14,@CF00080                                        0577
         BL    @RF00577                                            0577
*       DO;                            /* THREE OR GREATER, ABEND    */
*         CALL EXITFRR;               /* CALL ABEND ROUTINE  @ZM20856*/
         BAL   @14,EXITFRR                                         0579
*       END;                                                       0580
*      ELSE                                                        0581
*      /* CHECK FOR RESETSR HAVING BEEN ISSUED  **********************/
*      IF RPLRTNCD='0C'X & RPLFDB2='0A'X THEN  /* RESETSR ISSUED     */
         B     @RC00577                                            0581
@RF00577 CLI   RPLRTNCD(RPLREG),12                                 0581
         BNE   @RF00581                                            0581
         CLI   RPLFDB2(RPLREG),10                                  0581
         BNE   @RF00581                                            0581
*        REGRTN='1'B;                          /* REFRESH RPL        */
         L     @14,TIMGETMN                                        0582
         OI    REGRTN(@14),B'00001000'                             0582
*      ELSE                                                        0583
*      /* CHECK FOR EXCEPTION REQUEST   ******************************/
*      IF (RPLRTNCD='04'X & RPLFDB2=USFEXRQ) ×                     0583
*         (RPLRTNCD='04'X & RPLFDB2=USFEXRS) THEN                  0583
         B     @RC00581                                            0583
@RF00581 CLI   RPLRTNCD(RPLREG),4                                  0583
         BNE   @GL00025                                            0583
         CLI   RPLFDB2(RPLREG),3                                   0583
         BE    @RT00583                                            0583
@GL00025 CLI   RPLRTNCD(RPLREG),4                                  0583
         BNE   @RF00583                                            0583
         CLI   RPLFDB2(RPLREG),4                                   0583
         BNE   @RF00583                                            0583
@RT00583 DS    0H                                                  0584
*        DO;                                                       0584
*         IF RPLPATHI='1'B × RPLCPMI='1'B THEN                     0585
         TM    RPLPATHI(RPLREG),B'11000000'                        0585
         BZ    @RF00585                                            0585
*           DO;                        /* PATH OR CPM ERROR          */
*             RECERCNT = RECERCNT+1;   /* INCR RETRY COUNT   @ZM20858*/
         L     @14,TIMGETMN                                        0587
         LA    @11,1                                               0587
         AL    @11,RECERCNT(,@14)                                  0587
         ST    @11,RECERCNT(,@14)                                  0587
*             REGRTN = '0'B;           /* NO REFRESH         @ZM20858*/
         NI    REGRTN(@14),B'11110111'                             0588
