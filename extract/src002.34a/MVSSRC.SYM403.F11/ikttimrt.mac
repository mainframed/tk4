         LA    @11,OPARMS(,@11)                                    0522
         ST    @11,@AL00001                                        0522
         L     @11,CVTPTR                                          0522
         L     @11,CVTTCASP(,@11)                                  0522
         L     @15,TCASOQM(,@11)                                   0522
         LA    @01,@AL00001                                        0522
         BALR  @14,@15                                             0522
*    CALL SCHTOM;                      /* SCHEDULE  IKTTOMRT         */
         BAL   @14,SCHTOM                                          0523
*  END ATTN;                                                       0524
@EL00007 DS    0H                                                  0524
@EF00007 DS    0H                                                  0524
@ER00007 LM    @14,@12,@SA00007                                    0524
         BR    @14                                                 0524
         EJECT
