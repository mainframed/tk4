*/*** END -TPUNLOCK (DVTLOCK)- ***/                                0516
*                                         OCWDLOCK='0'B;           0516
         NI    OCWDLOCK(COCWPTR),B'11110111'                       0516
*                                         END;                     0517
*                                       ELSE;                      0518
@RF00498 DS    0H                                                  0519
*                                   END;                           0519
*                                ELSE DO;                          0520
         B     @RC00492                                            0520
@RF00492 DS    0H                                                  0521
*                                    OCWRC='24'X;                  0521
         MVI   OCWRC(COCWPTR),X'24'                                0521
*                                    OCWEC='10'X;                  0522
         MVI   OCWEC(COCWPTR),X'10'                                0522
*                                    END;                          0523
*                             END;                                 0524
*                     END;            /* IF UNSUCCESSFUL IN ANY     */
*                 ELSE;               /* OF THE ABOVE CALLS-RETURN  */
@RF00473 DS    0H                                                  0527
*             END;                                                 0527
*         ELSE CALL DONOSTR;                                       0528
*                                                                  0528
*                                                                  0528
         B     @RC00433                                            0528
@RF00433 BAL   @14,DONOSTR                                         0528
*     C14SVPTR=C14SVPTR-4;                                         0529
@RC00433 LA    @11,624                                             0529
         ALR   @11,COCWPTR                                         0529
         L     @00,C14SVPTR(,@11)                                  0529
         SL    @00,@CF00038                                        0529
         ST    @00,C14SVPTR(,@11)                                  0529
*     CR14PTR=C14SAVE;                                             0530
         LR    @11,@00                                             0530
         L     CR14PTR,C14SAVE(,@11)                               0530
*     RETURN CODE(RTNCODE);            /* RETURN TO RDVTBLD         */
@EL00002 DS    0H                                                  0531
@EF00002 DS    0H                                                  0531
@ER00002 BR    @14                                                 0531
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : SPLTMSTR - SPLIT MASTER                          */
*/*                                                                  */
*/*  FUNCTION : CALLS EPTBLD TO SETUP THE EPT (ENTRY POINT TABLE).   */
*/*             IF SUCCESSFUL, FILLS IN THE LAST EPT ENTRY.          */
*/*                                                                  */
*/*  INPUT : OCWEPTAB - POINTER TO THE ENTRY POINT TABLE             */
*/*                                                                  */
*/*  OUTPUT : EPTEPTR - POINTS TO THE FIRST EPT ENTRY (USED BY       */
*/*                     EPTFILL)                                     */
*/*                                                                  */
*/*  ROUTINES : EPTBLD - SCANS THE ENTIRE MASTER DVT AND CALLS       */
*/*                      ROUTINES TO FILL IN THE EPT.                */
*/*                                                                  */
*/*  MACROS : NONE                                                   */
*/*                                                                  */
*/********************************************************************/
*SPLTMSTR: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0532
SPLTMSTR DS    0H                                                  0533
*/*CALLED BY BLDREAL.                                                */
*     C14SAVE=CR14PTR;                                             0533
         LA    @11,624                                             0533
         ALR   @11,COCWPTR                                         0533
         L     @10,C14SVPTR(,@11)                                  0533
         ST    CR14PTR,C14SAVE(,@10)                               0533
*     C14SVPTR=C14SVPTR+4;                                         0534
         AL    @10,@CF00038                                        0534
         ST    @10,C14SVPTR(,@11)                                  0534
*     EPTEPTR=ADDR(EPTENTRY);         /*SET UP PTR TO FIRST EPT ENTRY*/
         L     EPTEPTR,OCWEPTAB(,COCWPTR)                          0535
         LA    EPTEPTR,EPTENTRY(,EPTEPTR)                          0535
*     CALL EPTBLD;                    /*CALL TO GET EPT FILLED       */
         BAL   @14,EPTBLD                                          0536
*     IF RTNCODE>ZERO                 /* SUCCESSFUL?                 */
*         THEN;                       /*  NO-RETURN.                 */
         LTR   RTNCODE,RTNCODE                                     0537
         BP    @RT00537                                            0537
*         ELSE DO;                    /*  YES-SETUP LAST ENTRY.      */
*             EPTEPTR=ADDR(EPTENTRY)+LENGTH(ISTEPTE)*(EPTHECNT-1); 0540
         L     @11,OCWEPTAB(,COCWPTR)                              0540
         LA    EPTEPTR,EPTENTRY(,@11)                              0540
         LH    @00,EPTHECNT(,@11)                                  0540
         BCTR  @00,0                                               0540
         MH    @00,@CH00032                                        0540
         ALR   EPTEPTR,@00                                         0540
*             EPTENDF='1'B;                                        0541
         OI    EPTENDF(EPTEPTR),B'10000000'                        0541
*             END;                                                 0542
*                                                                  0542
*     C14SVPTR=C14SVPTR-4;                                         0543
@RT00537 LA    @11,624                                             0543
         ALR   @11,COCWPTR                                         0543
         L     @00,C14SVPTR(,@11)                                  0543
         SL    @00,@CF00038                                        0543
         ST    @00,C14SVPTR(,@11)                                  0543
*     CR14PTR=C14SAVE;                                             0544
         LR    @11,@00                                             0544
         L     CR14PTR,C14SAVE(,@11)                               0544
*END SPLTMSTR;                        /* RETURN TO BLDREAL           */
@EL00003 DS    0H                                                  0545
@EF00003 DS    0H                                                  0545
@ER00003 BR    @14                                                 0545
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : EPTBLD - ENTRY POINT TABLE BUILDER               */
*/*                                                                  */
*/*  FUNCTION : SCANS THE ENTIRE MASTER DVT.  WHEN A DVT IS TO BE    */
*/*             BUILT EPTFILL IS CALLED.  IF THERE IS AN ERROR,      */
*/*             THE SCAN IS STOPPED.  CONTROL RETURNS TO THE CALLER  */
*/*                                                                  */
*/*  INPUT : MDVTPTR - POINTS TO THE FIRST ENTRY IN THE MASTER DVT   */
*/*                                                                  */
*/*  OUTPUT : NONE                                                   */
*/*                                                                  */
*/*  ROUTINES : EPTFILL - CALLED TO OBTAIN CORE FOR A DVT AND TO PUT */
*/*                       THE DVT ADDRESS IN THE EPT ENTRY.          */
*/*                                                                  */
*/*  MACROS : NONE                                                   */
*/*                                                                  */
*/********************************************************************/
*EPTBLD: PROC OPTIONS(NOSAVE,NOSAVEAREA);                          0546
EPTBLD   DS    0H                                                  0547
*/* CALLED BY SPLTMSTR.                                             */
*     C14SAVE=CR14PTR;                                             0547
         LA    @11,624                                             0547
         ALR   @11,COCWPTR                                         0547
         L     @10,C14SVPTR(,@11)                                  0547
         ST    CR14PTR,C14SAVE(,@10)                               0547
*     C14SVPTR=C14SVPTR+4;                                         0548
         AL    @10,@CF00038                                        0548
         ST    @10,C14SVPTR(,@11)                                  0548
*     RFY I RSTD;                                                  0549
*/* THE ENTIRE MASTER DVT IS SCANNED.  CALLS ARE MADE TO FILL IN THE */
*/*  EPT AND TO DO THE RDVT FILL AND BUILD.                          */
*     I=MDVTPTR+LENGTH(ISTSDVT)*SDVTOTAL; /*SET SCAN DELIMITER       */
         LH    I,SDVTOTAL(,MDVTPTR)                                0550
         MH    I,@CH02093                                          0550
         ALR   I,MDVTPTR                                           0550
*     DO WHILE (MDVTPTR<I);               /*SCAN:                    */
         B     @DE00551                                            0551
@DL00551 DS    0H                                                  0552
*         IF SDVID='RDVT'                 /* DVT ENTRY?              */
*             THEN IF SDVCNT=ZERO         /*  YES-MODULES TO INCLD?  */
         CLC   SDVID(4,MDVTPTR),@CC03250                           0552
         BNE   @RF00552                                            0552
*                     THEN;                                        0553
         LH    @00,SDVCNT(,MDVTPTR)                                0553
         LTR   @00,@00                                             0553
         BZ    @RT00553                                            0553
*                     ELSE DO;            /*    YES-CALL TO FILL EPT */
*                         CALL EPTFILL;   /*        ENTRY.           */
         BAL   @14,EPTFILL                                         0556
*                         IF RTNCODE>ZERO /*        SUCCESSFUL?      */
*                            THEN MDVTPTR=I; /*         NO-END SCAN  */
         LTR   RTNCODE,RTNCODE                                     0557
         BNP   @RF00557                                            0557
         LR    MDVTPTR,I                                           0558
*                            ELSE;                                 0559
@RF00557 DS    0H                                                  0560
*                         END;                                     0560
*             ELSE;                                                0561
@RF00552 DS    0H                                                  0562
*         MDVTPTR=MDVTPTR+LENGTH(ISTSDVT); /*STEP TO NEXT MDVT ENTRY */
@RC00552 AL    MDVTPTR,@CF02093                                    0562
*     END;                                 /*SCAN END.               */
*                                                                  0563
@DE00551 CR    MDVTPTR,I                                           0563
         BL    @DL00551                                            0563
*     MDVTPTR=OCWMDVT;                    /*RESTORE MDVT POINTER    */
         L     MDVTPTR,OCWMDVT(,COCWPTR)                           0564
*     C14SVPTR=C14SVPTR-4;                                         0565
         LA    @11,624                                             0565
         ALR   @11,COCWPTR                                         0565
         L     @00,C14SVPTR(,@11)                                  0565
         SL    @00,@CF00038                                        0565
         ST    @00,C14SVPTR(,@11)                                  0565
*     CR14PTR=C14SAVE;                     /*RETURN TO BLDREAL       */
         LR    @11,@00                                             0566
         L     CR14PTR,C14SAVE(,@11)                               0566
*END EPTBLD;                               /*VIA SPLTMSTR            */
@EL00004 DS    0H                                                  0567
@EF00004 DS    0H                                                  0567
@ER00004 BR    @14                                                 0567
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : EPTFILL - ENTRY POINT TABLE FILL                 */
*/*                                                                  */
*/*  FUNCTION : CALLED SUCCESSIVELY BY EPTBLD TO CREATE A DVT,       */
*/*             INITIALIZE THE DVT HDR, AND TO FILL THE ENTRY POINT  */
*/*             TABLE ENTRY WITH THE DVT NAME (IN THE MASTER DVT)    */
*/*             AND THE ADDRESS OF THE DVT CREATED.                  */
*/*                                                                  */
*/*  INPUT : MDVTPTR - POINTS TO MASTER DVT ENTRY                    */
*/*          EPTEPTR - POINTS TO ENTRY POINT TABLE ENTRY TO BE FILLED*/
*/*                                                                  */
*/*  OUTPUT : R15 = 4 IF UNSUCCESSFUL IN GETTING STORAGE FOR THE DVT */
*/*           R15 = 0 IF SUCCESSFUL                                  */
*/*                                                                  */
*/*  ROUTINES: DONOSTOR - HANDLES NON ZERO RETURN CODE FROM GETSTOR  */
*/*            DVTFREE - CALLED ON A GETSTORE FAILURE TO FREE        */
*/*                      PREVIOUSLY CREATED DVTS FOR THIS OPENDST    */
*/*                                                                  */
*/*  MACROS : GETSTOR - CALLED TO OBTAIN STORAGE FOR A DVT           */
*/*           FREESTOR - CALLED TO FREE EPT CORE WHEN THERE IS A     */
*/*                      GETSTOR FAILURE WHILE OBTAINING DVT CORE    */
*/*                                                                  */
*/********************************************************************/
*EPTFILL: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0568
EPTFILL  DS    0H                                                  0569
*/* FILLS IN AN ENTRY POINT TABLE ENTRY AND GETS CORE FOR ITS        */
*/*  ASSOCIATED REAL DVT.                                            */
*/* CALLED BY EPTBLD.                                                */
*     C14SAVE=CR14PTR;                                             0569
         LA    @11,624                                             0569
         ALR   @11,COCWPTR                                         0569
         L     @10,C14SVPTR(,@11)                                  0569
         ST    CR14PTR,C14SAVE(,@10)                               0569
*     C14SVPTR=C14SVPTR+4;                                         0570
         LA    @03,4                                               0570
         ALR   @10,@03                                             0570
         ST    @10,C14SVPTR(,@11)                                  0570
*     RFY RBLDCNT RSTD;                                            0571
*/* CALCULATE THE NUMBER OF BYTES NEEDED FOR THE REAL DVT.           */
*     RBLDCNT=LENGTH(ISTDVTE);                                     0572
         LA    @11,4                                               0572
         LR    RBLDCNT,@11                                         0572
*     RBLDCNT=RBLDCNT*(SDVCNT+1)+LENGTH(ISTDVT);                   0573
         LA    @11,1                                               0573
         AH    @11,SDVCNT(,MDVTPTR)                                0573
         MR    @10,RBLDCNT                                         0573
         AL    @11,@CF00054                                        0573
         LR    RBLDCNT,@11                                         0573
*     RDVTPTR=RBLDCNT;               /*MACRO EXPANSION DESTROYS REG3 */
         LR    RDVTPTR,RBLDCNT                                     0574
*      /*GETSTOR SIZE(RDVTPTR)CLEAR(YES)FETCHP(NO)SAVE(YES)*/      0575
*       DO;                                                        0575
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */                  0576
*/* END OF ATBUILD */                                              0576
*      RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;                     0576
*      RFY ISTRPH BASED(GPR01P);                                   0577
*      GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,                       0578
*               GPR06P,GPR07P,GPR08P,GPR09P,                       0578
*               GPR10P,GPR11P,GPR12P,GPR13P)                       0578
*          SETS(RPHWORK);                                          0578
*                                                                  0578
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS
*      GPR00F = RDVTPTR;                               /*SIZE IN REG0*/
         LR    GPR00F,RDVTPTR                                      0579
*      DCL 1 CMZA0061 FIXED(31) STATIC LOCAL,                      0580
*            2 * FIXED(8) INIT(96),                  /* OPTION FLAGS */
*            2 * FIXED(8) INIT(96),                   /* STORAGE KEY */
*            2 * FIXED(8) INIT(241),               /* SUBPOOL NUMBER */
*            2 * FIXED(8) INIT('00'X);               /* REQUEST TYPE */
*      GPR03F = CMZA0061;                                          0581
*                                                                  0581
         L     GPR03F,CMZA0061                                     0581
*      DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR02P)) */               0582
*        GPR02P = ATCLCPTR; /*SET UP ATCVT POINTER*/               0583
         L     GPR02P,ATCLCPTR                                     0583
*      END; /*LISATCVT*/                                           0584
*      CALL GPR02P -> ATCGTSTR;                       /*CALL ISTORFSR*/
         L     @15,ATCGTSTR(,GPR02P)                               0585
         BALR  @14,@15                                             0585
*      RFY (GPR00P,GPR01P,GPR02P,GPR03P) UNRSTD;                   0586
*      END ;                                         /*END OF GETSTOR*/
*              /* GET STOR FOR DVT @XL03HHG*/                      0588
*     IF RTNCODE=ZERO                    /* SUCCESSFUL?              */
*         THEN DO;                       /*  YES:                    */
         LTR   RTNCODE,RTNCODE                                     0588
         BNZ   @RF00588                                            0588
*             RDVTPTR=RPHSRPRM;          /*   SETUP REAL DVT PTR     */
         L     RDVTPTR,RPHSRPRM(,GPR01P)                           0590
*             DVTHLEN=RBLDCNT;           /*   SAVE LENGTH            */
         STH   RBLDCNT,DVTHLEN(,RDVTPTR)                           0591
*             DVTHUCNT=1;                /* DVT USED BY ONE EPT*/  0592
         LA    @02,1                                               0592
         STH   @02,DVTHUCNT(,RDVTPTR)                              0592
*             RFY RBLDCNT UNRSTD;                                  0593
*             DVTHECNT=SDVCNT+1;         /*   SAVE CNT OF ENTRIES    */
         AH    @02,SDVCNT(,MDVTPTR)                                0594
         STH   @02,DVTHECNT(,RDVTPTR)                              0594
*             EPTDVTA=ADDR(DVTENTRY);       /*   RDVT PTR IN EPT ENT */
         LA    @00,DVTENTRY(,RDVTPTR)                              0595
         STCM  @00,7,EPTDVTA(EPTEPTR)                              0595
*             EPTNAME=SDVNAME;           /*   PROCESS NAME IN EPT ENT*/
         MVC   EPTNAME(8,EPTEPTR),SDVNAME(MDVTPTR)                 0596
*             EPTEPTR=EPTEPTR+LENGTH(ISTEPTE); /*STEP TO NEXT EPT ENT*/
         AL    EPTEPTR,@CF00032                                    0597
*             RTNCODE=ZERO;              /*   SET SUCCESSFUL RETURN  */
         SLR   RTNCODE,RTNCODE                                     0598
*             END;                                                 0599
*         ELSE DO;                       /*  NO-SET END FLAG IN      */
         B     @RC00588                                            0600
@RF00588 DS    0H                                                  0601
*             EPTENDF='1'B;              /*     EPT FOR CLEANUP      */
         OI    EPTENDF(EPTEPTR),B'10000000'                        0601
*             CALL DONOSTR;              /*     SETUP RETURN CODES   */
         BAL   @14,DONOSTR                                         0602
*             CALL DVTFREE;              /*     FREE PREVIOUS RDVTS  */
         BAL   @14,DVTFREE                                         0603
*              /*FREESTOR AREA(OCWEPTAB)FETCHP(NO)SIZE(EPTHLEN)*/  0604
*               DO;                                                0604
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */                  0605
*/* END OF ATBUILD */                                              0605
*              RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;             0605
*              RFY ISTRPH BASED(GPR01P);                           0606
*              GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,               0607
*                       GPR06P,GPR07P,GPR08P,GPR09P,               0607
*                       GPR10P,GPR11P,GPR12P,GPR13P)               0607
*                  SETS(RPHWORK);                                  0607
*                                                                  0607
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS
*              GPR00F = EPTHLEN;                       /*SIZE IN REG0*/
         L     @11,OCWEPTAB(,COCWPTR)                              0608
         LH    GPR00F,EPTHLEN(,@11)                                0608
*              DCL 1 CMZA0079 FIXED(31) STATIC LOCAL,              0609
*                    2 * FIXED(8) INIT(64),          /* OPTION FLAGS */
*                    2 * FIXED(8) INIT(96),           /* STORAGE KEY */
*                    2 * FIXED(8) INIT(241),       /* SUBPOOL NUMBER */
*                    2 * FIXED(8) INIT('01'X);       /* REQUEST TYPE */
*              GPR03F = CMZA0079;                                  0610
         L     GPR03F,CMZA0079                                     0610
*              RFY (GPR04P) RSTD;                                  0611
*              GPR04P = OCWEPTAB;         /*PUT FREEMAIN ADDR IN REG4*/
*                                                                  0612
         LR    GPR04P,@11                                          0612
*              DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR02P)) */       0613
*                GPR02P = ATCLCPTR; /*SET UP ATCVT POINTER*/       0614
         L     GPR02P,ATCLCPTR                                     0614
*              END; /*LISATCVT*/                                   0615
*              CALL GPR02P -> ATCGTSTR;               /*CALL ISTORFSR*/
         L     @15,ATCGTSTR(,GPR02P)                               0616
         BALR  @14,@15                                             0616
*              RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P) UNRSTD;    0617
*              END ;                                /*END OF FREESTOR*/
*             /* FREE EPT CORE                   DM19033 */        0619
*             OCWEPTAB=ZERO;             /*     ZERO EPT POINTER      *
         SLR   @00,@00                                             0619
         ST    @00,OCWEPTAB(,COCWPTR)                              0619
*             RTNCODE=FOUR;              /*     SET UNSUCC. CODE     */
         LA    RTNCODE,4                                           0620
*             END;                                                 0621
*                                                                  0621
*     C14SVPTR=C14SVPTR-4;               /*RETURN TO BLDREAL VIA     */
@RC00588 LA    @11,624                                             0622
         ALR   @11,COCWPTR                                         0622
         L     @00,C14SVPTR(,@11)                                  0622
         SL    @00,@CF00038                                        0622
         ST    @00,C14SVPTR(,@11)                                  0622
*     CR14PTR=C14SAVE;                   /*EPTBLD AND SPLTMSTR.      */
         LR    @11,@00                                             0623
         L     CR14PTR,C14SAVE(,@11)                               0623
*END EPTFILL;                                                      0624
@EL00005 DS    0H                                                  0624
@EF00005 DS    0H                                                  0624
@ER00005 BR    @14                                                 0624
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : DVTFILL - DEVICE VECTOR TABLE FILL               */
*/*                                                                  */
*/*  FUNCTION : TO SCAN ENTIRE MASTER DVT.  WHEN A MASTER DVT HEADER */
*/*             ENTRY NAME MATCHES AN EPT ENTRY NAME, LOADPROC       */
*/*             IS CALLED TO FILL IN THE DVT WITH ADDRESSES OF THE   */
*/*             PROCESS MODULES.  IF A FAILURE OCCURS IN LOADPROC    */
*/*             BECAUSE AN ADDRESS COULD NOT BE OBTAINED, THE DVTS   */
*/*             ARE RELEASED BY CALLING DVTFREE AND THE EPT IS       */
*/*             FREED.                                               */
*/*                                                                  */
*/*  INPUT : NONE                                                    */
*/*                                                                  */
*/*  OUTPUT : R15 = 0 IF SUCCESSFUL                                  */
*/*           R15 = 4 IF LOADPROC WAS UNCESSFUL                      */
*/*                                                                  */
*/*  ROUTINES : LOADPROC - CALLED TO FILL THE DVT WITH ADDRESSES OF  */
*/*                        PROCESS MODULES.                          */
*/*             DVTFREE - CALLED TO FREE ALL THE DVTS BUILT FOR THIS */
*/*                       OPENDST.                                   */
*/*                                                                  */
*/*  MACROS : FREESTOR - TO RELEASE THE EPT CORE AFTER A FAILURE     */
*/*                      RETURN CODE FROM LOADPROC.                  */
*/*                                                                  */
*/********************************************************************/
*DVTFILL: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0625
DVTFILL  DS    0H                                                  0626
*/* SCANS THE ENTRY POINT TABLE WITHIN A MAIN SCAN OF THE ENTIRE     */
*/* MASTER DVT. FILLS IN THE RDVT WITH ADDRESSES OF THE PROCESS      */
*/* MODULES BE INCLUDED.                                             */
*/* CALLED FROM BLDREAL.                                             */
*     C14SAVE=CR14PTR;                                             0626
         LA    @11,624                                             0626
         ALR   @11,COCWPTR                                         0626
         L     @10,C14SVPTR(,@11)                                  0626
         ST    CR14PTR,C14SAVE(,@10)                               0626
*     C14SVPTR=C14SVPTR+4;                                         0627
         AL    @10,@CF00038                                        0627
         ST    @10,C14SVPTR(,@11)                                  0627
*     EPTEPTR=ADDR(EPTENTRY);        /*FIRST EPT ENTRY               */
         L     EPTEPTR,OCWEPTAB(,COCWPTR)                          0628
         LA    EPTEPTR,EPTENTRY(,EPTEPTR)                          0628
*     MDVTPTR=OCWMDVT;              /*FIRST MDVT ENTRY              */
         L     MDVTPTR,OCWMDVT(,COCWPTR)                           0629
*     I=MDVTPTR+LENGTH(ISTSDVT)*SDVTOTAL; /*DELIMITER ON MDVT SCAN   */
         LH    I,SDVTOTAL(,MDVTPTR)                                0630
         MH    I,@CH02093                                          0630
         ALR   I,MDVTPTR                                           0630
*     DO WHILE (MDVTPTR<I);          /*SCAN:                         */
         B     @DE00631                                            0631
@DL00631 DS    0H                                                  0632
*         IF EPTNAME=SDVNAME         /* WHEN ENTRY FOUND TO FILL     */
*             THEN DO;                                             0632
         CLC   EPTNAME(8,EPTEPTR),SDVNAME(MDVTPTR)                 0632
         BNE   @RF00632                                            0632
*                 CALL LOADPROC;     /* CALL TO GET MODULE ADDRESS   */
         BAL   @14,LOADPROC                                        0634
*                 IF RTNCODE>ZERO    /*  SUCCESSFUL?                 */
*                     THEN DO;       /*   NO:                        */
         LTR   RTNCODE,RTNCODE                                     0635
         BNP   @RF00635                                            0635
*                         CALL DVTFREE; /*  FREE ALL RDVT CORE       */
         BAL   @14,DVTFREE                                         0637
*                     /*FREESTOR AREA(OCWEPTAB)FETCHP(NO)SIZE(EPTHLEN)*
*                                                                  0638
*                      DO;                                         0638
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */                  0639
*/* END OF ATBUILD */ RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;      0639
*                     RFY ISTRPH BASED(GPR01P);                    0640
*                     GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,        0641
*                              GPR06P,GPR07P,GPR08P,GPR09P,        0641
*                              GPR10P,GPR11P,GPR12P,GPR13P)        0641
*                         SETS(RPHWORK);                           0641
*                                                                  0641
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS
*                     GPR00F = EPTHLEN;                /*SIZE IN REG0*/
         L     @11,OCWEPTAB(,COCWPTR)                              0642
         LH    GPR00F,EPTHLEN(,@11)                                0642
*                     DCL 1 CMZA0098 FIXED(31) STATIC LOCAL,       0643
*                           2 * FIXED(8) INIT(64),   /* OPTION FLAGS */
*                           2 * FIXED(8) INIT(96),    /* STORAGE KEY */
*                           2 * FIXED(8) INIT(241),/* SUBPOOL NUMBER */
*                           2 * FIXED(8) INIT('01'X);/* REQUEST TYPE */
*                     GPR03F = CMZA0098;                           0644
         L     GPR03F,CMZA0098                                     0644
*                     RFY (GPR04P) RSTD;                           0645
*                     GPR04P = OCWEPTAB;  /*PUT FREEMAIN ADDR IN REG4*/
*                                                                  0646
         LR    GPR04P,@11                                          0646
*                     DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR02P)) */
*                       GPR02P = ATCLCPTR; /*SET UP ATCVT POINTER*/
         L     GPR02P,ATCLCPTR                                     0648
*                     END; /*LISATCVT*/                            0649
*                     CALL GPR02P -> ATCGTSTR;        /*CALL ISTORFSR*/
         L     @15,ATCGTSTR(,GPR02P)                               0650
         BALR  @14,@15                                             0650
*                     RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P) UNRSTD;
*                     END ;                         /*END OF FREESTOR*/
*                           /* FETCHP ADDED    DM19033 */          0653
*                         OCWEPTAB=ZERO;  /*EPT AND ZERO POINTER     */
         SLR   @00,@00                                             0653
         ST    @00,OCWEPTAB(,COCWPTR)                              0653
*                         RTNCODE=FOUR;  /* SET UNSUCCESSFUL CODE.   */
         LA    RTNCODE,4                                           0654
*                         MDVTPTR=I; /*     FORCE END OF SCAN        */
         LR    MDVTPTR,I                                           0655
*                         END;                                     0656
*                     ELSE DO;       /*   YES: STEP TO NEXT EPT ENTRY*/
         B     @RC00635                                            0657
@RF00635 DS    0H                                                  0658
*                         EPTEPTR=EPTEPTR+LENGTH(ISTEPTE);         0658
         AL    EPTEPTR,@CF00032                                    0658
*                         IF EPTENDF='1'B           /*END EPT SCAN?  */
*                             THEN MDVTPTR=I;       /*YES-FORCE MDVT */
         TM    EPTENDF(EPTEPTR),B'10000000'                        0659
         BNO   @RF00659                                            0659
         LR    MDVTPTR,I                                           0660
*                             ELSE;                 /*    END TO END */
@RF00659 DS    0H                                                  0662
*                         END;                      /*    MAIN SCAN. */
*                 END;                                             0663
*             ELSE;                                                0664
@RF00632 DS    0H                                                  0665
*         MDVTPTR=MDVTPTR+LENGTH(ISTSDVT); /*STEP TO NEXT MDVT ENTRY */
@RC00632 AL    MDVTPTR,@CF02093                                    0665
*     END;                          /*SCAN END.                      */
*                                                                  0666
@DE00631 CR    MDVTPTR,I                                           0666
         BL    @DL00631                                            0666
*     MDVTPTR=OCWMDVT;              /*RESTORE MDVT POINTER          */
         L     MDVTPTR,OCWMDVT(,COCWPTR)                           0667
*     C14SVPTR=C14SVPTR-4;                                         0668
         LA    @11,624                                             0668
         ALR   @11,COCWPTR                                         0668
         L     @00,C14SVPTR(,@11)                                  0668
         SL    @00,@CF00038                                        0668
         ST    @00,C14SVPTR(,@11)                                  0668
*     CR14PTR=C14SAVE;                                             0669
         LR    @11,@00                                             0669
         L     CR14PTR,C14SAVE(,@11)                               0669
*END DVTFILL;                        /* RETURN TO BLDREAL            */
@EL00006 DS    0H                                                  0670
@EF00006 DS    0H                                                  0670
@ER00006 BR    @14                                                 0670
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : LOADPROC - LOAD PROCESS MODULE ADDRESSES         */
*/*                                                                  */
*/*  FUNCTION : CALLED FOR EACH EPT ENTRY IN ORDER TO FILL THE       */
*/*             ASSOCIATED DVT WITH THE ADDRESSES OF THE PROCESS     */
*/*             MODULES.                                             */
*/*                                                                  */
*/*  INPUT : EPTEPTR - POINTS TO THE ENTRY POINT TABLE ENTRY         */
*/*          MDVTPRT - POINTS TO HEADER ENTRY CORRESPONDING TO THE   */
*/*                    ENTRY POINT TABLE ENTRY.                      */
*/*                                                                  */
*/*  OUTPUT : R15 = 0 IF DVT ADDRESSES FILLED IN SUCCESSFULLY        */
*/*           R15 = 4 IF DVT ADDRESSES FILLED IN UNSUCCESSFULLY      */
*/*                                                                  */
*/*  ROUTINES : NONE                                                 */
*/*                                                                  */
*/*  MACROS : CRRMAINT - TO MAINTAIN A MODULE TRACE THRU VTAM        */
*/*                                                                  */
*/********************************************************************/
*LOADPROC: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0671
LOADPROC DS    0H                                                  0672
*/* FILLS IN THE REAL DVT WITH THE ADDRESSES OF THE PROCCESS MODULES */
*/*  THAT ARE TO BE INCLUDED.                                        */
*/* CALLED BY DVTFILL.                                               */
*     C14SAVE=CR14PTR;                                             0672
         LA    @11,624                                             0672
         ALR   @11,COCWPTR                                         0672
         L     @10,C14SVPTR(,@11)                                  0672
         ST    CR14PTR,C14SAVE(,@10)                               0672
*     C14SVPTR=C14SVPTR+4;                                         0673
         AL    @10,@CF00038                                        0673
         ST    @10,C14SVPTR(,@11)                                  0673
*     RTNCODE=ZERO;                                        /*@XL03HHG*/
         SLR   RTNCODE,RTNCODE                                     0674
*     RDVTPTR=EPTDVTA-LENGTH(ISTDVT);    /* SETUP THE RDVT BASE  */
         LA    @11,24                                              0675
         L     RDVTPTR,EPTDVTA-1(,EPTEPTR)                         0675
         LA    RDVTPTR,0(,RDVTPTR)                                 0675
         SLR   RDVTPTR,@11                                         0675
*     RDVTE=ADDR(DVTENTRY)+LENGTH(ISTDVTE)*(DVTHECNT-1); /* LAST ENT */
         LA    RDVTE,DVTENTRY(,RDVTPTR)                            0676
         LH    @00,DVTHECNT(,RDVTPTR)                              0676
         BCTR  @00,0                                               0676
         SLA   @00,2                                               0676
         ALR   RDVTE,@00                                           0676
*     DVTEXITF='1'B;                 /*SET LAST ENTRY FLAG.          */
         OI    DVTEXITF(RDVTE),B'10000000'                         0677
*     RDVTE=RDVTPTR+LENGTH(ISTDVT);  /*SET PTR TO FIRST RDVT ENT     */
*                                                                  0678
         ALR   @11,RDVTPTR                                         0678
         LR    RDVTE,@11                                           0678
*/* FILL IN THE REAL DVT.                                            */
*     DO WHILE (RDVTE<ADDR(DVTENTRY)+LENGTH(ISTDVTE)*(DVTHECNT-1));
         B     @DE00679                                            0679
@DL00679 DS    0H                                                  0680
*     MDVTPTR=MDVTPTR+LENGTH(ISTSDVT); /* PT TO NEXT MDVT PROC ENTRY */
         AL    MDVTPTR,@CF02093                                    0680
*         IF SDVINCLD='0'B           /*INCLUDE THIS PROC MODULE?     */
*             THEN;                  /* NO-CONTINUE SCAN.            */
         TM    SDVINCLD(MDVTPTR),B'10000000'                       0681
         BZ    @RT00681                                            0681
*             ELSE DO;               /* YES-CALL TO OBTAIN LOAD ADDR */
*                 IF SDVPMEPA^=ZERO  /*   VALID MODULE?      @XL03HHG*/
*                     THEN           /*     YES-SET UP ADDR. @XL03HHG*/
         L     @11,SDVPMEPA(,MDVTPTR)                              0684
         LTR   @11,@11                                             0684
         BZ    @RF00684                                            0684
*                         DVTPROCA=SDVPMEPA;               /*@XL03HHG*/
         STCM  @11,7,DVTPROCA(RDVTE)                               0685
*                     ELSE DO;       /*      NO-STOP RDVTBLD         */
         B     @RC00684                                            0686
@RF00684 DS    0H                                                  0687
*                         RTNCODE=FOUR;  /* IND UNSUCCESSFUL @XL03HHG*/
         LA    RTNCODE,4                                           0687
*                         OCWRC='08'X;    /* LOGIC ERROR     @XL03HHG*/
         MVI   OCWRC(COCWPTR),X'08'                                0688
*                         OCWEC='4F'X;    /* INVAL MODE NAME @XL03HHG*/
         MVI   OCWEC(COCWPTR),X'4F'                                0689
*                         RDVTE=ADDR(DVTENTRY)+LENGTH(ISTDVTE)*DVTHECNT
         LA    RDVTE,DVTENTRY(,RDVTPTR)                            0690
         LH    @00,DVTHECNT(,RDVTPTR)                              0690
         SLA   @00,2                                               0690
         ALR   RDVTE,@00                                           0690
*                         END;                                     0691
*                 RDVTE=RDVTE+LENGTH(ISTDVTE); /*PT TO NEXT RDVT ENT */
@RC00684 AL    RDVTE,@CF00038                                      0692
*                 END;                                             0693
*     END;                                                         0694
*                                                                  0694
@RT00681 DS    0H                                                  0694
@DE00679 LA    @00,DVTENTRY(,RDVTPTR)                              0694
         LH    @11,DVTHECNT(,RDVTPTR)                              0694
         BCTR  @11,0                                               0694
         SLA   @11,2                                               0694
         ALR   @00,@11                                             0694
         CR    RDVTE,@00                                           0694
         BL    @DL00679                                            0694
*     C14SVPTR=C14SVPTR-4;                                         0695
         LA    @11,624                                             0695
         ALR   @11,COCWPTR                                         0695
         L     @00,C14SVPTR(,@11)                                  0695
         SL    @00,@CF00038                                        0695
         ST    @00,C14SVPTR(,@11)                                  0695
*     CR14PTR=C14SAVE;                                             0696
         LR    @11,@00                                             0696
         L     CR14PTR,C14SAVE(,@11)                               0696
*END LOADPROC;                       /*RETURN TO BLDREAL VIA DVTFILL */
@EL00007 DS    0H                                                  0697
@EF00007 DS    0H                                                  0697
@ER00007 BR    @14                                                 0697
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : DVTFREE - DEVICE VECTOR TABLE FREE               */
*/*                                                                  */
*/*  FUNCTION : CALLED WHEN AN ERROR IS ENCOUNTERED IN DVTFILL OR    */
*/*             EPTFILL.  STEPS THRU THE EPT RELEASING THE CORE      */
*/*             OBTAINED FOR REAL DVTS.                              */
*/*                                                                  */
*/*  INPUT : OCWEPTAB - ADDRESS OF THE EPT                           */
*/*                                                                  */
*/*  OUTPUT : NONE                                                   */
*/*                                                                  */
*/*  ROUTINES : NONE                                                 */
*/*                                                                  */
*/*  MACROS : FREESTOR - CALLED TO RETURN THE DVT CORE TO STORAGE    */
*/*                      MANAGEMENT POOLS.                           */
*/*                                                                  */
*/********************************************************************/
*DVTFREE: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0698
DVTFREE  DS    0H                                                  0699
*/*CALLED TO FREE THE CORE OBTAINED FOR REAL DVTS WHEN THERE WAS    */
*/* A FAILURE IN THE BUILDING OF THE REAL DVTS.                     */
*/*CALLED BY DVTFILL OR EPTFILL.                                    */
*     C14SAVE=CR14PTR;                                             0699
         LA    @11,624                                             0699
         ALR   @11,COCWPTR                                         0699
         L     @10,C14SVPTR(,@11)                                  0699
         ST    CR14PTR,C14SAVE(,@10)                               0699
*     C14SVPTR=C14SVPTR+4;                                         0700
         AL    @10,@CF00038                                        0700
         ST    @10,C14SVPTR(,@11)                                  0700
*     EPTEPTR=ADDR(EPTENTRY);        /*POINT TO THE FIRST EPT ENTRY */
         L     EPTEPTR,OCWEPTAB(,COCWPTR)                          0701
         LA    EPTEPTR,EPTENTRY(,EPTEPTR)                          0701
*/*STEP THRU THE ENTIRE ENTRY POINT TABLE TO FREE THE CORE OBTAINED  */
*/* FOR THE REAL DVTS.                                               */
*     DO WHILE (EPTENDF='0'B);       /*SCAN: END OF EPT?             */
         B     @DE00702                                            0702
@DL00702 DS    0H                                                  0703
*         RDVTPTR=EPTDVTA-LENGTH(ISTDVT);/*ADDR OF THE REAL DVT  */
         L     RDVTPTR,EPTDVTA-1(,EPTEPTR)                         0703
         LA    RDVTPTR,0(,RDVTPTR)                                 0703
         SL    RDVTPTR,@CF00054                                    0703
*          /*FREESTOR AREA(RDVTPTR)FETCHP(NO)SIZE(DVTHLEN)*/       0704
*           DO;                                                    0704
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */                  0705
*/* END OF ATBUILD */                                              0705
*          RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;                 0705
*          RFY ISTRPH BASED(GPR01P);                               0706
*          GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,                   0707
*                   GPR06P,GPR07P,GPR08P,GPR09P,                   0707
*                   GPR10P,GPR11P,GPR12P,GPR13P)                   0707
*              SETS(RPHWORK);                                      0707
*                                                                  0707
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS
*          GPR00F = DVTHLEN;                           /*SIZE IN REG0*/
         LH    GPR00F,DVTHLEN(,RDVTPTR)                            0708
*          DCL 1 CMZA0117 FIXED(31) STATIC LOCAL,                  0709
*                2 * FIXED(8) INIT(64),              /* OPTION FLAGS */
*                2 * FIXED(8) INIT(96),               /* STORAGE KEY */
*                2 * FIXED(8) INIT(241),           /* SUBPOOL NUMBER */
*                2 * FIXED(8) INIT('01'X);           /* REQUEST TYPE */
*          GPR03F = CMZA0117;                                      0710
         L     GPR03F,CMZA0117                                     0710
*          RFY (GPR04P) RSTD;                                      0711
*          GPR04P = RDVTPTR;              /*PUT FREEMAIN ADDR IN REG4*/
*                                                                  0712
         LR    GPR04P,RDVTPTR                                      0712
*          DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR02P)) */           0713
*            GPR02P = ATCLCPTR; /*SET UP ATCVT POINTER*/           0714
         L     GPR02P,ATCLCPTR                                     0714
*          END; /*LISATCVT*/                                       0715
*          CALL GPR02P -> ATCGTSTR;                   /*CALL ISTORFSR*/
         L     @15,ATCGTSTR(,GPR02P)                               0716
         BALR  @14,@15                                             0716
*          RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P) UNRSTD;        0717
*          END ;                                    /*END OF FREESTOR*/
*/*DM19033*/                                                       0719
*         EPTEPTR=EPTEPTR+LENGTH(ISTEPTE); /*PT TO NEXT RDVT ADDR    */
         AL    EPTEPTR,@CF00032                                    0719
*     END;                           /*END OF SCAN.                  */
*                                                                  0720
@DE00702 TM    EPTENDF(EPTEPTR),B'10000000'                        0720
         BZ    @DL00702                                            0720
*     C14SVPTR=C14SVPTR-4;                                         0721
         LA    @11,624                                             0721
         ALR   @11,COCWPTR                                         0721
         L     @00,C14SVPTR(,@11)                                  0721
         SL    @00,@CF00038                                        0721
         ST    @00,C14SVPTR(,@11)                                  0721
*     CR14PTR=C14SAVE;               /*RETURN TO BLDREAL VIA DVTFILL */
         LR    @11,@00                                             0722
         L     CR14PTR,C14SAVE(,@11)                               0722
*END DVTFREE;                        /*OR VIA EPTFILL,EPTBLD,SPLTMSTR*/
@EL00008 DS    0H                                                  0723
@EF00008 DS    0H                                                  0723
@ER00008 BR    @14                                                 0723
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : DONOSTR - NO STORAGE ERROR HANDLER               */
*/*                                                                  */
*/*  FUNCTION : TO ANALIZE THE ERROR RETURN CODE FROM THE            */
*/*             GETSTOR MACRO AND TO PLACE AN EXPLANATORY RETURN     */
*/*              CODE IN THE ISTOCW SO IT CAN BE PASSED TO THE USER. */
*/*                                                                  */
*/*  INPUT : R15 - ERROR RETURN CODE FROM GETSTOR                    */
*/*                                                                  */
*/*  OUTPUT : OCWEC AND OCWRC CONTAIN A DETAILED ERROR CODE          */
*/*                                                                  */
*/*  ROUTINES:  NONE                                                 */
*/*                                                                  */
*/*  MACROS : NONE                                                   */
*/*                                                                  */
*/********************************************************************/
*DONOSTR: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0724
DONOSTR  DS    0H                                                  0725
*/* CALLED TO HANDLE ERROR RETURN CODES FROM GETSTOR                 */
*    C14SAVE=CR14PTR;       /*SAVE R14 FOR RETURN*/                0725
         LA    @11,624                                             0725
         ALR   @11,COCWPTR                                         0725
         L     @10,C14SVPTR(,@11)                                  0725
         ST    CR14PTR,C14SAVE(,@10)                               0725
*    C14SVPTR=C14SVPTR+4;                                          0726
         LA    @03,4                                               0726
         ALR   @10,@03                                             0726
         ST    @10,C14SVPTR(,@11)                                  0726
*    IF RTNCODE=4                                                  0727
*        THEN DO;                                                  0727
         CR    RTNCODE,@03                                         0727
         BNE   @RF00727                                            0727
*            OCWRC='14'X;                                          0729
         MVI   OCWRC(COCWPTR),X'14'                                0729
*            OCWEC='01'X;                                          0730
         MVI   OCWEC(COCWPTR),X'01'                                0730
*            END;                                                  0731
*        ELSE DO;                                                  0732
         B     @RC00727                                            0732
@RF00727 DS    0H                                                  0733
*            OCWRC='24'X;                                          0733
         MVI   OCWRC(COCWPTR),X'24'                                0733
*            OCWEC='01'X;                                          0734
         MVI   OCWEC(COCWPTR),X'01'                                0734
*            END;                                                  0735
*    C14SVPTR=C14SVPTR-4;                                          0736
@RC00727 LA    @11,624                                             0736
         ALR   @11,COCWPTR                                         0736
         L     @00,C14SVPTR(,@11)                                  0736
         SL    @00,@CF00038                                        0736
         ST    @00,C14SVPTR(,@11)                                  0736
*    CR14PTR=C14SAVE;                                              0737
         LR    @11,@00                                             0737
         L     CR14PTR,C14SAVE(,@11)                               0737
*END DONOSTR;                                                      0738
@EL00009 DS    0H                                                  0738
@EF00009 DS    0H                                                  0738
@ER00009 BR    @14                                                 0738
* RFY MDVTPTR UNRSTD;                                              0739
*END BLDREAL;                                                      0740
*/*******************************************************************/
*/*                                                                 */
*/*  ROUTINE NAME : DVTSERCH - DEVICE VECTOR TABLE SEARCH           */
*/*                                                                 */
*/*  FUNCTION : STEPS THRU THE ENTIRE EPT EXAMINING DVTS.  IF THE   */
*/*             ATCVT DVT CHAIN IS EMPTY, DVTADD IS CALLED TO       */
*/*             ADD THE DVT TO THE CHAIN.                           */
*/*             OTHERWISE COMPLEN IS CALLED TO COMPARE FOR          */
*/*             A DUPLICATE DVT ON THE CHAIN.  IF COMPLEN FINDS NO  */
*/*             DUPLICATE, DVTADD IS CALLED TO ADD THE DVT TO THE   */
*/*             CHAIN.                                              */
*/*                                                                 */
*/*  INPUT : NONE                                                   */
*/*                                                                 */
*/*  OUTPUT : NONE                                                  */
*/*                                                                 */
*/*  ROUTINES : DVTADD - ADDS A DVT TO THE ATCVT CHAIN              */
*/*             COMPLEN - COMPARES A DVT'S LENGTH WITH THE LENGTH   */
*/*                       OF EACH DVT ON THE ATCVT CHAIN.  IF AN    */
*/*                       EQUAL LENGTH DVT IS FOUND IT CALLS DVTCOMP*/
*/*                       TO MAKE A COMPLETE COMPARISON FOR         */
*/*                       EQUALITY.                                 */
*/*                                                                 */
*/*  MACROS : NONE                                                  */
*/*                                                                 */
*/*******************************************************************/
*DVTSERCH: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0741
DVTSERCH DS    0H                                                  0742
*C14SAVE=CR14PTR;       /*SAVE R14 FOR RETURN*/                    0742
         LA    @05,624                                             0742
         ALR   @05,COCWPTR                                         0742
         L     @11,C14SVPTR(,@05)                                  0742
         ST    CR14PTR,C14SAVE(,@11)                               0742
*C14SVPTR=C14SVPTR+4;                                              0743
         AL    @11,@CF00038                                        0743
         ST    @11,C14SVPTR(,@05)                                  0743
*               /*GET @ OF OCW FROM SAVEAREA*/                     0744
* RFY COCWPTR RSTD;                                                0744
* RFY ISTEPT BASED(OCWEPTAB);                                      0745
* RFY ISTEPTE BASED(EPTEPTR);                                      0746
* RFY (NEWDVPTR,PROCPTR,EPDVTPTR) RSTD;                            0747
* RDVTPTR = ATCFDVT;    /* GET PTR TO FIRST DVT OM ATCVT CHN*/     0748
         L     @05,OCWACVT(,COCWPTR)                               0748
         L     RDVTPTR,ATCFDVT(,@05)                               0748
* RDVTPTR=RDVTPTR&HIGHOFF; /* INSURE HIGH ORDER BIT OFF */         0749
         N     RDVTPTR,HIGHOFF                                     0749
* OCWSRDVT = CON;          /* START SEARCH FOR DUP DVT'S */        0750
         OI    OCWSRDVT(COCWPTR),B'10000000'                       0750
* EPTEPTR = OCWEPTAB+LENGTH(EPTHDR); /* INIT PTR TO EPT ENTS*/     0751
         LA    EPTEPTR,12                                          0751
         AL    EPTEPTR,OCWEPTAB(,COCWPTR)                          0751
* DO WHILE (OCWSRDVT=CON) ;/*START LOOP*/                          0752
         B     @DE00752                                            0752
@DL00752 DS    0H                                                  0753
* EPDVTPTR=EPTDVT-LENGTH(ISTDVT) ;                                 0753
         L     EPDVTPTR,EPTDVT(,EPTEPTR)                           0753
         SL    EPDVTPTR,@CF00054                                   0753
* IF RDVTPTR =  0  /* ANY DVT'S ON CHAIN */                        0754
*  THEN                                                            0754
         LTR   RDVTPTR,RDVTPTR                                     0754
         BNZ   @RF00754                                            0754
*    DO;                                                           0755
*     CALL DVTADD;  /* NO DVT'S ON CHAIN */                        0756
         BAL   @14,DVTADD                                          0756
*     RDVTPTR= ATCFDVT;   /* PT TO NEW CHN*/                       0757
         L     @05,OCWACVT(,COCWPTR)                               0757
         L     RDVTPTR,ATCFDVT(,@05)                               0757
*    END;                                                          0758
*  ELSE                                                            0759
*  DO;                                                             0759
         B     @RC00754                                            0759
@RF00754 DS    0H                                                  0760
*    CALL COMPLEN;          /* SEE IF DVT HAS DUP ON CHAIN*/       0760
         BAL   @14,COMPLEN                                         0760
*    IF OCWDPDVT = CON      /* WAS DVT ALREADY ON ATCVT CHN?*/     0761
*      THEN OCWDPDVT = COFF;  /* TURN OFF DUPLICATE INDICATOR */   0761
*                                                                  0761
*                                                                  0761
         TM    OCWDPDVT(COCWPTR),B'00001000'                       0761
         BNO   @RF00761                                            0761
         NI    OCWDPDVT(COCWPTR),B'11110111'                       0762
*      ELSE                                                        0763
*        CALL DVTADD;        /* PUT IT ON CHAIN*/                  0763
         B     @RC00761                                            0763
@RF00761 BAL   @14,DVTADD                                          0763
*  END;                                                            0764
@RC00761 DS    0H                                                  0765
* EPTEPTR=EPTEPTR+LENGTH(ISTEPTE); /*PT TO NEXT EPT ENTRY*/        0765
@RC00754 AL    EPTEPTR,@CF00032                                    0765
* IF EPTENDF = CON                /* LAST EPT ENTRY */             0766
*     THEN                                                         0766
         TM    EPTENDF(EPTEPTR),B'10000000'                        0766
         BNO   @RF00766                                            0766
*      OCWSRDVT = COFF;     /* STOP LOOP*/                         0767
         NI    OCWSRDVT(COCWPTR),B'01111111'                       0767
*     ELSE;                                                        0768
@RF00766 DS    0H                                                  0769
* END;                                                             0769
@DE00752 TM    OCWSRDVT(COCWPTR),B'10000000'                       0769
         BO    @DL00752                                            0769
*C14SVPTR=C14SVPTR-4;        /*RESTORE R14 FOR RETURN*/            0770
         LA    @05,624                                             0770
         ALR   @05,COCWPTR                                         0770
         L     @03,C14SVPTR(,@05)                                  0770
         SL    @03,@CF00038                                        0770
         ST    @03,C14SVPTR(,@05)                                  0770
*CR14PTR = C14SAVE;                                                0771
         L     CR14PTR,C14SAVE(,@03)                               0771
* END DVTSERCH;                                                    0772
@EL00010 DS    0H                                                  0772
@EF00010 DS    0H                                                  0772
@ER00010 BR    @14                                                 0772
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : COMPLEN - COMPARE DVT LENGTH                     */
*/*                                                                  */
*/*  FUNCTION : TO SCAN THE ENTIRE ATCVT CHAIN OF DVTS LOOKING FOR   */
*/*             A DVT OF EQUAL LENGTH.  IF A DVT OF EQUAL LENGTH IS  */
*/*             FOUND DVTCOMP IS CALLED TO COMPARE THE ENTIRE DVT FOR*/
*/*             A DUPLICATION.  WHEN A DUPLICATE DVT IS FOUND THE    */
*/*             SCAN STOPS. OTHERWISE THE SCAN CONTINUES.            */
*/*                                                                  */
*/*  INPUT : EPDVTPTR = ADDRESS OF A NEWLY CREATED DVT               */
*/*                                                                  */
*/*  OUTPUT : NONE                                                   */
*/*                                                                  */
*/*  ROUTINES : DVTCOMP - CALLED WHEN A DVT ON THE ATCVT CHAIN HAS   */
*/*                       THE SAME LENGTH AS A NEW DVT.              */
*/*                                                                  */
*/*  MACROS : NONE                                                   */
*/*                                                                  */
*/********************************************************************/
* COMPLEN: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0773
COMPLEN  DS    0H                                                  0774
*C14SAVE=CR14PTR;       /*SAVE R14 FOR RETURN*/                    0774
         LA    @05,624                                             0774
         ALR   @05,COCWPTR                                         0774
