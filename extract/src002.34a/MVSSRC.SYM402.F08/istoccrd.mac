         MVC   ODOBHSID(1,ODOPTR),OCWBHSID(COCWPTR)                0442
*             RFY EXCNT RSTD ;                                     0443
*             EXCNT = ODOECNT ;      /* GET EXCEPT LOOP CTL @D31JKQG*/
         SLR   EXCNT,EXCNT                                         0444
         IC    EXCNT,ODOECNT(,ODOPTR)                              0444
*             IF OCWRBH = '1'B × OCWPBH = '1'B            /*@ZA13648*/
*               THEN ODOPRBH = '1'B; /* IF PREFERRED OR     @ZA13648*/
         TM    OCWRBH(COCWPTR),B'00001100'                         0445
         BZ    @RF00445                                            0445
         OI    ODOPRBH(ODOPTR),B'10000000'                         0446
*                                    /* REQUIRED, SET FLAG  @ZA13648*/
*             DO WHILE EXCNT ^= 0 ;                       /*@D31JKQG*/
@RF00445 B     @DE00447                                            0447
@DL00447 DS    0H                                                  0448
*                   /* COPY EXCEPT DVC ENTRIES */                  0448
*               I -> ODOENTRY = ODOPTR -> ODOENTRY ;      /*@D31JKQG*/
         MVC   ODOENTRY(4,I),ODOENTRY(ODOPTR)                      0448
*               I = I + LENGTH(ODOENTRY) ;                /*@D31JKQG*/
         LA    @03,4                                               0449
         ALR   I,@03                                               0449
*               ODOPTR = ODOPTR + LENGTH(ODOENTRY) ;      /*@D31JKQG*/
         ALR   ODOPTR,@03                                          0450
*               EXCNT = EXCNT - 1 ;                       /*@D31JKQG*/
         BCTR  EXCNT,0                                             0451
*             END ;                                                0452
@DE00447 LTR   EXCNT,EXCNT                                         0452
         BNZ   @DL00447                                            0452
*             RFY EXCNT UNRSTD ;                                   0453
*             I = LENGTH(ISTODOR)+ 16 * LENGTH(ODOENTRY) ; /*@D31JKQG*/
         LA    I,84                                                0454
*                   /* FREE ISTOCCMD ISTODOR   */                  0455
*              /*FREESTOR AREA(OCWODOA)FETCHP(NO)SIZE(I)*/         0455
*               DO;                                                0455
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */                  0456
