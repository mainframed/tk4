*             EPTHLEN=I;             /*     SAVE BYTE LENGTH        */
         LR    @03,@00                                             0439
         STH   I,EPTHLEN(,@03)                                     0439
*             I=OCWEPTAB+LENGTH(ISTEPT)+EPTHECNT*LENGTH(ISTEPTE) ; 0440
         LA    @03,12                                              0440
         ALR   @00,@03                                             0440
         L     @11,OCWEPTAB(,COCWPTR)                              0440
         LH    @11,EPTHECNT(,@11)                                  0440
         MR    @10,@03                                             0440
         ALR   @00,@11                                             0440
         LR    I,@00                                               0440
*             I->ISTODOR= ISTODOR;   /* MOVE COPY ODO       @D31JKQG*/
         MVC   ISTODOR(20,I),ISTODOR(ODOPTR)                       0441
