*/*                                                                  */
*/*  ROUTINE NAME : DVTCOMP - DEVICE VECTOR TABLE COMPARE            */
*/*                                                                  */
*/*  FUNCTION : CALLED TO COMPARE A NEW DVT WITH AN EXISTING DVT OF  */
*/*             THE SAME LENGTH ON THE ATCVT CHAIN OF DVTS.  IF A    */
*/*             DUPLICATION IS FOUND THE NEWLY CREATED DVT IS        */
*/*             RELEASED AND THE USE COUNT IN THE CHAINED DVT IS     */
*/*             INCREMENTED.                                         */
*/*                                                                  */
*/*  INPUT : RDVTPTR - ADDRESS OF THE DVT ON THE ATCVT CHAIN         */
*/*          EPDVTPTR - POINTS TO THE NEWLY CREATED DVT OF THE       */
*/*                     SAME LENGTH.                                 */
*/*                                                                  */
*/*  OUTPUT : OCWDPDVT = ON IF THE DVTS ARE IDENTICAL                */
*/*           OCWDPDVT = OFF IF THE DVTS ARE NOT EQUAL               */
*/*                                                                  */
*/*  ROUTINES : NONE                                                 */
*/*                                                                  */
*/*  MACROS : FREESTOR - TO RELEASE THE NEWLY CREATED DVT WHEN A     */
*/*                      DUPLICATE EXISTS ON THE ATCVT CHAIN.        */
*/*                                                                  */
*/********************************************************************/
*DVTCOMP: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0790
DVTCOMP  DS    0H                                                  0791
*C14SAVE=CR14PTR;       /*SAVE R14 FOR RETURN*/                    0791
         LA    @05,624                                             0791
         ALR   @05,COCWPTR                                         0791
         L     @03,C14SVPTR(,@05)                                  0791
         ST    CR14PTR,C14SAVE(,@03)                               0791
*C14SVPTR=C14SVPTR+4;                                              0792
         AL    @03,@CF00038                                        0792
         ST    @03,C14SVPTR(,@05)                                  0792
*RFY ISTDVTE BASED(PROCPTR);                                       0793
* OCWCPARE = CON;       /* SWITCH FOR LOOP ON*/                    0794
* OCWDPDVT = COFF;      /* INIT DUPLICATE FLAG OFF*/               0795
         OI    OCWCPARE(COCWPTR),B'00100000'                       0795
         NI    OCWDPDVT(COCWPTR),B'11110111'                       0795
* PROCPTR = RDVTPTR +LENGTH(DVTHDR);  /* POINT TO 1RST CHN DVT ENTRY */
         LA    PROCPTR,24                                          0796
         ALR   PROCPTR,RDVTPTR                                     0796
* NEWDVPTR = EPTDVT;        /* PT TO 1RST ENTRY IN NEW DVT*/       0797
         L     NEWDVPTR,EPTDVT(,EPTEPTR)                           0797
* DO WHILE ( OCWCPARE = CON);                                      0798
         B     @DE00798                                            0798
@DL00798 DS    0H                                                  0799
*  IF DVTPROC = NEWPROC /* DVT ENTRIES EQUAL? */                   0799
*   THEN                                                           0799
         CLC   DVTPROC(4,PROCPTR),NEWPROC(NEWDVPTR)                0799
         BNE   @RF00799                                            0799
*    DO;                                                           0800
*     OCWDPDVT =CON;         /* TURN ON DUPLICATE DVT FLAG */      0801
         OI    OCWDPDVT(COCWPTR),B'00001000'                       0801
*     IF DVTEXITF = CON /* LAST ENTRY IN DVT'S?*/                  0802
*      THEN                                                        0802
         TM    DVTEXITF(PROCPTR),B'10000000'                       0802
         BNO   @RF00802                                            0802
*       OCWCPARE = COFF;     /* STOP LOOP */                       0803
         NI    OCWCPARE(COCWPTR),B'11011111'                       0803
*      ELSE                                                        0804
*       DO;                                                        0804
         B     @RC00802                                            0804
@RF00802 DS    0H                                                  0805
*                                 /* PT TO NEXT ENTRIES*/          0805
*         NEWDVPTR=NEWDVPTR+LENGTH(ISTDVTE);                       0805
         LA    @05,4                                               0805
         ALR   NEWDVPTR,@05                                        0805
*         PROCPTR=PROCPTR+LENGTH(ISTDVTE);                         0806
         ALR   PROCPTR,@05                                         0806
*        END;                                                      0807
*   END;                                                           0808
*   ELSE                                                           0809
*    DO;                                                           0809
         B     @RC00799                                            0809
@RF00799 DS    0H                                                  0810
*     OCWDPDVT = COFF;  /* DVT'S ARE NOT =*/                       0810
*     OCWCPARE = COFF;  /* STOP LOOPING*/                          0811
         NI    OCWDPDVT(COCWPTR),B'11010111'                       0811
*    END;                                                          0812
* END;                                                             0813
@RC00799 DS    0H                                                  0813
@DE00798 TM    OCWCPARE(COCWPTR),B'00100000'                       0813
         BO    @DL00798                                            0813
* IF OCWDPDVT = CON          /* DVT DUPLICATE */                   0814
*  THEN                                                            0814
         TM    OCWDPDVT(COCWPTR),B'00001000'                       0814
         BNO   @RF00814                                            0814
*   DO;                                                            0815
*    DVTHUCNT = DVTHUCNT + 1;     /* INCR DVT USE COUNT */         0816
         LA    @05,1                                               0816
         AH    @05,DVTHUCNT(,RDVTPTR)                              0816
         STH   @05,DVTHUCNT(,RDVTPTR)                              0816
*    /*FREESTOR SIZE(DVTHLEN)FETCHP(NO)AREA(EPDVTPTR)*/            0817
*     DO;                                                          0817
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */                  0818
*/* END OF ATBUILD */                                              0818
*    RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;                       0818
*    RFY ISTRPH BASED(GPR01P);                                     0819
*    GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,                         0820
*             GPR06P,GPR07P,GPR08P,GPR09P,                         0820
*             GPR10P,GPR11P,GPR12P,GPR13P)                         0820
*        SETS(RPHWORK);                                            0820
*                                                                  0820
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS
*    GPR00F = DVTHLEN;                                 /*SIZE IN REG0*/
         LH    GPR00F,DVTHLEN(,RDVTPTR)                            0821
*    DCL 1 CMZA0136 FIXED(31) STATIC LOCAL,                        0822
*          2 * FIXED(8) INIT(64),                    /* OPTION FLAGS */
*          2 * FIXED(8) INIT(96),                     /* STORAGE KEY */
*          2 * FIXED(8) INIT(241),                 /* SUBPOOL NUMBER */
*          2 * FIXED(8) INIT('01'X);                 /* REQUEST TYPE */
*    GPR03F = CMZA0136;                                            0823
         L     GPR03F,CMZA0136                                     0823
*    RFY (GPR04P) RSTD;                                            0824
*    GPR04P = EPDVTPTR;                   /*PUT FREEMAIN ADDR IN REG4*/
*                                                                  0825
         LR    GPR04P,EPDVTPTR                                     0825
*    DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR02P)) */                 0826
*      GPR02P = ATCLCPTR; /*SET UP ATCVT POINTER*/                 0827
         L     GPR02P,ATCLCPTR                                     0827
*    END; /*LISATCVT*/                                             0828
*    CALL GPR02P -> ATCGTSTR;                         /*CALL ISTORFSR*/
         L     @15,ATCGTSTR(,GPR02P)                               0829
         BALR  @14,@15                                             0829
*    RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P) UNRSTD;              0830
*    END ;                                          /*END OF FREESTOR*/
*  /*FREE STORAGE FOR DUPDVT DM19033*/                             0832
*    DVT@ = RDVTPTR+LENGTH(ISTDVT);    /* CHN DVT @ INTO TEMP SPACE */
         LA    @05,24                                              0832
         ALR   @05,RDVTPTR                                         0832
         ST    @05,DVT@(,COCWPTR)                                  0832
*                                 /* USED TO MOVE @ PORTION */     0833
*    EPTDVTA = RDVT@;   /* WILL NOT DISTROY FLAGS IN EPT*/         0833
         MVC   EPTDVTA(3,EPTEPTR),RDVT@(COCWPTR)                   0833
*    OCWCLEN = COFF;          /* STOP LOOP IN COMPLEN */           0834
         NI    OCWCLEN(COCWPTR),B'10111111'                        0834
*   END;                                                           0835
*  ELSE;                                                           0836
@RF00814 DS    0H                                                  0837
*C14SVPTR=C14SVPTR-4;        /*RESTORE R14 FOR RETURN*/            0837
         LA    @05,624                                             0837
         ALR   @05,COCWPTR                                         0837
         L     @03,C14SVPTR(,@05)                                  0837
         SL    @03,@CF00038                                        0837
         ST    @03,C14SVPTR(,@05)                                  0837
*CR14PTR = C14SAVE;                                                0838
         L     CR14PTR,C14SAVE(,@03)                               0838
*END DVTCOMP;                                                      0839
@EL00012 DS    0H                                                  0839
@EF00012 DS    0H                                                  0839
@ER00012 BR    @14                                                 0839
