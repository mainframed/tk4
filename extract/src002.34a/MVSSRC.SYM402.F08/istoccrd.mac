SVTM     TITLE 'ISTOCCRD - RDVTBLD                                     *
                        '
ISTOCCRD CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(8)                                              0001
         DC    C'ISTOCCRD'                                         0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
RDVTBLD  DS    0H                                                  0368
*                                                                  0368
*      DO;                                                         0369
*/***  CRRMAINT  PROCNM(ISTOCCRD)COMP(OC)FUNC(SUB)  ***/           0370
*      DCL CMZK0007 PTR(31) REG(1);                                0370
*      RFY (CMZK0007) RSTD;                                        0371
*                                                                  0371
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)  */                  0372
*      RFY ISTCNSTA;                                               0372
*      RFY ISTOCCRR;                                               0373
*/* END OF ATBUILD */                                              0374
*      /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                0374
*CMZK0007->RPHCRR->CRACRR->NOCCRD = '1'B;                          0374
         L     @02,RPHCRR(,CMZK0007)                               0374
         L     @02,CRACRR(,@02)                                    0374
         OI    NOCCRD(@02),B'10000000'                             0374
*      /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/               0375
*CMZK0007->RPHCRR->CRACRR->OCCCRT = '0'B;                          0375
         XC    OCCCRT+1(2,@02),OCCCRT+1(@02)                       0375
         MVI   OCCCRT(@02),B'00000000'                             0375
*CMZK0007->RPHCRR->CRACRR->COCCRD = '1'B;                          0376
         OI    COCCRD(@02),B'10000000'                             0376
*      END;                                                        0377
*     CSAVEPTR=CSAVEPTR+4*DIM(OCWSAVE1); /*SAVEAREA ADDR*/         0378
         AL    CSAVEPTR,@CF03024                                   0378
*     RTNCODE=ZERO;                                                0379
         SLR   RTNCODE,RTNCODE                                     0379
*     MDVTPTR=OCWMDVT;             /*SETUP MASTER DVT POINTER  */  0380
         L     MDVTPTR,OCWMDVT(,COCWPTR)                           0380
*     RFY R0 UNRSTD;                                               0381
*     RBLDCNT=ZERO;                 /*INITIALIZE BUILD COUNT        */
         SLR   RBLDCNT,RBLDCNT                                     0382
*     RFY I RSTD;                                                  0383
*/* THE ENTIRE MASTER DVT IS SCANNED TO CALCULATE THE NUMBER OF     */
*/* ENTRY POINT TABLE ENTRIES TO BE BUILT                           */
*     I=MDVTPTR+LENGTH(ISTSDVT)*SDVTOTAL;   /*SCAN DELIMITER        */
         LH    I,SDVTOTAL(,MDVTPTR)                                0384
         MH    I,@CH02093                                          0384
         ALR   I,MDVTPTR                                           0384
*     DO WHILE (MDVTPTR<I);         /*SCAN:                         */
         B     @DE00385                                            0385
@DL00385 DS    0H                                                  0386
*         IF SDVID='RDVT'           /*    DVT ENTRY?                */
*             THEN IF SDVCNT>ZERO   /*    MDLES TO INCLUDE?         */
         CLC   SDVID(4,MDVTPTR),@CC03250                           0386
         BNE   @RF00386                                            0386
*                 THEN RBLDCNT=RBLDCNT+1; /*YES-INCR. CNT           */
         LH    @02,SDVCNT(,MDVTPTR)                                0387
         LTR   @02,@02                                             0387
         BNP   @RF00387                                            0387
         AL    RBLDCNT,@CF00049                                    0388
*                 ELSE;                                            0389
@RF00387 DS    0H                                                  0390
*             ELSE;                                                0390
@RF00386 DS    0H                                                  0391
*         MDVTPTR=MDVTPTR+LENGTH(ISTSDVT); /*STEP TO NEXT ENTRY     */
         AL    MDVTPTR,@CF02093                                    0391
*     END;                          /*SCAN END                      */
*                                                                  0392
@DE00385 CR    MDVTPTR,I                                           0392
         BL    @DL00385                                            0392
*     MDVTPTR=OCWMDVT;             /*RESTORE MASTER DVT PTR        */
         L     MDVTPTR,OCWMDVT(,COCWPTR)                           0393
*     RFY I UNRSTD;                                                0394
*     IF RBLDCNT=ZERO               /*COUNT?                        */
*         THEN DO;                                                 0395
         LTR   RBLDCNT,RBLDCNT                                     0395
         BNZ   @RF00395                                            0395
*             OCWRC='08'X;          /*    NO-LOGIC ERROR            */
         MVI   OCWRC(COCWPTR),X'08'                                0397
*             OCWEC='4F'X;           /*       NO LOAD MODULES        */
         MVI   OCWEC(COCWPTR),X'4F'                                0398
*             RTNCODE=FOUR;         /*       SET UNSUCCESSFUL RTN   */
         LA    RTNCODE,4                                           0399
*             END;                                                 0400
*         ELSE DO;                  /*    YES-CALL ROUTINES TO BLD  */
         B     @RC00395                                            0401
@RF00395 DS    0H                                                  0402
*             CALL BLDREAL;         /*    EPT AND RDVTS             */
         BAL   @14,BLDREAL                                         0402
*             IF RTNCODE=ZERO       /*SET RETURN CODE DEPENDING     */
*                 THEN;             /*ON SUCCESS OF THE BUILD       */
         LTR   RTNCODE,RTNCODE                                     0403
         BZ    @RT00403                                            0403
*                 ELSE RTNCODE=FOUR;                               0405
         LA    RTNCODE,4                                           0405
*             END;                                                 0406
*                                                                  0406
@RT00403 DS    0H                                                  0407
*     CSAVEPTR=CSAVEPTR-4*DIM(OCWSAVE1); /*RESTORE SAVEAREA ADDR */
@RC00395 SL    CSAVEPTR,@CF03024                                   0407
*     GEN REFS(R0,CSAVEPTR,CR14PTR,R12); /*   RESTORE REGISTERS     */
          L   CR14PTR,12(CSAVEPTR)
          LM   R0,R12,20(CSAVEPTR)
          BR   CR14PTR
*/********************************************************************/
*/*                                                                  */
*/*  ROUTINE NAME : BLDREAL - REAL EPT BUILDER                       */
*/*                                                                  */
*/*  FUNCTION : GET CORE FOR ENTRY POINT TBL. INITIALIZE THE EPT     */
*/*             HEADER. CALL ROUTINES TO BUILD AND FILL THE EPT      */
*/*             AND THE REAL DVTS AND CHAIN THEM TO THE ATCVT        */
*/*                                                                  */
*/*  INPUT : RBLDCNT = NUMBER OF VALID ENTRIES (DVT POINTERS)        */
*/*                    IN THE ENTRY POINT TABLE                      */
*/*                                                                  */
*/*  OUTPUT : OCWRC, OCWEC, R15 ^= 0 WHEN TPLOCK WAS NOT SUCCESSFUL  */
*/*                                  OR GETSTORE UNSUCCESSFUL        */
*/*           OCWRC, OCWEC, R15 = 0  WHEN BUILDING OF EPT AND DVTS   */
*/*                                  WAS SUCCESSFUL                  */
*/*                                                                  */
*/*  ROUTINES : SPLTMSTR - CALLS EPTBLD WHICH LOOPS THRU MASTER DVT  */
*/*                        CALLING EPTFILL TO CREATE A DVT WHEN      */
*/*                        NECESSARY.FILLS IN LAST EPT ENTRY         */
*/*             DVTFILL - FILLS IN THE DVTS WITH ADDRESSES OF THE    */
*/*                       PROCESS MODULES.                           */
*/*             DVTSERCH - SEARCHES THE ATCVT CHAIN OF DVTS. IF A    */
*/*                        DUPLICATE IS FOUND THE DVT IS RELEASED    */
*/*                        AND THE USE COUNT IN THE CHAINED DVT      */
*/*                        IS INCREMENTED. IF NO DUPLICATE IS        */
*/*                        FOUND THE DVT IS CHAINED TO THE ATCVT     */
*/*                        CHAIN OF DVTS.                            */
*/*             EPTSERCH - SEARCHES THE ATCVT CHAIN OF EPTS. IF A    */
*/*                        DUPLICATE IS FOUND THE EPT IS RELEASED    */
*/*                        AND THE USE COUNT IN THE CHAINED EPT      */
*/*                        IS INCREMENTED. IF NO DUPLICATE IS        */
*/*                        FOUND THE EPT IS CHAINED TO THE ATCVT     */
*/*                        CHAIN OF EPTS.                            */
*/*             DONOSTOR - HANDLES NON ZERO RETURN CODE FROM GETSTOR */
*/*                                                                  */
*/*  MACROS : GETSTOR - TO OBTAIN STORAGE FOR THE EPT(ENTRY PT TBL)  */
*/*           TPLOCK & TPUNLOCK - TO PROVIDE PROTECTION TO THE ATCVT */
*/*                              EPT AND DVT CHAINS WHILE DVTSERCH   */
*/*                              AND EPTSERCH ARE PROCESSING.        */
*/*                                                                  */
*/********************************************************************/
*BLDREAL: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0409
         B     @PB00002                                            0409
BLDREAL  DS    0H                                                  0410
*     RFY R0 UNRSTD;                                               0410
*     C14SAVE=CR14PTR;                                             0411
         LA    @02,624                                             0411
         ALR   @02,COCWPTR                                         0411
         L     @11,C14SVPTR(,@02)                                  0411
         ST    CR14PTR,C14SAVE(,@11)                               0411
*     C14SVPTR=C14SVPTR+4;                                         0412
         AL    @11,@CF00038                                        0412
         ST    @11,C14SVPTR(,@02)                                  0412
*     RBLDCNT=RBLDCNT+1;             /*ADD 1 FOR LAST ZERO EPT ENTRY*/
         AL    RBLDCNT,@CF00049                                    0413
*     RFY I RSTD;                                                  0414
*     RFY ODOPTR RSTD ;                                            0415
*     ODOPTR = OCWODOA ;     /* GET ODO POINTER              @D31JKQG*/
         L     ODOPTR,OCWODOA(,COCWPTR)                            0416
*     I=LENGTH(ISTEPT);                                            0417
         LA    @11,12                                              0417
         LR    I,@11                                               0417
*     I=I+(RBLDCNT*LENGTH(ISTEPTE)); /*CALCULATE SIZE IN BYTES      */
         MR    @10,RBLDCNT                                         0418
         ALR   I,@11                                               0418
*     I = I + (ODOECNT*LENGTH(ODOENTRY))+LENGTH(ISTODOR); /* @D31JKQG*/
         SLR   @00,@00                                             0419
         IC    @00,ODOECNT(,ODOPTR)                                0419
         SLA   @00,2                                               0419
         ALR   @00,I                                               0419
         AL    @00,@CF01502                                        0419
         LR    I,@00                                               0419
*      /*GETSTOR SIZE(I)CLEAR(YES)FETCHP(NO)SAVE(YES)*/            0420
*       DO;                                                        0420
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */                  0421
*/* END OF ATBUILD */                                              0421
*      RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;                     0421
*      RFY ISTRPH BASED(GPR01P);                                   0422
