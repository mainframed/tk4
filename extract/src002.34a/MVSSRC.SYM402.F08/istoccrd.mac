         L     @15,ATCGTSTR(,GPR02P)                               0430
         BALR  @14,@15                                             0430
*      RFY (GPR00P,GPR01P,GPR02P,GPR03P) UNRSTD;                   0431
*      END ;                                         /*END OF GETSTOR*/
*   /*@XL03HHG*/                                                   0433
*                                  /* GET CORE FOR ENTRY POINT TABLE */
*     IF RTNCODE=ZERO                /* GETSTOR SUCCESSFUL?         */
*         THEN DO;                   /*    YES:                     */
         LTR   RTNCODE,RTNCODE                                     0433
         BNZ   @RF00433                                            0433
*             OCWEPTAB=RPHSRPRM;     /*     SETUP EPT POINTER       */
         L     @00,RPHSRPRM(,GPR01P)                               0435
         ST    @00,OCWEPTAB(,COCWPTR)                              0435
