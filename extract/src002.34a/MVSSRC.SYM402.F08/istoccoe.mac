         OI    COCCOE(@10),B'00010000'                             0649
*   END;                                                           0650
*   IF CSPRSC2F=COFF                /* IF RESOURCE 2 NAME NOT IN     */
*     THEN                          /* CON SVCS PARM LIST    @D32CKMP*/
         LA    @10,224                                             0651
         ALR   @10,OCWPTR                                          0651
         TM    CSPRSC2F(@10),B'00000010'                           0651
         BNZ   @RF00651                                            0651
