         ST    @09,SORTCID(,SORTPTR)                               0670
*       SORTPTR=SORTPTR+4;          /* PT TO NEXT SORT SLOT  @D32CKXY*/
         AH    SORTPTR,@CH00045                                    0671
*       OCWODNAS=OCWODNAS+LENGTH(ISTCSL);/* PT TO NXT LIST ENTRY   0672
*                                                            @D32CKXY*/
         LA    @10,12                                              0672
         AL    @10,OCWODNAS(,OCWPTR)                               0672
         ST    @10,OCWODNAS(,OCWPTR)                               0672
*       OCWODCT1=OCWODCT1+1;        /* INCREMENT LOOP CTR    @D32CKXY*/
         LA    @10,1                                               0673
         AH    @10,OCWODCT1(,OCWPTR)                               0673
         STH   @10,OCWODCT1(,OCWPTR)                               0673
*     END;                          /* END DO WHILE                  */
@DE00664 LH    @10,OCWDSTNO(,OCWPTR)                               0674
         CH    @10,OCWODCT1(,OCWPTR)                               0674
         BH    @DL00664                                            0674
*     RFY                                                          0675
*       SORTPTR UNRSTD;             /*                               */
*     RFY                                                          0676
*       ISTCSL BASED(ADDR(OCWDRDTE));/* RESTORE ORIG CSL LOC         */
*     IF OCWDSTNO=1 THEN                                           0677
         CH    @10,@CH00068                                        0677
         BNE   @RF00677                                            0677
*       OCWODNAS=ADDR(OCWCID);      /* PT TO SORT SLOT USED FOR SING
*                                      DEST REQUEST          @D32CKXY*/
         LA    @10,OCWCID(,OCWPTR)                                 0678
         ST    @10,OCWODNAS(,OCWPTR)                               0678
*     ELSE                                                         0679
*       ;                           /* PTR SETUP RT ALREADY  @D32CKXY*/
@RF00677 DS    0H                                                  0680
*   END DOBNSET;                    /*                       @D32CKXY*/
