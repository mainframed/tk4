*                                      DEST REQUEST          @D32CKXY*/
         B     @RC00659                                            0663
@RF00659 LA    SORTPTR,OCWCID(,OCWPTR)                             0663
*     DO WHILE OCWODCT1<OCWDSTNO;   /* PROCESS ALL DESTS     @D32CKXY*/
@RC00659 B     @DE00664                                            0664
@DL00664 DS    0H                                                  0665
*       IF CSLRDTEF=CON THEN                                       0665
         L     @10,OCWODNAS(,OCWPTR)                               0665
         TM    CSLRDTEF(@10),B'10000000'                           0665
         BNO   @RF00665                                            0665
*         DO;                       /* IF DEST OBTAINED      @D32CKXY*/
*           CSLRDTEB=CON;           /* TURN ON BIT INDICATING BIND 0667
*                                      FAILURE NCSPL SHOULD BE SENT
*                                      IF BIND IS NOT SENT   @D32CKXY*/
         OI    CSLRDTEB(@10),B'01000000'                           0667
*         END;                      /*                       @D32CKXY*/
*       ELSE                                                       0669
*         ;                         /*                       @D32CKXY*/
@RF00665 DS    0H                                                  0670
*       SORTPTR->SORTCID=SORTPTR->SORTCID&&SORTPTR->SORTCID;/* CLEAR
*                                      SORT SLOT             @D32CKXY*/
