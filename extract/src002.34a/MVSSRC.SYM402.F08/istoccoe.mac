         CH    CRETCODE,@CH00033                                   2425
         BNE   @RF02425                                            2425
*         OCWDRDTE=OCWODNAS->CSLRDEF;/* WITH HOBS SET        @D32CKXY*/
         L     @15,OCWODNAS(,OCWPTR)                               2426
         MVC   OCWDRDTE(4,OCWPTR),CSLRDEF(@15)                     2426
*       ELSE                                                       2427
