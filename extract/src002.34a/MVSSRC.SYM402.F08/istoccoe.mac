*/*  ROUTINE NAME: DOCDFND                                           */
*/*  FUNCTION:                                                       */
*/*       DOES A CIDCTL FIND TO OBTAIN THE DAF LOCK EXCLUSIVELY      */
*/*       AND SAVES THE FMCB AND DNCB PTRS IN THE OCW. IF THE        */
*/*       CIDCTL IS UNSUCCESSFUL THE DEST IS MARKED AS NOT           */
*/*       CONNECTED                                                  */
*/*  ROUTINES CALLED:                                                */
*/*       NONE                                                       */
*/*  MACROS USED:                                                    */
*/*       CIDCTL - OBTAIN DAF LOCK                                   */
*/********************************************************************/
*                                                                  2394
*   C14SAVE=CR14PTR;                /* SUBR RETURN           @D32CKXY*/
         LA    @15,624                                             2394
         ALR   @15,OCWPTR                                          2394
         L     @10,C14SVPTR(,@15)                                  2394
         ST    CR14PTR,C14SAVE(,@10)                               2394
*   C14SVPTR=C14SVPTR+4;            /*                       @D32CKXY*/
         AH    @10,@CH00045                                        2395
         ST    @10,C14SVPTR(,@15)                                  2395
*   IF OCWDSTNO>1 THEN                                             2396
         LH    @15,OCWDSTNO(,OCWPTR)                               2396
         CH    @15,@CH00068                                        2396
         BNH   @RF02396                                            2396
*     DO;                           /* IF A LIST REQUEST     @D32CKXY*/
*       RFY                                                        2398
*         SORTPTR RSTD;             /*                       @D32CKXY*/
*       SORTPTR=OCWODLST+(OCWDSTNO*LENGTH(ISTCSL))+OCWODCT1*4;/* PT
*                                      TO NTRY SORT SLOT     @D32CKXY*/
         MH    @15,@CH00033                                        2399
         AL    @15,OCWODLST(,OCWPTR)                               2399
         LH    SORTPTR,OCWODCT1(,OCWPTR)                           2399
         SLA   SORTPTR,2                                           2399
         ALR   @15,SORTPTR                                         2399
         LR    SORTPTR,@15                                         2399
*       OCWCID=SORTPTR->SORTCID;    /* ESTABLISH OCW'S CID   @D32CKXY*/
         MVC   OCWCID(4,OCWPTR),SORTCID(SORTPTR)                   2400
*       RFY                                                        2401
*         SORTPTR UNRSTD;           /*                       @D32CKXY*/
*     END;                                                         2402
*   ELSE                                                           2403
*     ;                             /* CID IS IN OCW         @D32CKXY*/
@RF02396 DS    0H                                                  2404
*   CPARMRG2=OCWCID;                /* SETUP PARM REG        @D32CKXY*/
         L     CPARMRG2,OCWCID(,OCWPTR)                            2404
*   DO;                             /* CIDCTL(FIND) LOCK(XCLUSIVE)   */
*     RFY                                                          2406
