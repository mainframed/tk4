*   RDTREG=ACDRDTE;                 /* SET UP APPL RDT BASE          */
         L     @11,ACDRDTE(,DEBREG)                                0467
         LR    RDTREG,@11                                          0467
*   IF RAPAPPWD^=BLANKS             /* IF PASSWD PROTECTED           */
*     THEN                                                         0468
         CLC   RAPAPPWD(8,RDTREG),@CB03848                         0468
         BE    @RF00468                                            0468
*     IF ACBPASSW^=0                /* USER SUPPLIED PASSWD          */
*       THEN                                                       0469
         L     @11,ACBPASSW(,ACBREG)                               0469
         LTR   @11,@11                                             0469
         BZ    @RF00469                                            0469
*       DO;                                                        0470
*         RFY                                                      0471
*           REG5 RSTD;                                             0471
*         REG5=ACBPASSW+1;          /* ACCOUNT FOR LNGTH BYTE        */
         AH    @11,@CH00051                                        0472
         LR    REG5,@11                                            0472
