         TM    RAPRSV10(RDTREG),B'10000000'                        0504
         BNO   @RF00504                                            0504
         L     @11,ACDTSKID(,DEBREG)                               0504
         L     @11,PSTTCBA(,@11)                                   0504
         L     @02,TCBJSCB(,@11)                                   0504
