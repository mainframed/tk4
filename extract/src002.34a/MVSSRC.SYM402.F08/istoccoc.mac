         BO    @RT00504                                            0504
         CLC   TCBJSTCB(4,@11),PSATOLD                             0504
         BNE   @RF00504                                            0504
@RT00504 DS    0H                                                  0505
*       DEBREG->ACTSOAPF='1'B;      /*                       @ZA08960*/
         OI    ACTSOAPF(DEBREG),B'00001000'                        0505
*     ELSE                                                         0506
