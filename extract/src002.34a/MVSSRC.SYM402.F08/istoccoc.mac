@RT00473 DS    0H                                                  0477
*       END;                                                       0477
*     ELSE                                                         0478
*       ACBERFLG='24'X;             /* BAD PASSWD ERROR CODE         */
         B     @RC00469                                            0478
@RF00469 MVI   ACBERFLG(ACBREG),X'24'                              0478
*   ELSE                                                           0479
*     ;                                                            0479
@RF00468 DS    0H                                                  0480
*   IF ACBERFLG='00'X               /* IF ACB COPY ERR CODE 0        */
*     THEN                                                         0480
@RC00468 CLI   ACBERFLG(ACBREG),X'00'                              0480
         BNE   @RF00480                                            0480
*     DO;                                                          0481
*       IF RAPAPPO=ON×RAPASPO=ON    /* IS THIS PGM OP?       @X50BHXL*/
*         THEN                      /*                       @X50BHXL*/
