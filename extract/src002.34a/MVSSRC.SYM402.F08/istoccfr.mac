SVTM     TITLE 'ISTOCCFR - FMCB RELEASE                                *
                        '
         ISEQ  73,80
ISTOCCFR CSECT ,                                                   0002
@MAINENT DS    0H                                                  0002
         USING *,@15                                               0002
         B     @PROLOG                                             0002
         DC    AL1(16)                                             0002
         DC    C'ISTOCCFR  78.051'                                 0002
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0002
         BALR  @12,0                                               0002
@PSTART  DS    0H                                                  0002
         USING @PSTART,@12                                         0002
*/*********************************************************************
*/*  INITIALIZE                                                       *
*/*   1. COMPONENT RECOVERY RECORD                                    *
*/*   2. RETURN CODES                                                 *
*/*   3. PTR TO NEXT AVAILABLE REG SAVE AREA                          *
*/*********************************************************************
* DO;                                                              0314
*/***  CRRMAINT  PROCNM(ISTOCCFR)FUNC(SUB)COMP(OC)  ***/           0315
* DCL CMZK0007 PTR(31) REG(1);                                     0315
* RFY (CMZK0007) RSTD;                                             0316
*                                                                  0316
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)  */                  0317
* RFY ISTZSAVE;                                                    0317
* RFY ISTPFCRR;                                                    0318
* RFY ISTCFCRR;                                                    0319
* RFY ISTZCRR ;                                                    0320
* RFY ISTASCRR;                                                    0321
* RFY ISTAPCRR;                                                    0322
* RFY ISTCRA  ;                                                    0323
* RFY ISTCNSTA;                                                    0324
* RFY ISTOCCRR;                                                    0325
*/* END OF ATBUILD */                                              0326
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0326
*CMZK0007->RPHCRR->CRACRR->NOCCFR = '1'B;                          0326
         L     @05,RPHCRR(,CMZK0007)                               0326
         L     @05,CRACRR(,@05)                                    0326
         OI    NOCCFR(@05),B'00000100'                             0326
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0327
*CMZK0007->RPHCRR->CRACRR->OCCCRT = '0'B;                          0327
         XC    OCCCRT+1(2,@05),OCCCRT+1(@05)                       0327
         MVI   OCCCRT(@05),B'00000000'                             0327
*CMZK0007->RPHCRR->CRACRR->COCCFR = '1'B;                          0328
         OI    COCCFR(@05),B'00000100'                             0328
* END;                                                             0329
*R13 = R13 + OCWSLEN;                    /* NXT AVAIL SAVE AREA PTR */
         SLR   @05,@05                                             0330
         IC    @05,OCWSLEN(,R4)                                    0330
         ALR   R13,@05                                             0330
*OCWRC = '00'X;                          /* INIT OCW RTRN CDE */   0331
         MVI   OCWRC(R4),X'00'                                     0331
*OCWEC = '00'X;                          /* INIT OCW ERROR CDE */  0332
         MVI   OCWEC(R4),X'00'                                     0332
*/*********************************************************************
*/*  1. REMOVE THE FMCB FROM THE ACDEB-FMCB CHAIN & THE NCB-FMCB CHAIN*
*/*  2. REMOVE THE NCB FROM THE VTAM CTRL BLOCK STRUCTURE             *
*/*  3. IF NO ERRORS OCCURRED WHILE BREAKING THE CHAINS               *
*/*       THEN RELEASE THE FMCB & NCB CORE                            *
*/*       ELSE A SYSTEM ERROR OCCURRED AND NOTHING MORE CAN BE DONE   *
*/*            WITH THE FMCB & NCB                                    *
*/*********************************************************************
*  /*SEGMENT(DOUNLNK)REQUEST*/DO;GOTO DOUNLNK;DOUNLNK#:END;        0333
         B     DOUNLNK                                             0334
DOUNLNK# DS    0H                                                  0336
*/* UNCHAIN FMCB & NCB */                                          0336
*IF OCWRC = '00'X                        /* IF NO ERRORS OCCURRED */
*  THEN   /*SEGMENT(DOFREE)REQUEST*/DO;GOTO DOFREE;DOFREE#:END;       /
* FREE FMCB & NCB CORE */                                          0336
         CLI   OCWRC(R4),X'00'                                     0336
         BNE   @RF00336                                            0336
         B     DOFREE                                              0338
*  ELSE;                                                           0340
@RF00336 DS    0H                                                  0341
*/*********************************************************************
*/*  PREPARE TO RETURN TO CALLER                                      *
*/*   1. CLEAR THE OCW CID AREA TO ZERO                               *
*/*   2. DECREMENT THE REG SAVE AREA PTR TO POINT TO FR'S SAVE AREA   *
*/*   RETURN TO CALLER WITH RTRN CODE IN REG.15                       *
*/*********************************************************************
*OCWCID = 0;                             /* CLEAR OCW CID AREA */  0341
@RC00336 SLR   @05,@05                                             0341
         ST    @05,OCWCID(,R4)                                     0341
*R13 = R13 - OCWSLEN;                   /* FR REG SAVE AREA PTR */ 0342
         SLR   @05,@05                                             0342
         IC    @05,OCWSLEN(,R4)                                    0342
         SLR   R13,@05                                             0342
*RETURN    CODE(OCWRC);                   /* RETURN TO CALLER */   0343
         SLR   @15,@15                                             0343
         IC    @15,OCWRC(,R4)                                      0343
         L     @14,12(,@13)                                        0343
         LM    @00,@12,20(@13)                                     0343
         BR    @14                                                 0343
*  /*SEGMENT(DOUNLNK)START*/DO;GOTO DOUNLNK@;DOUNLNK:END;          0344
DOUNLNK  DS    0H                                                  0347
*/*********************************************************************
*/*  REMOVE THE FMCB FROM THE ACDEB-FMCB CHAIN                        *
*/*  IF AN FMCB CANNOT BE LOCATED THRU THE OCW                        *
*/*    THEN NO FURTHER FMCB PROCESSING IS NECESSARY                   *
*/*    ELSE REMOVE THE FMCB FROM THE ACDEB                            *
*/*        IF THE CHAIN IS EMPTY                                      *
*/*          THEN GO TAKE THE FMCB OFF THE THE NCB CHAIN              *
*/*          ELSE 1. LOCK THE DEB                                     *
*/*               2. IF DEB LOCKED SUCCESSFULLY                       *
*/*                    THEN 1. SCAN THE FMCB CHAIN UNTIL THE ADDR OF  *
*/*                            THE FMCB MATCHES THE FMCB PTR IN OCW   *
*/*                         2. REMOVE THIS FMCB FROM THE CHAIN        *
*/*                         3. UNLOCK THE DEB                         *
*/*                    ELSE SET RTRN CODE = SYSTEM ERROR BECAUSE      *
*/*                         DEB COULD NOT BE LOCKED                   *
*/*********************************************************************
*IF OCWFMCB = 0                         /* IF NO FMCB PRESENT */   0347
*  THEN;                                /* RETURN TO CALLER */     0347
         ICM   @05,15,OCWFMCB(R4)                                  0347
         BZ    @RT00347                                            0347
*  ELSE DO;                             /* UNCHAIN FMCB FROM ACDEB 0349
*                                          AND NCB CHAINS */       0349
*        IF FMCDEBA = 0                 /* IF FMCB NOT ON DEB CHAIN */
*          THEN;                        /* REMOVE FMCB FROM NCB CHAIN *
         L     @05,OCWFMCB(,R4)                                    0350
         ICM   @00,15,FMCDEBA(@05)                                 0350
         BZ    @RT00350                                            0350
*          ELSE DO;                     /* REMOVE FMCB FROM DEB CHAIN *
*                                                                  0352
*/*** START -TPLOCK (DEBLOCK)TYPE(XCLSIVE)- ***/                   0353
*                 DO;                                              0353
*                                                                  0353
*                 RFY ISTDYPAB;                                    0354
*                 RFY ISTATCVT;                                    0355
*                 DCL GPR05F FIXED(31) REG(05);                    0356
*                 DCL GPR05P PTR(31)   REG(05);                    0357
*                 DCL GPR06F FIXED(31) REG(06);                    0358
*                 DCL GPR06P PTR(31)   REG(06);                    0359
*                 DCL GPR07F FIXED(31) REG(07);                    0360
*                 DCL GPR07P PTR(31)   REG(07);                    0361
*                 DCL GPR08F FIXED(31) REG(08);                    0362
*                 DCL GPR08P PTR(31)   REG(08);                    0363
*                 DCL GPR09F FIXED(31) REG(09);                    0364
*                 DCL GPR09P PTR(31)   REG(09);                    0365
*                 DCL GPR10F FIXED(31) REG(10);                    0366
*                 DCL GPR10P PTR(31)   REG(10);                    0367
*                 DCL GPR11F FIXED(31) REG(11);                    0368
*                 DCL GPR11P PTR(31)   REG(11);                    0369
*                 DCL GPR12F FIXED(31) REG(12);                    0370
*                 DCL GPR12P PTR(31)   REG(12);                    0371
*                 DCL GPR13F FIXED(31) REG(13);                    0372
*                 DCL GPR13P PTR(31)   REG(13);                    0373
*                 DCL GPR14F FIXED(31) REG(14);                    0374
*                 DCL GPR14P PTR(31)   REG(14);                    0375
*                 DCL GPR00F FIXED(31) REG(00);                    0376
*                 DCL GPR00P PTR(31)   REG(00);                    0377
*                 DCL GPR01F FIXED(31) REG(01);                    0378
*                 DCL GPR01P PTR(31)   REG(01);                    0379
*                 DCL GPR02F FIXED(31) REG(02);                    0380
*                 DCL GPR02P PTR(31)   REG(02);                    0381
*                 DCL GPR03F FIXED(31) REG(03);                    0382
*                 DCL GPR03P PTR(31)   REG(03);                    0383
*                 DCL GPR04F FIXED(31) REG(04);                    0384
*                 DCL GPR04P PTR(31)   REG(04);                    0385
*                 DCL GPR15F FIXED(31) REG(15);                    0386
*                 DCL GPR15P PTR(31)   REG(15);                    0387
*                 RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P,GPR15P) RSTD;
*        GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(RPHWORK);
         STM GPR00P,GPR14P,RPHWORK(GPR01P)
*        GPR02P = ADDR(ACDLOCK);        /*SET PARAM - LOCKWORD PTR*/
         L     GPR02P,OCWACDEB(,R4)                                0390
         LA    GPR02P,ACDLOCK(,GPR02P)                             0390
*        GPR04P = GPR01P->RPHCRR;       /*SET PARAM - ISTCRA PTR*/ 0391
         L     GPR04P,RPHCRR(,GPR01P)                              0391
*        GPR00P = CRALMS05;             /*SET PARAM - LOCK LEVEL FLAG*/
         LA    GPR00P,16                                           0392
*        GPR03P = ADDR(GPR04P->CRAL5PTR);                          0393
         LA    GPR03P,CRAL5PTR(,GPR04P)                            0393
*                                       /*SET PARAM - LOCK LEVEL PTR*/
*        GPR15P=ATCLCPTR->ATCAP36;      /*ADDRESS OF LOCK ROUTINE*/
         L     @05,ATCLCPTR                                        0394
         L     GPR15P,ATCAP36(,@05)                                0394
*        DCL FMZL0012 BASED(GPR15P) LABEL VALUERANGE(*);           0395
*        CALL FMZL0012;                                            0396
         BALR  @14,@15                                             0396
*        GENERATE REFS(GPR00P,GPR01P,GPR14P,RPHWORK) SETS(GPR00P,GPR01P
*GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,GPR09P,GPR10P,GPR11P,
*PR12P,GPR13P,GPR14P,GPR15P);                                      0397
         LM GPR00P,GPR15P,RPHWORK(GPR01P)
*                 RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P,GPR15P) UNRST
*;       END;                                                      0398
*/*** END -TPLOCK (DEBLOCK)TYPE(XCLSIVE)- ***/  /* LOCK ACB DEB */ 0400
*                RFY RCREG RSTD;        /* RSTRCT REG.15 FOR RETCODE */
*                IF RCREG = 0           /* IF DEB IS LOCKED */     0401
*                  THEN DO;             /* DLTE FMCB FROM CHAIN */ 0401
         LTR   RCREG,RCREG                                         0401
         BNZ   @RF00401                                            0401
*                        RFY CFMCBPTR RSTD;  /* RSTRCT R11 - FMCB PTR *
*                        CFMCBPTR = FMCDEBA->ACDFMCB;  /* PTR TO FIRST
*                                                        FMCB ON DEB */
         L     @14,OCWFMCB(,R4)                                    0404
         L     @14,FMCDEBA(,@14)                                   0404
         L     CFMCBPTR,ACDFMCB(,@14)                              0404
*                        DO WHILE CFMCBPTR ^= 0;  /* SCAN DEB FMCB'S */
         B     @DE00405                                            0405
@DL00405 DS    0H                                                  0406
*                         IF OCWFMCB = CFMCBPTR                    0406
*                           THEN DO;    /* REMOVE FIRST FMCB ON CHAIN *
         L     @14,OCWFMCB(,R4)                                    0406
         CR    @14,CFMCBPTR                                        0406
         BNE   @RF00406                                            0406
*                                 ACDFMCB = FMCNXTCB;  /* UPDTE DEB PTR
*                                                        1ST FMCB */
         L     @10,OCWACDEB(,R4)                                   0408
         MVC   ACDFMCB(4,@10),FMCNXTCB(@14)                        0408
*                                 CFMCBPTR = 0;  /* END FMCB SCAN */
         SLR   CFMCBPTR,CFMCBPTR                                   0409
*                                END;                              0410
*                           ELSE DO;    /* LOOK AT NXT FMCB ON CHAIN */
         B     @RC00406                                            0411
@RF00406 DS    0H                                                  0412
*                                 IF CFMCBPTR ->FMCNXTCB = OCWFMCB 0412
*                                   THEN DO;  /* UNCHAIN FMCB */   0412
         L     @14,OCWFMCB(,R4)                                    0412
         C     @14,FMCNXTCB(,CFMCBPTR)                             0412
         BNE   @RF00412                                            0412
*                                         CFMCBPTR->FMCNXTCB       0414
*                                                     =FMCNXTCB;   0414
         MVC   FMCNXTCB(4,CFMCBPTR),FMCNXTCB(@14)                  0414
*                                         CFMCBPTR = 0;            0415
         SLR   CFMCBPTR,CFMCBPTR                                   0415
*                                        END;                      0416
*                                   ELSE DO;  /* POINT TO NXT FMCB */
         B     @RC00412                                            0417
@RF00412 DS    0H                                                  0418
*                                         CFMCBPTR = CFMCBPTR->FMCNXTCB
         L     CFMCBPTR,FMCNXTCB(,CFMCBPTR)                        0418
*                                        END;                      0419
*                                END;                              0420
@RC00412 DS    0H                                                  0421
*                        END;                                      0421
@RC00406 DS    0H                                                  0421
@DE00405 LTR   CFMCBPTR,CFMCBPTR                                   0421
         BNZ   @DL00405                                            0421
*                        RFY CFMCBPTR UNRSTD;  /* RLSE REG.11 */   0422
*                                                                  0422
*/*** START -TPUNLOCK (DEBLOCK)- ***/                              0423
*                         DO;                                      0423
*                                                                  0423
*/* ATBUILD (ISTRPH,ISTLOK,ISTCRA)  */                             0424
*/* END OF ATBUILD */                                              0424
*                         RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P,GPR15
*) RSTD; GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(RPHWORK);
         STM GPR00P,GPR15P,RPHWORK(GPR01P)
*        GPR02P = ADDR(ACDLOCK);        /*SET PARAM - LOCKWORD PTR*/
         L     GPR02P,OCWACDEB(,R4)                                0426
         LA    GPR02P,ACDLOCK(,GPR02P)                             0426
*        GPR04P = GPR01P->RPHCRR;       /*SET PARAM - ISTCRA PTR*/ 0427
         L     GPR04P,RPHCRR(,GPR01P)                              0427
*        GPR00P = CRALMS05;             /*SET PARAM - LOCK LEVEL FLAG*/
         LA    GPR00P,16                                           0428
*        GPR03P = ADDR(GPR04P->CRAL5PTR);                          0429
         LA    GPR03P,CRAL5PTR(,GPR04P)                            0429
*                                       /*SET PARAM - LOCK LEVEL PTR*/
*                                                                  0430
*                         DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */
*                           GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/
         L     GPR15P,ATCLCPTR                                     0431
*                         END; /*LISATCVT*/                        0432
*        GPR15P=GPR15P->ATCAP33;                                   0433
         L     GPR15P,ATCAP33(,GPR15P)                             0433
*        DCL FMZL0017 BASED(GPR15P) LABEL VALUERANGE(*);           0434
*        CALL FMZL0017;                                            0435
         BALR  @14,@15                                             0435
*        GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(GPR00P,GPR01P
*GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,GPR09P,GPR10P,GPR11P,
*PR12P,GPR13P,GPR14P,GPR15P);                                      0436
         LM GPR00P,GPR15P,RPHWORK(GPR01P)
*                         RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P,GPR15
*) UNRSTD;                                                         0437
*        END;                                                      0438
*/*** END -TPUNLOCK (DEBLOCK)- ***/   /* UNLOCK ACB DEB */         0439
*                        RFY RCREG RSTD; /* RSTRCT REG.15 FOR RETCODE *
*                       END;                                       0440
*                  ELSE DO;             /* NO DEB LOCK OBTAINED */ 0441
         B     @RC00401                                            0441
@RF00401 DS    0H                                                  0442
*                        OCWRC = '24'X;     /* R.C. = SYSTEM ERROR */
         MVI   OCWRC(R4),X'24'                                     0442
*                        OCWEC = '01'X;     /* E.C. = UNXPECTED RC */
         MVI   OCWEC(R4),X'01'                                     0443
*                       END;                                       0444
*                RFY RCREG UNRSTD;    /* RLSE REG.15 */            0445
@RC00401 DS    0H                                                  0446
*               END;                                               0446
*/*********************************************************************
*/*  REMOVE THE FMCB FROM THE NCB-FMCB CHAIN & TAKE THE NCB OUT OF    *
*/*   THE VTAM CTRL BLOCK STRUCTURE                                   *
*/*  IF NO ERROR OCCURRED TRYING TO BREAK THE ACDEB-FMCB CHAIN        *
*/*     AND THE FMCB IS NOT CHAINED TO THE NCB                        *
*/*    THEN NO MORE FMCB PROCESSING IS REQUIRED                       *
*/*    ELSE IF THE FMCB IS THE ONLY ONE ON THE CHAIN                  *
*/*           THEN 1. REMOVE THE NCB FROM THE VTAM CTRL BLOCK         *
*/*                   STRUCTURE WITH THE FMCB STILL ATTACHED          *
*/*                2. BREAK THE CHAIN BETWEEN THE FMCB & THE NCB      *
*/*           ELSE JUST REMOVE THE FMCB FROM THE NCB-FMCB CHAIN       *
*/*********************************************************************
*        IF OCWRC = '00'X               /* IF NO ERRORS OCCURRED */
*          THEN DO;                     /* REMOVE FMCB FROM NCB CHAIN *
@RT00350 CLI   OCWRC(R4),X'00'                                     0447
         BNE   @RF00447                                            0447
*                IF FMCDNCBA = 0         /* IF FMCB NOT ON CHAIN */
*                  THEN;                /* RETURN TO CALLER */     0449
         L     @05,OCWFMCB(,R4)                                    0449
         ICM   @00,15,FMCDNCBA(@05)                                0449
         BZ    @RT00449                                            0449
*                  ELSE DO;                                        0451
*                        IF NCBFMCBA = OCWFMCB  /* IF FMCB IS 1ST  0452
*                                                   ON CHAIN */    0452
*                          THEN DO;                                0452
         L     @05,OCWNCB(,R4)                                     0452
         L     @15,OCWFMCB(,R4)                                    0452
         C     @15,NCBFMCBA(,@05)                                  0452
         BNE   @RF00452                                            0452
*                                IF FMCFMCBA = 0  /* IF NO MORE ON 0454
*                                                     CHAIN */     0454
*                                  THEN DO;     /* DLTE NCB FROM NCB
*                                                  CHAIN */        0454
         ICM   @05,15,FMCFMCBA(@15)                                0454
         BNZ   @RF00454                                            0454
*                                          /*SEGMENT(DELNCB)REQUEST*/DO
*GOTO DELNCB;DELNCB#:END;                                          0457
         B     DELNCB                                              0457
DELNCB#  DS    0H                                                  0459
*                                       END;                       0459
*                                  ELSE;  /* JUST REMOVE FMCB */   0460
@RF00454 DS    0H                                                  0461
*                               END;                               0461
*                          ELSE;        /* JUST REMOVE FMCB */     0462
@RF00452 DS    0H                                                  0463
*                          /*SEGMENT(DELFMCB)REQUEST*/DO;GOTO DELFMCB;D
*LFMCB#:END;                                                       0463
         B     DELFMCB                                             0464
DELFMCB# DS    0H                                                  0466
*                       END;                                       0466
*               END;                                               0467
*          ELSE;                        /* RETURN TO CALLER */     0468
@RF00447 DS    0H                                                  0469
*        END;                                                      0469
@RC00447 DS    0H                                                  0470
*  /*SEGMENT(DOUNLNK)FINISH*/DO;GOTO DOUNLNK#;DOUNLNK@:END;        0470
@RT00347 DS    0H                                                  0471
         B     DOUNLNK#                                            0471
DOUNLNK@ DS    0H                                                  0473
*  /*SEGMENT(DELNCB)START*/DO;GOTO DELNCB@;DELNCB:END;             0473
         B     DELNCB@                                             0474
DELNCB   DS    0H                                                  0476
*/*********************************************************************
*/*  REMOVE THE NCB FROM THE VTAM CTRL BLOCK STRUCTURE                *
*/*  IF THE NCB IS THE RN THEN NO MORE PROCESSING REQUIRED            *
*/*    ELSE 1. LOCK THE DAF OF THE NCB'S HIGHER NODE TO PRESERVE      *
*/*            THE INTEGRITY OF THE CHAIN                             *
*/*         2. IF THE DAF CANNOT BE LOCKED                            *
*/*              THENSET RTRN CODE = SYSTEM ERROR                     *
*/*         IF THE DAF OF THE HIGHER OF THE NODE IS LOCKED            *
*/*            SUCCESSFULLY                                           *
*/*           THENSCAN THE NCB CHAIN LOOKING FOR A DAF THAT MATCHES   *
*/*                THE DAF OF THE NCB LOCATED THRU OCW                *
*/*             1. IF A MATCH IS FOUND                                *
*/*                  THEN  UPDATE THE NEXT NCB LOCATOR IN THE         *
*/*                        LOGICALLY PREVIOUS NCB & DECRMNT THE COUNT *
*/*                        OF THE # OF ACTIVE TRMNLS ATTACHED TO      *
*/*                        THE LOCKED NODE                            *
*/*             2. IF THE NCB BEING REMOVED IS AT THE SAME LEVEL AS   *
*/*                   AS THE LOGICALLY PREVIOUS NCB                   *
*/*                  THEN UPDATE THE NXT NCB(SAME LEVEL) LOCATOR      *
*/*                       IN THE LOGICALLY PREVIOUS NCB               *
*/*             3. IF A MATCH IS NOT FOUND                            *
*/*                  THEN LOCATE THE NXT NCB IN THE CHAIN & CNTINUE   *
*/*                       LOOPING UNTIL A MATCH IS FOUND OR THE       *
*/*                       END OF THE CHAIN IS FOUND                   *
*/*             4. RELEASE THE DAF LOCK                               *
*/*********************************************************************
*IF NCBNSNA = 0   /* IF NCB IS THE RN */                           0476
*  THEN;          /* NO MORE NCB PROCESSING REQUIRED */            0476
         L     @05,OCWNCB(,R4)                                     0476
         ICM   @00,15,NCBNSNA(@05)                                 0476
         BZ    @RT00476                                            0476
*  ELSE DO;       /* REMOVE IT FROM THE NCB CHAIN    */            0478
*OCWCID = 0;                            /* CLEAR CID AREA IN OCW */
         SLR   @05,@05                                             0479
         ST    @05,OCWCID(,R4)                                     0479
*OCWDAF = NCBNSNA->NCBDAF;              /* PUT DAF FOR NXT HIGHER  0480
*                                         NODE INTO CID */         0480
         L     @05,OCWNCB(,R4)                                     0480
         L     @05,NCBNSNA(,@05)                                   0480
         MVC   OCWDAF(2,R4),NCBDAF(@05)                            0480
*RFY CREG2 RSTD;                        /* RSTRCT REG.2 FOR CID */ 0481
*CREG2 = OCWCID;                        /* PUT CID IN REG.2 */     0482
*                                                                  0482
         L     CREG2,OCWCID(,R4)                                   0482
* DO; /* CIDCTL(FIND) LOCK(XCLUSIVE) */                            0483
*                                                                  0483
* RFY (EMCR2,EMCR3) RSTD;                                          0484
*   GEN SETS(RPHWORK)                                              0485
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      0485
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH
*   EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                            0486
         SLR   EMCR3,EMCR3                                         0486
*   EMCR3 = '02'X; /*INDICATE EXCLUSIVE LOCK*/                     0487
         LA    EMCR3,2                                             0487
*   CALL ATCLCPTR->ATCCDFND->ISTSDCCF; /*PROCESS REQUEST*/         0488
*                                                                  0488
         L     @05,ATCLCPTR                                        0488
         L     @15,ATCCDFND(,@05)                                  0488
         BALR  @14,@15                                             0488
* RFY (EMCR2,EMCR3) UNRSTD;                                        0489
* END; /*CIDCTL*/         /* LOCK HIGHER NODE DAF */               0490
