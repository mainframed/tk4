         SLR   @08,@08                                             0219
         ICM   @08,7,CCWADDRB(RWORK3)                              0219
         LR    RWORK3,@08                                          0219
*   IF DEBACCS = '0001'B THEN           /*IF READ BACK, CCW     YM7595*
         TM    DEBACCS(DEBPTR),B'00000001'                         0220
         BNO   @RF00220                                            0220
         TM    DEBACCS(DEBPTR),B'00001110'                         0220
         BNZ   @RF00220                                            0220
*     DO;                               /* BUFFER ADDR POINTS   YM7595*
