*    GPR00F = RWORK2; /* LENGTH REQUESTED*/                        0186
         LR    GPR00F,RWORK2                                       0186
*    GPR01F = 0; /* SP IN REG 1 */                                 0187
         SLR   GPR01F,GPR01F                                       0187
*    GEN   (SLL   GPR01F,24)                                       0188
*    NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;                  0188
         SLL   GPR01F,24
*    GPR00F = GPR00F × GPR01F; /* SP IN HI BYTE */                 0189
         OR    GPR00F,GPR01F                                       0189
*    GPR01F=ADDR(BCBLK); /*SET REGISTER ONE*/                      0190
         LR    GPR01F,BCBPTR                                       0190
*    SVC(10); /* R-FORM OF GETMAIN*/                               0191
         SVC   10                                                  0191
*    RESPECIFY (GPR01F,GPR00F) UNRESTRICTED;                       0192
*    END; /*FREEMAIN(R) LV(RWORK2)SP(0)A(BCBLK)*/                  0193
*                                                                  0193
*    DO; /*MODESET  EXTKEY(DATAMGT)*/                              0194
*      SPKA (80); /*SET PSW KEY*/                                  0195
         LA    @01,80                                              0195
         SPKA  0(@01)                                              0195
*    END; /*MODESET  EXTKEY(DATAMGT)*/         /*AUDIT TRAIL IN KEY 5
*     */                                                           0196
*                                                                  0196
* FCACBUFC = SWON;                      /*INDIC BUFFERS FREED         *
*                                                                  0197
         OI    FCACBUFC(RCORE),B'00100000'                         0197
*    DO; /*MODESET  KEYADDR(DXUKEY)*/                              0198
*      SPKA (DXUKEY); /*SET PSW KEY*/                              0199
         SLR   @01,@01                                             0199
         IC    @01,DXUKEY(,RCORE)                                  0199
         SPKA  0(@01)                                              0199
*    END; /*MODESET  KEYADDR(DXUKEY)*/                             0200
*                                                                  0200
* GOTO SAMIOB;                          /*BYPASS RELINKING UBFS YM7595*
