         SLR   RWORK2,RWORK2                                       0209
         IC    RWORK2,DCBOFFSR+52(,DCBPTR)                         0209
* IF DEBACCS = '1111'B THEN             /*TEST FOR OUTPUT       YM7595*
         TM    DEBACCS(DEBPTR),B'00001111'                         0210
         BNO   @RF00210                                            0210
*   RWORK2 = DCBOFFSW;                  /*GET OUTPUT OFFSET     YM7595*
         SLR   RWORK2,RWORK2                                       0211
         IC    RWORK2,DCBOFFSW+52(,DCBPTR)                         0211
