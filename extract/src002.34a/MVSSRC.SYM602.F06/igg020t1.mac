*    END; /*FREEMAIN(R) LV(LRILNGTH)SP(0)A(LRIAREA)*/              0164
*                                                                  0164
*    DO; /*MODESET  EXTKEY(DATAMGT)*/                              0165
*      SPKA (80); /*SET PSW KEY*/                                  0166
         LA    @01,80                                              0166
         SPKA  0(@01)                                              0166
*    END; /*MODESET  EXTKEY(DATAMGT)*/          /*AUDIT TRAIL IN KEY 5
*      */                                                          0167
