*                                                                  0168
         OI    FCACRECA(RCORE),B'01000000'                         0168
*    DO; /*MODESET  KEYADDR(DXUKEY)*/                              0169
*      SPKA (DXUKEY); /*SET PSW KEY*/                              0170
         SLR   @01,@01                                             0170
         IC    @01,DXUKEY(,RCORE)                                  0170
         SPKA  0(@01)                                              0170
*    END; /*MODESET  KEYADDR(DXUKEY)*/                             0171
*                                                                  0171
*   RFY RECARPTR UNRSTD;                                           0172
*   END;                                                           0173
* ELSE;                                                            0174
*                                                                  0174
@RF00150 DS    0H                                                  0175
*              /*******************************************************
*              /*THE NEXT CHECK IS TO SEE IF THE OPEN EXECUTORS       *
*              /*GETMAINED BUFFERS. IF YES, AND THE USER SPECIFIED    *
*              /*CONCATINATION OF UNLIKE ATTRIBUTES, THEY MUST BE     *
