*                                                                  0212
@RF00210 DS    0H                                                  0213
* RFY IOBPTR RSTD;                                                 0213
* IOBPTR = DCBIOBAB;                    /*GET ADDR AND CLEAR    YM7595*
         SLR   IOBPTR,IOBPTR                                       0214
         ICM   IOBPTR,7,DCBIOBAB+52(DCBPTR)                        0214
