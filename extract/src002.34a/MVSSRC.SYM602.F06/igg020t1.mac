*    NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;                  0159
         SLL   GPR01F,24
*    GPR00F = GPR00F × GPR01F; /* SP IN HI BYTE */                 0160
         OR    GPR00F,GPR01F                                       0160
*    GPR01F=ADDR(LRIAREA); /*SET REGISTER ONE*/                    0161
         LR    GPR01F,RECARPTR                                     0161
