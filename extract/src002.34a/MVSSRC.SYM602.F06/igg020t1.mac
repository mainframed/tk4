*                                                                  0142
* IF DCBCNQSM = SWOFF THEN              /*IF NOT QSAM GO FREE IOBS    *
         TM    DCBCNQSM+52(DCBPTR),B'00000001'                     0142
         BZ    @RT00142                                            0142
