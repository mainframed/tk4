*                                                                  0175
* IF DCBOFEOV = SWOFF ×                 /*TEST FOR CONCAT UNLIK YM7595*
*    FCAOBUF = SWOFF THEN               /*WAS A BUF POOL GOTTEN YM7595*
         TM    DCBOFEOV+40(DCBPTR),B'00100000'                     0175
         BZ    @RT00175                                            0175
         TM    FCAOBUF(RCORE),B'00100000'                          0175
         BZ    @RT00175                                            0175
