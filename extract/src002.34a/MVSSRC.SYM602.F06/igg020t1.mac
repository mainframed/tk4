         LH    RWORK5,DCBBLKSI+52(,DCBPTR)                         0222
*     RWORK3 = RWORK3 - RWORK5 - 1;     /* BUFFER               YM7595*
         LR    @08,RWORK3                                          0223
         SLR   @08,RWORK5                                          0223
         BCTR  @08,0                                               0223
         LR    RWORK3,@08                                          0223
