*                                       /*  BEEN UNCHAINED      YM7595*
* ELSE                                                             0205
*   IF FCAOBUF = SWON THEN              /*DID OPEN GET BUFFERS  YM7595*
         TM    FCAOBUF(RCORE),B'00100000'                          0205
         BO    @RT00205                                            0205
*                                       /*  THIS TIME           YM7595*
*     GOTO SAMBUFFR;                    /*YES, FREE BUFFERS     YM7595*
