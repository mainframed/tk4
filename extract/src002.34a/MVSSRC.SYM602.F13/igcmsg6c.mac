WTOX05   DC    B'0000000010100000'
         ORG   WTOX+9
         DS    CL3
SECN05B  DS    CL12
         ORG   SECN05B
@NM00005 DC    AL4(IHJ101I)
         DC    AL4(IHJ101I)
         DC    F'0'
         ORG   SECN05B+12
SECK05B  DS    CL12
         ORG   SECK05B
@NM00031 DC    AL4(IHJ102I)
         DC    AL4(IHJ102I)
         DC    F'0'
         ORG   SECK05B+12
