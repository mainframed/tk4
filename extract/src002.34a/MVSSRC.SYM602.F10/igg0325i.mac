         TITLE 'IGG0325I - VIO ALLOCATION                              *
                        '
IGG0325I CSECT ,                                                   0001
@PROLOG  BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
JSCBDISP EQU 188
JSCBAOS1 EQU  72
JSCBAOS2 EQU 192
K2       EQU   2                        CONSTANT OF 2            YM1110
K4       EQU   4                        CONSTANT OF 4            YM1110
K32      EQU   32                       CONSTANT OF 32
K38      EQU   38                       CONSTANT OF 38           YM1110
SCHEDKEY EQU   1                        PROTECT KEY OF SCHEDULER
HEXZERO  EQU   X'00'                    HEX CONSTANT OF 0
         USING ALLOCWKA,RALLOCWA        ALLOCATE WORK AREA
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZATION                                                */
*   /*                                                               */
*   /*****************************************************************/
*   AVJFCB = DUMYJFCB;                  /*COPY JFCB                  */
         MVC   AVJFCB(176),DUMYJFCB(JFCBPTR)                       0097
*   AVDEVTYP = DEVICTYP;                /*COPY DEVICE TYPE FIELD     */
         MVC   AVDEVTYP(4),DEVICTYP(R7)                            0098
*   JFCBPTR = ADDR(AVJFCB);             /*USE WORK AREA JFCB         */
         LA    JFCBPTR,AVJFCB                                      0099
*   GEN(MODESET EXTKEY=SCHED);          /*ASSUME KEY 1 TO CONTINUE   */
         MODESET EXTKEY=SCHED
*   /*****************************************************************/
*   /*                                                               */
*   /*  BUILD VDSCB                                                  */
*   /*                                                               */
*   /*****************************************************************/
*   /*                                                               */
*   /* GET CORE FOR VIRTUAL DSCB                                     */
*   /*                                                               */
*   RESPECIFY R9 RESTRICTED;                                       0101
*   RESPECIFY JSCBACTP RESTRICTED;                                 0102
*   JSCBACTP = JSCBACT;                 /*GET ADDR OF ACTIVE JSCB    */
         L     @08,CVTPTR                                          0103
         L     @08,CVTTCBP(,@08)                                   0103
         L     @08,TCBPTR(,@08)                                    0103
         L     @08,TCBJSCB(,@08)                                   0103
         L     JSCBACTP,JSCBACT(,@08)                              0103
*   RESPECIFY IEZJSCB BASED(JSCBACTP);                             0104
*   R9 = JSCBSWSP;                      /*GET SWA SUBPOOL NUMBER     */
         SLR   R9,R9                                               0105
         IC    R9,JSCBSWSP(,JSCBACTP)                              0105
*   RESPECIFY JSCBACTP UNRESTRICTED;                               0106
*   RESPECIFY IEZJSCB BASED(TCBJSCB);                              0107
*   RESPECIFY R1 RESTRICTED;                                       0108
*   /*GET VDSCB CORE                                                 */
*   R1 = LENGTH(VDSCB);                                            0109
         LA    R1,205                                              0109
*   GEN REFS (R1,R9,CVT);                                          0110
         IECRES GET,LV=(R1),SP=(R9),KEY=SCHEDKEY,PREFIX=NO,            X
               STM=(R2,RF,IECREGSV+12)
*   DEVICTYP = R1;                      /*ADDRESS OF VDSCB           */
         ST    R1,DEVICTYP(,R7)                                    0111
*   VDSCBPTR = R1;                      /*SET UP BASE FOR VDSCB      */
         LR    VDSCBPTR,R1                                         0112
*   DSMADTW3 = VDSCBPTR;                /*SAVE POINTER TO VDSCB      */
         ST    VDSCBPTR,DSMADTW3                                   0113
*   GEN(OI    DSMADTB1,DSMVIO);         /*SET VIO DATA SET SWITCH    */
         OI    DSMADTB1,DSMVIO
*   RESPECIFY (R1,R9) UNRESTRICTED;                                0115
*   /*                                                               */
*   /* GO TO ROUTINE TO BUILD THE VDSCB                              */
*   /*                                                               */
*   CALL BLDVDSCB;                                                 0116
         BAL   @14,BLDVDSCB                                        0116
*   /*****************************************************************/
*   /*                                                               */
*   /* MODULE TERMINATION                                            */
*   /*                                                               */
*   /*****************************************************************/
*   RFY RETCODE RSTD;                                              0117
*   RETCODE = ECNORM;                   /*RETURN CODE EQUALS ZERO    */
         SLR   RETCODE,RETCODE                                     0118
*   RFY RETCODE UNRSTD;                                            0119
*   GEN(MODESET EXTKEY=ZERO);           /*RETURN TO KEY 0      YM3171*/
         MODESET EXTKEY=ZERO
*   IF JFCBDQTY ^= 0 THEN               /*IF PDS, GO TO IGG0CLF2     */
         ICM   @06,7,JFCBDQTY(JFCBPTR)                             0121
         BZ    @RF00121                                            0121
*     DO;                                                          0122
*       GEN(ST    VDSCBPTR,DEB+K32);    /*PUT UCB ADDRESS IN DEB     */
         ST    VDSCBPTR,DEB+K32
*       RESPECIFY (R1,R2,R8) RESTRICTED;                           0124
*       R1 = JFCBDQTY;                  /*PASS DIRECTORY QUANTITY    */
         SLR   R1,R1                                               0125
         ICM   R1,7,JFCBDQTY(JFCBPTR)                              0125
*       R2 = AVDIRBT;                   /*PASS NO. DIR BLKS PER TRACK*/
         LH    R2,AVDIRBT                                          0126
*                                       /*PASS D/S START CCHH  YM1110*/
*       GEN(MVC   DEB+K38(K4),VDSDSCB+DS1EXT1+K2(VDSCBPTR)); /*YM1110*/
         MVC   DEB+K38(K4),VDSDSCB+DS1EXT1+K2(VDSCBPTR)
*       GEN(IECRES LOAD,EXTPR=(RALLOCWA),MODNM=AVCLF2,BRANCH=DIRECT)
*         REFS(CVT);                                               0128
         IECRES LOAD,EXTPR=(RALLOCWA),MODNM=AVCLF2,BRANCH=DIRECT
*     END;                                                         0129
*   RESPECIFY (R1,R2) UNRESTRICTED;                                0130
@RF00121 DS    0H                                                  0131
*   RFY RETCODE RSTD;                                              0131
*   R8 = RETCODE;                       /*SAVE RETURN CODE IN REG 8  */
         LR    R8,RETCODE                                          0132
*   RFY RETCODE UNRSTD;                                            0133
*FREEDSCB:                                                         0134
*   IF R8 ^= 0 THEN                     /*IF ERROR RETURN, FREE VDSCB*/
FREEDSCB LTR   R8,R8                                               0134
         BZ    @RF00134                                            0134
*     DO;                                                          0135
*       RFY R2 RSTD;                                               0136
*       R2 = LENGTH(VDSCB);                                  /*YM3952*/
         LA    R2,205                                              0137
*   RESPECIFY IEZJSCB BASED(TCBJSCB);                              0138
*   RESPECIFY (R9,JSCBACTP) RESTRICTED;                            0139
*   JSCBACTP = JSCBACT;                                            0140
         L     @03,CVTPTR                                          0140
         L     @03,CVTTCBP(,@03)                                   0140
         L     @03,TCBPTR(,@03)                                    0140
         L     @03,TCBJSCB(,@03)                                   0140
         L     JSCBACTP,JSCBACT(,@03)                              0140
*   RESPECIFY IEZJSCB BASED(JSCBACTP);                             0141
*   R9 = JSCBSWSP;                                                 0142
         SLR   R9,R9                                               0142
         IC    R9,JSCBSWSP(,JSCBACTP)                              0142
*   GEN REFS(CVT,RA,R2);                                     /*YM3952*/
         IECRES FREE,A=(RA),LV=(R2),PREFIX=NO,               /*YM3952*/*
               STM=(2,14,IECREGSV+12),SP=(R9),KEY=SCHEDKEY   /*YM3952*/
*   RESPECIFY (R2,R9,JSCBACTP) UNRESTRICTED;                       0144
*     END;                                                         0145
*   GEN(IECRES FREE,A=(RALLOCWA),PREFIX=FIRST)                     0146
*     REFS(CVT,RALLOCWA);                                          0146
@RF00134 DS    0H                                                  0146
         IECRES FREE,A=(RALLOCWA),PREFIX=FIRST
*   RESPECIFY RETCODE RESTRICTED;                                  0147
*   RETCODE = R8;                       /*PUT RETURN CODE IN REG 15  */
         LR    RETCODE,R8                                          0148
*   GEN(MODESET EXTKEY=ZERO);           /*RETURN TO KEY 0      YM3905*/
         MODESET EXTKEY=ZERO
*   GO TO CALLER;                       /*RETURN TO CALLER OF ALLOCAT*/
         L     @14,CVTPTR                                          0150
         L     @14,CVTEXPRO(,@14)                                  0150
         BR    @14                                                 0150
*   RESPECIFY (R8,RETCODE) UNRESTRICTED;                           0151
*   GEN(EJECT);                                                    0152
         EJECT
*BLDVDSCB:                                                         0153
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0153
         B     @PB00002                                            0153
BLDVDSCB DS    0H                                                  0154
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINE TO BUILD VDSCB                                        */
*   /*                                                               */
*   /*****************************************************************/
*   RFY RET RSTD;                                                  0154
*   SAV14B = RET;                       /*SAVE REGISTER 14           */
         ST    RET,SAV14B                                          0155
*   RFY RET UNRSTD;                                                0156
*   /*                                                               */
*   /* CALL ROUTINE TO BUILD VDSUCB                                  */
*   /*                                                               */
*   CALL BLDUCB;                                                   0157
         BAL   @14,BLDUCB                                          0157
*   /*                                                               */
*   /* CALL ROUTINE TO BUILD VDSDSCB                                 */
*   /*                                                               */
*   CALL BLDDSCB;                                                  0158
         BAL   @14,BLDDSCB                                         0158
*   /*                                                               */
*   /* ROUTINE TO BUILD VDSUCBEX (UCB EXTENSION)                     */
*   /*                                                               */
*   IF AVULABST = 0 THEN                /*CHECK IF USER LABEL TRACK  */
         ICM   @06,3,AVULABST                                      0159
         BNZ   @RF00159                                            0159
*     DO;                               /*WAS ALLOCATED              */
*       VDSABSTT = AVABSTRK;            /*FIRST ALLOCATED TRACK OF DS*/
         MVC   VDSABSTT(2,VDSCBPTR),AVABSTRK                       0161
*       VDSNMTRK = AVNODSTK;            /*NUMBER OF TRACKS ALLOCATED */
         MVC   VDSNMTRK(2,VDSCBPTR),AVNODSTK+2                     0162
*     END;                                                         0163
*   ELSE                                /*YES, USER LABEL TRK ALLOC  */
*     DO;                                                          0164
         B     @RC00159                                            0164
@RF00159 DS    0H                                                  0165
*       VDSABSTT = AVULABST;            /*FIRST ALLOCATE TRACK OF DS */
         MVC   VDSABSTT(2,VDSCBPTR),AVULABST                       0165
*       VDSNMTRK = AVNODSTK + 1;        /*NUMBER OF TRACKS ALLOCATED */
         LA    @06,1                                               0166
         AL    @06,AVNODSTK                                        0166
         STH   @06,VDSNMTRK(,VDSCBPTR)                             0166
*     END;                                                         0167
*   RFY RET RSTD;                                                  0168
@RC00159 DS    0H                                                  0169
*   RET = SAV14B;                       /*RESTORE REGISTER 14        */
         L     RET,SAV14B                                          0169
*   RFY RET UNRSTD;                                                0170
*   END BLDVDSCB;                                                  0171
@EL00002 DS    0H                                                  0171
@EF00002 DS    0H                                                  0171
@ER00002 BR    @14                                                 0171
@PB00002 DS    0H                                                  0172
*   GEN(EJECT);                                                    0172
         EJECT
*BLDDSCB:                                                          0173
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0173
         B     @PB00003                                            0173
BLDDSCB  DS    0H                                                  0174
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUTINE TO BUILD VDSDSCB                                      */
*   /*                                                               */
*   /*****************************************************************/
*   RFY RET RSTD;                                                  0174
*   SAV14C = RET;                       /*SAVE REGISTER 14           */
         ST    RET,SAV14C                                          0175
*   RFY RET UNRSTD;                                                0176
*   /*                                                               */
*   /* CALL ROUTINE TO DETERMINE STARTING CCHH                       */
*   /*                                                               */
*   CALL DETSCCHH;                                                 0177
         BAL   @14,DETSCCHH                                        0177
*   /*                                                               */
*   /* CALL ROUTINE TO INITIALIZE DS1LSTAR AND DS1TRBAL FIELDS       */
*   /*                                                               */
*   CALL INITLSTR;                                                 0178
         BAL   @14,INITLSTR                                        0178
*   /*                                                               */
*   /* CALL ROUTINE TO ALLOCATE TRACKS                               */
*   /*                                                               */
*   CALL ALLOCTRK;                                                 0179
         BAL   @14,ALLOCTRK                                        0179
*   /*****************************************************************/
*   /*                                                               */
*   /* VALIDITY CHECK SPACE REQUESTED                                */
*   /*                                                               */
*   /*****************************************************************/
*   IF AVLSTAR ^= 0 THEN                /*IF PDS ALLOCATED     YM8310*/
         LA    @06,80                                              0180
         ALR   @06,VDSCBPTR                                        0180
         ICM   @15,7,AVLSTAR(@06)                                  0180
         BZ    @RF00180                                            0180
*     IF AVTRKPRI < (AVLBPTRK + 1) THEN /*IF PDS DIRECTORY DOES NOT  */
         LA    @15,1                                               0181
         AH    @15,AVLBPTRK(,@06)                                  0181
         C     @15,AVTRKPRI                                        0181
         BNH   @RF00181                                            0181
*       DO;                             /*FIT IN PRIMARY SPACE -ERROR*/
*         RFY R8 RSTD;                                             0183
*         R8 = ECDIRPRI;                /*DIR SPACE > PRIMARY SPACE  */
         LA    R8,128                                              0184
*         GO TO FREEDSCB;               /*GO PERFORM ERROR PROCESSING*/
         B     FREEDSCB                                            0185
*       END;                                                       0186
*   AVVOLTRK = AVNUMCYL * AVNUMTRK - 1; /*TOTAL NO. TRACKS ON VOLUME */
@RF00181 DS    0H                                                  0187
@RF00180 LH    @09,AVNUMCYL                                        0187
         MH    @09,AVNUMTRK                                        0187
         BCTR  @09,0                                               0187
         STH   @09,AVVOLTRK                                        0187
*   AVAVAILT = AVVOLTRK - AVABSTRK + 1; /*NO. OF AVAILABLE TRACKS    */
         SH    @09,AVABSTRK                                        0188
         AH    @09,@CH00068                                        0188
         STH   @09,AVAVAILT                                        0188
*   IF AVTRKPRI > AVAVAILT THEN         /*IF NO. TRKS ALLOCATED TO DS*/
         C     @09,AVTRKPRI                                        0189
         BNL   @RF00189                                            0189
*     DO;                               /*DOESN'T FIT ON VOLUME-ERROR*/
*       R8 = ECNOSPAC;                  /*SPACE NOT AVAILABLE        */
         LA    R8,20                                               0191
*       GO TO FREEDSCB;                 /*GO PERFORM ERROR PROCESSING*/
         B     FREEDSCB                                            0192
*       RFY R8 UNRSTD;                                             0193
*     END;                                                         0194
*   IF AVNODSTK > AVAVAILT THEN         /*IF TOTAL DS SPACE > TOTAL  */
@RF00189 LH    @06,AVAVAILT                                        0195
         C     @06,AVNODSTK                                        0195
         BNL   @RF00195                                            0195
*     AVNODSTK = AVAVAILT;              /*VOL SPACE, GIVE DS TOT VOL */
         ST    @06,AVNODSTK                                        0196
*   /*                                                               */
*   /* CALL ROUTINE TO ALLOCATE A USER LABEL TRACK                   */
*   /*                                                               */
*   CALL ALLOCULT;                                                 0197
@RF00195 BAL   @14,ALLOCULT                                        0197
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD USER LABEL AND DATA SET EXTENTS                         */
*   /*                                                               */
*   /*****************************************************************/
*   RESPECIFY EXTPTR RESTRICTED;                                   0198
*   IF AVULABST ^= 0 THEN               /*IF USER LABEL TRK ALLOCATED*/
         LH    @08,AVULABST                                        0199
         LTR   @08,@08                                             0199
         BZ    @RF00199                                            0199
*     DO;                               /*BUILD USER LABEL EXTENT    */
*       EXTPTR = ADDR(DS1EXT1);         /*POINT TO FIRST EXTENT      */
         LA    EXTPTR,DS1EXT1+80(,VDSCBPTR)                        0201
*       VDS1EXTD = VDS1ULAB;            /*INDICATE USER LABELS       */
         MVI   VDS1EXTD(EXTPTR),B'01000000'                        0202
*       VDS1EXTN = VDS1HEX0;            /*SET EXTENT NUMBER TO 0     */
         MVI   VDS1EXTN(EXTPTR),X'00'                              0203
*       VDS1STCC = AVULABST / AVNUMTRK; /*STARTING CYLINDER NUMBER   */
         LH    @06,AVNUMTRK                                        0204
         LR    @00,@08                                             0204
         SRDA  @00,32                                              0204
         DR    @00,@06                                             0204
         STH   @01,VDS1STCC(,EXTPTR)                               0204
*       VDS1STHH = AVULABST // AVNUMTRK; /*STARTING HEAD NUMBER      */
         LR    @02,@08                                             0205
         SRDA  @02,32                                              0205
         DR    @02,@06                                             0205
         STH   @02,VDS1STHH(,EXTPTR)                               0205
*       VDS1ECC = VDS1STCC;             /*ENDING CYL = STARTING CYL  */
         STH   @01,VDS1ECC(,EXTPTR)                                0206
*       VDS1EHH = VDS1STHH;             /*ENDING HEAD = STARTING HEAD*/
         STH   @02,VDS1EHH(,EXTPTR)                                0207
*     END;                                                         0208
*   IF AVNODSTK ^= 0 THEN               /*IF TRACKS WERE ALLOCATED   */
@RF00199 ICM   @08,15,AVNODSTK                                     0209
         BZ    @RF00209                                            0209
*     CALL BLDDSEXT;                    /*GO BUILD DATA SET EXTENT   */
         BAL   @14,BLDDSEXT                                        0210
*   ELSE                                /*NO TRACKS WERE ALLOCATED   */
*     DS1NOEPV = 0;                     /*SET NO. EXTENTS TO 0       */
         B     @RC00209                                            0211
@RF00209 MVI   DS1NOEPV+80(VDSCBPTR),X'00'                         0211
*   RESPECIFY EXTPTR UNRESTRICTED;                                 0212
@RC00209 DS    0H                                                  0213
*   /*                                                               */
*   /* CALL ROUTINE TO PERFORM MERGE PROCESSING (JFCB TO DSCB)       */
*   /*                                                               */
*   CALL MRGJFCB;                                                  0213
         BAL   @14,MRGJFCB                                         0213
*   RFY RET RSTD;                                                  0214
*   RET = SAV14C;                       /*RESTORE REGISTER 14        */
         L     RET,SAV14C                                          0215
*   RFY RET UNRSTD;                                                0216
*   END BLDDSCB;                                                   0217
@EL00003 DS    0H                                                  0217
@EF00003 DS    0H                                                  0217
@ER00003 BR    @14                                                 0217
@PB00003 DS    0H                                                  0218
*   GEN(EJECT);                                                    0218
         EJECT
*DETSCCHH:                                                         0219
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0219
         B     @PB00004                                            0219
DETSCCHH DS    0H                                                  0220
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE THE BEGINNING CCHH FOR FIRST EXTENT                 */
*   /*                                                               */
*   /*****************************************************************/
*   RFY RET RSTD;                                                  0220
*   SAV14D = RET;                       /*SAVE REGISTER 14           */
         ST    RET,SAV14D                                          0221
*   RFY RET UNRSTD;                                                0222
*   /*                                                               */
*   /* CHECK FOR ABSOLUTE TRACK ALLOCATION                           */
*   /*                                                               */
*   IF JFCBSPAC = JFCABSTR THEN                                    0223
         TM    JFCBSPAC(JFCBPTR),B'11000000'                       0223
         BNZ   @RF00223                                            0223
*     DO;                                                          0224
*      AVCYLST = JFCBABST / AVNUMTRK;   /*STARTING CYLINDER NUMBER   */
         LH    @06,AVNUMTRK                                        0225
         LH    @15,JFCBABST(,JFCBPTR)                              0225
         LR    @00,@15                                             0225
         SRDA  @00,32                                              0225
         DR    @00,@06                                             0225
         STH   @01,AVCYLST                                         0225
*      AVHEADST = JFCBABST // AVNUMTRK; /*STARTING HEAD NUMBER       */
         LR    @00,@15                                             0226
         SRDA  @00,32                                              0226
         DR    @00,@06                                             0226
         STH   @00,AVHEADST                                        0226
*      AVABSTRK = JFCBABST;             /*ABSOLUTE TRACK NUMBER      */
         STH   @15,AVABSTRK                                        0227
*     END;                                                         0228
*   ELSE                                                           0229
*     /*                                                             */
*     /* CHECK FOR TRACK ALLOCATION                                  */
*     /*                                                             */
*     IF JFCBSPAC = JFCTRK THEN                                    0229
         B     @RC00223                                            0229
@RF00223 TM    JFCBSPAC(JFCBPTR),B'10000000'                       0229
         BNO   @RF00229                                            0229
         TM    JFCBSPAC(JFCBPTR),B'01000000'                       0229
         BNZ   @RF00229                                            0229
*       DO;                                                        0230
*         AVCYLST = 0;                  /*STARTING CYLINDER NUMBER   */
         SLR   @06,@06                                             0231
         STH   @06,AVCYLST                                         0231
*         AVHEADST = 1;                 /*STARTING HEAD NUMBER       */
         LA    @06,1                                               0232
         STH   @06,AVHEADST                                        0232
*         AVABSTRK = 1;                 /*ABSOLUTE TRACK NUMBER      */
         STH   @06,AVABSTRK                                        0233
*       END;                                                       0234
*     ELSE                                                         0235
*       /*                                                           */
*       /* CHECK FOR CYLINDER OR AVERAGE BLOCK LENGTH                */
*       /* WITH ROUND ALLOCATION                                     */
*       /*                                                           */
*       IF JFCBSPAC = JFCCYLIN ×                                   0235
*          (JFCBSPAC = JFCAVBL & JFCROUND = ON) THEN               0235
         B     @RC00229                                            0235
@RF00229 TM    JFCBSPAC(JFCBPTR),B'11000000'                       0235
         BO    @RT00235                                            0235
         TM    JFCBSPAC(JFCBPTR),B'01000000'                       0235
         BNO   @RF00235                                            0235
         TM    JFCBSPAC(JFCBPTR),B'10000000'                       0235
         BNZ   @RF00235                                            0235
         TM    JFCROUND(JFCBPTR),B'00000001'                       0235
         BNO   @RF00235                                            0235
@RT00235 DS    0H                                                  0236
*         DO;                                                      0236
*           AVCYLST = 1;                /*STARTING CYLINDER NUMBER   */
         MVC   AVCYLST(2),@CH00068                                 0237
*           AVHEADST = 0;               /*STARTING HEAD NUMBER       */
         SLR   @06,@06                                             0238
         STH   @06,AVHEADST                                        0238
*           AVABSTRK = AVNUMTRK;        /*ABSOLUTE TRACK NUMBER      */
         MVC   AVABSTRK(2),AVNUMTRK                                0239
*         END;                                                     0240
*       ELSE                                                       0241
*         /*                                                         */
*         /* AVERAGE BLOCK LENGTH ALLOCATION                         */
*         /*                                                         */
*         DO;                                                      0241
         B     @RC00235                                            0241
@RF00235 DS    0H                                                  0242
*           AVCYLST = 0;                /*STARTING CYLINDER NUMBER   */
         SLR   @06,@06                                             0242
         STH   @06,AVCYLST                                         0242
*           AVHEADST = 1;               /*STARTING HEAD NUMBER       */
         LA    @06,1                                               0243
         STH   @06,AVHEADST                                        0243
*           AVABSTRK = 1;               /*ABSOLUTE TRACK NUMBER      */
         STH   @06,AVABSTRK                                        0244
*         END;                                                     0245
*   RFY RET RSTD;                                                  0246
@RC00235 DS    0H                                                  0246
@RC00229 DS    0H                                                  0246
@RC00223 DS    0H                                                  0247
*   RET = SAV14D;                       /*RESTORE REGISTER 14        */
         L     RET,SAV14D                                          0247
*   RFY RET UNRSTD;                                                0248
*   END DETSCCHH;                                                  0249
@EL00004 DS    0H                                                  0249
@EF00004 DS    0H                                                  0249
@ER00004 BR    @14                                                 0249
@PB00004 DS    0H                                                  0250
*   GEN(EJECT);                                                    0250
         EJECT
*INITLSTR:                                                         0251
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0251
         B     @PB00005                                            0251
INITLSTR DS    0H                                                  0252
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE DS1LSTAR AND DS1TRBAL FIELDS IN THE VDSDSCB        */
*   /*                                                               */
*   /*****************************************************************/
*   RFY RET RSTD;                                                  0252
*   SAV14D = RET;                       /*SAVE REGISTER 14           */
         ST    RET,SAV14D                                          0253
*   RFY RET UNRSTD;                                                0254
*   IF JFCBDQTY ^= 0 THEN               /*CHECK FOR PARTITION DATASET*/
         ICM   @06,7,JFCBDQTY(JFCBPTR)                             0255
         BZ    @RF00255                                            0255
*     DO;                                                          0256
*     /*                                                             */
*     /* PROCESS FOR PARTITIONED DATA SET                            */
*     /*                                                             */
*       IF (JFCSUL = ON & JFCSL = ON) THEN /*CHECK FOR USER LABELS   */
