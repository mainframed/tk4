         L     @15,CRACRR(,@15)                                    0588
         ST    RTNREG,ZCRSVF9(,@15)                                0588
*   RFY                                                            0589
*     RTNREG UNRSTD;                /* LET CMPLR USE REGISTER        */
*   RFY                                                            0590
*     ISTZIBUF BASED(PTRZIB);                                      0590
*   IF PTRLCP=0 THEN                /* IS THERE AN LCPB      @X50BHXF*/
         LTR   PTRLCP,PTRLCP                                       0591
         BNZ   @RF00591                                            0591
*     NEWFSBP=PTRFSB;               /* NEW = OLD             @X50BHXF*/
         LR    NEWFSBP,PTRFSB                                      0592
*   ELSE                            /*                       @X50BHXF*/
*     NEWFSBP=LCPFDBA&GATEMASK;     /* POINT TO FIRST (NEWEST) FSB 0593
*                                      OFF LCPB/LSCB                 */
         B     @RC00591                                            0593
