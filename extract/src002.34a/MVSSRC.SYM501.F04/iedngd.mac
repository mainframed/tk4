         TITLE 'IEDNGD, OUTPUT DATA NIR GENERATOR                      *
                        '
IEDNGD   CSECT ,                                                01S0001
@PROLOG  DS    0H                                               01S0002
*                                                         /* @Y17XAIK*/
*                                   /*
         USING *,RBASE                 MODULE ADDRESSABILITY
         DC    AL4(0)                  ELEMENT CHAIN
         DC    AL4(0)                  LINK FIELD
         DC    XL1'06'                 STCB VTO
         DC    AL3(*-1)                POINTER TO STCB VTO
*   RBASE=IEDQQPTR;                 /* SET MODULE BASE REG           */
         LR    RBASE,IEDQQPTR                                   02S0530
*   GEN REFS(RBASE) NOSETS;                                     02S0531
*                                                         /* @Y17XAIK*/
*                                   /*
IEDNGD   IEDHJN AFTERHJN,HJN            IDENTIFICATION MACRO
*   RFY                                                         02S0532
*     IEDQQPTR UNRSTD;              /* RELEASE REGISTER              */
*                                                               02S0533
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS: PROCESS SEGMENT - THIS SEGMENT IS THE TOP LEVEL      */
*   /* PROCESS SEGMENT FOR THIS MODULE.                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0533
*   IF RPARM->PRFTICON=ON THEN      /* IF BUFFER THEN                */
         TM    PRFTICON(RPARM),B'00001000'                      02S0533
         BNO   @RF00533                                         02S0533
*                                                               03S0534
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS.BUFFER: BUFFER INPUT SEGMENT.                       */
*     /*                                                             */
*     /***************************************************************/
*                                                               03S0534
*     DO;                                                       03S0534
*       RPRF=RPARM;                 /* SET BFR BASE REG              */
         LR    RPRF,RPARM                                       03S0535
*       RLCB=PRFLCB;                /* SET PLCB BASE REG             */
         L     RLCB,PRFLCB-1(,RPRF)                             03S0536
         LA    RLCB,0(,RLCB)                                    03S0536
*       RPARM=LCBTTCIN;             /* GET TNT INDEX                 */
         LH    RPARM,LCBTTCIN(,RLCB)                            03S0537
         N     RPARM,@CF02391                                   03S0537
*       CALL IEDQTNT;               /* GET TRM ADDRESS               */
         L     @15,AVTRNMPT(,RAVT)                              03S0538
         BALR  @14,@15                                          03S0538
*       RTRM=RPARM-LENGTH(IEDNTRM); /* SET TRM BASE REG              */
         LR    RTRM,RPARM                                       03S0539
         SL    RTRM,@CF02037                                    03S0539
*       IF TRMSNA=ON THEN           /* IF SNA RESOURCE THEN          */
         TM    TRMSNA(RTRM),B'00000010'                         03S0540
         BNO   @RF00540                                         03S0540
*         DO;                                                   03S0541
*           IF PRFSRCE=SAVTSCPT THEN/* IF OUTPUT BUFFER FROM SSCP MH
*                                      THEN                          */
         L     @10,AVTSAVTP(,RAVT)                              03S0542
         LH    @10,SAVTSCPT(,@10)                               03S0542
         N     @10,@CF02391                                     03S0542
         CH    @10,PRFSRCE(,RPRF)                               03S0542
         BNE   @RF00542                                         03S0542
*                                                               04S0543
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS.BUFFER.SNASSCP: SNA SSCP SEGMENT - THIS     */
*             /* SEGMENT PROCESSES OUTPUT FROM SSCP.                 */
*             /*                                                     */
*             /*******************************************************/
*                                                               04S0543
*             DO;                                               04S0543
*               LCBBFRSV=RPRF;      /* SET BFR FIRST IN SAVE CHAIN   */
         STCM  RPRF,7,LCBBFRSV(RLCB)                            04S0544
*               PRFLINK=CHAINEND;   /* SET BFR AS END OF CHAIN       */
         SLR   @10,@10                                          04S0545
         STCM  @10,7,PRFLINK(RPRF)                              04S0545
*               PRFLCB=0;           /* CLEAR PRFLCB FOR IEDXFMO 04S0546
*                                                            @YM03748*/
         STCM  @10,7,PRFLCB(RPRF)                               04S0546
*               PRFISIZE=LCBISZE;   /* SET RESERVE COUNT             */
         IC    @10,LCBISZE(,RLCB)                               04S0547
         STC   @10,PRFISIZE(,RPRF)                              04S0547
*               CALL IPXFMO;        /* POST BFR TO IEDXFMO           */
         BAL   @14,IPXFMO                                       04S0548
*             END;                                              04S0549
*           ELSE                                                03S0550
*             IF PRFPRI=PRIRESUM THEN/* IF RESUME BUFFER THEN IT IS
*                                      THE RESPONSE TO BID FROM 03S0550
*                                      IEDRSPIN TO RESUME LU REQUEST
*                                      PROCESSING                    */
         B     @RC00542                                         03S0550
@RF00542 CLI   PRFPRI(RPRF),X'E5'                               03S0550
         BNE   @RF00550                                         03S0550
*                                                               04S0551
*               /*****************************************************/
*               /*                                                   */
*               /* PROCESS.BUFFER.SNALURSM: SNA LU RESUME SEGMENT -  */
*               /* THIS SEGMENT PROCESSES THE RESPONSE TO BID FROM   */
*               /* IEDRSPIN AND RESUMES THE OUTPUT OPERATION         */
*               /* ACCORDINGLY.                                      */
*               /*                                                   */
*               /*****************************************************/
*                                                               04S0551
*               DO;                                             04S0551
*                 SVDBUFAD=RPRF;    /* SAVE BUFFER ADDRESS FOR LATER
*                                      FREEING               @Y17XAIK*/
         LR    SVDBUFAD,RPRF                                    04S0552
*                 IF SCBSLCTN=OFF   /* IF WE GOT A GOOD RESPONSE TO
*                                      BID                   @Y17XAIK*/
*                   THEN                                        04S0553
         L     @10,LCBSCBA-1(,RLCB)                             04S0553
         TM    SCBSLCTN(@10),B'10000000'                        04S0553
         BNZ   @RF00553                                         04S0553
*                   DO;             /* POST OUR MESSAGE ON TO IEDXFMO
*                                                            @Y17XAIK*/
*                     CALL IPSNRHST;/* SET RH FLAGS IN THE MESSAGE
*                                      BUFFERS               @Y17XAIK*/
         BAL   @14,IPSNRHST                                     04S0555
*                     CALL IPXFMO;  /* POST THE BUFFER(S) TO IEDXFMO
*                                                            @Y17XAIK*/
         BAL   @14,IPXFMO                                       04S0556
*                     LCBBFRSV=SVDBUFAD;/* MOVE THE SAVED BUFFER
*                                      ADDRESS               @Y17XAIK*/
         STCM  SVDBUFAD,7,LCBBFRSV(RLCB)                        04S0557
*                     SVDBUFAD->PRFLINK=CHAINEND;/* MAKE LAST BUFFER
*                                      ON CHAIN              @Y17XAIK*/
         SLR   @10,@10                                          04S0558
         STCM  @10,7,PRFLINK(SVDBUFAD)                          04S0558
*                     IF LGBOPACE=0&LGBPCINO=OFF&LCBEOMSG=OFF/* IF WE
*                                      ARE NOT PACING THIS SNA FLOW
*                                      AND PCI=(,A) AND THERE ARE
*                                      MORE BUFFERS ON THE QUEUE FOR
*                                      THIS MESSAGE          @YM06454*/
*                       THEN                                    04S0559
         L     @10,LCBDCBPT(,RLCB)                              04S0559
         CLI   LGBOPACE(@10),0                                  04S0559
         BNE   @RF00559                                         04S0559
         TM    LGBPCINO(@10),B'00000100'                        04S0559
         BNZ   @RF00559                                         04S0559
         TM    LCBEOMSG(RLCB),B'01000000'                       04S0559
         BNZ   @RF00559                                         04S0559
*                       CALL IPQFAPST;/* ..THEN POST IEDQFA TO GET
*                                      ADDITIONAL            @Y17XAIK
*                                      BUFFERS IN THE MESSAGE   04S0560
*                                                            @Y17XAIK*/
         BAL   @14,IPQFAPST                                     04S0560
*                     ELSE                                      04S0561
*                       ;           /*                       @Y17XAIK*/
@RF00559 DS    0H                                               04S0562
*                   END;            /*                       @Y17XAIK*/
*                 ELSE                                          04S0563
*                   DO;             /* ..ELSE WE GOT A BAD RESPONSE
*                                      TO BID-FREE UP THE    @Y17XAIK
*                                      BUFFERS WE HAVE SAVED AWAITING
*                                      BID RESPONSE          @Y17XAIK*/
         B     @RC00553                                         04S0563
@RF00553 DS    0H                                               04S0564
*                     RPRF=LCBBFRSV;/* SETUP BUFFER ADDRESSABILITY
*                                                            @Y17XAIK*/
         L     RPRF,LCBBFRSV-1(,RLCB)                           04S0564
         LA    RPRF,0(,RPRF)                                    04S0564
*                     IEDPF1=SVDBUFAD->IEDPF1;/* MOVE BUFFER PREFIX
*                                      FROM BID RSP          @Y17XAIK
*                                      TO MESSAGE BUFFER     @Y17XAIK*/
         LR    @10,RPRF                                         04S0565
         SL    @10,@CF00044                                     04S0565
         MVC   IEDPF1(8,@10),IEDPF1(SVDBUFAD)                   04S0565
*                     PRF1FORM=PRF1SQLI;/* SET FLAG SO THAT     04S0566
*                                      INTERROGATORS WILL    @Y17XAIK
*                                      KNOW THAT THE RU LENGTH IS IN
*                                      THE BUFFER PREFIX     @Y17XAIK*/
         OI    PRF1FORM(@10),B'00110000'                        04S0566
*                     PRF1RULN=LENGTH(NEGRSPRU);/* SETUP RU LENGTH
*                                      FOR MH USE            @Y17XAIK*/
         MVC   PRF1RULN(2,@10),@CH00152                         04S0567
*                     EVAL(RPRF+LENGTH(IEDQPRF)+PRFISIZE)->NEGRSPRU=
*                                   /*                       @Y17XAIK*/
*                         EVAL(SVDBUFAD+LENGTH(IEDQPRF)+(SVDBUFAD->
*                         PRFISIZE))/*                       @Y17XAIK*/
*                         ->NEGRSPRU;/* MOVE BID RESPONSE RU TO 04S0568
*                                      MESSAGE BUFFER        @Y17XAIK*/
         LA    @10,42                                           04S0568
         LR    @09,RPRF                                         04S0568
         ALR   @09,@10                                          04S0568
         SLR   @07,@07                                          04S0568
         IC    @07,PRFISIZE(,RPRF)                              04S0568
         ALR   @09,@07                                          04S0568
         ALR   @10,SVDBUFAD                                     04S0568
         SLR   @07,@07                                          04S0568
         IC    @07,PRFISIZE(,SVDBUFAD)                          04S0568
         ALR   @10,@07                                          04S0568
         MVC   NEGRSPRU(5,@09),NEGRSPRU(@10)                    04S0568
*                     CALL IPERRBFR;/* POST ERROR BUFFER TO THE MH
*                                                            @Y17XAIK*/
         BAL   @14,IPERRBFR                                     04S0569
*                     SVDBUFAD->PRFLINK=LCBBFRSV;/* SET BID RSP 04S0570
*                                      BUFFER POINTING TO    @Y17XAIK
*                                      REMAINING MESSAGE BUFFERS
*                                                            @Y17XAIK*/
         MVC   PRFLINK(3,SVDBUFAD),LCBBFRSV(RLCB)               04S0570
*                     LCBBFRSV=SVDBUFAD;/* PLACE BID RSP BUFFER AT
*                                      HEAD OF BUFFER        @Y17XAIK
*                                      CHAIN                 @Y17XAIK*/
         STCM  SVDBUFAD,7,LCBBFRSV(RLCB)                        04S0571
*                   END;            /*                       @Y17XAIK*/
*                 CALL IPFREBFR;    /* FREE REMAINING BUFFERS   04S0573
*                                                            @Y17XAIK*/
@RC00553 BAL   @14,IPFREBFR                                     04S0573
*               END;                                            04S0574
*             ELSE                  /* IT IS A REQUEST BUFFER   03S0575
*                                      OUTBOUND FOR AN LU            */
*                                                               04S0575
*               /*****************************************************/
*               /*                                                   */
*               /* PROCESS.BUFFER.SNALURQ: SNA LU REQUEST SEGMENT -  */
*               /* THIS SEGMENT PROCESSES OUTBOUND LU-LU REQUEST     */
*               /* BUFFERS FROM IEDQA4 AFTER THEY HAVE EXECUTED THE  */
*               /* OUTPUT MH.                                        */
*               /*                                                   */
*               /*****************************************************/
*                                                               04S0575
*               DO;                                             04S0575
         B     @RC00550                                         04S0575
@RF00550 DS    0H                                               04S0576
*                 RFY                                           04S0576
*                  (RUNITNUM,                                   04S0576
*                   RBFRNUM) RSTD;  /*                       @Y17XAIK*/
*                 RFY                                           04S0577
*                   IEDRH BASED(ADDR(PRF1RH));/*             @Y17XAIK*/
*                 PRFISIZE=LCBISZE; /* NUMBER OF RESERVES    @YM05981*/
         IC    @10,LCBISZE(,RLCB)                               04S0578
         STC   @10,PRFISIZE(,RPRF)                              04S0578
*                 IF PRFNHDRN=OFF   /* IF THE BUFFER WE WERE INVOKED
*                                      WITH IS A             @Y17XAIK
*                                      HEADER BUFFER         @Y17XAIK*/
*                   THEN                                        04S0579
         TM    PRFNHDRN(RPRF),B'00000001'                       04S0579
         BNZ   @RF00579                                         04S0579
*                   DO;             /* ..THEN DO NEW MESSAGE    04S0580
*                                      PROCESSING            @Y17XAIK*/
*                     LCBBFRSV=RPRF;/* MOVE BUFFER ADDRESS TO SAVE
*                                      CHAIN                 @Y17XAIK*/
         STCM  RPRF,7,LCBBFRSV(RLCB)                            04S0581
*                     PRFLINK=CHAINEND;/* MAKE LAST BUFFER ON THE
*                                      CHAIN                 @Y17XAIK*/
         SLR   @10,@10                                          04S0582
         STCM  @10,7,PRFLINK(RPRF)                              04S0582
*                     IF PRFCNCLN=ON/* IF THE MESSAGE WAS CANCELLED
*                                      IN THE MH BY          @Y17XAIK
*                                      EXECUTION OF CANCELMSG IN
*                                      OUTHDR                @Y17XAIK*/
*                       THEN                                    04S0583
         TM    PRFCNCLN(RPRF),B'10000000'                       04S0583
         BNO   @RF00583                                         04S0583
*                       DO;         /* ..THEN POST THE BUFFER TO THE
*                                      MH AS AN ERROR        @Y17XAIK
*                                      BUFFER                @Y17XAIK*/
*                         SCBSLCTN=ON;/* SET SELECTION ERROR @Y17XAIK*/
*                         SCBTRMLN=ON;/* SET TERMINAL ERROR  @Y17XAIK*/
         L     @10,LCBSCBA-1(,RLCB)                             04S0586
         OI    SCBSLCTN(@10),B'10010000'                        04S0586
*                         IEDPF1=''B;/* ZERO BUFFER PREFIX   @Y17XAIK*/
         LR    @10,RPRF                                         04S0587
         AL    @10,@CF02393                                     04S0587
         XC    IEDPF1(8,@10),IEDPF1(@10)                        04S0587
*                         CALL IPERRBFR;/* POST THE BUFFER TO THE MH
*                                      AS AN ERROR           @Y17XAIK
*                                      BUFFER                @Y17XAIK*/
         BAL   @14,IPERRBFR                                     04S0588
*                       END;        /*                       @Y17XAIK*/
*                     ELSE                                      04S0590
*                       DO;         /* ..ELSE THE MESSAGE WAS NOT
*                                      CANCELLED--CONTINUE   @Y17XAIK
*                                      PROCESSING THE HEADER BUFFER
*                                                            @Y17XAIK*/
         B     @RC00583                                         04S0590
@RF00583 DS    0H                                               04S0591
*                         RBFRNUM=0;/* INITIALIZE TO NO BUFFERS 04S0591
*                                      REQUESTED             @Y17XAIK*/
         SLR   RBFRNUM,RBFRNUM                                  04S0591
*                         RUNITNUM=1;/* INITIALIZE TO ONE UNIT PER
*                                      BUFFER REQUESTED      @Y17XAIK*/
         LA    RUNITNUM,1                                       04S0592
*                         IF LCBRTRRO=ON&(TRHBB=ON×LCBMHBID=ON)/* IF
*                                      A RESPONSE            @Y17XAIK
*                                      IS OWED TO RTR AND WE CAN SEND
*                                      IT(THE USER HAS SET BEGIN
*                                                            @Y17XAIK
*                                      BRACKET IN THE RH OR ELSE HAS
*                                      REQUESTED BID)        @Y17XAIK*/
*                           THEN                                04S0593
         TM    LCBRTRRO(RLCB),B'00000100'                       04S0593
         BNO   @RF00593                                         04S0593
         LR    @10,RPRF                                         04S0593
         AL    @10,@CF02394                                     04S0593
         TM    TRHBB-2(@10),B'10000000'                         04S0593
         BO    @RT00593                                         04S0593
         TM    LCBMHBID(RLCB),B'00000010'                       04S0593
         BNO   @RF00593                                         04S0593
@RT00593 DS    0H                                               04S0594
*                           RBFRNUM=RBFRNUM+1;/* ..THEN INCREMENT THE
*                                      NUMBER OF             @Y17XAIK
*                                      BUFFERS TO REQUEST    @Y17XAIK*/
         AL    RBFRNUM,@CF00059                                 04S0594
*                         ELSE                                  04S0595
*                           ;       /*                       @Y17XAIK*/
@RF00593 DS    0H                                               04S0596
*                         IF LCBMHBID=ON/* IF THE USER HAS REQUESTED
*                                      A BID                 @Y17XAIK*/
*                           THEN                                04S0596
         TM    LCBMHBID(RLCB),B'00000010'                       04S0596
         BNO   @RF00596                                         04S0596
*                           RBFRNUM=RBFRNUM+1;/* ..THEN INCREMENT THE
*                                      NUMBER OF             @Y17XAIK
*                                      BUFFERS TO REQUEST    @Y17XAIK*/
         AL    RBFRNUM,@CF00059                                 04S0597
*                         ELSE                                  04S0598
*                           ;       /*                       @Y17XAIK*/
@RF00596 DS    0H                                               04S0599
*                         IF RBFRNUM>0/* IF WE SHOULD SEND RTR AND/OR
*                                      BID                   @Y17XAIK*/
*                           THEN                                04S0599
         CL    RBFRNUM,@CF00607                                 04S0599
         BNH   @RF00599                                         04S0599
*                           CALL IPSNBRBP;/* ..THEN INVOKE THE  04S0600
*                                      PROCEDURE WHICH       @Y17XAIK
*                                      WILL ATTEMPT TO DO SO @Y17XAIK*/
         BAL   @14,IPSNBRBP                                     04S0600
*                         ELSE                                  04S0601
*                           DO;     /* ..ELSE POST THE HEADER BUFFER
*                                      TO OUTBOUND DFC       @Y17XAIK*/
         B     @RC00599                                         04S0601
@RF00599 DS    0H                                               04S0602
*                             CALL IPSNRHST;/* SET APPROPRIATE BITS
*                                      IN THE RH             @Y17XAIK*/
         BAL   @14,IPSNRHST                                     04S0602
*                             CALL IPXFMO;/* POST THE HEADER TO 04S0603
*                                      IEDXFMO               @Y17XAIK*/
         BAL   @14,IPXFMO                                       04S0603
*                             IF LGBOPACE=0&LGBPCINO=OFF&LCBEOMSG=OFF&
*                                 LCBRTRRO=OFF/* IF WE ARE NOT PACING
*                                      THIS SNA FLOW AND PCI=(,A) AND
*                                      WE HAVE NOT DEQUEUED THE LAST
*                                      BUFFER IN THE MESSAGE AND A
*                                      RESPONSE WAS NOT OWED TO RTR
*                                                            @YM06454*/
*                               THEN                            04S0604
         L     @10,LCBDCBPT(,RLCB)                              04S0604
         CLI   LGBOPACE(@10),0                                  04S0604
         BNE   @RF00604                                         04S0604
         TM    LGBPCINO(@10),B'00000100'                        04S0604
         BNZ   @RF00604                                         04S0604
         TM    LCBEOMSG(RLCB),B'01000000'                       04S0604
         BNZ   @RF00604                                         04S0604
         TM    LCBRTRRO(RLCB),B'00000100'                       04S0604
         BNZ   @RF00604                                         04S0604
*                               CALL IPQFAPST;/* ..THEN POST IEDQFA
*                                      TO GET MORE BUFFERS   @Y17XAIK
*                                      OF THE MESSAGE        @Y17XAIK*/
         BAL   @14,IPQFAPST                                     04S0605
*                             ELSE                              04S0606
*                               ;   /*                       @Y17XAIK*/
@RF00604 DS    0H                                               04S0607
*                           END;    /*                       @Y17XAIK*/
*                       END;        /*                       @Y17XAIK*/
*                   END;            /*                       @Y17XAIK*/
*                                                               04S0610
*                 /***************************************************/
*                 /*                                                 */
*                 /* THIS IS A CONTINUATION OF SEGMENT               */
*                 /* PROCESS.BUFFER.SNALURQ. THIS PORTION OF THE     */
*                 /* SEGMENT PROCESSES TEXT BUFFERS.         @Y17XAIK*/
*                 /*                                                 */
*                 /***************************************************/
*                                                               04S0610
*                 ELSE                                          04S0610
*                   DO;             /* ..ELSE PROCESS A TEXT BUFFER
*                                                            @Y17XAIK*/
         B     @RC00579                                         04S0610
@RF00579 DS    0H                                               04S0611
*                     IF SCBTRMLN=ON/* IF THE MESSAGE HAS BEEN  04S0611
*                                      CANCELLED DUE TO      @Y17XAIK
*                                      BID REJECT OR USER PURGE OR
*                                      USER CANCELLATION     @Y17XAIK*/
*                       THEN                                    04S0611
         L     @10,LCBSCBA-1(,RLCB)                             04S0611
         TM    SCBTRMLN(@10),B'00010000'                        04S0611
         BNO   @RF00611                                         04S0611
*                       DO;         /* ..THEN FREE THE BUFFER   04S0612
*                                                            @Y17XAIK*/
*                         LCBBFRSV=RPRF;/* MOVE THE BUFFER ADDRESS TO
*                                      THE SAVE CHAIN        @Y17XAIK*/
         STCM  RPRF,7,LCBBFRSV(RLCB)                            04S0613
*                         PRFLINK=CHAINEND;/* MAKE LAST BUFFER ON THE
*                                      CHAIN                 @Y17XAIK*/
         SLR   @10,@10                                          04S0614
         STCM  @10,7,PRFLINK(RPRF)                              04S0614
*                         CALL IPFREBFR;/* FREE THE BUFFER   @Y17XAIK*/
         BAL   @14,IPFREBFR                                     04S0615
*                       END;        /*                       @Y17XAIK*/
*                     ELSE                                      04S0617
*                       DO;         /* ..ELSE PROCESS THE STILL GOOD
*                                      TEXT BUFFER           @Y17XAIK*/
         B     @RC00611                                         04S0617
@RF00611 DS    0H                                               04S0618
*                         IF LCBBFRSV=CHAINEND/* IF THE BUFFER SAVE
*                                      CHAIN IS EMPTY        @Y17XAIK*/
*                           THEN                                04S0618
         L     @10,LCBBFRSV-1(,RLCB)                            04S0618
         LA    @10,0(,@10)                                      04S0618
         LTR   @10,@10                                          04S0618
         BNZ   @RF00618                                         04S0618
*                           DO;     /* ..THEN WE CAN POST THE BUFFER
*                                      TO IEDXFMO            @Y17XAIK*/
*                             LCBBFRSV=RPRF;/* MOVE THE BUFFER TO THE
*                                      SAVE CHAIN            @Y17XAIK*/
         STCM  RPRF,7,LCBBFRSV(RLCB)                            04S0620
*                             PRFLINK=CHAINEND;/* MAKE LAST BUFFER ON
*                                      THE CHAIN             @Y17XAIK*/
         SLR   @10,@10                                          04S0621
         STCM  @10,7,PRFLINK(RPRF)                              04S0621
*                             CALL IPSNRHST;/* SET APPROPRIATE BITS
*                                      IN THE RH             @Y17XAIK*/
         BAL   @14,IPSNRHST                                     04S0622
*                             CALL IPXFMO;/* POST THE BUFFER TO 04S0623
*                                      IEDXFMO               @Y17XAIK*/
         BAL   @14,IPXFMO                                       04S0623
*                           END;    /*                       @Y17XAIK*/
*                         ELSE                                  04S0625
*                           CALL IPCHNBFR;/* ..ELSE ADD THE BUFFER TO
*                                      THE SAVED             @Y17XAIK
*                                      CHAIN OF BUFFERS      @Y17XAIK*/
         B     @RC00618                                         04S0625
@RF00618 BAL   @14,IPCHNBFR                                     04S0625
*                       END;        /*                       @Y17XAIK*/
@RC00618 DS    0H                                               04S0627
*                   END;            /*                       @Y17XAIK*/
@RC00611 DS    0H                                               04S0628
*                 RFY                                           04S0628
*                  (RUNITNUM,                                   04S0628
*                   RBFRNUM) UNRSTD;/*                       @Y17XAIK*/
@RC00579 DS    0H                                               04S0629
*                 RFY                                           04S0629
*                   IEDRH BASED;    /*                       @Y17XAIK*/
*               END;                                            04S0630
*         END;                                                  03S0631
*       ELSE                        /* IT IS A BUFFER OUTBOUND TO A
*                                      BSC/SS TERMINAL               */
*                                                               04S0632
*         /***********************************************************/
*         /*                                                         */
*         /* PROCESS.BUFFER.BSWRITE: BISYNC/START STOP WRITE SEGMENT */
*         /* - THIS SEGMENT PROCESSES OUTPUT DATA BUFFERS FROM IEDQA4*/
*         /* FOR BISYNC/START STOP DEVICES AFTER THEY HAVE EXECUTED  */
*         /* THE OUTPUT MH.                                          */
*         /*                                                         */
*         /***********************************************************/
*                                                               04S0632
*         DO;                                                   04S0632
         B     @RC00540                                         04S0632
@RF00540 DS    0H                                               04S0633
*           RFY                                                 04S0633
*            (RUNITNUM,                                         04S0633
