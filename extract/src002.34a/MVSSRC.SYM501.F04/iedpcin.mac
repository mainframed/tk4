*/*                                                                  */
*/********************************************************************/
*                                                                  0667
*SNDERRSP:                                                         0667
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0667
SNDERRSP DS    0H                                                  0668
*   R14SV06=R14;                    /* SAVE RETURN REG               */
         ST    R14,R14SV06                                         0668
*/*                                                                  */
*                                                                  0669
*   IF TTHFTYPE=TTHFID1&            /* FID1 PIU AND...               */
*       TTHBBIU=ON&                 /* BEGIN BIU AND...              */
*       TTHDCF>=3&                  /* RH IS PRESENT AND...          */
*       TRHTYPE=OFF&                /* REQUEST                       */
*       (TRHDR1=ONTRHDR2=ON) THEN  /* REQUEST ASKING FOR A DEFINITE
*                                      OR EXCEPTION RESPONSE         */
         TM    TTHFTYPE(PIUSTART),B'00010000'                      0669
         BNO   @RF00669                                            0669
         TM    TTHFTYPE(PIUSTART),B'11100000'                      0669
         BNZ   @RF00669                                            0669
         TM    TTHBBIU(PIUSTART),B'00001000'                       0669
         BNO   @RF00669                                            0669
         CLC   TTHDCF(2,PIUSTART),@CH00065                         0669
         BL    @RF00669                                            0669
         LA    @09,10                                              0669
         ALR   @09,PIUSTART                                        0669
         TM    TRHTYPE(@09),B'10000000'                            0669
         BNZ   @RF00669                                            0669
         TM    TRHDR1(@09),B'10100000'                             0669
         BZ    @RF00669                                            0669
*     DO;                           /* YES                           */
*       IF PRF2TNTI=ON THEN         /* ARE OAF AND DAF TNT OFFSETS   */
         L     @09,IEDQPPTR                                        0671
         AL    @09,@CF01670                                        0671
         TM    PRF2TNTI(@09),B'10000000'                           0671
         BNO   @RF00671                                            0671
*         DO;                       /* YES                           */
*           PLTNTDAF=TTHDAF;        /* SETUP DAF TNT OFFSET FOR    0673
*                                      RSPBUILD ROUTINE              */
         MVC   PLTNTDAF(2),TTHDAF(PIUSTART)                        0673
*           CALL RSPBUILD;          /* BUILD AN ERROR RESPONSE       */
         BAL   @14,RSPBUILD                                        0674
*           CALL POSTPCO;           /* POST THE ERROR RSP TO PATH  0675
*                                      CONTROL OUTBOUND              */
         BAL   @14,POSTPCO                                         0675
*         END;                                                     0676
*       ELSE                        /* OAF AND DAF ARE NETWORK     0677
*                                      ADDRESSES                     */
*         DO;                                                      0677
         B     @RC00671                                            0677
@RF00671 DS    0H                                                  0678
*           RPARM=TTHOAF;           /* OAF NETWORK ADDRESS           */
         SLR   RPARM,RPARM                                         0678
         ICM   RPARM,3,TTHOAF(PIUSTART)                            0678
*           CALL IEDIAP03;          /* CONVERT THE OAF NETWORK     0679
*                                      ADDRESS TO ITS TNT OFFSET.  0679
*                                      THIS IS DONE TO VALIDITY CHECK
*                                      THE OAF.                      */
         L     @09,AVTSAVTP(,IEDQAPTR)                             0679
         L     @15,SAVTNAX(,@09)                                   0679
         BALR  @14,@15                                             0679
*           IF R15RC>0 THEN         /* IS THE OAF VALID              */
         LTR   R15RC,R15RC                                         0680
         BNP   @RF00680                                            0680
*             DO;                   /* YES-SET UP DAF TNT FOR      0681
*                                      RSPBUILD ROUTINE              */
*               RPARM=TTHDAF;       /* DAF NETWORK ADDRESS           */
         SLR   RPARM,RPARM                                         0682
         ICM   RPARM,3,TTHDAF(PIUSTART)                            0682
*               CALL IEDIAP03;      /* CONVERT DAF TO ITS TNT OFFSET */
         L     @09,AVTSAVTP(,IEDQAPTR)                             0683
         L     @15,SAVTNAX(,@09)                                   0683
         BALR  @14,@15                                             0683
*               IF R15RC>0 THEN     /* DAF CONVERSION SUCESSFUL      */
         LTR   R15RC,R15RC                                         0684
         BNP   @RF00684                                            0684
*                 PLTNTDAF=R15RC;   /* SET UP DAF TNT OFFSET         */
         STH   R15RC,PLTNTDAF                                      0685
*               ELSE                /* DAF CONVERSION NOT SUCESSFUL  */
*                 PLTNTDAF=0;       /* SET DAF TNT OFFSET TO 0       */
         B     @RC00684                                            0686
@RF00684 SLR   @09,@09                                             0686
         STH   @09,PLTNTDAF                                        0686
*/*                                                                  */
*                                                                  0687
*               CALL RSPBUILD;      /* BUILD AN ERROR RESPONSE       */
@RC00684 BAL   @14,RSPBUILD                                        0687
*               CALL POSTPCO;       /* POST IT TO PATH CONTROL     0688
*                                      OUTBOUND                      */
         BAL   @14,POSTPCO                                         0688
*             END;                                                 0689
*           ELSE                    /* INVALID OAF                   */
*             DO;                                                  0690
         B     @RC00680                                            0690
@RF00680 DS    0H                                                  0691
*               RPARM=CURPIUP;      /* ADDRESS OF PIU BUFFER         */
         L     RPARM,CURPIUP                                       0691
*               CALL DISCARD;       /* DISCARD PIU BUFFER            */
         BAL   @14,DISCARD                                         0692
*             END;                                                 0693
*         END;                                                     0694
*     END;                                                         0695
*   ELSE                                                           0696
*     DO;                                                          0696
         B     @RC00669                                            0696
@RF00669 DS    0H                                                  0697
*       RPARM=CURPIUP;              /* ADDRESS OF CURRENT PIU        */
         L     RPARM,CURPIUP                                       0697
