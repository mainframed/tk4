         SLR   @02,@02                                             0644
         IC    @02,PRFPIUO(,@09)                                   0644
         ALR   PIUSTART,@02                                        0644
*                   TTHEBIU=ON;     /* MARK PIU AS LAST BIU          */
         OI    TTHEBIU(PIUSTART),B'00000100'                       0645
