         BAL   @14,DISCARD                                         0374
*     END;                                                         0375
*                                                                  0375
*/*                                                                  */
*                                                                  0376
*   ASEGTH->TTHDCF=ASEGTH->TTHDCF+SAVEDCF;/* UPDATE DATA COUNT OF THE
*                                      ACCUMULATED SEGMENT           */
@RF00371 L     @10,ASEGTH                                          0376
         SLR   @09,@09                                             0376
         ICM   @09,3,TTHDCF(@10)                                   0376
