@RC00669 L     R14,R14SV06                                         0700
*   RETURN;                         /* RETURN TO CALLER              */
@EL00012 DS    0H                                                  0701
@EF00012 DS    0H                                                  0701
@ER00012 BR    @14                                                 0701
