@EF00001 DS    0H                                                  0345
@ER00001 BR    @14                                                 0345
COMBINE  DS    0H                                                  0346
*   R14SV10=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,R14SV10                                         0346
*/*                                                                  */
*                                                                  0347
*   UNITPTR=SIBRUSEG;               /* ADDR OF PARTIALLY ACCUMULATED
*                                      SEGMENT                       */
         L     @09,SIBRUSEG(,SIBPTR)                               0347
         ST    @09,UNITPTR                                         0347
*   DO WHILE(UNITPTR->PRFTIC^=INVALTIC);/* FIND LAST UNIT OF       0348
