*/*                                                                  */
*/* THIS ROUTINE DISCARDS ALL THE UNITS IN A PIU(BUFFER). IT POSTS   */
*/* THEM TO BUF  RETURN(IEDQGB).                                     */
*/*       INPUT= REG1 POINTS TO PIU TO BE DISCARDED                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0447
*DISCARD:                                                          0447
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0447
DISCARD  DS    0H                                                  0448
*   R14SV07=R14;                    /* SAVE RETURN REG               */
         ST    R14,R14SV07                                         0448
*/*                                                                  */
*                                                                  0449
*   RPARM->PRFNBUNT=0;              /* SET NUMBER OF UNITS = 0 (THIS
*                                      MEANS IEDQGB WILL CALCULATE 0449
*                                      THE NUMBER OF UNITS TO BE   0449
*                                      FREED)                        */
         MVI   PRFNBUNT(RPARM),X'00'                               0449
*   RPARM->PRFQCBA=ADDR(AVTBFRTB);  /* ADDR OF BUFFER RETURN QCB     */
         LA    @09,AVTBFRTB(,IEDQAPTR)                             0450
         STCM  @09,7,PRFQCBA(RPARM)                                0450
*   RPARM->PRFPRI=PRIBFRTB;         /* POST PRIORITY                 */
         MVI   PRFPRI(RPARM),X'E0'                                 0451
*   CALL DSPPOSTR;                  /* FREE UNITS                    */
         LA    @15,DSPPOSTR(,RDISP)                                0452
         BALR  @14,@15                                             0452
*/*                                                                  */
*                                                                  0453
*   R14=R14SV07;                    /* RESTORE RETURN REG            */
         L     R14,R14SV07                                         0453
*   RETURN;                         /* RETURN TO CALLER              */
@EL00004 DS    0H                                                  0454
@EF00004 DS    0H                                                  0454
@ER00004 BR    @14                                                 0454
