*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE DEBLOCKS A PIU FROM THE BTU.                        */
*/*    INPUT=NEXTPIUP(A PTR TO THE PIU TO BE DEBLOCKED)              */
*/*    OUTPUT=                                                       */
*/*            CURPIUP-PTR TO THE PIU WHICH JUST GOT DEBLOCKED       */
*/*            PRFTIC FIELD (OF PIU JUST DEBLOCKED)-CONTAINS         */
*/*                   INVALID TIC TO INDICATE LAST UNIT IN PIU       */
*/*            NEXTPIUP CONTAINS EITHER                              */
*/*             1) A POINTER TO THE NEXT PIU IN THE BTU (THE ONE     */
*/*                AFTER THE ONE JUST DEBLOCKED) OR                  */
*/*             2) 0 IF THERE ARE NO MORE PIUS IN THE BTU            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0456
*   RFY                                                            0456
*    (REG0,                                                        0456
*     R2) RSTD;                                                    0456
*GETPIU:                                                           0457
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0457
GETPIU   DS    0H                                                  0458
*   CURPIUP=NEXTPIUP;               /* SET CURRENT PIU PTR TO NEXT 0458
*                                      PIU PTR                       */
         L     @09,NEXTPIUP                                        0458
         ST    @09,CURPIUP                                         0458
*   PIUSTART=ADDR(PRFPIUO)+PRFPIUO; /* ADDRESS OF PIU(TH-RH-RU)      */
         L     @09,IEDQPPTR                                        0459
         LA    PIUSTART,PRFPIUO(,@09)                              0459
         SLR   @06,@06                                             0459
         IC    @06,PRFPIUO(,@09)                                   0459
         ALR   PIUSTART,@06                                        0459
*/*                                                                  */
*                                                                  0460
*   IEDPF2=IEDPF2&&IEDPF2;          /* ZERO OUT THE 8 BYTE PREFIX    */
         AL    @09,@CF01670                                        0460
         XC    IEDPF2(8,@09),IEDPF2(@09)                           0460
*/*                                                                  */
*/* CALCULATE THE NUMBER OF UNITS IN THE PIU TO BE DEBLOCKED         */
*                                                                  0461
*   R2=AVTKEYLE;                    /* TCAM BUFFER UNIT LENGTH       */
         LH    R2,AVTKEYLE(,IEDQAPTR)                              0461
*   REG0=0;                         /* ZERO LEFT PART OF DIVIDEND    */
         SLR   REG0,REG0                                           0462
*   RPARM=TTHDCF+LENGTH(IEDTH)+PRFPIUO;/* SET UP DIVIDEND            */
         LA    RPARM,10                                            0463
         SLR   @09,@09                                             0463
         ICM   @09,3,TTHDCF(PIUSTART)                              0463
         ALR   RPARM,@09                                           0463
         ALR   RPARM,@06                                           0463
*   GEN(DR  REG0,R2);               /* CALC MINIMUM # UNITS IN THIS
*                                      PIU                           */
         DR  REG0,R2
*/*AFTER DIVIDE REG0 CONTAINS REMAINDER AND RPARM CONTAINS QUOTIENT  */
*                                                                  0465
*   IF REG0^=0 THEN                 /* NON 0 REMAINDER               */
         LTR   REG0,REG0                                           0465
         BZ    @RF00465                                            0465
*     RPARM=RPARM+1;                /* ADD THE UNIT (CONTAINING THE
*                                      REMAINDER) TO THE COUNT       */
         AL    RPARM,@CF00059                                      0466
*   UNITPTR=CURPIUP;                /* SET UNIT PTR TO 1ST UNIT OF 0467
*                                      PIU BUFFER                    */
@RF00465 L     @09,CURPIUP                                         0467
         ST    @09,UNITPTR                                         0467
*   COUNTER=1;                      /* SET COUNTER TO 1              */
         MVC   COUNTER(2),@CH00059                                 0468
*   DO WHILE(COUNTER<RPARM);        /* FIND LAST UNIT IN PIU BUFFER  */
         B     @DE00469                                            0469
@DL00469 DS    0H                                                  0470
*     UNITPTR=UNITPTR->PRFTIC;      /* GET NEXT UNIT                 */
         L     @09,UNITPTR                                         0470
         L     @09,PRFTIC(,@09)                                    0470
         ST    @09,UNITPTR                                         0470
*     COUNTER=COUNTER+1;            /* INCREMENT COUNTER             */
         LH    @09,COUNTER                                         0471
         LA    @09,1(,@09)                                         0471
         STH   @09,COUNTER                                         0471
*   END;                                                           0472
*                                                                  0472
@DE00469 LH    @09,COUNTER                                         0472
         N     @09,@CF01667                                        0472
         CR    RPARM,@09                                           0472
         BH    @DL00469                                            0472
*/*                                                                  */
*                                                                  0473
*   IF UNITPTR->PRFTIC=INVALTIC THEN/* LAST PIU IN BTU?              */
         L     @09,UNITPTR                                         0473
         CLC   PRFTIC(4,@09),@CB01097                              0473
         BNE   @RF00473                                            0473
*     NEXTPIUP=0;                   /* YES - SET NEXT PIU POINTER=0  */
         SLR   @09,@09                                             0474
         ST    @09,NEXTPIUP                                        0474
*   ELSE                            /* NOT LAST PIU IN BTU           */
*     DO;                                                          0475
         B     @RC00473                                            0475
@RF00473 DS    0H                                                  0476
*       NEXTPIUP=UNITPTR->PRFTIC;   /* SET NEXT PIU PTR TO NEXT PIU
*                                      IN BTU                        */
         L     @09,UNITPTR                                         0476
         L     @06,PRFTIC(,@09)                                    0476
         ST    @06,NEXTPIUP                                        0476
*       UNITPTR->PRFTIC=INVALTIC;   /* SET LAST UNIT OF PIU JUST   0477
*                                      DEBLOCKED TO INVALID TIC      */
         MVC   PRFTIC(4,@09),@CB01097                              0477
*     END;                                                         0478
*                                                                  0478
*/*                                                                  */
*                                                                  0479
*   RETURN;                         /* RETURN TO CALLER              */
@EL00005 DS    0H                                                  0479
@EF00005 DS    0H                                                  0479
@ER00005 BR    @14                                                 0479
*   END GETPIU;                                                    0480
         B     @EL00005                                            0480
*   RFY                                                            0481
*    (REG0,                                                        0481
*     R2) UNRSTD;                                                  0481
*                                                                  0482
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE LOCATES THE CORRECT SIB FOR THIS OAF/DAF PAIR.      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0482
*GETSIB:                                                           0482
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0482
GETSIB   DS    0H                                                  0483
*   R14SV01=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,R14SV01                                         0483
*/*                                                                  */
*                                                                  0484
*   SIBPTR=TRMSIBPT;                /* GET 1ST SIB ADDRESS (NOTE-TTE
*                                      ADDRESS IS ALREADY SET UP)    */
         LR    @09,IEDQTPTR                                        0484
         AL    @09,@CF01671                                        0484
         SLR   SIBPTR,SIBPTR                                       0484
         ICM   SIBPTR,7,TRMSIBPT-7(@09)                            0484
*   FOUNDSW=OFF;                    /* INIT FOUND SWITCH TO OFF      */
         NI    FOUNDSW,B'01111111'                                 0485
*   DO WHILE(SIBPTR>0&FOUNDSW=OFF); /* SIB SEARCH LOOP               */
         B     @DE00486                                            0486
@DL00486 DS    0H                                                  0487
*     IF SIBINDEX=TTHDAF THEN       /* IS THIS THE SIB FOR THIS    0487
*                                      OAF/DAF PAIR?                 */
         CLC   SIBINDEX(2,SIBPTR),TTHDAF(PIUSTART)                 0487
         BNE   @RF00487                                            0487
*       FOUNDSW=ON;                 /* YES- SET 'FOUND' INDICATOR    */
         OI    FOUNDSW,B'10000000'                                 0488
*     ELSE                          /* NO                            */
*       SIBPTR=SIBCHAIN;            /* GET NEXT SIB IN CHAIN         */
         B     @RC00487                                            0489
@RF00487 L     SIBPTR,SIBCHAIN(,SIBPTR)                            0489
*   END;                            /* END OF LOOP                   */
@RC00487 DS    0H                                                  0490
@DE00486 LTR   SIBPTR,SIBPTR                                       0490
         BNP   @DC00486                                            0490
         TM    FOUNDSW,B'10000000'                                 0490
         BZ    @DL00486                                            0490
@DC00486 DS    0H                                                  0491
*/*                                                                  */
*                                                                  0491
*   R14=R14SV01;                    /* RESTORE RETURN ADDRESS        */
         L     R14,R14SV01                                         0491
*   RETURN;                         /* RETURN TO CALLER              */
@EL00006 DS    0H                                                  0492
@EF00006 DS    0H                                                  0492
@ER00006 BR    @14                                                 0492
*   END GETSIB;                                                    0493
*                                                                  0494
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE VALIDITY CHECKS THE OAF AND DAF IN THE TH.          */
*/* IT ALSO CONVERTS THE OAF AND DAF TO TNT OFFST VALUES.            */
*/* IF OAF AND DAF ARE VALID THEIR TNT OFFSETS ARE STORED IN THE     */
*/* TH.                                                              */
*/*                                                                  */
*/* THIS ROUTINE ALSO TRACES THE PIU IF REQUIRED.                    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0494
*OAFDAFCK:                                                         0494
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0494
OAFDAFCK DS    0H                                                  0495
*   R14SV03=R14;                    /* SAVE RETURN REG               */
         ST    R14,R14SV03                                         0495
*/*                                                                  */
*                                                                  0496
*   RC=BAD;                         /* INIT RETURN CODE TO BAD       */
         LA    RC,4                                                0496
*/* INITIALIZE THE TRACE PARAMETER LIST                              */
*                                                                  0497
*   PLPIUPTR=CURPIUP;               /* ADDR OF PIU TO BE TRACED      */
         L     @09,CURPIUP                                         0497
         ST    @09,PLPIUPTR                                        0497
*   PLTNTOAF=0;                     /* INIT OAF TNT OFFSET FIELD     */
         SLR   @09,@09                                             0498
         STH   @09,PLTNTOAF                                        0498
*   PLTNTDAF=0;                     /* INIT DAF TNT OFFSET FIELD     */
         STH   @09,PLTNTDAF                                        0499
*/*                                                                  */
*                                                                  0500
*   RPARM=TTHDAF;                   /* GET DAF NETWORK ADDRESS       */
         SLR   RPARM,RPARM                                         0500
         ICM   RPARM,3,TTHDAF(PIUSTART)                            0500
*   CALL IEDIAP03;                  /* CONVERT DAF TO TNT OFFSET R15
*                                      CONTAINS THE RETURN CODE      */
         L     @09,AVTSAVTP(,IEDQAPTR)                             0501
         L     @15,SAVTNAX(,@09)                                   0501
         BALR  @14,@15                                             0501
*   IF R15RC^=0 THEN                /* VALID DAF SUBAREA             */
         SLR   @09,@09                                             0502
         CR    R15RC,@09                                           0502
         BE    @RF00502                                            0502
*     DO;                           /* YES                           */
*       TEMPNA=TTHDAF;              /* MOVE DAF NETWORK ADDRESS TO 0504
*                                      TEMPORARY AREA                */
         MVC   TEMPNA(2),TTHDAF(PIUSTART)                          0504
*       TEMPNA1=TEMPNA1&SAVTSUBM;   /* 'AND' OFF ELEMENT PORTION OF
*                                      NETWORK ADDRESS               */
         L     @02,AVTSAVTP(,IEDQAPTR)                             0505
         SLR   @00,@00                                             0505
         IC    @00,TEMPNA1                                         0505
         SLR   @05,@05                                             0505
         IC    @05,SAVTSUBM(,@02)                                  0505
         NR    @00,@05                                             0505
         STC   @00,TEMPNA1                                         0505
*       IF TEMPNA1=SAVTSUBV THEN    /* DAF SUBAREA=TCAM SUBAREA      */
         CLC   TEMPNA1(1),SAVTSUBV(@02)                            0506
         BNE   @RF00506                                            0506
*         DO;                       /* YES                           */
*           IF R15RC>0 THEN         /* VALID DAF ELEMENT ADDRESS     */
         CR    R15RC,@09                                           0508
         BNH   @RF00508                                            0508
*             DO;                   /* YES                           */
*               PLTNTDAF=R15RC;     /* SAVE DAF TNT OFFSET VALUE     */
         STH   R15RC,PLTNTDAF                                      0510
*/*                                                                  */
*                                                                  0511
*               RPARM=TTHOAF;       /* GET OAF NETWORK ADDRESS       */
         SLR   @09,@09                                             0511
         ICM   @09,3,TTHOAF(PIUSTART)                              0511
         LR    RPARM,@09                                           0511
*               PLTNTNET=TTHOAF;    /* SAVE NETWORK ADDRESS  @YM09076*/
         STH   @09,PLTNTNET                                        0512
*               CALL IEDIAP03;      /* CONVERT OAF NETWORK ADDRESS TO
*                                      TNT OFFSET                    */
         L     @15,SAVTNAX(,@02)                                   0513
         BALR  @14,@15                                             0513
*               IF R15RC>0 THEN     /* VALID OAF NETWORK ADDRESS     */
         LTR   R15RC,R15RC                                         0514
         BNP   @RF00514                                            0514
*                 DO;               /* YES                           */
*                   PLTNTOAF=R15RC; /* PUT OAF TNT IN TRACE PARM LIST*/
         STH   R15RC,PLTNTOAF                                      0516
*                   RPARM=R15RC;    /* OAF TNT OFFSET                */
         LR    RPARM,R15RC                                         0517
*                   CALL IEDQTNT;   /* GET OAF TTE ADDRESS           */
         L     @15,AVTRNMPT(,IEDQAPTR)                             0518
         BALR  @14,@15                                             0518
*                   RTRM=RPARM;     /* TTE ADDRESSABILITY            */
         LR    RTRM,RPARM                                          0519
*/*                                                                  */
*                                                                  0520
*                   IF TRMTRACE=ON THEN/* IS TRACE ACTIVE FOR THIS 0520
*                                      TERMINAL                      */
         LR    @09,IEDQTPTR                                        0520
         AL    @09,@CF01673                                        0520
         TM    TRMTRACE-3(@09),B'00001000'                         0520
         BNO   @RF00520                                            0520
*                     CALL TRACE;   /* TRACE THE PIU                 */
         BAL   @14,TRACE                                           0521
*/*                                                                  */
*                                                                  0522
*                   TTHOAF=PLTNTOAF;/* MOVE OAF TNT OFFSET INTO TH   */
@RF00520 MVC   TTHOAF(2,PIUSTART),PLTNTOAF                         0522
*                   TTHDAF=PLTNTDAF;/* MOVE DAF TNT OFFSET INTO TH   */
         MVC   TTHDAF(2,PIUSTART),PLTNTDAF                         0523
*                   PRF2TNTI=ON;    /* SET 'TNT OFFSET' INDICATOR    */
         L     @09,IEDQPPTR                                        0524
         AL    @09,@CF01670                                        0524
         OI    PRF2TNTI(@09),B'10000000'                           0524
*                   RC=GOOD;        /* SET GOOD RETURN CODE          */
         SLR   RC,RC                                               0525
*                 END;                                             0526
*               ELSE                /* INVALID OAF NETWORK ADDRESS   */
*                 DO;                                              0527
         B     @RC00514                                            0527
@RF00514 DS    0H                                                  0528
*                   CALL TRACE;     /* TRACE THE PIU                 */
         BAL   @14,TRACE                                           0528
*                   RPARM=CURPIUP;  /* SET PTR TO CURRENT PIU        */
         L     RPARM,CURPIUP                                       0529
*                   CALL DISCARD;   /* DISCARD THE PIU               */
         BAL   @14,DISCARD                                         0530
*                 END;                                             0531
*             END;                                                 0532
*           ELSE                    /* INVALID DAF ELEMENT ADDRESS   */
*             DO;                   /* YES                           */
         B     @RC00508                                            0533
@RF00508 DS    0H                                                  0534
*               CALL TRACE;         /* TRACE PIU                     */
         BAL   @14,TRACE                                           0534
*               SNC=SNSNGDAF;       /* SET NO GOOD DAF SENSE         */
         MVC   SNC(4),@CB01562                                     0535
*               CALL SNDERRSP;      /* SEND -RSP                     */
         BAL   @14,SNDERRSP                                        0536
*             END;                                                 0537
*         END;                                                     0538
*       ELSE                        /* DAF SUBAREA NOT TCAM SUBAREA  */
*         DO;                                                      0539
         B     @RC00506                                            0539
@RF00506 DS    0H                                                  0540
*           CALL TRACE;             /* TRACE PIU                     */
         BAL   @14,TRACE                                           0540
*           CALL POSTPCO;           /* POST PIU TO PATH CONTROL    0541
*                                      OUTBOUND                      */
         BAL   @14,POSTPCO                                         0541
*         END;                                                     0542
*     END;                                                         0543
*   ELSE                            /* INVALID DAF SUBAREA           */
*     DO;                           /* YES                           */
         B     @RC00502                                            0544
@RF00502 DS    0H                                                  0545
*       CALL TRACE;                 /* TRACE PIU                     */
         BAL   @14,TRACE                                           0545
*       SNC=SNSNGDAF;               /* UNRECOGNIZED DAF SENSE        */
         MVC   SNC(4),@CB01562                                     0546
*       CALL SNDERRSP;              /* SEND -RSP                     */
         BAL   @14,SNDERRSP                                        0547
*     END;                                                         0548
*                                                                  0548
*/*                                                                  */
*                                                                  0549
*   R14=R14SV03;                    /* RESTORE RETURN REG            */
@RC00502 L     R14,R14SV03                                         0549
*   RETURN;                         /* RETURN TO CALLER              */
@EL00007 DS    0H                                                  0550
@EF00007 DS    0H                                                  0550
@ER00007 BR    @14                                                 0550
*   END OAFDAFCK;                                                  0551
*                                                                  0552
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE POST THE CURRENT PIU TO COMMON SESSION CONTROL      */
*/* INBOUND(IEDCSCIN).                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0552
*POSTCSCI:                                                         0552
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0552
POSTCSCI DS    0H                                                  0553
*   R14SV09=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,R14SV09                                         0553
*/*                                                                  */
*                                                                  0554
*   PRFQCBA=SAVTCSCI;               /* ADDRESS OF COMMON SESSION   0554
*                                      CONTROL INBOUND QCB           */
         L     @10,IEDQPPTR                                        0554
         L     @09,AVTSAVTP(,IEDQAPTR)                             0554
         MVC   PRFQCBA(3,@10),SAVTCSCI+1(@09)                      0554
*   PRFPRI=PRIFIDIN;                /* POST PRIORITY                 */
         MVI   PRFPRI(@10),X'E4'                                   0555
*   RPARM=CURPIUP;                  /* CURRENT PIU ADDRESS           */
         L     RPARM,CURPIUP                                       0556
*   CALL DSPPOSTR;                  /* POST THE PIU TH IEDCSCIN      */
         LA    @15,DSPPOSTR(,RDISP)                                0557
         BALR  @14,@15                                             0557
*/*                                                                  */
*                                                                  0558
*   R14=R14SV09;                    /* RESTORE RETURN REG            */
         L     R14,R14SV09                                         0558
*   RETURN;                         /* RETURN TO CALLER              */
@EL00008 DS    0H                                                  0559
@EF00008 DS    0H                                                  0559
@ER00008 BR    @14                                                 0559
*   END POSTCSCI;                                                  0560
*                                                                  0561
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE POST THE CURRENT PIU TO PATH CONTROL OUTBOUND       */
*/* (IEDPCOUT).                                                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0561
*POSTPCO:                                                          0561
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0561
POSTPCO  DS    0H                                                  0562
*   R14SV08=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,R14SV08                                         0562
*/*                                                                  */
*                                                                  0563
*   PRFQCBA=ADDR(IEDPCOUT);         /* ADDRESS OF IEDPCOUT'S QCB     */
         L     @09,IEDQPPTR                                        0563
         L     @02,@CV01568                                        0563
         STCM  @02,7,PRFQCBA(@09)                                  0563
*   PRFPRI=PRIPCOUT;                /* POST PRIORITY                 */
         MVI   PRFPRI(@09),X'E4'                                   0564
*   RPARM=CURPIUP;                  /* CURRENT PIU ADDRESS           */
         L     RPARM,CURPIUP                                       0565
*   CALL DSPPOSTR;                  /* POST PIU TO IEDPCOUT QCB      */
         LA    @15,DSPPOSTR(,RDISP)                                0566
         BALR  @14,@15                                             0566
*/*                                                                  */
*                                                                  0567
*   R14=R14SV08;                    /* RESTORE RETURN REG            */
         L     R14,R14SV08                                         0567
*   RETURN;                         /* RETURN TO CALLER              */
@EL00009 DS    0H                                                  0568
@EF00009 DS    0H                                                  0568
@ER00009 BR    @14                                                 0568
*   END POSTPCO;                                                   0569
*                                                                  0570
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE BUILDS AN EXCEPTION RESPONSE(-RSP)                  */
*/*   INPUT=                                                         */
*/*         CURPIUP-CONTAINS A PTR TO THE PIU IN WHICH THE           */
*/*                 EXCEPTION RESPONSE IS TO BE BUILT                */
*/*         SNC- CONTAINS THE 4 SNA SENSE BYTES TO BE USED IN        */
*/*              THE RESPONSE                                        */
*/*         PLTNTDAF-CONTAINS THE DAF TNT OFFSET                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0570
*RSPBUILD:                                                         0570
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0570
RSPBUILD DS    0H                                                  0571
*   R14SV05=R14;                    /* SAVE RETURN REG               */
         ST    R14,R14SV05                                         0571
*/*                                                                  */
*                                                                  0572
*   IF PRFTIC^=INVALTIC THEN        /* LAST UNIT IN PIU              */
         L     @09,IEDQPPTR                                        0572
         CLC   PRFTIC(4,@09),@CB01097                              0572
         BE    @RF00572                                            0572
*     DO;                           /* NO                            */
*       RPARM=PRFTIC;               /* GET 2ND UNIT OF PIU           */
         L     RPARM,PRFTIC(,@09)                                  0574
*       CALL DISCARD;               /* DISCARD ALL BUT 1ST UNIT OF 0575
*                                      PIU                           */
         BAL   @14,DISCARD                                         0575
*       PRFTIC=INVALTIC;            /* MARK 1ST UNIT OF PIU 'LAST'   */
         L     @09,IEDQPPTR                                        0576
         MVC   PRFTIC(4,@09),@CB01097                              0576
*     END;                                                         0577
*                                                                  0577
*/*                                                                  */
*                                                                  0578
*   IF TRHRUCAT=TRHFMD THEN         /* FM DATA                       */
@RF00572 LA    @09,10                                              0578
         LR    @02,PIUSTART                                        0578
         ALR   @02,@09                                             0578
         TM    TRHRUCAT(@02),B'01100000'                           0578
         BNZ   @RF00578                                            0578
*     DO;                           /* YES                           */
*       IF PLTNTDAF=SAVTSCPT THEN   /* IS DAF SSCP                   */
         L     @05,AVTSAVTP(,IEDQAPTR)                             0580
         CLC   PLTNTDAF(2),SAVTSCPT(@05)                           0580
         BNE   @RF00580                                            0580
*         DO;                       /* YES                           */
*           TTHDCF=10;              /* SET DATA COUNT                */
         STCM  @09,3,TTHDCF(PIUSTART)                              0582
*           IF TRHSDI=OFF THEN      /* SENSE DATA INCLUDED           */
         TM    TRHSDI(@02),B'00000100'                             0583
         BNZ   @RF00583                                            0583
*             RU(5:7)=RU(1:3);      /* MOVE COMMAND BYTES OVER TO  0584
*                                      LEAVE ROOM FOE SENSE          */
         LA    @09,13                                              0584
         ALR   @09,PIUSTART                                        0584
         MVC   RU+4(3,@09),RU(@09)                                 0584
*         END;                                                     0585
*       ELSE                        /* NOT SSCP                      */
*         TTHDCF=7;                 /* SET DATA COUNT = 7            */
         B     @RC00580                                            0586
@RF00580 MVC   TTHDCF(2,PIUSTART),@CH00568                         0586
*     END;                                                         0587
*   ELSE                            /* NOT FMD                       */
*     DO;                           /* YES                           */
         B     @RC00578                                            0588
@RF00578 DS    0H                                                  0589
*       IF TRHSDI=OFF THEN          /* NO SENSE INCLUDED             */
         TM    TRHSDI+10(PIUSTART),B'00000100'                     0589
         BNZ   @RF00589                                            0589
*         RU(5)=RU(1);              /* YES-MOVE COMMAND CODE TO 5TH
*                                      BYTE (LEAVES 4 BYTES FOR    0590
*                                      SENSE)                        */
         LA    @09,13                                              0590
         ALR   @09,PIUSTART                                        0590
         MVC   RU+4(1,@09),RU(@09)                                 0590
*       TTHDCF=8;                   /* SET DATA COUNT TO 8           */
@RF00589 MVC   TTHDCF(2,PIUSTART),@CH00044                         0591
*     END;                                                         0592
*                                                                  0592
*/*                                                                  */
*/* SWITCH OAF AND DAF                                               */
*                                                                  0593
*   PLTNTOAF=TTHOAF;                /* SAVE OAF FOR A SECOND         */
@RC00578 MVC   PLTNTOAF(2),TTHOAF(PIUSTART)                        0593
*   TTHOAF=TTHDAF;                  /* MOVE DAF INTO OAF             */
         MVC   @TS00001(2),TTHDAF(PIUSTART)                        0594
         MVC   TTHOAF(2,PIUSTART),@TS00001                         0594
*   TTHDAF=PLTNTOAF;                /* MOVE OAF INTO DAF             */
         MVC   TTHDAF(2,PIUSTART),PLTNTOAF                         0595
