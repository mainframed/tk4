         TITLE 'IEDQNW, 3705 CHECKPOINT/RESTART I/O                    *
                        '
IEDQNW   CSECT ,                                                01S0001
@PROLOG  DS    0H                                               01S0002
*   STM(REG14,REG12,SAVEREGS);      /* SAVE REGISTERS IN CALLER SAVE
*                                      AREA                          */
         STM   REG14,REG12,SAVEREGS(REG13)                      02S0120
*   RBASE=REG15;                    /* SET BASE ADDRESS FROM ENTRY
*                                      REGISTER                      */
         LR    RBASE,REG15                                      02S0121
*   GEN NOREFS NOSETS;                                          02S0122
*                                   /*
         USING IEDQNW,RBASE            ESTABLISH ADDRESSABILITY
IEDQNW   IEDHJN AFTERHJN,HJN           MODULE IDENTIFICATION MACRO
*   RSAVE=REG13;                    /* SAVE ADDRESS OF CALLERS SAVE
*                                      AREA                          */
         LR    RSAVE,REG13                                      02S0123
*   RFY                                                         02S0124
*     REG13 UNRSTD;                                             02S0124
*                                                               02S0125
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS: THIS SEGMENT DOES THE PROCESSING THAT IS NECESSARY TO*/
*   /* PERFORM THE FUNCTION OF THE MODULE. IF THE I/O IS TO THE 3705 */
*   /* CONTROL RECORD, THEN USE THE FIRST RELATIVE RECORD IN THE DATA*/
*   /* SET. OTHERWISE, FIND THE ADDRESS VIA A CONVERSION OF THE TTRN.*/
*   /* ISSUE EXCP TO START THE I/O AND SET UP THE WAIT LIST TO WAIT  */
*   /* FOR THE I/O TO COMPLETE.                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0125
*   RSTATE=0;                       /* INITIALIZE TO INDICATE   02S0125
*                                      SUCCESSFUL OPERATION          */
         SLR   RSTATE,RSTATE                                    02S0125
*   IF CKPRWADD=ADDR(CKPNCNTL)      /* TEST FOR REQUEST TYPE         */
*     THEN                          /* REQUEST TO START I/O TO THE
*                                      CONTROL RECORD                */
         LA    @14,CKPNCNTL(,RCKPT)                             02S0126
         L     @12,CKPRWADD-1(,RCKPT)                           02S0126
         LA    @12,0(,@12)                                      02S0126
         CR    @14,@12                                          02S0126
         BNE   @RF00126                                         02S0126
*     DO;                                                       02S0127
*       RDEB=IEDQDPTR-PREFDEB;      /* GET ADDRESS OF THE DEB PREFIX */
         L     RDEB,IEDQDPTR(,RCKPT)                            02S0128
         SH    RDEB,PREFDEB                                     02S0128
*       CKPIOM=0;                   /* SET TO WRITE TO FIRST EXTENT  */
         MVI   CKPIOM(RCKPT),X'00'                              02S0129
*       CKPIOSK(2:7)=EXSADDR;       /* GET BIN, CYLINDER, AND TRACK
*                                      NUMBERS FROM THE DEB EXTENT
*                                      DATA                          */
         MVC   CKPIOSK+1(6,RCKPT),EXSADDR(RDEB)                 02S0130
*       CKPIOR=1;                   /* WRITE TO FIRST RECORD         */
         MVI   CKPIOR(RCKPT),X'01'                              02S0131
*       CKPSECTR=0;                 /* CLEAR SECTOR ID               */
         MVI   CKPSECTR(RCKPT),X'00'                            02S0132
*     END;                                                      02S0133
*   ELSE                            /* I/O NOT FOR CONTROL RECORD    */
*                                                               03S0134
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS.FINDADDR: THIS SEGMENT DOES THE CALCULATIONS TO FIND*/
*     /* THE TRACK ADDRESS FOR A GIVEN RESOURCE.                     */
*     /*                                                             */
*     /***************************************************************/
*                                                               03S0134
*     DO;                                                       03S0134
         B     @RC00126                                         03S0134
@RF00126 DS    0H                                               03S0135
*       IF CKPNRSID<CKPTRKCN        /* TEST FOR VALID RESOURCE       */
*         THEN                      /* RESOURCE IS VALID             */
         LH    @14,CKPNRSID(,RCKPT)                             03S0135
         CH    @14,CKPTRKCN(,RCKPT)                             03S0135
         BNL   @RF00135                                         03S0135
*         DO;                                                   03S0136
*           RPARM=CKPNRSID+CKPNTRK; /* CALCULATE THE RELATIVE OFFSET
*                                      OF THE RECORD                 */
         AH    @14,CKPNTRK(,RCKPT)                              03S0137
         LR    RPARM,@14                                        03S0137
*           REG00=0;                /* CLEAR FOR DIVIDE              */
         SLR   REG00,REG00                                      03S0138
*           RDATA=CKPBLKPT;         /* GET BLOCKS PER TRACK COUNT    */
         SLR   RDATA,RDATA                                      03S0139
         IC    RDATA,CKPBLKPT(,RCKPT)                           03S0139
*           GEN REFS(REG00,RPARM,RDATA) SETS(REG00,RPARM);      03S0140
*                                   /*
            DR    REG00,RDATA          DIVIDE TO GET TRACK NUMBER
*                                      AND RECORD NUMBER
*           SLL(RPARM,16);          /* SHIFT TRACK NUMBER SO THAT REG
*                                      CONTAINS NUMBER OF THE FORM
*                                      TT00                          */
         SLL   RPARM,16                                         03S0141
*           REG00=REG00+RECNO;      /* INCREMENT RECORD NUMBER TO
*                                      SKIP PAST RELATIVE RECORD 0   */
         AL    REG00,@CF00059                                   03S0142
*           SLL(REG00,8);           /* SHIFT RECORD NUMBER SO THAT
*                                      REG CONTAINS NUMBER OF THE
*                                      FORM 00RN, WHERE N IS    03S0143
*                                      CONCATENATION NUMBER OF 0     */
         SLL   REG00,8                                          03S0143
*           REG00=REG00+RPARM;      /* CREATE TTRN VALUE             */
         ALR   REG00,RPARM                                      03S0144
*           LM(RPARM,RDATA,CKPCPARM);/* SET DEB ADDRESS AND AREA
*                                      ADDRESS TO CONTAIN THE   03S0145
*                                      CONVERTED ADDRESS             */
         LM    RPARM,RDATA,CKPCPARM(RCKPT)                      03S0145
*           CALL IECPCNVT;          /* CALL CONVERSION ROUTINE - SEE
*                                      INTRODUTORY COMMENTS FOR 03S0146
*                                      INPUT/OUTPUT FOR THIS ROUTINE */
         L     @12,CVTPTR                                       03S0146
         L     @15,CVTPCNVT(,@12)                               03S0146
         BALR  @14,@15                                          03S0146
*           IF REG15^=0             /* TEST RETURN CODE FROM    03S0147
*                                      CONVERSION                    */
*             THEN                  /* CONVERSION UNSUCCESSFUL       */
         LTR   REG15,REG15                                      03S0147
         BZ    @RF00147                                         03S0147
*             RSTATE=CKPEODAT;      /* SET ERROR CODE                */
         LA    RSTATE,8                                         03S0148
*         END;                                                  03S0149
*       ELSE                        /* RESOURCE IS NOT VALID         */
*         RSTATE=CKPEODAT;          /* SET ERROR CODE                */
         B     @RC00135                                         03S0150
@RF00135 LA    RSTATE,8                                         03S0150
*     END;                                                      03S0151
@RC00135 DS    0H                                               03S0152
*   IF RSTATE=0                     /* TEST FOR STATE IN WHICH I/O
*                                      CAN BE REQUESTED              */
*     THEN                          /* I/O CAN BE REQUESTED          */
@RC00126 LTR   RSTATE,RSTATE                                    02S0152
         BNZ   @RF00152                                         02S0152
*     DO;                                                       02S0153
*       GEN REFS(CKPIOB) SETS(RPARM);                           02S0154
*                                   /*
         EXCP  CKPIOB(,RCKPT)          ISSUE I/O
*       CAVTEOL2=0;                 /* MAKE THE 3705 CKPT DATA SET
*                                      I/O ECB ...                   */
         MVI   CAVTEOL2(RCAVT),X'00'                            02S0155
*       CAVTEOL3=EOL;               /* ... THE LAST ECB IN THE WAIT
*                                      LIST                          */
         MVI   CAVTEOL3(RCAVT),X'80'                            02S0156
*       CAVTNIOE=ADDR(CKPECB);      /* PUT THE 3705 I/O ECB ADDR INTO
*                                      THE WAIT LIST                 */
         LA    @14,CKPECB(,RCKPT)                               02S0157
         STCM  @14,7,CAVTNIOE(RCAVT)                            02S0157
*       REG15=0;                    /* CLEAR TO INDICATE SUCCESSFUL
*                                      OPERATION                     */
         SLR   REG15,REG15                                      02S0158
*     END;                                                      02S0159
*   ELSE                            /* I/O CANNOT BE REQUESTED       */
*     REG15=RSTATE;                 /* SAVE ERROR CODE               */
         B     @RC00152                                         02S0160
@RF00152 LR    REG15,RSTATE                                     02S0160
*                                                               02S0161
*   /*****************************************************************/
*   /*                                                               */
*   /* EPILOGUE: THIS SEGMENT COMPLETES THE EPILOGUE CODE THAT WAS   */
*   /* ALTERED BY THE MODULE PROCEDURE STATEMENT.                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0161
*   RFY                                                         02S0161
*    (REG00,                                                    02S0161
*     REG12,                                                    02S0161
*     REG13,                                                    02S0161
*     REG14) RSTD;                                              02S0161
@RC00152 DS    0H                                               02S0162
*   REG13=RSAVE;                    /* RESTORE POINTER TO CALLER'S
*                                      SAVE AREA                     */
         LR    REG13,RSAVE                                      02S0162
*   REG14=SAVER14;                  /* RESTORE CALLER'S REGISTER 14  */
         L     REG14,SAVER14(,REG13)                            02S0163
*   LM(REG00,REG12,SAVEREM);        /* RESTORE REGISTERS 0 THRU 12   */
         LM    REG00,REG12,SAVEREM(REG13)                       02S0164
*   RFY                                                         02S0165
*    (REG00,                                                    02S0165
*     REG12,                                                    02S0165
*     REG13,                                                    02S0165
*     REG14) UNRSTD;                                            02S0165
*   END IEDQNW                                                  01S0166
