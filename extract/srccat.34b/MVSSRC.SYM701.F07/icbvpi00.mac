         TITLE 'ICBVPI00 MSVC PRE-INITIALIZER MODULE (JULY 1, 1976)    *
                        '
ICBVPI00 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ICBVPI00  77.355'                                 0001
         DROP  @15
@PROLOG  STM   @14,@15,12(@13)                                     0001
         STM   @01,@12,24(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*                                                                  0050
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP ADDRESSABILITY AND INITIALIZE VARIABLES        @Y30LB24*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0050
*   PARMPTR=REG1;                   /* SAVE PARAMETER ADDRESS      0050
*                                                            @Y30LB24*/
         LR    PARMPTR,REG1                                        0050
*   PARM0=REG0;                     /* SAVE PARAMETER REGISTER     0051
*                                                            @Y30LB24*/
         LR    PARM0,REG0                                          0051
*   VVIPTR=ICBVVICB;                /* INITIALIZE MSVC CB PTR      0052
*                                                            @Y30LB24*/
         L     @11,CVTPTR                                          0052
         L     @11,CVTEXT2(,@11)                                   0052
         L     @11,CVTICB(,@11)                                    0052
         L     VVIPTR,ICBVVICB(,@11)                               0052
*   FAILCODE=RCON0;                 /* INITIALIZE REASON CODE      0053
*                                                            @Y30LB24*/
         SLR   FAILCODE,FAILCODE                                   0053
*   REG0=FAILCODE;                  /* INITIALIZE USER REASON CODE 0054
*                                                            @Y30LB24*/
         LR    REG0,FAILCODE                                       0054
*   RFY                                                            0055
*     SELRB BASED(PARMPTR);         /* BASE SELECT RB        @ZA24770*/
*                                                                  0056
*   /*****************************************************************/
*   /*                                                               */
*   /* IF MSVC IS DISABLED BECAUSE OF PREVIOUS ERROR THEN RETURN TO  */
*   /* CALLER WITH NO PROCESSING REQUIRED                    @Y30LB24*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0056
*   IF ICBV=ON THEN                 /* NO PREVIOUS ERROR     @Y30LB24*/
         TM    ICBV(@11),B'00100000'                               0056
         BNO   @RF00056                                            0056
*     DO;                           /*                       @Y30LB24*/
*                                                                  0057
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK ORDER FOR VALIDITY DURING IPL               @Y30LB24*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0058
*       CALL VPICHKOR;              /* VALIDATE ORDER        @Y30LB24*/
*                                                                  0058
         BAL   @14,VPICHKOR                                        0058
*       /*************************************************************/
*       /*                                                           */
*       /* IF THIS IS THE FIRST ENTRY THROUGH THIS CODE, THE MSSC-SVC*/
*       /* CONTROL BLOCK WILL HAVE TO BE INITIALIZED.        @Y30LB24*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0059
*       IF FAILCODE=ZERO THEN       /* RETURN CODE 0?        @Y30LB24*/
         SLR   @11,@11                                             0059
         CR    FAILCODE,@11                                        0059
         BNE   @RF00059                                            0059
*         DO;                       /*                       @Y30LB24*/
*           IF VVIPTR=ZERO THEN     /* HAS MSVC CB BEEN CREATED    0061
*                                                            @Y30LB24*/
         CR    VVIPTR,@11                                          0061
         BNE   @RF00061                                            0061
*             CALL VPIBLDCB;        /* GO BUILD CONTROL BLOCK      0062
*                                                            @Y30LB24*/
         BAL   @14,VPIBLDCB                                        0062
*         END;                      /*                       @Y30LB24*/
*                                                                  0063
@RF00061 DS    0H                                                  0064
*       /*************************************************************/
*       /*                                                           */
*       /* CREATE RPLV AND ADD TO THE QUEUE IF TRACKING REQUIRED     */
*       /*                                                   @Y30LB24*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0064
*       IF FAILCODE=ZERO THEN       /* RETURN CODE 0?        @Y30LB24*/
@RF00059 LTR   FAILCODE,FAILCODE                                   0064
         BNZ   @RF00064                                            0064
*         CALL VPIADDOR;            /* BUILD RPL AND ADD TO QUEUE  0065
*                                                            @Y30LB24*/
*                                                                  0065
         BAL   @14,VPIADDOR                                        0065
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK FOR ERRORS AND RETURN TO CALLER             @Y30LB24*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0066
*       REG0=FAILCODE;              /* SET USER REASON CODE  @Y30LB24*/
@RF00064 LR    REG0,FAILCODE                                       0066
*       IF FAILCODE^=ZERO THEN      /* RETURN CODE 0?        @Y30LB24*/
         LTR   FAILCODE,FAILCODE                                   0067
         BZ    @RF00067                                            0067
*         DO;                       /*                       @Y30LB24*/
*           CALL VPINOTOP;          /* PRINT ERROR MESSAGE AND CLEAN
*                                      UP                    @Y30LB24*/
         BAL   @14,VPINOTOP                                        0069
*           REG0=FAILCODE;          /* SET USER REASON CODE  @Y30LB24*/
         LR    REG0,FAILCODE                                       0070
*           FAILCODE=FOUR;          /* SET RETURN CODE TO 4  @Y30LB24*/
         LA    FAILCODE,4                                          0071
*         END;                      /*                       @Y30LB24*/
*                                                                  0072
*       /*************************************************************/
*       /*                                                           */
*       /* IF THIS IS SUSPEND THEN CLEAN UP                  @Y30LB24*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0073
*       IF OPCODE='22'X THEN        /* SUSPEND REQUEST?      @Y30LB24*/
@RF00067 CLI   OPCODE(PARMPTR),X'22'                               0073
         BNE   @RF00073                                            0073
*         DO;                       /*                       @Y30LB24*/
*           FAILCODE=RCON4;         /* SET FAIL CODE TO 1    @Y30LB24*/
         LA    FAILCODE,1                                          0075
*           CALL VPINOTOP;          /* CLEAN UP              @Y30LB24*/
         BAL   @14,VPINOTOP                                        0076
*           REG0=RCON0;             /* SET USER REASON CODE  @Y30LB24*/
         SLR   REG0,REG0                                           0077
*           FAILCODE=REG0;          /* SET RETURN CODE TO 0          */
         LR    FAILCODE,REG0                                       0078
*         END;                      /*                       @Y30LB24*/
*     END;                          /*                       @Y30LB24*/
@RF00073 DS    0H                                                  0081
*   RETURN CODE(FAILCODE);          /* RETURN TO MSSC-SVC    @Y30LB24*/
@RF00056 LR    @15,FAILCODE                                        0081
         L     @14,12(,@13)                                        0081
         LM    @01,@12,24(@13)                                     0081
         BR    @14                                                 0081
*                                                                  0082
*/* ******************************************************************
*                                                                  0082
*     NAME = VPICHKOR, VALIDATE IPL ORDER                      @Y30LB24
*                                                                  0082
*     INPUT = POINTER TO PARMLIST PASSED TO THE MSSC-SVC.      @Y30LB24
*                                                                  0082
*     PROCESS = THIS SUBROUTINE IS CALLED TO DETERMINE THE VALIDITY
*     OF THE ORDERS.  VALID ORDERS ARE MOUNT SPECIFIC, PURGE       0082
**     ASSOCIATE, DISASSOCIATE, SELECT AND TERMINATION.            0082
*     IF AN INVALID ORDER IS FOUND,  A FAILCODE IS SET FOR USE     0082
*     BY VPINOTOP TO PRODUCE AN INVALID ORDER MESSAGE TO THE       0082
**     CONSOLE AND TO THE USER.                                @ZA24770
*                                                                  0082
*     OUTPUT = INTERNAL WORK FLAG INDICATING THE VALIDITY OF THE   0082
*     REQUEST.                                                 @Y30LB24
*                                                                  0082
******************************************************************** */
*/*VPICHKOR: CHART                                                   */
*/*HEADER                                                          0082
*/*JUNE 14, 1974                                 ICBVPI00 - SEGMENT
*/*VPICHKOR - VALIDATE IPL ORDER                                     */
*/*          E                    VPICHKOR                           */
*/*          M                    SET LOOP CONTROL FOR ORDER CHECK   */
*/*%CHKOR:   D (NO,,YES,%END)     ORDER MATCH TABLE ENTRY            */
*/*          D (NO,,YES,%INVALOR) ALL ORDERS CHECKED                 */
*/*          COMMENT (1,5,)       PURGE, ASSOC, DISASSOC,            */
*/*          COMMENT (2,5,)       MOUNT SPEC., DEMOUNT               */
*/*          COMMENT (3,5,)       SUSPEND ARE VALID                  */
*/*          M (,%CHKOR)          PREPARE TO CHECK NEXT ORDER        */
*/*%INVALOR: M                    INDICATE INVALID ORDER             */
*/*%END:     R                    RETURN TO ICBVPI00                 */
*/*VPICHKOR: END                                                     */
*                                                                  0082
*VPICHKOR:                                                         0082
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0082
*                                                                  0082
VPICHKOR DS    0H                                                  0083
*   /*****************************************************************/
*   /*                                                               */
*   /* ORDERS - ARRAY OF THE FIVE VALID TYPES OF ORDERS USED @Y30LB24*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0083
*   DECLARE                                                        0083
*     ORDERS(8) CHAR(1) INIT('06'X,'08'X,'14'X,'02'X,'22'X,'2C'X,'89'X,
*         '8A'X);                   /*                       @ZA24770*/
*                                                                  0083
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR INVALID MOUNT                               @Y30LB24*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0084
*   VALIDORD=ZERO;                  /* INITIALIZE VALIDORD SWITCH  0084
*                                                            @Y30LB24*/
         SLR   VALIDORD,VALIDORD                                   0084
*   IF OPCODE=ORDERS(2)&SCRCHBIT=ON THEN/* MOUNT SCRATCH     @Y30LB24*/
         CLC   OPCODE(1,PARMPTR),ORDERS+1                          0085
         BNE   @RF00085                                            0085
         TM    SCRCHBIT(PARMPTR),B'00010000'                       0085
         BNO   @RF00085                                            0085
*     DO;                           /*                       @Y30LB24*/
*       FAILCODE=RCON2;             /* REASON CODE (217)     @Y30LB24*/
         LA    FAILCODE,535                                        0087
*       VALIDORD=ONE;               /* TERMINATE SEARCH      @Y30LB24*/
         LA    VALIDORD,1                                          0088
*     END;                          /*                       @Y30LB24*/
*                                                                  0089
*   /*****************************************************************/
*   /*                                                               */
*   /* IS THE REQUEST A VALID ORDER?                         @Y30LB24*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0090
*   IF PLENGTH<=ZERO×               /* INVALID MSC LENGTH    @Y30LB24*/
*       PLENGTH>N256 THEN           /* INVALID MSC LENGTH    @Y30LB24*/
@RF00085 L     @11,PLENGTH(,PARMPTR)                               0090
         LTR   @11,@11                                             0090
         BNP   @RT00090                                            0090
         C     @11,@CF00064                                        0090
         BNH   @RF00090                                            0090
@RT00090 DS    0H                                                  0091
*     DO;                           /* YES                   @Y30LB24*/
*       FAILCODE=RCON1;             /* REASON CODE (216)     @Y30LB24*/
         LA    FAILCODE,534                                        0092
*       VALIDORD=ONE;               /* TERMINATE SEARCH      @Y30LB24*/
         LA    VALIDORD,1                                          0093
*     END;                          /*                       @Y30LB24*/
*   DO I=ONE TO EIGHT WHILE VALIDORD=ZERO;/*                 @ZA24770*/
@RF00090 LA    I,1                                                 0095
@DL00095 LTR   VALIDORD,VALIDORD                                   0095
         BNZ   @DC00095                                            0095
*     IF OPCODE=ORDERS(I) THEN      /* VALID ORDER           @Y30LB24*/
         LA    @11,ORDERS-1(I)                                     0096
         CLC   OPCODE(1,PARMPTR),0(@11)                            0096
         BNE   @RF00096                                            0096
*       VALIDORD=ONE;               /* YES, TERMINATE SEARCH @Y30LB24*/
         LA    VALIDORD,1                                          0097
*     ELSE                          /* OTHERWISE             @Y30LB24*/
*                                                                  0098
*       /*************************************************************/
*       /*                                                           */
*       /* IF I = EIGHT THEN ORDER INVALID. (ONLY 7 VALID OPCODES)   */
*       /*                                                   @ZA24770*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0098
*       IF I=EIGHT THEN             /* INVALID MSC ORDER?    @ZA24770*/
         B     @RC00096                                            0098
@RF00096 C     I,@CF00062                                          0098
         BNE   @RF00098                                            0098
*         DO;                       /* YES                   @Y30LB24*/
*           FAILCODE=RCON2;         /* REASON CODE (217)     @Y30LB24*/
         LA    FAILCODE,535                                        0100
*           VALIDORD=ONE;           /* TERMINATE SEARCH      @Y30LB24*/
         LA    VALIDORD,1                                          0101
*         END;                      /*                       @Y30LB24*/
*   END;                            /*                       @Y30LB24*/
@RF00098 DS    0H                                                  0103
@RC00096 AL    I,@CF00053                                          0103
         C     I,@CF00062                                          0103
         BNH   @DL00095                                            0103
@DC00095 DS    0H                                                  0104
*   RETURN;                         /* RETURN TO ICBVPI00    @Y30LB24*/
@EL00002 DS    0H                                                  0104
@EF00002 DS    0H                                                  0104
@ER00002 BR    @14                                                 0104
*   END VPICHKOR;                   /* END CHECK ORDER       @Y30LB24*/
*                                                                  0106
*/* ******************************************************************
*                                                                  0106
*     NAME = VPIBLDCB, INITIALIZE ICBVVICB                     @Y30LB24
*                                                                  0106
*     INPUT = MSSC-SVC CONTROL BLOCK POINTED TO BY THE CVT.    @Y30LB24
*                                                                  0106
*     PROCESS = THIS SUBROUTINE IS CALLED WHEN MAIN STORAGE IS     0106
*     REQUIRED FOR THE CONTROL BLOCK.  THE CONTROL BLOCK IS ZEROED 0106
*     AND INITIALIZED WITH AN IDENTIFIER.                          0106
*     IF STORAGE CAN NOT BE OBTAINED, A FAILCODE IS SET FOR USE BY 0106
*     VPINOTOP TO RELEASE RESOURCES USED BY THE MSVC PRE-INITIALIZER.
*                                                              @Y30LB24
*                                                                  0106
*     OUTPUT = ICBVVICB POINTED TO BY THE MSSC-SVC CONTROL BLOCK.  0106
*                                                              @Y30LB24
*                                                                  0106
******************************************************************** */
*/*VPIBLDCB: CHART                                                   */
*/*HEADER                                                          0106
*/*JUNE 14, 1974                                ICBVPI00 - SEGMENT 0106
*/*VPIBLDCB - CREATE AND INITIALIZE ICBVVICB                         */
*/*          E                    VPIBLDCB                           */
*/*          D (NO,,YES,%END)     INITIAL ENTRY FOR ORDER            */
*/*          L                    GETMAIN FOR ICBVVICB SP=241        */
*/*          D (YES,,NO,%INITCB)  ERROR FROM GETMAIN                 */
*/*          M (,%END)            INDICATE GETMAIN ERR FOR ICBVVICB  */
*/*%INITCB:  P                    INITIALIZE CONTROL BLOCK - CLEAR &
*/*                               AND SET ID                         */
*/*%END:     R                    RETURN TO ICBVPI00                 */
*/*VPIBLDCB: END                                                     */
*                                                                  0106
*VPIBLDCB:                                                         0106
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0106
VPIBLDCB DS    0H                                                  0107
*   DECLARE                                                        0107
*     VID CHAR(4) INIT('MSVC');     /* THE MSVC ID           @Y30LB24*/
*   IF PARM0=ZERO THEN              /* FIRST ENTRY FOR ORDER @Y30LB24*/
         LTR   PARM0,PARM0                                         0108
         BNZ   @RF00108                                            0108
*     DO;                           /* YES                   @Y30LB24*/
*       RETURN;                     /* RETURN TO ICBVPI00    @Y30LB24*/
@EL00003 DS    0H                                                  0110
@EF00003 DS    0H                                                  0110
@ER00003 BR    @14                                                 0110
*     END;                          /*                       @Y30LB24*/
*   REG0=LENGTH(VVICB);             /* SET GETMAIN REG0      @Y30LB24*/
@RF00108 LA    REG0,120                                            0112
*LGET1:                                                            0113
*   GEN(GETMAIN RC,LV=(0),SP=241);  /* PERFORM GETMAIN 241   @Y30LB24*/
LGET1    GETMAIN RC,LV=(0),SP=241
*   IF REG15=ZERO THEN              /* CORE AVAILABLE?       @Y30LB24*/
         LTR   REG15,REG15                                         0114
         BNZ   @RF00114                                            0114
*     DO;                           /* YES                   @Y30LB24*/
*       ICBVVICB=REG1;              /* SET PTR IN MSSC CB    @Y30LB24*/
         L     @11,CVTPTR                                          0116
         L     @11,CVTEXT2(,@11)                                   0116
         L     @11,CVTICB(,@11)                                    0116
         ST    REG1,ICBVVICB(,@11)                                 0116
*       VVIPTR=REG1;                /* INITIAL VVICB PTR     @Y30LB24*/
         LR    VVIPTR,REG1                                         0117
*       VVICB=VVICB&&VVICB;         /* ZERO VVICB            @Y30LB24*/
         XC    VVICB(120,VVIPTR),VVICB(VVIPTR)                     0118
*       VVICID=VID;                 /* SET CB ID             @Y30LB24*/
         MVC   VVICID(4,VVIPTR),VID                                0119
*       RETURN;                     /* RETURN TO ICBVPI00    @Y30LB24*/
         B     @EL00003                                            0120
*     END;                          /*                       @Y30LB24*/
*   FAILCODE=RCON3;                 /* REASON CODE (22A)     @Y30LB24*/
@RF00114 LA    FAILCODE,554                                        0122
*   RETURN;                         /* RETURN TO ICBVPI00    @Y30LB24*/
         B     @EL00003                                            0123
*   END VPIBLDCB;                   /* END CONTROL BLOCK BUILD     0124
*                                                            @Y30LB24*/
*                                                                  0125
*/* ******************************************************************
*                                                                  0125
*     NAME = VPIADDOR, ADD MSC ORDER ENTRY TO RPLV QUEUE       @Y30LB24
*                                                                  0125
*     INPUT = POINTER TO THE MSSC-SVC REQUEST, AND ICBVVICB POINTING
*     TO THE RPLV QUEUE.                                       @Y30LB24
*                                                                  0125
*     PROCESS = THIS SUBROUTINE IS CALLED FOR TRACKING OF ORDERS.  0125
**     ALL VALID ORDERS EXCEPT SPECIFIC MOUNT, DEMOUNT, SELECT,    0125
**     TERMINATE AND SUSPEND ORDERS ARE TRACKED.                   0125
*     A GETMAIN IS DONE FOR STORAGE FOR THE MSVC RPLV.  THE MSVC   0125
*     RPLV IS ZEROED AND INITIALIZED WITH THE PROPER FLAGS AND     0125
*     ENTRIES AND CHAINED TO THE LOW PRIORITY QUEUE IN THE ICBVVICB.
*     IF STORAGE CAN NOT BE OBTAINED, A FAILCODE IS SET FOR USE BY 0125
*     VPINOTOP TO RELEASE RESOURCES USED BY THE MSVC PRE-INITIALIZER.
**                                                             @ZA24770
*                                                                  0125
*     OUTPUT = MSSC-SVC REQUEST ENTRY ADDED TO RPLV CHAIN.     @Y30LB24
*                                                                  0125
******************************************************************** */
*/*VPIADDOR: CHART                                                   */
*/*HEADER                                                          0125
*/*JUNE 14, 1974                                ICBVPI00 - SEGMENT 0125
*/*VPIADDOR - ADD ENTRY TO IPL TABLE                                 */
*/*          E                    VPIADDOR                           */
*/*          D (NO,,YES,%END)     INITIAL ENTRY FOR ORDER            */
*/*          D (NO,,YES,%END)     MOUNT REQUEST                      */
*/*          D (NO,,YES,%END)     DEMOUNT REQUEST                    */
*/*          D (NO,,YES,%END)     SUSPEND REQUEST                    */
*/*          D (YES,,NO,%END)     MSC REASON CODE ZERO               */
*/*          L                    GETMAIN FOR RPLV                   */
*/*          D (YES,,NO,%BLDRPL)  ERROR FROM GETMAIN                 */
*/*          P  (,%END)           INDICATE GETMAIN ERROR FOR RPLV    */
*/*%BLDRPL:  P                    BUILD RPLV                         */
*/*%NXTSEG:  P                    SET PTR TO FIRST/NEXT RPLV         */
*/*          D (YES,,NO,%NXTSEG)  LAST RPLV SEGMENT                  */
*/*          P                    CHAIN RPLVS                        */
*/*%END:     R                    RETURN TO ICBVPI00 MAINLINE        */
*/*VPIADDOR: END                                                     */
*                                                                  0125
*VPIADDOR:                                                         0125
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0125
VPIADDOR DS    0H                                                  0126
*   IF OPCODE='08'X×                /* MOUNT ORDER           @Y30LB24*/
*       OPCODE='06'X×               /* DEMOUNT ORDER         @Y30LB24*/
*       OPCODE='22'X×               /* SUSPEND ORDER         @ZA24770*/
*       OPCODE='02'X×               /* ACQUIRE ORDER         @ZA24770*/
*       OPCODE='8A'X THEN           /* TERMNAT ORDER         @ZA24770*/
         CLI   OPCODE(PARMPTR),X'08'                               0126
         BE    @RT00126                                            0126
         CLI   OPCODE(PARMPTR),X'06'                               0126
         BE    @RT00126                                            0126
         CLI   OPCODE(PARMPTR),X'22'                               0126
         BE    @RT00126                                            0126
         CLI   OPCODE(PARMPTR),X'02'                               0126
         BE    @RT00126                                            0126
         CLI   OPCODE(PARMPTR),X'8A'                               0126
         BNE   @RF00126                                            0126
@RT00126 DS    0H                                                  0127
*     DO;                           /* YES                   @Y30LB24*/
*       RETURN;                     /* RETURN TO ICBVPI00    @Y30LB24*/
@EL00004 DS    0H                                                  0128
@EF00004 DS    0H                                                  0128
@ER00004 BR    @14                                                 0128
*     END;                          /*                       @Y30LB24*/
*   IF OPCODE='89'X THEN            /* IS THIS SELECT@ZA24770        */
@RF00126 CLI   OPCODE(PARMPTR),X'89'                               0130
         BNE   @RF00130                                            0130
*     DO;                           /* YES                   @ZA24770*/
*       SELNOVSL=ON;                /* NO VOL SELECT         @ZA24770*/
         OI    SELNOVSL(PARMPTR),B'00000001'                       0132
*       RETURN;                     /* RETURN TO VPI         @ZA24770*/
         B     @EL00004                                            0133
*     END;                          /*                       @ZA24770*/
*   IF PARM0=ZERO THEN              /* FIRST ENTRY FOR ORDER @Y30LB24*/
@RF00130 LTR   PARM0,PARM0                                         0135
         BNZ   @RF00135                                            0135
*     DO;                           /* YES                   @Y30LB24*/
*       RETURN;                     /* RETURN TO ICBVPI00    @Y30LB24*/
         B     @EL00004                                            0137
*     END;                          /*                       @Y30LB24*/
*   IF PARM0^=RZERO THEN            /* MSC REASON CODE ZERO  @Y30LB24*/
@RF00135 C     PARM0,RZERO                                         0139
         BE    @RF00139                                            0139
*     DO;                           /* NO                    @Y30LB24*/
*       RETURN;                     /* RETURN TO VPI         @Y30LB24*/
         B     @EL00004                                            0141
*     END;                          /*                       @Y30LB24*/
*   REG2=LENGTH(RPLV);              /* SET RPLV LENGTH       @Y30LB24*/
@RF00139 LA    REG2,132                                            0143
*   REG3=PLENGTH;                   /* SET ORDER LENGTH      @Y30LB24*/
         L     REG3,PLENGTH(,PARMPTR)                              0144
*   REG2=REG2+REG3;                 /* ADD ORDER LENGTH      @Y30LB24*/
         ALR   REG2,REG3                                           0145
*   REG0=REG2;                      /* SET GETMAIN REG0      @Y30LB24*/
         LR    REG0,REG2                                           0146
*LGET2:                                                            0147
*   GEN(GETMAIN RC,LV=(0),SP=241);  /* PERFORM GETMAIN 241   @Y30LB24*/
LGET2    GETMAIN RC,LV=(0),SP=241
*   IF REG15^=ZERO THEN             /* CORE AVAILABLE?       @Y30LB24*/
         LTR   REG15,REG15                                         0148
         BZ    @RF00148                                            0148
*     DO;                           /* NO                    @Y30LB24*/
*       FAILCODE=RCON3;             /* REASON CODE (22A)     @Y30LB24*/
         LA    FAILCODE,554                                        0150
*       RETURN;                     /* RETURN TO ICBVPI00    @Y30LB24*/
         B     @EL00004                                            0151
*     END;                          /*                       @Y30LB24*/
*   RPLVPTR=REG1;                   /* BASE RPLV             @Y30LB24*/
@RF00148 LR    RPLVPTR,REG1                                        0153
*   RPLV=RPLV&&RPLV;                /* ZERO RPLV             @Y30LB24*/
         XC    RPLV(132,RPLVPTR),RPLV(RPLVPTR)                     0154
*   RPLVTLEN=REG2;                  /* SET RPL LENGTH        @Y30LB24*/
         STH   REG2,RPLVTLEN(,RPLVPTR)                             0155
*   RPLVQPTR=ZERO;                  /* ZERO CHAIN ADDRESS    @Y30LB24*/
         SLR   @11,@11                                             0156
         ST    @11,RPLVQPTR(,RPLVPTR)                              0156
*   RPLVECB=ZERO;                   /* ZERO ECB              @Y30LB24*/
         ST    @11,RPLVECB(,RPLVPTR)                               0157
*   RPLVVICB=VVIPTR;                /* SET MSVC CB PTR       @Y30LB24*/
         ST    VVIPTR,RPLVVICB(,RPLVPTR)                           0158
*   RPLFTIME=ON;                    /* INDICATE SECOND ENTRY @Y30LB24*/
*   RPLVPOST=OFF;                   /* INDICATE NO ECB POST  @Y30LB24*/
         OI    RPLFTIME(RPLVPTR),B'10000000'                       0160
         NI    RPLVPOST(RPLVPTR),B'10111111'                       0160
*   RPLVUTIL(1:REG3)=PARMLIST;      /* COPY PARM             @Y30LB24*/
         LR    @11,REG3                                            0161
         BCTR  @11,0                                               0161
         EX    @11,@SM01305                                        0161
