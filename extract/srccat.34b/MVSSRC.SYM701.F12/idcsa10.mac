         L     @10,@PC00001                                        0509
         L     @10,GDTSAH(,@10)                                    0509
         L     @10,SAHSTA(,@10)                                    0509
         NI    STAVDMNT(@10),B'10101111'                           0509
*     END;                          /*                       @Y30LB35*/
*                                                                  0511
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE STACLEAR BIT IS ON (INDICATING A REAL          @Y30LB35*/
*   /* UCB MUST BE POSTED), DO THE FOLLOWING TO              @Y30LB35*/
*   /* POST IT:                                              @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0511
*   IF STACLEAR=ON                  /*                       @Y30LB35*/
*     THEN                          /*                       @Y30LB35*/
@RF00498 L     @10,@PC00001                                        0511
         L     @10,GDTSAH(,@10)                                    0511
         L     @10,SAHSTA(,@10)                                    0511
         TM    STACLEAR(@10),B'00001000'                           0511
         BNO   @RF00511                                            0511
*     DO;                           /*                       @Y30LB35*/
*                                                                  0512
*       /*************************************************************/
*       /*                                                           */
*       /* CONSTRUCT THE SVC 82 ARGUMENT LIST TO POST        @Y30LB35*/
*       /* A UCB. RETRIEVE THE VOLUME SERIAL NUMBER          @Y30LB35*/
*       /* FROM THE STARVOL FIELD AND THE VTOC TTR           @Y30LB35*/
*       /* FROM THE STARTTR FIELD. GET THE UCB               @Y30LB35*/
*       /* ADDRESS FROM THE STARUCB FIELD.                   @Y30LB35*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0513
*       SV82LIST=''B;               /*                       @Y30LB35*/
         LA    @09,SVC82ARG                                        0513
         XC    SV82LIST(16,@09),SV82LIST(@09)                      0513
*       SV82CODE=POSTCD;            /*                       @Y30LB35*/
         MVC   SV82CODE(1,@09),POSTCD                              0514
*       SV82UCBP=STARUCB;           /*                       @Y30LB35*/
         MVC   SV82UCBP(3,@09),STARUCB+1(@10)                      0515
*       SV82VOLP=ADDR(STARVOL);     /*                       @Y30LB35*/
         LA    @03,STARVOL(,@10)                                   0516
         ST    @03,SV82VOLP(,@09)                                  0516
*       SV82TTRP=ADDR(STARTTR);     /*                       @Y30LB35*/
         LA    @10,STARTTR(,@10)                                   0517
         ST    @10,SV82TTRP(,@09)                                  0517
*       RESPECIFY                                                  0518
*         REG1 RESTRICTED;          /*                       @Y30LB35*/
*       REG1=ADDR(SV82LIST);        /*                       @Y30LB35*/
*                                                                  0519
         LR    REG1,@09                                            0519
*       /*************************************************************/
*       /*                                                           */
*       /* ISSUE SVC 82. IGNORE ANY ERRORS.                  @Y30LB35*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0520
*       SVC(82);                    /*                       @Y30LB35*/
         SVC   82                                                  0520
*       RESPECIFY                                                  0521
*         REG1 UNRESTRICTED;        /*                       @Y30LB35*/
*                                                                  0521
*       /*************************************************************/
*       /*                                                           */
*       /* SET THE STACLEAR BIT OFF.                         @Y30LB35*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0522
*       STACLEAR=OFF;               /*                       @Y30LB35*/
         L     @10,@PC00001                                        0522
         L     @10,GDTSAH(,@10)                                    0522
         L     @10,SAHSTA(,@10)                                    0522
         NI    STACLEAR(@10),B'11110111'                           0522
*     END;                          /*                       @Y30LB35*/
*                                                                  0524
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE STAVENQ BIT IS THE ONLY RECOVERY BIT           @Y30LB35*/
*   /* ON, DO THE FOLLOWING TO DEQUEUE THE VOLUME            @Y30LB35*/
*   /* SERIAL NUMBER. (IF THE STAVMNT AND STAVPOST           @Y30LB35*/
*   /* BITS ARE STILL ON, THE VOLUME HAS NOT BEEN            @Y30LB35*/
*   /* DEMOUNTED AND SO THE VOLUME SERIAL NUMBER             @Y30LB35*/
*   /* DOES NOT GET DEQUEUED.)                               @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0524
*   IF STAVENQ=ON&STAVMNT=OFF&STAVPOST=OFF/*                 @Y30LB35*/
*     THEN                          /*                       @Y30LB35*/
@RF00511 L     @10,@PC00001                                        0524
         L     @10,GDTSAH(,@10)                                    0524
         L     @10,SAHSTA(,@10)                                    0524
         TM    STAVENQ(@10),B'00100000'                            0524
         BNO   @RF00524                                            0524
         TM    STAVMNT(@10),B'10010000'                            0524
         BNZ   @RF00524                                            0524
*     DO;                           /*                       @Y30LB35*/
*       TCBP=STATCB;                /*                       @Y30LB35*/
         L     @09,STATCB(,@10)                                    0526
         ST    @09,TCBP                                            0526
*       VOLSER=STAVVOL;             /*                       @Y30LB35*/
         MVC   VOLSER(6),STAVVOL(@10)                              0527
*       DEQEXEC(1:DEQLEN)=DEQCORE(1:DEQLEN);/*               @Y30LB35*/
*                                                                  0528
         L     @10,DEQLEN                                          0528
         BCTR  @10,0                                               0528
         EX    @10,@SM02046                                        0528
*       /*************************************************************/
*       /*                                                           */
*       /* ISSUE THE DEQ MACRO - MAJOR NAME IS SYSZVOLS,     @Y30LB35*/
*       /* MINOR NAME IS THE VOLUME SERIAL NUMBER FROM       @Y30LB35*/
*       /* THE STAVVOL FIELD. ISSUE DEQ WITH THE HAVE        @Y30LB35*/
*       /* OPTION, IN CASE ENQUEUE BIT WAS SET BEFORE        @Y30LB35*/
*       /* THE ENQUEUE HAS ACTUALLY BEEN COMPLETED.          @Y30LB35*/
*       /* ISSUE A DEQ DIRECTED TO THE INITIATOR'S           @Y30LB35*/
*       /* TCB WHOSE ADDRESS IS IN THE STATCB FIELD.         @Y30LB35*/
*       /* IGNORE ANY ERRORS.                                @Y30LB35*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0529
*LDEQ1:                             /*                       @Y30LB35*/
*       GENERATE REFS(MAJNAME,VOLSER,TCBP,DEQLIST);                0529
LDEQ1    DS    0H                                                  0529
*                                                         /*@Y30LB35*/
        DEQ (MAJNAME,VOLSER,6,SYSTEM),RET=HAVE,MF=(E,DEQLIST),TCB=TCBP
*     END;                          /*                       @Y30LB35*/
*                                                                  0531
*   /*****************************************************************/
*   /*                                                               */
*   /* CONTROL IS RETURNED TO STAEEXIT WITH                  @Y30LB35*/
*   /* A ZERO RETURN CODE IN REG 15.                         @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0531
*   RETURN CODE(0);                 /*                       @Y30LB35*/
@RF00524 SLR   @15,@15                                             0531
@EL00005 DS    0H                                                  0531
@EF00005 DS    0H                                                  0531
@ER00005 L     @14,@SA00005                                        0531
         BR    @14                                                 0531
