         TITLE '******** IDCSA10:  SYSTEM ADAPTOR USTAE MODULE *********
               *        '
        MACRO
        GETMAIN &R,&LV=
        L     REG15,0(REG1)           GDTTBL ADDRESS
        L     REG15,GDTPRL(REG15)     PROL ROUTINE ADDRESS
        CNOP  2,4                     BOUNDARY ALIGNMENT
        LA    REG14,*+14              RETURN ADDRESS
        BALR  REG1,REG15              LINKAGE TO PROL ROUTINE
        DC    A(MODID)                MODULE ID ADDRESS
        DC    A(@SIZDATD)             AUTOMATIC STORAGE SIZE ADDRESS
        MEND
IDCSA10  CSECT ,                                                   0002
@MAINENT DS    0H                                                  0002
         USING *,@15                                               0002
         B     @PROLOG                                             0002
         DC    AL2(@EP00002-@MAINENT)
         DC    AL1(16)                                             0002
         DC    C'IDCSA10   77.062'                                 0002
IDCSAST  DS    0H                                                  0002
         USING *,@15                                               0002
         B     @PROLOG                                             0002
         DC    AL2(@EP00247-IDCSAST)
         ENTRY IDCSAST
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0002
         BALR  @12,0                                               0002
@PSTART  DS    0H                                                  0002
         USING @PSTART,@12                                         0002
         L     @00,@SIZDATD                                        0002
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0002
         USING @DATD,@11                                           0002
         ST    @13,@SA00001+4                                      0002
         LM    @15,@01,16(@13)                                     0002
         ST    @11,8(,@13)                                         0002
         LR    @13,@11                                             0002
         AH    @15,4(,@15)                                         0002
         BR    @15                                                 0002
@EP00002 DS    0H                                                  0003
*                                                                  0247
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY POINT THROUGH WHICH USTAE IS INVOKED            @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0247
*IDCSAST:                                                          0247
*   ENTRY(GDTTBL,STAETYPE);         /*                       @Y30LB35*/
*                                                                  0247
         B     @EC00247                                            0247
@EP00247 MVC   @PC00001(8),0(@01)                                  0247
@EC00247 DS    0H                                                  0248
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS IS THE START OF THE MAIN PROCEDURE.              @Y30LB35*/
*   /* HERE INITIALIZATION TAKES PLACE. THE                  @Y30LB35*/
*   /* STAEPARM ADDRESSING IS SET UP. ALSO HERE              @Y30LB35*/
*   /* IS THE MAIN ENTRY TRACE POINT 'STST' AND A            @Y30LB35*/
*   /* FULL REGION DUMP POINT 'STBG'. FINALLY THE            @Y30LB35*/
*   /* RETURN CODE IS INITIALIZED TO 0.                      @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0248
*   RESPECIFY                                                      0248
*     STAEPARM BASED(ADDR(STAEWORK));/*                      @Y30LB35*/
*   OLDERID2=NEWERID2;                                             0249
         L     @10,@PC00001                                        0249
         L     @15,GDTTR2(,@10)                                    0249
         MVC   @TS00001(95),NEWERID2(@15)                          0249
         MVC   OLDERID2(95,@15),@TS00001                           0249
*   NEWID2='STST';                  /*                       @Y30LB35*/
         MVC   NEWID2(4,@15),@CC01913                              0250
*   IF GDTDBG=NULLPTR THEN                                         0251
         L     @10,GDTDBG(,@10)                                    0251
         LTR   @10,@10                                             0251
         BZ    @RT00251                                            0251
*     ;                                                            0252
*   ELSE                                                           0253
*     CALL IDCDB010(GDTTBL,'STBG'); /*                       @Y30LB35*/
         L     @10,@PC00001                                        0253
         ST    @10,@AL00001                                        0253
         LA    @15,@CC01914                                        0253
         ST    @15,@AL00001+4                                      0253
         MVI   @AL00001+4,X'80'                                    0253
         L     @15,GDTDBG(,@10)                                    0253
         LA    @01,@AL00001                                        0253
         BALR  @14,@15                                             0253
*   TESTRC=0;                       /*                       @Y30LB35*/
*                                                                  0254
@RT00251 SLR   @10,@10                                             0254
         STH   @10,TESTRC                                          0254
*   /*****************************************************************/
*   /*                                                               */
*   /* HERE THE DETERMINATION IS MADE AS TO WHICH            @Y30LB35*/
*   /* SERVICE IS REQUESTED. SETSTAE IS CALLED               @Y30LB35*/
*   /* IF THE REQUEST IS "SET".                              @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0255
*   IF STAETYPE=SET                 /*                       @Y30LB35*/
*     THEN                          /*                       @Y30LB35*/
         L     @10,@PC00001+4                                      0255
         CLC   STAETYPE(8,@10),SET                                 0255
         BNE   @RF00255                                            0255
*     DO;                           /*                       @Y30LB35*/
*       CALL SETSTAE;               /*                       @Y30LB35*/
         BAL   @14,SETSTAE                                         0257
*       RESPECIFY                                                  0258
*         RTNREG RSTD;                                             0258
*       TESTRC=RTNREG;                                             0259
         STH   RTNREG,TESTRC                                       0259
*       RESPECIFY                                                  0260
*         RTNREG UNRSTD;            /*                       @Y30LB35*/
*     END;                          /*                       @Y30LB35*/
*   ELSE                            /*                       @Y30LB35*/
*                                                                  0262
*     /***************************************************************/
*     /*                                                             */
*     /* CANCEL IS CALLED IF THE REQUEST IS "CANCEL".        @Y30LB35*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0262
*     IF STAETYPE=CANCEL            /*                       @Y30LB35*/
*       THEN                        /*                       @Y30LB35*/
         B     @RC00255                                            0262
@RF00255 L     @10,@PC00001+4                                      0262
         CLC   STAETYPE(8,@10),CANCEL                              0262
         BNE   @RF00262                                            0262
*       DO;                         /*                       @Y30LB35*/
*         CALL CANSTAE;             /*                       @Y30LB35*/
         BAL   @14,CANSTAE                                         0264
*         RESPECIFY                                                0265
*           RTNREG RSTD;                                           0265
*         TESTRC=RTNREG;                                           0266
         STH   RTNREG,TESTRC                                       0266
*         RESPECIFY                                                0267
*           RTNREG UNRSTD;          /*                       @Y30LB35*/
*       END;                        /*                       @Y30LB35*/
*                                                                  0268
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE REQUEST IS INVALID UABORT(40) IS               @Y30LB35*/
*   /* ISSUED.                                               @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0269
*     ELSE                          /*                       @Y30LB35*/
*       DO;                                                        0269
         B     @RC00262                                            0269
@RF00262 DS    0H                                                  0270
*         RESPECIFY                                                0270
*          (REG13,                                                 0270
*           REG14,                                                 0270
*           REG15) RSTD;                                           0270
*         REG15=BADARG;                                            0271
         LA    REG15,40                                            0271
*         REG14=GDTABT;                                            0272
         L     @10,@PC00001                                        0272
         L     REG14,GDTABT(,@10)                                  0272
*         REG13=GDTABH;                                            0273
         L     REG13,GDTABH(,@10)                                  0273
*         GEN(BR    REG14);                                        0274
         BR    REG14
*         RESPECIFY                                                0275
*          (REG13,                                                 0275
*           REG14,                                                 0275
*           REG15) UNRSTD;                                         0275
*       END;                        /*                       @Y30LB35*/
*                                                                  0276
*   /*****************************************************************/
*   /*                                                               */
*   /* HERE IDCSA10 WILL EXIT. BEFORE EXIT, THE              @Y30LB35*/
*   /* DUMP POINT 'STEN' IS SET UP.                          @Y30LB35*/
*   /* THE RETURN CODE SAVED IN TESTRC IS RETURNED           @Y30LB35*/
*   /* 0 SUCCESSFUL OR 4 ERROR                               @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0277
*   IF GDTDBG=NULLPTR THEN                                         0277
@RC00262 DS    0H                                                  0277
@RC00255 L     @10,@PC00001                                        0277
         L     @10,GDTDBG(,@10)                                    0277
         LTR   @10,@10                                             0277
         BZ    @RT00277                                            0277
*     ;                                                            0278
*   ELSE                                                           0279
*     CALL IDCDB010(GDTTBL,'STEN'); /*                       @Y30LB35*/
         L     @10,@PC00001                                        0279
         ST    @10,@AL00001                                        0279
         LA    @15,@CC01917                                        0279
         ST    @15,@AL00001+4                                      0279
         MVI   @AL00001+4,X'80'                                    0279
         L     @15,GDTDBG(,@10)                                    0279
         LA    @01,@AL00001                                        0279
         BALR  @14,@15                                             0279
*   CALL IDCSAEP0(GDTTBL,MODID,TESTRC);/*                    @Y30LB35*/
@RT00277 L     @10,@PC00001                                        0280
         ST    @10,@AL00001                                        0280
         LA    @15,MODID                                           0280
         ST    @15,@AL00001+4                                      0280
         LA    @15,TESTRC                                          0280
         ST    @15,@AL00001+8                                      0280
         MVI   @AL00001+8,X'80'                                    0280
         L     @15,GDTEPL(,@10)                                    0280
         LA    @01,@AL00001                                        0280
         BALR  @14,@15                                             0280
*                                                                  0281
*/********************************************************************/
*/*                                                          @Y30LB35*/
*/* PROCEDURE NAME - SETSTAE                                 @Y30LB35*/
*/*                                                          @Y30LB35*/
*/* FUNCTION -  THE FUNCTIONS PERFORMED WILL BE              @Y30LB35*/
*/*             TO ISSUE ESTAE MACRO, BUILD THE STAEPARM,    @Y30LB35*/
*/*             PUT THE ADDRESS OF STAEPARM INTO SAHSTA OF   @Y30LB35*/
*/*             SYSTEM ADAPTOR HISTORICAL DATA AREA (SAHIST),@Y30LB35*/
*/*             AND PRINT MESSAGE IF ESTAE WAS UNSUCCESSFUL. @Y30LB35*/
*/*                                                          @Y30LB35*/
*/* INPUT -     SAHIST (SYSTEM ADAPTOR HISTORICAL DATA AREA) @Y30LB35*/
*/*                                                          @Y30LB35*/
*/* OUTPUT -    SAHSTA OF SAHIST                             @Y30LB35*/
*/*             STAEXSAV OF STAEPARM                         @Y30LB35*/
*/*             STAOFF   OF STAEPARM                         @Y30LB35*/
*/*             DARGSMOD OF DARGLIST                         @Y30LB35*/
*/*             DARGSENT OF DARGLIST                         @Y30LB35*/
*/*             DARGDBP  OF DARGLIST                         @Y30LB35*/
*/*             DARGILP  OF DARGLIST                         @Y30LB35*/
*/*             STAEPARM                                     @Y30LB35*/
*/*             REGISTER 15 = 0, SUCCESSFUL                  @Y30LB35*/
*/*                           4, ERROR DURING ESTAE          @Y30LB35*/
*/*                                                          @Y30LB35*/
*/********************************************************************/
*                                                                  0281
*                                   /*                       @Y30LB35*/
*SETSTAE:                                                          0281
*   PROC;                           /*                       @Y30LB35*/
*                                                                  0281
@EL00001 L     @13,4(,@13)                                         0281
@EF00001 L     @00,@SIZDATD                                        0281
         LR    @01,@11                                             0281
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0281
         BR    @14                                                 0281
SETSTAE  STM   @14,@12,@SA00002                                    0281
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCAL DECLARES                                        @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0282
*   GENERATE DATA DEFS(STAELIST);                                  0282
*                                                                  0282
*   /*****************************************************************/
*   /*                                                               */
*   /* DEFINE STAE ARGUMENT LIST                             @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0283
*   DCL                                                            0283
*     STAELIST CHAR(20) GEND STATIC LOCAL;/*                 @Y30LB35
*                                      DEF STAELIST TO PLS   @Y30LB35*/
*   DCL                                                            0284
*     STAEARG CHAR(20) BDY(WORD);   /*                       @Y30LB35
*                                      DEF CORE FOR LIST     @Y30LB35*/
*   DCL                                                            0285
*     HEADER CHAR(8) INIT('STAEPARM');/*                     @Y30LB35
*                                      HEADER - STAEPARM     @Y30LB35*/
*                                                                  0286
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP TRACE POINT 'STSS' IN TRACE TABLE              @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0286
*   OLDERID2=NEWERID2;                                             0286
         L     @10,@PC00001                                        0286
         L     @15,GDTTR2(,@10)                                    0286
         MVC   @TS00001(95),NEWERID2(@15)                          0286
         MVC   OLDERID2(95,@15),@TS00001                           0286
*   NEWID2='STSS';                  /*                       @Y30LB35*/
*                                                                  0287
         MVC   NEWID2(4,@15),@CC01926                              0287
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE ADDRESS OF THE STAE PARAMETER LIST             @Y30LB35*/
*   /* IS NOT IN THE SAHSTA FIELD, INDICATING A NEW          @Y30LB35*/
*   /* STAE PARAMETER LIST IS REQUIRED, INITIALIZE           @Y30LB35*/
*   /* THE ENTIRE STAEPARM AREA TO ZERO AND PUT THE          @Y30LB35*/
*   /* ADDRESS OF THE AREA IN THE SAHSTA FIELD               @Y30LB35*/
*   /* OF THE SYSTEM ADAPTOR HISTORICAL DATA AREA,           @Y30LB35*/
*   /* AND INITIALIZE THE HEADER OF STAEPARM AREA.           @Y30LB35*/
*   /* OTHERWISE, USE THE STAE PARAMETER LIST THAT           @Y30LB35*/
*   /* IS ESTABLISTED BUT SET THE STAOFF BIT OFF.            @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0288
*   IF SAHSTA=NULLPTR               /*                       @Y30LB35*/
*     THEN                          /*                       @Y30LB35*/
         L     @10,GDTSAH(,@10)                                    0288
         L     @15,SAHSTA(,@10)                                    0288
         LTR   @15,@15                                             0288
         BNZ   @RF00288                                            0288
*     DO;                           /*                       @Y30LB35*/
*       STAEWORK=''B;               /*                       @Y30LB35*/
         XC    STAEWORK(58),STAEWORK                               0290
*       SAHSTA=ADDR(STAEWORK);      /*                       @Y30LB35*/
         LA    @15,STAEWORK                                        0291
         ST    @15,SAHSTA(,@10)                                    0291
*       STAHEAD=HEADER;             /*                       @Y30LB35*/
*                                                                  0292
         MVC   STAHEAD(8,@15),HEADER                               0292
*       /*************************************************************/
*       /*                                                           */
*       /* SAVE THE REGISTERS TO USE DURING RECOVERY         @Y30LB35*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0293
*       RESPECIFY                                                  0293
*         REG1 RESTRICTED;          /*                       @Y30LB35*/
*       REG1=ADDR(SAVEREGS);        /*                       @Y30LB35*/
         LA    REG1,SAVEREGS                                       0294
*       GEN(STM 0,15,0(REG1));      /*                       @Y30LB35*/
         STM 0,15,0(REG1)
*       RESPECIFY                                                  0296
*         REG1 UNRESTRICTED;        /*                       @Y30LB35*/
*       STAEXSAV=ADDR(SAVEREGS);    /*                       @Y30LB35*/
         LA    @10,STAEWORK                                        0297
         LA    @15,SAVEREGS                                        0297
         ST    @15,STAEXSAV(,@10)                                  0297
*     END;                          /*                       @Y30LB35*/
*   ELSE                            /*                       @Y30LB35*/
*     STAOFF=OFF;                   /*                       @Y30LB35*/
         B     @RC00288                                            0299
@RF00288 LA    @10,STAEWORK                                        0299
         NI    STAOFF(@10),B'10111111'                             0299
*                                                                  0300
*   /*****************************************************************/
*   /*                                                               */
*   /* HERE THE INITIALIZATION IS DONE TO ISSUE              @Y30LB35*/
*   /* ESTAE.                                                @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0300
*   RESPECIFY                                                      0300
*    (REG2,                                                        0300
*     REG3,                                                        0300
*     REG4) RESTRICTED;             /*                       @Y30LB35*/
@RC00288 DS    0H                                                  0301
*   STAEARG=STAELIST;               /*                       @Y30LB35*/
         MVC   STAEARG(20),STAELIST                                0301
*   REG2=ADDR(STAEEXIT);            /*                       @Y30LB35*/
         LA    REG2,STAEEXIT                                       0302
*   REG3=ADDR(STAEPARM);            /*                       @Y30LB35*/
         LA    REG3,STAEWORK                                       0303
*   REG4=ADDR(STAEARG);             /*                       @Y30LB35*/
*                                                                  0304
         LA    REG4,STAEARG                                        0304
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE ESTAE                                           @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0305
*LSTAE1:                                                           0305
*   GEN(ESTAE   (REG2),PARAM=(REG3),TERM=YES,MF=(E,(REG4)));       0305
LSTAE1   ESTAE   (REG2),PARAM=(REG3),TERM=YES,MF=(E,(REG4))
*   RESPECIFY                                                      0306
*    (REG2,                                                        0306
*     REG3,                                                        0306
*     REG4) UNRESTRICTED;           /*                       @Y30LB35*/
*   RESPECIFY                                                      0307
*     RTNREG RSTD;                                                 0307
*   TESTRC=RTNREG;                                                 0308
         STH   RTNREG,TESTRC                                       0308
*   RESPECIFY                                                      0309
*     RTNREG UNRSTD;                /*                       @Y30LB35*/
*                                                                  0309
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE ESTAE RETURN CODE IS NON-ZERO, SET THE         @Y30LB35*/
*   /* SAHSTA FIELD TO 0, AND ISSUE UPRINT TO PRINT          @Y30LB35*/
*   /* MESSAGE SA6-12, INSERTING THE RETURN CODE             @Y30LB35*/
*   /* IN HEX FROM REGISTER 15. SET RETURN CODE              @Y30LB35*/
*   /* TO RCFAIL.                                            @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0310
*   IF TESTRC^=0                    /*                       @Y30LB35*/
*     THEN                          /*                       @Y30LB35*/
         LH    @10,TESTRC                                          0310
         SLR   @15,@15                                             0310
         CR    @10,@15                                             0310
         BE    @RF00310                                            0310
*     DO;                           /*                       @Y30LB35*/
*       SAHSTA=NULLPTR;             /*                       @Y30LB35*/
         L     @10,@PC00001                                        0312
         L     @01,GDTSAH(,@10)                                    0312
         ST    @15,SAHSTA(,@01)                                    0312
*       DDSTRU=ADDR(PRINTARG);      /*                       @Y30LB35*/
         LA    @15,PRINTARG                                        0313
         ST    @15,DDSTRU                                          0313
*       DARGLIST=''B;               /*                       @Y30LB35*/
         XC    DARGLIST(20,@15),DARGLIST(@15)                      0314
*       DARGSMOD=SA06MSG;           /*                       @Y30LB35*/
         MVC   DARGSMOD(3,@15),@CC01881                            0315
*       DARGSENT=MSG2391I;          /*                       @Y30LB35*/
         MVI   DARGSENT(@15),X'0B'                                 0316
*       DARGDBP=ADDR(TESTRC);       /*                       @Y30LB35*/
         LA    @14,TESTRC                                          0317
         ST    @14,DARGDBP(,@15)                                   0317
*       DARGILP=LENGTH(TESTRC);     /*                       @Y30LB35*/
         MVC   DARGILP(2,@15),@CH00147                             0318
*       CALL AMSTP011(GDTTBL,PRTFILE,DDSTRU);/*              @Y30LB35*/
         ST    @10,@AL00001                                        0319
         LA    @15,@CF00108                                        0319
         ST    @15,@AL00001+4                                      0319
         LA    @15,DDSTRU                                          0319
         ST    @15,@AL00001+8                                      0319
         MVI   @AL00001+8,X'80'                                    0319
         L     @15,GDTPRT(,@10)                                    0319
         LA    @01,@AL00001                                        0319
         BALR  @14,@15                                             0319
*       TESTRC=4;                   /*                       @Y30LB35*/
         MVC   TESTRC(2),@CH00034                                  0320
*     END;                          /*                       @Y30LB35*/
*                                                                  0321
*   /*****************************************************************/
*   /*                                                               */
*   /* HERE THE CONTROL IS RETURNED TO IDCSA10               @Y30LB35*/
*   /* WITH A RETURN CODE IN 15 OF 0 SUCCESSFUL              @Y30LB35*/
*   /* OR 4 ESTAE NOT ESTABLISHED.                           @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0322
*   RETURN CODE(TESTRC);            /*                       @Y30LB35*/
@RF00310 LH    @15,TESTRC                                          0322
         L     @14,@SA00002                                        0322
         LM    @00,@12,@SA00002+8                                  0322
         BR    @14                                                 0322
*   END SETSTAE;                    /*                       @Y30LB35*/
*                                                                  0324
*/********************************************************************/
*/*                                                          @Y30LB35*/
*/* PROCEDURE NAME - CANSTAE                                 @Y30LB35*/
*/*                                                          @Y30LB35*/
*/* FUNCTION -  THIS PROCEDURE ISSUES ESTAE 0 AND REMOVES    @Y30LB35*/
*/*             THE STAEPARM ADDRESS FROM THE SAHSTA OF      @Y30LB35*/
*/*             THE SAHIST (SYSTEM ADAPTOR HISTORICAL DATA   @Y30LB35*/
*/*             AREA)                                        @Y30LB35*/
*/*                                                          @Y30LB35*/
*/* INPUT -     STAOFF OF STAEPARM                           @Y30LB35*/
*/*                                                          @Y30LB35*/
*/* OUTPUT -    SAHSTA OF SAHIST                             @Y30LB35*/
*/*             REGISTER 15 = 0, SUCCESSFUL                  @Y30LB35*/
*/*                                                          @Y30LB35*/
*/********************************************************************/
*                                                                  0324
*                                   /*                       @Y30LB35*/
*CANSTAE:                                                          0324
*   PROC;                           /*                       @Y30LB35*/
CANSTAE  STM   @14,@12,@SA00003                                    0324
*                                                                  0325
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP TRACE POINT 'STCS' IN TRACE TABLE              @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0325
*   OLDERID2=NEWERID2;                                             0325
         L     @10,@PC00001                                        0325
         L     @10,GDTTR2(,@10)                                    0325
         MVC   @TS00001(95),NEWERID2(@10)                          0325
         MVC   OLDERID2(95,@10),@TS00001                           0325
*   NEWID2='STCS';                  /*                       @Y30LB35*/
*                                                                  0326
         MVC   NEWID2(4,@10),@CC01934                              0326
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE STAOFF BIT IS OFF, ESTAE HAS NOT               @Y30LB35*/
*   /* BEEN CANCELLED YET, SO ISSUE THE ESTAE                @Y30LB35*/
*   /* MACRO TO CANCEL THE ESTAE. IGNORE ANY ERRORS.         @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0327
*   IF STAOFF=OFF                   /*                       @Y30LB35*/
*     THEN                          /*                       @Y30LB35*/
         LA    @10,STAEWORK                                        0327
         TM    STAOFF(@10),B'01000000'                             0327
         BNZ   @RF00327                                            0327
*     DO;                           /*                       @Y30LB35*/
*       GEN(ESTAE 0);               /*                       @Y30LB35*/
*                                                                  0329
         ESTAE 0
*       /*************************************************************/
*       /*                                                           */
*       /* SET THE SAHSTA FIELD TO 0 TO INDICATE THE         @Y30LB35*/
*       /* ESTAE IS NOT IN EFFECT                            @Y30LB35*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0330
*       SAHSTA=NULLPTR;             /*                       @Y30LB35*/
         SLR   @10,@10                                             0330
         L     @15,@PC00001                                        0330
         L     @01,GDTSAH(,@15)                                    0330
         ST    @10,SAHSTA(,@01)                                    0330
*       TESTRC=0;                   /*                       @Y30LB35*/
         STH   @10,TESTRC                                          0331
*     END;                          /*                       @Y30LB35*/
*                                                                  0332
*   /*****************************************************************/
*   /*                                                               */
*   /* CONTROL IS RETURNED TO IDCSA10 WITH A                 @Y30LB35*/
*   /* ZERO CODE IN REGISTER 15.                             @Y30LB35*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0333
*   RETURN CODE(TESTRC);            /*                       @Y30LB35*/
@RF00327 LH    @15,TESTRC                                          0333
         L     @14,@SA00003                                        0333
         LM    @00,@12,@SA00003+8                                  0333
         BR    @14                                                 0333
