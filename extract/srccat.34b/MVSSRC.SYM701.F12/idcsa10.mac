*/*             STARVOL  FROM STAEPARM                       @Y30LB35*/
*/*             STARTTR  FROM STAEPARM                       @Y30LB35*/
*/*             STAVMNT  FROM STAEPARM                       @Y30LB35*/
*/*             STAVPOST FROM STAEPARM                       @Y30LB35*/
*/*             STACLEAR FROM STAEPARM                       @Y30LB35*/
*/*             STAVENQ  FROM STAEPARM                       @Y30LB35*/
*/*             STATCB   FROM STAEPARM                       @Y30LB35*/
*/*                                                          @Y30LB35*/
*/* OUTPUT -    DMNTLPTR                                     @Y30LB35*/
*/*             DEMNTLST                                     @Y30LB35*/
*/*             ISUEDRC                                      @Y30LB35*/
*/*             STAVMNT  OF STAEPARM                         @Y30LB35*/
*/*             STAVDMNT OF STAEPARM                         @Y30LB35*/
*/*             STACLEAR OF STAEPARM                         @Y30LB35*/
*/*             STAVPOST OF STAEPARM                         @Y30LB35*/
*/*             STAVENQ  OF STAEPARM                         @Y30LB35*/
*/*             SV82CODE OF SV82LIST                         @Y30LB35*/
*/*             SV82UCBP OF SV82LIST                         @Y30LB35*/
*/*             SV82WRKP OF SV82LIST                         @Y30LB35*/
*/*             SV82VOLP OF SV82LIST                         @Y30LB35*/
*/*             SV82TTRP OF SV82LIST                         @Y30LB35*/
*/*             VOLSER                                       @Y30LB35*/
*/*             TCBP                                         @Y30LB35*/
