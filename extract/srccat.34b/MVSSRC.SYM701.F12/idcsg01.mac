         L     @10,@PC00001                                        0237
         L     @15,GDTTR2(,@10)                                    0237
         MVC   @TS00001(95),NEWERID2(@15)                          0237
         MVC   OLDERID2(95,@15),@TS00001                           0237
*   NEWID2='SSAA';                  /*                       @Y30LB39
*                                                            @Y30LB39*/
*                                                                  0238
         MVC   NEWID2(4,@15),@CC00872                              0238
*   /*****************************************************************/
