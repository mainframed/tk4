*   /*                                                       @Y30LB39*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0232
*                                   /*                       @Y30LB39*/
*   DECLARE                                                        0232
*     ENTRYNUM FIXED(8) UNSIGNED;   /*                       @Y30LB39
*                                                            @Y30LB39*/
*                                                                  0232
*   /*****************************************************************/
*   /*                                                               */
*   /*                                                       @Y30LB39*/
