*   /*                                                               */
*   /*****************************************************************/
*                                                                  0305
*   DECLARE                                                        0305
*     CCFAIL FIXED(31) STATIC INIT('2F'X);/* COPY CARTRIDGE FAILED 0305
*                                      DUE TO EQUIP MENT EJECT     0305
*                                      COMPLETE                      */
*   DECLARE                                                        0306
*     DTMSAVE FIXED(31);            /* SAVE OF DARGDTM       @ZA26543*/
*   DECLARE                                                        0307
*     SEQ#1 FIXED(31) STATIC INIT(1);/* REPLACE SEQUENCE 1           */
*   DECLARE                                                        0308
