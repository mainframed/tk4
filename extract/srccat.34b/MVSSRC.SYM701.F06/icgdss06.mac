         TITLE 'ICGDSS06 MSC TABLE CREATE, SCAN ''MSFX'' STATEMENT     *
                          '
ICGDSS06 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ICGDSS06  76.337'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
*                                                                  0023
*/********************************************************** @Y30LB12*/
*/*                                                          @Y30LB12*/
*/*            CALL S06INIT, S06SCAN, ANALSIZE.  IF NO       @Y30LB12*/
*/*            TERMINATING ERRORS, CALL ANALREST, S06BUILD.  @Y30LB12*/
*/*            RETURN TO ICGDSSCN.                           @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @Y30LB12*/
*                                                                  0023
*   CALL S06INIT;                   /* INITIALIZE            @Y30LB12*/
         BAL   @14,S06INIT                                         0023
*   CALL S06SCAN;                   /* GET KEYWDS, PARMS     @Y30LB12*/
         BAL   @14,S06SCAN                                         0024
*   CALL ANALSIZE;                  /* CHECK SIZE PARM       @Y30LB12*/
         BAL   @14,ANALSIZE                                        0025
*   IF STOPSCAN^='1'B               /* TERM. ERROR FLAG      @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         TM    STOPSCAN,B'01000000'                                0026
         BO    @RF00026                                            0026
*     CALL S06BUILD;                /* UPDATE COMTAB         @Y30LB12*/
         BAL   @14,S06BUILD                                        0027
*   ELSE                                                           0028
*     ;                             /*                       @Y30LB12*/
@RF00026 DS    0H                                                  0029
*   RETURN;                         /* RET. TO ICGDSSCN      @Y30LB12*/
@EL00001 L     @13,4(,@13)                                         0029
@EF00001 L     @00,@SIZDATD                                        0029
         LR    @01,@11                                             0029
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0029
         BR    @14                                                 0029
*                                                                  0030
*/********************************************************** @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - S06INIT                                 @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - CLEAR WORK AREAS GOTTEN WITH THIS MODULE.     @Y30LB12*/
*/*            DETERMINE IF THE COMMAND WORD (MSFX) IS       @YD00113*/
*/*            IN THE PROPER FORMAT AND CONTAINS A VALID     @Y30LB12*/
*/*            LIBRARY NUMBER IN ITS SUFFIX.                 @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @Y30LB12*/
*                                                                  0030
*S06INIT:                                                          0030
*   PROCEDURE;                      /*                       @Y30LB12*/
S06INIT  STM   @14,@12,@SA00002                                    0030
*   LIBNUM=0;                       /* LOGICAL LIB WORKA     @Y30LB12*/
         SLR   @10,@10                                             0031
         STH   @10,LIBNUM                                          0031
*   WORKDSM='00'X;                  /* DSM INTERFACE ID      @Y30LB12*/
         MVI   WORKDSM,X'00'                                       0032
*   WORKSIZE='00'X;                 /* NUMBER OF FRAMES      @Y30LB12*/
         MVI   WORKSIZE,X'00'                                      0033
*   WORKMAP='I';                    /* LOADING SEQUENCE      @Y30LB12*/
         MVI   WORKMAP,C'I'                                        0034
*   WORKEXIT='        ';            /* USER EXIT NAME        @Y30LB12*/
         MVC   WORKEXIT(8),@CC00379                                0035
*   HAVES='00'X;                    /* KEYWDS SPECIFIED      @Y30LB12*/
         MVI   HAVES,X'00'                                         0036
*   LIBMSGID='00'X;                 /* FLAG BYTE             @Y30LB12*/
*                                                                  0037
         MVI   LIBMSGID,X'00'                                      0037
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @Y30LB12*/
*   /* TEST 'X' PORTION OF MSFX COMMAND TO SET INDEX         @YD00113*/
*   /* (LIBNUM) POINTER TO ENTRY IN TABLE                    @YD00113*/
*   /* ***********************************************       @Y30LB12*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0038
*   IF SCANDATA(4)='0'              /* CMD WORD 'LIB0'       @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         L     @10,STARTCOL(,R4)                                   0038
         CLI   SCANDATA+3(@10),C'0'                                0038
         BNE   @RF00038                                            0038
*     LIBNUM=1;                     /* PTR TO 1ST ENTRY      @Y30LB12*/
         MVC   LIBNUM(2),@CH00042                                  0039
*   ELSE                                                           0040
*     ;                             /*                       @Y30LB12*/
@RF00038 DS    0H                                                  0041
*   IF SCANDATA(4)='1'              /* CMD WORD 'LIB1'       @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         L     @10,STARTCOL(,R4)                                   0041
         CLI   SCANDATA+3(@10),C'1'                                0041
         BNE   @RF00041                                            0041
*     LIBNUM=2;                     /* PTR TO 2ND ENTRY      @Y30LB12*/
         MVC   LIBNUM(2),@CH00050                                  0042
*   ELSE                                                           0043
*     ;                             /*                       @Y30LB12*/
*                                                                  0043
@RF00041 DS    0H                                                  0044
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @Y30LB12*/
*   /* IF SECOND CHARACTER FOLLOWING 'MSF' IS NOT            @Y30LB12*/
*   /* BLANK, OR IF FIRST CHARACTER FOLLOWING 'MSF'          @Y30LB12*/
*   /* IS NOT 0 OR 1, ERROR IN COMMAND WORD.                 @Y30LB12*/
*   /* ***********************************************       @Y30LB12*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0044
*   IF SCANDATA(5)^='40'X×LIBNUM=0  /*                       @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         L     @10,STARTCOL(,R4)                                   0044
         CLI   SCANDATA+4(@10),X'40'                               0044
         BNE   @RT00044                                            0044
         LH    @10,LIBNUM                                          0044
         LTR   @10,@10                                             0044
         BNZ   @RF00044                                            0044
@RT00044 DS    0H                                                  0045
*     DO;                           /*                       @Y30LB12*/
*                                                                  0045
*       /*************************************************************/
*       /*                                                           */
*       /* *****************************************         @Y30LB12*/
*       /* 'ICG204I INVALID COMMAND WORD'                    @Y30LB12*/
*       /* *****************************************         @Y30LB12*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0046
*       MSGNO=ICG204I;              /* ID FOR ICGDS204       @Y30LB12*/
         MVC   MSGNO(2,R4),ICG204I                                 0046
*       CALL PUTIT2;                /* TO MESSAGE WRITER     @Y30LB12*/
         BAL   @14,PUTIT2                                          0047
*     END;                          /*                       @Y30LB12*/
*   ELSE                                                           0049
*     ;                             /*                       @Y30LB12*/
@RF00044 DS    0H                                                  0050
*   END S06INIT;                    /* RET. TO MAINLINE      @Y30LB12*/
@EL00002 DS    0H                                                  0050
@EF00002 DS    0H                                                  0050
@ER00002 LM    @14,@12,@SA00002                                    0050
         BR    @14                                                 0050
*                                                                  0051
*/********************************************************** @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - S06SCAN                                 @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - EXTRACT THE KEYWORD FROM THE CONTROL          @Y30LB12*/
*/*            STATEMENT AND CALL THE PROPER PROCEDURE       @Y30LB12*/
*/*            TO HANDLE THE PARAMETER                       @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @Y30LB12*/
*                                                                  0051
*S06SCAN:                                                          0051
*   PROCEDURE;                      /*                       @Y30LB12*/
S06SCAN  STM   @14,@12,@SA00003                                    0051
*   DO WHILE(LASTPARM^='1'B);       /* TILL NO MORE DATA     @Y30LB12*/
         B     @DE00052                                            0052
@DL00052 DS    0H                                                  0053
*     CALL READS99;                 /* EXTRACT KEYWORD       @Y30LB12*/
*                                                                  0053
         BAL   @14,READS99                                         0053
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @Y30LB12*/
*     /* LASTPARM IS SET BY ICGDSS99 WHEN NO MORE            @Y30LB12*/
*     /* KEYWORDS OR PARAMETERS IN CONTROL STATEMENT         @Y30LB12*/
*     /* AND CONTROL STATEMENT NOT CONTINUED ONTO            @Y30LB12*/
*     /* ANOTHER RECORD.                                     @Y30LB12*/
*     /* ***********************************************     @Y30LB12*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0054
*     IF LASTPARM='1'B              /* IF NO DATA LEFT       @Y30LB12*/
*       THEN                        /* TO PROCESS, THEN      @Y30LB12*/
         TM    LASTPARM(R4),B'00001000'                            0054
         BO    @RT00054                                            0054
*       RETURN;                     /* RET. TO MAINLINE      @Y30LB12*/
*     ELSE                                                         0056
*       ;                           /* WAS SOMETHING         @Y30LB12*/
*     KNOWN='0'B;                   /* REC.KEYWORD FLAG      @Y30LB12*/
*                                                                  0057
         NI    KNOWN,B'01111111'                                   0057
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @Y30LB12*/
*     /* DATA JUST EXTRACTED MUST BE KEYWORD (A FIELD        @Y30LB12*/
*     /* TERMINATED BY AN EQUAL SIGN). OTHERWISE,            @YD00113*/
*     /* ISSUE ERROR MESSAGE                                 @YD00113*/
*     /* ***********************************************     @Y30LB12*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0058
*     IF KEYBIT^='1'B               /* SET BY ICGDSS99       @Y30LB12*/
*       THEN                        /*                       @Y30LB12*/
         TM    KEYBIT(R4),B'01000000'                              0058
         BO    @RF00058                                            0058
*       DO;                         /*                       @Y30LB12*/
*                                                                  0059
*         /***********************************************************/
*         /*                                                         */
*         /* *****************************************       @Y30LB12*/
*         /* ICG209I EXPECTED, BUT DID NOT FIND,             @Y30LB12*/
*         /* KEYWORD                                         @Y30LB12*/
*         /* *****************************************       @Y30LB12*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0060
*         MSGNO=ICG209I;            /* ID FOR ICGDS209       @Y30LB12*/
         MVC   MSGNO(2,R4),ICG209I                                 0060
*         CALL PUTIT2;              /* TO MESSAGE WRITER     @Y30LB12*/
         BAL   @14,PUTIT2                                          0061
*         RETURN;                   /* RET TO MAINLINE       @Y30LB12*/
@EL00003 DS    0H                                                  0062
@EF00003 DS    0H                                                  0062
@ER00003 LM    @14,@12,@SA00003                                    0062
         BR    @14                                                 0062
*       END;                        /*                       @Y30LB12*/
*     ELSE                                                         0064
*       ;                           /*                       @Y30LB12*/
*                                                                  0064
@RF00058 DS    0H                                                  0065
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @Y30LB12*/
*     /* IF KEYWORD SCANNED WAS 'SIZE', 'MSFID', OR          @YD00113*/
*     /* 'LOADMAP', CALL APPROPRIATE SUB-PROCEDURE           @YD00113*/
*     /* TO GET ASSOCIATED PARAMETER, AND SET 'KNOWN'        @YD00113*/
*     /* FLAG ON. IF KEYWORD IS NOT RECOGNIZED, CALL         @YD00113*/
*     /* 'UNKNOWN' PROCEDURE TO ISSUE ERROR MESSAGE          @YD00113*/
*     /* ***********************************************     @Y30LB12*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0065
*     IF SCANDATA(1:5)='SIZE='      /*                       @Y30LB12*/
*       THEN                        /*                       @Y30LB12*/
         L     @10,STARTCOL(,R4)                                   0065
         CLC   SCANDATA(5,@10),@CC00388                            0065
         BNE   @RF00065                                            0065
*       DO;                         /*                       @Y30LB12*/
*         KNOWN='1'B;               /* RECOGNIZED KEYWD      @Y30LB12*/
         OI    KNOWN,B'10000000'                                   0067
*         CALL S06SIZE;             /* TO GET PARAMETER      @Y30LB12*/
         BAL   @14,S06SIZE                                         0068
*       END;                        /*                       @Y30LB12*/
*     ELSE                                                         0070
*       ;                           /*                       @Y30LB12*/
*                                                                  0070
@RF00065 DS    0H                                                  0071
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @Y30LB12*/
*     /* IF KEYWORD SCANNED WAS 'MSFID'                      @Y30LB12*/
*     /* ***********************************************     @Y30LB12*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0071
*     IF SCANDATA(1:6)='MSFID='     /*                       @Y30LB12*/
*       THEN                        /*                       @Y30LB12*/
         L     @10,STARTCOL(,R4)                                   0071
         CLC   SCANDATA(6,@10),@CC00390                            0071
         BNE   @RF00071                                            0071
*       DO;                         /*                       @Y30LB12*/
*         KNOWN='1'B;               /* RECOGNIZED KEYWD      @Y30LB12*/
         OI    KNOWN,B'10000000'                                   0073
*         CALL S06DSMID;            /* TO GET PARAMETER      @Y30LB12*/
         BAL   @14,S06DSMID                                        0074
*       END;                        /*                       @Y30LB12*/
*     ELSE                                                         0076
*       ;                           /*                       @Y30LB12*/
*                                                                  0076
@RF00071 DS    0H                                                  0077
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @Y30LB12*/
*     /* IF KEYWORD SCANNED WAS 'LOADMAP'                    @Y30LB12*/
*     /* ***********************************************     @Y30LB12*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0077
*     IF SCANDATA(1:8)='LOADMAP='   /*                       @Y30LB12*/
*       THEN                        /*                       @Y30LB12*/
         L     @10,STARTCOL(,R4)                                   0077
         CLC   SCANDATA(8,@10),@CC00392                            0077
         BNE   @RF00077                                            0077
*       DO;                         /*                       @Y30LB12*/
*         KNOWN='1'B;               /* RECOGNIZED KEYWD      @Y30LB12*/
         OI    KNOWN,B'10000000'                                   0079
*         CALL S06MAP;              /* TO GET PARAMETER      @Y30LB12*/
         BAL   @14,S06MAP                                          0080
*       END;                        /*                       @Y30LB12*/
*     ELSE                                                         0082
*       ;                           /*                       @Y30LB12*/
*                                                                  0082
@RF00077 DS    0H                                                  0083
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @Y30LB12*/
*     /* IF THE KEYWORD SCANNED WAS NOT RECOGNIZED AS        @Y30LB12*/
*     /* ONE OF THE ABOVE                                    @Y30LB12*/
*     /* ***********************************************     @Y30LB12*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0083
*     IF KNOWN='0'B                 /* REC. KEY FLAG OFF     @Y30LB12*/
*       THEN                        /*                       @Y30LB12*/
         TM    KNOWN,B'10000000'                                   0083
         BNZ   @RF00083                                            0083
*       CALL UNKNOWN;               /* TO POSITION           @Y30LB12*/
         BAL   @14,UNKNOWN                                         0084
*     ELSE                                                         0085
*       ;                           /*                       @Y30LB12*/
@RF00083 DS    0H                                                  0086
*   END;                            /* END OF DO WHILE       @Y30LB12*/
@DE00052 TM    LASTPARM(R4),B'00001000'                            0086
         BNO   @DL00052                                            0086
*   END S06SCAN;                    /* RET. TO MAINLINE      @Y30LB12*/
         B     @EL00003                                            0087
*                                                                  0088
*/********************************************************** @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - S06SIZE                                 @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - CHECK FOR DUPLICATE KEYWORD 'SIZE' , WRONG    @Y30LB12*/
*/*            LENGTH PARAMETER, AND/OR MISSING PARAMETER.   @YD00113*/
*/*            IF THERE WAS NO ERROR, SAVE THE PARAMETER.    @YD00113*/
*/*            IF AN ERROR OCCURRED, ISSUE THE APPROPRIATE   @YD00113*/
*/*            ERROR MESSAGE.                                @YD00113*/
*/*                                                          @Y30LB12*/
*/********************************************************** @Y30LB12*/
*                                                                  0088
*S06SIZE:                                                          0088
*   PROCEDURE;                      /*                       @Y30LB12*/
S06SIZE  STM   @14,@12,@SA00004                                    0088
*   CALL READS99;                   /* EXTRACT PARAMETER     @Y30LB12*/
*                                                                  0089
         BAL   @14,READS99                                         0089
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @Y30LB12*/
*   /* TEST FOR DUPLICATE KEYWORD                            @YD00113*/
*   /* ***********************************************       @Y30LB12*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0090
*   IF HAVESIZE^='1'B               /* IF NOT DUPLICATE      @Y30LB12*/
*     THEN                          /* KEYWORD, SET FLAG     @Y30LB12*/
         TM    HAVESIZE,B'10000000'                                0090
         BO    @RF00090                                            0090
*     HAVESIZE='1'B;                /* FOR KEYWORD USED      @Y30LB12*/
         OI    HAVESIZE,B'10000000'                                0091
*   ELSE                            /* ELSE WRITE MSG        @Y30LB12*/
*     DO;                           /*                       @Y30LB12*/
*                                                                  0092
         B     @RC00090                                            0092
@RF00090 DS    0H                                                  0093
*       /*************************************************************/
*       /*                                                           */
*       /* *****************************************         @Y30LB12*/
*       /* 'ICG219I DUPLICATE KEYWORD SIZE'                  @Y30LB12*/
*       /* *****************************************         @Y30LB12*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0093
*       OUTBUF2(1:27)=ICGDS219;     /*                       @Y30LB12*/
         MVC   OUTBUF2(27,R4),ICGDS219                             0093
*       OUTBUF2(28:31)='SIZE';      /*                       @Y30LB12*/
         MVC   OUTBUF2+27(4,R4),@CC00399                           0094
*       CALL PUTIT1;                /* TO MESSAGE WRITER     @Y30LB12*/
         BAL   @14,PUTIT1                                          0095
*       RETURN;                     /* RET. TO S06SCAN       @Y30LB12*/
@EL00004 DS    0H                                                  0096
@EF00004 DS    0H                                                  0096
@ER00004 LM    @14,@12,@SA00004                                    0096
         BR    @14                                                 0096
*     END;                          /*                       @Y30LB12*/
*                                                                  0097
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @Y30LB12*/
*   /* TEST FOR MISSING PARAMETER                            @YD00113*/
*   /* ***********************************************       @Y30LB12*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0098
*   IF PARMBIT^='1'B                /* SET IN ICGDSS99       @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
@RC00090 TM    PARMBIT(R4),B'00100000'                             0098
         BO    @RF00098                                            0098
*     DO;                           /*                       @Y30LB12*/
*                                                                  0099
*       /*************************************************************/
*       /*                                                           */
*       /* *****************************************         @Y30LB12*/
*       /* ICG211I EXPECTED, BUT DID NOT FIND,               @Y30LB12*/
*       /* PARAMETER                                         @Y30LB12*/
*       /* *****************************************         @Y30LB12*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0100
*       MSGNO=ICG211I;              /* ID FOR ICGDS211       @Y30LB12*/
         MVC   MSGNO(2,R4),ICG211I                                 0100
*       CALL PUTIT2;                /* TO MESSAGE WRITER     @Y30LB12*/
         BAL   @14,PUTIT2                                          0101
*       RETURN;                     /* RET. TO S06SCAN       @Y30LB12*/
         B     @EL00004                                            0102
*     END;                          /*                       @Y30LB12*/
*   ELSE                                                           0104
*     ;                             /*                       @Y30LB12*/
*                                                                  0104
@RF00098 DS    0H                                                  0105
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @Y30LB12*/
*   /* TEST FOR INCORRECT PARAMETER LENGTH (1 BYTE)          @YD00113*/
*   /* ***********************************************       @Y30LB12*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0105
*   IF SCANLEN^=1                   /* SET BY ICGDSS99       @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         CLC   SCANLEN(2,R4),@CH00042                              0105
         BE    @RF00105                                            0105
*     DO;                           /*                       @Y30LB12*/
*                                                                  0106
*       /*************************************************************/
*       /*                                                           */
*       /* *****************************************         @Y30LB12*/
*       /* 'ICG220I INVALID PARAMETER LENGTH,                @Y30LB12*/
*       /* KEYWORD IS SIZE'                                  @Y30LB12*/
*       /* *****************************************         @Y30LB12*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0107
*       OUTBUF2(1:46)=ICGDS220;     /*                       @Y30LB12*/
         MVC   OUTBUF2(46,R4),ICGDS220                             0107
*       OUTBUF2(47:50)='SIZE';      /*                       @Y30LB12*/
         MVC   OUTBUF2+46(4,R4),@CC00399                           0108
*       CALL PUTIT1;                /* TO MESSAGE WRITER     @Y30LB12*/
         BAL   @14,PUTIT1                                          0109
*     END;                          /*                       @Y30LB12*/
*   ELSE                            /*                       @Y30LB12*/
*     WORKSIZE=SCANDATA(1);         /* SAVE SIZE             @Y30LB12*/
         B     @RC00105                                            0111
@RF00105 L     @10,STARTCOL(,R4)                                   0111
         MVC   WORKSIZE(1),SCANDATA(@10)                           0111
*   END S06SIZE;                    /* RET. TO S06SCAN       @Y30LB12*/
         B     @EL00004                                            0112
*                                                                  0113
*/********************************************************** @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - S06DSMID                                @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - EXTRACT THE PARAMETER.  CHECK FOR DUPLICATE   @Y30LB12*/
*/*            KEYWORD, WRONG LENGTH PARAMETER, PARAMETER    @Y30LB12*/
*/*            NOT HEXADECIMAL, AND PARAMETER MISSING.       @YD00113*/
*/*            IF NO ERROR OCCURRED, CONVERT THE VALUE TO    @YD00113*/
*/*            BINARY AND SAVE.  IF AN ERROR OCCURRED, ISSUE @YD00113*/
*/*            THE APPROPRIATE ERROR MESSAGE.                @YD00113*/
*/*                                                          @Y30LB12*/
*/********************************************************** @Y30LB12*/
*                                                                  0113
*S06DSMID:                                                         0113
*   PROCEDURE;                      /*                       @Y30LB12*/
S06DSMID STM   @14,@12,@SA00005                                    0113
*   CALL READS99;                   /* EXTRACT PARAMETER     @Y30LB12*/
*                                                                  0114
         BAL   @14,READS99                                         0114
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @Y30LB12*/
*   /* TEST FOR DUPLICATE KEYWORD                            @YD00113*/
*   /* ***********************************************       @Y30LB12*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0115
*   IF HAVEDSM^='1'B                /* IF KEYWORD NOT        @Y30LB12*/
*     THEN                          /* ALREADY SPECIFIED     @Y30LB12*/
         TM    HAVEDSM,B'01000000'                                 0115
         BO    @RF00115                                            0115
*     HAVEDSM='1'B;                 /* SET KEYWORD FLAG      @Y30LB12*/
         OI    HAVEDSM,B'01000000'                                 0116
*   ELSE                            /*                       @Y30LB12*/
*     DO;                           /*                       @Y30LB12*/
*                                                                  0117
         B     @RC00115                                            0117
@RF00115 DS    0H                                                  0118
*       /*************************************************************/
*       /*                                                           */
*       /* *****************************************         @Y30LB12*/
*       /* 'ICG219I DUPLICATE KEYWORD MSFID'                 @Y30LB12*/
*       /* *****************************************         @Y30LB12*/
*       /*                                                           */
