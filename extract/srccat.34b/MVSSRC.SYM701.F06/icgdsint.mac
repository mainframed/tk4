*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - DOPAGE                                  @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - HANDLE THE STARTING PAGE NUMBER FIELD, IF ONE @Y30LB12*/
*/*            IS SUPPLIED. THE PAGE NUMBER MUST BE A        @Y30LB12*/
*/*            4-CHARACTER ZONED DECIMAL NUMBER.             @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @YD00113*/
*                                                                  0135
*DOPAGE:                                                           0135
*   PROCEDURE;                      /*                       @Y30LB12*/
DOPAGE   STM   @14,@12,12(@13)                                     0135
*   IF PAGELEN^=4                   /* IF LENGTH NOT 4       @Y30LB12*/
*     THEN                          /* ERROR, WRITE MSG      @Y30LB12*/
         L     @10,PARMPTR(,R4)                                    0136
         L     @10,PAGEPTR(,@10)                                   0136
         CLC   PAGELEN(2,@10),@CH00070                             0136
         BE    @RF00136                                            0136
*     NEED102='1'B;                 /* ICGDS102, DEFAULT     @Y30LB12*/
         OI    NEED102(R4),B'01000000'                             0137
*   ELSE                            /* CHK ZONED DECIMAL     @Y30LB12*/
*     DO;                           /*                       @Y30LB12*/
         B     @RC00136                                            0138
@RF00136 DS    0H                                                  0139
*       IF STARTNUM(1:4)='1111'B    /*                       @Y30LB12*/
*           &STARTNUM(9:12)='1111'B /*                       @Y30LB12*/
*           &STARTNUM(17:20)='1111'B/*                       @Y30LB12*/
*           &STARTNUM(25:26)='11'B  /* PLUS ZONE             @Y30LB12*/
*         THEN                      /*                       @Y30LB12*/
         L     @10,PARMPTR(,R4)                                    0139
         L     @10,PAGEPTR(,@10)                                   0139
         TM    STARTNUM(@10),B'11110000'                           0139
         BNO   @RF00139                                            0139
         TM    STARTNUM+1(@10),B'11110000'                         0139
         BNO   @RF00139                                            0139
         TM    STARTNUM+2(@10),B'11110000'                         0139
         BNO   @RF00139                                            0139
         TM    STARTNUM+3(@10),B'11000000'                         0139
         BNO   @RF00139                                            0139
*         PAGENO=STARTNUM;          /* ACCEPT PASSED NUM     @Y30LB12*/
         MVC   PAGENO(4,R4),STARTNUM(@10)                          0140
*       ELSE                        /*                       @Y30LB12*/
*         NEED102='1'B;             /* FOR MSG ICGDS102      @Y30LB12*/
         B     @RC00139                                            0141
