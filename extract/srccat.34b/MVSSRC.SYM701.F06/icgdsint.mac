*/********************************************************** @YD00113*/
*/*                                                          @Y30LB12*/
*/*             ICGDSINT - GETCLEAR                          @Y30LB12*/
*/*                                                          @Y30LB12*/
*/*             THIS ROUTINE ISSUES AN ASSEMBLER GETMAIN FOR @Y30LB12*/
*/*             40,000 BYTES OF MAIN STORAGE.  THIS STORAGE  @Y30LB12*/
*/*             IS DESCRIBED BY THE MAPPING STRUCTURES       @Y30LB12*/
*/*             COMTAB AND COMTAB2.  THE AREA IS ADDRESSED   @Y30LB12*/
*/*             USING REGISTER 4.                            @Y30LB12*/
*/*             COMTAB2 IS BASED OFF THE POINTER COMPTR2 IN  @Y30LB12*/
*/*             COMTAB.                                      @Y30LB12*/
*/*             BEFORE THE GETMAIN IS ISSUED, THE INVOKER'S  @Y30LB12*/
*/*             REGISTER 1 IS STORED TEMPORARILY IN REG 8.   @Y30LB12*/
*/*             THE ENTIRE AREA IS CLEARED TO BINARY ZEROS.  @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @YD00113*/
*                                                                  0043
*   R8=R1;                          /* SAVE INVOKER'S R1     @Y30LB12*/
         LR    R8,R1                                               0043
*   GEN(GETMAIN R,LV=40000);        /* GET COMTAB AREA       @Y30LB12*/
         GETMAIN R,LV=40000
*   R4=R1;                          /* BASE COMTAB ON R4     @Y30LB12*/
*                                                                  0045
         LR    R4,R1                                               0045
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* THE FOLLOWING ROUTINE CLEARS THE ENTIRE GOTTEN        @Y30LB12*/
*   /* AREA TO BINARY ZEROS. THIS STORAGE IS                 @Y30LB12*/
*   /* DESCRIBED BY THE STRUCTURES COMTAB AND COMTAB2        @Y30LB12*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0046
*CLEAR:                                                            0046
*   COMTAB(1)='00'X;                /* SET UP CLEAR CHAR     @Y30LB12*/
CLEAR    MVI   COMTAB(R4),X'00'                                    0046
*   R5=ADDR(COMTAB);                /* 1ST BYTE TO CLEAR     @Y30LB12*/
*                                                                  0047
         LR    R5,R4                                               0047
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* CLEAR CORE TO '00'X.                                  @XD00153*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0048
*   R6=MAXCORE;                     /* LENGTH OF CLEAR       @XD00153*/
         L     R6,MAXCORE                                          0048
*   R6=R6-1;                        /* REDUCE LNGTH BY 1     @Y30LB12*/
         BCTR  R6,0                                                0049
*STARTCLR:                                                         0050
*   IF R6=0                         /* NOTHING TO CLEAR      @Y30LB12*/
*     THEN                          /* IF NOTHING, THEN      @Y30LB12*/
STARTCLR LTR   R6,R6                                               0050
         BZ    @RT00050                                            0050
*     GO TO ENDCLEAR;               /* STOP CLEARING         @Y30LB12*/
*   ELSE                                                           0052
*     ;                             /* SOMETHING LEFT        @Y30LB12*/
*   IF R6>256                       /* IF WHAT IS LEFT       @Y30LB12*/
*     THEN                          /* IS MORE THAN 256      @Y30LB12*/
         CH    R6,@CH00037                                         0053
         BNH   @RF00053                                            0053
*     DO;                           /* CLEAR 256 BYTES       @Y30LB12*/
*EXANDMVC:                                                         0055
*       GEN(MVC   1(256,5),0(5));   /* ALSO EXECUTED         @Y30LB12*/
EXANDMVC MVC   1(256,5),0(5)
*       R5=R5+256;                  /* NEXT BYTE TO CLR      @Y30LB12*/
         LA    @10,256                                             0056
         ALR   R5,@10                                              0056
*       R6=R6-256;                  /* BYTES LEFT NOW        @Y30LB12*/
         SLR   R6,@10                                              0057
*       GO TO STARTCLR;             /* LOOP BACK             @Y30LB12*/
         B     STARTCLR                                            0058
*     END;                          /*                       @Y30LB12*/
*   ELSE                            /* LESS THAN 256         @Y30LB12*/
*     DO;                           /* CLEAR THEM            @Y30LB12*/
@RF00053 DS    0H                                                  0061
*       R6=R6-1;                    /* DECREASE LENGTH       @Y30LB12*/
         BCTR  R6,0                                                0061
*       GEN(EX    R6,EXANDMVC);     /* EXECUTE MVC           @Y30LB12*/
         EX    R6,EXANDMVC
*     END;                          /*                       @Y30LB12*/
*ENDCLEAR:                                                         0064
*   ;                               /* DONE WITH CLEAR       @Y30LB12*/
ENDCLEAR DS    0H                                                  0065
*   COMPTR2=ADDR(COM2BASE);         /* PTR TO COMTAB2        @Y30LB12*/
         LA    @10,COM2BASE(,R4)                                   0065
         ST    @10,COMPTR2(,R4)                                    0065
*   PARMPTR=R8;                     /* INVOKER'S REG 1       @Y30LB12*/
         ST    R8,PARMPTR(,R4)                                     0066
*   CALL INVOKE;                    /* INVOCATION PARMS      @Y30LB12*/
         BAL   @14,INVOKE                                          0067
*   CALL OPENPRT;                   /* OPEN SYSPRINT         @Y30LB12*/
         BAL   @14,OPENPRT                                         0068
*   CALL SCNANBLD;                  /* CALL MAJOR PHASES     @Y30LB12*/
         BAL   @14,SCNANBLD                                        0069
*   CALL CLOSEPRT;                  /* CLOSE SYSPRINT        @Y30LB12*/
         BAL   @14,CLOSEPRT                                        0070
*   R8=RETCODE;                     /* FINAL RETURN CODE     @YM31087*/
         LH    R8,RETCODE(,R4)                                     0071
*   R1=ADDR(COMTAB);                /* PTR TO GOTTEN AREA    @Y30LB12*/
         LR    R1,R4                                               0072
*   GEN(FREEMAIN R,LV=40000,A=(1)); /* FREE IT               @Y30LB12*/
         FREEMAIN R,LV=40000,A=(1)
*   R15=R8;                         /* PASS RETURN CODE      @YM31087*/
         LR    R15,R8                                              0074
*   RETURN CODE(R15);               /* RETURN TO SYSTEM      @Y30LB12*/
         LR    @10,@15                                             0075
         L     @13,4(,@13)                                         0075
         L     @00,@SIZDATD                                        0075
         LR    @01,@11                                             0075
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@10                                             0075
         L     @14,12(,@13)                                        0075
         LM    @00,@12,20(@13)                                     0075
         BR    @14                                                 0075
*                                                                  0076
*/********************************************************** @YD00113*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - INVOKE                                  @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - CHECK FOR INVOCATION PARAMETERS PASSED EITHER @Y30LB12*/
*/*            BY THE SYSTEM OR BY A USER CALLING PROGRAM.   @Y30LB12*/
*/*            THE DEFAULT VALUES FOR POSSIBLE PASSED        @Y30LB12*/
*/*            PARAMETERS ARE FILLED IN BEFORE ANY CHECK IS  @Y30LB12*/
*/*            MADE TO SEE IF PARAMETERS ACTUALLY WERE       @Y30LB12*/
*/*            PASSED.                                       @Y30LB12*/
*/*                                                          @Y30LB12*/
*/*            THIS ROUTINE ALSO COPIES A MODEL DCB FOR      @Y30LB12*/
*/*            SYSPRINT TO THE AREA OBTAINED WITH GETMAIN    @Y30LB12*/
*/*            AND SETS UP TO ACTIVATE THE DCB EXLST FOR     @Y30LB12*/
*/*            THE COPIED DCB.                               @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @YD00113*/
*                                                                  0076
*INVOKE:                                                           0076
*   PROCEDURE;                      /*                       @Y30LB12*/
INVOKE   STM   @14,@12,@SA00002                                    0076
*   PAGENO='F0F0F0F1'X;             /* STARTING PAGE NO.     @Y30LB12*/
         MVC   PAGENO(4,R4),@CB00655                               0077
*   LINECT=58;                      /* 58 LINES PER PAGE     @Y30LB12*/
         MVC   LINECT(2,R4),@CH00656                               0078
*   DCBPRINT=MYDCB1A;               /* COPY SYSPRINT DCB     @Y30LB12*/
         LA    @10,MYDCB1                                          0079
         MVC   DCBPRINT(96,R4),MYDCB1A(@10)                        0079
*   DCBPTR=ADDR(DCBPRINT);          /* SET DCB OVERLAY       @Y30LB12*/
         LA    @10,DCBPRINT(,R4)                                   0080
         ST    @10,DCBPTR(,R4)                                     0080
*   DCBEXLST=ADDR(EXLST);           /* EXIT LIST ADDRESS     @Y30LB12*/
         LA    @09,EXLST(,R4)                                      0081
         ST    @09,DCBEXLST+28(,@10)                               0081
*   EXLST=ADDR(DCBROUT1);           /* EXIT ROUTINE ADDR     @Y30LB12*/
         LA    @10,DCBROUT1                                        0082
         ST    @10,EXLST(,R4)                                      0082
*   EXLST(1)='85'X;                 /* ACTIVATE DCB EXIT     @Y30LB12*/
         MVI   EXLST(R4),X'85'                                     0083
*   IF OPTLNGTH^='0000'X            /* LENGTH OF OPTLIST     @YM32174*/
*     THEN                          /*                       @Y30LB12*/
         L     @10,PARMPTR(,R4)                                    0084
         L     @10,OPTPTR(,@10)                                    0084
         LH    @10,OPTLNGTH(,@10)                                  0084
         LTR   @10,@10                                             0084
         BZ    @RF00084                                            0084
*     CALL DOOPTLST;                /* OPTLIST PRESENT       @Y30LB12*/
         BAL   @14,DOOPTLST                                        0085
*   ELSE                                                           0086
*     ;                             /*                       @Y30LB12*/
@RF00084 DS    0H                                                  0087
*   IF OPTFLG^='1'B                 /* ONLY OPTION LIST      @Y30LB12*/
*     THEN                          /* END-OF-LIST FLAG      @Y30LB12*/
         L     @10,PARMPTR(,R4)                                    0087
         TM    OPTFLG(@10),B'10000000'                             0087
         BO    @RF00087                                            0087
*     DO;                           /*                       @Y30LB12*/
*       CALL DODDNAME;              /* CHECK DDNAME LIST     @Y30LB12*/
         BAL   @14,DODDNAME                                        0089
*       IF DDFLG^='1'B              /* END-OF-LIST FLAG      @Y30LB12*/
*         THEN                      /*                       @Y30LB12*/
         L     @10,PARMPTR(,R4)                                    0090
         TM    DDFLG(@10),B'10000000'                              0090
         BO    @RF00090                                            0090
*         IF PAGELEN^=0             /* ENTRY FOR PAGE        @Y30LB12*/
*           THEN                    /*                       @Y30LB12*/
         L     @10,PAGEPTR(,@10)                                   0091
         LH    @10,PAGELEN(,@10)                                   0091
         LTR   @10,@10                                             0091
         BZ    @RF00091                                            0091
*           CALL DOPAGE;            /* CHECK PAGE LIST       @Y30LB12*/
         BAL   @14,DOPAGE                                          0092
*         ELSE                                                     0093
*           ;                       /*                       @Y30LB12*/
@RF00091 DS    0H                                                  0094
*       ELSE                                                       0094
*         ;                         /*                       @Y30LB12*/
@RF00090 DS    0H                                                  0095
*     END;                          /*                       @Y30LB12*/
*   ELSE                                                           0096
*     ;                             /*                       @Y30LB12*/
@RF00087 DS    0H                                                  0097
*   END INVOKE;                     /* RET. TO MAINLINE      @Y30LB12*/
@EL00002 DS    0H                                                  0097
@EF00002 DS    0H                                                  0097
@ER00002 LM    @14,@12,@SA00002                                    0097
         BR    @14                                                 0097
*                                                                  0098
*/********************************************************** @YD00113*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - DOOPTLST                                @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - CHECK THE OPTIONS FIELD FOR THE PARAMETER     @Y30LB12*/
*/*            'LINE=NN', WHERE NN IS A TWO-DIGIT DECIMAL    @Y30LB12*/
*/*            NUMBER                                        @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @YD00113*/
*                                                                  0098
*DOOPTLST:                                                         0098
*   PROCEDURE;                      /*                       @Y30LB12*/
DOOPTLST STM   @14,@12,@SA00003                                    0098
*   IF OPTCONST='LINES='            /* 1ST 6 CHARACTERS      @Y30LB12*/
*       &OPTLNGTH='0008'X           /* PROPER LENGTH         @YM32174*/
*     THEN                          /* ARE 'LINES=', CHK     @Y30LB12*/
         L     @10,PARMPTR(,R4)                                    0099
         L     @10,OPTPTR(,@10)                                    0099
         CLC   OPTCONST(6,@10),@CC00667                            0099
         BNE   @RF00099                                            0099
         CLC   OPTLNGTH(2,@10),@CH00668                            0099
         BNE   @RF00099                                            0099
*     DO;                           /* LAST 2 CHARS NUM.     @Y30LB12*/
*       IF OPTLINES(1:4)^='1111'B   /* IF 1ST BYTE           @Y30LB12*/
*           ×OPTLINES(9:12)^='1111'B/* OR 2ND BYTE           @Y30LB12*/
*         THEN                      /* NOT ZONED DECIMAL     @Y30LB12*/
         TM    OPTLINES(@10),B'11110000'                           0101
         BNO   @RT00101                                            0101
         TM    OPTLINES+1(@10),B'11110000'                         0101
         BO    @RF00101                                            0101
@RT00101 DS    0H                                                  0102
*         NEED101='1'B;             /* NEED ERR MESSAGE      @Y30LB12*/
         OI    NEED101(R4),B'10000000'                             0102
*       ELSE                        /*                       @Y30LB12*/
*         DO;                       /*                       @Y30LB12*/
         B     @RC00101                                            0103
@RF00101 DS    0H                                                  0104
*           SAVELINE=OPTLINES;      /*                       @YM31087*/
         L     @10,PARMPTR(,R4)                                    0104
         L     @10,OPTPTR(,@10)                                    0104
         MVC   SAVELINE(2),OPTLINES(@10)                           0104
*           PACKWORK='0000000000000000'X;/*                  @Y30LB12*/
         MVC   PACKWORK(8),@CB00670                                0105
*           GEN(PACK  PACKWORK+6(2),SAVELINE(2));                  0106
         PACK  PACKWORK+6(2),SAVELINE(2)
*           GEN(CVB   6,PACKWORK);  /*                       @Y30LB12*/
         CVB   6,PACKWORK
*           GEN(STH   6,LINECT(R4));/*                       @Y30LB12*/
         STH   6,LINECT(R4)
*         END;                      /*                       @Y30LB12*/
*     END;                          /*                       @Y30LB12*/
*   ELSE                            /* FALL THRU TO ERR      @Y30LB12*/
*     NEED101='1'B;                 /* SET UP ERR MSG        @Y30LB12*/
         B     @RC00099                                            0111
@RF00099 OI    NEED101(R4),B'10000000'                             0111
*   END DOOPTLST;                   /* RETURN TO INVOKE      @Y30LB12*/
@EL00003 DS    0H                                                  0112
@EF00003 DS    0H                                                  0112
@ER00003 LM    @14,@12,@SA00003                                    0112
         BR    @14                                                 0112
*                                                                  0113
*/********************************************************** @YD00113*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - DODDNAME                                @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - EXTRACT THE SUBSTITUTE DDNAME LIST FROM THE   @Y30LB12X
*                                                                    */
*/*            INVOKER'S AREA AND MOVE TO TO THE DDNAME SAVE @Y30LB12*/
*/*            AREA.  ONLY THE SYSPRINT SUBSTITUTE DDNAME IS @Y30LB12*/
*/*            CHECKED FOR VALIDITY HERE.  ALL OTHER         @Y30LB12*/
*/*            SUBSTITUTE DDNAMES ARE CHECKED WHEN THE DATA  @Y30LB12*/
*/*            SET IS TO BE OPENED.                          @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @YD00113*/
*                                                                  0113
*DODDNAME:                                                         0113
*   PROCEDURE;                      /*                       @Y30LB12*/
DODDNAME STM   @14,@12,12(@13)                                     0113
*   IF DDLENGTH>=40                 /* LENGTH OKAY FOR       @Y30LB12*/
*     THEN                          /* POSSIBLE              @Y30LB12*/
         L     @10,PARMPTR(,R4)                                    0114
         L     @10,DDPTR(,@10)                                     0114
         LH    @09,DDLENGTH(,@10)                                  0114
         CH    @09,@CH00350                                        0114
         BL    @RF00114                                            0114
*     SVSYSIN=DDSYSIN;              /* SYSIN SUBSTITUTE      @Y30LB12*/
         MVC   SVSYSIN(8,R4),DDSYSIN(@10)                          0115
*   ELSE                                                           0116
*     ;                             /*                       @Y30LB12*/
@RF00114 DS    0H                                                  0117
*   IF DDLENGTH>=48                 /* LENGTH OKAY FOR       @Y30LB12*/
*     THEN                          /* POSSIBLE SYSPRINT     @Y30LB12*/
         L     @10,PARMPTR(,R4)                                    0117
         L     @10,DDPTR(,@10)                                     0117
         LH    @09,DDLENGTH(,@10)                                  0117
         CH    @09,@CH00237                                        0117
         BL    @RF00117                                            0117
*     IF DDSYSPRT^='0000000000000000'X/* AND                 @Y30LB12*/
*       THEN                        /* SUBSTITUTE THERE,     @Y30LB12*/
         CLC   DDSYSPRT(8,@10),@CB00670                            0118
         BE    @RF00118                                            0118
*       DCBPRINT(41:48)=DDSYSPRT;   /* MOVE NAME             @Y30LB12*/
         MVC   DCBPRINT+40(8,R4),DDSYSPRT(@10)                     0119
*     ELSE                                                         0120
*       ;                           /* NO SUBSTITUTE         @Y30LB12*/
@RF00118 DS    0H                                                  0121
*   ELSE                                                           0121
*     ;                             /* LIST TOO SHORT        @Y30LB12*/
@RF00117 DS    0H                                                  0122
*   IF DDLENGTH>=56                 /* LENGTH OKAY FOR       @Y30LB12*/
*     THEN                          /* POSSIBLE SYSPUNCH     @Y30LB12*/
         L     @10,PARMPTR(,R4)                                    0122
         L     @10,DDPTR(,@10)                                     0122
         LH    @09,DDLENGTH(,@10)                                  0122
         CH    @09,@CH00676                                        0122
         BL    @RF00122                                            0122
*     SVSYSPCH=DDSYSPCH;            /* SUBSTITUTE, MOVE      @Y30LB12*/
         MVC   SVSYSPCH(8,R4),DDSYSPCH(@10)                        0123
*   ELSE                                                           0124
*     ;                             /* LIST TOO SHORT        @Y30LB12*/
@RF00122 DS    0H                                                  0125
*   IF DDLENGTH>=64                 /* LENGTH OKAY FOR       @Y30LB12*/
*     THEN                          /* POSSIBLE SYSUT1       @Y30LB12*/
         L     @10,PARMPTR(,R4)                                    0125
         L     @10,DDPTR(,@10)                                     0125
         LH    @09,DDLENGTH(,@10)                                  0125
         CH    @09,@CH00247                                        0125
         BL    @RF00125                                            0125
*     SVSYSUT1=DDSYSUT1;            /* THEN MOVE IT          @Y30LB12*/
         MVC   SVSYSUT1(8,R4),DDSYSUT1(@10)                        0126
*   ELSE                                                           0127
*     ;                             /* LIST YOO SHORT        @Y30LB12*/
@RF00125 DS    0H                                                  0128
*   IF DDLENGTH>=72                 /* LENGTH OKAY FOR       @Y30LB12*/
*     THEN                          /* POSSIBLE SYSUT2       @Y30LB12*/
         L     @10,PARMPTR(,R4)                                    0128
         L     @10,DDPTR(,@10)                                     0128
         LH    @09,DDLENGTH(,@10)                                  0128
         CH    @09,@CH00274                                        0128
         BL    @RF00128                                            0128
*     SVSYSUT2=DDSYSUT2;            /* NAME, MOVE IT         @Y30LB12*/
         MVC   SVSYSUT2(8,R4),DDSYSUT2(@10)                        0129
*   ELSE                                                           0130
*     ;                             /* LIST TOO SHORT        @Y30LB12*/
@RF00128 DS    0H                                                  0131
*   IF DDLENGTH>=80                 /* LENGTH OKAY FOR       @Y30LB12*/
*     THEN                          /* POSSIBLE SYSUT3       @Y30LB12*/
         L     @10,PARMPTR(,R4)                                    0131
         L     @10,DDPTR(,@10)                                     0131
         LH    @09,DDLENGTH(,@10)                                  0131
         CH    @09,@CH00056                                        0131
         BL    @RF00131                                            0131
*     SVSYSUT3=DDSYSUT3;            /* NAME, MOVE IT         @Y30LB12*/
         MVC   SVSYSUT3(8,R4),DDSYSUT3(@10)                        0132
*   ELSE                                                           0133
*     ;                             /* NOT LONG ENOUGH       @Y30LB12*/
@RF00131 DS    0H                                                  0134
*   END DODDNAME;                   /* RETURN TO INVOKE      @Y30LB12*/
@EL00004 DS    0H                                                  0134
@EF00004 DS    0H                                                  0134
@ER00004 LM    @14,@12,12(@13)                                     0134
         BR    @14                                                 0134
