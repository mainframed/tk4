*                                                                  0058
*/********************************************************** @YD00113*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - ICGDSB01 (MAINLINE)                     @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - INITIALIZE AREAS. DETERMINE WHETHER CALL IS   @Y30LB12*/
*/*            TO OPEN OR TO CLOSE THE TABLES DATA SETS AND  @Y30LB12*/
*/*            CALL THE OPEN PROCEDURE OR THE CLOSE          @Y30LB12*/
*/*            PROCEDURE.  RETURN TO ICGDSBLD.               @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @YD00113*/
*                                                                  0058
*   /*****************************************************************/
*   /*                                                               */
*   /* GET IOB POINTER                                       @Y30LB10*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0058
*   DCBPTSAV=DCBPTR;                /*                       @Y30LB10*/
         L     @14,DCBPTR(,R4)                                     0058
         LR    DCBPTSAV,@14                                        0058
*   IOBSTPTR=ADDR(DSIOB);           /*                       @Y30LB10*/
*                                                                  0059
         LA    IOBSTPTR,DSIOB(,@14)                                0059
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* ZERO DCB PARAMETER AREA. USED FOR PASSING DCBS        @Y30LB12*/
*   /* TO ICGDSB03.                                          @Y30LB12*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0060
*   VTADDR1=''B;                    /*                       @Y30LB10*/
         SLR   @14,@14                                             0060
         ST    @14,VTADDR1                                         0060
*   VTADDR2=''B;                    /*                       @Y30LB10*/
         ST    @14,VTADDR2                                         0061
*   VTADDR3=''B;                    /*                       @Y30LB10*/
         ST    @14,VTADDR3                                         0062
*   UT1VOERR='0'B;                  /*                       @Y30LB12*/
*   UT1VWERR='0'B;                  /*                       @Y30LB12*/
         L     @14,BUILDPTR(,R4)                                   0064
         NI    UT1VOERR(@14),B'10011111'                           0064
*   UT2VOERR='0'B;                  /*                       @Y30LB12*/
*   UT2VWERR='0'B;                  /*                       @Y30LB12*/
         NI    UT2VOERR(@14),B'10011111'                           0066
*   UT3VOERR='0'B;                  /*                       @Y30LB12*/
*   UT3VWERR='0'B;                  /*                       @Y30LB12*/
*                                                                  0068
         NI    UT3VOERR(@14),B'10011111'                           0068
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* TEST TO SEE IF CALL IS FOR OPEN OR CLOSE.             @Y30LB10*/
*   /* IF FOR CLOSE, GO TO CLOSE CODE.                       @Y30LB10*/
*   /* DCBFLAG = 00/CLOSE, 80/OPEN INPUT,                    @Y30LB10*/
*   /* 8F/OPEN OUTPUT                                        @Y30LB12*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0069
*   IF DCBFLAG=CHARZERO             /*                       @Y30LB10*/
*     THEN                          /*                       @Y30LB10*/
         CLC   DCBFLAG(1,R4),CHARZERO                              0069
         BNE   @RF00069                                            0069
*     CALL CLOSE;                   /*                       @Y30LB12*/
         BAL   @14,CLOSE                                           0070
*   ELSE                            /*                       @Y30LB12*/
*     CALL OPEN;                    /*                       @Y30LB12*/
         B     @RC00069                                            0071
@RF00069 BAL   @14,OPEN                                            0071
*   DCBPTR=DCBPTSAV;                /*                       @Y30LB10*/
@RC00069 ST    DCBPTSAV,DCBPTR(,R4)                                0072
*   RETURN;                         /*                       @Y30LB10*/
@EL00001 L     @13,4(,@13)                                         0073
@EF00001 L     @00,@SIZDATD                                        0073
         LR    @01,@11                                             0073
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0073
         BR    @14                                                 0073
*                                                                  0074
*/********************************************************** @YD00113*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - OPEN                                    @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - OPEN THE PARTICULAR DATA SET WHOSE ADDRESS    @YD00113*/
*/*            IS PASSED IN DCBPTR.  OPENING THE DATA SET    @YD00113*/
*/*            CONSISTS OF OPENING THE VTOC ON THE WANTED    @YD00113*/
*/*            PACK FOR WRITE AND WRITING A FORMAT1 DSCB     @YD00113*/
*/*            FOR TABLE CREATE'S USAGE, AND THEN OPENING    @YD00113*/
*/*            THE DATA SET ITSELF USING THE FORMAT1 DSCB    @YD00113*/
*/*            JUST BUILT.                                   @YD00113*/
*/*                                                          @YD00113*/
*/*            IF THE DATA SET BEING OPENED IS THE CURRENT   @YD00113*/
*/*            TABLES PACK (SYSUT1), THE VERIFICATION TABLE  @YD00113*/
*/*            IS READ INTO UT1BUFER.                        @YD00113*/
*/*                                                          @YD00113*/
*/********************************************************** @YD00113*/
*                                                                  0074
*OPEN:                                                             0074
*   PROCEDURE;                      /*                       @Y30LB12*/
*                                                                  0074
OPEN     STM   @14,@12,@SA00002                                    0074
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* SET PARAMETER LIST TO PASS TO ICGDSB03.               @Y30LB10*/
*   /* SET IN A (1) INDICATOR FOR WRITE FORMAT 1 DSCB        @Y30LB10*/
*   /* IN THE VTOC. SET IN THE DCB ADDRESS.                  @Y30LB10*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0075
*   VTTYPE=VTTYPEOP;                /*                       @Y30LB10*/
         MVI   VTTYPE,X'01'                                        0075
*   VTADDR1=DCBPTR;                 /*                       @Y30LB10*/
         MVC   VTADDR1(4),DCBPTR(R4)                               0076
*   PARMPTZ=ADDR(VTPARM);           /*                       @Y30LB10*/
*                                                                  0077
         LA    @14,VTPARM                                          0077
         ST    @14,PARMPTZ                                         0077
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* LINK TO ICGDSB03 TO UPDATE THE VTOC.                  @Y30LB10*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0078
*   R1=ADDR(PARMPTZ);               /*                       @Y30LB10*/
         LA    R1,PARMPTZ                                          0078
*   GEN(LINK EP=ICGDSB03);          /*                       @Y30LB10*/
*                                                                  0079
         LINK EP=ICGDSB03
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* CHECK IF THE WRITE WAS OK FROM THE VTOC UPDATE        @Y30LB10*/
*   /* MODULE                                                @Y30LB10*/
*   /*                                                       @Y30LB10*/
*   /* IF RETURN CODE IN REG 15 IS 0, IT IS OK.              @Y30LB10*/
*   /*                                                       @Y30LB10*/
*   /* IF RETURN CODE IS 8, CALL 'IOERR' TO PRINT            @Y30LB10*/
*   /* A MESSAGE, SET 'RETCODE' TO 16 AND SET                @Y30LB10*/
*   /* 'UT-VWERR' TO A 1 AND RETURN TO THE CALLER.           @Y30LB10*/
*   /*                                                       @Y30LB10*/
*   /* IF RETURN CODE IS 4, CALL 'OPENERR' TO PRINT          @Y30LB10*/
*   /* A MESSAGE, SET 'RETCODE' TO 16, SET 'UT-VOERR'        @Y30LB10*/
*   /* TO A 1 AND RETURN TO THE CALLER.                      @Y30LB10*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0080
*   IF RCREG^=WORDZERO              /*                       @Y30LB10*/
*     THEN                          /*                       @Y30LB10*/
         LTR   RCREG,RCREG                                         0080
         BZ    @RF00080                                            0080
*     IF RCREG=WORDEIGH             /*                       @Y30LB10*/
*       THEN                        /*                       @Y30LB10*/
         CH    RCREG,@CH00131                                      0081
         BNE   @RF00081                                            0081
*IOER1:                                                            0082
*       DO;                         /*                       @Y30LB10*/
IOER1    DS    0H                                                  0083
*         CALL IOERR;               /*                       @Y30LB10*/
         BAL   @14,IOERR                                           0083
*         RETURN;                   /* RET. TO MAINLINE      @Y30LB12*/
@EL00002 DS    0H                                                  0084
@EF00002 DS    0H                                                  0084
@ER00002 LM    @14,@12,@SA00002                                    0084
         BR    @14                                                 0084
*       END IOER1;                  /*                       @Y30LB10*/
*     ELSE                          /*                       @Y30LB10*/
*OPER1:                                                            0086
*       DO;                         /*                       @Y30LB10*/
@RF00081 DS    0H                                                  0086
OPER1    DS    0H                                                  0087
*         CALL OPENERR;             /*                       @Y30LB10*/
         BAL   @14,OPENERR                                         0087
*         RETURN;                   /* RET. TO MAINLINE      @Y30LB12*/
         B     @EL00002                                            0088
*       END OPER1;                  /*                       @Y30LB10*/
*   ELSE                                                           0090
*     ;                             /*                       @Y30LB10*/
@RF00080 DS    0H                                                  0091
*                                                                  0091
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* SET UP A OPEN (J) PARAMETER LIST AND                  @Y30LB10*/
*   /* DO A OPEN J ON THIS DEVICE.                           @Y30LB10*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0091
*   DCBEXLSA=ADDR(XLIST);           /*                       @Y30LB10*/
         L     @14,DCBPTR(,R4)                                     0091
         LA    @10,XLIST                                           0091
         ST    @10,@TF00001                                        0091
         MVC   DCBEXLSA+28(3,@14),@TF00001+1                       0091
*   XLISTOP=XLISTOPJ;               /*                       @Y30LB10*/
         MVC   XLISTOP(1),XLISTOPJ                                 0092
*   XLISTAD=ADDR(DSJFCB);           /*                       @Y30LB10*/
         LA    @10,DSJFCB(,@14)                                    0093
         ST    @10,@TF00001                                        0093
         MVC   XLISTAD(3),@TF00001+1                               0093
*   PARMLIST=DCBPTR;                /*                       @Y30LB10*/
         ST    @14,PARMLIST                                        0094
*   PARMEND=ON;                     /*                       @Y30LB10*/
         OI    PARMEND,B'10000000'                                 0095
*   R1=ADDR(PARMLIST);              /*                       @Y30LB10*/
         LA    R1,PARMLIST                                         0096
*   GEN(OPEN TYPE=J,MF=(E,(R1)));   /*                       @Y30LB10*/
*                                                                  0097
         OPEN TYPE=J,MF=(E,(R1))
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* IF THE DCB DID NOT OPEN CALL 'OPENERR'                @Y30LB10*/
*   /* TO PRINT THE 'UNABLE TO OPEN MESSAGE,                 @Y30LB10*/
*   /* SET 'RETCODE' TO 16, SET 'UT-VOERR' TO A 1            @Y30LB10*/
*   /* AND RETURN TO THE CALLER.                             @Y30LB10*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0098
*   IF DCBOFOPN=OFF                 /*                       @Y30LB10*/
*     THEN                          /*                       @Y30LB10*/
         L     @14,DCBPTR(,R4)                                     0098
         TM    DCBOFOPN+40(@14),B'00010000'                        0098
         BNZ   @RF00098                                            0098
*OPER2:                                                            0099
*     DO;                           /*                       @Y30LB10*/
OPER2    DS    0H                                                  0100
*       CALL OPENERR;               /*                       @Y30LB10*/
         BAL   @14,OPENERR                                         0100
*       RETURN;                     /* RET. TO MAINLINE      @Y30LB12*/
         B     @EL00002                                            0101
*     END OPER2;                    /*                       @Y30LB10*/
*   ELSE                                                           0103
*     ;                             /*                       @Y30LB10*/
@RF00098 DS    0H                                                  0104
*                                                                  0104
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* AT THIS POINT, THE VTOC OF THE APPROPRIATE            @YD00113*/
*   /* TABLES PACK HAS BEEN OPENED AND A FORMAT 1            @YD00113*/
*   /* FOR TABLE CREATE'S USAGE HAS BEEN WRITTEN IN          @YD00113*/
*   /* THE VTOC. THE DSCB JUST WRITTEN HAS ALSO BEEN         @YD00113*/
*   /* OPENED. THE ECB AND IOB ARE INITIALIZED AT            @YD00113*/
*   /* THIS TIME.                                            @YD00113*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0104
*   DSECB=''B;                      /* ZERO THE ECB          @YD00113*/
*                                                                  0104
         L     @14,DCBPTR(,R4)                                     0104
         SLR   @10,@10                                             0104
         ST    @10,DSECB(,@14)                                     0104
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* ZERO THE IOB. SET IN COMMAND CHAIN FLAG. SET          @Y30LB10*/
*   /* IN THE ECB AND DCB ADDRESSES.                         @Y30LB10*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0105
*   DSIOB=''B;                      /*                       @Y30LB10*/
         XC    DSIOB(40,@14),DSIOB(@14)                            0105
*   IOBFLAG1=CHAR40;                /*                       @Y30LB10*/
         MVC   IOBFLAG1(1,IOBSTPTR),CHAR40                         0106
*   IOBECBPB=ADDR(DSECB);           /*                       @Y30LB10*/
         LA    @10,DSECB(,@14)                                     0107
         ST    @10,@TF00001                                        0107
         MVC   IOBECBPB(3,IOBSTPTR),@TF00001+1                     0107
*   IOBDCBPB=DCBPTR;                /*                       @Y30LB10*/
*                                                                  0108
         ST    @14,@TF00001                                        0108
         MVC   IOBDCBPB(3,IOBSTPTR),@TF00001+1                     0108
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* IF OPEN FOR OUTPUT, RETURN TO CALLER. IF OPEN         @Y30LB10*/
*   /* FOR INPUT, BUILD THE READ INTERFACE AND LINK          @Y30LB10*/
*   /* TO ICGDSB05 TO READ THE VERIFICATION DATA.            @Y30LB10*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0109
*   IF DCBFLAG^=CHAR8F              /*                       @Y30LB10*/
*     THEN                          /*                       @Y30LB10*/
*                                                                  0109
         CLC   DCBFLAG(1,R4),CHAR8F                                0109
         BE    @RF00109                                            0109
*     /***************************************************************/
*     /*                                                             */
*     /* *******************************************         @YD00113*/
*     /* SET UP THE DCB ADDRESS, BUFFER ADDRESS,             @YD00113*/
*     /* AND THE CCHHR OF THE VERIFICATION TABLE.            @YD00113*/
*     /* SET THE LOGICAL-TO-REAL TRANSLATE TABLE             @YD00113*/
*     /* ENTRY FOR THE VERIFICATION TABLE CYLINDER           @YD00113*/
*     /* (VIRTUAL CYLINDER 7) TO ZERO. LINK TO               @YD00113*/
*     /* ICGDSB05 TO READ THE VERIFICATION TABLE             @YD00113*/
*     /* TRACK.                                              @YD00113*/
*     /* IF AN I/O ERROR OCCURS DURING THE READ OF           @YD00113*/
*     /* VERIFICATION TABLE TRACK, ICGDSB05 WILL             @YD00113*/
*     /* PRINT A SYNAD MESSAGE AND RETURN WITH THE           @YD00113*/
*     /* ERROR FLAGS SET.                                    @YD00113*/
*     /* *******************************************         @YD00113*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0110
*READ:                                                             0110
*     DO;                           /*                       @Y30LB10*/
READ     DS    0H                                                  0111
*       B05DCBPT=DCBPTR;            /*                       @Y30LB10*/
         L     @14,BUILDPTR(,R4)                                   0111
         MVC   B05DCBPT(4,@14),DCBPTR(R4)                          0111
*       B05BUFPT=ADDR(UT1BUFER);    /*                       @Y30LB10*/
         LR    @10,@14                                             0112
         AL    @10,@CF00997                                        0112
         ST    @10,B05BUFPT(,@14)                                  0112
*       B05ADDR=VCCHHR;             /*                       @Y30LB10*/
         MVC   B05ADDR(3,@14),VCCHHR                               0113
*       LTRTRANI(7)=ZERO;           /*                       @Y30LB10*/
         AL    @14,@CF00998                                        0114
         SLR   @10,@10                                             0114
         STH   @10,LTRTRANI-22316(,@14)                            0114
*       GEN(LINK EP=ICGDSB05);      /*                       @Y30LB10*/
         LINK EP=ICGDSB05
*     END READ;                     /*                       @Y30LB10*/
*   ELSE                                                           0117
*     ;                             /*                       @Y30LB10*/
@RF00109 DS    0H                                                  0118
*   END OPEN;                       /* RET. TO MAINLINE      @Y30LB12*/
         B     @EL00002                                            0118
*                                                                  0119
*/********************************************************** @YD00113*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - CLOSE                                   @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - CLOSE ALL DATA SETS THAT ARE OPEN             @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @YD00113*/
*                                                                  0119
*CLOSE:                                                            0119
*   PROCEDURE;                      /*                       @Y30LB12*/
*                                                                  0119
CLOSE    STM   @14,@12,@SA00003                                    0119
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* IF SYSUT1 OPEN, PUT DCB ADDRESS IN PARAMETER          @Y30LB10*/
*   /* AND CLOSE SYSUT1.                                     @Y30LB10*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0120
*   DCBPTR=ADDR(UT1DCB);            /*                       @Y30LB10*/
         L     @14,BUILDPTR(,R4)                                   0120
         LA    @14,UT1DCB(,@14)                                    0120
         ST    @14,DCBPTR(,R4)                                     0120
*   IF DCBOFOPN=ON                  /*                       @Y30LB10*/
*     THEN                          /*                       @Y30LB10*/
         TM    DCBOFOPN+40(@14),B'00010000'                        0121
         BNO   @RF00121                                            0121
*CLOS1:                                                            0122
*     DO;                           /*                       @Y30LB10*/
CLOS1    DS    0H                                                  0123
*       VTADDR1=DCBPTR;             /*                       @Y30LB12*/
         MVC   VTADDR1(4),DCBPTR(R4)                               0123
*       CALL COMCLOSE;              /* TO CLOSE SYSUT1       @Y30LB10*/
         BAL   @14,COMCLOSE                                        0124
*     END CLOS1;                    /*                       @Y30LB10*/
*   ELSE                                                           0126
*     ;                             /*                       @Y30LB10*/
*                                                                  0126
@RF00121 DS    0H                                                  0127
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* IF SYSUT2 OPEN, PUT DCB ADDRESS IN PARAMETER          @Y30LB10*/
*   /* LIST AND CLOSE SYSUT2.                                @Y30LB10*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0127
*   DCBPTR=ADDR(UT2DCB);            /* PTR TO DCB SYSUT2     @Y30LB10*/
         L     @14,BUILDPTR(,R4)                                   0127
         LA    @14,UT2DCB(,@14)                                    0127
         ST    @14,DCBPTR(,R4)                                     0127
*   IF DCBOFOPN=ON                  /* IF DCB IS OPEN        @Y30LB10*/
*     THEN                          /*                       @Y30LB10*/
         TM    DCBOFOPN+40(@14),B'00010000'                        0128
         BNO   @RF00128                                            0128
*CLOS2:                                                            0129
*     DO;                           /*                       @Y30LB10*/
*                                                                  0129
CLOS2    DS    0H                                                  0130
*       /*************************************************************/
*       /*                                                           */
*       /* *****************************************         @YD00113*/
*       /* LINK TO ICGDSB18 TO WRITE VERIFICATION            @Y30LB10*/
*       /* DATA. RESET DCB POINTER AND CALL                  @Y30LB12*/
*       /* COMMON CLOSE ROUTINE                              @Y30LB12*/
*       /* *****************************************         @YD00113*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0130
*       GEN(LINK EP=ICGDSB18);      /*                       @Y30LB10*/
         LINK EP=ICGDSB18
*       DCBPTR=ADDR(UT2DCB);        /*                       @Y30LB10*/
         L     @14,BUILDPTR(,R4)                                   0131
         LA    @14,UT2DCB(,@14)                                    0131
         ST    @14,DCBPTR(,R4)                                     0131
*       VTADDR2=DCBPTR;             /*                       @Y30LB10*/
         ST    @14,VTADDR2                                         0132
*       CALL COMCLOSE;              /* TO CLOSE SYSUT2       @Y30LB12*/
         BAL   @14,COMCLOSE                                        0133
*     END CLOS2;                    /*                       @Y30LB10*/
*   ELSE                                                           0135
*     ;                             /*                       @Y30LB10*/
*                                                                  0135
@RF00128 DS    0H                                                  0136
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* IF SYSUT3 OPEN, PUT DCB ADDRESS IN PARAMETER          @Y30LB10*/
*   /* LIST AND CLOSE SYSUT3.                                @Y30LB10*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0136
*   DCBPTR=ADDR(UT3DCB);            /*                       @Y30LB10*/
         L     @14,BUILDPTR(,R4)                                   0136
         LA    @14,UT3DCB(,@14)                                    0136
         ST    @14,DCBPTR(,R4)                                     0136
*   IF DCBOFOPN=ON                  /*                       @Y30LB10*/
*     THEN                          /*                       @Y30LB10*/
         TM    DCBOFOPN+40(@14),B'00010000'                        0137
         BNO   @RF00137                                            0137
*CLOS3:                                                            0138
*     DO;                           /*                       @Y30LB10*/
CLOS3    DS    0H                                                  0139
*       VTADDR3=DCBPTR;             /*                       @Y30LB10*/
         MVC   VTADDR3(4),DCBPTR(R4)                               0139
*       CALL COMCLOSE;              /* TO CLOSE SYSUT3       @Y30LB12*/
         BAL   @14,COMCLOSE                                        0140
*     END CLOS3;                    /*                       @Y30LB10*/
*   ELSE                                                           0142
*     ;                             /*                       @Y30LB10*/
@RF00137 DS    0H                                                  0143
*                                                                  0143
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00113*/
*   /* SET PARAMETER LIST FLAG TO WRITE FORMAT 0 DSCB ON VTOCS AND   */
*   /* CALL ICGDSB03 TO RESET THE VTOCS THAT WERE OPEN.      @Y30LB10*/
*   /* ***********************************************       @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0143
*   VTTYPE=VTTYPECL;                /*                       @Y30LB10*/
         MVI   VTTYPE,X'00'                                        0143
*   PARMPTZ=ADDR(VTPARM);           /*                       @Y30LB10*/
         LA    @14,VTPARM                                          0144
