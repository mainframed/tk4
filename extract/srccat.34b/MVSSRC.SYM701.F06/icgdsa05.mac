*                                                                  0070
*BADCPUID:                                                         0070
*   PROCEDURE;                      /*                       @Y30LB12*/
BADCPUID STM   @14,@12,@SA00003                                    0070
*   ERROR1='1'B;                    /* SET LOCAL ERR FLG     @Y30LB12*/
         OI    ERROR1,B'01000000'                                  0071
*   CALL GETPRIID;                  /*                       @Y30LB12*/
         BAL   @14,GETPRIID                                        0072
*   OUTBUF2(1:82)=ICGDS312;         /*                       @Y30LB12*/
         MVC   OUTBUF2(82,R4),ICGDS312                             0073
*   OUTBUF2(42)=PRIDS;              /*                       @Y30LB12*/
         MVC   OUTBUF2+41(1,R4),PRIDS                              0074
*   OUTBUF2(48)=PRICI;              /*                       @Y30LB12*/
         MVC   OUTBUF2+47(1,R4),PRICI                              0075
*   OUTBUF2(69)=ALTDS;              /*                       @Y30LB12*/
         MVC   OUTBUF2+68(1,R4),ALTDS                              0076
*   OUTBUF2(75)=ALTCI;              /*                       @Y30LB12*/
         MVC   OUTBUF2+74(1,R4),ALTCI                              0077
*   OUTBUF2(81:82)=TWOCHAR(DEVINDEX);/*                      @Y30LB12*/
         L     @10,DEVINDEX                                        0078
         ALR   @10,@10                                             0078
         LA    @05,TWOCHAR-2(@10)                                  0078
         MVC   OUTBUF2+80(2,R4),0(@05)                             0078
*   CALL PUTIT1;                    /*                       @Y30LB12*/
         BAL   @14,PUTIT1                                          0079
*   END BADCPUID;                   /*                       @Y30LB12*/
@EL00003 DS    0H                                                  0080
@EF00003 DS    0H                                                  0080
@ER00003 LM    @14,@12,@SA00003                                    0080
         BR    @14                                                 0080
