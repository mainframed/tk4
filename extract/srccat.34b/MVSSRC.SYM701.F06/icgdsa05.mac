         TITLE 'ICGDSA05 MSC TABLE CREATE, ANALYZE ALTERNATE PATHS     *
                        '
ICGDSA05 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ICGDSA05  76.335'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
*                                                                  0015
*/********************************************************** @Y30LB12*/
*/*                                                          @Y30LB12*/
*/*            ICGDSA05 (MAINLINE)                           @YD00113*/
*/*                                                          @Y30LB12*/
*/*            EXAMINE THE AREA IN WHICH ALTERNATE PATH      @YD00113*/
*/*            INFORMATION IS STORED TO DETERMINE WHETHER    @Y30LB12*/
*/*            AN ALTERNATE PATH IS SPECIFIED.  THE INDEX    @Y30LB12*/
*/*            DSINDEX IS THE INDEX TO DATA STAGER/CHANNEL   @Y30LB12*/
*/*            INTERFACE.  THE INNER INDEX DEVINDEX IS       @Y30LB12*/
*/*            THE INDEX TO DEVICE WITHIN DATA STAGER/       @Y30LB12*/
*/*            CHANNEL INTERFACE.                            @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @Y30LB12*/
*                                                                  0015
*   DO DSINDEX=1 TO 48;             /*                       @Y30LB12*/
*                                                                  0015
         LA    @10,1                                               0015
         ST    @10,DSINDEX                                         0015
@DL00015 DS    0H                                                  0016
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @ZD02028*/
*     /* IF THE STRUCTURE CPUASSOC INDICATES THAT THE        @ZD02028*/
*     /* OPERATING SYSTEM IS VS2, SET THE FLAG               @ZD02028*/
*     /* VS2LOCAL. NOTE THAT THE SETTING OF CPUREFNO         @ZD02028*/
*     /* IS DEPENDENT ON THE PREVIOUS EXECUTION OF THE       @ZD02028*/
*     /* MODULE ICGDSA01.                                    @ZD02028*/
*     /* ***********************************************     @ZD02028*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0016
*     VS2LOCAL='0'B;                /* INITIALIZE FLAG       @ZD02028*/
         NI    VS2LOCAL,B'11011111'                                0016
*     IF OPVS2(CPUREFNO(DSINDEX)+1)='1'B/*                   @ZD02028*/
*       THEN                        /*                       @ZD02028*/
         MH    @10,@CH00112                                        0017
         L     @05,COMPTR2(,R4)                                    0017
         SLR   @15,@15                                             0017
         IC    @15,CPUREFNO-9(@10,@05)                             0017
         SLA   @15,3                                               0017
         ALR   @15,R4                                              0017
         TM    OPVS2(@15),B'00001000'                              0017
         BNO   @RF00017                                            0017
*       VS2LOCAL='1'B;              /*                       @ZD02028*/
         OI    VS2LOCAL,B'00100000'                                0018
*     ELSE                                                         0019
*       ;                           /*                       @ZD02028*/
@RF00017 DS    0H                                                  0020
*     DO DEVINDEX=1 TO 64;          /*                       @Y30LB12*/
         LA    @10,1                                               0020
         ST    @10,DEVINDEX                                        0020
@DL00020 DS    0H                                                  0021
*       IF ALTPATH1(DSINDEX,DEVINDEX)^='000000'X/*           @Y30LB12*/
*         THEN                      /*                       @Y30LB12*/
         L     @05,DSINDEX                                         0021
         SLA   @05,6                                               0021
         ALR   @05,@10                                             0021
         MH    @05,@CH00246                                        0021
         L     @10,COMPTR2(,R4)                                    0021
         ST    @05,@TF00001                                        0021
         ALR   @05,@10                                             0021
         AL    @05,@CF00507                                        0021
         CLC   ALTPATH1-466(3,@05),@CB00436                        0021
         BE    @RF00021                                            0021
*         DO;                       /*                       @Y30LB12*/
*           WORKPATH=ALTPATH1(DSINDEX,DEVINDEX);/*           @Y30LB12*/
         AL    @10,@TF00001                                        0023
         AL    @10,@CF00507                                        0023
         MVC   WORKPATH(3),ALTPATH1-466(@10)                       0023
*           CALL CHKPATH;           /*                       @Y30LB12*/
         BAL   @14,CHKPATH                                         0024
*         END;                      /*                       @Y30LB12*/
*       ELSE                                                       0026
*         ;                         /*                       @Y30LB12*/
@RF00021 DS    0H                                                  0027
*       IF ALTPATH2(DSINDEX,DEVINDEX)^='000000'X/*           @Y30LB12*/
*           &VS2LOCAL='0'B          /* NOT 3 PATHS VS2       @ZD02028*/
*         THEN                      /*                       @Y30LB12*/
         L     @10,DSINDEX                                         0027
         SLA   @10,6                                               0027
         AL    @10,DEVINDEX                                        0027
         MH    @10,@CH00246                                        0027
         L     @05,COMPTR2(,R4)                                    0027
         ST    @10,@TF00001                                        0027
         ALR   @10,@05                                             0027
         AL    @10,@CF00508                                        0027
         CLC   ALTPATH2-469(3,@10),@CB00436                        0027
         BE    @RF00027                                            0027
         TM    VS2LOCAL,B'00100000'                                0027
         BNZ   @RF00027                                            0027
*         DO;                       /*                       @Y30LB12*/
*           WORKPATH=ALTPATH2(DSINDEX,DEVINDEX);/*           @Y30LB12*/
         AL    @05,@TF00001                                        0029
         AL    @05,@CF00508                                        0029
         MVC   WORKPATH(3),ALTPATH2-469(@05)                       0029
*           CALL CHKPATH;           /*                       @Y30LB12*/
         BAL   @14,CHKPATH                                         0030
*         END;                      /*                       @Y30LB12*/
*       ELSE                                                       0032
*         ;                         /*                       @Y30LB12*/
@RF00027 DS    0H                                                  0033
*       IF ALTPATH3(DSINDEX,DEVINDEX)^='000000'X/*           @Y30LB12*/
*           &VS2LOCAL='0'B          /* NOT 4 PATHS VS2       @ZD02028*/
*         THEN                      /*                       @Y30LB12*/
         L     @10,DSINDEX                                         0033
         SLA   @10,6                                               0033
         AL    @10,DEVINDEX                                        0033
         MH    @10,@CH00246                                        0033
         L     @05,COMPTR2(,R4)                                    0033
         ST    @10,@TF00001                                        0033
         ALR   @10,@05                                             0033
         AL    @10,@CF00509                                        0033
         CLC   ALTPATH3-472(3,@10),@CB00436                        0033
         BE    @RF00033                                            0033
         TM    VS2LOCAL,B'00100000'                                0033
         BNZ   @RF00033                                            0033
*         DO;                       /*                       @Y30LB12*/
*           WORKPATH=ALTPATH3(DSINDEX,DEVINDEX);/*           @Y30LB12*/
         AL    @05,@TF00001                                        0035
         AL    @05,@CF00509                                        0035
         MVC   WORKPATH(3),ALTPATH3-472(@05)                       0035
*           CALL CHKPATH;           /*                       @Y30LB12*/
         BAL   @14,CHKPATH                                         0036
*         END;                      /*                       @Y30LB12*/
*       ELSE                                                       0038
*         ;                         /*                       @Y30LB12*/
@RF00033 DS    0H                                                  0039
*       IF ALTPATH2(DSINDEX,DEVINDEX)^='000000'X/*           @ZD02028*/
*           &VS2LOCAL='1'B          /* 3 PATHS FOR VS2       @ZD02028*/
*         THEN                      /*                       @ZD02028*/
         L     @10,DSINDEX                                         0039
         SLA   @10,6                                               0039
         AL    @10,DEVINDEX                                        0039
         MH    @10,@CH00246                                        0039
         L     @05,COMPTR2(,R4)                                    0039
         ALR   @05,@10                                             0039
         AL    @05,@CF00508                                        0039
         CLC   ALTPATH2-469(3,@05),@CB00436                        0039
         BE    @RF00039                                            0039
         TM    VS2LOCAL,B'00100000'                                0039
         BNO   @RF00039                                            0039
*         DO;                       /*                       @ZD02028*/
*                                                                  0040
*           /*********************************************************/
*           /*                                                       */
*           /* *****************************************     @ZD02028*/
*           /* ICG340I FOR SA X INT X DEV XX, TOO MANY       @ZD02028*/
*           /* ALTERNATE PATHS                               @ZD02028*/
*           /* *****************************************     @ZD02028*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0041
*           OUTBUF2(1:85)=ICGDS340; /*                       @ZD02028*/
         MVI   OUTBUF2+56(R4),C' '                                 0041
         MVC   OUTBUF2+57(28,R4),OUTBUF2+56(R4)                    0041
         MVC   OUTBUF2(56,R4),ICGDS340                             0041
*           CALL GETPRIID;          /*                       @ZD02028*/
         BAL   @14,GETPRIID                                        0042
*           OUTBUF2(17)=PRIDS;      /*                       @ZD02028*/
         MVC   OUTBUF2+16(1,R4),PRIDS                              0043
*           OUTBUF2(23)=PRICI;      /*                       @ZD02028*/
         MVC   OUTBUF2+22(1,R4),PRICI                              0044
*           OUTBUF2(29:30)=TWOCHAR(DEVINDEX);/*              @ZD02028*/
         L     @10,DEVINDEX                                        0045
         ALR   @10,@10                                             0045
         LA    @05,TWOCHAR-2(@10)                                  0045
         MVC   OUTBUF2+28(2,R4),0(@05)                             0045
*           CALL PUTIT1;            /*                       @ZD02028*/
         BAL   @14,PUTIT1                                          0046
*         END;                      /*                       @ZD02028*/
*       ELSE                                                       0048
*         ;                         /*                       @ZD02028*/
@RF00039 DS    0H                                                  0049
*     END;                          /* OF DO DEVINDEX        @Y30LB12*/
         LA    @10,1                                               0049
         AL    @10,DEVINDEX                                        0049
         ST    @10,DEVINDEX                                        0049
         CH    @10,@CH00245                                        0049
         BNH   @DL00020                                            0049
*   END;                            /* OF DO DSINDEX         @Y30LB12*/
         LA    @10,1                                               0050
         AL    @10,DSINDEX                                         0050
         ST    @10,DSINDEX                                         0050
         CH    @10,@CH00235                                        0050
         BNH   @DL00015                                            0050
*   RETURN;                         /* RET. TO ICGDSANL      @Y30LB12*/
@EL00001 L     @13,4(,@13)                                         0051
@EF00001 L     @00,@SIZDATD                                        0051
         LR    @01,@11                                             0051
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0051
         BR    @14                                                 0051
*                                                                  0052
*/********************************************************** @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - CHKPATH                                 @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - THIS PROCEDURE DEVELOPS THE INDEX TO          @Y30LB12*/
*/*            LOCATE THE DATA STAGER/CHANNEL INTERFACE      @Y30LB12*/
*/*            SPECIFIED AS THE ALTERNATE PATH.  EACH        @Y30LB12*/
*/*            DATA STAGER HAS THREE POSSIBLE CHANNEL        @Y30LB12*/
*/*            INTERFACES AND VEIFIES THAT THE CPUID         @Y30LB12*/
*/*            FOR THE ALTERNATE PATH IS THE SAME AS         @Y30LB12*/
*/*            THAT OF THE PRINARY PATH, THAT THE CONTRAL    @Y30LB12*/
*/*            UNIT IDENTIFICATION OF THE ALTERNATE          @Y30LB12*/
*/*            PATH IS THE SAME AS THAT OF THE PRIMARY,      @Y30LB12*/
*/*            AND THAT THE CHANNEL NUMBER OF THE            @Y30LB12*/
*/*            ALTERNATE PATH IS HIGHER THAN THAT OF         @Y30LB12*/
*/*            THE PRIMARY.  (THE ALTERNATES THEMSELVES      @Y30LB12*/
*/*            WILL BE PLACED IN ASCENDING SEQUENCE          @Y30LB12*/
*/*            WHEN THE IODEVICE STATEMENTS ARE PUNCHED.)    @Y30LB12*/
*/*            IF THE ABOVE TESTS ARE SUCCESSFUL, THE        @Y30LB12*/
*/*            DEVICE ADDRESS ON THE ALTERNATE PATH          @Y30LB12*/
*/*            DATA STAGER/CHANNEL INTERFACE ARE CHECKED     @Y30LB12*/
*/*            FOR PREVIOUS USAGE.  IF PREVIOUSLY USED,      @Y30LB12*/
*/*            AN ERROR MESSAGE IS WRITTEN INDICATING        @Y30LB12*/
*/*            USAGE.  IF THE PATH HAS NOT BEEN USED,        @Y30LB12*/
*/*            THE FLAGS USESPEC AND INISALTP ARE SET        @Y30LB12*/
*/*            FOR THE DEVICE ADDRESS ON THE ALTERNATE       @Y30LB12*/
*/*            DATA STAGER/CHANNEL INTERFACE.                @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @Y30LB12*/
*                                                                  0052
*CHKPATH:                                                          0052
*   PROCEDURE;                      /*                       @Y30LB12*/
CHKPATH  STM   @14,@12,@SA00002                                    0052
*   ALTINDEX=WORKADS;               /*                       @Y30LB12*/
         SLR   @10,@10                                             0053
         IC    @10,WORKADS                                         0053
         ST    @10,ALTINDEX                                        0053
*   ALTINDEX=ALTINDEX*3;            /*                       @Y30LB12*/
         MH    @10,@CH00048                                        0054
         ST    @10,ALTINDEX                                        0054
*   ALTINDEX=ALTINDEX+WORKACI;      /*                       @Y30LB12*/
         MVC   @ZT00003+3(1),WORKACI                               0055
         AL    @10,@ZT00003                                        0055
         ST    @10,ALTINDEX                                        0055
*   ERROR1='0'B;                    /*                       @Y30LB12*/
         NI    ERROR1,B'10111111'                                  0056
*   IF RSGCPUID(ALTINDEX)^=RSGCPUID(DSINDEX)/*               @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         LA    @05,9                                               0057
         LR    @01,@10                                             0057
         MR    @00,@05                                             0057
         L     @10,COMPTR2(,R4)                                    0057
         LR    @03,@05                                             0057
         M     @02,DSINDEX                                         0057
         ALR   @01,@10                                             0057
         ALR   @10,@03                                             0057
         CLC   RSGCPUID-9(5,@01),RSGCPUID-9(@10)                   0057
         BE    @RF00057                                            0057
*     CALL BADCPUID;                /*                       @Y30LB12*/
         BAL   @14,BADCPUID                                        0058
*   ELSE                                                           0059
*     ;                             /*                       @Y30LB12*/
@RF00057 DS    0H                                                  0060
*   IF RSGCUA(ALTINDEX)^=RSGCUA(DSINDEX)/*                   @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         LA    @10,9                                               0060
         L     @01,ALTINDEX                                        0060
         MR    @00,@10                                             0060
         L     @05,COMPTR2(,R4)                                    0060
         LR    @03,@10                                             0060
         M     @02,DSINDEX                                         0060
         ALR   @01,@05                                             0060
         ALR   @05,@03                                             0060
         CLC   RSGCUA-9(1,@01),RSGCUA-9(@05)                       0060
         BE    @RF00060                                            0060
*     CALL BADCUID;                 /*                       @Y30LB12*/
         BAL   @14,BADCUID                                         0061
*   ELSE                                                           0062
*     ;                             /*                       @Y30LB12*/
@RF00060 DS    0H                                                  0063
*   IF RSGCHAN(ALTINDEX)<=RSGCHAN(DSINDEX)/*                 @Y30LB12*/
*       ×RSGCHAN(ALTINDEX)^=WORKACH /*                       @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         LA    @10,9                                               0063
         L     @01,ALTINDEX                                        0063
         MR    @00,@10                                             0063
         L     @05,COMPTR2(,R4)                                    0063
         LR    @03,@10                                             0063
         M     @02,DSINDEX                                         0063
         ST    @01,@TF00001                                        0063
         ALR   @01,@05                                             0063
         ALR   @03,@05                                             0063
         CLC   RSGCHAN-9(1,@01),RSGCHAN-9(@03)                     0063
         BNH   @RT00063                                            0063
         L     @10,@TF00001                                        0063
         SLR   @15,@15                                             0063
         IC    @15,RSGCHAN-9(@10,@05)                              0063
         MVC   @ZT00003+3(1),WORKACH                               0063
         C     @15,@ZT00003                                        0063
         BE    @RF00063                                            0063
@RT00063 DS    0H                                                  0064
*     CALL BADCHAN;                 /*                       @Y30LB12*/
         BAL   @14,BADCHAN                                         0064
*   ELSE                                                           0065
*     ;                             /*                       @Y30LB12*/
@RF00063 DS    0H                                                  0066
*   IF ERROR1='0'B                  /*                       @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         TM    ERROR1,B'01000000'                                  0066
         BNZ   @RF00066                                            0066
*     CALL CHECKDEV;                /*                       @Y30LB12*/
         BAL   @14,CHECKDEV                                        0067
*   ELSE                                                           0068
*     ;                             /*                       @Y30LB12*/
@RF00066 DS    0H                                                  0069
*   END CHKPATH;                    /*                       @Y30LB12*/
@EL00002 DS    0H                                                  0069
@EF00002 DS    0H                                                  0069
@ER00002 LM    @14,@12,@SA00002                                    0069
         BR    @14                                                 0069
*                                                                  0070
