         TITLE 'ICGDSS01 MSC TABLE CREATE, SCAN SDGXX STATEMENT        *
                        '
ICGDSS01 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ICGDSS01  76.337'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
*                                                                  0020
*/********************************************************** @YD00113*/
*/*                                                          @Y30LB12*/
*/*            ICGDSS01 (MAINLINE)                           @YD00113*/
*/*                                                          @Y30LB12*/
*/*            CALL S01INIT, S01SCAN, S01ANAL. IF NO         @YD00113*/
*/*            TERMINATING ERRORS, CALL S01BUILD.  RETURN    @Y30LB12*/
*/*            TO ICGDSSCN.                                  @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @YD00113*/
*                                                                  0020
*   CALL S01INIT;                   /* TO INITIALIZE         @Y30LB12*/
         BAL   @14,S01INIT                                         0020
*   CALL S01SCAN;                   /* READ KEYS, PARMS      @Y30LB12*/
         BAL   @14,S01SCAN                                         0021
*   CALL S01ANAL;                   /* CHECK VALUES          @Y30LB12*/
         BAL   @14,S01ANAL                                         0022
*   IF STOPSCAN^='1'B               /* TERM ERROR FLAG       @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         TM    STOPSCAN,B'01000000'                                0023
         BO    @RF00023                                            0023
*     CALL S01BUILD;                /* PERMANENTLY SAVE      @Y30LB12*/
         BAL   @14,S01BUILD                                        0024
*   ELSE                                                           0025
*     ;                             /*                       @Y30LB12*/
@RF00023 DS    0H                                                  0026
*   RETURN;                         /* RET. TO ICGDSSCN      @Y30LB12*/
@EL00001 L     @13,4(,@13)                                         0026
@EF00001 L     @00,@SIZDATD                                        0026
         LR    @01,@11                                             0026
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0026
         BR    @14                                                 0026
*                                                                  0027
*/********************************************************** @YD00113*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - S01INIT                                 @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - THIS PROCEDURE INITIALIZES THE LOCAL WORK     @Y30LB12*/
*/*            AREA RSGWORKA AND EXTRACTS THE SDG NUMBER     @YD00113*/
*/*            FROM THE SUFFIX OF THE COMMAND WORD.          @Y30LB12*/
*/*            IT THEN TESTS THE LENGTH OF THE COMMAND AND   @YD00113*/
*/*            SETS AN ERROR FLAG IF THE LENGTH IS TOO LONG  @YD00113*/
*/*            OR TOO SHORT.  IF A LENGTH ERROR IS DETECTED  @YD00113*/
*/*            AN ERROR MESSAGE IS PRINTED VIA 'PUTIT2'.     @YD00113*/
*/*            IF NO LENGTH ERROR IS FOUND, THE VALUE        @YD00113*/
*/*            RETURNED FOR THE SDG IS TESTED.  IF THE VALUE @YD00113*/
*/*            IS GREATER THAN 27 (DEC.), ERROR MESSAGE      @YD00113*/
*/*            ICG223I IS PRINTED VIA 'PUTIT1'.              @YD00113*/
*/*                                                          @Y30LB12*/
*/********************************************************** @YD00113*/
*                                                                  0027
*S01INIT:                                                          0027
*   PROCEDURE;                      /*                       @Y30LB12*/
S01INIT  STM   @14,@12,@SA00002                                    0027
*   RSGMSGID='00'X;                 /* CONTROL FLAGS         @Y30LB12*/
         MVI   RSGMSGID,X'00'                                      0028
*   HAVES='00'X;                    /* 'FOUND IT' FLAGS      @Y30LB12*/
         MVI   HAVES,X'00'                                         0029
*   WRKRSGNO=0;                     /* INDEX TO BUILDRSG     @Y30LB12*/
         SLR   @10,@10                                             0030
         STH   @10,WRKRSGNO                                        0030
*   WORKRSG='0000'X;                /* SDG NUMBER            @YD00113*/
         MVC   WORKRSG(2),@CB00349                                 0031
*   DO I=1 TO 4;                    /*                       @OZ05139*/
         LA    @10,1                                               0032
         STH   @10,I                                               0032
@DL00032 DS    0H                                                  0033
*     WKSTAMPS(I)='00'X;            /* CLEAR LRUGROUPS       @OZ05139*/
         LA    @07,WKSTAMPS-1(@10)                                 0033
         MVI   0(@07),X'00'                                        0033
*   END;                            /*                       @OZ05139*/
         AH    @10,@CH00042                                        0034
         STH   @10,I                                               0034
         CH    @10,@CH00068                                        0034
         BNH   @DL00032                                            0034
*   IF SCANDATA(4)^='40'X           /*                       @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         L     @10,STARTCOL(,R4)                                   0035
         CLI   SCANDATA+3(@10),X'40'                               0035
         BE    @RF00035                                            0035
*     IF SCANDATA(5)^='40'X         /*                       @Y30LB12*/
*       THEN                        /*                       @Y30LB12*/
         CLI   SCANDATA+4(@10),X'40'                               0036
         BE    @RF00036                                            0036
*       IF SCANDATA(6)='40'X        /*                       @Y30LB12*/
*         THEN                      /*                       @Y30LB12*/
         CLI   SCANDATA+5(@10),X'40'                               0037
         BNE   @RF00037                                            0037
*         DO;                       /*                       @Y30LB12*/
*           WORKRSG=SCANDATA(4:5);  /*                       @Y30LB12*/
         MVC   WORKRSG(2),SCANDATA+3(@10)                          0039
*           CALL CONVDEC;           /* CONVERT TO BINARY     @Y30LB12*/
         BAL   @14,CONVDEC                                         0040
*         END;                      /*                       @Y30LB12*/
*       ELSE                        /*                       @Y30LB12*/
*         ERROR1='1'B;              /* COMMAND TOO LONG      @Y30LB12*/
         B     @RC00037                                            0042
@RF00037 OI    ERROR1,B'00010000'                                  0042
*     ELSE                          /*                       @Y30LB12*/
*       DO;                         /*                       @Y30LB12*/
         B     @RC00036                                            0043
@RF00036 DS    0H                                                  0044
*         WORKRSG(1)='F0'X;         /*                       @Y30LB12*/
         MVI   WORKRSG,X'F0'                                       0044
*         WORKRSG(2)=SCANDATA(4);   /*                       @Y30LB12*/
         L     @10,STARTCOL(,R4)                                   0045
         MVC   WORKRSG+1(1),SCANDATA+3(@10)                        0045
*         CALL CONVDEC;             /* CONVERT TO BINARY     @Y30LB12*/
         BAL   @14,CONVDEC                                         0046
*       END;                        /*                       @Y30LB12*/
*   ELSE                            /*                       @Y30LB12*/
*     ERROR1='1'B;                  /* COMMAND TOO SHORT     @Y30LB12*/
         B     @RC00035                                            0048
@RF00035 OI    ERROR1,B'00010000'                                  0048
*   IF ERROR1='1'B                  /* LENGTH ERROR          @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
@RC00035 TM    ERROR1,B'00010000'                                  0049
         BNO   @RF00049                                            0049
*     DO;                           /*                       @Y30LB12*/
*                                                                  0050
*       /*************************************************************/
*       /*                                                           */
*       /* *****************************************         @YD00113*/
*       /* ICG204I INVALID COMMAND WORD                      @Y30LB12*/
*       /* *****************************************         @YD00113*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0051
*       MSGNO=ICG204I;              /* ID FOR ICGDS204       @Y30LB12*/
         MVC   MSGNO(2,R4),ICG204I                                 0051
*       CALL PUTIT2;                /* TO MESSAGE WRITER     @Y30LB12*/
         BAL   @14,PUTIT2                                          0052
*     END;                          /*                       @Y30LB12*/
*   ELSE                            /*                       @Y30LB12*/
*     DO;                           /*                       @Y30LB12*/
         B     @RC00049                                            0054
@RF00049 DS    0H                                                  0055
*       IF WORKRSG>27               /* RET'D BY CONVDEC      @Y30LB12*/
*         THEN                      /*                       @Y30LB12*/
         CLC   WORKRSG(2),@CH00313                                 0055
         BNH   @RF00055                                            0055
*         DO;                       /*                       @Y30LB12*/
*                                                                  0056
*           /*********************************************************/
*           /*                                                       */
*           /* ***********************************           @YD00113*/
*           /* ICG223I VALUE ERROR FOR SDG                   @YD00113*/
*           /* ***********************************           @YD00113*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0057
*           OUTBUF2(1:25)=ICGDS223; /*                       @Y30LB12*/
         MVC   OUTBUF2(25,R4),ICGDS223                             0057
*           OUTBUF2(26:28)='SDG';   /*                       @Y30LB12*/
         MVC   OUTBUF2+25(3,R4),@CC00356                           0058
*           CALL PUTIT1;            /* TO MESSAGE WRITER     @Y30LB12*/
         BAL   @14,PUTIT1                                          0059
*         END;                      /*                       @Y30LB12*/
*       ELSE                        /*                       @Y30LB12*/
*         WRKRSGNO=WORKRSG+1;       /*                       @Y30LB12*/
         B     @RC00055                                            0061
@RF00055 LA    @10,1                                               0061
         MVC   @ZT00002+2(2),WORKRSG                               0061
         AL    @10,@ZT00002                                        0061
         STH   @10,WRKRSGNO                                        0061
*     END;                          /*                       @Y30LB12*/
*   END S01INIT;                    /* RET. TO MAINLINE      @Y30LB12*/
@EL00002 DS    0H                                                  0063
@EF00002 DS    0H                                                  0063
@ER00002 LM    @14,@12,@SA00002                                    0063
         BR    @14                                                 0063
*                                                                  0064
*/********************************************************** @YD00113*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - S01SCAN                                 @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - SCAN KEYWORDS AND CALL PROPER ROUTINE TO      @Y30LB12*/
*/*            HANDLE PARAMETERS.                            @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @YD00113*/
*                                                                  0064
*S01SCAN:                                                          0064
*   PROCEDURE;                      /*                       @Y30LB12*/
S01SCAN  STM   @14,@12,@SA00003                                    0064
*   DO WHILE(LASTPARM^='1'B);       /* TILL NO MORE DATA     @Y30LB12*/
         B     @DE00065                                            0065
@DL00065 DS    0H                                                  0066
*     CALL READS99;                 /* EXTRACT KEYWORD       @Y30LB12*/
         BAL   @14,READS99                                         0066
*     KNOWN='0'B;                   /* RESET REC. FLAG       @Y30LB12*/
         NI    KNOWN,B'01111111'                                   0067
*     IF LASTPARM='1'B              /* IF LAST OF DATA       @Y30LB12*/
*       THEN                        /*                       @Y30LB12*/
         TM    LASTPARM(R4),B'00001000'                            0068
         BO    @RT00068                                            0068
*       RETURN;                     /* RET. TO MAINLINE      @Y30LB12*/
*     ELSE                                                         0070
*       ;                           /*                       @Y30LB12*/
*                                                                  0070
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @YD00113*/
*     /* IF THE FIELD JUST EXTRACTED IS NOT A KEYWORD,       @Y30LB12*/
*     /* WRITE MESSAGE ICG209I EXPECTED, BUT DID NOT         @Y30LB12*/
*     /* FIND, KEYWORD', AND RETURN TO MAINLINE              @Y30LB12*/
*     /* ***********************************************     @YD00113*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0071
*     IF KEYBIT^='1'B               /* SET BY ICGDSS99       @Y30LB12*/
*       THEN                        /*                       @Y30LB12*/
         TM    KEYBIT(R4),B'01000000'                              0071
         BO    @RF00071                                            0071
*       DO;                         /*                       @Y30LB12*/
*         MSGNO=ICG209I;            /* ID FOR ICGDS209       @Y30LB12*/
         MVC   MSGNO(2,R4),ICG209I                                 0073
*         CALL PUTIT2;              /* TO MESSAGE WRITER     @Y30LB12*/
         BAL   @14,PUTIT2                                          0074
*         RETURN;                   /* RET. TO MAINLINE      @Y30LB12*/
@EL00003 DS    0H                                                  0075
@EF00003 DS    0H                                                  0075
@ER00003 LM    @14,@12,@SA00003                                    0075
         BR    @14                                                 0075
*       END;                        /*                       @Y30LB12*/
*     ELSE                                                         0077
*       ;                           /*                       @Y30LB12*/
*                                                                  0077
@RF00071 DS    0H                                                  0078
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @YD00113*/
*     /* IF THE FIELD LAST SCANNED WAS THE KEYWORD           @Y30LB12*/
*     /* ACTPAGE, SET THE 'KNOWN' FLAG ON, AND CALL          @YD00140*/
*     /* 'S01THOLD' TO GET THE PARAMETERS                    @YD00113*/
*     /* ***********************************************     @YD00113*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0078
*     IF SCANDATA(1:8)='ACTPAGE='   /*                       @YD00140*/
*       THEN                        /*                       @Y30LB12*/
         L     @10,STARTCOL(,R4)                                   0078
         CLC   SCANDATA(8,@10),@CC00363                            0078
         BNE   @RF00078                                            0078
*       DO;                         /*                       @Y30LB12*/
*         KNOWN='1'B;               /* RECOGNIZED KEYWD      @Y30LB12*/
         OI    KNOWN,B'10000000'                                   0080
*         CALL S01THOLD;            /* TO GET PARAMETERS     @Y30LB12*/
         BAL   @14,S01THOLD                                        0081
*       END;                        /*                       @Y30LB12*/
*     ELSE                                                         0083
*       ;                           /*                       @Y30LB12*/
*                                                                  0083
@RF00078 DS    0H                                                  0084
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @YD00113*/
*     /* IF THE FIELD LAST SCANNED WAS THE KEYWORD           @Y30LB12*/
*     /* LRUGROUP, SET 'KNOWN' FLAG ON AND CALL              @YD00113*/
*     /* 'S01STAMP' TO GET THE PARAMETERS                    @YD00113*/
*     /* ***********************************************     @YD00113*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0084
*     IF SCANDATA(1:9)='LRUGROUP='  /*                       @Y30LB12*/
*       THEN                        /*                       @Y30LB12*/
         L     @10,STARTCOL(,R4)                                   0084
         CLC   SCANDATA(9,@10),@CC00365                            0084
         BNE   @RF00084                                            0084
*       DO;                         /*                       @Y30LB12*/
*         KNOWN='1'B;               /* RECOGNIZED KEYWD      @Y30LB12*/
         OI    KNOWN,B'10000000'                                   0086
*         CALL S01STAMP;            /* TO GET PARAMETERS     @Y30LB12*/
         BAL   @14,S01STAMP                                        0087
*       END;                        /*                       @Y30LB12*/
*     ELSE                                                         0089
*       ;                           /*                       @Y30LB12*/
*                                                                  0089
@RF00084 DS    0H                                                  0090
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @YD00113*/
*     /* THE KEYWORD WAS NOT RECOGNIZED AS EITHER            @Y30LB12*/
*     /* OF THE ABOVE, SET 'KNOWN' FLAGG TO ZERO (OFF)       @YD00113*/
*     /* AND CALL 'UNKNOWN' FOR ERROR MESSAGE AND            @YD00113*/
*     /* SET UP FOR NEXT KEYWORD.                            @YD00113*/
*     /* ***********************************************     @YD00113*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0090
*     IF KNOWN='0'B                 /* UNKNOWN KEYWORD       @Y30LB12*/
*       THEN                        /*                       @Y30LB12*/
         TM    KNOWN,B'10000000'                                   0090
         BNZ   @RF00090                                            0090
*       CALL UNKNOWN;               /* TO WRITE MSG, ETC     @Y30LB12*/
         BAL   @14,UNKNOWN                                         0091
*     ELSE                                                         0092
*       ;                           /*                       @Y30LB12*/
@RF00090 DS    0H                                                  0093
*   END;                            /* END OF DO WHILE       @Y30LB12*/
@DE00065 TM    LASTPARM(R4),B'00001000'                            0093
         BNO   @DL00065                                            0093
*   END S01SCAN;                    /* RET. TO MAINLINE      @Y30LB12*/
         B     @EL00003                                            0094
*                                                                  0095
*/********************************************************** @YD00113*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - S01THOLD                                @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - EXTRACT TWO PARAMETERS, THE LOWER AND UPPER   @Y30LB12*/
*/*            THRESHOLD VALUES FOR THE THE LRU MECHANISM,   @Y30LB12*/
*/*            FROM THE CONTROL STATEMENT.                   @Y30LB12*/
*/*            THE ACTUAL EXTRACTION AND FORMATTING OF THE   @Y30LB12*/
*/*            DATA IS PERFORMED IN THE CALLED PROCEDURE     @Y30LB12*/
*/*            CHECKDEC.  IF AN ERROR IS FOUND IN THE DATA,  @Y30LB12*/
*/*            EITHER THE ERROR MESSAGE IS WRITTEN IN        @Y30LB12*/
*/*            CHECKDEC OR THE FLAG NOTPARM IS SET ON.       @Y30LB12*/
*/*                                                          @Y30LB12*/
*/********************************************************** @YD00113*/
*                                                                  0095
*S01THOLD:                                                         0095
*   PROCEDURE;                      /*                       @Y30LB12*/
S01THOLD STM   @14,@12,@SA00004                                    0095
*   NOTPARM='0'B;                   /* CONTROL FLAG OFF      @Y30LB12*/
         NI    NOTPARM,B'11011111'                                 0096
*   PLUGIN='ACTPAGE ';              /* FOR MESSAGE TEXT      @YD00140*/
*                                                                  0097
         MVC   PLUGIN(8),@CC00372                                  0097
*   /*****************************************************************/
*   /*                                                               */
*   /* **********************************************        @YD00113*/
*   /* TEST FOR DUPLICATE KEYWORD 'ACTPAGE'. IF NO           @YD00140*/
*   /* DUPLICATION, SET FLAG 'HAVTHOLD' ON. OTHERWISE        @YD00113*/
*   /* ISSUE ERROR MESSAGE ICG219I VIA 'PUTIT1'              @YD00113*/
*   /* **********************************************        @YD00113*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0098
*   IF HAVTHOLD^='1'B               /*                       @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         TM    HAVTHOLD,B'10000000'                                0098
         BO    @RF00098                                            0098
*     HAVTHOLD='1'B;                /*                       @Y30LB12*/
         OI    HAVTHOLD,B'10000000'                                0099
*   ELSE                            /*                       @Y30LB12*/
*     DO;                           /*                       @Y30LB12*/
*                                                                  0100
         B     @RC00098                                            0100
@RF00098 DS    0H                                                  0101
*       /*************************************************************/
*       /*                                                           */
*       /* *****************************************         @YD00113*/
*       /* ICG219I DUPLICATE KEYWORD ACTPAGE                 @YD00140*/
*       /* *****************************************         @YD00113*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0101
*       OUTBUF2(1:27)=ICGDS219;     /*                       @Y30LB12*/
         MVC   OUTBUF2(27,R4),ICGDS219                             0101
*       OUTBUF2(28:34)='ACTPAGE';   /*                       @YD00140*/
         MVC   OUTBUF2+27(7,R4),@CC00373                           0102
*       CALL PUTIT1;                /* TO MESSAGE WRITER     @Y30LB12*/
         BAL   @14,PUTIT1                                          0103
*     END;                          /*                       @Y30LB12*/
*   CALL CHECKDEC;                  /* GET FIRST VALUE       @Y30LB12*/
@RC00098 BAL   @14,CHECKDEC                                        0105
*   IF NOTPARM^='1'B                /* IS A PARAMETER        @Y30LB12*/
*     THEN                          /*                       @Y30LB12*/
         TM    NOTPARM,B'00100000'                                 0106
         BO    @RF00106                                            0106
*     DO;                           /*                       @Y30LB12*/
*       WRKLOWER=WRKLOWUP;          /* SAVE LOW VALUE        @Y30LB12*/
         MVC   WRKLOWER(1),WRKLOWUP                                0108
*       CALL CHECKDEC;              /* GET OTHER VALUE       @Y30LB12*/
*                                                                  0109
         BAL   @14,CHECKDEC                                        0109
*       /*************************************************************/
*       /*                                                           */
*       /* *****************************************         @YD00113*/
*       /* TEST 'NOTPARM' FLAG FROM 'CHECKDEC' TO            @YD00113*/
*       /* DETERMINE IF VALUE WAS THE LOW VALUE              @YD00113*/
*       /* PARAMETER. IF IT WAS, REISSUE 'CHECKDEC'          @YD00113*/
*       /* TO GET THE HIGH VALUE. THE SAME TEST              @YD00113*/
*       /* IS MADE. IF BOTH VALUES ARE OK THEY ARE           @YD00113*/
*       /* ARE SAVED. IF EITHER VALUE IS NOT OK,             @YD00113*/
*       /* ERROR MESSAGE ICG211I IS ISSUED.                  @YD00113*/
*       /* *****************************************         @YD00113*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0110
*       IF NOTPARM^='1'B            /* IS A PARAMETER        @Y30LB12*/
*         THEN                      /*                       @Y30LB12*/
         TM    NOTPARM,B'00100000'                                 0110
         BO    @RF00110                                            0110
*         WRKUPPER=WRKLOWUP;        /* SAVE HIGH VALUE       @Y30LB12*/
         MVC   WRKUPPER(1),WRKLOWUP                                0111
*       ELSE                        /*                       @Y30LB12*/
*         DO;                       /*                       @Y30LB12*/
*                                                                  0112
         B     @RC00110                                            0112
@RF00110 DS    0H                                                  0113
*           /*********************************************************/
*           /*                                                       */
*           /* ***********************************           @YD00113*/
*           /* ICG211I EXPECTED, BUT DID NOT @Y30LB12X FIND,         */
*           /* PARAMETER                                     @Y30LB12*/
*           /* ***********************************           @YD00113*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0113
*           STARTCOL=SAVESCOL;      /* FOR RESTART           @Y30LB12*/
         MVC   STARTCOL(4,R4),SAVESCOL(R4)                         0113
*           NEXTCOL=SAVENCOL;       /* FOR RESTART           @Y30LB12*/
         MVC   NEXTCOL(4,R4),SAVENCOL(R4)                          0114
*           MSGNO=ICG211I;          /* ID FOR ICGDS211       @Y30LB12*/
         MVC   MSGNO(2,R4),ICG211I                                 0115
*           CALL PUTIT2;            /* TO MESSAGE WRITER     @Y30LB12*/
         BAL   @14,PUTIT2                                          0116
*         END;                      /*                       @Y30LB12*/
*     END;                          /*                       @Y30LB12*/
*   ELSE                            /*                       @Y30LB12*/
*     DO;                           /*                       @Y30LB12*/
*                                                                  0119
         B     @RC00106                                            0119
@RF00106 DS    0H                                                  0120
*       /*************************************************************/
*       /*                                                           */
*       /* *****************************************         @YD00113*/
*       /* ICG211I EXPECTED, BUT DID NOT FIND,               @Y30LB12*/
*       /* PARAMETER                                         @Y30LB12*/
*       /* *****************************************         @YD00113*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0120
*       STARTCOL=SAVESCOL;          /* FOR RESTART           @Y30LB12*/
         MVC   STARTCOL(4,R4),SAVESCOL(R4)                         0120
*       NEXTCOL=SAVENCOL;           /* FOR RESTART           @Y30LB12*/
         MVC   NEXTCOL(4,R4),SAVENCOL(R4)                          0121
*       MSGNO=ICG211I;              /* ID FOR ICGDS211       @Y30LB12*/
         MVC   MSGNO(2,R4),ICG211I                                 0122
*       CALL PUTIT2;                /* TO MESSAGE WRITER     @Y30LB12*/
         BAL   @14,PUTIT2                                          0123
*     END;                          /*                       @Y30LB12*/
*   END S01THOLD;                   /* RETURN TO S01SCAN     @Y30LB12*/
@EL00004 DS    0H                                                  0125
@EF00004 DS    0H                                                  0125
@ER00004 LM    @14,@12,@SA00004                                    0125
         BR    @14                                                 0125
*                                                                  0126
*/********************************************************** @YD00113*/
*/*                                                          @Y30LB12*/
*/* PROCEDURE NAME - CHECKDEC                                @Y30LB12*/
*/*                                                          @Y30LB12*/
*/* FUNCTION - EXTRACT NEXT PARAMETER FROM CONTROL           @Y30LB12*/
*/*            STATEMENT.  IF VALUE EXTRACTED IS NOT A       @Y30LB12*/
*/*            PARAMETER, RETURN TO CALLING ROUTINE.  IF     @Y30LB12*/
*/*            LENGTH OF PARAMETER NOT 1 OR 2, WRITE MESSAGE @Y30LB12*/
