         SLR   @15,@15                                             0021
         IC    @15,TOMPCPU(@03,R4)                                 0021
         SLA   @15,3                                               0021
         ST    @10,@TF00001                                        0021
         ALR   @10,@05                                             0021
         ALR   @15,R4                                              0021
         CLC   RSGCPUID-9(5,@10),CPUID(@15)                        0021
         BNE   @RF00021                                            0021
         LR    @10,@05                                             0021
         AL    @10,@TF00001                                        0021
         CLC   RSGCHAN-9(1,@10),RSGCHAN(@05)                       0021
