*                                                                  0074
*BUILDLTR:                                                         0074
*   PROCEDURE;                      /*                       @YD00113*/
BUILDLTR STM   @14,@12,12(@13)                                     0074
*   DO I=1 TO 7;                    /* 8 ENTRIES PAGE 0      @YD00113*/
         LA    I,1                                                 0075
@DL00075 DS    0H                                                  0076
*     LTRTRANO(I)=0;                /*                       @YD00113*/
         LR    @10,I                                               0076
         ALR   @10,@10                                             0076
         L     @07,BUILDPTR(,R4)                                   0076
         SLR   @03,@03                                             0076
         AL    @07,@CF00770                                        0076
         STH   @03,LTRTRANO-22236(@10,@07)                         0076
*   END;                            /*                       @YD00113*/
         AH    I,@CH00048                                          0077
         CH    I,@CH00044                                          0077
         BNH   @DL00075                                            0077
*   DO J=1 TO 4;                    /* 4 TABLES PAGES        @YD00113*/
         LA    J,1                                                 0078
@DL00078 DS    0H                                                  0079
*     DO I=0 TO 7;                  /* 8 CYLS PER PAGE       @YD00113*/
         SLR   I,I                                                 0079
@DL00079 DS    0H                                                  0080
*       LTRTRANO((J*8)+I)=(VPAGES(J)*8)+I-7;/*               @YD00113*/
         LR    @10,J                                               0080
         SLA   @10,3                                               0080
         ALR   @10,I                                               0080
         ALR   @10,@10                                             0080
         L     @07,BUILDPTR(,R4)                                   0080
         SLR   @15,@15                                             0080
         IC    @15,VPAGES-1(J,R4)                                  0080
         SLA   @15,3                                               0080
         ALR   @15,I                                               0080
         SH    @15,@CH00044                                        0080
         AL    @07,@CF00770                                        0080
         STH   @15,LTRTRANO-22236(@10,@07)                         0080
