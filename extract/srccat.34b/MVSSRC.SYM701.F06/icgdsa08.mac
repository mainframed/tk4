         TITLE 'ICGDSA08 MSC TABLE CREATE, CONFIGURATION VERIFICATION  *
                        '
ICGDSA08 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ICGDSA08  76.337'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
*                                                                  0024
*/********************************************************** @Y30LB15*/
*/*                                                          @Y30LB15*/
*/*   ICGDSA08 - MAINLINE                                    @Y30LB15*/
*/*                                                          @Y30LB15*/
*/* CALL ROUTINE TO CHECK DSM INTERFACES TO THE LIBRARIES    @Y30LB15*/
*/* AGAINST DSM INTERFACES TO DSS TO BE SURE NONE ARE        @Y30LB15*/
*/* THE SAME.                                                @Y30LB15*/
*/*                                                          @Y30LB15*/
*/********************************************************** @Y30LB15*/
*                                                                  0024
*   CALL LIBNOTDS;                  /*                       @Y30LB15*/
         BAL   @14,LIBNOTDS                                        0024
*/********************************************************** @Y30LB15*/
*/*                                                          @Y30LB15*/
*/* CALL ROUTINE TO INSURE THAT IF VIRTUAL ADDRESSES ARE     @Y30LB15*/
*/* DEFINED FOR A DATA STAGER/CHANNEL INTERFACE THAT AN      @Y30LB15*/
*/* INTERFACE IS DEFINED TO THE LIBRARY ALSO.                @Y30LB15*/
*/*                                                          @Y30LB15*/
*/********************************************************** @Y30LB15*/
*                                                                  0025
*   CALL LIBVIRT;                   /*                       @Y30LB15*/
         BAL   @14,LIBVIRT                                         0025
*/********************************************************** @Y30LB15*/
*/*                                                          @Y30LB15*/
*/* CALL ROUTINE TO CHECK SPINDLES, LOWERCON, AND UPPERCON   @Y30LB15*/
*/* STATEMENTS TO BE SURE DSS ARE DEFINED IN ALL NOT IN JUST @Y30LB15*/
*/* ONE OR TWO.                                              @Y30LB15*/
*/*                                                          @Y30LB15*/
*/********************************************************** @Y30LB15*/
*                                                                  0026
*   CALL DSDEFINE;                  /*                       @Y30LB15*/
         BAL   @14,DSDEFINE                                        0026
*   RETURN;                         /*                       @Y30LB15*/
@EL00001 L     @13,4(,@13)                                         0027
@EF00001 L     @00,@SIZDATD                                        0027
         LR    @01,@11                                             0027
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0027
         BR    @14                                                 0027
*                                                                  0028
*/********************************************************** @Y30LB15*/
*/*                                                          @Y30LB15*/
*/* PROCEDURE NAME - LIBNOTDS                                @Y30LB15*/
*/*                                                          @Y30LB15*/
*/* FUNCTION - FOR EACH OF THE FOUR LIBRARIES NOTE THE       @Y30LB15*/
*/*            DSM INTERFACES AND BE SURE THAT THE ADDRESSES @YD00113*/
*/*            ARE NOT USED FOR DS ADDRESSES AS WELL AS      @YD00113*/
*/*            LIBRARY ADDRESSES.  THE DSM CAN ADDRESS 16    @YD00113*/
*/*            DEVICES, 0 THRU F.  IF 2 DEVICES ARE USING    @YD00113*/
*/*            THE SAME INTERFACE, PRINT AN ERROR MESSAGE.   @YD00113*/
*/*                                                          @YD00113*/
*/*            THE FOLLOWING MESSAGE IS ISSUED BY THIS       @YD00113*/
*/*            ROUTINE:                                      @YD00113*/
*/*                                                          @YD00113*/
*/*            ICG326I IDENTIFICATION OF LOGICAL MSF X ALSO  @YD00113*/
*/*                    USED BY SA X.                         @YD00113*/
*/*                                                          @Y30LB15*/
*/********************************************************** @Y30LB15*/
*                                                                  0028
*LIBNOTDS:                                                         0028
*   PROCEDURE;                      /*                       @Y30LB15*/
LIBNOTDS STM   @14,@12,@SA00002                                    0028
*DOI:                                                              0029
*   DO I=1 TO 4;                    /*                       @Y30LB15*/
DOI      LA    I,1                                                 0029
@DL00029 DS    0H                                                  0030
*     IF LIBSPEC(I)='1'B            /*                       @Y30LB15*/
*       THEN                        /*                       @Y30LB15*/
         LR    @10,I                                               0030
         MH    @10,@CH00128                                        0030
         ST    @10,@TF00001                                        0030
         ALR   @10,R4                                              0030
         TM    LIBSPEC-12(@10),B'10000000'                         0030
         BNO   @RF00030                                            0030
*       DO;                         /*                       @Y30LB15*/
*                                                                  0031
*         /***********************************************************/
*         /*                                                         */
*         /* **********************************************  @YD00113*/
*         /* IF THE LOGICAL MSF (I) IS SPECIFIED (LIBSPEC    @Y30LB12*/
*         /* = '1'B), THEN CALCULATE THE INDICES OF THE 3    @Y30LB12*/
*         /* MATCHING UPPER INTERFACES (J, J+1, AND J+2) AND @Y30LB12*/
*         /* VERIFY THAT NONE OF THESE INTERFACES ARE USED.  @Y30LB12*/
*         /* **********************************************  @YD00113*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0032
*         J=(LIBDSMID(I)+1)*3-2;    /*                       @Y30LB15*/
         LA    J,1                                                 0032
         LR    @10,R4                                              0032
         AL    @10,@TF00001                                        0032
         MVC   @ZT00003+3(1),LIBDSMID-12(@10)                      0032
         AL    J,@ZT00003                                          0032
         MH    J,@CH00048                                          0032
         BCTR  J,0                                                 0032
         BCTR  J,0                                                 0032
*DOK:                                                              0033
*         DO K=J TO J+2;            /*                       @Y30LB15*/
DOK      LR    K,J                                                 0033
         B     @DE00033                                            0033
@DL00033 DS    0H                                                  0034
*           IF CISPEC(K)='1'B       /*                       @Y30LB15*/
*             THEN                  /*                       @Y30LB15*/
         LR    @10,K                                               0034
         MH    @10,@CH00112                                        0034
         L     @01,COMPTR2(,R4)                                    0034
         ALR   @01,@10                                             0034
         TM    CISPEC-9(@01),B'10000000'                           0034
         BNO   @RF00034                                            0034
*             DO;                   /*                       @Y30LB15*/
*               OUTBUF2(1:58)=A08MSG26;/*                    @Y30LB15*/
         MVC   OUTBUF2(58,R4),A08MSG26                             0036
*               DSCHAR=I-1;         /* LOGICAL MSF NUM.      @Y30LB12*/
         LR    @10,I                                               0037
         BCTR  @10,0                                               0037
         STC   @10,DSCHAR                                          0037
*               TR(DSCHAR,TRTABLE); /* TRANSLATE IT          @Y30LB12*/
         TR    DSCHAR(1),TRTABLE                                   0038
*               OUTBUF2(40)=DSCHAR; /* TO PRINT BUFFER       @Y30LB12*/
         MVC   OUTBUF2+39(1,R4),DSCHAR                             0039
*               DSCHAR=LIBDSMID(I); /*                       @Y30LB15*/
         LR    @10,I                                               0040
         MH    @10,@CH00128                                        0040
         ALR   @10,R4                                              0040
         MVC   DSCHAR(1),LIBDSMID-12(@10)                          0040
*               TR(DSCHAR,TRTABLE); /*                       @Y30LB15*/
         TR    DSCHAR(1),TRTABLE                                   0041
*               OUTBUF2(58)=DSCHAR; /*                       @Y30LB15*/
         MVC   OUTBUF2+57(1,R4),DSCHAR                             0042
*               CALL PUTIT1;        /*                       @Y30LB15*/
         BAL   @14,PUTIT1                                          0043
*               K=J+2;              /*                       @Y30LB15*/
         LA    K,2                                                 0044
         ALR   K,J                                                 0044
*             END;                  /*                       @Y30LB15*/
*           ELSE                                                   0046
*             ;                     /*                       @Y30LB15*/
@RF00034 DS    0H                                                  0047
*         END DOK;                  /*                       @Y30LB15*/
         AH    K,@CH00042                                          0047
@DE00033 LA    @10,2                                               0047
         ALR   @10,J                                               0047
         CR    K,@10                                               0047
         BNH   @DL00033                                            0047
*       END;                        /*                       @Y30LB15*/
*   END DOI;                        /*                       @Y30LB15*/
@RF00030 AH    I,@CH00042                                          0049
         CH    I,@CH00068                                          0049
         BNH   @DL00029                                            0049
*   END LIBNOTDS;                   /*                       @Y30LB15*/
@EL00002 DS    0H                                                  0050
@EF00002 DS    0H                                                  0050
@ER00002 LM    @14,@12,@SA00002                                    0050
         BR    @14                                                 0050
*                                                                  0051
*/********************************************************** @Y30LB15*/
*/*                                                          @Y30LB15*/
*/* PROCEDURE NAME - LIBVIRT                                 @Y30LB15*/
*/*                                                          @Y30LB15*/
*/* FUNCTION - THERE ARE 3 POSSIBLE UPPER INTERFACES TO THE  @Y30LB15*/
*/*            DS AND 64 POSSIBLE UCBS FOR EACH INTERFACE    @YD00113*/
*/*            IF ANY OF THE INTERFACES IS DEFINED AS        @YD00113*/
*/*            VIRTUAL, OR AS AN ALTERNATE PATH, THERE MUST  @YD00113*/
*/*            ALSO BE A PATH FROM THE SDG TO EACH LIBRARY.  @YD00143*/
*/*                                                          @YD00143*/
*/*            IF ONE OR MORE VIRTUAL ADDRESSES ARE          @YD00143*/
*/*            SPECIFIED THROUGH THE STAGING ADAPTER, EITHER @YD00143*/
*/*            'TORSG1' OR BOTH 'TORSG1' AND 'TORSG2' IN THE @YD00143*/
*/*            STRUCTURE 'DSTORSG' CONTAIN VALUES OTHER THAN @YD00143*/
*/*            'FF'X TO INDICATE THE ONE OR TWO STAGING      @YD00143*/
*/*            DRIVE GROUPS THAT ARE SPECIFIED THROUGH THE   @YD00143*/
*/*            STAGING ADAPTER.                              @YD00143*/
*/*                                                          @YD00143*/
*/*            USING FIRST 'TORSG1' AND THEN 'TORSG2'        @YD00143*/
*/*            (ASSUMING ONE OR BOTH ARE SPECIFIED), LOCATE  @YD00143*/
*/*            IN THE STRUCTURE 'BUILDRSG' THE ONE OR TWO    @YD00143*/
*/*            STAGING ADAPTER POINTERS ('TODS1' & 'TODS2')  @YD00143*/
*/*            FOR THE STAGING DRIVE GROUP.                  @YD00143*/
*/*                                                          @YD00143*/
*/*            THEN USING THE FLAG 'LIBSPEC' TO VERIFY THE   @YD00143*/
*/*            EXISTENCE OF THE MSF IN THE CONFIGURATION,    @YD00143*/
*/*            MAKE SURE THAT EITHER ONE OR THE OTHER OF     @YD00143*/
*/*            THE TWO POSSIBLE SA'S INTERFACES TO EACH OF   @YD00143*/
*/*            MSFS PRESENT IN THE CONFIGURATION.            @YD00143*/
*/*                                                          @YD00143*/
*/*            IF A PATH IS NOT FOUND, AN ERROR MESSAGE IS   @YD00143*/
*/*            ISSUED VIA PUTIT1.                            @YD00113*/
*/*                                                          @YD00113*/
*/*            THE FOLLOWING MESSAGE IS ISSUED BY THIS       @YD00113*/
*/*            ROUTINE:                                      @YD00113*/
*/*                                                          @YD00113*/
*/*            ICG327I NO MSFX PATH FOR SDG XX (SA=X)        @YD00143*/
*/*                                                          @Y30LB15*/
*/********************************************************** @Y30LB15*/
*                                                                  0051
*LIBVIRT:                                                          0051
*   PROCEDURE;                      /*                       @Y30LB15*/
LIBVIRT  STM   @14,@12,@SA00003                                    0051
*   DO I=1 TO MAXSA;                /* CHECK EACH SA         @YD00143*/
*                                                                  0052
         LA    I,1                                                 0052
@DL00052 DS    0H                                                  0053
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @YD00143*/
*     /* A VIRTUAL ADDRESS WAS SPECIFIED THROUGH ONE OF      @YD00143*/
*     /* CHANNEL INTERFACES (B, C, OR D) IF 'TORSG1'         @YD00143*/
*     /* CONTAINS ANYTHING OTHER THAN 'FF'X.                 @YD00143*/
*     /* ***********************************************     @YD00143*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0053
*     IF TORSG1(I)^='FF'X           /*                       @YD00143*/
*       THEN                        /*                       @YD00143*/
         LR    @10,I                                               0053
         ALR   @10,@10                                             0053
         L     @05,COMPTR2(,R4)                                    0053
         ST    @10,@TF00001                                        0053
         ALR   @10,@05                                             0053
         AL    @10,@CF00448                                        0053
         CLI   TORSG1(@10),X'FF'                                   0053
         BE    @RF00053                                            0053
*       DO;                         /*                       @YD00143*/
*                                                                  0054
*         /***********************************************************/
*         /*                                                         */
*         /* *****************************************       @YD00143*/
*         /* DEVELOP INDEX TO LOCATE TODS1 AND TODS2         @YD00143*/
*         /* IN THE STRUCTURE BUILDRSG.                      @YD00143*/
*         /* *****************************************       @YD00143*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0055
*         J=TORSG1(I)+1;            /*                       @YD00143*/
         LA    J,1                                                 0055
         AL    @05,@TF00001                                        0055
         AL    @05,@CF00448                                        0055
         MVC   @ZT00003+3(1),TORSG1(@05)                           0055
         AL    J,@ZT00003                                          0055
*         CALL LIBVIRT2;            /*                       @YD00143*/
         BAL   @14,LIBVIRT2                                        0056
*       END;                        /*                       @YD00143*/
*                                                                  0057
*     /***************************************************************/
*     /*                                                             */
*     /* ***********************************************     @YD00143*/
*     /* PERFORM SAME OPERATIONS FOR SECOND SDG              @YD00143*/
*     /* ***********************************************     @YD00143*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0058
*     IF TORSG2(I)^='FF'X           /*                       @YD00143*/
*       THEN                        /*                       @YD00143*/
@RF00053 LR    @10,I                                               0058
         ALR   @10,@10                                             0058
         L     @05,COMPTR2(,R4)                                    0058
         ST    @10,@TF00001                                        0058
         ALR   @10,@05                                             0058
         AL    @10,@CF00449                                        0058
         CLI   TORSG2-1(@10),X'FF'                                 0058
         BE    @RF00058                                            0058
*       DO;                         /*                       @YD00143*/
*                                                                  0059
*         /***********************************************************/
*         /*                                                         */
*         /* *****************************************       @YD00143*/
*         /* DEVELOP INDEX TO LOCATE TODS1 AND TODS2         @YD00143*/
*         /* IN THE STRUCTURE BUILDRSG.                      @YD00143*/
*         /* *****************************************       @YD00143*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0060
*         J=TORSG2(I)+1;            /*                       @YD00143*/
         LA    J,1                                                 0060
         AL    @05,@TF00001                                        0060
         AL    @05,@CF00449                                        0060
         MVC   @ZT00003+3(1),TORSG2-1(@05)                         0060
         AL    J,@ZT00003                                          0060
*         CALL LIBVIRT2;            /*                       @YD00143*/
         BAL   @14,LIBVIRT2                                        0061
*       END;                        /*                       @YD00143*/
*   END;                            /* END OF DO I           @YD00143*/
@RF00058 AH    I,@CH00042                                          0063
         CH    I,@CH00162                                          0063
         BNH   @DL00052                                            0063
*   END LIBVIRT;                    /*                       @YD00143*/
@EL00003 DS    0H                                                  0064
@EF00003 DS    0H                                                  0064
@ER00003 LM    @14,@12,@SA00003                                    0064
         BR    @14                                                 0064
*                                                                  0065
*/********************************************************** @YD00143*/
*/*                                                          @YD00143*/
*/* PROCEDURE NAME - LIBVIRT2                                @YD00143*/
*/*                                                          @YD00143*/
*/* FUNCTION - TODS1 AND TODS2 ARE FILLED IN BY THE          @YD00143*/
*/*            DASD CONTROL STATEMENT.  IF NO DASD           @YD00143*/
*/*            WAS SUPPLIED (CAUGHT IN OTHER ERROR           @YD00143*/
*/*            MESSAGES), USE THE CURRENT STAGING            @YD00143*/
*/*            ADAPTER INDEX IN PLACE OF THE MISSING         @YD00143*/
*/*            VALUES.  NOTE THAT IN ANY CASE TODS2          @YD00143*/
*/*            IS OPTIONAL AND MAY NOT BE PRESENT.           @YD00143*/
*/*                                                          @YD00143*/
*/********************************************************** @YD00143*/
*                                                                  0065
*LIBVIRT2:                                                         0065
*   PROCEDURE;                      /*                       @YD00143*/
LIBVIRT2 STM   @14,@12,@SA00004                                    0065
*   IF TODS1(J)^='FF'X              /*                       @YD00143*/
*     THEN                          /*                       @YD00143*/
         LR    @10,J                                               0066
         MH    @10,@CH00112                                        0066
         ST    @10,@TF00001                                        0066
         ALR   @10,R4                                              0066
         CLI   TODS1-9(@10),X'FF'                                  0066
         BE    @RF00066                                            0066
*     K=TODS1(J)+1;                 /* FIRST SA              @YD00143*/
         LA    K,1                                                 0067
         LR    @10,R4                                              0067
         AL    @10,@TF00001                                        0067
         MVC   @ZT00003+3(1),TODS1-9(@10)                          0067
         AL    K,@ZT00003                                          0067
*   ELSE                            /*                       @YD00143*/
*     K=I;                          /* CURRENT SA            @YD00143*/
         B     @RC00066                                            0068
@RF00066 LR    K,I                                                 0068
*   IF TODS2(J)^='FF'X              /*                       @YD00143*/
*     THEN                          /*                       @YD00143*/
@RC00066 LR    @10,J                                               0069
         MH    @10,@CH00112                                        0069
         ST    @10,@TF00001                                        0069
         ALR   @10,R4                                              0069
         CLI   TODS2-9(@10),X'FF'                                  0069
         BE    @RF00069                                            0069
*     L=TODS2(J)+1;                 /* SECOND SA             @YD00143*/
         LA    L,1                                                 0070
         LR    @10,R4                                              0070
         AL    @10,@TF00001                                        0070
         MVC   @ZT00003+3(1),TODS2-9(@10)                          0070
         AL    L,@ZT00003                                          0070
*   ELSE                            /*                       @YD00143*/
*     L=I;                          /* CURRENT SA            @YD00143*/
*                                                                  0071
         B     @RC00069                                            0071
@RF00069 LR    L,I                                                 0071
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00143*/
*   /* NOW READY TO CHECK WHETHER ONE OR BOTH                @YD00143*/
*   /* OF THE STAGING ADAPTERS HAS A PATH TO                 @YD00143*/
*   /* EACH SPECIFIED MSF.                                   @YD00143*/
*   /* ***********************************************       @YD00143*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0072
*   CALL LIBVIRT3;                  /*                       @YD00143*/
@RC00069 BAL   @14,LIBVIRT3                                        0072
*   END LIBVIRT2;                   /*                       @YD00143*/
@EL00004 DS    0H                                                  0073
@EF00004 DS    0H                                                  0073
@ER00004 LM    @14,@12,@SA00004                                    0073
         BR    @14                                                 0073
*                                                                  0074
*/********************************************************** @YD00143*/
*/*                                                          @YD00143*/
*/* PROCEDURE NAME - LIBVIRT3                                @YD00143*/
*/*                                                          @YD00143*/
*/* FUNCTION - DETERMINE IF EACH POSSIBLE LOGICAL MSF IS     @YD00143*/
*/*            PRESENT IN THE CONFIGURATION AND, IF THE MSF  @YD00143*/
*/*            IS PRESENT, SAVE ITS IDENTIFICATION IN        @YD00143*/
*/*            CHARACTER FORM FOR COMPARISON TO THE FIELD    @YD00143*/
*/*            'LOWLIB' AND CALL LIBVIRT4.                   @YD00143*/
*/*                                                          @YD00143*/
*/********************************************************** @YD00143*/
*                                                                  0074
*LIBVIRT3:                                                         0074
*   PROCEDURE;                      /*                       @YD00143*/
LIBVIRT3 STM   @14,@12,@SA00005                                    0074
*   IF LIBSPEC(1)='1'B              /* MSF0 PRESENT          @YD00143*/
*     THEN                          /*                       @YD00143*/
         TM    LIBSPEC(R4),B'10000000'                             0075
         BNO   @RF00075                                            0075
*     DO;                           /*                       @YD00143*/
*       COMPMSF='0';                /*                       @YD00143*/
         MVI   COMPMSF,C'0'                                        0077
*       CALL LIBVIRT4;              /*                       @YD00143*/
         BAL   @14,LIBVIRT4                                        0078
*     END;                          /*                       @YD00143*/
*   IF LIBSPEC(2)='1'B              /* MSF1 PRESENT          @YD00143*/
*     THEN                          /*                       @YD00143*/
@RF00075 TM    LIBSPEC+12(R4),B'10000000'                          0080
         BNO   @RF00080                                            0080
*     DO;                           /*                       @YD00143*/
*       COMPMSF='1';                /*                       @YD00143*/
         MVI   COMPMSF,C'1'                                        0082
*       CALL LIBVIRT4;              /*                       @YD00143*/
         BAL   @14,LIBVIRT4                                        0083
*     END;                          /*                       @YD00143*/
*   IF LIBSPEC(3)='1'B              /* MSF2 PRESENT          @YD00143*/
*     THEN                          /*                       @YD00143*/
@RF00080 TM    LIBSPEC+24(R4),B'10000000'                          0085
         BNO   @RF00085                                            0085
*     DO;                           /*                       @YD00143*/
*       COMPMSF='2';                /*                       @YD00143*/
         MVI   COMPMSF,C'2'                                        0087
*       CALL LIBVIRT4;              /*                       @YD00143*/
         BAL   @14,LIBVIRT4                                        0088
*     END;                          /*                       @YD00143*/
*   IF LIBSPEC(4)='1'B              /* MSF3 PRESENT          @YD00143*/
*     THEN                          /*                       @YD00143*/
@RF00085 TM    LIBSPEC+36(R4),B'10000000'                          0090
         BNO   @RF00090                                            0090
*     DO;                           /*                       @YD00143*/
*       COMPMSF='3';                /*                       @YD00143*/
         MVI   COMPMSF,C'3'                                        0092
*       CALL LIBVIRT4;              /*                       @YD00143*/
         BAL   @14,LIBVIRT4                                        0093
*     END;                          /*                       @YD00143*/
*   END LIBVIRT3;                   /*                       @YD00143*/
@EL00005 DS    0H                                                  0095
@EF00005 DS    0H                                                  0095
@ER00005 LM    @14,@12,@SA00005                                    0095
         BR    @14                                                 0095
*                                                                  0096
*/********************************************************** @YD00143*/
*/*                                                          @YD00143*/
*/* PROCEDURE NAME - LIBVIRT4                                @YD00143*/
*/*                                                          @YD00143*/
*/* FUNCTION - CHECK THE FIELD 'LIB' IN THE STRUCTURE        @YD00143*/
*/*            LOWERCON TO MAKE SURE THAT ONE OF THE STAGING @YD00143*/
*/*            ADAPTERS INTERFACES TO THE MSF INDICATED BY   @YD00143*/
*/*            'COMPMSF'.  IF NO MATCH, WRITE MESSAGE        @YD00143*/
*/*            ICG327I NO MSFX PATH FOR SDG XX (SA=X)        @YD00143*/
*/*                                                          @YD00143*/
*/********************************************************** @YD00143*/
*                                                                  0096
*LIBVIRT4:                                                         0096
*   PROCEDURE;                      /*                       @YD00143*/
LIBVIRT4 STM   @14,@12,@SA00006                                    0096
*   LWFNDSWT='0'B;                  /* RESET FOUND SW        @YD00143*/
*                                                                  0097
         NI    LWFNDSWT,B'01111111'                                0097
*   /*****************************************************************/
*   /*                                                               */
*   /* ***********************************************       @YD00143*/
*   /* 'I' IS THE CURRENT STAGING ADAPTER. 'K' IS            @YD00143*/
*   /* THE FIRST SA SPECIFIED IN THE DASD CONTROL            @YD00143*/
*   /* STATEMENT. 'L' IS THE (OPTIONAL) SECOND SA            @YD00143*/
*   /* SPECIFIED IN THE DASD STATEMENT. EITHER I=K           @YD00143*/
*   /* OR I=L. IT IS POSSIBLE THAT I=K=L.                    @YD00143*/
*   /* 'M' IS THE 'CUXX' SPECIFIED IN THE LOWERCON           @YD00143*/
*   /* STATEMENT.                                            @YD00143*/
*   /* ***********************************************       @YD00143*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0098
*   DO M=1 TO 4;                    /*                       @YD00143*/
         LA    M,1                                                 0098
@DL00098 DS    0H                                                  0099
*     IF LOWLIB(I,M)=COMPMSF        /*                       @YD00143*/
*         ×LOWLIB(K,M)=COMPMSF      /*                       @YD00143*/
*         ×LOWLIB(L,M)=COMPMSF      /*                       @YD00143*/
