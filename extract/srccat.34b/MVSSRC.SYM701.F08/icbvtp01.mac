*/*********************************************************  @G24LB18*/
*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - BLDDDENT                                @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS ROUTINE IS CALLED ON BUILD DD ENTRY      @G24LB18*/
*/*            REQUESTS. IT DETERMINES THE LENGTH OF THE     @G24LB18*/
*/*            ENTRY, GETS STORAGE FOR IT, FILLS IT IN WITH  @G24LB18*/
*/*            THE INFORMATION PASSED IN THE REQUEST, AND    @G24LB18*/
*/*            ADDS IT TO THE STEP TABLE FOR THE REQUESTED   @G24LB18*/
*/*            STEP, IF ONE EXISTS. IF NOT, IT BUILDS A NEW  @G24LB18*/
*/*            STEP TABLE AND ADDS THE ENTRY TO IT.          @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - TABLE PROCESSOR REQUEST PARAMETER LIST, SELECT   @G24LB18*/
*/*         REQUEST BLOCK                                    @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - A NEW DD ENTRY IS BUILT AND ADDED TO THE STEP   @G24LB18*/
*/*          TABLE CHAIN FOR THE REQUEST. A RETURN CODE OF   @G24LB18*/
*/*          0 IS RETURNED IF THE ENTRY IS BUILT SUCCESS-    @G24LB18*/
*/*          FULLY. A RETURN CODE OF 4 IS RETURNED IF A      @G24LB18*/
*/*          GETMAIN FAILURE OCCURRED AND CONSOLE ERROR      @G24LB18*/
*/*          MESSAGE ICB409I 'NO STORAGE AVAILABLE' IS       @G24LB18*/
*/*          ISSUED.                                         @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0089
*BLDDDENT:                          /*                       @G24LB18*/
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
BLDDDENT DS    0H                                                  0090
*   JFCBPTR=SELJFCBA;               /* SET UP JFCB PTR       @G24LB18*/
*                                                                  0090
         L     @15,TABSELRB(,PARMPTR)                              0090
         L     @15,SELJFCBA(,@15)                                  0090
         ST    @15,JFCBPTR                                         0090
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE THE NUMBER OF VOL SERS FOR THE REQUEST.     @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0091
*   IF JFCBNVOL^=ZERO THEN          /* SPECIFIC DD?          @G24LB18*/
         CLI   JFCBNVOL(@15),0                                     0091
         BE    @RF00091                                            0091
*     NVOL=JFCBNVOL;                /* YES-NO. OF VOLS =     @G24LB18*/
         SLR   NVOL,NVOL                                           0092
         IC    NVOL,JFCBNVOL(,@15)                                 0092
*   ELSE                            /* JFCBNVOL              @G24LB18*/
*     IF SELPARMT=ON THEN           /* PARALLEL MOUNT?       @G24LB18*/
         B     @RC00091                                            0093
@RF00091 L     @15,TABSELRB(,PARMPTR)                              0093
         TM    SELPARMT(@15),B'10000000'                           0093
         BNO   @RF00093                                            0093
*       NVOL=JFCBVLCT;              /* YES-NO. OF VOLS =     @G24LB18*/
         L     @15,JFCBPTR                                         0094
         SLR   NVOL,NVOL                                           0094
         IC    NVOL,JFCBVLCT(,@15)                                 0094
*     ELSE                          /* JFCBVLCT              @G24LB18*/
*       NVOL=SELUNCNT;              /* NO-NO. OF VOLS = UNIT @G24LB18
*                                      COUNT                 @G24LB18*/
*                                                                  0095
         B     @RC00093                                            0095
@RF00093 L     @15,TABSELRB(,PARMPTR)                              0095
         SLR   NVOL,NVOL                                           0095
         IC    NVOL,SELUNCNT(,@15)                                 0095
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE LENGTH OF DD ENTRY AND ISSUE GETMAIN FOR    @G24LB18*/
*   /* IT FROM SUBPOOL 241. IF GETMAIN FAILS, ISSUE CONSOLE  @G24LB18*/
*   /* ERROR MESSAGE ICB409I 'NO STORAGE AVAILABLE', SET     @G24LB18*/
*   /* RETURN CODE = 4, AND RETURN TO MAINLINE ICBVTP01. IF  @G24LB18*/
*   /* GETMAIN IS SUCCESSFUL, CLEAR THE GOTTEN AREA AND CALL @G24LB18*/
*   /* SUBPROCEDURE FILDDENT TO FILL IN THE DD ENTRY.        @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0096
*   RFY                                                            0096
*     REG0 RSTD;                    /* RESTRICT REG 0        @G24LB18*/
@RC00093 DS    0H                                                  0096
@RC00091 DS    0H                                                  0097
*   RFY                                                            0097
*     REG1 RSTD;                    /* RESTRICT REG 1        @G24LB18*/
*   RFY                                                            0098
*     REG15 RSTD;                   /* RESTRICT REG 15       @G24LB18*/
*   GETLEN=NVOL*SIX+LENGTH(STPDDENT);/* SET ENTRY LENGTH     @G24LB18*/
         LR    @08,NVOL                                            0099
         MH    @08,@CH00061                                        0099
         AL    @08,@CF01544                                        0099
         ST    @08,GETLEN                                          0099
*   REG0=GETLEN;                    /* SET LEN FOR GETMAIN   @G24LB18*/
         LR    REG0,@08                                            0100
*LGET1:                                                            0101
*   GEN(GETMAIN RC,LV=(0),SP=241);  /* GETMAIN DD ENTRY      @G24LB18*/
LGET1    GETMAIN RC,LV=(0),SP=241
*   IF REG15^=ZERO THEN             /* STORAGE GOTTEN?       @G24LB18*/
         LTR   REG15,REG15                                         0102
         BZ    @RF00102                                            0102
*     DO;                           /* NO                    @G24LB18*/
*       REG1=ADDR(MSG1);            /* POINT TO ERROR MSG    @G24LB18*/
         LA    REG1,MSG1                                           0104
*LWTO1:                                                            0105
*       GEN(WTO   MF=(E,(REG1)));   /* SEND ERROR MESSAGE    @G24LB18*/
LWTO1    WTO   MF=(E,(REG1))
*       RETCODE=FOUR;               /* SET RETURN CODE = 4   @G24LB18*/
         LA    RETCODE,4                                           0106
*       RETURN;                     /* RETURN TO ICBVTP01    @G24LB18*/
@EL00002 DS    0H                                                  0107
@EF00002 DS    0H                                                  0107
@ER00002 BR    @14                                                 0107
*     END;                          /*                       @G24LB18*/
*   SAVERET1=REG14;                 /* SAVE RETURN ADDRESS   @G24LB18*/
@RF00102 ST    REG14,SAVERET1                                      0109
*   DDENTPTR=REG1;                  /* SET UP DD ENTRY PTR   @G24LB18*/
         LR    DDENTPTR,REG1                                       0110
*   STPDDENT=STPDDENT&&STPDDENT;    /* CLEAR DD ENTRY        @G24LB18*/
         XC    STPDDENT(26,DDENTPTR),STPDDENT(DDENTPTR)            0111
*   CALL FILDDENT;                  /* CALL FILL-IN RTN      @G24LB18*/
*                                                                  0112
         BAL   @14,FILDDENT                                        0112
*   /*****************************************************************/
*   /*                                                               */
*   /* SET THE LOCAL AND CMS LOCKS AND CALL SUBPROCEDURE     @G24LB18*/
*   /* SRCJOBID TO SEARCH FOR THE STEP TABLE FOR THE         @G24LB18*/
*   /* REQUEST. IF RETURN CODE = 4 (STEP TABLE NOT FOUND),   @G24LB18*/
*   /* CALL SUBPROCEDURE BLDSTHDR TO BUILD A STEP TABLE      @G24LB18*/
*   /* HEADER FOR THE REQUESTED STEP AND TO ADD THE DD ENTRY @G24LB18*/
*   /* TO IT, AND RETURN TO MAINLINE ICBVTP01.               @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0113
*   GENERATE;                                                      0113
*                                                          /*@G24LB18*/
LGETLCL2 SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,               *
               RELATED=(ICBVTP01(LFRELCL1,LFRELCL2))
LGETCMS2 SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,                 *
               RELATED=(ICBVTP01(LFRECMS1,LFRECMS2))
*   JOBID=SELJOBID;                 /* GET JOBID             @G24LB18*/
         L     @07,TABSELRB(,PARMPTR)                              0114
         L     JOBID,SELJOBID(,@07)                                0114
*   CALL SRCJOBID;                  /* CALL SEARCH RTN       @G24LB18*/
         BAL   @14,SRCJOBID                                        0115
*   IF RETCODE=FOUR THEN            /* STEP TABLE FOUND?     @G24LB18*/
         C     RETCODE,@CF00057                                    0116
         BNE   @RF00116                                            0116
*     DO;                           /* NO                    @G24LB18*/
*       CALL BLDSTHDR;              /* CALL BUILD HDR RTN    @G24LB18*/
         BAL   @14,BLDSTHDR                                        0118
*       REG14=SAVERET1;             /* RESTORE RETURN ADDR   @G24LB18*/
         L     REG14,SAVERET1                                      0119
*       RETURN;                     /* RETURN TO ICBVTP01    @G24LB18*/
         B     @EL00002                                            0120
*     END;                          /*                       @G24LB18*/
*                                                                  0122
*   /*****************************************************************/
*   /*                                                               */
*   /* IF STEP TABLE WAS FOUND, TEST FOR INITIAL CALL FROM   @G24LB18*/
*   /* ALLOCATION. IF SO, INCREMENT THE INITIAL CALL COUNTER @G24LB18*/
*   /* BY 1. SEARCH FOR THE LAST DD ENTRY IN THE STEP TABLE, @G24LB18*/
*   /* CHAIN THE NEW DD ENTRY TO IT, AND RETURN TO MAINLINE  @G24LB18*/
*   /* ICBVTP01.                                             @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0122
*   IF SELINITC=ON THEN             /* INITIAL CALL?         @G24LB18*/
@RF00116 L     @07,TABSELRB(,PARMPTR)                              0122
         TM    SELINITC(@07),B'00000100'                           0122
         BNO   @RF00122                                            0122
*     STPICCTR=STPICCTR+ONE;        /* YES-INCREMENT COUNTER @G24LB18*/
         LA    @07,1                                               0123
         SLR   @09,@09                                             0123
         IC    @09,STPICCTR(,STHDRPTR)                             0123
         ALR   @07,@09                                             0123
         STC   @07,STPICCTR(,STHDRPTR)                             0123
*   SAVENTRY=DDENTPTR;              /* SAVE NEW DD ENTRY PTR @G24LB18*/
@RF00122 LR    SAVENTRY,DDENTPTR                                   0124
*   DDENTPTR=STPDDPTR;              /* GET 1ST DD ENTRY      @G24LB18*/
         L     DDENTPTR,STPDDPTR(,STHDRPTR)                        0125
*   DO WHILE STPDDNXT^=ZERO;        /* LAST DD ENTRY?        @G24LB18*/
         B     @DE00126                                            0126
@DL00126 DS    0H                                                  0127
*     DDENTPTR=STPDDNXT;            /* NO - GET NEXT ENTRY   @G24LB18*/
         L     DDENTPTR,STPDDNXT(,DDENTPTR)                        0127
*   END;                            /* YES - CONTINUE        @G24LB18*/
@DE00126 L     @08,STPDDNXT(,DDENTPTR)                             0128
         LTR   @08,@08                                             0128
         BNZ   @DL00126                                            0128
*   STPDDNXT=SAVENTRY;              /* SET NEXT DD PTR OF LAST ENTRY
*                                      = NEW DD ENTRY ADDR   @G24LB18*/
         ST    SAVENTRY,STPDDNXT(,DDENTPTR)                        0129
*   REG14=SAVERET1;                 /* RESTORE RETURN ADDR   @G24LB18*/
         L     REG14,SAVERET1                                      0130
*   RFY                                                            0131
*     REG0 UNRSTD;                  /* UNRESTRICT REG 0      @G24LB18*/
*   RFY                                                            0132
*     REG1 UNRSTD;                  /* UNRESTRICT REG 1      @G24LB18*/
*   RFY                                                            0133
*     REG15 UNRSTD;                 /* UNRESTRICT REG 15     @G24LB18*/
*   RETURN;                         /* RETURN TO ICBVTP01    @G24LB18*/
         B     @EL00002                                            0134
*   END BLDDDENT;                   /* END BUILD DD ENT RTN  @G24LB18*/
*                                                                  0136
*/*********************************************************  @G24LB18*/
*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - FILDDENT                                @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS ROUTINE IS CALLED ON BUILD DD ENTRY      @G24LB18*/
*/*            REQUESTS WHEN STORAGE FOR THE ENTRY HAS BEEN  @G24LB18*/
*/*            GOTTEN. IT FILLS IN THE ENTRY WITH THE        @G24LB18*/
*/*            INFORMATION PASSED IN THE SELECT REQUEST      @G24LB18*/
*/*            BLOCK AND THE JFCB. IF THERE ARE JFCB         @G24LB18*/
*/*            EXTENSIONS, IT CALLS SUBPROCEDURE MOVEEXT     @G24LB18*/
*/*            TO FILL IN THE VOL SERS FROM THE EXTENSIONS.  @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - SELECT REQUEST BLOCK, JFCB, NEW DD ENTRY,        @G24LB18*/
*/*          LENGTH OF DD ENTRY, AND NO. OF VOL SERS.        @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - COMPLETED DD ENTRY                              @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0136
*FILDDENT:                                                         0136
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
*                                                                  0136
FILDDENT DS    0H                                                  0137
*   /*****************************************************************/
*   /*                                                               */
*   /* STORE THE LENGTH OF THE ENTRY AND THE NO. OF VOL SERS @G24LB18*/
*   /* REQUESTED INTO THE DD ENTRY. COPY THE DD # FROM THE   @G24LB18*/
*   /* SELECT REQUEST BLOCK INTO THE ENTRY.                  @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0137
*   STPDDLEN=GETLEN;                /* STORE LENGTH INTO ENT @G24LB18*/
         L     @15,GETLEN                                          0137
         STH   @15,STPDDLEN(,DDENTPTR)                             0137
*   STPDDNVL=NVOL;                  /* STORE NO. OF VOLS     @G24LB18*/
         STC   NVOL,STPDDNVL(,DDENTPTR)                            0138
*   STPDDNUM=SELDDNUM;              /* COPY DD #             @G24LB18*/
*                                                                  0139
         L     @15,TABSELRB(,PARMPTR)                              0139
         LH    @07,SELDDNUM(,@15)                                  0139
         STH   @07,STPDDNUM(,DDENTPTR)                             0139
*   /*****************************************************************/
*   /*                                                               */
*   /* IF A SPECIFIC UNIT ADDRESS WAS REQUESTED, COPY THE    @G24LB18*/
*   /* UCB ADDRESS FROM THE SELECT REQUEST BLOCK INTO THE    @G24LB18*/
*   /* INTERNAL DEVICE TYPE FIELD OF THE DD ENTRY. OTHERWISE @G24LB18*/
*   /* COPY THE INTERNAL DEVICE TYPE FROM THE SELECT REQUEST @G24LB18*/
*   /* BLOCK INTO THE DD ENTRY.                              @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0140
*   IF SELUCBAD^=ZERO THEN          /* SPECIFIC UNIT ADDR?   @G24LB18*/
         L     @15,SELUCBAD(,@15)                                  0140
         LTR   @15,@15                                             0140
         BZ    @RF00140                                            0140
*     STPDDDVT=SELUCBAD;            /* YES-COPY UCB ADDR     @G24LB18*/
         ST    @15,STPDDDVT(,DDENTPTR)                             0141
*   ELSE                            /* INTO INT DEV TYPE     @G24LB18*/
*     STPDDDVT=SELDEVTP;            /* NO-COPY INT DEV TYPE  @G24LB18*/
         B     @RC00140                                            0142
@RF00140 L     @15,TABSELRB(,PARMPTR)                              0142
         MVC   STPDDDVT(4,DDENTPTR),SELDEVTP(@15)                  0142
*                                                                  0143
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE REQUEST IS FOR DEFERRED MOUNT OR UNIT=AFF, OR  @G24LB18*/
*   /* IF VOLUME COUNT > UNIT COUNT AND PARALLEL MOUNT IS    @G24LB18*/
*   /* NOT REQUESTED, TURN ON THE EXCLUSIVE USE INDICATOR IN @G24LB18*/
*   /* THE DD ENTRY.                                         @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0143
*   IF JFCBVLCT=ZERO THEN           /* ANY NON-SPECIFIC VOL? @G24LB18*/
@RC00140 L     @15,JFCBPTR                                         0143
         CLI   JFCBVLCT(@15),0                                     0143
         BNE   @RF00143                                            0143
*     NVOL=JFCBNVOL;                /* NO-VOL CNT= JFCBNVOL  @G24LB18*/
         SLR   NVOL,NVOL                                           0144
         IC    NVOL,JFCBNVOL(,@15)                                 0144
*   ELSE                            /* YES -                 @G24LB18*/
*     NVOL=JFCBVLCT;                /* VOL COUNT = JFCBVLCT  @G24LB18*/
         B     @RC00143                                            0145
@RF00143 L     @15,JFCBPTR                                         0145
         SLR   NVOL,NVOL                                           0145
         IC    NVOL,JFCBVLCT(,@15)                                 0145
*   IF SELDEFER=ON×                 /* DEFERRED MOUNT OR     @G24LB18*/
*       JFCBUAFF=ON×                /* UNIT=AFF OR           @G24LB18*/
*       (NVOL>SELUNCNT&             /* VOL CNT > UNIT CNT &  @G24LB18*/
*       SELPARMT=OFF) THEN          /* NO PARALLEL MOUNT?    @G24LB18*/
@RC00143 L     @15,TABSELRB(,PARMPTR)                              0146
         TM    SELDEFER(@15),B'01000000'                           0146
         BO    @RT00146                                            0146
         L     @08,JFCBPTR                                         0146
         TM    JFCBUAFF(@08),B'00000001'                           0146
         BO    @RT00146                                            0146
         SLR   @08,@08                                             0146
         IC    @08,SELUNCNT(,@15)                                  0146
         CR    NVOL,@08                                            0146
         BNH   @RF00146                                            0146
         TM    SELPARMT(@15),B'10000000'                           0146
         BNZ   @RF00146                                            0146
@RT00146 DS    0H                                                  0147
*     STPDDEXC=ON;                  /* YES-SET EXCLUSIVE IND @G24LB18*/
*                                                                  0147
         OI    STPDDEXC(DDENTPTR),B'10000000'                      0147
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE THE NUMBER OF VOL SERS IN THE JFCB AND MOVE @G24LB18*/
*   /* THEM INTO THE DD ENTRY. IF THERE ARE JFCB EXTENSIONS, @G24LB18*/
*   /* CALL SUBPROCEDURE MOVEEXT TO MOVE THE VOL SERS FROM   @G24LB18*/
*   /* THE EXTENSIONS INTO THE DD ENTRY. RETURN TO BLDDDENT. @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0148
*   IF STPDDNVL>FIVE THEN           /* NO. OF VOLS > 5?      @G24LB18*/
@RF00146 CLI   STPDDNVL(DDENTPTR),5                                0148
         BNH   @RF00148                                            0148
*     NVOL=FIVE;                    /* YES-SET NO. VOLS = 5  @G24LB18*/
         LA    NVOL,5                                              0149
*   ELSE                            /* NO -                  @G24LB18*/
*     NVOL=STPDDNVL;                /* SET NO. VOLS FROM ENT @G24LB18*/
         B     @RC00148                                            0150
@RF00148 SLR   NVOL,NVOL                                           0150
         IC    NVOL,STPDDNVL(,DDENTPTR)                            0150
*   RFY                                                            0151
*     REG1 RSTD;                    /* RESTRICT REG 1        @G24LB18*/
@RC00148 DS    0H                                                  0152
*   REG1=NVOL*SIX;                  /* GET LENGTH OF VOLSERS @G24LB18*/
         LR    REG1,NVOL                                           0152
         MH    REG1,@CH00061                                       0152
*   STPDDVOL(ONE)=JFCBVOLS(1:REG1); /* COPY VOLSERS INTO ENT @G24LB18*/
         MVI   STPDDVOL+1(DDENTPTR),C' '                           0153
         MVC   STPDDVOL+2(4,DDENTPTR),STPDDVOL+1(DDENTPTR)         0153
         LR    @15,REG1                                            0153
         BCTR  @15,0                                               0153
         L     @07,JFCBPTR                                         0153
         EX    @15,@SM01548                                        0153
*   RFY                                                            0154
*     REG1 UNRSTD;                  /* UNRESTRICT REG 1      @G24LB18*/
*   IF SELJFCBX^=ZERO THEN          /* ANY JFCB EXTENSIONS?  @G24LB18*/
         L     @15,TABSELRB(,PARMPTR)                              0155
         L     @15,SELJFCBX(,@15)                                  0155
         LTR   @15,@15                                             0155
         BZ    @RF00155                                            0155
*     DO;                           /* YES                   @G24LB18*/
*       SAVERET2=REG14;             /* SAVE RETURN ADDRESS   @G24LB18*/
         ST    REG14,SAVERET2                                      0157
*       CALL MOVEEXT;               /* CALL MOVE VOLSERS RTN @G24LB18*/
         BAL   @14,MOVEEXT                                         0158
*       REG14=SAVERET2;             /* RESTORE RETURN ADDR   @G24LB18*/
         L     REG14,SAVERET2                                      0159
*     END;                          /*                       @G24LB18*/
*   RETURN;                         /* RETURN TO BLDDDENT    @G24LB18*/
@EL00003 DS    0H                                                  0161
@EF00003 DS    0H                                                  0161
@ER00003 BR    @14                                                 0161
*   END FILDDENT;                   /* END FILL DD ENT RTN   @G24LB18*/
         B     @EL00003                                            0162
*                                                                  0163
*/*********************************************************  @G24LB18*/
*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - MOVEEXT                                 @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS ROUTINE IS CALLED ON BUILD DD ENTRY      @G24LB18*/
*/*            REQUESTS WITH JFCB EXTENSIONS. IT MOVES THE   @G24LB18*/
*/*            VOL SERS FROM THE EXTENSIONS INTO THE DD      @G24LB18*/
*/*            ENTRY.                                        @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - SELECT REQUEST BLOCK, JFCB, JFCB EXTENSIONS,     @G24LB18*/
*/*         AND DD ENTRY.                                    @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - DD ENTRY CONTAINING VOL SERS FROM THE JFCB      @G24LB18*/
*/*          EXTENSIONS.                                     @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0163
*MOVEEXT:                                                          0163
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
*                                                                  0163
MOVEEXT  DS    0H                                                  0164
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE JFCB EXTENSION POINTER, THE DD ENTRY   @G24LB18*/
*   /* VOL SER INDEX, AND THE TOTAL NO. OF VOL SERS.         @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0164
*   JFCBXPTR=SELJFCBX;              /* SET JFCBX PTR         @G24LB18*/
         L     @15,TABSELRB(,PARMPTR)                              0164
         L     JFCBXPTR,SELJFCBX(,@15)                             0164
*   INDEX=SIX;                      /* INIT DD ENTRY INDEX   @G24LB18*/
         MVI   INDEX,X'06'                                         0165
*   NVOL=JFCBNVOL-FIVE;             /* SET NO. OF VOL SERS   @G24LB18*/
         L     @15,JFCBPTR                                         0166
         SLR   NVOL,NVOL                                           0166
         IC    NVOL,JFCBNVOL(,@15)                                 0166
         SL    NVOL,@CF00059                                       0166
*   RFY                                                            0167
*     REG1 RSTD;                    /* RESTRICT REG 1        @G24LB18*/
*                                                                  0167
*   /*****************************************************************/
*   /*                                                               */
*   /* COPY THE VOL SERS FROM EACH JFCB EXTENSION INTO THE   @G24LB18*/
*   /* DD ENTRY AND RETURN TO FILDDENT.                      @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0168
*   DO WHILE JFCBXNXT^=ZERO;        /* DO FOR EACH EXTENSION @G24LB18*/
         B     @DE00168                                            0168
@DL00168 DS    0H                                                  0169
*     STPDDVOL(INDEX,1:LENGTH(VOLJFCBX))=VOLJFCBX;/* COPY VOLSERS  0169
*                                      FROM EXTENSION        @G24LB18*/
         SLR   @15,@15                                             0169
         IC    @15,INDEX                                           0169
         LR    @08,@15                                             0169
         MH    @08,@CH00061                                        0169
         ALR   @08,DDENTPTR                                        0169
         MVC   STPDDVOL-6(90,@08),VOLJFCBX(JFCBXPTR)               0169
*     JFCBXPTR=JFCBXNXT;            /* GET NEXT JFCB EXTN    @G24LB18*/
         L     JFCBXPTR,JFCBXNXT(,JFCBXPTR)                        0170
*     INDEX=INDEX+FIFTEEN;          /* INCREMENT DD ENT INDX @G24LB18*/
         LA    @08,15                                              0171
         ALR   @15,@08                                             0171
         STC   @15,INDEX                                           0171
*     NVOL=NVOL-FIFTEEN;            /* DECREMENT NO. OF VOLS @G24LB18*/
         SLR   NVOL,@08                                            0172
*   END;                            /*                       @G24LB18*/
@DE00168 L     @15,JFCBXNXT(,JFCBXPTR)                             0173
         LTR   @15,@15                                             0173
         BNZ   @DL00168                                            0173
*   REG1=NVOL*SIX;                  /* GET LENGTH OF VOLSERS @G24LB18*/
         LA    @15,6                                               0174
         LR    REG1,NVOL                                           0174
         MR    @00,@15                                             0174
*   STPDDVOL(INDEX)=JFCBXVOL(1,1:REG1);/* COPY VOLSERS FROM  @G24LB18
*                                      LAST EXTN             @G24LB18*/
         LR    @03,@15                                             0175
         SLR   @15,@15                                             0175
         IC    @15,INDEX                                           0175
         MR    @02,@15                                             0175
         ALR   @03,DDENTPTR                                        0175
         MVI   STPDDVOL-5(@03),C' '                                0175
         MVC   STPDDVOL-4(4,@03),STPDDVOL-5(@03)                   0175
         LR    @15,REG1                                            0175
         BCTR  @15,0                                               0175
         EX    @15,@SM01550                                        0175
*   RFY                                                            0176
*     REG1 UNRSTD;                  /* UNRESTRICT REG 1      @G24LB18*/
*   RETURN;                         /* RETURN TO FILDDENT    @G24LB18*/
@EL00004 DS    0H                                                  0177
@EF00004 DS    0H                                                  0177
@ER00004 BR    @14                                                 0177
*   END MOVEEXT;                    /* END MOVE VOLSERS RTN  @G24LB18*/
*                                                                  0179
*/*********************************************************  @G24LB18*/
*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - BLDSTHDR                                @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS ROUTINE IS CALLED ON BUILD DD ENTRY      @G24LB18*/
*/*            REQUESTS FOR WHICH A STEP TABLE DOES NOT      @G24LB18*/
*/*            ALREADY EXIST. IT GETS STORAGE FOR A STEP     @G24LB18*/
*/*            TABLE HEADER, FILLS IN THE HEADER, CHAINS     @G24LB18*/
*/*            THE NEW DD ENTRY TO THE HEADER, AND CHAINS    @G24LB18*/
*/*            THE HEADER TO THE END OF THE STEP TABLE       @G24LB18*/
*/*            CHAIN.                                        @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - SELECT REQUEST BLOCK, VVICB, AND NEW DD ENTRY.   @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - A NEW STEP TABLE HEADER WITH THE NEW DD ENTRY   @G24LB18*/
*/*          CHAINED TO IT IS BUILT AND ADDED TO THE STEP    @G24LB18*/
*/*          TABLE CHAIN. A RETURN CODE OF 0 IS RETURNED IF  @G24LB18*/
*/*          THE HEADER IS BUILT SUCCESSFULLY. IF A GETMAIN  @G24LB18*/
*/*          FAILURE OCCURS, CONSOLE ERROR MESSAGE ICB409I   @G24LB18*/
*/*          'NO STORAGE AVAILABLE' IS ISSUED, THE NEW DD    @G24LB18*/
*/*          ENTRY IS FREEMAINED, AND A RETURN CODE = 4 IS   @G24LB18*/
*/*          RETURNED.                                       @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0179
*BLDSTHDR:                                                         0179
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
*                                                                  0179
BLDSTHDR DS    0H                                                  0180
*   /*****************************************************************/
*   /*                                                               */
*   /* RELEASE THE CMS AND LOCAL LOCKS AND ISSUE A GETMAIN   @G24LB18*/
*   /* FOR THE STEP TABLE HEADER.                            @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0180
*   SAVERET2=REG14;                 /* SAVE RETURN ADDR      @G24LB18*/
         ST    REG14,SAVERET2                                      0180
*   GENERATE;                                                      0181
*                                                          /*@G24LB18*/
LFRECMS2 SETLOCK RELEASE,TYPE=CMS,REGS=USE,                            *
               RELATED=(ICBVTP01(LGETCMS2))
LFRELCL2 SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,                          *
               RELATED=(ICBVTP01(LGETLCL2))
*   RFY                                                            0182
*     REG0 RSTD;                    /* RESTRICT REG 0        @G24LB18*/
*   RFY                                                            0183
*     REG1 RSTD;                    /* RESTRICT REG 1        @G24LB18*/
*   RFY                                                            0184
*     REG15 RSTD;                   /* RESTRICT REG 15       @G24LB18*/
*   RETCODE=ZERO;                   /* ZERO RETURN CODE      @G24LB18*/
         SLR   RETCODE,RETCODE                                     0185
*   REG0=LENGTH(STPTB);             /* SET LENGTH OF HDR     @G24LB18*/
         LA    REG0,20                                             0186
*LGET2:                                                            0187
*   GEN(GETMAIN RC,LV=(0),SP=241);  /* GETMAIN HEADER        @G24LB18*/
LGET2    GETMAIN RC,LV=(0),SP=241
*                                                                  0188
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE GETMAIN FAILS, ISSUE CONSOLE ERROR MESSAGE     @G24LB18*/
*   /* ICB409I 'NO STORAGE AVAILABLE', FREEMAIN THE NEW DD   @G24LB18*/
*   /* ENTRY, SET RETURN CODE = 4, AND RETURN TO BLDDDENT.   @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0188
*   IF REG15^=ZERO THEN             /* STORAGE GOTTEN?       @G24LB18*/
         LTR   REG15,REG15                                         0188
         BZ    @RF00188                                            0188
*     DO;                           /* NO                    @G24LB18*/
*       REG1=ADDR(MSG1);            /* POINT TO ERROR MSG    @G24LB18*/
         LA    REG1,MSG1                                           0190
*LWTO2:                                                            0191
*       GEN(WTO   MF=(E,(REG1)));   /* ISSUE ERROR MESSAGE   @G24LB18*/
LWTO2    WTO   MF=(E,(REG1))
*       REG0=STPDDLEN;              /* GET LENGTH OF DD ENT  @G24LB18*/
         LH    REG0,STPDDLEN(,DDENTPTR)                            0192
*       REG1=DDENTPTR;              /* GET ADDR OF DD ENTRY  @G24LB18*/
         LR    REG1,DDENTPTR                                       0193
*LFREM1:                                                           0194
*       GEN(FREEMAIN RC,LV=(0),A=(1),SP=241);/* FREEMAIN ENT @G24LB18*/
LFREM1   FREEMAIN RC,LV=(0),A=(1),SP=241
*       REG14=SAVERET2;             /* RESTORE RET ADDR      @G24LB18*/
         L     REG14,SAVERET2                                      0195
*       RETCODE=FOUR;               /* SET RETURN CODE = 4   @G24LB18*/
         LA    RETCODE,4                                           0196
*       RETURN;                     /* RETURN TO BLDDDENT    @G24LB18*/
@EL00005 DS    0H                                                  0197
@EF00005 DS    0H                                                  0197
@ER00005 BR    @14                                                 0197
*     END;                          /*                       @G24LB18*/
*                                                                  0198
*   /*****************************************************************/
*   /*                                                               */
*   /* CLEAR THE STEP TABLE HEADER AND COPY THE JOBID FROM   @G24LB18*/
*   /* THE SELECT REQUEST BLOCK INTO IT. STORE THE LENGTH OF @G24LB18*/
*   /* THE HEADER AND THE ADDRESS OF THE NEW DD ENTRY INTO   @G24LB18*/
*   /* THE HEADER. INITIALIZE THE INITIAL CALL COUNTER TO 1. @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0199
*   STHDRPTR=REG1;                  /* SET UP HEADER PTR     @G24LB18*/
@RF00188 LR    STHDRPTR,REG1                                       0199
*   STPTB=STPTB&&STPTB;             /* CLEAR HEADER          @G24LB18*/
         XC    STPTB(20,STHDRPTR),STPTB(STHDRPTR)                  0200
*   STPJOBID=SELJOBID;              /* COPY JOBID INTO HDR   @G24LB18*/
         L     @08,TABSELRB(,PARMPTR)                              0201
         L     @08,SELJOBID(,@08)                                  0201
         ST    @08,STPJOBID(,STHDRPTR)                             0201
*   STPHDRLN=LENGTH(STPTB);         /* STORE LENGTH OF HDR   @G24LB18*/
         MVC   STPHDRLN(2,STHDRPTR),@CH01481                       0202
*   STPDDPTR=DDENTPTR;              /* CHAIN DD ENTRY TO HDR @G24LB18*/
         ST    DDENTPTR,STPDDPTR(,STHDRPTR)                        0203
*   SAVENTRY=STHDRPTR;              /* SAVE HEADER PTR       @G24LB18*/
         LR    SAVENTRY,STHDRPTR                                   0204
*   STPICCTR=ONE;                   /* SET INIT CALL CTR = 1 @G24LB18*/
         MVI   STPICCTR(STHDRPTR),X'01'                            0205
*                                                                  0206
*   /*****************************************************************/
*   /*                                                               */
*   /* SET THE LOCAL AND CMS LOCKS AND SEARCH FOR THE END OF @G24LB18*/
*   /* THE STEP TABLE CHAIN. ADD THE NEW STEP TABLE HEADER   @G24LB18*/
*   /* TO THE END OF THE CHAIN AND RETURN TO BLDDDENT.       @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0206
*   GENERATE;                                                      0206
*                                                          /*@G24LB18*/
LGETLCL3 SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,               *
               RELATED=(ICBVTP01(LFRELCL1))
LGETCMS3 SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,                 *
               RELATED=(ICBVTP01(LFRECMS1))
*   STHDRPTR=ADDR(VVISTPTB);        /* GET 1ST HEADER PTR    @G24LB18*/
         L     STHDRPTR,VVIPTR                                     0207
         LA    STHDRPTR,VVISTPTB(,STHDRPTR)                        0207
*   DO WHILE STPNXTTB^=ZERO;        /* IS IT LAST HEADER?    @G24LB18*/
         B     @DE00208                                            0208
@DL00208 DS    0H                                                  0209
*     STHDRPTR=STPNXTTB;            /* NO-GET NEXT HEADER    @G24LB18*/
         L     STHDRPTR,STPNXTTB(,STHDRPTR)                        0209
*   END;                            /* YES -                 @G24LB18*/
@DE00208 L     @08,STPNXTTB(,STHDRPTR)                             0210
         LTR   @08,@08                                             0210
         BNZ   @DL00208                                            0210
*   STPNXTTB=SAVENTRY;              /* CHAIN NEW HDR TO LAST @G24LB18*/
         ST    SAVENTRY,STPNXTTB(,STHDRPTR)                        0211
*   SAVENTRY->STPPRVTB=STHDRPTR;    /* SET BACKWARD PTR      @G24LB18*/
         ST    STHDRPTR,STPPRVTB(,SAVENTRY)                        0212
*   RFY                                                            0213
*     REG0 UNRSTD;                  /* UNRESTRICT REG 0      @G24LB18*/
*   RFY                                                            0214
*     REG1 UNRSTD;                  /* UNRESTRICT REG 1      @G24LB18*/
*   RFY                                                            0215
*     REG15 UNRSTD;                 /* UNRESTRICT REG 15     @G24LB18*/
*   REG14=SAVERET2;                 /* RESTORE RETURN ADDR   @G24LB18*/
         L     REG14,SAVERET2                                      0216
*   RETURN;                         /* RETURN TO BLDDDENT    @G24LB18*/
         B     @EL00005                                            0217
*   END BLDSTHDR;                   /* END BUILD HEADER RTN  @G24LB18*/
*                                                                  0219
*/*********************************************************  @G24LB18*/
*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - SRCDDENT                                @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS ROUTINE IS CALLED ON SEARCH FOR DD ENTRY @G24LB18*/
*/*            REQUESTS. IT SEARCHES FOR THE STEP TABLE      @G24LB18*/
*/*            HEADER FOR THE REQUEST AND THEN SEARCHES THE  @G24LB18*/
*/*            DD ENTRIES IN THAT STEP TABLE FOR ONE WHOSE   @G24LB18*/
*/*            DD # MATCHES THE DD # PASSED WITH THE         @G24LB18*/
*/*            REQUEST.                                      @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - TABLE PROCESSOR REQUEST PARAMETER LIST           @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - IF A MATCHING DD ENTRY IS FOUND, ITS ADDRESS    @G24LB18*/
*/*          IS RETURNED IN THE TABP PARM LIST ALONG WITH A  @G24LB18*/
*/*          RETURN CODE = 0. IF NO MATCHING DD ENTRY IS     @G24LB18*/
*/*          FOUND OR IF NO STEP TABLE HEADER IS FOUND FOR   @G24LB18*/
*/*          THE REQUEST, A RETURN CODE = 4 IS RETURNED.     @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0219
*SRCDDENT:                                                         0219
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
*                                                                  0219
SRCDDENT DS    0H                                                  0220
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL SUBPROCEDURE SRCJOBID TO SEARCH FOR THE STEP     @G24LB18*/
*   /* TABLE HEADER FOR THE REQUEST. IF THE HEADER IS NOT    @G24LB18*/
*   /* FOUND (RETURN CODE = 4), RETURN TO MAINLINE ICBVTP01. @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0220
*   SAVERET1=REG14;                 /* SAVE RETURN ADDRESS   @G24LB18*/
         ST    REG14,SAVERET1                                      0220
*   JOBID=TABJOBID;                 /* GET JOBID             @G24LB18*/
         L     JOBID,TABJOBID(,PARMPTR)                            0221
*   CALL SRCJOBID;                  /* CALL SEARCH RTN       @G24LB18*/
         BAL   @14,SRCJOBID                                        0222
*   REG14=SAVERET1;                 /* RESTORE RETURN ADDR   @G24LB18*/
         L     REG14,SAVERET1                                      0223
*   IF RETCODE=FOUR THEN            /* HEADER FOUND?         @G24LB18*/
         C     RETCODE,@CF00057                                    0224
         BE    @RT00224                                            0224
*     RETURN;                       /* NO-RETURN TO ICBVTP01 @G24LB18*/
*                                                                  0226
*   /*****************************************************************/
*   /*                                                               */
*   /* COMPARE THE DD # FROM THE PARM LIST TO THE DD # IN    @G24LB18*/
*   /* EACH DD ENTRY IN THE STEP TABLE. IF A MATCH IS FOUND, @G24LB18*/
*   /* STORE THE ADDRESS OF THE DD ENTRY IN THE PARM LIST    @G24LB18*/
*   /* AND RETURN TO ICBVTP01. IF THE END OF THE STEP TABLE  @G24LB18*/
*   /* IS REACHED BEFORE A MATCH IS FOUND, SET RETURN CODE   @G24LB18*/
*   /* = 4 AND RETURN TO MAINLINE ICBVTP01.                  @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0226
*   DDENTPTR=STPDDPTR;              /* GET 1ST DD ENTRY PTR  @G24LB18*/
         L     DDENTPTR,STPDDPTR(,STHDRPTR)                        0226
*   DO WHILE DDENTPTR^=ZERO;        /* DO FOR EACH DD ENTRY  @G24LB18*/
         B     @DE00227                                            0227
@DL00227 DS    0H                                                  0228
*     IF TABDDNUM=STPDDNUM THEN     /* DD #S MATCH?          @G24LB18*/
         CLC   TABDDNUM(2,PARMPTR),STPDDNUM(DDENTPTR)              0228
         BNE   @RF00228                                            0228
*       DO;                         /* YES                   @G24LB18*/
*         TABDDRET=DDENTPTR;        /* STORE DD ENTRY ADDR   @G24LB18*/
         ST    DDENTPTR,TABDDRET(,PARMPTR)                         0230
*         RETURN;                   /* RETURN TO ICBVTP01    @G24LB18*/
@EL00006 DS    0H                                                  0231
@EF00006 DS    0H                                                  0231
@ER00006 BR    @14                                                 0231
*       END;                        /*                       @G24LB18*/
*     DDENTPTR=STPDDNXT;            /* GET NEXT DD ENTRY     @G24LB18*/
@RF00228 L     DDENTPTR,STPDDNXT(,DDENTPTR)                        0233
*   END;                            /* NO MATCH FOUND        @G24LB18*/
@DE00227 LTR   DDENTPTR,DDENTPTR                                   0234
         BNZ   @DL00227                                            0234
*   RETCODE=FOUR;                   /* SET RETURN CODE = 4   @G24LB18*/
         LA    RETCODE,4                                           0235
*   RETURN;                         /* RETURN TO ICBVTP01    @G24LB18*/
         B     @EL00006                                            0236
*   END SRCDDENT;                   /* END SEARCH FOR DD RTN @G24LB18*/
         B     @EL00006                                            0237
*                                                                  0238
*/*********************************************************  @G24LB18*/
*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - UNSUCTRM                                @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS ROUTINE IS CALLED ON UNSUCCESSFUL        @G24LB18*/
*/*            TERMINATION REQUESTS. IT SEARCHES FOR THE     @G24LB18*/
*/*            STEP TABLE HEADER FOR THE REQUEST, MARKS IT   @G24LB18*/
*/*            INVALID AND NOT TO BE FREED, AND RETURNS ITS  @G24LB18*/
*/*            ADDRESS TO THE CALLER IN THE TABP PARM LIST.  @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - TABLE PROCESSOR REQUEST PARAMETER LIST           @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - IF THE STEP TABLE HEADER FOR THE REQUEST IS     @G24LB18*/
*/*          FOUND, THE ADDRESS OF THE HEADER IS RETURNED    @G24LB18*/
*/*          IN THE TABP PARM LIST ALONG WITH A RETURN       @G24LB18*/
*/*          CODE = 0. IF THE HEADER IS NOT FOUND, A RETURN  @G24LB18*/
*/*          CODE = 4 IS RETURNED.                           @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0238
*UNSUCTRM:                                                         0238
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
*                                                                  0238
UNSUCTRM DS    0H                                                  0239
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL SUBPROCEDURE SRCJOBID TO SEARCH FOR THE STEP     @G24LB18*/
*   /* TABLE HEADER FOR THE REQUEST. IF THE HEADER IS NOT    @G24LB18*/
*   /* FOUND (RETURN CODE = 4), RETURN TO MAINLINE ICBVTP01. @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0239
*   SAVERET1=REG14;                 /* SAVE RETURN ADDRESS   @G24LB18*/
         ST    REG14,SAVERET1                                      0239
*   JOBID=TABJOBID;                 /* GET JOBID             @G24LB18*/
         L     JOBID,TABJOBID(,PARMPTR)                            0240
*   CALL SRCJOBID;                  /* CALL SEARCH RTN       @G24LB18*/
         BAL   @14,SRCJOBID                                        0241
*   REG14=SAVERET1;                 /* RESTORE RETURN ADDR   @G24LB18*/
         L     REG14,SAVERET1                                      0242
*   IF RETCODE=FOUR THEN            /* HEADER FOUND?         @G24LB18*/
         C     RETCODE,@CF00057                                    0243
         BE    @RT00243                                            0243
*     RETURN;                       /* NO-RETURN TO ICBVTP01 @G24LB18*/
*                                                                  0244
*   /*****************************************************************/
*   /*                                                               */
*   /* STORE THE ADDRESS OF THE STEP TABLE HEADER IN THE     @G24LB18*/
*   /* PARM LIST. TURN ON THE INVALID AND DON'T FREE         @G24LB18*/
*   /* INDICATORS IN THE HEADER AND RETURN TO MAINLINE       @G24LB18*/
*   /* ICBVTP01.                                             @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0245
*   TABHDRET=STHDRPTR;              /* STORE HEADER ADDR     @G24LB18*/
         ST    STHDRPTR,TABHDRET(,PARMPTR)                         0245
*   STPINVAL=ON;                    /* TURN ON INVALID IND   @G24LB18*/
*   STPNOFRE=ON;                    /* TURN ON DON'T FREE    @G24LB18*/
         OI    STPINVAL(STHDRPTR),B'11000000'                      0247
*   RETURN;                         /* RETURN TO ICBVTP01    @G24LB18*/
@EL00007 DS    0H                                                  0248
@EF00007 DS    0H                                                  0248
@ER00007 BR    @14                                                 0248
*   END UNSUCTRM;                   /* END TERMINATION RTN   @G24LB18*/
         B     @EL00007                                            0249
*                                                                  0250
