*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - FREESTAB                                @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS ROUTINE IS CALLED ON FREE STEP TABLE     @G24LB18*/
*/*            REQUESTS. IT BUILDS A CHAIN OF STEP TABLES TO @G24LB18*/
*/*            BE FREED (VIA SUBPROCEDURE DECHAIN),          @G24LB18*/
*/*            INCLUDING THE STEP TABLE FOR THE REQUEST AND  @G24LB18*/
*/*            ALL INVALID TABLES THAT ARE NOT MARKED NOT TO @G24LB18*/
*/*            BE FREED. IT THEN FREES THE HEADERS AND THE   @G24LB18*/
*/*            DD ENTRIES ONE AT A TIME.                     @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - TABLE PROCESSOR REQUEST PARAMETER LIST, VVICB.   @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - FREED STEP TABLE HEADERS AND DD ENTRIES FOR THE @G24LB18*/
*/*          REQUESTED STEP TABLE AND ANY INVALID FREEABLE   @G24LB18*/
*/*          STEP TABLES.                                    @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0250
*FREESTAB:                                                         0250
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
*                                                                  0250
FREESTAB DS    0H                                                  0251
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL SUBPROCEDURE DECHAIN TO BUILD A CHAIN OF STEP    @G24LB18*/
*   /* TABLES TO BE FREED.                                   @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0251
*   SAVERET1=REG14;                 /* SAVE RETURN ADDRESS   @G24LB18*/
         ST    REG14,SAVERET1                                      0251
*   CALL DECHAIN;                   /* CALL DECHAIN RTN      @G24LB18*/
*                                                                  0252
         BAL   @14,DECHAIN                                         0252
*   /*****************************************************************/
*   /*                                                               */
*   /* RELEASE CMS AND LOCAL LOCKS. IF FREE CHAIN POINTER    @G24LB18*/
*   /* IS 0, RETURN TO MAINLINE ICBVTP01. DECHAIN THE 1ST    @G24LB18*/
*   /* STEP TABLE AND SAVE THE 1ST DD ENTRY POINTER. FREE    @G24LB18*/
*   /* THE STEP TABLE HEADER, THEN FREE EACH DD ENTRY,       @G24LB18*/
*   /* SAVING THE NEXT DD ENTRY POINTER EACH TIME BEFORE     @G24LB18*/
*   /* FREEING THE CURRENT ENTRY. WHEN THE NEXT DD ENTRY     @G24LB18*/
*   /* POINTER IS 0, GET THE NEXT STEP TABLE ON THE FREE     @G24LB18*/
*   /* CHAIN AND REPEAT THE PROCESS UNTIL THE FREE CHAIN     @G24LB18*/
*   /* IS EMPTY. THEN RETURN TO MAINLINE ICBVTP01.           @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0253
*   RFY                                                            0253
*     REG0 RSTD;                    /* RESTRICT REG 0        @G24LB18*/
*   RFY                                                            0254
*     REG1 RSTD;                    /* RESTRICT REG 1        @G24LB18*/
*   GENERATE;                                                      0255
*                                                          /*@G24LB18*/
LFRECMS3 SETLOCK RELEASE,TYPE=CMS,REGS=USE,                            *
               RELATED=(ICBVTP01(LGETCMS1))
LFRELCL3 SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,                          *
               RELATED=(ICBVTP01(LGETLCL1))
*   DO WHILE VVITPFCH^=ZERO;        /* DO FOR EACH TABLE     @G24LB18*/
         B     @DE00256                                            0256
@DL00256 DS    0H                                                  0257
*     STHDRPTR=VVITPFCH;            /* GET STEP TABLE PTR    @G24LB18*/
         L     @15,VVIPTR                                          0257
         L     STHDRPTR,VVITPFCH(,@15)                             0257
*     VVITPFCH=STPNXTTB;            /* DECHAIN STEP TABLE    @G24LB18*/
         L     @08,STPNXTTB(,STHDRPTR)                             0258
         ST    @08,VVITPFCH(,@15)                                  0258
*     SAVENTRY=STPDDPTR;            /* SAVE 1ST DD ENTRY PTR @G24LB18*/
         L     SAVENTRY,STPDDPTR(,STHDRPTR)                        0259
*     REG0=STPHDRLN;                /* GET HEADER LENGTH     @G24LB18*/
         LH    REG0,STPHDRLN(,STHDRPTR)                            0260
*     REG1=STHDRPTR;                /* GET ADDR OF HEADER    @G24LB18*/
         LR    REG1,STHDRPTR                                       0261
*LFREM2:                                                           0262
*     GEN(FREEMAIN RC,LV=(0),A=(1),SP=241);/* FREEMAIN HDR   @G24LB18*/
LFREM2   FREEMAIN RC,LV=(0),A=(1),SP=241
*     DO WHILE SAVENTRY^=ZERO;      /* DO FOR EACH DD ENTRY  @G24LB18*/
         B     @DE00263                                            0263
@DL00263 DS    0H                                                  0264
*       DDENTPTR=SAVENTRY;          /* GET DD ENTRY PTR      @G24LB18*/
         LR    DDENTPTR,SAVENTRY                                   0264
*       SAVENTRY=STPDDNXT;          /* SAVE NEXT DD ENT PTR  @G24LB18*/
         L     SAVENTRY,STPDDNXT(,DDENTPTR)                        0265
*       REG0=STPDDLEN;              /* GET LENGTH OF DD ENT  @G24LB18*/
         LH    REG0,STPDDLEN(,DDENTPTR)                            0266
*       REG1=DDENTPTR;              /* GET ADDR OF DD ENTRY  @G24LB18*/
         LR    REG1,DDENTPTR                                       0267
*LFREM3:                                                           0268
*       GEN(FREEMAIN RC,LV=(0),A=(1),SP=241);/* FREEMAIN ENT @G24LB18*/
LFREM3   FREEMAIN RC,LV=(0),A=(1),SP=241
*     END;                          /* END DD ENTRY LOOP     @G24LB18*/
@DE00263 LTR   SAVENTRY,SAVENTRY                                   0269
         BNZ   @DL00263                                            0269
*   END;                            /* END STEP TABLE LOOP   @G24LB18*/
@DE00256 L     @15,VVIPTR                                          0270
         L     @15,VVITPFCH(,@15)                                  0270
         LTR   @15,@15                                             0270
         BNZ   @DL00256                                            0270
*   RFY                                                            0271
*     REG0 UNRSTD;                  /* UNRESTRICT REG 0      @G24LB18*/
*   RFY                                                            0272
*     REG1 UNRSTD;                  /* UNRESTRICT REG 1      @G24LB18*/
*   REG14=SAVERET1;                 /* RESTORE RETURN ADDR   @G24LB18*/
         L     REG14,SAVERET1                                      0273
*   RETURN;                         /* RETURN TO ICBVTP01    @G24LB18*/
@EL00008 DS    0H                                                  0274
@EF00008 DS    0H                                                  0274
@ER00008 BR    @14                                                 0274
*   END FREESTAB;                   /* END FREE TABLES RTN   @G24LB18*/
*                                                                  0276
