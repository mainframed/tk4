*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - DECHAIN                                 @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS ROUTINE IS CALLED ON FREE STEP TABLE     @G24LB18*/
*/*            REQUESTS. IT SEARCHES THE STEP TABLE CHAIN    @G24LB18*/
*/*            FOR THE TABLE WHOSE JOBID MATCHES THE JOBID   @G24LB18*/
*/*            PASSED WITH THE REQUEST. IT DECREMENTS THE    @G24LB18*/
*/*            INITIAL CALL COUNTER FOR THE STEP BY ONE.     @G24LB18*/
*/*            IF IT IS NOW 0, IT DECHAINS THE TABLE FROM    @G24LB18*/
*/*            THE STEP TABLE CHAIN AND RECHAINS IT TO A     @G24LB18*/
*/*            FREE CHAIN OF TABLES TO BE FREED. THE         @G24LB18*/
*/*            ROUTINE ALSO SEARCHES THE STEP TABLE CHAIN    @G24LB18*/
*/*            FOR ALL INVALID STEP TABLES WHICH CAN BE      @G24LB18*/
*/*            FREED, DECHAINS THEM FROM THE STEP TABLE      @G24LB18*/
*/*            CHAIN, AND RECHAINS THEM TO THE FREE CHAIN.   @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - TABLE PROCESSOR REQUEST PARAMETER LIST, VVICB    @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - THE STEP TABLE CHAIN WITH THE REQUESTED STEP    @G24LB18*/
*/*          TABLE (IF THE INITIAL CALL COUNTER = 0) AND ALL @G24LB18*/
*/*          INVALID FREEABLE STEP TABLES REMOVED, AND A     @G24LB18*/
*/*          FREE CHAIN CONTAINING THE TABLES REMOVED FROM   @G24LB18*/
*/*          THE STEP TABLE CHAIN.                           @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0276
*DECHAIN:                                                          0276
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
*                                                                  0276
DECHAIN  DS    0H                                                  0277
*   /*****************************************************************/
*   /*                                                               */
*   /* GET THE ADDRESS OF STEP TABLE CHAIN FROM THE VVICB.   @G24LB18*/
*   /* IF IT IS 0, RETURN TO FREESTAB. OTHERWISE GET THE     @G24LB18*/
*   /* FIRST STEP TABLE HEADER.                              @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0277
*   STHDRPTR=VVISTPTB;              /* GET STEP TABLE CHAIN  @G24LB18*/
         L     @15,VVIPTR                                          0277
         L     STHDRPTR,VVISTPTB(,@15)                             0277
*   DO WHILE STHDRPTR^=ZERO;        /* DO FOR EACH HEADER    @G24LB18*/
         B     @DE00278                                            0278
@DL00278 DS    0H                                                  0279
*                                                                  0279
*     /***************************************************************/
*     /*                                                             */
*     /* IF THE JOBID OF THE HEADER MATCHES THE JOBID IN THE @G24LB18*/
*     /* PARM LIST, DECREMENT THE INITIAL CALL COUNTER BY 1. @G24LB18*/
*     /* IF THE HEADER IS MARKED INVALID AND FREEABLE OR IF  @G24LB18*/
*     /* THE JOBIDS MATCH AND THE INITIAL CALL COUNTER = 0,  @G24LB18*/
*     /* DECHAIN THE HEADER FROM THE STEP TABLE CHAIN,       @G24LB18*/
*     /* SAVING THE POINTER TO THE NEXT HEADER IN THE CHAIN. @G24LB18*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0279
*     IF TABJOBID=STPJOBID THEN     /* JOBIDS MATCH?         @G24LB18*/
         CLC   TABJOBID(4,PARMPTR),STPJOBID(STHDRPTR)              0279
         BNE   @RF00279                                            0279
*       STPICCTR=STPICCTR-ONE;      /* YES-DECREMENT COUNTER @G24LB18*/
         SLR   @15,@15                                             0280
         IC    @15,STPICCTR(,STHDRPTR)                             0280
         BCTR  @15,0                                               0280
         STC   @15,STPICCTR(,STHDRPTR)                             0280
*     IF(STPINVAL=ON&               /* HEADER INVALID AND    @G24LB18*/
*         STPNOFRE=OFF)×            /* FREEABLE OR           @G24LB18*/
*         (TABJOBID=STPJOBID&       /* JOBIDS MATCH AND      @G24LB18*/
*         STPICCTR=ZERO) THEN       /* INIT CALL CTR = 0?    @G24LB18*/
@RF00279 TM    STPINVAL(STHDRPTR),B'10000000'                      0281
         BNO   @GL00004                                            0281
         TM    STPNOFRE(STHDRPTR),B'01000000'                      0281
         BZ    @RT00281                                            0281
@GL00004 CLC   TABJOBID(4,PARMPTR),STPJOBID(STHDRPTR)              0281
         BNE   @RF00281                                            0281
         CLI   STPICCTR(STHDRPTR),0                                0281
         BNE   @RF00281                                            0281
@RT00281 DS    0H                                                  0282
*       DO;                         /* YES                   @G24LB18*/
*         NXTHDRSV=STPNXTTB;        /* SAVE NEXT HDR PTR     @G24LB18*/
         L     @15,STPNXTTB(,STHDRPTR)                             0283
         LR    NXTHDRSV,@15                                        0283
*         STPPRVTB->STPNXTTB=STPNXTTB;/* DECHAIN HDR FROM PREV HDR   */
         L     @07,STPPRVTB(,STHDRPTR)                             0284
         ST    @15,STPNXTTB(,@07)                                  0284
*         IF STPNXTTB^=ZERO THEN    /* IF NEXT HDR EXISTS,   @G24LB18*/
         LTR   @15,@15                                             0285
         BZ    @RF00285                                            0285
*           STPNXTTB->STPPRVTB=STPPRVTB;/* RECHAIN IT TO PREV HDR    */
*                                                                  0286
         ST    @07,STPPRVTB(,@15)                                  0286
*         /***********************************************************/
*         /*                                                         */
*         /* GET THE FREE CHAIN POINTER. IF IT IS 0, CHAIN   @G24LB18*/
*         /* THE DECHAINED HEADER TO IT. OTHERWISE SEARCH    @G24LB18*/
*         /* THE FREE CHAIN FOR THE LAST ENTRY AND CHAIN THE @G24LB18*/
*         /* DECHAINED HEADER TO IT. ZERO THE NEXT HEADER    @G24LB18*/
*         /* POINTER FIELD IN THE HEADER, RESTORE THE        @G24LB18*/
*         /* ADDRESS OF THE NEXT HEADER IN THE STEP TABLE    @G24LB18*/
*         /* CHAIN AND CONTINUE SEARCHING FOR TABLES TO BE   @G24LB18*/
*         /* FREED.                                          @G24LB18*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0287
*         STPNXTTB=ZERO;            /* ZERO NEXT HDR PTR     @G24LB18*/
@RF00285 SLR   @15,@15                                             0287
         ST    @15,STPNXTTB(,STHDRPTR)                             0287
*         SAVENTRY=STHDRPTR;        /* SAVE DECHAINED HDR    @G24LB18*/
         LR    SAVENTRY,STHDRPTR                                   0288
*         STHDRPTR=ADDR(VVITPFCH);  /* GET FREE CHAIN PTR    @G24LB18*/
         L     STHDRPTR,VVIPTR                                     0289
         LA    STHDRPTR,VVITPFCH(,STHDRPTR)                        0289
*         DO WHILE STPNXTTB^=ZERO;  /* IS IT LAST HEADER?    @G24LB18*/
         B     @DE00290                                            0290
@DL00290 DS    0H                                                  0291
*           STHDRPTR=STPNXTTB;      /* NO-GET NEXT HEADER    @G24LB18*/
         L     STHDRPTR,STPNXTTB(,STHDRPTR)                        0291
*         END;                      /* LAST HEADER FOUND     @G24LB18*/
@DE00290 L     @15,STPNXTTB(,STHDRPTR)                             0292
         LTR   @15,@15                                             0292
         BNZ   @DL00290                                            0292
*         STPNXTTB=SAVENTRY;        /* CHAIN HDR TO LAST     @G24LB18*/
         ST    SAVENTRY,STPNXTTB(,STHDRPTR)                        0293
*         STHDRPTR=NXTHDRSV;        /* RESTORE NEXT HDR PTR  @G24LB18*/
         LR    STHDRPTR,NXTHDRSV                                   0294
*       END;                        /* END STEP TABLE SEARCH @G24LB18*/
*     ELSE                          /*                       @G24LB18*/
*       STHDRPTR=STPNXTTB;          /* GET NEXT HEADER       @G24LB18*/
         B     @RC00281                                            0296
@RF00281 L     STHDRPTR,STPNXTTB(,STHDRPTR)                        0296
*   END;                            /* END STEP TABLE SEARCH @G24LB18*/
@RC00281 DS    0H                                                  0297
@DE00278 LTR   STHDRPTR,STHDRPTR                                   0297
         BNZ   @DL00278                                            0297
*   RETURN;                         /* RETURN TO FREESTAB    @G24LB18*/
@EL00009 DS    0H                                                  0298
@EF00009 DS    0H                                                  0298
@ER00009 BR    @14                                                 0298
*   END DECHAIN;                    /* END DECHAIN TABLE RTN @G24LB18*/
*                                                                  0300
*/*********************************************************  @G24LB18*/
*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - SRCJOBID                                @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS SUBROUTINE IS CALLED WHEN THE STEP TABLE @G24LB18*/
*/*            FOR THE CURRENT REQUEST MUST BE FOUND. THE    @G24LB18*/
*/*            ROUTINE SEARCHES THE STEP TABLE CHAIN FOR     @G24LB18*/
*/*            THE STEP TABLE WHOSE JOBID MATCHES THE JOBID  @G24LB18*/
*/*            PASSED WITH THE REQUEST.                      @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - TABLE PROCESSOR REQUEST PARAMETER LIST, VVICB    @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - IF A STEP TABLE IS FOUND WITH A MATCHING JOBID  @G24LB18*/
*/*          AND IT IS NOT MARKED INVALID, ITS ADDRESS IS    @G24LB18*/
*/*          RETURNED IN FIELD STHDRPTR WITH A RETURN CODE   @G24LB18*/
*/*          OF 0. OTHERWISE, A RETURN CODE = 4 IS RETURNED. @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0300
*SRCJOBID:                                                         0300
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
*                                                                  0300
SRCJOBID DS    0H                                                  0301
*   /*****************************************************************/
*   /*                                                               */
*   /* GET THE ADDRESS OF THE STEP TABLE CHAIN FROM THE      @G24LB18*/
*   /* VVICB. IF IT IS 0, RETURN TO THE CALLER WITH RETURN   @G24LB18*/
*   /* CODE = 4. OTHERWISE GET THE FIRST STEP TABLE HEADER.  @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0301
*   STHDRPTR=ADDR(VVISTPTB);        /* GET STEP TABLE CHAIN  @G24LB18*/
         L     STHDRPTR,VVIPTR                                     0301
         LA    STHDRPTR,VVISTPTB(,STHDRPTR)                        0301
*   DO WHILE STPNXTTB^=ZERO;        /* DO FOR EACH HEADER    @G24LB18*/
         B     @DE00302                                            0302
@DL00302 DS    0H                                                  0303
*     STHDRPTR=STPNXTTB;            /* GET NEXT HEADER       @G24LB18*/
*                                                                  0303
         L     STHDRPTR,STPNXTTB(,STHDRPTR)                        0303
*     /***************************************************************/
*     /*                                                             */
*     /* IF THE JOBID OF THE HEADER MATCHES THE JOBID IN THE @G24LB18*/
*     /* TABP PARM LIST AND IF THE HEADER IS NOT MARKED      @G24LB18*/
*     /* INVALID, RETURN TO THE CALLER WITH THE ADDRESS OF   @G24LB18*/
*     /* THE HEADER IN FIELD STHDRPTR.                       @G24LB18*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0304
*     IF JOBID=STPJOBID&            /* JOBIDS MATCH AND      @G24LB18*/
*         STPINVAL=OFF THEN         /* HEADER NOT INVALID?   @G24LB18*/
         C     JOBID,STPJOBID(,STHDRPTR)                           0304
         BNE   @RF00304                                            0304
         TM    STPINVAL(STHDRPTR),B'10000000'                      0304
         BZ    @RT00304                                            0304
*       RETURN;                     /* YES-RETURN TO CALLER  @G24LB18*/
*   END;                            /* END STEP TABLE SEARCH @G24LB18*/
@RF00304 DS    0H                                                  0306
@DE00302 L     @15,STPNXTTB(,STHDRPTR)                             0306
         LTR   @15,@15                                             0306
         BNZ   @DL00302                                            0306
*                                                                  0307
*   /*****************************************************************/
*   /*                                                               */
*   /* IF NO VALID STEP TABLE IS FOUND WITH A MATCHING       @G24LB18*/
*   /* JOBID, RETURN TO THE CALLER WITH A RETURN CODE = 4.   @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0307
*   RETCODE=FOUR;                   /* SET RETURN CODE = 4   @G24LB18*/
         LA    RETCODE,4                                           0307
*   RETURN;                         /* RETURN TO CALLER      @G24LB18*/
@EL00010 DS    0H                                                  0308
@EF00010 DS    0H                                                  0308
@ER00010 BR    @14                                                 0308
*   END SRCJOBID;                   /* END JOBID SEARCH RTN  @G24LB18*/
         B     @EL00010                                            0309
*   END ICBVTP01                    /* END TABLE PROCESSOR   @G24LB18*/
