         TITLE '************* ICBVTP00 - MSVC TABLE PROCESSOR **********
               *****    '
ICBVTP00 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ICBVTP00  76.275'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @07,@SA00001                                        0001
         ST    @07,8(,@13)                                         0001
         LR    @13,@07                                             0001
*   PARMPTR=PARMADDR;               /* SAVE PARM LIST ADDR   @G24LB18*/
         L     PARMPTR,PARMADDR(,REG1)                             0077
*   VVIPTR=ICBVVICB;                /* SET UP MSVC CB PTR    @G24LB18*/
         L     @15,CVTPTR                                          0078
         L     @15,CVTEXT2(,@15)                                   0078
         L     @15,CVTICB(,@15)                                    0078
         L     @15,ICBVVICB(,@15)                                  0078
         ST    @15,VVIPTR                                          0078
*   RFY                                                            0079
*     REG1 UNRSTD;                  /* UNRESTRICT REG 1      @G24LB18*/
*   RFY                                                            0080
*     TABP BASED(PARMPTR);          /* BASE TABP PARM LIST   @G24LB18*/
*   RFY                                                            0081
*     SELRB BASED(TABSELRB);        /* BASE SELECT REQ BLK   @G24LB18*/
*   RFY                                                            0082
*     STPTB BASED(STHDRPTR);        /* BASE STEP TABLE HDR   @G24LB18*/
*   RFY                                                            0083
*     STPDDENT BASED(DDENTPTR);     /* BASE DD ENTRY         @G24LB18*/
*   RETCODE=ZERO;                   /* SET RETURN CODE = 0   @G24LB18*/
         SLR   @15,@15                                             0084
         ST    @15,RETCODE                                         0084
*                                                                  0085
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE WHICH FUNCTION WAS REQUESTED AND CALL THE   @G24LB18*/
*   /* APPROPRIATE PROCEDURE TO PROCESS IT.                  @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0085
*                                                                  0085
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD DD ENTRY REQUEST                                @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0085
*   IF TABFBLDE=ON THEN             /* BUILD DD ENTRY REQ?   @G24LB18*/
         TM    TABFBLDE(PARMPTR),B'10000000'                       0085
         BNO   @RF00085                                            0085
*     DO;                           /* YES                   @G24LB18*/
*       CALL BLDDDENT;              /* CALL BUILD DD ENT RTN @G24LB18*/
         BAL   @14,BLDDDENT                                        0087
*       IF RETCODE=FOUR THEN        /* DD ENTRY NOT BUILT?   @G24LB18*/
         L     @15,RETCODE                                         0088
         C     @15,@CF00060                                        0088
         BNE   @RF00088                                            0088
*         RETURN CODE(RETCODE);     /* YES-RETURN TO CALLER  @G24LB18*/
         L     @13,4(,@13)                                         0089
         L     @14,12(,@13)                                        0089
         LM    @00,@12,20(@13)                                     0089
         BR    @14                                                 0089
*     END;                          /* WITH RETURN CODE = 4  @G24LB18*/
*                                                                  0090
*   /*****************************************************************/
*   /*                                                               */
*   /* FOR ALL REQUESTS EXCEPT 'BUILD DD ENTRY', SET THE     @G24LB18*/
*   /* LOCAL AND CMS LOCKS BEFORE PROCESSING.                @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0091
*   ELSE                            /*                       @G24LB18*/
*     DO;                           /*                       @G24LB18*/
         B     @RC00085                                            0091
@RF00085 DS    0H                                                  0092
*       VVIFLG6=ON;                 /* SET LOCAL LOCK FLAG   @G24LB18*/
*       VVIFLG7=ON;                 /* SET CMS LOCK FLAG     @G24LB18*/
         L     @15,VVIPTR                                          0093
         OI    VVIFLG6(@15),B'00000011'                            0093
*       GENERATE;                                                  0094
*                                                          /*@G24LB18*/
LGETLCL1 SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,               *
               RELATED=(ICBVTP00(LFRELCL1))
LGETCMS1 SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,REGS=USE,                 *
               RELATED=(ICBVTP00(LFRECMS1))
*     END;                          /*                       @G24LB18*/
*                                                                  0095
*   /*****************************************************************/
*   /*                                                               */
*   /* SEARCH FOR DD ENTRY REQUEST                           @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0096
*   IF TABFSRCD=ON THEN             /* SEARCH FOR DD ENTRY?  @G24LB18*/
@RC00085 TM    TABFSRCD(PARMPTR),B'01000000'                       0096
         BNO   @RF00096                                            0096
*     CALL SRCDDENT;                /* YES - CALL SEARCH RTN @G24LB18*/
         BAL   @14,SRCDDENT                                        0097
*                                                                  0098
*   /*****************************************************************/
*   /*                                                               */
*   /* UPDATE BEST FIT TABLE REQUEST                         @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0098
*   IF TABFBFIT=ON THEN             /* UPDATE BEST FIT TAB?  @G24LB18*/
@RF00096 TM    TABFBFIT(PARMPTR),B'00100000'                       0098
         BNO   @RF00098                                            0098
*     CALL UPDTBFTB;                /* YES-CALL UPDATE RTN   @G24LB18*/
*                                                                  0099
         BAL   @14,UPDTBFTB                                        0099
*   /*****************************************************************/
*   /*                                                               */
*   /* GET TOTAL SPACE FOR VOLSER REQUEST                    @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0100
*   IF TABFTSPC=ON THEN             /* TOTAL SPACE REQ?      @G24LB18*/
@RF00098 TM    TABFTSPC(PARMPTR),B'00010000'                       0100
         BNO   @RF00100                                            0100
*     CALL TOTSPACE;                /* YES-CALL SPACE RTN    @G24LB18*/
*                                                                  0101
         BAL   @14,TOTSPACE                                        0101
*   /*****************************************************************/
*   /*                                                               */
*   /* RELEASE THE CMS AND LOCAL LOCKS AND RETURN TO THE     @G24LB18*/
*   /* CALLER WITH A RETURN CODE IN REGISTER 15.             @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0102
*   GENERATE;                                                      0102
@RF00100 DS    0H                                                  0102
*                                                          /*@G24LB18*/
LFRECMS1 SETLOCK RELEASE,TYPE=CMS,REGS=USE,                            *
               RELATED=(ICBVTP00(LGETCMS1,LGETCMS2,LGETCMS3))
LFRELCL1 SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,                          *
               RELATED=(ICBVTP00(LGETLCL1,LGETLCL2,LGETLCL3))
*   VVIFLG6=OFF;                    /* TURN OFF LOCAL AND    @G24LB18*/
*   VVIFLG7=OFF;                    /* CMS LOCK FLAGS        @G24LB18*/
         L     @15,VVIPTR                                          0104
         NI    VVIFLG6(@15),B'11111100'                            0104
*   RETURN CODE(RETCODE);           /* RETURN TO CALLER      @G24LB18*/
*                                                                  0105
         L     @15,RETCODE                                         0105
         L     @13,4(,@13)                                         0105
         L     @14,12(,@13)                                        0105
         LM    @00,@12,20(@13)                                     0105
         BR    @14                                                 0105
*   /*****************************************************************/
*   /*                                                               */
*   /* END OF MAINLINE PROCESSING FOR ICBVTP00. THE          @G24LB18*/
*   /* REMAINDER OF THE MODULE CONSISTS OF INTERNAL          @G24LB18*/
*   /* PROCEDURES FOR ICBVTP00.                              @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
