*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - SRCDDENT                                @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS ROUTINE IS CALLED ON SEARCH FOR DD ENTRY @G24LB18*/
*/*            REQUESTS. IT SEARCHES FOR THE STEP TABLE      @G24LB18*/
*/*            HEADER FOR THE REQUEST AND THEN SEARCHES THE  @G24LB18*/
*/*            DD ENTRIES IN THAT STEP TABLE FOR ONE WHOSE   @G24LB18*/
*/*            DD # MATCHES THE DD # PASSED WITH THE         @G24LB18*/
*/*            REQUEST.                                      @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - TABLE PROCESSOR REQUEST PARAMETER LIST           @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - IF A MATCHING DD ENTRY IS FOUND, ITS ADDRESS    @G24LB18*/
*/*          IS RETURNED IN THE TABP PARM LIST ALONG WITH A  @G24LB18*/
*/*          RETURN CODE = 0. IF NO MATCHING DD ENTRY IS     @G24LB18*/
*/*          FOUND OR IF NO STEP TABLE HEADER IS FOUND FOR   @G24LB18*/
*/*          THE REQUEST, A RETURN CODE = 4 IS RETURNED.     @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0268
*SRCDDENT:                                                         0268
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
*                                                                  0268
SRCDDENT DS    0H                                                  0269
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL SUBPROCEDURE SRCJOBID TO SEARCH FOR THE STEP     @G24LB18*/
*   /* TABLE HEADER FOR THE REQUEST. IF THE HEADER IS NOT    @G24LB18*/
*   /* FOUND (RETURN CODE = 4), RETURN TO MAINLINE ICBVTP00. @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0269
*   SAVERET1=REG14;                 /* SAVE RETURN ADDRESS   @G24LB18*/
         ST    REG14,SAVERET1                                      0269
*   JOBID=TABJOBID;                 /* GET JOBID             @G24LB18*/
         L     JOBID,TABJOBID(,PARMPTR)                            0270
*   CALL SRCJOBID;                  /* CALL SEARCH RTN       @G24LB18*/
         BAL   @14,SRCJOBID                                        0271
*   REG14=SAVERET1;                 /* RESTORE RETURN ADDR   @G24LB18*/
         L     REG14,SAVERET1                                      0272
*   IF RETCODE=FOUR THEN            /* HEADER FOUND?         @G24LB18*/
         CLC   RETCODE(4),@CF00060                                 0273
         BE    @RT00273                                            0273
*     RETURN;                       /* NO-RETURN TO ICBVTP00 @G24LB18*/
*                                                                  0275
*   /*****************************************************************/
*   /*                                                               */
*   /* COMPARE THE DD # FROM THE PARM LIST TO THE DD # IN    @G24LB18*/
*   /* EACH DD ENTRY IN THE STEP TABLE. IF A MATCH IS FOUND, @G24LB18*/
*   /* STORE THE ADDRESS OF THE DD ENTRY IN THE PARM LIST    @G24LB18*/
*   /* AND RETURN TO ICBVTP00. IF THE END OF THE STEP TABLE  @G24LB18*/
*   /* IS REACHED BEFORE A MATCH IS FOUND, SET RETURN CODE   @G24LB18*/
*   /* = 4 AND RETURN TO MAINLINE ICBVTP00.                  @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0275
*   DDENTPTR=STPDDPTR;              /* GET 1ST DD ENTRY PTR  @G24LB18*/
         L     DDENTPTR,STPDDPTR(,STHDRPTR)                        0275
*   DO WHILE DDENTPTR^=ZERO;        /* DO FOR EACH DD ENTRY  @G24LB18*/
         B     @DE00276                                            0276
@DL00276 DS    0H                                                  0277
*     IF TABDDNUM=STPDDNUM THEN     /* DD #S MATCH?          @G24LB18*/
         CLC   TABDDNUM(2,PARMPTR),STPDDNUM(DDENTPTR)              0277
         BNE   @RF00277                                            0277
*       DO;                         /* YES                   @G24LB18*/
*         TABDDRET=DDENTPTR;        /* STORE DD ENTRY ADDR   @G24LB18*/
         ST    DDENTPTR,TABDDRET(,PARMPTR)                         0279
*         RETURN;                   /* RETURN TO ICBVTP00    @G24LB18*/
@EL00006 DS    0H                                                  0280
@EF00006 DS    0H                                                  0280
@ER00006 BR    @14                                                 0280
*       END;                        /*                       @G24LB18*/
*     DDENTPTR=STPDDNXT;            /* GET NEXT DD ENTRY     @G24LB18*/
@RF00277 L     DDENTPTR,STPDDNXT(,DDENTPTR)                        0282
*   END;                            /* NO MATCH FOUND        @G24LB18*/
@DE00276 LTR   DDENTPTR,DDENTPTR                                   0283
         BNZ   @DL00276                                            0283
*   RETCODE=FOUR;                   /* SET RETURN CODE = 4   @G24LB18*/
         MVC   RETCODE(4),@CF00060                                 0284
*   RETURN;                         /* RETURN TO ICBVTP00    @G24LB18*/
         B     @EL00006                                            0285
*   END SRCDDENT;                   /* END SEARCH FOR DD RTN @G24LB18*/
         B     @EL00006                                            0286
*                                                                  0287
*/*********************************************************  @G24LB18*/
*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - UPDTBFTB                                @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS ROUTINE IS CALLED ON UPDATE BEST FIT     @G24LB18*/
*/*            TABLE REQUESTS. IT SEARCHES THE STEP TABLE    @G24LB18*/
*/*            CHAIN FOR EACH OCCURRENCE OF EACH VOL SER     @G24LB18*/
*/*            CONTAINED IN THE BEST FIT TABLE. WHEN AN      @G24LB18*/
*/*            OCCURRENCE IS FOUND, IT UPDATES THE USAGE     @G24LB18*/
*/*            AND SHAREABILITY FIELDS FOR THE VOLUME IN     @G24LB18*/
*/*            THE BEST FIT TABLE (VIA SUBPROCEDURE          @G24LB18*/
*/*            VOLUSAGE).                                    @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - TABLE PROCESSOR REQUEST PARAMETER LIST, VVICB,   @G24LB18*/
*/*         AND BEST FIT TABLE.                              @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - UPDATED BEST FIT TABLE.                         @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0287
*UPDTBFTB:                                                         0287
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
UPDTBFTB DS    0H                                                  0288
*   RFY                                                            0288
*     BFTBL BASED(TABBFTBL);        /* BASE BEST FIT TABLE   @G24LB18*/
*                                                                  0288
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THERE ARE NO STEP TABLES IN THE STEP TABLE CHAIN,  @G24LB18*/
*   /* RETURN TO MAINLINE ICBVTP00 WITHOUT PROCESSING THE    @G24LB18*/
*   /* BEST FIT TABLE.                                       @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0289
*   IF VVISTPTB=ZERO THEN           /* ANY STEP TABLES?      @G24LB18*/
         L     @15,VVIPTR                                          0289
         L     @15,VVISTPTB(,@15)                                  0289
         LTR   @15,@15                                             0289
         BZ    @RT00289                                            0289
*     RETURN;                       /* NO-RETURN TO ICBVTP00 @G24LB18*/
*                                                                  0290
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE BEST FIT TABLE ELEMENT INDEX AND THE   @G24LB18*/
*   /* VOLUME USE COUNT. GET THE ADDRESS OF THE STEP TABLE   @G24LB18*/
*   /* CHAIN FROM THE VVICB AND GET THE FIRST STEP TABLE     @G24LB18*/
*   /* HEADER.                                               @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0291
*   SAVERET1=REG14;                 /* SAVE RETURN ADDRESS   @G24LB18*/
         ST    REG14,SAVERET1                                      0291
*   INDEX=ONE;                      /* INIT ELEMENT INDEX    @G24LB18*/
         LA    INDEX,1                                             0292
*   DO WHILE INDEX^>BFTOTIDX;       /* DO FOR EACH ELEMENT   @G24LB18*/
         B     @DE00293                                            0293
@DL00293 DS    0H                                                  0294
*     USECOUNT=ZERO;                /* INIT VOL USE COUNT    @G24LB18*/
         MVI   USECOUNT,X'00'                                      0294
*     STHDRPTR=ADDR(VVISTPTB);      /* GET STEP TABLE CHAIN  @G24LB18*/
         L     STHDRPTR,VVIPTR                                     0295
         LA    STHDRPTR,VVISTPTB(,STHDRPTR)                        0295
*     DO WHILE STPNXTTB^=ZERO;      /* DO FOR EACH HEADER    @G24LB18*/
         B     @DE00296                                            0296
@DL00296 DS    0H                                                  0297
*       STHDRPTR=STPNXTTB;          /* GET STEP TAB HEADER   @G24LB18*/
         L     STHDRPTR,STPNXTTB(,STHDRPTR)                        0297
*                                                                  0298
*       /*************************************************************/
*       /*                                                           */
*       /* IF THE STEP TABLE IS NOT INVALID, GET THE FIRST   @G24LB18*/
*       /* DD ENTRY. INITIALIZE THE VOL SER INDEX AND GET    @G24LB18*/
*       /* THE FIRST VOL SER IN THE DD ENTRY.                @G24LB18*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0298
*       IF STPINVAL=OFF THEN        /* STEP TABLE INVALID?   @G24LB18*/
         TM    STPINVAL(STHDRPTR),B'10000000'                      0298
         BNZ   @RF00298                                            0298
*         DO;                       /* NO                    @G24LB18*/
*           DDENTPTR=ADDR(STPDDPTR);/* GET DD ENTRY CHAIN    @G24LB18*/
         LA    DDENTPTR,STPDDPTR(,STHDRPTR)                        0300
*           DO WHILE STPDDNXT^=ZERO;/* DO FOR EACH DD ENT    @G24LB18*/
         B     @DE00301                                            0301
@DL00301 DS    0H                                                  0302
*             DDENTPTR=STPDDNXT;    /* GET DD ENTRY          @G24LB18*/
         L     DDENTPTR,STPDDNXT(,DDENTPTR)                        0302
*             VOLINDX=ONE;          /* INIT VOLSER INDEX     @G24LB18*/
*                                                                  0303
         MVI   VOLINDX,X'01'                                       0303
*             /*******************************************************/
*             /*                                                     */
*             /* IF THE VOLSER FROM THE DD ENTRY MATCHES THE @G24LB18*/
*             /* VOLSER FROM THE BEST FIT TABLE ELEMENT,     @G24LB18*/
*             /* CALL SUBPROCEDURE VOLUSAGE TO UPDATE THE    @G24LB18*/
*             /* BEST FIT TABLE FOR THE VOLUME. REPEAT FOR   @G24LB18*/
*             /* EACH VOLSER IN THE DD ENTRY.                @G24LB18*/
*             /*                                                     */
*             /*******************************************************/
*                                                                  0304
*             DO WHILE VOLINDX^>STPDDNVL;/* DO FOR EACH VOL  @G24LB18*/
         B     @DE00304                                            0304
@DL00304 DS    0H                                                  0305
*               IF STPDDVOL(VOLINDX)=BFVOLID(INDEX) THEN/*   @G24LB18
*                                      VOLSERS = ?           @G24LB18*/
         SLR   @15,@15                                             0305
         IC    @15,VOLINDX                                         0305
         MH    @15,@CH00064                                        0305
         LR    @11,INDEX                                           0305
         MH    @11,@CH01134                                        0305
         L     @01,TABBFTBL(,PARMPTR)                              0305
         ALR   @15,DDENTPTR                                        0305
         ALR   @01,@11                                             0305
         CLC   STPDDVOL-6(6,@15),BFVOLID-18(@01)                   0305
         BNE   @RF00305                                            0305
*                 CALL VOLUSAGE;    /* YES-CALL VOLUSAGE RTN @G24LB18*/
         BAL   @14,VOLUSAGE                                        0306
*               VOLINDX=VOLINDX+ONE;/* INCREMENT VOL INDEX   @G24LB18*/
@RF00305 LA    @15,1                                               0307
         SLR   @11,@11                                             0307
         IC    @11,VOLINDX                                         0307
         ALR   @15,@11                                             0307
         STC   @15,VOLINDX                                         0307
*             END;                  /* END VOL SER LOOP      @G24LB18*/
*                                                                  0308
@DE00304 CLC   VOLINDX(1),STPDDNVL(DDENTPTR)                       0308
         BNH   @DL00304                                            0308
*             /*******************************************************/
*             /*                                                     */
*             /* GET THE NEXT DD ENTRY AND REPEAT THE VOLSER @G24LB18*/
*             /* LOOP. CONTINUE UNTIL ALL DD ENTRIES IN THE  @G24LB18*/
*             /* STEP TABLE HAVE BEEN PROCESSED.             @G24LB18*/
*             /*                                                     */
*             /*******************************************************/
*                                                                  0309
*           END;                    /* END DD ENTRY LOOP     @G24LB18*/
@DE00301 L     @15,STPDDNXT(,DDENTPTR)                             0309
         LTR   @15,@15                                             0309
         BNZ   @DL00301                                            0309
*         END;                      /*                       @G24LB18*/
*                                                                  0310
*       /*************************************************************/
*       /*                                                           */
*       /* GET THE NEXT STEP TABLE HEADER AND REPEAT THE     @G24LB18*/
*       /* DD ENTRY LOOP. CONTINUE UNTIL ALL STEP TABLES     @G24LB18*/
*       /* IN THE STEP TABLE CHAIN HAVE BEEN PROCESSED.      @G24LB18*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0311
*     END;                          /* END STEP TABLE LOOP   @G24LB18*/
@RF00298 DS    0H                                                  0311
@DE00296 L     @15,STPNXTTB(,STHDRPTR)                             0311
         LTR   @15,@15                                             0311
         BNZ   @DL00296                                            0311
*                                                                  0312
*     /***************************************************************/
*     /*                                                             */
*     /* IF THE VOLUME USE COUNT IS NOT 0, TURN OFF THE 'NOT @G24LB18*/
*     /* MOUNTED' AND 'MOUNTED TO OTHER HOST' INDICATORS IN  @G24LB18*/
*     /* THE BEST FIT TABLE ELEMENT AND INCREMENT THE        @G24LB18*/
*     /* CONCURRENT USER COUNT IN THE ELEMENT BY THE USE     @G24LB18*/
*     /* COUNT.                                              @G24LB18*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0312
*     IF USECOUNT>ZERO THEN         /* VOL USE COUNT > 0?    @G24LB18*/
         CLI   USECOUNT,0                                          0312
         BNH   @RF00312                                            0312
*       DO;                         /* YES                   @G24LB18*/
*         BFNMTDFL(INDEX)=OFF;      /* TURN OFF NOT MOUNTED  @G24LB18*/
*         BFMTDOHF(INDEX)=OFF;      /* TURN OFF MTD OTH HOST @G24LB18*/
         LR    @15,INDEX                                           0315
         MH    @15,@CH01134                                        0315
         L     @11,TABBFTBL(,PARMPTR)                              0315
         ST    @15,@TF00001                                        0315
         ALR   @15,@11                                             0315
         NI    BFNMTDFL-18(@15),B'10011111'                        0315
*         BFCONUSE(INDEX)=BFCONUSE(INDEX)+USECOUNT;/*        @G24LB18*/
         L     @15,@TF00001                                        0316
         SLR   @10,@10                                             0316
         IC    @10,BFCONUSE-18(@15,@11)                            0316
         SLR   @15,@15                                             0316
         IC    @15,USECOUNT                                        0316
         ALR   @10,@15                                             0316
         L     @15,@TF00001                                        0316
         STC   @10,BFCONUSE-18(@15,@11)                            0316
*       END;                        /* INCREMENT CONC USERS  @G24LB18*/
*                                                                  0317
*     /***************************************************************/
*     /*                                                             */
*     /* INCREMENT THE BEST FIT TABLE ELEMENT INDEX AND      @G24LB18*/
*     /* REPEAT THE STEP TABLE LOOP. CONTINUE UNTIL ALL      @G24LB18*/
*     /* ELEMENTS IN THE BEST FIT TABLE HAVE BEEN PROCESSED. @G24LB18*/
*     /* THEN RETURN TO MAINLINE ICBVTP00.                   @G24LB18*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0318
*     INDEX=INDEX+ONE;              /* INCREMENT EL INDEX    @G24LB18*/
@RF00312 AL    INDEX,@CF00052                                      0318
*   END;                            /* END ELEMENT LOOP      @G24LB18*/
@DE00293 L     @15,TABBFTBL(,PARMPTR)                              0319
         CH    INDEX,BFTOTIDX(,@15)                                0319
         BNH   @DL00293                                            0319
*   REG14=SAVERET1;                 /* RESTORE RETURN ADDR   @G24LB18*/
         L     REG14,SAVERET1                                      0320
*   RETURN;                         /* RETURN TO ICBVTP00    @G24LB18*/
@EL00007 DS    0H                                                  0321
@EF00007 DS    0H                                                  0321
@ER00007 BR    @14                                                 0321
*   END UPDTBFTB;                   /* END BEST FIT TAB RTN  @G24LB18*/
         B     @EL00007                                            0322
*                                                                  0323
*/*********************************************************  @G24LB18*/
*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - VOLUSAGE                                @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS ROUTINE IS CALLED ON UPDATE BEST FIT     @G24LB18*/
*/*            TABLE REQUESTS WHEN A DD ENTRY HAS BEEN FOUND @G24LB18*/
*/*            WITH A VOLSER WHICH MATCHES THE VOLSER IN THE @G24LB18*/
*/*            BEST FIT TABLE ELEMENT BEING PROCESSED. THE   @G24LB18*/
*/*            ROUTINE INCREMENTS THE USE COUNT FOR THE      @G24LB18*/
*/*            VOLUME, DETERMINES WHETHER IT IS A WAIT       @G24LB18*/
*/*            VOLUME OR A SHAREABILITY CONSIDERATION VOLUME @G24LB18*/
*/*            AND SETS THE APPROPRIATE FLAGS IN THE BEST    @G24LB18*/
*/*            FIT TABLE ELEMENT.                            @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - BEST FIT TABLE, BEST FIT TABLE ELEMENT, STEP     @G24LB18*/
*/*         TABLE HEADER, AND DD ENTRY.                      @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - VOLUME USE COUNT AND UPDATED BEST FIT TABLE     @G24LB18*/
*/*          ELEMENT.                                        @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0323
*VOLUSAGE:                                                         0323
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
*                                                                  0323
VOLUSAGE DS    0H                                                  0324
*   /*****************************************************************/
*   /*                                                               */
*   /* INCREMENT THE VOLUME USE COUNT BY 1. DETERMINE        @G24LB18*/
*   /* WHETHER THE DD ENTRY IS FROM THE REQUESTING JOB STEP. @G24LB18*/
*   /* IF NOT, TEST TO SEE IF THE VOLUME IS A WAIT VOLUME,   @G24LB18*/
*   /* MARK IT ACCORDINGLY IN THE BEST FIT TABLE ELEMENT,    @G24LB18*/
*   /* AND RETURN TO UPDTBFTB.                               @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0324
*   USECOUNT=USECOUNT+ONE;          /* INCREMENT USE COUNT   @G24LB18*/
         LA    @15,1                                               0324
         SLR   @11,@11                                             0324
         IC    @11,USECOUNT                                        0324
         ALR   @15,@11                                             0324
         STC   @15,USECOUNT                                        0324
*   IF STPJOBID^=BFJOBID THEN       /* DD IN CURRENT STEP?   @G24LB18*/
         L     @15,TABBFTBL(,PARMPTR)                              0325
         CLC   STPJOBID(4,STHDRPTR),BFJOBID(@15)                   0325
         BE    @RF00325                                            0325
*     DO;                           /* NO                    @G24LB18*/
*       IF BFWAITFL(INDEX)=ON THEN  /* WAIT VOLUME IND ON?   @G24LB18*/
         LR    @11,INDEX                                           0327
         MH    @11,@CH01134                                        0327
         ALR   @11,@15                                             0327
         TM    BFWAITFL-18(@11),B'01000000'                        0327
         BO    @RT00327                                            0327
*         RETURN;                   /* YES-RETURN TO UPDTBFT @G24LB18*/
*                                                                  0328
*       /*************************************************************/
*       /*                                                           */
*       /* IF THE NON-SHAREABLE REQUEST FLAG IS ON IN THE    @G24LB18*/
*       /* BEST FIT TABLE OR THE EXCLUSIVE USE FLAG IS ON IN @G24LB18*/
*       /* THE DD ENTRY OR THE INTERNAL DEVICE TYPE FROM THE @G24LB18*/
*       /* BEST FIT TABLE DOES NOT MATCH THAT IN THE DD      @G24LB18*/
*       /* ENTRY, TURN ON THE 'WAIT VOLUME' INDICATOR IN THE @G24LB18*/
*       /* BEST FIT TABLE ELEMENT AND RETURN TO UPDTBFTB.    @G24LB18*/
*       /* OTHERWISE, JUST RETURN TO UPDTBFTB.               @G24LB18*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0329
*       IF BFNSHRFL=ON×             /* REQ NON-SHARE OR      @G24LB18*/
*           STPDDEXC=ON×            /* DD ENT EXCLUSIVE OR   @G24LB18*/
*           BFDEVTP^=STPDDDVT THEN  /* DEV TYPES NOT = ?     @G24LB18*/
         TM    BFNSHRFL(@15),B'10000000'                           0329
         BO    @RT00329                                            0329
         TM    STPDDEXC(DDENTPTR),B'10000000'                      0329
         BO    @RT00329                                            0329
         CLC   BFDEVTP(4,@15),STPDDDVT(DDENTPTR)                   0329
         BE    @RF00329                                            0329
@RT00329 DS    0H                                                  0330
*         BFWAITFL(INDEX)=ON;       /* YES-SET WAIT VOLUME   @G24LB18*/
         LR    @15,INDEX                                           0330
         MH    @15,@CH01134                                        0330
         L     @01,TABBFTBL(,PARMPTR)                              0330
         ALR   @01,@15                                             0330
         OI    BFWAITFL-18(@01),B'01000000'                        0330
*       RETURN;                     /* RETURN TO UPDTBFTB    @G24LB18*/
@EL00008 DS    0H                                                  0331
@EF00008 DS    0H                                                  0331
@ER00008 BR    @14                                                 0331
*     END;                          /*                       @G24LB18*/
*                                                                  0332
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE DD ENTRY IS FROM THE REQUESTING JOB STEP, TEST @G24LB18*/
*   /* TO SEE IF THE VOLUME IS A SHAREABILITY CONSIDERATION  @G24LB18*/
*   /* VOLUME, MARK IT ACCORDINGLY IN THE BEST FIT TABLE     @G24LB18*/
*   /* ELEMENT, AND RETURN TO UPDTBFTB.                      @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0333
*   ELSE                            /* DD IN CURRENT STEP?   @G24LB18*/
*     DO;                           /* YES                   @G24LB18*/
*                                                                  0333
@RF00325 DS    0H                                                  0334
*       /*************************************************************/
*       /*                                                           */
*       /* IF THE NON-SHAREABLE REQUEST FLAG IS ON IN THE    @G24LB18*/
*       /* BEST FIT TABLE OR THE EXCLUSIVE USE FLAG IS ON    @G24LB18*/
*       /* IN THE DD ENTRY, TURN ON THE 'SHAREABILITY        @G24LB18*/
*       /* CONSIDERATION' INDICATOR IN THE BEST FIT TABLE    @G24LB18*/
*       /* ELEMENT AND RETURN TO UPDTBFTB. OTHERWISE, JUST   @G24LB18*/
*       /* RETURN TO UPDTBFTB.                               @G24LB18*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0334
*       IF BFNSHRFL=ON×             /* REQ NON-SHARE OR      @G24LB18*/
*           STPDDEXC=ON THEN        /* DD ENTRY EXCLUSIVE?   @G24LB18*/
         L     @15,TABBFTBL(,PARMPTR)                              0334
         TM    BFNSHRFL(@15),B'10000000'                           0334
         BO    @RT00334                                            0334
         TM    STPDDEXC(DDENTPTR),B'10000000'                      0334
         BNO   @RF00334                                            0334
@RT00334 DS    0H                                                  0335
*         BFSHCNFL(INDEX)=ON;       /* YES-SET SHARE CONSID  @G24LB18*/
         LR    @15,INDEX                                           0335
         MH    @15,@CH01134                                        0335
         L     @01,TABBFTBL(,PARMPTR)                              0335
         ALR   @01,@15                                             0335
         OI    BFSHCNFL-18(@01),B'10000000'                        0335
*       RETURN;                     /* RETURN TO UPDTBFTB    @G24LB18*/
         B     @EL00008                                            0336
*     END;                          /*                       @G24LB18*/
*   END VOLUSAGE;                   /* END VOL USAGE RTN     @G24LB18*/
         B     @EL00008                                            0338
*                                                                  0339
*/*********************************************************  @G24LB18*/
*/*                                                          @G24LB18*/
*/* PROCEDURE NAME - TOTSPACE                                @G24LB18*/
*/*                                                          @G24LB18*/
*/* FUNCTION - THIS ROUTINE IS CALLED ON GET TOTAL SPACE FOR @G24LB18*/
*/*            VOL SER REQUESTS. IT SEARCHES THE STEP TABLE  @G24LB18*/
*/*            CHAIN FOR THE VOL SER THAT IS PASSED WITH THE @G24LB18*/
*/*            REQUEST, AND KEEPS A RUNNING TOTAL OF THE     @G24LB18*/
*/*            PRIMARY SPACE RECORDED IN THE DD ENTRIES      @G24LB18*/
*/*            FOUND FOR THAT VOL SER.                       @G24LB18*/
*/*                                                          @G24LB18*/
*/* INPUT - TABLE PROCESSOR REQUEST PARAMETER LIST, VVICB    @G24LB18*/
*/*                                                          @G24LB18*/
*/* OUTPUT - THE TOTAL SPACE FOUND FOR THE VOL SER IS        @G24LB18*/
*/*          RETURNED IN THE TABP PARM LIST.                 @G24LB18*/
*/*                                                          @G24LB18*/
*/*********************************************************  @G24LB18*/
*                                                                  0339
*TOTSPACE:                                                         0339
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/
*                                                                  0339
TOTSPACE DS    0H                                                  0340
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE TOTAL SPACE TO 0. GET THE ADDRESS OF   @G24LB18*/
*   /* THE STEP TABLE CHAIN FROM THE VVICB. IF IT IS 0,      @G24LB18*/
*   /* STORE THE TOTAL SPACE (=0) IN THE TABP PARM LIST AND  @G24LB18*/
*   /* RETURN TO MAINLINE ICBVTP00. OTHERWISE GET THE FIRST  @G24LB18*/
*   /* STEP TABLE HEADER.                                    @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0340
*   TPSPACE=ZERO;                   /* INIT TOTAL SPACE = 0  @G24LB18*/
         SLR   TPSPACE,TPSPACE                                     0340
*   STHDRPTR=ADDR(VVISTPTB);        /* GET STEP TABLE CHAIN  @G24LB18*/
         L     STHDRPTR,VVIPTR                                     0341
         LA    STHDRPTR,VVISTPTB(,STHDRPTR)                        0341
*   DO WHILE STPNXTTB^=ZERO;        /* DO FOR EACH HEADER    @G24LB18*/
         B     @DE00342                                            0342
@DL00342 DS    0H                                                  0343
*     STHDRPTR=STPNXTTB;            /* GET STEP TAB HEADER   @G24LB18*/
*                                                                  0343
         L     STHDRPTR,STPNXTTB(,STHDRPTR)                        0343
*     /***************************************************************/
*     /*                                                             */
*     /* IF THE STEP TABLE IS NOT INVALID, GET THE FIRST DD  @G24LB18*/
*     /* ENTRY.                                              @G24LB18*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0344
*     IF STPINVAL=OFF THEN          /* STEP TABLE INVALID?   @G24LB18*/
         TM    STPINVAL(STHDRPTR),B'10000000'                      0344
         BNZ   @RF00344                                            0344
*       DO;                         /* NO                    @G24LB18*/
*         DDENTPTR=ADDR(STPDDPTR);  /* GET DD ENTRY CHAIN    @G24LB18*/
         LA    DDENTPTR,STPDDPTR(,STHDRPTR)                        0346
*         DO WHILE STPDDNXT^=ZERO;  /* DO FOR EACH DD ENTRY  @G24LB18*/
         B     @DE00347                                            0347
@DL00347 DS    0H                                                  0348
*           DDENTPTR=STPDDNXT;      /* GET DD ENTRY          @G24LB18*/
         L     DDENTPTR,STPDDNXT(,DDENTPTR)                        0348
*                                                                  0349
*           /*********************************************************/
*           /*                                                       */
*           /* IF THE VOLSER FROM THE PARM LIST = THE FIRST  @G24LB18*/
*           /* VOLSER IN THE DD ENTRY, ADD THE PRIMARY SPACE @G24LB18*/
*           /* FROM THE DD ENTRY TO THE TOTAL SPACE.         @G24LB18*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0349
*           IF STPDDVOL(ONE)=TABVOLID THEN/* VOLSERS = ?     @G24LB18*/
         CLC   STPDDVOL(6,DDENTPTR),TABVOLID(PARMPTR)              0349
         BNE   @RF00349                                            0349
*             TPSPACE=TPSPACE+STPDDPRM;/* ADD SPACE TO TOT   @G24LB18*/
*                                                                  0350
         AH    TPSPACE,STPDDPRM(,DDENTPTR)                         0350
*           /*********************************************************/
*           /*                                                       */
*           /* GET THE NEXT DD ENTRY AND REPEAT UNTIL ALL DD @G24LB18*/
*           /* ENTRIES IN THE STEP TABLE HAVE BEEN PROCESSED.        */
*           /*                                               @G24LB18*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0351
*         END;                      /* END DD ENTRY LOOP     @G24LB18*/
@RF00349 DS    0H                                                  0351
@DE00347 L     @15,STPDDNXT(,DDENTPTR)                             0351
         LTR   @15,@15                                             0351
         BNZ   @DL00347                                            0351
*       END;                        /*                       @G24LB18*/
*                                                                  0352
*     /***************************************************************/
*     /*                                                             */
*     /* GET THE NEXT STEP TABLE HEADER AND REPEAT THE DD    @G24LB18*/
*     /* ENTRY LOOP. CONTINUE UNTIL ALL STEP TABLES IN THE   @G24LB18*/
*     /* STEP TABLE CHAIN HAVE BEEN PROCESSED.               @G24LB18*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0353
*   END;                            /*                       @G24LB18*/
*                                                                  0353
@RF00344 DS    0H                                                  0353
@DE00342 L     @15,STPNXTTB(,STHDRPTR)                             0353
         LTR   @15,@15                                             0353
         BNZ   @DL00342                                            0353
*   /*****************************************************************/
*   /*                                                               */
*   /* STORE THE TOTAL SPACE INTO THE TABP PARM LIST AND     @G24LB18*/
*   /* RETURN TO MAINLINE ICBVTP00.                          @G24LB18*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0354
*   TABSPRET=TPSPACE;               /* STORE TOTAL SPACE     @G24LB18*/
         STH   TPSPACE,TABSPRET(,PARMPTR)                          0354
*   RETURN;                         /* RETURN TO ICBVTP00    @G24LB18*/
@EL00009 DS    0H                                                  0355
@EF00009 DS    0H                                                  0355
@ER00009 BR    @14                                                 0355
