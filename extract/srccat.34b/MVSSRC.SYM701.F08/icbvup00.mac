UP00     TITLE '**********  ICBVUP00:  MSVC UNIT PROCESSOR MODULE  *****
               *******  '
ICBVUP00 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ICBVUP00  76.279'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         MVC   @PC00001(8),0(@01)                                  0001
*   VVIPTR=ADDR(VVICBLK);           /*                       @G24LB37*/
         L     VVIPTR,@PC00001+4                                   0069
*   UNITPTR=ADDR(UNITRPL);          /*                       @G24LB37*/
         L     UNITPTR,@PC00001                                    0070
*   RETCODE=FOUR;                   /*                       @G24LB37*/
*                                                                  0071
         LA    RETCODE,4                                           0071
*   /*****************************************************************/
*   /*                                                               */
*   /* IF, IN THE UNITP, THE UNIFMTD FLAG IS ON, REQUESTING  @G24LB37*/
*   /* THE UNIT PROCESSOR TO DETERMINE IF THE GIVEN VOLUME   @G24LB37*/
*   /* SERIAL IS MOUNTED ON A 3330V UNIT, WITHIN THE SYSTEM, @G24LB37*/
*   /* CALL SCANUCBS.                                        @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0072
*   IF UNIFMTD=ON THEN                                             0072
         TM    UNIFMTD(UNITPTR),B'10000000'                        0072
         BNO   @RF00072                                            0072
*     CALL SCANUCBS;                /*                       @G24LB37*/
*                                                                  0073
         BAL   @14,SCANUCBS                                        0073
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE UNIFINCL FLAG IS ON, THEN CALL UNITINCL TO     @G24LB37*/
*   /* DETERMINE IF THE SPECIFIED UCB ADDRESS IS INCLUDED    @G24LB37*/
*   /* OR NOT INCLUDED WITHIN THE SPECIFIED DEVICE TYPE.     @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0074
*   ELSE                                                           0074
*     IF UNIFINCL=ON THEN                                          0074
         B     @RC00072                                            0074
@RF00072 TM    UNIFINCL(UNITPTR),B'01000000'                       0074
         BNO   @RF00074                                            0074
*       CALL UNITINCL;              /*                       @G24LB37*/
*                                                                  0075
         BAL   @14,UNITINCL                                        0075
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE UNIFALLV FLAG IS ON, THEN CALL ALLVIRT TO      @G24LB37*/
*   /* DETERMINE IF THE SPECIFIED DEVICE TYPE IS ALL 3330V   @G24LB37*/
*   /* UNITS.                                                @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0076
*     ELSE                                                         0076
*       IF UNIFALLV=ON THEN                                        0076
         B     @RC00074                                            0076
@RF00074 TM    UNIFALLV(UNITPTR),B'00100000'                       0076
         BNO   @RF00076                                            0076
*         CALL ALLVIRT;             /*                       @G24LB37*/
         BAL   @14,ALLVIRT                                         0077
*                                                                  0078
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO CALLER. RETURN CODE IS RETURNED IN REG 15.  @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0078
*   RETURN CODE(RETCODE);           /* RETURN TO CALLER      @G24LB37*/
@RF00076 DS    0H                                                  0078
@RC00074 DS    0H                                                  0078
@RC00072 LR    @15,RETCODE                                         0078
         L     @13,4(,@13)                                         0078
         L     @14,12(,@13)                                        0078
         LM    @00,@12,20(@13)                                     0078
         BR    @14                                                 0078
*                                                                  0079
*/********************************************************************/
*/*                                                                  */
*/*     PROCEDURE NAME - SCANUCBS                            @G24LB37*/
*/*                                                          @G24LB37*/
*/*     FUNCTION - THIS PROCEDURE SCANS THE 3330V UCB TABLE  @G24LB37*/
*/*                BUILT BY MSVC FOR THE UCB THAT CONTAINS   @G24LB37*/
*/*                THE VOLUME SERIAL IN THE UCBVOLI FIELD    @G24LB37*/
*/*                WHICH MATCHES THE ONE PASSED BY THE       @G24LB37*/
*/*                CALLER.                                   @G24LB37*/
*/*                                                          @G24LB37*/
*/*     INPUT - THE VOLUME SERIAL TO SEARCH FOR IN THE UCBS. @G24LB37*/
*/*             THE POINTER TO THE MSVC UCB LOOK UP TABLE    @G24LB37*/
*/*             IN THE VVICB.                                @G24LB37*/
*/*                                                          @G24LB37*/
*/*     OUTPUT -   THE ADDRESS OF THE UCB THAT THE VOLUME    @G24LB37*/
*/*                WAS FOUND MOUNTED ON AND/OR A RETURN      @G24LB37*/
*/*                CODE IN RETCODE AREA AS FOLLOWS:          @G24LB37*/
*/*                0 = SUCCESSFUL                            @G24LB37*/
*/*                4 = NOT FOUND                             @G24LB37*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0079
*SCANUCBS:                                                         0079
*   PROCEDURE OPTIONS(NOSAVE);      /*                       @G24LB37*/
SCANUCBS DS    0H                                                  0080
*                                                                  0080
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE AND ESTABLISH ADDRESSIBILITY TO THE MSVC   @G24LB37*/
*   /* 3330V UCB ADDRESS TABLE USING THE VVIUCB FIELD IN THE @G24LB37*/
*   /* VVICB CONTROL BLOCK. TURN FOUNDSW OFF.                @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0080
*   MSVLKPTR=VVIUCB;                /*                       @G24LB37*/
         L     MSVLKPTR,VVIUCB(,VVIPTR)                            0080
*   FOUNDSW=OFF;                    /*                       @G24LB37*/
*                                                                  0081
         NI    FOUNDSW,B'01111111'                                 0081
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN THE MSVC 3330V UCB ADDRESS TABLE, TAKE EACH UCB  @G24LB37*/
*   /* ADDRESS AND CHECK THE UCBVOLI FIELD FOR THE MATCHING  @G24LB37*/
*   /* VOLSER THAT WAS PASSED IN THE UNITP. ADD A PLUS FOUR  @G24LB37*/
*   /* TO THE ADDRESS OF THE TABLE TO INCREMENT TO THE NEXT  @G24LB37*/
*   /* UCB ADDRESS ENTRY. IF THE UCB ADDRESS IS ALL HEX      @G24LB37*/
*   /* FOXES THEN THE END OF THE TABLE HAS BEEN REACHED.     @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0082
*   DO WHILE(MSVUCBP^=UCBDELIM)&(FOUNDSW=OFF);/*             @G24LB37*/
         B     @DE00082                                            0082
@DL00082 DS    0H                                                  0083
*     UCBPTR=MSVUCBP;               /*                       @G24LB37*/
         L     UCBPTR,MSVUCBP(,MSVLKPTR)                           0083
*     IF UCBVOLI=UNIVOL THEN                                       0084
         CLC   UCBVOLI(6,UCBPTR),UNIVOL(UNITPTR)                   0084
         BNE   @RF00084                                            0084
*       DO;                         /*                       @G24LB37*/
*                                                                  0085
*         /***********************************************************/
*         /*                                                         */
*         /* IF THE VOLSER IS FOUND TO BE MOUNTED SET THE    @G24LB37*/
*         /* RETURN CODE TO ZERO AND INSERT THE UCB ADDRESS  @G24LB37*/
*         /* INTO THE UNITP FIELD UNIUCBP, AND TURN ON THE   @G24LB37*/
*         /* FOUND SWITCH. IF THE VOLSER IS NOT FOUND        @G24LB37*/
*         /* (END OF TABLE REACHED), THEN THE RETURN CODE    @G24LB37*/
*         /* IS ALREADY SET TO FOUR.                         @G24LB37*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0086
*         FOUNDSW=ON;               /*                       @G24LB37*/
         OI    FOUNDSW,B'10000000'                                 0086
*         UNIUCBP=UCBPTR;           /*                       @G24LB37*/
         ST    UCBPTR,UNIUCBP(,UNITPTR)                            0087
*         RETCODE=ZERO;             /*                       @G24LB37*/
         SLR   RETCODE,RETCODE                                     0088
*       END;                        /*                       @G24LB37*/
*     ELSE                                                         0090
*       MSVLKPTR=MSVLKPTR+FOUR;     /*                       @G24LB37*/
         B     @RC00084                                            0090
@RF00084 AL    MSVLKPTR,@CF00039                                   0090
*   END;                            /*                       @G24LB37*/
*                                                                  0091
@RC00084 DS    0H                                                  0091
@DE00082 CLC   MSVUCBP(4,MSVLKPTR),UCBDELIM                        0091
         BE    @DC00082                                            0091
         TM    FOUNDSW,B'10000000'                                 0091
         BZ    @DL00082                                            0091
@DC00082 DS    0H                                                  0092
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN CODE IS SET IN RETCODE AREA.                   @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0092
*   RETURN;                         /* RET TO MAIN ROUTINE   @G24LB37*/
@EL00002 DS    0H                                                  0092
@EF00002 DS    0H                                                  0092
@ER00002 BR    @14                                                 0092
*   END SCANUCBS;                   /* END OF SCANUCBS PROC  @G24LB37*/
*                                                                  0094
*/********************************************************************/
*/*                                                                  */
*/*     PROCEDURE NAME - ALLVIRT                             @G24LB37*/
*/*                                                          @G24LB37*/
*/*     FUNCTION - THIS PROCEDURE DETERMINES IF THE PASSED   @G24LB37*/
*/*                DEVICE TYPE HAS ANY UCB'S WITHIN THE      @G24LB37*/
*/*                ESOTERIC GROUP THAT ARE NOT 3330V         @G24LB37*/
*/*                DEVICES (I.E. MIXED ESOTERIC).            @G24LB37*/
*/*                                                          @G24LB37*/
*/*     INPUT - THE DEVICE TYPE TO SEARCH FOR, PASSED IN THE @G24LB37*/
*/*             UNITP.  THE DEVICE MASK TABLE POINTED TO BY  @G24LB37*/
*/*             THE VVICB FIELD VVIDVMTA, AND THE LENGTH OF  @G24LB37*/
*/*             A DEVICE MASK ENTRY IN VVICB FIELD VVIDVMEL. @G24LB37*/
*/*                                                          @G24LB37*/
*/*     OUTPUT -   RETURN CODE IS RETURNED IN RETCODE AREA   @G24LB37*/
*/*                AS FOLLOWS:                               @G24LB37*/
*/*                0 = DEVICE TYPE IS ALL 3330V'S            @G24LB37*/
*/*                4 = DEVICE TYPE HAS AT LEAST ONE NON-     @G24LB37*/
*/*                    3330V UNIT                            @G24LB37*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0094
*ALLVIRT:                                                          0094
*   PROCEDURE OPTIONS(NOSAVE);      /*                       @G24LB37*/
ALLVIRT  DS    0H                                                  0095
*                                                                  0095
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE AND ESTABLISH ADDRESSIBILITY TO THE DEVICE @G24LB37*/
*   /* MASK TABLE USING THE VVIDVMTA FIELD IN THE VVICB      @G24LB37*/
*   /* CONTROL BLOCK. TAKE THE NUMBER OF WORDS IN VVIDVMEL,  @G24LB37*/
*   /* ADD TWO TO THAT TOTAL FOR THE TWO EXTRA WORDS IN A    @G24LB37*/
*   /* ENTRY AND MULTIPLY BY 4 TO GET THE NUMBER OF BYTES.   @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0095
*   DVMSKPTR=VVIDVMTA;              /*                       @G24LB37*/
         L     DVMSKPTR,VVIDVMTA(,VVIPTR)                          0095
*   DVMSKMLN=VVIDVMEL*FOUR;         /*                       @G24LB37*/
         L     @11,VVIDVMEL(,VVIPTR)                               0096
         LR    DVMSKMLN,@11                                        0096
         SLA   DVMSKMLN,2                                          0096
*   DVMSKELN=(VVIDVMEL+TWO)*FOUR;   /*                       @G24LB37*/
         AL    @11,@CF00043                                        0097
         SLA   @11,2                                               0097
         LR    DVMSKELN,@11                                        0097
*   DVMSKELP=ADDR(DEVMSKEL);        /*                       @G24LB37*/
         LA    DVMSKELP,DEVMSKEL(,DVMSKPTR)                        0098
*   FOUNDSW=OFF;                    /*                       @G24LB37*/
*   EXITSW=OFF;                     /*                       @G24LB37*/
         NI    FOUNDSW,B'00111111'                                 0100
*   SAV3330P=ZERO;                  /*                       @G24LB37*/
*                                                                  0101
         SLR   SAV3330P,SAV3330P                                   0101
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN THE DEVICE MASK TABLE FOR THE 3330V DEVICE MASK  @G24LB37*/
*   /* USING THE CONSTANT '30582009'X DEVICE TYPE. IF THE    @G24LB37*/
*   /* DEVICE TYPE IS FOUND, SAVE THE ADDRESS. ALSO,         @G24LB37*/
*   /* TAKE THE PASSED DEVICE TYPE AND FIND ITS CORRESPOND-  @G24LB37*/
*   /* ING DEVICE BIT MASK, IN THE DEVICE MASK TABLE.        @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0102
*   DO X=ONE TO DEVNUMEL WHILE EXITSW=OFF;/*                 @G24LB37*/
         LA    X,1                                                 0102
         B     @DE00102                                            0102
@DL00102 TM    EXITSW,B'01000000'                                  0102
         BNZ   @DC00102                                            0102
*     IF DEVTYPE=DEV3330V THEN                                     0103
         CLC   DEVTYPE(4,DVMSKELP),DEV3330V                        0103
         BNE   @RF00103                                            0103
*       SAV3330P=DVMSKELP;          /*                       @G24LB37*/
         LR    SAV3330P,DVMSKELP                                   0104
*     IF DEVTYPE=UNIDEVTP THEN                                     0105
@RF00103 CLC   DEVTYPE(4,DVMSKELP),UNIDEVTP(UNITPTR)               0105
         BNE   @RF00105                                            0105
*       DO;                         /*                       @G24LB37*/
*         FOUNDSW=ON;               /*                       @G24LB37*/
         OI    FOUNDSW,B'10000000'                                 0107
*         SAVDEVTP=DVMSKELP;        /*                       @G24LB37*/
         ST    DVMSKELP,SAVDEVTP                                   0108
*       END;                        /*                       @G24LB37*/
*     IF SAV3330P^=ZERO&FOUNDSW=ON THEN                            0110
@RF00105 LTR   SAV3330P,SAV3330P                                   0110
         BZ    @RF00110                                            0110
         TM    FOUNDSW,B'10000000'                                 0110
         BNO   @RF00110                                            0110
*       EXITSW=ON;                  /*                       @G24LB37*/
         OI    EXITSW,B'01000000'                                  0111
*     DVMSKELP=DVMSKELP+DVMSKELN;   /*                       @G24LB37*/
@RF00110 ALR   DVMSKELP,DVMSKELN                                   0112
*   END;                            /*                       @G24LB37*/
         AL    X,@CF00048                                          0113
@DE00102 CH    X,DEVNUMEL(,DVMSKPTR)                               0113
         BNH   @DL00102                                            0113
@DC00102 DS    0H                                                  0114
*                                                                  0114
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE FOUND SWITCH IS ON, THEN 'AND' THE TWO MASKS   @G24LB37*/
*   /* TOGETHER AND CHECK THE RESULT FOR AN EQUAL CONDITION. @G24LB37*/
*   /* IF THE RESULT IS EQUAL THEN THE DEVICE TYPE IS NOT    @G24LB37*/
*   /* MIXED. SET THE RETURN CODE TO ZERO IF EQUAL, ELSE,    @G24LB37*/
*   /* SET THE RETURN CODE TO EIGHT, INDICATING THE DEVICE   @G24LB37*/
*   /* TYPE PASSED IS NOT ALL 3330V UNITS.                   @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0114
*   IF FOUNDSW=ON THEN                                             0114
         TM    FOUNDSW,B'10000000'                                 0114
         BNO   @RF00114                                            0114
*     DO;                           /*                       @G24LB37*/
*       DVMSKELP=SAVDEVTP;          /*                       @G24LB37*/
         L     DVMSKELP,SAVDEVTP                                   0116
*       WORKMASK(ONE:DVMSKMLN)=DEVMASK(ONE:DVMSKMLN)&SAVMASK(ONE:  0117
*           DVMSKMLN);              /*                       @G24LB37*/
         LR    @06,DVMSKMLN                                        0117
         BCTR  @06,0                                               0117
         EX    @06,@SM01249                                        0117
         EX    @06,@SN01251                                        0117
*       IF WORKMASK(ONE:DVMSKMLN)=DEVMASK(ONE:DVMSKMLN) THEN       0118
         EX    @06,@SC01253                                        0118
         BNE   @RF00118                                            0118
*         RETCODE=ZERO;             /*                       @G24LB37*/
         SLR   RETCODE,RETCODE                                     0119
*       ELSE                                                       0120
*         RETCODE=EIGHT;            /*                       @G24LB37*/
         B     @RC00118                                            0120
@RF00118 LA    RETCODE,8                                           0120
*     END;                          /*                       @G24LB37*/
*   ELSE                                                           0122
*     RETCODE=EIGHT;                /*                       @G24LB37*/
         B     @RC00114                                            0122
@RF00114 LA    RETCODE,8                                           0122
*   RETURN;                         /* RET TO MAIN ROUTINE   @G24LB37*/
@EL00003 DS    0H                                                  0123
@EF00003 DS    0H                                                  0123
@ER00003 BR    @14                                                 0123
*   END ALLVIRT;                    /* END OF ALLVIRT PROC   @G24LB37*/
         B     @EL00003                                            0124
*                                                                  0125
*/********************************************************************/
*/*                                                                  */
*/*     PROCEDURE NAME - UNITINCL                            @G24LB37*/
*/*                                                          @G24LB37*/
*/*     FUNCTION - THIS PROCEDURE DETERMINES IF THE PASSED   @G24LB37*/
*/*                UCB ADDRESS IS INCLUDED WITHIN THE SPEC-  @G24LB37*/
*/*                IFIED DEVICE TYPE, CALLS SCANLKUP TO SCAN @G24LB37*/
*/*                THE UCB LOOK UP TABLE FOR THE PASSED UCB  @G24LB37*/
*/*                ADDRESS, AND TO COUNT ITS OFFSET WITHIN   @G24LB37*/
*/*                THE LOOK UP TABLE.  IF THE SCAN IS SUC-   @G24LB37*/
*/*                CESSFUL, THEN CALLS THE SCANMASK PROC-    @G24LB37*/
*/*                EDURE TO DETERMINE BY SCANNING THE DEVICE @G24LB37*/
*/*                MASK TABLE IF THE UCB OFFSET BIT IN THE   @G24LB37*/
*/*                MASK IS ON FOR THAT DEVICE TYPE.          @G24LB37*/
*/*                                                          @G24LB37*/
*/*     INPUT - THE DEVICE TYPE AND THE UCB ADDRESS OF THE   @G24LB37*/
*/*             UNIT TO DETERMINE IF INCLUDED, PASSED IN THE @G24LB37*/
*/*             FIELDS UNIDEVTP AND UNIUCBP IN THE UNITP.    @G24LB37*/
*/*                                                          @G24LB37*/
*/*     OUTPUT -   RETURN CODE IS RETURNED IN RETCODE AREA   @G24LB37*/
*/*                AS FOLLOWS:                               @G24LB37*/
*/*                0 = UCB ADDRESS IS INCLUDED WITHIN THE    @G24LB37*/
*/*                    SPECIFIED DEVICE TYPE                 @G24LB37*/
*/*                4 = UCB ADDRESS IS NOT INCLUDED WITHIN    @G24LB37*/
*/*                    THE SPECIFIED DEVICE TYPE, OR THE     @G24LB37*/
*/*                    SPECIFIED DEVICE TYPE OR UCB ADDRESS  @G24LB37*/
*/*                    WAS NOT FOUND                         @G24LB37*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0125
*UNITINCL:                                                         0125
*   PROCEDURE OPTIONS(SAVE(REG14)); /*                       @G24LB37*/
UNITINCL ST    @14,@SA00004                                        0125
*                                                                  0126
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE AND ESTABLISH ADDRESSIBILITY TO THE UCB    @G24LB37*/
*   /* BY SETTING THE PASSED UCB ADDRESS INTO THE UCBPTR.    @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0126
*   UCBPTR=UNIUCBP;                 /*                       @G24LB37*/
*                                                                  0126
         L     UCBPTR,UNIUCBP(,UNITPTR)                            0126
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL THE SCANLKUP PROCEDURE TO SCAN THE SYSTEM UCB    @G24LB37*/
*   /* LOOK UP TABLE TO FIND THE MATCHING UCB ADDRESS AS     @G24LB37*/
*   /* THE ONE PASSED, TO COMPARE AGAINST THE DEVICE MASK    @G24LB37*/
*   /* TABLE. IF THE UCB ADDRESS IS NOT FOUND, A RETURN      @G24LB37*/
*   /* CODE OF FOUR WILL BE RETURNED. IF FOUND A COUNTER     @G24LB37*/
*   /* (UCBCNTR) WILL BE SET INDICATING WHAT POSITION WITHIN @G24LB37*/
*   /* THE LOOK UP TABLE THE ADDRESS WAS FOUND (1ST, 2ND,    @G24LB37*/
*   /* ETC.), AND A RETURN CODE OF ZERO.                     @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0127
*   CALL SCANLKUP;                  /*                       @G24LB37*/
*                                                                  0127
         BAL   @14,SCANLKUP                                        0127
*   /*****************************************************************/
*   /*                                                               */
*   /* IF RETURN CODE IS ZERO, CALL THE SCANMASK PROCEDURE   @G24LB37*/
*   /* TO DETERMINE IF THE UCB IS INCLUDED WITHIN THE        @G24LB37*/
*   /* SPECIFIED DEVICE TYPE. A RETURN CODE OF ZERO WILL BE  @G24LB37*/
*   /* RETURNED IF INCLUDED, OR A RETURN CODE OF FOUR, IF    @G24LB37*/
*   /* NOT INCLUDED.                                         @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0128
*   IF RETCODE=ZERO THEN                                           0128
         LTR   RETCODE,RETCODE                                     0128
         BNZ   @RF00128                                            0128
*     CALL SCANMASK;                /*                       @G24LB37*/
         BAL   @14,SCANMASK                                        0129
*   RETURN;                         /* RET TO MAIN ROUTINE   @G24LB37*/
@EL00004 DS    0H                                                  0130
@EF00004 DS    0H                                                  0130
@ER00004 L     @14,@SA00004                                        0130
         BR    @14                                                 0130
*   END UNITINCL;                   /* END OF UNITINCL PROC  @G24LB37*/
         B     @EL00004                                            0131
*                                                                  0132
*/********************************************************************/
*/*                                                                  */
*/*     PROCEDURE NAME - SCANLKUP                            @G24LB37*/
*/*                                                          @G24LB37*/
*/*     FUNCTION - THIS PROCEDURE DETERMINES IF THE PASSED   @G24LB37*/
*/*                UCB ADDRESS IS IN THE SYSTEM LOOK UP      @G24LB37*/
*/*                TABLE, AND COUNTS THE UCB'S OFFSET WITHIN @G24LB37*/
*/*                THE SYSTEM UCB LOOK UP TABLE.             @G24LB37*/
*/*                                                          @G24LB37*/
*/*     INPUT - THE UCB ADDRESS OF THE UNIT, PASSED IN THE   @G24LB37*/
*/*             UNITP.  THE ADDRESS OF THE SYSTEM LOOK UP    @G24LB37*/
*/*             TABLE IN THE CVT FIELD CVTILK2.              @G24LB37*/
*/*                                                          @G24LB37*/
*/*     OUTPUT -   RETURN CODE IS RETURNED IN RETCODE AREA   @G24LB37*/
*/*                AS FOLLOWS:                               @G24LB37*/
*/*                0 = UCB ADDRESS WAS FOUND                 @G24LB37*/
*/*                4 = UCB ADDRESS WAS NOT FOUND             @G24LB37*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0132
*SCANLKUP:                                                         0132
*   PROCEDURE OPTIONS(NOSAVE);      /*                       @G24LB37*/
SCANLKUP DS    0H                                                  0133
*                                                                  0133
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE AND ESTABLISH ADDRESSIBILITY TO THE SYSTEM @G24LB37*/
*   /* UCB LOOK UP TABLE USING THE CVTILK2 FIELD IN THE CVT. @G24LB37*/
*   /* CLEAR COUNTER AND TURN FOUNDSW OFF.                   @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0133
*   UCBLOKUP=CVTILK2;               /*                       @G24LB37*/
         L     @09,CVTPTR                                          0133
         L     UCBLOKUP,CVTILK2(,@09)                              0133
*   UCBCNTR=ZERO;                   /*                       @G24LB37*/
         SLR   UCBCNTR,UCBCNTR                                     0134
*   FOUNDSW=OFF;                    /*                       @G24LB37*/
*                                                                  0135
         NI    FOUNDSW,B'01111111'                                 0135
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN THE SYSTEM UCB LOOK UP TABLE, TAKE EACH UCB      @G24LB37*/
*   /* ADDRESS AND CHECK THAT ADDRESS FIELD FOR THE MATCHING @G24LB37*/
*   /* UCBADDR THAT WAS PASSED IN THE UNITP. ADD A PLUS TWO  @G24LB37*/
*   /* TO THE ADDRESS OF THE TABLE TO INCREMENT TO THE NEXT  @G24LB37*/
*   /* UCB ADDRESS ENTRY. IF THE UCB ADDRESS IS ALL HEX      @G24LB37*/
*   /* FOXES THEN THE END OF THE TABLE HAS BEEN REACHED.     @G24LB37*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0136
*   DO WHILE(UCBADDR^=UCBDELIM(ONE:TWO))&(FOUNDSW=OFF);/*    @G24LB37*/
         B     @DE00136                                            0136
@DL00136 DS    0H                                                  0137
*     IF UCBADDR=UCBPTR THEN                                       0137
         LH    @09,UCBADDR(,UCBLOKUP)                              0137
