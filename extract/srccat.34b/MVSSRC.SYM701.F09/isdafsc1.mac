*   FYR(2)=FSCYR(2);                /*                       @Y30LB51*/
         MVC   FYR+1(1,TRANSPTR),FSCYR+1(TBLPTR)                   0213
*   FYR=FYR×'00';                   /*                       @Y30LB51*/
         OC    FYR(2,TRANSPTR),@CC00517                            0214
*   FSLASH='/';                     /* INSERT SLASH.         @Y30LB51*/
         MVI   FSLASH(TRANSPTR),C'/'                               0215
*   FBLANK='';                      /* INSERT BLANK.         @Y30LB51*/
         MVI   FBLANK(TRANSPTR),C' '                               0216
*   FHR(1)=FSCHR;                   /* CONVERT HOURS.        @Y30LB51*/
         MVC   FHR(1,TRANSPTR),FSCHR(TBLPTR)                       0217
*   SRL(FHR(1),4);                  /*                       @Y30LB51*/
         SLR   @10,@10                                             0218
         IC    @10,FHR(,TRANSPTR)                                  0218
         SRL   @10,4                                               0218
         STC   @10,FHR(,TRANSPTR)                                  0218
*   FHR(2)=FSCHR;                   /*                       @Y30LB51*/
         MVC   FHR+1(1,TRANSPTR),FSCHR(TBLPTR)                     0219
*   FHR=FHR×'00';                   /*                       @Y30LB51*/
         OC    FHR(2,TRANSPTR),@CC00517                            0220
*   FCOLON=':';                     /* INSERT COLON.         @Y30LB51*/
         MVI   FCOLON(TRANSPTR),C':'                               0221
*   FMIN(1)=FSCMIN;                 /* CONVERT MINUTES.      @Y30LB51*/
