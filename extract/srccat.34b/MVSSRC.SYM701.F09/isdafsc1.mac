*                                                                  0032
*   /*****************************************************************/
*   /*                                                               */
*   /* PROGRAM START.                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0032
*   CVTPTR=WORKPTR;                 /* SET LOCAL PTR TO SDACVT.    0032
*                                                            @Y30LB51*/
         L     @10,@PC00001                                        0032
         L     CVTPTR,WORKPTR(,@10)                                0032
*   LPARMPTR=PARMPTR;               /* SET LOCAL PTR TO SDAPARMS.  0033
*                                                            @Y30LB51*/
         L     LPARMPTR,PARMPTR(,CVTPTR)                           0033
*   FSCRETCD=0;                     /* INITIALIZE RETURN CODE.     0034
*                                                            @Y30LB51*/
         SLR   @10,@10                                             0034
         LR    FSCRETCD,@10                                        0034
*   TRANSPTR=0;                     /* POINTERS.             @Y30LB51*/
         LR    TRANSPTR,@10                                        0035
*   FSCWKPTR=FSC00PTR;              /* SET PTR TO WORK AREA. @Y30LB51*/
         L     FSCWKPTR,FSC00PTR(,CVTPTR)                          0036
*   IF FSCWKPTR^=0 THEN             /* IF WORK AREA PRESENT  @Y30LB51*/
         CR    FSCWKPTR,@10                                        0037
         BE    @RF00037                                            0037
*     DO;                           /* GET PRINT TABLE.      @Y30LB51*/
*       PRTPTR=PRINTTBL;            /* SET PTR TO PRINT TABLE.     0039
*                                                            @Y30LB51*/
         L     PRTPTR,PRINTTBL(,FSCWKPTR)                          0039
*       IF PRTTPTR^=0 THEN          /* IF DATA IN PRINT TABLE      0040
*                                                            @Y30LB51*/
         C     @10,PRTTPTR(,PRTPTR)                                0040
         BE    @RF00040                                            0040
*         DO;                       /* GET WORK SPACE.       @Y30LB51*/
*           CALL GETTBLS;           /* GET NEEDED WORK SPACE.      0042
*                                                            @Y30LB51*/
         BAL   @14,GETTBLS                                         0042
*           IF FSCRETCD=0 THEN      /* IF SPACE SECURED      @Y30LB51*/
         LTR   FSCRETCD,FSCRETCD                                   0043
         BNZ   @RF00043                                            0043
*             DO;                   /* PREPARE THE REPORT.   @Y30LB51*/
*               CALL PRTHDR;        /* PRINT PAGE HEADERS.   @Y30LB51*/
         BAL   @14,PRTHDR                                          0045
*               CALL PRTLINE;       /* PREPARE AND PRINT THE REPORT.
*                                                            @Y30LB51*/
         BAL   @14,PRTLINE                                         0046
*               CALL FREECORE;      /* FREE THE GOTTEN CORE. @Y30LB51*/
         BAL   @14,FREECORE                                        0047
*             END;                  /*                       @Y30LB51*/
*           ELSE                    /* SPACE WAS NOT AVAILABLE,    0049
*                                                            @Y30LB51*/
*             DO;                   /* FREE ALL DYNAMIC STORAGE.   0049
*                                                            @Y30LB51*/
         B     @RC00043                                            0049
@RF00043 DS    0H                                                  0050
*               CALL FREECORE(WORKPTR);/* GO FREE DYNAMIC STORAGE. 0050
*                                                            @Y30LB51*/
         L     @10,@PC00001                                        0050
         ST    @10,@AL00001                                        0050
         LA    @01,@AL00001                                        0050
         BAL   @14,FREECORE                                        0050
*               FSCRETCD=NOSPACE;   /* SET RETURN CODE.      @Y30LB51*/
         LA    FSCRETCD,4                                          0051
*             END;                  /*                       @Y30LB51*/
*         END;                      /*                       @Y30LB51*/
*       ELSE                        /* NO DATA IN THE FSC TABLE.   0054
*                                                            @Y30LB51*/
*         DO;                       /* FREE ALL DYNAMIC STORAGE.   0054
*                                                            @Y30LB51*/
         B     @RC00040                                            0054
@RF00040 DS    0H                                                  0055
*           CALL FREECORE(WORKPTR); /* GO FREE DYNAMIC STORAGE.    0055
*                                                            @Y30LB51*/
         L     @10,@PC00001                                        0055
         ST    @10,@AL00001                                        0055
         LA    @01,@AL00001                                        0055
         BAL   @14,FREECORE                                        0055
*           FSCRETCD=NODATA;        /* SET RETURN CODE.      @Y30LB51*/
         LA    FSCRETCD,8                                          0056
*         END;                      /*                       @Y30LB51*/
*     END;                          /*                       @Y30LB51*/
*   ELSE                            /* NO FSC WORK AREA.     @Y30LB51*/
*     FSCRETCD=NODATA;              /*                       @Y30LB51*/
         B     @RC00037                                            0059
@RF00037 LA    FSCRETCD,8                                          0059
*   RETURN CODE(FSCRETCD);          /*                       @Y30LB51*/
@RC00037 L     @13,4(,@13)                                         0060
         L     @00,@SIZDATD                                        0060
         LR    @01,@11                                             0060
         ISDAFSPC R,LV=(0),A=(1)
