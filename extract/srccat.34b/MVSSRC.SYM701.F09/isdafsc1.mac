*GETTBLS:                                                          0061
*   PROC OPTIONS(SAVE(14));         /*                       @AX14534*/
*                                                                  0061
@EL00001 L     @13,4(,@13)                                         0061
@EF00001 L     @00,@SIZDATD                                        0061
         LR    @01,@11                                             0061
         ISDAFSPC R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0061
         BR    @14                                                 0061
GETTBLS  ST    @14,@SA00002                                        0061
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE WILL SECURE SPACE FOR THE TRANSLATION WORK AREA. */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0062
*   SPNO='04'X;                     /* GET SUBPOOL NUMBER.   @Y30LB51*/
         MVI   SPNO(CVTPTR),X'04'                                  0062
*   CORELEN=LENGTH(CVTAREA);        /* GET LENGTH OF CONVERSION AREA.
*                                                            @Y30LB51*/
         MVC   CORELEN(3,CVTPTR),@CF00530+1                        0063
*   CALL ISDAGET1(WORKPTR);         /* GET CONVERSION AREA SPACE.  0064
*                                                            @Y30LB51*/
         L     @10,@PC00001                                        0064
         ST    @10,@AL00001                                        0064
         L     @15,@CV00382                                        0064
         LA    @01,@AL00001                                        0064
         BALR  @14,@15                                             0064
*   RFY                                                            0065
*     RF RSTD;                      /*                       @Y30LB51*/
*   FSCRETCD=RF;                    /* SAVE RETURN CODE.     @Y30LB51*/
         LR    FSCRETCD,RF                                         0066
*   RFY                                                            0067
*      RF UNRSTD;                   /*                       @Y30LB51*/
*   IF FSCRETCD=0 THEN              /* IF SPACE SECURED      @Y30LB51*/
         LTR   FSCRETCD,FSCRETCD                                   0068
         BNZ   @RF00068                                            0068
*     TRANSPTR=COREADDR;            /* SET PTR TO IT.        @Y30LB51*/
         L     TRANSPTR,COREADDR(,CVTPTR)                          0069
*   ELSE                            /* SPACE NOT AVAILABLE.  @Y30LB51*/
*     FSCRETCD=NOSPACE;             /*                       @Y30LB51*/
         B     @RC00068                                            0070
@RF00068 LA    FSCRETCD,4                                          0070
*   END GETTBLS;                    /*                       @Y30LB51*/
@EL00002 DS    0H                                                  0071
@EF00002 DS    0H                                                  0071
@ER00002 L     @14,@SA00002                                        0071
         BR    @14                                                 0071
*PRTHDR:                                                           0072
*   PROC OPTIONS(SAVE(14));         /*                       @AX14534*/
*                                                                  0072
PRTHDR   ST    @14,@SA00003                                        0072
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE WILL PRINT THE HEADERS ON EACH PAGE OF THE       */
*   /* REPORT.                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0073
*   OUTLINE=HEADER;                 /* PRINT THE REPORT TITLE.     0073
*                                                            @Y30LB51*/
         MVI   OUTLINE+38(CVTPTR),C' '                             0073
         MVC   OUTLINE+39(94,CVTPTR),OUTLINE+38(CVTPTR)            0073
         MVC   OUTLINE(38,CVTPTR),@CC00395                         0073
*   CALL ISDAPRT0(WORKPTR);         /*                       @Y30LB51*/
         L     @10,@PC00001                                        0074
         ST    @10,@AL00001                                        0074
         L     @15,@CV00383                                        0074
         LA    @01,@AL00001                                        0074
         BALR  @14,@15                                             0074
*   OUTLINE=SUBHDR1;                /* PRINT THE SUB-HEADINGS.     0075
*                                                            @Y30LB51*/
         MVI   OUTLINE+107(CVTPTR),C' '                            0075
         MVC   OUTLINE+108(25,CVTPTR),OUTLINE+107(CVTPTR)          0075
         MVC   OUTLINE(107,CVTPTR),@CC00400                        0075
*   CALL ISDAPRT0(WORKPTR);         /*                       @Y30LB51*/
         L     @10,@PC00001                                        0076
         ST    @10,@AL00001                                        0076
         L     @15,@CV00383                                        0076
         LA    @01,@AL00001                                        0076
         BALR  @14,@15                                             0076
*   OUTLINE=SUBHDR2;                /*                       @Y30LB51*/
         MVI   OUTLINE+81(CVTPTR),C' '                             0077
         MVC   OUTLINE+82(51,CVTPTR),OUTLINE+81(CVTPTR)            0077
         MVC   OUTLINE(81,CVTPTR),@CC00410                         0077
*   CALL ISDAPRT0(WORKPTR);         /*                       @Y30LB51*/
         L     @10,@PC00001                                        0078
         ST    @10,@AL00001                                        0078
         L     @15,@CV00383                                        0078
         LA    @01,@AL00001                                        0078
         BALR  @14,@15                                             0078
*   OUTLINE=SUBHDR3;                /*                       @Y30LB51*/
         MVI   OUTLINE+81(CVTPTR),C' '                             0079
         MVC   OUTLINE+82(51,CVTPTR),OUTLINE+81(CVTPTR)            0079
         MVC   OUTLINE(81,CVTPTR),@CC00417                         0079
*   CALL ISDAPRT0(WORKPTR);         /*                       @Y30LB51*/
         L     @10,@PC00001                                        0080
         ST    @10,@AL00001                                        0080
         L     @15,@CV00383                                        0080
         LA    @01,@AL00001                                        0080
         BALR  @14,@15                                             0080
*   OUTLINE=SUBHDR4;                /*                       @Y30LB51*/
         MVI   OUTLINE+54(CVTPTR),C' '                             0081
         MVC   OUTLINE+55(78,CVTPTR),OUTLINE+54(CVTPTR)            0081
         MVC   OUTLINE(54,CVTPTR),@CC00425                         0081
*   CALL ISDAPRT0(WORKPTR);         /*                       @Y30LB51*/
         L     @10,@PC00001                                        0082
         ST    @10,@AL00001                                        0082
         L     @15,@CV00383                                        0082
         LA    @01,@AL00001                                        0082
         BALR  @14,@15                                             0082
*   OUTLINE='';                     /*                       @Y30LB51*/
         MVI   OUTLINE(CVTPTR),C' '                                0083
         MVC   OUTLINE+1(132,CVTPTR),OUTLINE(CVTPTR)               0083
*   CALL ISDAPRT0(WORKPTR);         /* SPACE ONE LINE.       @Y30LB51*/
         L     @10,@PC00001                                        0084
         ST    @10,@AL00001                                        0084
         L     @15,@CV00383                                        0084
         LA    @01,@AL00001                                        0084
         BALR  @14,@15                                             0084
*   END PRTHDR;                     /*                       @Y30LB51*/
@EL00003 DS    0H                                                  0085
@EF00003 DS    0H                                                  0085
@ER00003 L     @14,@SA00003                                        0085
         BR    @14                                                 0085
*PRTLINE:                                                          0086
*   PROC OPTIONS(SAVE(14));         /*                       @Y30LB51*/
*                                                                  0086
PRTLINE  ST    @14,@SA00004                                        0086
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE WILL FORMAT THE DATA FROM THE FSC TABLE ELEMENT  */
*   /* AND CALL THE ISDAPRT0 ROUTINE TO PRINT IT.                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0087
*   OUTLINE='';                     /* BLANK OUT THE OUTPUT LINE.  0087
*                                                            @Y30LB51*/
         MVI   OUTLINE(CVTPTR),C' '                                0087
         MVC   OUTLINE+1(132,CVTPTR),OUTLINE(CVTPTR)               0087
*   PRTPTR=PRINTTBL;                /* SET PTR TO PRINT TABLE ENTRY.
*                                                            @Y30LB51*/
         L     PRTPTR,PRINTTBL(,FSCWKPTR)                          0088
*   TBLPTR=PRTTPTR;                 /* SET PTR TO FSC TABLE ENTRY. 0089
*                                                            @Y30LB51*/
         L     TBLPTR,PRTTPTR(,PRTPTR)                             0089
*   DO WHILE TBLPTR^=0;             /*                       @Y30LB51*/
         B     @DE00090                                            0090
@DL00090 DS    0H                                                  0091
*     IF LINECT>=LINENR THEN        /* IF NO SPACE ON PAGE   @Y30LB51*/
         LH    @10,LINECT(,FSCWKPTR)                               0091
         CH    @10,@CH00370                                        0091
         BL    @RF00091                                            0091
*       DO;                         /* PRINT FOOTINGS AND PREPARE NEW
*                                      PAGE.                 @Y30LB51*/
*         CALL PRTFOOT;             /* PRINT FOOTINGS.       @Y30LB51*/
         BAL   @14,PRTFOOT                                         0093
*         CALL PRTHDR;              /* PRTINT HEADERS ON NEW PAGE. 0094
*                                                            @Y30LB51*/
         BAL   @14,PRTHDR                                          0094
*         LINECT=0;                 /* RESET THE LINE COUNTER.     0095
*                                                            @Y30LB51*/
         SLR   @10,@10                                             0095
         STH   @10,LINECT(,FSCWKPTR)                               0095
*       END;                        /*                       @Y30LB51*/
*     ELSE                                                         0097
*       ;                           /*                       @Y30LB51*/
@RF00091 DS    0H                                                  0098
*     CALL CVTSSID;                 /* CONVERT SSID TO PRINTABLE.  0098
*                                                            @Y30LB51*/
         BAL   @14,CVTSSID                                         0098
*     IF FSCAPR='1'B THEN           /* IF APR ALTERED SSID   @Y30LB51*/
         TM    FSCAPR(TBLPTR),B'00010000'                          0099
         BNO   @RF00099                                            0099
*       SSIDTYPE='-A';              /* PUT 'A' IN SSID TYPE. @ZM30730*/
         MVC   SSIDTYPE(2,CVTPTR),@CC00487                         0100
*     ELSE                                                         0101
*       ;                           /* SSID NOT ALTERED.     @Y30LB51*/
@RF00099 DS    0H                                                  0102
*     CALL CVTFSC;                  /* CONVERT FSC TO PRINTABLE.   0102
*                                                            @Y30LB51*/
         BAL   @14,CVTFSC                                          0102
*     DECCVT=''B;                   /* ZERO OUT WORK AREA.   @Y30LB51*/
         XC    DECCVT(4,TRANSPTR),DECCVT(TRANSPTR)                 0103
*     CVD(FSCCTR,DECCVT);           /* CONVERT COUNT TO DECIMAL.   0104
*                                                            @Y30LB51*/
         LH    @10,FSCCTR(,TBLPTR)                                 0104
         CVD   @10,@TS00001                                        0104
         MVC   DECCVT(4,TRANSPTR),@TS00001+4                       0104
*     EDITCNT=PATTERNA;             /* GET EDIT PATTERN AND  @Y30LB51*/
         MVC   EDITCNT(8,TRANSPTR),PATTERNA                        0105
*     ED(EDITCNT,DECCVT);           /* EDIT THE COUNT.       @Y30LB51*/
         ED    EDITCNT(8,TRANSPTR),DECCVT(TRANSPTR)                0106
*     NUMBERS=EDITCNT;              /* PUT COUNT IN OUTPUT SKELETON.
*                                                            @Y30LB51*/
         MVI   NUMBERS+8(CVTPTR),C' '                              0107
         MVC   NUMBERS(8,CVTPTR),EDITCNT(TRANSPTR)                 0107
*     DECCVT=''B;                   /* ZERO OUT WORK AREA.   @Y30LB51*/
         XC    DECCVT(4,TRANSPTR),DECCVT(TRANSPTR)                 0108
*     CVD(FSCPCT,DECCVT);           /* CONVERT PERCENT TO DECIMAL. 0109
*                                                            @Y30LB51*/
         LH    @10,FSCPCT(,TBLPTR)                                 0109
         CVD   @10,@TS00001                                        0109
         MVC   DECCVT(4,TRANSPTR),@TS00001+4                       0109
*     EDITPCT=PATTERNB;             /* GET EDIT PATTERN AND  @Y30LB51*/
         MVC   EDITPCT(9,TRANSPTR),PATTERNB                        0110
*     ED(EDITPCT,DECCVT);           /* EDIT THE PERCENTAGE.  @Y30LB51*/
         ED    EDITPCT(9,TRANSPTR),DECCVT(TRANSPTR)                0111
*     PERCENT=EDITPCT;              /* PUT PERCENTAGE IN SKELETON. 0112
*                                                            @Y30LB51*/
         MVC   PERCENT(9,CVTPTR),EDITPCT(TRANSPTR)                 0112
*     CALL CVTTIME;                 /* CONVERT TIME TO PRINTABLE.  0113
*                                                            @Y30LB51*/
         BAL   @14,CVTTIME                                         0113
*     CALL ISDAPRT0(WORKPTR);       /* PRINT THE LINE.       @Y30LB51*/
         L     @10,@PC00001                                        0114
         ST    @10,@AL00001                                        0114
         L     @15,@CV00383                                        0114
         LA    @01,@AL00001                                        0114
         BALR  @14,@15                                             0114
*     OUTLINE='';                   /* BLANK THE OUTPUT LINE @AX14534*/
         MVI   OUTLINE(CVTPTR),C' '                                0115
         MVC   OUTLINE+1(132,CVTPTR),OUTLINE(CVTPTR)               0115
*     LINECT=LINECT+1;              /* ADD ONE TO THE LINE COUNTER.
*                                                            @Y30LB51*/
         LA    @10,1                                               0116
         LH    @15,LINECT(,FSCWKPTR)                               0116
         ALR   @15,@10                                             0116
         STH   @15,LINECT(,FSCWKPTR)                               0116
*     PRTPTR=PRTPTR+LENGTH(PRTTBL); /* SET PTR TO NEXT TABLE ENTRY.
*                                                            @Y30LB51*/
         AH    PRTPTR,@CH00104                                     0117
*     PTRSAVE=TBLPTR;               /* SAVE PTR TO CURRENT FSC TABLE
*                                      ENTRY.                @Y30LB51*/
         ST    TBLPTR,PTRSAVE(,FSCWKPTR)                           0118
*     TBLPTR=PRTTPTR;               /* SET PTR TO NEXT FSC TABLE   0119
*                                      ENTRY.                @Y30LB51*/
         L     TBLPTR,PRTTPTR(,PRTPTR)                             0119
*     IF FSCSSID^=PTRSAVE->FSCSSID THEN/* IF NEW SSID        @Y30LB51*/
         L     @14,PTRSAVE(,FSCWKPTR)                              0120
         CLC   FSCSSID(4,TBLPTR),FSCSSID(@14)                      0120
         BE    @RF00120                                            0120
*       DO;                         /* SPACE THE REPORT.     @Y30LB51*/
*         FSCANSI=LSKIP2;           /* SET CONTROL CHARACTER.      0122
*                                                            @Y30LB51*/
         MVI   FSCANSI(CVTPTR),C'0'                                0122
*         LINECT=LINECT+1;          /* ADD ONE TO LINE COUNTER.    0123
*                                                            @Y30LB51*/
         ALR   @15,@10                                             0123
         STH   @15,LINECT(,FSCWKPTR)                               0123
*       END;                        /*                       @Y30LB51*/
*     ELSE                                                         0125
*       ;                           /* NOT A NEW SSID.       @Y30LB51*/
@RF00120 DS    0H                                                  0126
*   END;                            /*                       @Y30LB51*/
@DE00090 LTR   TBLPTR,TBLPTR                                       0126
         BNZ   @DL00090                                            0126
*   CALL PRTFOOT;                   /* PRINT FOOTINGS.       @Y30LB51*/
         BAL   @14,PRTFOOT                                         0127
*   END PRTLINE;                    /*                       @Y30LB51*/
@EL00004 DS    0H                                                  0128
@EF00004 DS    0H                                                  0128
@ER00004 L     @14,@SA00004                                        0128
         BR    @14                                                 0128
*CVTSSID:                                                          0129
*   PROC OPTIONS(SAVE(14));         /*                       @Y30LB51*/
*                                                                  0129
CVTSSID  ST    @14,@SA00005                                        0129
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE WILL CONVERT THE SSID TO PRINTABLE CHARACTERS AND*/
*   /* DETERMINE THE DEVICE TYPE.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0130
*   SSIDCVT(1)=FSCSSID(1);          /* GET FIRST BYTE SSID AND     0130
*                                                            @Y30LB51*/
         MVC   SSIDCVT(1,TRANSPTR),FSCSSID(TBLPTR)                 0130
*   SSIDCVT(1)=SSIDCVT(1)&'0F'X;    /* ISOLATE FIRST CHARACTER.    0131
*                                                            @Y30LB51*/
         NI    SSIDCVT(TRANSPTR),X'0F'                             0131
*   SSIDCVT(2)=FSCSSID(2);          /* GET SECOND BYTE SSID AND    0132
*                                                            @Y30LB51*/
         MVC   SSIDCVT+1(1,TRANSPTR),FSCSSID+1(TBLPTR)             0132
*   SRL(SSIDCVT(2),4);              /* ISOLATE SECOND CHARACTER.   0133
*                                                            @Y30LB51*/
         SLR   @10,@10                                             0133
         IC    @10,SSIDCVT+1(,TRANSPTR)                            0133
         SRL   @10,4                                               0133
         STC   @10,SSIDCVT+1(,TRANSPTR)                            0133
*   SSIDCVT(3)=FSCSSID(2);          /* GET SECOND BYTE SSID AND    0134
*                                                            @Y30LB51*/
         MVC   SSIDCVT+2(1,TRANSPTR),FSCSSID+1(TBLPTR)             0134
*   SSIDCVT(3)=SSIDCVT(3)&'0F'X;    /* ISOLATE THIRD CHARACTER.    0135
*                                                            @Y30LB51*/
         NI    SSIDCVT+2(TRANSPTR),X'0F'                           0135
*   TR(SSIDCVT,TRANSTAB);           /* TRANSLATE SSID TO PRINTABLE.
*                                                            @Y30LB51*/
         TR    SSIDCVT(3,TRANSPTR),@CC00372                        0136
*   IF((MSCFLAG='1'B)×(REALDASD='1'B)) THEN/* IF MSC OR DASD RECORD
*                                                            @ZD00149*/
         TM    MSCFLAG(TBLPTR),B'10000000'                         0137
         BO    @RT00137                                            0137
         TM    REALDASD(TBLPTR),B'10000000'                        0137
         BNO   @RF00137                                            0137
@RT00137 DS    0H                                                  0138
*     RUACHAR=SSIDCVT;              /* PUT UNIT ADDRESS INTO       0138
*                                      SKELETON.             @ZM31022*/
         MVC   RUACHAR(3,CVTPTR),SSIDCVT(TRANSPTR)                 0138
*   ELSE                            /* NOT MSC OR DASD RECORD,     0139
*                                                            @ZD00149*/
*     SSIDCHAR=SSIDCVT;             /* PUT SSID IN SKELETON. @ZM31022*/
         B     @RC00137                                            0139
@RF00137 MVC   SSIDCHAR(3,CVTPTR),SSIDCVT(TRANSPTR)                0139
*   IF MSCFLAG='1'B THEN            /* IF MSC DEVICE         @Y30LB51*/
@RC00137 TM    MSCFLAG(TBLPTR),B'10000000'                         0140
         BNO   @RF00140                                            0140
*     DO;                           /* PREPARE SKELETON.     @Y30LB51*/
*       DEVICE='MSC ';              /* PUT MSC DEVICE AND    @Y30LB51*/
         MVC   DEVICE(4,CVTPTR),@CC00499                           0142
*       RUATYPE='-M';               /* TYPE IN SKELETON.     @ZM30730*/
         MVC   RUATYPE(2,CVTPTR),@CC00500                          0143
*     END;                          /*                       @Y30LB51*/
*   ELSE                            /* NOT MSC DEVICE.       @Y30LB51*/
*     IF SAFLAG='1'B THEN           /* IF STAGING ADAPTOR    @Y30LB51*/
         B     @RC00140                                            0145
@RF00140 TM    SAFLAG(TBLPTR),B'01000000'                          0145
         BNO   @RF00145                                            0145
*       DEVICE='SA  ';              /* PUT IT IN SKELETON.   @Y30LB51*/
         MVC   DEVICE(4,CVTPTR),@CC00501                           0146
*     ELSE                          /* NOT STAGING ADAPTOR.  @Y30LB51*/
*       IF DRCFLAG='1'B THEN        /* IF R/W CONTROLLER     @Y30LB51*/
         B     @RC00145                                            0147
@RF00145 TM    DRCFLAG(TBLPTR),B'00100000'                         0147
