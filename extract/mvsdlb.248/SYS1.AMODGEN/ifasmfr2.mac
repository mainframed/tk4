SMF26JAF DS    CL42     JOB ACCT FIELDS (MAX. LEN 42)          @ZA05171
         AGO   .CONTB                                            Y02120
         MEND       */
 %IFAL20: IF IFAR20='' %THEN %GOTO IFAL21;
    DCL 1 SMFRCD20 BASED(SMF20PTR) BDY(WORD),
 /* RECORD TYPE 20 - JOB INITIATION                                  */
 /* WRITTEN BY JOB INITIATION                                        */
         3 SMF20LEN FIXED(15),    /* RDW RECORD LENGTH               */
         3 SMF20SEG FIXED(15),    /* RDW SEGMENT DESCRIPTOR          */
         3 SMF20FLG BIT(8),       /* OPERATING ENVIRONMENT           */
         3 SMF20RTY FIXED(8),     /* RECORD TYPE 20                  */
         3 SMF20TME FIXED(31) BDY(BYTE), /* TOD RECORD WRITTEN       */
         3 SMF20DTE CHAR(4),      /* DATE RECORD WRITTEN             */
         3 SMF20SID CHAR(4),      /* SYSTEM ID FROM INSTALLATION     */
         3 SMF20JBN CHAR(8),      /* JOB NAME                        */
         3 SMF20RST FIXED(31) BDY(BYTE), /*READER START TIME(.01 SEC)*/
         3 SMF20RSD CHAR(4),      /* READER START DATE               */
         3 SMF20UIF CHAR(8),      /* USER ID FIELD                   */
          5 SMF20UID CHAR(8),
         3 SMF20RIN FIXED(15) BDY(BYTE),             /*      @G32FPRL*/
           5 SMF20RLO FIXED(15) BDY(BYTE), /* OFFSET FROM BEGINNING
                                              OF RECORD TO RELOCATABLE
                                              AREA           @G32FPRL*/
         3 SMF20PGM CHAR(20),     /* PROGRAMMER'S NAME               */
         3 SMF20NAF FIXED(8),     /* # OF ACCT FIELDS FOLLOWING      */
         3 SMF20ACT CHAR(*);      /* ACCOUNTING FIELDS       @G32FPRL*/
    DCL 1 SMF20AFD BASED BDY(BYTE), /* ACCT SECTION                  */
         3 SMF20ANC PTR(8),       /* # OF CHAR IN THIS ACCT FIELD    */
         3 SMF20BCD CHAR(*);      /* ACCT FIELD CHARACTERS           */
         /* RELOCATABLE AREA  @G32FPRL*/
    DCL 1 SMF20RLA BASED(ADDR(SMF20FLG)+SMF20RLO) BDY(BYTE),
         3 SMF20RLS FIXED(15) BDY(BYTE), /* SIZE OF RELOCATABLE AREA
                                                             @G32FPRL*/
         3 SMF20GRP CHAR(8),      /* RACF GROUP ID           @G32FPRS*/
         3 SMF20RUD CHAR(8),      /* RACF USER ID            @G32FPRS*/
         3 SMF20TID CHAR(8);      /* RACF TERMINAL ID        @G32FPRS*/
 %IFAL21: IF IFAR21='' %THEN %GOTO IFAL22;
    DCL 1 SMFRCD21 BASED(SMF21PTR) BDY(WORD),
 /* RECORD TYPE 21 - ERROR STATISTICS BY VOLUME                      */
 /* WRITTEN WHENEVER A TAPE CONTAINING A USER'S DATA SET IS DEMOUNTED*/
         3 SMF21LEN FIXED(15),    /* RDW RECORD LENGTH               */
         3 SMF21SEG FIXED(15),    /* RDW SEGMENT DESCRIPTOR          */
         3 SMF21FLG BIT(8),       /* OPERATING ENVIRONMENT           */
         3 SMF21RTY FIXED(8),     /* RECORD TYPE 21                  */
         3 SMF21TME FIXED(31) BDY(BYTE), /* TOD RECORD WRITTEN       */
         3 SMF21DTE CHAR(4),      /* DATE RECORD WRITTEN             */
         3 SMF21SID CHAR(4),      /* SYSTEM ID FROM INSTALLATION     */
         3 SMF21LGH FIXED(15),    /* LGTH FROM HERE ON + THESE 2     */
         3 SMF21VOL CHAR(6),      /* VOLUME SERIAL NUMBER            */
         3 SMF21CA CHAR(2),       /* CHANNEL ADDRESS                 */
         3 SMF21UCB CHAR(4),      /* UCB TYPE CODE                   */
         3 SMF21TR FIXED(8),      /* # OF TEMPORARY READ ERRORS      */
         3 SMF21TW FIXED(8),      /* # OF TEMPORARY WRITE ERRORS     */
         3 SMF21SIO FIXED(15),    /* # OF START I/O'S                */
         3 SMF21PR FIXED(8),      /* # OF PERMANENT READ ERRORS      */
         3 SMF21PW FIXED(8),      /* # OF PERMANENT WRITE ERRORS     */
         3 SMF21NB FIXED(8),      /* # OF NOISE BLOCKS               */
         3 SMF21ERG FIXED(15) BDY(BYTE), /* # OF ERASE GAPS          */
         3 SMF21CLN FIXED(15) BDY(BYTE), /* # OF CLEANER ACTIONS     */
         3 SMF21DEN FIXED(8),     /* TAPE DENSITY CODE               */
         3 SMF21BLS FIXED(15),    /* BLOCK SIZE FOR FIXED LGTH ORG   */
         3 SMF21RV0 CHAR(2);      /* RESV                            */
 %IFAL22: IF IFAR22='' %THEN %GOTO IFAL23;
    DCL 1 SMFRCD22 BASED(SMF22PTR) BDY(WORD),
 /* RECORD TYPE 22 - CPU, CHANNEL AND STORAGE CONFIGURATION          */
 /* WRITTEN AT IPL AND BY VARY ON OR OFF WHENEVER A CPU, CHANNEL OR  */
 /* STORAGE BOX IS CHANGED                                           */
         3 SMF22LEN FIXED(15),    /* RDW RECORD LENGTH               */
         3 SMF22SEG FIXED(15),    /* RDW SEGMENT DESCRIPTOR          */
         3 SMF22FLG BIT(8),       /* OPERATING ENVIRONMENT           */
         3 SMF22RTY FIXED(8),     /* RECORD TYPE 22                  */
         3 SMF22TME FIXED(31) BDY(BYTE), /* TOD RECORD WRITTEN       */
         3 SMF22DTE CHAR(4),      /* DATE RECORD WRITTEN             */
         3 SMF22SID CHAR(4),      /* SYSTEM ID FROM INSTALLATION     */
         3 SMF22IND FIXED(15),    /* RECORD CREATOR INDICATOR
                                     X'0001' AT IPL
                                     X'0002' BY VARY ON
                                     X'0003' BY VARY OFF             */
         3 SMF22ECT FIXED(15);    /* # RESOURCE ELEMENTS FOLLOWING   */
    DCL 1 SMF22CPU BASED BDY(HWORD), /* CPU ELEMENT                  */
         3 SMF22RV0 CHAR(1),      /* RESV                            */
         3 SMF22PID FIXED(8),     /* ELEMENT ID, X'01' FOR CPU       */
         3 SMF22CPN CHAR(2),      /* CPU MODEL #                     */
         3 SMF22RV1 CHAR(1),      /* RESV                            */
         3 SMF22CPA CHAR(1);      /* CPU ADDRESS                     */
    DCL 1 SMF22CHL BASED BDY(HWORD), /* CHANNEL ELEMENT              */
         3 SMF22CSD CHAR(1),      /* CHANNEL SET ID        @G64APJW  */
         3 SMF22CID FIXED(8),     /* ELEMENT ID, X'02' FOR CHANNEL   */
         3 SMF22CHN CHAR(2),      /* CHANNEL TYPE AND MODEL #        */
         3 SMF22CHA CHAR(1),      /* CHANNEL ADDRESS                 */
         3 SMF22CPD CHAR(1);      /* CPU ADDRESS                     */
    DCL 1 SMF22STR BASED BDY(HWORD), /* STORAGE ELEMENT              */
         3 SMF22RV3 CHAR(1),      /* RESV                            */
         3 SMF22TID FIXED(8),     /* ELEMENT ID, X'03' FOR STORAGE   */
         3 SMF22PGL FIXED(15),    /* PG # OF LOWEST PAGE IN STORAGE  */
         3 SMF22NPG FIXED(15);    /* # OF PAGES IN STORAGE           */
    DCL 1  SMF22IPL BASED BDY(BYTE),   /* IPL ELEMENT        @Y30AQPF*/
         3 SMF22RV4 CHAR(1),       /*RESV                    @Y30AQPF*/
         3 SMF22IID FIXED(8),      /* ELEMENT ID, X'04' FOR IPL
                                        @Y30AQPF*/
         3 SMF22ION BIT(320);      /* SS/I UNITS ONLINE AT IPL
                                       @Y30AQPF*/
    DCL 1 SMF22VON BASED BDY(BYTE), /* VARY S ONLINE ELEMENT @Y30AQPF*/
         3 SMF22RV5 CHAR(1),       /* RESERVED               @Y30AQPF*/
         3 SMF22NID FIXED(8),      /* ELEMENT ID, X'05' FOR VARY ONLI
                                       @Y30AQPF*/
         3 SMF22RVA CHAR(1),       /* RESERVED               @Y30AQPF*/
         3 SMF22NSI CHAR(3);       /* SUBSYSTEM ID           @Y30AQPF*/
    DCL 1 SMF22VOF BASED BDY(BYTE), /* VARY S OFFLINE ELEMT  @Y30AQPF*/
         3 SMF22RV6 CHAR(1),       /* RESERVED               @Y30AQPF*/
         3 SMF22FID FIXED(8),      /* ELEMENT ID, X'06' FOR VARY OFFL
                                       @Y30AQPF*/
         3 SMF22RVB CHAR(1),       /* RESERVED               @Y30AQPF*/
         3 SMF22FSI CHAR(3);       /* SUBSYSTEM ID           @Y30AQPF*/
 %IFAL23: ;
 %IFAL25: IF IFAR25='' %THEN %GOTO IFAL26;
   DCL 1 SMFRCD25 BASED(SMF25PTR) BDY(WORD),               /*@Y02BPSE*/
 /* JES3 WILL PRODUCE ONE SMF RECORD 25 FOR EACH JOB TO      @Y02BPSE*/
 /* REPRESENT ALL JCL STATIC ALLOCATIONS, PLUS SEPARATE      @Y02BPSE*/
 /* RECORDS FOR EACH DYNAMIC ALLOCATION REQUEST AND          @Y02BPSE*/
 /* SEPARATE RECORDS FOR EACH GROUP OF CATALOG ALLOCATIONS.  @Y02BPSE*/
        3 SMF25LEN FIXED(15), /* RDW RECORD LENGTH           @Y02BPSE*/
        3 SMF25SEG FIXED(15), /* RDW SEGMENT DESCRIPTOR      @Y02BPSE*/
        3 SMF25FLG BIT(8),    /* OPERATING ENVIRONMENT       @Y02BPSE*/
        3 SMF25RTY FIXED(8),  /* RECORD TYPE 25              @Y02BPSE*/
        3 SMF25TME FIXED(31) BDY(BYTE), /* TOD RECRD WRITTEN @Y02BPSE*/
        3 SMF25DTE CHAR(4),   /* DATE RECORD WRITTEN         @Y02BPSE*/
        3 SMF25SID CHAR(4),   /* SYSTEM ID                   @Y02BPSE*/
        3 SMF25JBN CHAR(8),   /* JOBNAME                     @Y02BPSE*/
        3 SMF25RST FIXED(31) BDY(BYTE), /* READER START TIME @Y02BPSE*/
        3 SMF25RSD CHAR(4),   /* READER START DATE           @Y02BPSE*/
        3 SMF25UIF CHAR(8),   /* USER ID FIELD               @Y02BPSE*/
 /*************************************/
 /* BEGINNING OF DESCRIPTIVE SECTION */
 /*************************************/
        3 SMF25IND BIT(16),   /* FUNCTION FLAGS              @Y02BPSE*/
          5 SMF25DYN BIT(1),  /* 1 = DYNAMIC ALLOCATION      @Y02BPSE*/
                              /* 0 = JCL, STATIC ALLOCATION  @Y02BPSE*/
          5 SMF25CAT BIT(1),  /* 1 = NON-EXECUTION ALLOCATION@Y02BPSE*/
                              /* 0 = EXECUTION ALLOCATIONS   @Y02BPSE*/
          5 SMF25FCH BIT(1),  /* 1 = AUTOMATIC ALLOCATIONS   @Y02BPSE*/
                              /* 0 = MANUAL ALLOCATIONS      @Y02BPSE*/
          5 SMF25RSV BIT(13), /* RESERVED                    @Y02BPSE*/
        3 SMF25NTF FIXED(31), /* NUMBER OF TAPES FETCHED     @Y02BPSE*/
        3 SMF25NDF FIXED(31), /* NO. DISK VOLUMES FETCHED    @Y02BPSE*/
        3 SMF25FST FIXED(31), /* TOD FETCH START             @Y02BPSE*/
        3 SMF25FSD CHAR(4),   /* DATE FETCH START            @Y02BPSE*/
        3 SMF25SST FIXED(31), /* TOD *START SETUP ISSUED
                                     (ZERO FOR AUTO FETCH)   @Y02BPSE*/
        3 SMF25SSD CHAR(4),   /* DATE *START SETUP ISSUED
                                     (ZERO FOR AUTO FETCH)   @Y02BPSE*/
        3 SMF25NTM FIXED(31), /* NUMBER OF TAPES MOUNTED     @Y02BPSE*/
        3 SMF25NDM FIXED(31), /* NO. DISK VOLUMES MOUNTED    @Y02BPSE*/
        3 SMF25MST FIXED(31), /* TOD 1ST MOUNT MESSAGE       @Y02BPSE*/
        3 SMF25MSD CHAR(4),   /* DATE 1ST MOUNT MESSAGE      @Y02BPSE*/
        3 SMF25VVT FIXED(31), /* TOD JOB VERIFIED            @Y02BPSE*/
        3 SMF25VVD CHAR(4),   /* DATE JOB VERIFIED           @Y02BPSE*/
        3 SMF25NMV FIXED(31),         /* NO. MSS VOLUMES     @Z40LL00*/
        3 SMF25END CHAR(0);   /* END OF RECORD               @ZA05171*/
 %IFAL26: IF IFAR26='' %THEN %GOTO IFALR3;
    DCL 1 SMFRCD26 BASED(SMF26PTR) BDY(WORD),
 /* RECORD TYPE 26 - JOB PURGE BY SUBSYSTEM                          */
 /* WRITTEN WHEN ALL SYSOUT FINISHED FOR JOB(FORE & BACKGROUND)      */
         3 SMF26LEN FIXED(15),    /* RDW RECORD LENGTH               */
         3 SMF26SEG FIXED(15),    /* RDW SEGMENT DESCRIPTOR          */
         3 SMF26FLG BIT(8),       /* OPERATING ENVIRONMENT           */
         3 SMF26RTY FIXED(8),     /* RECORD TYPE 26                  */
         3 SMF26TME FIXED(31) BDY(BYTE), /* TOD RECORD WRITTEN       */
         3 SMF26DTE CHAR(4),      /* DATE RECORD WRITTEN             */
         3 SMF26SID CHAR(4),      /* SYSTEM ID FROM INSTALLATION     */
         3 SMF26JBN CHAR(8),      /* JOB NAME                        */
         3 SMF26RST FIXED(31) BDY(BYTE), /*READER START TIME(.01 SEC)*/
         3 SMF26RSD CHAR(4),      /* READER START DATE               */
         3 SMF26UIF CHAR(8),      /* USER IDENTIFICATION FIELD       */
         3 SMF26RSV CHAR(4),      /* RESV                            */
         3 SMF26SBS FIXED(15),    /* SUBSYSTEM ID, JES2-2 JES3-5     */
         3 SMF26IND BIT(16),      /* SECTION INDICATORS              */
          5 SMF26DES BIT(1),      /* DESCRIPTIVE SECTION PRESENT     */
          5 SMF26EVT BIT(1),      /* EVENT SECTION PRESENT           */
          5 SMF26ATU BIT(1),      /* ACTUALS SECTION PRESENT         */
          5 * BIT(13),            /* RESV                            */
 /* DESCRIPTIVE SECTION BEGINS                                       */
         3 SMF26LN1 PTR(15),      /* LGTH OF DESCRIPTIVE SECTION     */
         3 SMF26RV1 CHAR(2),      /* RESV                    @YL02120*/
         3 * BIT(8),              /*ADDITIONAL JOB INFO      @YL02120*/
           5 SMF26IN2 BIT(0),     /*ADDITIONAL JOB INFO(JES2 ONLY)
                                    0-BACKGROUNG JOB         @YL02120
                                    1-FOREGROUNG TIME SHARING@YL02120
                                    2-SYSTEM TASK            @YL02120
                                    3-NO JOURNAL OPTION      @YL02120
                                    4-NO OUTPUT OPTION       @YL02120
                                    5-TYPRUN=SCAN              @Z30AQPF
                                    6 - TYPRUN=COPY           @Z30AQPF
                                    7 - RESERVED             @YL02120*/
           5 SMF26IN3 BIT(8),     /*ADDITIONAL JOB INFO(JES3 ONLY)   */
             7 SMF26DJC BIT(1),   /*0-DEPENDENT JOB(NET JOB
                                      PROCESSED              @ZA05171*/
             7 SMF26DLJ BIT(1),   /*1-JOB SPECIFIED DEADLINE
                                      SCHEDULING             @ZA05171*/
             7 SMF26DLM BIT(1),   /*2-JOB MET DEADLINE       @ZA05171*/
             7 SMF26PRJ BIT(1),   /*3-PROCESS STMT PROCESSED @ZA05171*/
             7 SMF26NJX BIT(1),   /*4-JOB LEFT SYSTEM VIA NJP@ZA05171*/
             7 SMF26NJE BIT(1),   /*5-JOB ENTERED SYS VIA NJP@ZA05171*/
             7 SMF26DJO BIT(1),   /*6-JOB LEFT SYSTEM VIA DJ @ZA05171*/
             7 SMF27DJE BIT(1),   /*7-JOB ENTERED SYS VIA DJ @ZA05171*/
         3 SMF26INF BIT(8),       /* JOB INFORMATION         @YL02120*/
           5 SMF26JCP BIT(1),     /* 0-JOB PRIORITY EXTERNALLY ASSIGNED
                                       JES2-VIA  PRIORITY STMT
                                       JES3-VIA PRTY PARM ON JOB STMT*/
           5 SMF26STU BIT(1),     /* 1-SETUP JOB             @ZA05171
                                       JES2- SETUP STMT PROCESSED
                                     JES3-PROCESSED BY PRE-EXEC STEUP*/
           5 SMF26TRH BIT(1),     /* 2-JOB HELD VIA 'TYPERUN=HOLD'   */
           5 *        BIT(1),     /* 3-JOB REQUESTED NO JES JOB LOG
                                       (JES2 ONLY)           @ZA05171*/
           5 *        BIT(1),     /* 4-EXEC BATCH JOB(JES2)  @ZA05171*/
           5 SMF26EIR BIT(1),     /* 5-JOB ENTERED VIA INTERNAL RDR  */
           5 SMF26MRE BIT(1),     /* 6-JOB WAS RERUN BY JES  @ZA05171*/
           5 SMF26OPC BIT(1),     /* 7-OPER CANCELED VIA CMND@ZA05171*/
         3 SMF26JNM CHAR(4),      /* JES ASSIGNED JOB NUMBER @ZA05171*/
         3 SMF26JID CHAR(8),      /*JOB ID OF FORM XXX0YYYY WHERE YYYY
                                     IS JES ASSIGNED JOB # & XXX IS
                                     'JOB' - NORMAL JES JOB  @YL02120
                                     'STC' - STARTED TASK CONTROL
                                     'TSU' - TSO USER        @YL02120*/
         3 SMF26NAM CHAR(20),     /* PROGRAMMER'S NAME FROM JOB CARD */
         3 SMF26MSG CHAR(1),      /* MESSAGE CLASS FROM JOB CARD     */
         3 SMF26CLS CHAR(1),      /* JOB CLASS FROM JOB CARD         */
         3 SMF26XPI FIXED(8),     /* INITIAL JOB PRIORITY    @YL02120*/
         3 SMF26XPS FIXED(8),     /* SELECTION PRI AT SELECTION      */
         3 SMF26RV8 FIXED(32),    /* RESERVED(JES3)          @ZA05171*/
           5 SMF26OPI FIXED(8),   /* INITIAL OUTPUT PRI(JES2 ONLY)   */
           5 SMF26OPS FIXED(8),   /*OUTPUT PRI AT SELECTION-JES2 ONLY*/
           5 SMF26LOC FIXED(15),  /* INPUT ROUTE CODE (JES2 ONLY)    */
         3 SMF26DEV CHAR(8),      /* LOG DEV NAME OF WHERE JOB READ
                                     USERID IF TSO SUBMIT    @YL02120
                                     SYSTEM NAME IF NJP      @YL02120*/
         3 SMF26RVA CHAR(8),      /* RESERVED (JES3)         @ZA05171*/
           5 SMF26ACT CHAR(4),    /* PROGRAMMER'S ACCT #(JES2 ONLY)  */
           5 SMF26ROM CHAR(4),    /* PROGRAMMER'S ROOM #(JES2 ONLY)  */
         3 SMF26XTM FIXED(31),    /* EST EXECUTION TIME(SEC) @YL026WA*/
         3 SMF26ELN FIXED(31),    /* EST OUTPUT LINES                */
         3 SMF26EPU FIXED(31),    /* EST OUTPUT PUNCHED CARDS        */
         3 SMF26J2D CHAR(20),     /* JES2 ONLY DESCRIPTIVE SECTION   */
           5 SMF26FRM CHAR(4),    /* DEFAULT OUTPUT FORM NUMBER      */
           5 SMF26CYP FIXED(15) BDY(BYTE), /* PRINT COPY     @YL02120
                                     COUNT FOR ALL OF JOB    @YL02120*/
           5 SMF26LIN FIXED(15) BDY(BYTE), /* LINES PER PAGE @YL02120*/
           5 SMF26PRR FIXED(15) BDY(BYTE), /*PRINT ROUTE CODE@YL02120*/
           5 SMF26PUR FIXED(15) BDY(BYTE), /*PUNCH ROUTE CODE@YL02120*/
           5 SMF26PDD CHAR(8);    /* JES2 PROC DDN - JCL CNVT@YM04806*/
 DCL 1 SMF26J3D BASED(ADDR(SMF26J2D)),                    /* @ZA05171*/
         3 SMF26DTY CHAR(1),      /* DEADLINE SCHEDULE TYPE  @YL02120*/
         3 SMF26RV6 CHAR(3),      /* RESERVED                @YL02120*/
         3 SMF26IGP CHAR(8),      /* LOG INPUT DEV GP NAM(JOB SOURCE)*/
         3 SMF26PD3 CHAR(8),      /* PROCEDURE DD NAME       @YL02120*/
         3 SMF26NJO CHAR(8),      /* SYS NAME WHERE JOB SENT VIA NJP */
         3 SMF26NJI CHAR(8),      /* SYS WHICH SENT JOB VIA NJP      */
         3 SMF26NET CHAR(8),      /* ID OF DEPENDENT JOB NET TO WHICH
                                    THIS JOB BELONGS(FROM //*NET STMT*/
         3 SMF26DTM FIXED(31) BDY(BYTE),   /*DEADLINE SCH TOD@YL02120*/
         3 SMF26DDT CHAR(4),      /* DEADLINE SCHEDULE DATE  @YL02120*/
         3 SMF26CLN CHAR(8);      /* JOB CLASS NAME          @YL02120*/
 /* EVENT SECTION BEGINS HERE                                        */
    DCL 1 SMF26A BASED(ADDR(SMF26LN1)+SMF26LN1) BDY(WORD),
         3 SMF26LN2 PTR(15),      /* LGTH OF EVENT SECTION           */
         3 SMF26RV2 CHAR(2),      /* RESV                            */
         3 SMF26RPT FIXED(31),    /* TOD READER STOP                 */
         3 SMF26RPD CHAR(4),      /* READER STOP DATE                */
         3 SMF26CST FIXED(31),    /* TOD CNVT START                  */
         3 SMF26CSD CHAR(4),      /* CNVT START DATE                 */
         3 SMF26CPT FIXED(31),    /* TOD CNVT STOP                   */
         3 SMF26CPD CHAR(4),      /* CNVT STOP DATE                  */
         3 SMF26XST FIXED(31),    /* TOD EXEC START                  */
         3 SMF26XSD CHAR(4),      /* EXEC START DATE                 */
         3 SMF26XPT FIXED(31),    /* TOD EXEC STOP                   */
         3 SMF26XPD CHAR(4),      /* EXEC STOP DATE                  */
         3 SMF26OST FIXED(31),    /* TOD OUTPUT PROCESSOR START      */
         3 SMF26OSD CHAR(4),      /* OUTPUT PROCESSOR START DATE     */
         3 SMF26OPT FIXED(31),    /* TOD OUTPUT PROCESSOR STOP       */
         3 SMF26OPD CHAR(4);      /* OUTPUT PROCESSOR STOP DATE      */
 /* ACTUALS SECTION BEGINS HERE                                      */
    DCL 1 SMF26B BASED(ADDR(SMF26LN2)+SMF26LN2) BDY(WORD),
         3 SMF26LN3 PTR(15),      /* LGTH OF ACTUALS SECTION         */
         3 SMF26RV4 CHAR(2),      /* RESV                            */
         3 SMF26ICD FIXED(31),    /* # INPUT CARDS(JCL + DATA)       */
         3 SMF26XLN FIXED(31),    /* OUTPUT LINES GENERATER TO SPOOL */
         3 SMF26XPU FIXED(31),    /* OUTPUT PUNCH CARDS GEN TO SPOOL */
         3 SMF26RID CHAR(4),    /* INPUT PROCESSOR SYSTEM ID @Z30AQPF*/
         3 SMF26CID CHAR(4),      /* JCL CONVERSION PROCESSOR
                                    SYSTEM ID @Z30AQPF*/
         3 SMF26XID CHAR(4),  /* EXECUTION PROCESSOR SYS ID @Z30AQPF*/
         3 SMF26OID CHAR(4),  /* OUTPUT PROCESSOR SYSTEM ID @Z30AQPF*/
         3 SMF26JAF CHAR(42); /* JOB ACCT FIELDS(MAX LEN 42)@ZA05171*/
 %IFALR3: ;
