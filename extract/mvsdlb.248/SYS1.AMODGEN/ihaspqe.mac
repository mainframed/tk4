*        %GOTO SPQBSL;       /* BILINGUAL MAPPING           /*      /*
         MACRO
         IHASPQE    &TYPE
*
*        RELEASE 2 AOS, 3/7/73, LEVEL=1
*
         AIF      ('&TYPE' EQ 'INTERNAL').EQU
SPQESECT DSECT      SUBPOOL QUEUE ELEMENT
SPQEAD   DS    F   ADDRESS OF NEXT OLDEST SPQE
SPQEPTR  EQU   SPQEAD
SPDQEPTR  DS   F   POINTER TO FIRST DQE FOR SUBPOOL
SPDQEAD  EQU   SPDQEPTR
SPQEFLGS DS    BL1  SPQE FLAGS
SPSHARE  EQU   X'80' 0=SUBPOOL OWNED
*                    1=SUBPOOL SHARED, NOT OWNED
LASTSPQE EQU   X'40' LAST SPQE ON CHAIN
SPQEOWN  EQU   X'20' 0=SUBPOOL IS OWNED,NOT SHARED
*                    1=SUBPOOL IS OWNED AND SHARED
SPQERES1 DS    C    RESERVED
SPQEID   DS    C    IDENTIFYING NUMBER OF SUBPOOL
SPQEKEY  DS    C    KEY OF THE OWNING TASK
SPQERES2 DS    F    RESERVED
         MEXIT
.EQU     ANOP
SPQEAD   EQU   0  ADDRESS OF NEXT OLDEST SPQE
SPQEPTR  EQU   0  ADDRESS OF NEXT OLDEST SPQE
SPDQEPTR EQU   4  POINTER TO FIRST DQE FOR SUBPOOL
SPDQEAD  EQU   4  POINTER TO FIRST DQE FOR SUBPOOL
SPQEFLGS EQU   8  SPQE FLAGS
SPSHARE  EQU   X'80' 0=SUBPOOL IS OWNED
*                    1=SUBPOOL IS SHARED, NOT OWNED
LASTSPQE EQU   X'40' LAST SPQE ON CHAIN
SPQEOWN  EQU   X'20' 0=SUBPOOL IS OWNED, NOT SHARED
*                    1=SUBPOOL IS OWNED AND SHARED
SPQEID   EQU   10   IDENTIFYING NUMBER OF SUBPOOL
SPQEKEY  EQU   11   KEY OF THE OWNING TASK
         MEND  , */
*%SPQBSL:   ;
*
*/*      RELEASE 2 AOS, 3/7/73, LEVEL=1                             */
*
* DECLARE
*   1 SPQESECT  BASED(SPQESPTR),     /* SUBPOOL QUEUE ELEMENT        */
*    2 SPQEAD PTR(31),               /* POINTER TO NEXT SPQE         */
*      3 SPQEPTR     PTR(31),        /* ADDRESS OF NEXT OLDEST SPQE  */
*    2 SPDQEPTR PTR(31),             /* POINTER TO FIRST DQE FOR SP  */
*      3 SPDQEAD    PTR(31),         /* POINTER TO FIRST DQE FOR SP  */
*    2 SPQEFLGS     BIT(8),          /* SPQE FLAGS                   */
*       3 SPSHARE   BIT(1),          /* 0=SUBPOOL IS OWNED           */
*                                    /*1=SUBPOOL IS SHARED, NOT OWNED*/
*       3 LASTSPQE  BIT(1),          /* LAST SPQE ON CHAIN           */
*       3 SPQEOWN   BIT(1),          /*0=SUBPOOL IS OWNED, NOT SHARED*/
*                                    /*1=SUBPOOL IS OWNED AND SHARED */
*    2 SPQERES1     CHAR(1),         /* RESERVED                     */
*    2 SPQEID      CHAR(1),          /* SUBPOOL NUMBER               */
*    2 SPQEKEY     CHAR(1),          /*KEY OF THE OWNING TASK        */
*    2 SPQERES2    PTR(31);          /* RESERVED                     */
