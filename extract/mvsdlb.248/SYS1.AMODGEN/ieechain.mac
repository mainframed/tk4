*    2 CHCIBCTR    PTR(8),           /* MAXIMAL NUMBER OF QUEUED
*                                       CIB'S                        */
*    2 CHPKE       CHAR(1) BDY(WORD),/* PROTECT KEY (0S/VS1)   ICB345*/
*     3 CHTRKID     CHAR(1),         /* DISPLAY/TRACK IDENTIFIER
*                                       (OS/VS2)               MDC014*/
*%CHTSID='''01''X';                  /* TIME SHARING USER IDENTIFIER
