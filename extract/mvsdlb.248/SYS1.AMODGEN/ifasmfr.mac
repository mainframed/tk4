*%GOTO IFABGN1;  /*
         MACRO
&NAME    IFASMFR &RECTYPE                                            */
*%IFABGN1: ;
* /*                                                                 */
* /* MACMEAN IFASMFR MEANS SYSTEM MANAGEMENT FACILITY RECORD MAPPING */
* /* MACCOMP Y-2 SC102/BL27P                                         */
* /* MACSTAT Y-2 73298/42020                                         */
* /* METHOD OF ACCESS                                                */
* /*  PL/S - USER DEFINES MACRO VARIABLE IFARXX WHERE XX=RECORD NUM  */
* /*         DECLARE SMFXXPTR PTR(31) OR RESPECIFY FOR BASE          */
* /*         INCLUDE MACRO FROM LIBRARY                              */
* /*          EXAMPLE %IFAR08 = 'YES',           NOTE. THE COMMA REP-*/
* /*                  DCL SMF08PTR PTR(31),      RESENTS A SEMI-COLON*/
* /*                  %INCLUDE SYSLIB(IFASMFR),  BUT ISN'T TO AVIOD  */
* /*                                             A DIAGNOSTIC.       */
* /*                                                                 */
* /*                                                                 */
* /*         THIS MACRO PROCESSES RECORDS IN THE RANGE 00-06. IT     */
* /*         ACTS AS A ROUTER TO OTHER MACROS TO PROCESS OTHER       */
* /*         RECORDS AS FOLLOWS:                                     */
* /*                                                                 */
* /*             MACRO        RECORDS                                */
* /*            ********      *******                                */
* /*            IFASMFR1      07-19                                  */
* /*            IFASMFR2      20-27                                  */
* /*            IFASMFR3      28-36                                  */
* /*            IFASMFR4      37-46                                  */
* /*            IFASMFR5      47-54                                  */
* /*            IFASMFR6      55-69                                  */
* /*            IFASMFR7      70-73                                  */
* /*            IFASMFR8      74-79                                  */
* /*            IFASMFR9      80-93                                  */
* /*            IFASMFRA      94-103                                 */
* /*            IFASMFRB      104-113                                */
* /*            IFAMSFRC      114-123                                */
* /*            IFASMFRD      124-127                                */
*%GOTO IFABGN2;   /*
.*
.*
.* CHANGE LEVEL= Z40MPTH, ZA05171, Z40FPTH, ZA10800, ZA27193
.*
.*
.* CHANGE ACTIVITY:
.*
.*    $T8=UZ90390 ,EBB1102,850312,PDFS: TYPE 6 RESTRUCTURE        @T8A
.*
.*
*  THIS IS AN SMF MACRO WHICH CONTROLS THE BUILDING OF SMF RECORDS. THE
*  REQUIRED FORMAT IS
*
*        IFASMFR  &RECTYPE
*
*  NOTE: VALUES FOR &RECTYPE MUST BE ENCLOSED IN PARENS(UNLESS ONLY 1).
*
         LCLA  &NUM,&B
.* &NUM IS SET TO THE NUMBER OF SUBSTRING ITEMS IN THE FIRST PARAMETER
.* AND &B IS USED TO POINT AT EACH SUBSTRING ITEM.
         AIF   ('&RECTYPE' EQ '').E1
&NUM     SETA  N'&RECTYPE
         AGO   .CONTC
.CONTB   ANOP
&NUM     SETA  &NUM-1
.CONTC   ANOP
         AIF   (&NUM LE 0).OUT
&B       SETA  &B+1
.RTYCHK  AIF   ('&RECTYPE(&B)' EQ '').E3
         AIF   (&RECTYPE(&B) GT 127 OR &RECTYPE(&B) LT 0).E5   @Z40FPTH
         AGO   .CONTA
.E1      ANOP
         SPACE 3
         MNOTE 8,'PARAMETERS OMITTED, PROCESSING TERMINATED'
         MEXIT
.E3      ANOP
         SPACE 3
         MNOTE 4,'NULL RECORD TYPE PASSED'
         AGO   .CONTB
.E5      ANOP
         SPACE 3
         MNOTE 4,'&RECTYPE(&B) INVALID RECORD TYPE SPECIFIED'
         AGO   .CONTB
.CONTA   AIF   (&RECTYPE(&B) GE 0 AND &RECTYPE(&B) LE 6).G0
         AIF   (&RECTYPE(&B) GE 7 AND &RECTYPE(&B) LE 19).G1
         AIF   (&RECTYPE(&B) GE 20 AND &RECTYPE(&B) LE 27).G2
         AIF   (&RECTYPE(&B) GE 28 AND &RECTYPE(&B) LE 36).G3
         AIF   (&RECTYPE(&B) GE 37 AND &RECTYPE(&B) LE 46).G4
         AIF   (&RECTYPE(&B) GE 47 AND &RECTYPE(&B) LE 54).G5
         AIF   (&RECTYPE(&B) GE 55 AND &RECTYPE(&B) LE 69).G6
         AIF   (&RECTYPE(&B) GE 70 AND &RECTYPE(&B) LE 73).G7
         AIF   (&RECTYPE(&B) GE 74 AND &RECTYPE(&B) LE 79).G8
         AIF   (&RECTYPE(&B) GE 80 AND &RECTYPE(&B) LE 93).G9
         AIF   (&RECTYPE(&B) GE 94 AND &RECTYPE(&B) LE 103).GA
         AIF   (&RECTYPE(&B) GE 104 AND &RECTYPE(&B) LE 113).GB
         AIF   (&RECTYPE(&B) GE 114 AND &RECTYPE(&B) LE 123).GC
         AIF   (&RECTYPE(&B) GE 124 AND &RECTYPE(&B) LE 127).GD
         AGO   .E5
.G0      ANOP
         AIF   (&RECTYPE(&B) EQ 0).IFAS000
         AIF   (&RECTYPE(&B) EQ 2).IFAS002
         AIF   (&RECTYPE(&B) EQ 3).IFAS003
         AIF   (&RECTYPE(&B) EQ 4).IFAS004
         AIF   (&RECTYPE(&B) EQ 5).IFAS005
         AIF   (&RECTYPE(&B) EQ 6).IFAS006
         AGO   .E5
.G1      ANOP
         IFASMFR1 &RECTYPE(&B)
         AGO   .CONTB
.G2      ANOP
         IFASMFR2 &RECTYPE(&B)
         AGO   .CONTB
.G3      ANOP
         IFASMFR3 &RECTYPE(&B)
         AGO   .CONTB
.G4      ANOP
         IFASMFR4 &RECTYPE(&B)
         AGO   .CONTB
.G5      ANOP
         IFASMFR5 &RECTYPE(&B)
         AGO   .CONTB
.G6      ANOP
         IFASMFR6 &RECTYPE(&B)
         AGO   .CONTB
.G7      ANOP
         IFASMFR7 &RECTYPE(&B)
         AGO   .CONTB
.G8      ANOP
         IFASMFR8 &RECTYPE(&B)
         AGO   .CONTB
.G9      ANOP
         IFASMFR9 &RECTYPE(&B)
         AGO   .CONTB
.GA      ANOP
         IFASMFRA &RECTYPE(&B)
         AGO   .CONTB
.GB      ANOP
         IFASMFRB &RECTYPE(&B)
         AGO   .CONTB
.GC      ANOP
         IFASMFRC &RECTYPE(&B)
         AGO   .CONTB
.GD      ANOP
         IFASMFRD &RECTYPE(&B)
         AGO   .CONTB
.OUT     MEXIT
.IFAS000 ANOP
         EJECT
