         3 SMF4PSEC CHAR(8);             /* COUNT OF PAGE SECONDS
                                                             @Z40FPPF*/
 %IFAL05: IF IFAR05='' %THEN %GOTO IFAL06;
    DCL 1 SMFRCD05 BASED(SMF05PTR) BDY(WORD),
 /* RECORD TYPE 5 - JOB TERMINATION                                  */
 /* WRITTEN AT NORMAL OR ABNORMAL TERMINATION OF A JOB               */
