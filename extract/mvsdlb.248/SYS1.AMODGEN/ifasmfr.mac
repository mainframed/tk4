         3 SMF4PSO  FIXED(31) BDY(BYTE), /* # OF PAGES SWAPPED OUT   */
          5 SMF4SOUT FIXED(31) BDY(BYTE),
         3 SMF4VPI  FIXED(31) BDY(BYTE), /* # OF VIO PAGE-INS        */
