                                     INDICATORS TO BE SET IN
                                     BIT VALUE FORMAT.  EXAMPLE:
                                     LEVEL 1=X'01', LEVEL 12=X'0C'.
                                     THIS NUMBER WILL BE INCREMENTED
                                     BY 1 EACH TIME A NEW RELEASE
                                     CHANGES THE RECORD.         @T8A*/
         3 SMF6JNM CHAR(4),       /* JES ASSIGNED JOB NUMBER @YL02120*/
         3 SMF6OUT CHAR(8),       /* LOGICAL OUTPUT DEV NAME @YL02120*/
         3 SMF6FCB CHAR(4),       /* FCB ID                          */
         3 SMF6UCS CHAR(4),       /* UCS ID,  END OF REC FOR EXT WTR */
         3 SMF6PGE FIXED(31) BDY(BYTE), /* APPROX PAGE COUNT @YL026WA*/
         3 SMF6J2S CHAR(2),       /* SPECIAL JES2 SECTION        @T8C*/
           5 SMF6RTE FIXED(15) BDY(BYTE),  /* OUTPUT ROUTE CODE
                                                 OR ZERO         @T8C*/
         3 SMF6END2 CHAR(0);      /* END OF JES2 BASE RECORD     @T8A*/
   DCL 1 * CHAR(24) DEFINED(SMF6J2S),       /*                   @T8A*/
         3 SMF6J3S CHAR(24),      /* SPECIAL JES3 SECTION        @T8C*/
           5 SMF6DFE BIT(16),     /* DATA FORMAT ERROR INDICATORS    */
             7 *     BIT(6),      /* 0-5 RESERVED                @T8C*/
             7 SMF6CCE BIT(1),    /* 6-ONE OR MORE RECORDS CONTAINED
                                       INVALID 1ST CHAR CONTROL DATA,
                                       DEFAULT SUBSTITUTED       @T8C*/
             7 SMF6RBE BIT(1),    /* 7-INVALID LGTH REC IN DATA,
                                       TRUNCATED OR PADDED       @T8C*/
             7 *       BIT(8),    /* 8-15 RESERVED               @T8C*/
           5 SMF6OPR FIXED(16) BDY(BYTE),  /* OUTPUT PRIORITY    @T8C*/
           5 SMF6GRP CHAR(8),     /*LOG OUTPUT DEV GROUP NAME    @T8C*/
           5 SMF6RSVJ CHAR(8),    /*RESERVED FOR JES3            @T8C*/
           5 SMF6RSVU CHAR(4),    /*RESERVED FOR USER            @T8C*/
         3 SMF6END CHAR(0);       /* END OF JES3 BASE RECORD     @T8C*/
      /* BEGINNING OF FIRST EXTENSION - THIS SECTION WILL ONLY BE
         PRESENT WHEN SMF6SBS IS SET TO 2 OR 5  INDICATING THAT
         JES2 OR JES3 HAS GENERATED THIS RECORD.                 @T8A*/
   DCL 1 SMF6EXT1 BASED,          /* FIRST EXTENSION - NON-IMPACT
                                     PRINTING SUBSYSTEM SECTION  @T8C*/
         3 SMF6LN2 FIXED(15),   /* LENGTH OF FIRST EXTENSION
                                     INCLUDING THIS FIELD        @T8C*/
         3 SMF6CPS(8) CHAR(1),    /* COPIES DISTRIBUTION     @ZA10800*/
         3 SMF6CHR CHAR(16),      /* TRANSLATE TABLE NAMES   @ZA10800
                                     FROM CHARS PARAMETER    @ZA10800*/
         3 SMF6MID CHAR(4),       /* COPY MODIFICATION       @ZA10800
                                     MODULE NAME             @ZA10800*/
         3 SMF6FLI CHAR(4),       /* FLASH OVERLAY NAME      @ZA10800*/
         3 SMF6FLC BIT(8),        /* NUMBER OF COPIES FLASHED@ZA10800*/
         3 SMF6BID BIT(8),        /* FLAG BYTE               @ZA10800*/
           4 SMF6BTS BIT(1),      /* THE BTSS WAS USED FOR   @ZA10800
