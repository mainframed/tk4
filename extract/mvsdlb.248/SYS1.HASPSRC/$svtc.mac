         MACRO -- $SVTC -- GENERATE HASP SSVT FUNCTION MATRIX
&NAME    $SVTC
&NAME    DC    AL1(($SVSOUT-$SVNULL)/4) ..1 PROCESS SYSOUT
         DC    AL1(($SVCANC-$SVNULL)/4) ..2 TSO CANCEL
         DC    AL1(($SVSTAT-$SVNULL)/4) ..3 TSO STATUS
         DC    AL1(($SVEOT-$SVNULL)/4) ...4 END OF TASK
         DC    AL1(($SVJBSL-$SVNULL)/4) ..5 JOB SELECTION
         DC    AL1(($SVALOC-$SVNULL)/4) ..6 ALLOCATION
         DC    AL1(($SVUNAL-$SVNULL)/4) ..7 UNALLOCATION
         DC    AL1(($SVEOM-$SVNULL)/4) ...8 END OF MEMORY
         DC    AL1(($SVWTO-$SVNULL)/4) ...9 WTO
         DC    AL1(($SVCMND-$SVNULL)/4) .10 COMMAND PROCESSING
         DC    AL1(($SVUSER-$SVNULL)/4) .11 USERID VALIDITY CHECK
         DC    AL1(($SVTERM-$SVNULL)/4) .12 JOB DELETION
         DC    AL1(($SVRENQ-$SVNULL)/4) .13 RE-ENQUEUE JOB
         DC    AL1(($SVDOM-$SVNULL)/4) ..14 DOM
         DC    X'0'
         DC    AL1(($SVOPEN-$SVNULL)/4) .16 OPEN
         DC    AL1(($SVCLOS-$SVNULL)/4) .17 CLOSE
         DC    AL1(($SVCKPT-$SVNULL)/4) .18 CHECKPOINT
         DC    AL1(($SVREST-$SVNULL)/4) .19 RESTART
         DC    AL1(($SVREQID-$SVNULL)/4) 20 REQUEST JOB ID
         DC    AL1(($SVRETID-$SVNULL)/4) 21 RETURN JOB ID
         MEND
