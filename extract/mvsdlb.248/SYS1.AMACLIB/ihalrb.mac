                4 LRBNCEM BIT(1),  /*  PENDING CHANNEL END          */
                4 LRBNDEM BIT(1),  /*  PENDING DEVICE END           */
            2 LRBHSW3 CHAR(1),     /* DEPENDENT SWITCH BYTE 2   */
