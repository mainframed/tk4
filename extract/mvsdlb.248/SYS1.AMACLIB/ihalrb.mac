           4 LRBMTSEC BIT(1),       /* SECONDARY ERROR FLAG          */
           4 LRBMTCKS BIT(1),       /* CHECK STOP FLAG               */
           4 LRBMTWRN BIT(1),       /* POWER WARNING FLAG            */
           4 LRBMTDMG BIT(1),       /* SYSTEM DAMAGE FLAG            */
