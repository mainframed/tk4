             3 *      BIT(3),           /* RESERVED */
           2 LRBHSW1 CHAR(1),      /*DEPENDENT SWITCH BYTE 0      */
            2 LRBHSW2 CHAR (1),    /* DEPENDENT SWITCH BYTE 1       */
