         MACRO
         SGICH400
.*
.* $MAC(SGICH400) COMP(SC1BN) PROD(RACF):
.*
.* DESCRIPTIVE NAME = SYSGEN MACRO FOR RACF SVCS
.*
.* STATUS = OS/VS2 EBB1102
.*
.* CHANGE ACTIVITY = 0
.*
.*
.*
         COPY  SGGBLPAK            DEFINE GLOBAL SYMBOLS
&SGCTRLC(6)  SETC ''               SET NULL PROG NAME
&SGCTRLC(7)  SETC '&SGCTRLC(20)'   SET VOLUME FOR LPALIB
&SGCTRLC(8)  SETC '&SGCTRLC(19)'   SET UNIT NAME FOR LPALIB
&SGCTRLC(9)  SETC 'LPALIB'         DSNAME FOR LPALIB DATA SET
&SGCTRLC(10) SETC ',LET'           SET LINKEDIT OPTIONS XREF
&SGCTRLC(11) SETC ',RENT'          AND LIST ARE SET BY GENERATE
         COPY  SGLEDPK1            LINKEDIT JCL DECK1
  PUNCH '//AOSBN DD DISP=SHR,VOLUME=(,RETAIN),DSNAME=SYS1.AOSBN'
         COPY  SGLEDPK2            LINKEDIT DECK2
.*       SVC 130 THRU SVC 133
         PUNCH ' INCLUDE AOSBN(ICHRIN00)'
         PUNCH ' ALIAS IGC0013A,IGC0013B,IGC0013C'
         PUNCH ' ENTRY ICHRIN00'
         PUNCH ' NAME IGC0013{(R)'
         PUNCH '/* '
         MEND
