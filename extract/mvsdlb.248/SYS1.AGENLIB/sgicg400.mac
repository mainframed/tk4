         MACRO
         SGICG400
.* /* START OF SPECIFICATIONS ****
.*
.*01  MODULE-NAME = SGICG400
.*
.*01  DESCRIPTIVE-NAME = MSCTC VS/2 SYSGEN LINKLIB LINKEDIT MACRO
.*
.*01  STATUS = OS/VS2 RELEASE 3 LEVEL 0
.*
.*01  FUNCTION = SGICG400 LINKEDITS MSC TABLE CREATE MODULE
.*               WITH APF AUTHORIZATION INTO THE TARGET
.*               SYS1.LINKLIB (VS2) FROM THE
.*               DISTRIBUTION LIBRARY SYS1.AOSD0
.*
.*01  OPERATION = VIA THE ASSEMBLER PUNCH COMMAND, LINKAGE
.*                EDITOR INCLUDE STATEMENTS ARE GENERATED
.*                FOR THE MODULES
.*
.*01  MODULE-TYPE = MACRO
.*02    PROCESSOR = ASSEMBLER XF
.*02    MACRO-SIZE = 18 STATEMENTS
.*
.*01  OUTPUT = LINKAGE EDITOR JCL AND CONTROL STATEMENTS
.*
.*01  INNER-MACRO = SGGBLPAK - DEFINE SYSTEM GLOBALS
.*                  SGLEDPK1 - GENERATE LINKEDIT JCL
.*                  SGLEDPK2 - GENERATE LINKEDIT SYSIN JCL
.*
.*01  MESSAGES = NONE
.*
.**** END OF SPECIFICATIONS ***/
         COPY  SGGBLPAK                DEFINE SYSTEM GLOBAL VARIABLES
&SGCTRLC(6)    SETC ''                 SYSLMOD MEMBER
&SGCTRLC(7)    SETC '&SGCTRLC(32)'     SYSLMOD VOLUME
&SGCTRLC(8)    SETC '&SGCTRLC(31)'     SYSLMOD UNIT
&SGCTRLC(9)    SETC 'LINKLIB'          SYSLMOD DSNAME
&SGCTRLC(10)   SETC ''                 NULL ATTRIBUTE
&SGCTRLC(11)   SETC ''                 NULL ATTRIBUTE
         COPY  SGLEDPK1                GENERATE LINK EDIT STEP
 PUNCH '//AOSD0 DD DSNAME=SYS1.AOSD0,VOLUME=(,RETAIN),DISP=SHR'
         COPY  SGLEDPK2                LINK EDIT SYSIN STAGE
.* INCLUDE MSCTC COMPONENT MODULE
 PUNCH ' INCLUDE AOSD0(ICGDSINT)'
 PUNCH ' ENTRY ICGDSINT'
 PUNCH ' SETCODE AC(1)'
 PUNCH ' NAME MSCTC(R)'
 PUNCH '/*'
         MEND
