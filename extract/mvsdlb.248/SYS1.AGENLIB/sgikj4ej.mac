          MACRO
         SGIKJ4EJ
.********************************************************************
.***  THIS MACRO PRODUCES THE LINKEDIT CONTROL STATEMENTS
.***  FOR THE UTILITY COMMANDS  RENAME, AND PROTECT.
.********************************************************************
          COPY SGGBLPAK
&SGCTRLC(7)   SETC  '&SGCTRLC(20)'        CMDVOL
&SGCTRLC(8)   SETC  '&SGCTRLC(19)'        UNIT NAME OF CMDVOL
&SGCTRLC(9)   SETC  'LPALIB'              LIBRARY NAME
&SGCTRLC(11)   SETC  ',REFR'
&SGCTRLC(12)   SETC  ',RENT'
          COPY SGLEDPK1
 PUNCH '//ACMDLIB DD DISP=SHR,VOLUME=(,RETAIN),DSNAME=SYS1.ACMDLIB'
          COPY SGLEDPK2
 PUNCH '  ENTRY IKJEHDEF'
 PUNCH '  INCLUDE ACMDLIB(IKJEHDEF,IKJEHCIR)'
 PUNCH '  ALIAS IKJDFLT,IKJEHCIR'
 PUNCH '  NAME IKJEHDEF(R)'
 PUNCH '/*'
          MEND
