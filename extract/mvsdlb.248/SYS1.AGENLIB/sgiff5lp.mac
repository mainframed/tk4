         MACRO
         SGIFF5LP
         COPY  SGGBLPAK
.* A009020                                                    LF YM6974
.* A005100-006000,C009200,A009300                             LF YM4325
.* A000000-999999                                             LF Y02971
.*  THIS MACRO, NEW FOR VS2/R2, PRODUCES IEBCOPY STATEMENTS ONLY FOR
.*  THE LPALIB.  THIS FUNCTION WAS PREVIOUSLY DONE AT THE BEGINNING
.*  OF SGIFF5LS.  THE PUNCH STATEMENTS HAVE BEEN UPDATED TO REFLECT
.*  MODULE CONSOLIDATIONS.
.*
         PUNCH ' COPY OUTDD=LPALIB,INDD=AOSG0'                   Y01007
.*  IF GRAPHICS IS NOT IN SYSTEM, ONLY TASK TERMINATION/RECOVERY YM4325
.*  IS NEEDED.                                                LF YM4325
         PUNCH ' SELECT MEMBER=(IGG0203Z,IGG0213Z)'           LF YM4325
         AIF   (NOT &SGMENTB(12)).LP10                        LF YM4325
.*  DO NOT LOAD BFRMGT MODULES UNLESS 2250 IS AVAILABLE
         AIF   (NOT &SGDCLSB(9)).LP05
         PUNCH ' SELECT MEMBER=(IGC0007A,IGC0207A)'
.LP05    PUNCH ' SELECT MEMBER=(IGC0007C,IGC0007D,IGC0007E)'
         PUNCH ' SELECT MEMBER=(IGC0107C,IGC0107D)'           LF YM6974
         PUNCH ' SELECT MEMBER=(IGG019OA,IGG019OB,IGG019OC)'
         PUNCH ' SELECT MEMBER=(IGG019OE,IGG019OJ)'
         PUNCH ' SELECT MEMBER=(IGG0193Y,IGG0203Y)'           LF YM4325
.LP10    ANOP                                                 LF YM4325
         MEND
