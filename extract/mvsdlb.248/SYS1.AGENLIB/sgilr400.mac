         MACRO
         SGILR400
.*A-000000-999999                                               @Y026SG
.* /* START OF SPECIFICATIONS ****
.*
.*01  MODULE-NAME = SGILR400
.*
.*01  DESCRIPTIVE-NAME = ASM LINKLIB SYSGEN MACRO
.*
.*01  STATUS = OS/VS2 RELEASE 4.1
.*
.*01  FUNCTION = SGILR400 CAUSES CERTAIN ASM PROGRAM MODULES TO BE
.*               LINK-EDITED INTO THE TARGET SYS1.LINKLIB
.*               FROM THE SYSGEN DISTRIBUTION LIBRARY SYS1.AOSC5.
.*
.*01  OPERATION = VIA THE ASSEMBLER PUNCH COMMAND, LINKAGE EDITOR
.*                CONTROL STATEMENTS ARE GENERATED FOR EACH MODULE.
.*
.*01  MODULE-TYPE = MACRO
.*02    PROCESSOR = ASSEMBLER XF
.*02    MACRO-SIZE = 38 STATEMENTS
.*
.*01  OUTPUT = LINKAGE EDITOR JCL AND CONTROL STATEMENTS
.*
.*01  INNER-MACRO = SGGBLPAK - DEFINE SYSTEM GLOBALS
.*                  SGLEDPK1 - GENERATE LINKEDIT JCL
.*                  SGLEDPK2 - GENERATE LINKEDIT SYSIN JCL
.*
.*01  MESSAGES = NONE
.*
.**** END OF SPECIFICATIONS ***/
         COPY  SGGBLPAK           DEFINE SYSTEM GLOBAL VARIABLES
&SGCTRLC(7)    SETC '&SGCTRLC(32)'     SYSLMOD VOLUME
&SGCTRLC(8)    SETC '&SGCTRLC(31)'     SYSLMOD UNIT
&SGCTRLC(9)    SETC 'LINKLIB'          SYSLMOD DSNAME
&SGCTRLC(10)   SETC ',RENT'
&SGCTRLC(11)   SETC ',REFR'
         COPY  SGLEDPK1           GENERATE LINK EDIT STEP
 PUNCH '//AOSC5 DD DSNAME=SYS1.AOSC5,VOLUME=(,RETAIN),DISP=SHR'
         COPY  SGLEDPK2           LINK EDIT SYSIN STAGE
 PUNCH ' INCLUDE AOSC5(ILROPS00)'                             @Z40WPWN
 PUNCH ' ENTRY ILROPS00'                                      @Z40WPWN
 PUNCH ' NAME ILROPS00(R)'                                    @Z40WPWN
 PUNCH ' INCLUDE AOSC5(ILRPREAD)'                             @Z40WPWN
 PUNCH ' ENTRY ILRPREAD'                                      @Z40WPWN
 PUNCH ' NAME ILRPREAD(R)'                                    @Z40WPWN
 PUNCH ' INCLUDE AOSC5(ILRPGEXP)'                             @Z40WPWN
 PUNCH ' ENTRY ILRPGEXP'                                      @Z40WPWN
 PUNCH ' ALIAS IEEPGEXP'                                      @Z40WPWN
 PUNCH ' NAME ILRPGEXP(R)'                                    @Z40WPWN
 PUNCH '/*'
&SGCTRLC(7)    SETC '&SGCTRLC(32)'     SYSLMOD VOLUME
&SGCTRLC(8)    SETC '&SGCTRLC(31)'     SYSLMOD UNIT
&SGCTRLC(9)    SETC 'LINKLIB'          SYSLMOD DSNAME
&SGCTRLC(10)   SETC ',REUS'
         COPY  SGLEDPK1           GENERATE LINK EDIT STEP
 PUNCH '//AOSC5 DD DSNAME=SYS1.AOSC5,VOLUME=(,RETAIN),DISP=SHR'
         COPY  SGLEDPK2           LINK EDIT SYSIN STAGE
 PUNCH ' INCLUDE AOSC5(ILRFMT00)'
 PUNCH ' INCLUDE AOSC5(ILRFMTPG)'                             @Z40WPWN
 PUNCH ' INCLUDE AOSC5(ILRFMTSW)'                             @Z40WPWN
 PUNCH ' INCLUDE AOSC5(ILRFMTCV)'                             @Z40WPWN
 PUNCH ' ENTRY ILRFMT00'
 PUNCH ' NAME ILRFMT00(R)'
 PUNCH ' INCLUDE AOSC5(ILRTMI00)'                             @Z40WPWN
 PUNCH ' ENTRY ILRTMI00'                                      @Z40WPWN
 PUNCH ' NAME ILRTMI00(R)'                                    @Z40WPWN
 PUNCH '/*'
         MEND
