         MACRO
         SGIFF5LS
         COPY  SGGBLPAK
.*  C000000-999999  MACRO HAS BEEN REWRITTEN FOR VS2/R2       LF Y02971
.* C723500                                                    LF YM2014
.* A905020 SUPPORT FOR IFFAFA20 GSP ENHANCEMENT FOR 3250       @ZA48073
.*
.*
.*
.*
.*
.*
.*  AS OF VS2/R2, SGIFF5LS PRODUCES IEBCOPY STATEMENTS ONLY FOR THE
.*  LINKLIB.  MACRO SGIFF5LP, NEW FOR VS2/R2, PRODUCES THE LPALIB
.*  STATEMENTS PREVIOUSLY AT THE BEGINNING OF SGIFF5LS.
.*
.*  &SGGRPHB (2) AND (4) ARE INVALID FOR EXCLUSION CHECKS.
.*
.*  THE NUMBER OF PUNCH SELECT STATEMENTS HAS BEEN REDUCED BY
.*  CONSOLIDATING THE VARIOUS FUNCTIONAL AREAS ONTO SINGLE STATEMENTS.
.*
         PUNCH ' COPY OUTDD=LINKLIB,INDD=AOSG0'                   AOS/1
         PUNCH ' SELECT MEMBER=(IFFANA,ANLZ,IGG019OK)'
         PUNCH ' SELECT MEMBER=(IFFCAN01,IFFCAN02,IFFCAN03)'       000A
         AIF   (&GETB(3)).LS06          CHECK IF IODEVICE SYSGEN   AHD3
         AIF   (NOT &SGGRPHB(1)).LS05            PORS INCLUDED
.*
.*  ALIAS ENTRY NAME OF POR FOLLOWS EACH ALPHABETIZED MODULE NAME
 PUNCH ' SELECT MEMBER=IFFPAAST,GSTOR,IFFPBAPR,GCPRNT,IFFPCAAR,GARC'
 PUNCH ' SELECT MEMBER=IFFPDAPL,GSPLOT,IFFPEAGR,GCGRID,IFFPFAVA,GVARC'
 PUNCH ' SELECT MEMBER=IFFPGAVP,GSVPLT,IFFPHALA,GLABEL,IFFPIAPG,GPGRID'
 PUNCH ' SELECT MEMBER=IFFPJAPV,GPVGRD,IFFPPASG,GOFFSG,IFFPLARE,PENTRK'
 PUNCH ' SELECT MEMBER=IFFPKADG,GSDPLT'
.*
.LS05    AIF   (NOT &SGGRPHB(3)).LS06    CHECK IF GSP INCLUDED     JFCF
         PUNCH ' COPY OUTDD=LINKLIB,INDD=AOS07'                   AOS/1
 PUNCH ' SELECT MEMBER=(IHCGSP01,IHCGSP02,IHCGSP03,IHCGSP04,BCNV)'
 PUNCH ' SELECT MEMBER=(IHDGSP01,IHDGSP02,IHDGSP03,INGSP,TMGSP)' YM2014
