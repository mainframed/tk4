         MACRO
         SGIKJ4EG
.********************************************************************
.***  THIS MACRO PRODUCES THE LINKEDIT CONTROL STATEMENTS
.***  NECESSARY TO LINKAGE EDIT THE LINK-EDITABLE MODULES
.***  FOR THE TSO TEST FUNCTION.
.********************************************************************
         COPY  SGGBLPAK
&SGCTRLC(7) SETC  '&SGCTRLC(23)'        CMDVOL
&SGCTRLC(8) SETC  '&SGCTRLC(24)'        UNIT NAME OF CMDVOL
&SGCTRLC(9) SETC  'CMDLIB'              LIBRARY NAME
&SGCTRLC(10) SETC ',REFR'               LINKAGE EDITOR OPTION
&SGCTRLC(11) SETC ',RENT'               LINKAGE EDITOR OPTION
         COPY  SGLEDPK1
 PUNCH '//ACMDLIB DD DISP=SHR,VOLUME=(,RETAIN),DSNAME=SYS1.ACMDLIB'
         COPY  SGLEDPK2
 PUNCH '  INCLUDE ACMDLIB(IKJEGCVT,IKJEGATN,IKJEGSTA)'
 PUNCH '  INCLUDE ACMDLIB(IKJEGIO,IKJEGSRH,IKJEGMNL)'
 PUNCH '  INCLUDE ACMDLIB(IKJEGINT,IKJEGMSG)'
 PUNCH '  ENTRY  IKJEGINT'
 PUNCH '  NAME   TEST(R)'
 PUNCH '  INCLUDE ACMDLIB(IKJEGPCH,IKJEGASN)'
 PUNCH '  ENTRY  IKJEGPCH'
 PUNCH '  NAME   IKJEGPCH(R)'
 PUNCH '/*'
          MEND
