         MACRO
         SGICH401
.*
.* $MAC(SGICH401) COMP(SC1BN) PROD(RACF):
.*
.* DESCRIPTIVE NAME = DUMMY SYSGEN SUPPORT FOR RACF
.*                      MASTER SCHEDULER INITIALIZATION PROCESSING
.*
.* STATUS = OS/VS2 EBB1102
.*
.* CHANGE LEVEL = 0
.*
.*
         COPY  SGGBLPAK
&SGCTRLC(6) SETC ''               SET 'MOD' PARM NULL
&SGCTRLC(7) SETC '&SGCTRLC(32)'   SET VOLUME FOR LINKLIB
&SGCTRLC(8) SETC '&SGCTRLC(31)'   SET UNIT NAME FOR LINKLIB
&SGCTRLC(9) SETC 'LINKLIB'        SET DSNAME FOR LINKLIB
&SGCTRLC(10) SETC ',LET'
&SGCTRLC(11) SETC ''
         COPY  SGLEDPK1           PUNCH L.E. JCL DECK
         PUNCH '//AOSB3 DD DISP=SHR,VOL=(,RETAIN),DSNAME=SYS1.AOSB3'
         COPY  SGLEDPK2
.*
.*       ADD ICHSEC00 LOAD
.*
         PUNCH ' INCLUDE AOSB3(IEFBR14)'
         PUNCH ' NAME ICHSEC00(R)'
         PUNCH '/*'
         MEND
