         MACRO
         SGICG500
.* /* START OF SPECIFICATIONS ****
.*
.*01  MODULE-NAME = SGICG500
.*
.*01  DESCRIPTIVE-NAME = MSCTC AND TRACE REPORTS VS/2 SYSGEN
.*                       LINKLIB COPY MACRO
.*
.*01  STATUS = OS/VS2 RELEASE 3 LEVEL 0
.*
.*01  FUNCTION = SGICG500 COPIES MSC TABLE CREATE MODULES
.*               INTO THE TARGET SYS1.LINKLIB (VS2) FROM THE
.*               DISTRIBUTION LIBRARY SYS1.AOSD0
.*
.*               SGICG500 COPIES TRACE REPORT MODULES
.*               INTO THE TARGET SYS1.LINKLIB (VS2) FROM THE
.*               DISTRIBUTION LIBRARY SYS1.AOSD0
.*
.*01  OPERATION = VIA THE ASSEMBLER PUNCH COMMAND, IEBCOPY
.*                SELECT STATEMENTS ARE GENERATED
.*                FOR THE MODULES
.*
.*01  MODULE-TYPE = MACRO
.*02    PROCESSOR = ASSEMBLER XF
.*02    MACRO-SIZE = 16 STATEMENTS
.*
.*01  OUTPUT = IEBCOPY JCL AND CONTROL STATEMENTS
.*
.*01  INNER-MACRO = SGGBLPAK - DEFINE SYSTEM GLOBALS
.*
.*01  MESSAGES = NONE
.*
.**** END OF SPECIFICATIONS ***/
         COPY  SGGBLPAK                DEFINE SYSTEM GLOBAL VARIABLES
.* COPY MSCTC COMPONENT MODULES
 PUNCH ' COPY   OUTDD=LINKLIB,INDD=AOSD0'
 PUNCH ' SELECT MEMBER=(ICGDSMSG,ICGDSSCN,ICGDSS01,ICGDSS02)'
 PUNCH ' SELECT MEMBER=(ICGDSS04,ICGDSS05,ICGDSS06,ICGDSS08)'
 PUNCH ' SELECT MEMBER=(ICGDSS09,ICGDSS99,ICGDSANL,ICGDSA01)'
 PUNCH ' SELECT MEMBER=(ICGDSA02,ICGDSA04,ICGDSA05,ICGDSA06)'
 PUNCH ' SELECT MEMBER=(ICGDSA07,ICGDSA08,ICGDSA09,ICGDSBLD)'
 PUNCH ' SELECT MEMBER=(ICGDSB01,ICGDSB03,ICGDSB05,ICGDSB06)'
 PUNCH ' SELECT MEMBER=(ICGDSB07,ICGDSB08,ICGDSB09,ICGDSB10)'
 PUNCH ' SELECT MEMBER=(ICGDSB11,ICGDSB12,ICGDSB13,ICGDSB14)'
 PUNCH ' SELECT MEMBER=(ICGDSB15,ICGDSB16,ICGDSB18,ICGDSB19)'
 PUNCH ' SELECT MEMBER=(ICGDSB20)'
.* COPY TRACE REPORT COMPONENT MODULES
 PUNCH ' SELECT MEMBER=(ICGTRINT,ICGTRSUM,ICGTRDST,ICGTRVID)'
         MEND
