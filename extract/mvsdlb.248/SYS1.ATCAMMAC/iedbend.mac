         MACRO
&NAME    IEDBEND &TOTE=NO,&TSO=NO
.*A000000-999999                                               @Y17XAFN
.*C005500,007000,007500,011000,011600,012500,013100,014100     @OZ27691
.*C015100                                                      @OZ27691
.*C015100                                                      @OZ38859
.*C011500,013000                                               @OZ40246
.* &TOTE - THIS OPERAND DETERMINES IF THE TOTE BIND IMAGES SHOULD BE
.*         INCLUDED IN THE BIND IMAGE TABLE.
.* &TSO - THIS OPERAND DETERMINES IF THE TSO BIND IMAGES SHOULD BE
.*        INCLUDED IN THE BIND IMAGE TABLE.
.* THE IEDBEND MACRO ALSO ENDS THE BIND IMAGE TABLE WITH A X'FF'.
         AIF   ('&TOTE' NE 'YES').BITOTNO INCLUDE TOTE BI
         ENTRY IEDTOTB1
IEDTOTB1 IEDBENT PRIPROT=X'A1',SECPROT=X'90',COMPROT=XL2'3080',        *
               LUPROF=X'01',TSUSAGE=XL6'000185850100'
         ENTRY IEDTOTB2
IEDTOTB2 IEDBENT FMPROF=X'02',TSPROF=X'02',                    @OZ27691*
               COMPROT=XL2'2000',TSUSAGE=XL6'000185850100',    @YM08396X
               SECPROT=X'40',PRIPROT=X'21'                     @OZ27691
         AGO   .BITSO                    CHECK FOR TSO=YES
.BITOTNO AIF   ('&TOTE' EQ 'NO').BITSO    DO NOT INCLUDE TSO BI
         MNOTE 8,'*** IEDB05 TOTE OPERAND INCORRECTLY DEFINED, DEFAULT *
               TO TOTE=NO.'
.BITSO   AIF   ('&TSO' NE 'YES').BITSONO  INCLUDE TSO BI
         ENTRY IEDTSOB0
IEDTSOB0 IEDBENT FMPROF=X'02',TSPROF=X'02',PRIPROT=X'21',              X
               COMPROT=XL2'2000',TSUSAGE=XL6'000000000000',    @OZ40246X
               LOGON=C'   LOGON ',SECPROT=X'40'                @OZ27691
         ENTRY IEDTSOB1
IEDTSOB1 IEDBENT PRIPROT=X'A1',SECPROT=X'90',COMPROT=XL2'3040',        X
               TSUSAGE=XL6'000085850000',LUPROF=X'01',         @OZ40246X
               LOGON=C'LOGON '                                 @OZ27691
         ENTRY IEDTSOB2
IEDTSOB2 IEDBENT PRIPROT=X'B1',SECPROT=X'B0',COMPROT=XL2'3080',        X
               TSUSAGE=XL6'000085850000',LUPROF=X'02',         @ZM47842X
               PRESERC=XL11'0200',                             @ZM47842X
               LOGON=C'     LOGON '                            @OZ38859
         AGO   .BITBEND                   END THE BI TABLE
.BITSONO AIF   ('&TSO' EQ 'NO').BITBEND   DO NOT INCLUDE TSO BI
         MNOTE 8,'*** IEDB07 TSO OPERAND INCORRECTLY DEFINED, DEFAULT T*
               O TSO=NO.'
.BITBEND ANOP
         DC    X'FF'                    DELIMITER FOR END OF BI TABLE
         END
.MEND    MEND
