         MACRO
&NAME    HANGUP
         GBLA  &IEDQZE(64)
         GBLB  &IEDQZA(64)
         LCLA  &ALG,&BIT,&LEN
         LCLB  &NOGO
.*             VERSION DATED MAY 22,1970
         SPACE
         AIF   (&IEDQZA(9)).H1 .   HANGUP FOR TSO ONLY
         MNOTE 12,'*** IHB312 - MACRO MUST FOLLOW STARTMH MACRO WITH   *
                TSOMH=YES.'
&NOGO    SETB  1
.H1      AIF   (&IEDQZA(5)).CKERR
         MNOTE 12,'*** IHB070 - SEQUENCE ERROR-MUST FOLLOW INMSG OR    *
                OUTMSG MACRO.'
&NOGO    SETB  1
.CKERR   AIF   (&NOGO).MEND
&LEN     SETA  8
&BIT     SETA  1
.SKP     IEDQVCON 37,IEDAYX .      INMSG/OUTMSG INTERFACE
&NAME    DC    AL1(&IEDQZE(37)+&BIT),AL1(&LEN),X'0000'
         DC    V(IEDAYH)
.MEND    SPACE
         MEND
