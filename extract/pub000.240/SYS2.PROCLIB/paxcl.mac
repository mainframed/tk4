//PAXCL  PROC  LKEDREG=300K,LKEDPRM=MAP,MEMBER=GO####,
//             SOUT='*',WORK=VIO,OPT='M+' 1..72 MARGINS
//*
//*
//COMPILE EXEC PGM=PASCAL,REGION=8192K,PARM='&OPT'
//STEPLIB  DD  DSN=PASCAL.PASLIB,DISP=SHR
//INPUT    DD  DDNAME=SYSIN
//PRD      DD  DSN=PASCAL.PASOBJ(PASMSG),DISP=SHR
//OUTPUT   DD  UNIT=&WORK,DSN=&&XIN,SPACE=(TRK,(19,19)),DISP=(,PASS)
//PRR      DD  DSN=&&PCODE,UNIT=&WORK,DCB=RECFM=VB,
//             SPACE=(TRK,(20,5),RLSE),DISP=(,PASS)
//QRR      DD  DSN=&&TABLES,UNIT=&WORK,DCB=RECFM=VB,
//             SPACE=(TRK,(5,2),RLSE),DISP=(,PASS)
//*
//*
//XREF   EXEC  PGM=PASCREF,REGION=2048K,COND=EVEN
//STEPLIB  DD  DISP=SHR,DSN=PASCAL.PASLIB
//INPUT    DD  DISP=(OLD,DELETE),DSN=*.COMPILE.OUTPUT
//OUTPUT   DD  SYSOUT=&SOUT
//*
//*
//POSTPROC EXEC PGM=ASMPCODE,COND=(0,LT,COMPILE),REGION=2048K
//STEPLIB  DD  DSN=PASCAL.PASLIB,DISP=SHR
//INPUT    DD  DSN=*.COMPILE.PRR,DISP=(OLD,DELETE)
//PRD      DD  DSN=*.COMPILE.QRR,DISP=(OLD,PASS)
//OUTPUT   DD  SYSOUT=&SOUT
//PRR      DD  DSN=&&LOADSET,UNIT=&WORK,DCB=RECFM=FB,
//             SPACE=(TRK,(10,5),RLSE),DISP=(,PASS)
//*
//*
//LKED   EXEC  PGM=IEWLF880,PARM=(LET,LIST,&LKEDPRM),
//             COND=((0,LT,COMPILE),(0,LT,POSTPROC)),REGION=&LKEDREG
//SYSLIB   DD  DISP=SHR,DSN=PASCAL.PASLIB
//         DD  DISP=SHR,DSN=SYS1.FORTLIB
//SYSLMOD  DD  DSN=&&GOSET(&MEMBER),DISP=(MOD,PASS),UNIT=&WORK,
//             SPACE=(CYL,(5,5,27),,CONTIG)
//SYSPRINT DD  SYSOUT=&SOUT,DCB=BLKSIZE=605
//SYSUT1   DD  UNIT=SYSDA,SPACE=(CYL,(10,10))
//SYSLIN   DD  DSN=&&LOADSET,DISP=(OLD,DELETE)
//         DD  DDNAME=SYSIN
//*
