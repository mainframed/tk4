//JCCCLG  PROC SOUT='*',JCC='JCC',
//        INFILE='',JOPTS='-o -list=//DDN:SYSPRINT'
//********************************************************************
//*
//* Name: SYS2.PROCLIB(JCCCLG)
//*
//* Desc: Compile and link C program using JCC, PRELINK and IEWL,
//*       then execute the newly linked program.
//*
//********************************************************************
//COMPILE  EXEC PGM=JCC,
//         PARM='-I//DDN:JCCINCL //DDN:SYSIN &JOPTS'
//STEPLIB  DD   DSN=&JCC..LINKLIB,DISP=SHR
//JCCINCL  DD   DSN=&JCC..INCLUDE,DISP=SHR
//JCCINCS  DD   DSN=&JCC..INCLUDE,DISP=SHR
//JCCOUTPT DD   UNIT=SYSDA,SPACE=(TRK,(50,20)),DISP=(,PASS),DSN=&&OUTPT
//STDOUT   DD   SYSOUT=&SOUT
//SYSPRINT DD   SYSOUT=&SOUT
//JCCOASM  DD   UNIT=VIO,SPACE=(3200,(200,100),RLSE),DSN=&&OBJ,
//         DCB=(RECFM=FB,LRECL=80,BLKSIZE=3200),DISP=(,PASS)
//SYSIN    DD   DSN=&INFILE,DISP=SHR
//*
//PRELINK  EXEC PGM=PRELINK,
//         PARM='-r //DDN:L //DDN:O //DDN:I'
//STEPLIB  DD   DSN=&JCC..LINKLIB,DISP=SHR
//STDOUT   DD   SYSOUT=&SOUT
//L        DD   DSN=&JCC..OBJ,DISP=SHR
//I        DD   DSN=&&OBJ,DISP=(OLD,DELETE)
//O        DD   UNIT=VIO,SPACE=(3200,(200,100),RLSE),DSN=&&OBJMOD,
//         DCB=(RECFM=FB,LRECL=80,BLKSIZE=3200),DISP=(,PASS)
//*
//LKED     EXEC PGM=IEWL,PARM='NCAL,MAP,LIST,XREF,NORENT'
//SYSUT1   DD UNIT=SYSDA,SPACE=(CYL,(5,2))
//SYSPRINT DD SYSOUT=&SOUT
//SYSLIN   DD DSN=&&OBJMOD,DISP=(OLD,DELETE)
//SYSLMOD  DD DSN=&&GOSET(GO),UNIT=SYSDA,SPACE=(4096,(100,20,1)),
//         DCB=(RECFM=U,BLKSIZE=4096),DISP=(,PASS)
//*
//GO       EXEC PGM=GO
//STEPLIB  DD DSN=&&GOSET,DISP=(OLD,DELETE)
//STDOUT   DD SYSOUT=&SOUT
//STDERR   DD SYSOUT=&SOUT
//STDIN    DD DDNAME=SYSIN
