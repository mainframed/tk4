//DEFAULT PROC RGPARM='NODECK,LOAD,LIST,NOSEQN',SOUT='*',              1
//             RGU3SPC='(600,(100,20))',RGU2SPC='(600,(100,20))',      2
//             RGU1SPC='(600,(100,20))',RGGOSPC='(80,(200,50))',       3
//             LKPARM='XREF,LIST,LET',                                 4
//             LKMDSPC='(1024,(50,20,1))',LKU1SPC='(1024,(50,20))',    5
//             GOCOND='((9,LT,RPG),(5,LT,LKED))',GOPARM='DATE=000000'
//RPG     EXEC PGM=IESRPG,PARM=(&RGPARM)
//SYSPRINT DD  SYSOUT=&SOUT
//SYSPUNCH DD  SYSOUT=B
//SYSUT3   DD  DSNAME=&&SYSUT3,UNIT=SYSSQ,SPACE=&RGU3SPC
//SYSUT2   DD  DSNAME=&&SYSUT2,UNIT=SYSSQ,SPACE=&RGU2SPC
//SYSUT1   DD  DSNAME=&&SYSUT1,UNIT=SYSSQ,SPACE=&RGU1SPC
//SYSGO   DD  DSNAME=&&LOADSET,UNIT=(SYSSQ,SEP=SYSPUNCH),              1
//             DISP=(MOD,PASS,DELETE),SPACE=&RGGOSPC
//LKED    EXEC PGM=IEWL,PARM=(&LKPARM)
//SYSLIN   DD  DSNAME=&&LOADSET,DISP=(OLD,DELETE)
//         DD  DDNAME=SYSIN
//SYSLMOD  DD  DSNAME=&&GOSET(RPG),UNIT=SYSDA,DISP=(NEW,PASS,DELETE),  1
//             SPACE=&LKMDSPC
//SYSUT1   DD  DSNAME=&&SYSUT1,SPACE=&LKU1SPC,                         1
//             UNIT=(SYSDA,SEP=(SYSLIN,SYSLMOD))
//SYSPRINT DD  SYSOUT=&SOUT
//GO      EXEC PGM=*.LKED.SYSLMOD,COND=&GOCOND,PARM='&GOPARM'
