//HERC01  JOB  (SETUP),
//             'Install DYNABLDL',
//             CLASS=A,
//             MSGCLASS=A,
//             MSGLEVEL=(1,1),
//             NOTIFY=HERC01
//********************************************************************
//*
//* Name: SYS2.DYNABLDL.ASM(INSTALL)
//*
//* Desc: Install DYNABLDL package into MVS 38J
//*
//* Note: The DYNABLDL package was provided by Brian Westerman
//*
//********************************************************************
//ASM     EXEC PGM=IFOX00,REGION=1024K,
//             PARM='TEST,TERM,LOAD,NODECK'
//SYSTERM  DD  SYSOUT=*
//SYSPRINT DD  SYSOUT=*
//SYSLIB   DD  DISP=SHR,DSN=SYS1.MACLIB,DCB=BLKSIZE=32760
//         DD  DISP=SHR,DSN=SYS1.AMODGEN
//         DD  DISP=SHR,DSN=SYS2.DYNABLDL.ASM
//SYSUT1   DD  UNIT=VIO,SPACE=(CYL,(1,1))
//SYSUT2   DD  UNIT=VIO,SPACE=(CYL,(1,1))
//SYSUT3   DD  UNIT=VIO,SPACE=(CYL,(1,1))
//SYSGO    DD  DISP=(,PASS),UNIT=VIO,SPACE=(CYL,(1,1))
//SYSIN    DD  DISP=SHR,DSN=SYS2.DYNABLDL.ASM(DYNABLDL)
//*
//LKED    EXEC PGM=IEWL,REGION=1024K,COND=(4,LT),
//             PARM='TEST,NCAL,LIST,XREF'
//SYSPRINT DD  SYSOUT=*
//SYSUT1   DD  UNIT=VIO,SPACE=(CYL,(1,1))
//SYSLMOD  DD  DISP=SHR,DSN=SYS2.LINKLIB(DYNABLDL)
//SYSLIN   DD  DISP=(OLD,DELETE),DSN=*.ASM.SYSGO
//PROC    EXEC PGM=IEBCOPY
//SYSPRINT DD  SYSOUT=*
//IN       DD  DISP=SHR,DSN=SYS2.DYNABLDL.ASM
//OUT1     DD  DISP=SHR,DSN=SYS2.PROCLIB
//SYSIN    DD  *
 COPY INDD=IN,OUTDD=OUT1,LIST=NO
 SELECT MEMBER=((PROC,DYNABLDL))
 SELECT MEMBER=DBREPORT
 SELECT MEMBER=DBSTART
 SELECT MEMBER=DBSTOP
