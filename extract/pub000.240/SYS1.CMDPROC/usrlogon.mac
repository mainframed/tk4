        PROC 0
CONTROL NOMSG,NOLIST,NOSYMLIST,NOCONLIST,NOFLUSH
FREE FILE(SYSHELP)
ALLOC FILE(SYSHELP) DSN('SYS1.HELP','SYS2.HELP') SHR
ALLOC FILE(X1) DSN('&SYSUID..CMDPROC(STDLOGON)') SHR
IF &LASTCC = 0 THEN +
   DO
      FREE FILE(SYSPROC)
      FREE FILE(X1)
      ALLOC FILE(SYSPROC) +
       DSN('&SYSUID..CMDPROC','SYS1.CMDPROC','SYS2.CMDPROC') SHR
   END
ELSE +
   DO
      FREE FILE(X1)
   END
%STDLOGON
%REVINIT
TERMTYPE
SET SZ = &LASTCC
IF &SZ NE 0 THEN -
  DO
    WRITENR ***
    READ BLABLA
    CLS
  END
BSPFCOOK
IF &SZ NE 0 THEN -
  DO
    WRITENR ***
    READ BLABLA
    CLS
    TSOAPPLS
  END
ELSE -
  DO
    TERMINAL LINESIZE(100)
    %TTYINTRO
  END
EXIT
