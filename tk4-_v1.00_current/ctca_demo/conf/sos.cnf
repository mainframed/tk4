020E 1403 prt_${SYSNUM}/stream-3_output.txt ${SOSCRLF}
030E 1403 prt_${SYSNUM}/stream-4_output.txt ${SOSCRLF}
0610 CTCE ${CTCL} 127.0.0.1 ${CTCR}
DEFSYM RDR 00C
DEFSYM GRAF 0C0
